{"dta.poem.20697": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der P\u00e4chter und der arme Sch\u00e4fer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem reichen P\u00e4chter Schinkenrund                 ", "tokens": ["Dem", "rei\u00b7chen", "P\u00e4ch\u00b7ter", "Schin\u00b7ken\u00b7rund"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gefiel im n\u00e4chsten Dorfe", "tokens": ["Ge\u00b7fiel", "im", "n\u00e4chs\u00b7ten", "Dor\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des sch\u00f6nsten M\u00e4dchens Rosenmund.", "tokens": ["Des", "sch\u00f6ns\u00b7ten", "M\u00e4d\u00b7chens", "Ro\u00b7sen\u00b7mund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Herz hatt\u2019 gleich dem Torfe", "tokens": ["Sein", "Herz", "hatt'", "gleich", "dem", "Tor\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Geglommen, als er sie gesehn,", "tokens": ["Ge\u00b7glom\u00b7men", ",", "als", "er", "sie", "ge\u00b7sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es war in Brand gerathen", "tokens": ["Es", "war", "in", "Brand", "ge\u00b7ra\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wollte nicht um Rettung flehn,", "tokens": ["Und", "woll\u00b7te", "nicht", "um", "Ret\u00b7tung", "flehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Denn Dorchens Blicke thaten", "tokens": ["Denn", "Dor\u00b7chens", "Bli\u00b7cke", "tha\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ihm immer tapfern Widerstand.", "tokens": ["Ihm", "im\u00b7mer", "tap\u00b7fern", "Wi\u00b7der\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er konnte nichts gewinnen", "tokens": ["Er", "konn\u00b7te", "nichts", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Bei ihrem L\u00e4cheln, denn er fand", "tokens": ["Bei", "ih\u00b7rem", "L\u00e4\u00b7cheln", ",", "denn", "er", "fand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Tugend Sprache drinnen.", "tokens": ["Der", "Tu\u00b7gend", "Spra\u00b7che", "drin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Einst sa\u00df er in dem Traubenmond", "tokens": ["Einst", "sa\u00df", "er", "in", "dem", "Trau\u00b7ben\u00b7mond"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor seiner Th\u00fcr und zechte \u2014", "tokens": ["Vor", "sei\u00b7ner", "Th\u00fcr", "und", "zech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er war des Zechens sehr gewohnt;", "tokens": ["Er", "war", "des", "Ze\u00b7chens", "sehr", "ge\u00b7wohnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein P\u00e4chter trinkt mit Rechte", "tokens": ["Ein", "P\u00e4ch\u00b7ter", "trinkt", "mit", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den jungen Most, den alten Wein", "tokens": ["Den", "jun\u00b7gen", "Most", ",", "den", "al\u00b7ten", "Wein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus einer gro\u00dfen Kanne,", "tokens": ["Aus", "ei\u00b7ner", "gro\u00b7\u00dfen", "Kan\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Gl\u00e4ser sind ihm viel zu klein,", "tokens": ["Die", "Gl\u00e4\u00b7ser", "sind", "ihm", "viel", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die sind dem B\u00fcrgersmanne", "tokens": ["Die", "sind", "dem", "B\u00fcr\u00b7gers\u00b7man\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Nur angemessen, nur gesund,", "tokens": ["Nur", "an\u00b7ge\u00b7mes\u00b7sen", ",", "nur", "ge\u00b7sund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nach kluger Aerzte Sagen \u2014", "tokens": ["Nach", "klu\u00b7ger", "A\u00b7erz\u00b7te", "Sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Aus Kannen trinket Schinkenrund,", "tokens": ["Aus", "Kan\u00b7nen", "trin\u00b7ket", "Schin\u00b7ken\u00b7rund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Kopf kann Wein vertragen.", "tokens": ["Sein", "Kopf", "kann", "Wein", "ver\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Claus Aermlich, Dorchens Vater, k\u00f6mmt", "tokens": ["Claus", "A\u00b7erm\u00b7lich", ",", "Dor\u00b7chens", "Va\u00b7ter", ",", "k\u00f6mmt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "NE", "$,", "ADV", "NN", "$,", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Recht zu gelegner Stunde,", "tokens": ["Recht", "zu", "ge\u00b7leg\u00b7ner", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf seinen Sch\u00e4ferstab gest\u00e4mmt,", "tokens": ["Auf", "sei\u00b7nen", "Sch\u00e4\u00b7fer\u00b7stab", "ge\u00b7st\u00e4mmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und gr\u00fc\u00dft mit trocknem Munde", "tokens": ["Und", "gr\u00fc\u00dft", "mit", "trock\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den P\u00e4chter und Gevattersmann;", "tokens": ["Den", "P\u00e4ch\u00b7ter", "und", "Ge\u00b7vat\u00b7ters\u00b7mann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der horcht mit schlauem Ohre,", "tokens": ["Der", "horcht", "mit", "schlau\u00b7em", "Oh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Dankt ihm so freundlich als er kann,", "tokens": ["Dankt", "ihm", "so", "freund\u00b7lich", "als", "er", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "KOKOM", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und fr\u00e4gt nach seiner Dore,", "tokens": ["Und", "fr\u00e4gt", "nach", "sei\u00b7ner", "Do\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und trinkts dem alten Wei\u00dfkopf zu", "tokens": ["Und", "trinkts", "dem", "al\u00b7ten", "Wei\u00df\u00b7kopf", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mit Bitte, mit Bedingen", "tokens": ["Mit", "Bit\u00b7te", ",", "mit", "Be\u00b7din\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Ihm morgen um die Mittagsruh", "tokens": ["Ihm", "mor\u00b7gen", "um", "die", "Mit\u00b7tags\u00b7ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sein Dorchen herzubringen.", "tokens": ["Sein", "Dor\u00b7chen", "her\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Er wollt ihr einen Vorschlag thun,", "tokens": ["Er", "wollt", "ihr", "ei\u00b7nen", "Vor\u00b7schlag", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der werth sey anzunehmen;", "tokens": ["Der", "werth", "sey", "an\u00b7zu\u00b7neh\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Auch sollte Claus im Alter ruhn,", "tokens": ["Auch", "soll\u00b7te", "Claus", "im", "Al\u00b7ter", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und ohne Noth und Gr\u00e4men", "tokens": ["Und", "oh\u00b7ne", "Noth", "und", "Gr\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Sein Brod genie\u00dfen, seinen Wein,", "tokens": ["Sein", "Brod", "ge\u00b7nie\u00b7\u00dfen", ",", "sei\u00b7nen", "Wein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Das Herz damit zu laben, \u2014", "tokens": ["Das", "Herz", "da\u00b7mit", "zu", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PAV", "PTKZU", "VVINF", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Claus Aermlich h\u00f6rts und saget nein,", "tokens": ["Claus", "A\u00b7erm\u00b7lich", "h\u00f6rts", "und", "sa\u00b7get", "nein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "KON", "VVFIN", "PTKANT", "$,"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.22": {"text": "Ich will, ich mag nichts haben.", "tokens": ["Ich", "will", ",", "ich", "mag", "nichts", "ha\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "PIS", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Behaltet euren Labetrank,", "tokens": ["Be\u00b7hal\u00b7tet", "eu\u00b7ren", "La\u00b7be\u00b7trank", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Ich trinke reines Wasser", "tokens": ["Ich", "trin\u00b7ke", "rei\u00b7nes", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Und bin Zeitlebens, Gott sey Dank!", "tokens": ["Und", "bin", "Zeit\u00b7le\u00b7bens", ",", "Gott", "sey", "Dank", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "$,", "NN", "VAFIN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.26": {"text": "Des Lasterlebens Hasser.", "tokens": ["Des", "Las\u00b7ter\u00b7le\u00b7bens", "Has\u00b7ser", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}