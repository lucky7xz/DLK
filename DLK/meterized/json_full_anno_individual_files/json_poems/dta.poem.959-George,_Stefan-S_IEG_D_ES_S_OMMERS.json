{"dta.poem.959": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "S IEG   D ES   S OMMERS", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Ruhm diesen wipfeln dieser farbenflur               ", "tokens": ["Ruhm", "die\u00b7sen", "wip\u00b7feln", "die\u00b7ser", "far\u00b7ben\u00b7flur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PDAT", "NN", "PDAT", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sie lehrten uns das gl&#252;ck in seinem fl&#252;chten               ", "tokens": ["Sie", "lehr\u00b7ten", "uns", "das", "gl", "&#252;", "ck", "in", "sei\u00b7nem", "fl", "&#252;", "ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "FM", "FM", "FM", "APPR", "PPOSAT", "NN", "$(", "NE"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zu streifen und es bleibt noch zarte spur               ", "tokens": ["Zu", "strei\u00b7fen", "und", "es", "bleibt", "noch", "zar\u00b7te", "spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "KON", "PPER", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "An unsrer hand wie schmelz von reifen fr&#252;chten             ", "tokens": ["An", "uns\u00b7rer", "hand", "wie", "schmelz", "von", "rei\u00b7fen", "fr", "&#252;", "ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["APPR", "PPOSAT", "NN", "KOKOM", "ADJD", "APPR", "VVFIN", "NE", "$(", "NE"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Schon weht das wimpel und es s&#228;umt nicht mehr               ", "tokens": ["Schon", "weht", "das", "wim\u00b7pel", "und", "es", "s", "&#228;", "umt", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "PPER", "NE", "$(", "VVFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Aus scheidestunden werden thr&#228;nen rinnen               ", "tokens": ["Aus", "schei\u00b7des\u00b7tun\u00b7den", "wer\u00b7den", "thr", "&#228;", "nen", "rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["APPR", "VVINF", "VAFIN", "ADJD", "$(", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob einer zweifelhaften wiederkehr               ", "tokens": ["Ob", "ei\u00b7ner", "zwei\u00b7fel\u00b7haf\u00b7ten", "wie\u00b7der\u00b7kehr"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In offnem schmerze zogest du von hinnen             ", "tokens": ["In", "off\u00b7nem", "schmer\u00b7ze", "zo\u00b7gest", "du", "von", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVFIN", "VVFIN", "PPER", "APPR", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich aber horche in die nahe nacht               ", "tokens": ["Ich", "a\u00b7ber", "hor\u00b7che", "in", "die", "na\u00b7he", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ob dort ein lezter vogelruf vermelde               ", "tokens": ["Ob", "dort", "ein", "lez\u00b7ter", "vo\u00b7gel\u00b7ruf", "ver\u00b7mel\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den schlaf aus dem sie frisch und sch\u00f6n erwacht               ", "tokens": ["Den", "schlaf", "aus", "dem", "sie", "frisch", "und", "sch\u00f6n", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PRELS", "PPER", "ADJD", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der liebe sachten schlaf im blumenfelde.             ", "tokens": ["Der", "lie\u00b7be", "sach\u00b7ten", "schlaf", "im", "blu\u00b7men\u00b7fel\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}