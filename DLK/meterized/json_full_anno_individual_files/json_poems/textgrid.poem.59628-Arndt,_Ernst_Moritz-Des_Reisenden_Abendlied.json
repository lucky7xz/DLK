{"textgrid.poem.59628": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Des Reisenden Abendlied", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gegangen ist das Sonnenlicht,", "tokens": ["Ge\u00b7gan\u00b7gen", "ist", "das", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Still schweiget Feld und Hain,", "tokens": ["Still", "schwei\u00b7get", "Feld", "und", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hell am Firmamente bricht", "tokens": ["Und", "hell", "am", "Fir\u00b7ma\u00b7men\u00b7te", "bricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hervor der Sterne Schein,", "tokens": ["Her\u00b7vor", "der", "Ster\u00b7ne", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und hell aus stiller Seele blitzt", "tokens": ["Und", "hell", "aus", "stil\u00b7ler", "See\u00b7le", "blitzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein wundersamer Strahl", "tokens": ["Ein", "wun\u00b7der\u00b7sa\u00b7mer", "Strahl"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von dem, der ewig waltend sitzt", "tokens": ["Von", "dem", ",", "der", "e\u00b7wig", "wal\u00b7tend", "sitzt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "PRELS", "ADJD", "VVPP", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im hohen Himmelssaal.", "tokens": ["Im", "ho\u00b7hen", "Him\u00b7mels\u00b7saal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie w\u00e4re doch das Menschenkind", "tokens": ["Wie", "w\u00e4\u00b7re", "doch", "das", "Men\u00b7schen\u00b7kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So elend, so allein,", "tokens": ["So", "e\u00b7lend", ",", "so", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn nicht von oben zart und lind", "tokens": ["Wenn", "nicht", "von", "o\u00b7ben", "zart", "und", "lind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "APPR", "ADV", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm k\u00e4me dieser Schein?", "tokens": ["Ihm", "k\u00e4\u00b7me", "die\u00b7ser", "Schein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es w\u00e4re nichts als Trug und Wahn,", "tokens": ["Es", "w\u00e4\u00b7re", "nichts", "als", "Trug", "und", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein zitternd Blatt am Baum,", "tokens": ["Ein", "zit\u00b7ternd", "Blatt", "am", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein K\u00f6rnlein Sand im Ozean,", "tokens": ["Ein", "K\u00f6rn\u00b7lein", "Sand", "im", "O\u00b7ze\u00b7an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Traumbild fast vom Traum.", "tokens": ["Ein", "Traum\u00b7bild", "fast", "vom", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das Leben wallt von Ort zu Ort,", "tokens": ["Das", "Le\u00b7ben", "wallt", "von", "Ort", "zu", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat nimmer Ruh' noch Rast", "tokens": ["Hat", "nim\u00b7mer", "Ruh'", "noch", "Rast"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "NN", "ADV", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und treibt im wilden Fluge fort,", "tokens": ["Und", "treibt", "im", "wil\u00b7den", "Flu\u00b7ge", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geschnellt durch eigne Last;", "tokens": ["Ge\u00b7schnellt", "durch", "eig\u00b7ne", "Last", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es brauset wie ein sch\u00e4umend Meer,", "tokens": ["Es", "brau\u00b7set", "wie", "ein", "sch\u00e4u\u00b7mend", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das keine Ufer kennt,", "tokens": ["Das", "kei\u00b7ne", "U\u00b7fer", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wirft uns Tropfen hin und her", "tokens": ["Und", "wirft", "uns", "Trop\u00b7fen", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im wilden Element.", "tokens": ["Im", "wil\u00b7den", "E\u00b7le\u00b7ment", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Drum komm, o du, der Frieden bringt,", "tokens": ["Drum", "komm", ",", "o", "du", ",", "der", "Frie\u00b7den", "bringt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "FM", "PPER", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Gott, in stiller Nacht,", "tokens": ["O", "Gott", ",", "in", "stil\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo hell die Engelglocke klingt", "tokens": ["Wo", "hell", "die", "En\u00b7gel\u00b7glo\u00b7cke", "klingt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei goldner Sterne Pracht \u2013", "tokens": ["Bei", "gold\u00b7ner", "Ster\u00b7ne", "Pracht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Komm, wirf den frommen Liebesstrahl", "tokens": ["Komm", ",", "wirf", "den", "from\u00b7men", "Lie\u00b7bes\u00b7strahl"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVIMP", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir warm ins arme Herz,", "tokens": ["Mir", "warm", "ins", "ar\u00b7me", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und die Gedanken allzumal,", "tokens": ["Und", "die", "Ge\u00b7dan\u00b7ken", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O zieh sie himmelw\u00e4rts!", "tokens": ["O", "zieh", "sie", "him\u00b7mel\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.5": {"line.1": {"text": "Drum komm mit deinem Engelheer,", "tokens": ["Drum", "komm", "mit", "dei\u00b7nem", "En\u00b7gel\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Vater lieb und gut!", "tokens": ["Du", "Va\u00b7ter", "lieb", "und", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du bist die einzig feste Wehr,", "tokens": ["Du", "bist", "die", "ein\u00b7zig", "fes\u00b7te", "Wehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die einzig sichre Hut;", "tokens": ["Die", "ein\u00b7zig", "sich\u00b7re", "Hut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gar nichtig ist der Menschen Macht,", "tokens": ["Gar", "nich\u00b7tig", "ist", "der", "Men\u00b7schen", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die eitle Eitelkeit:", "tokens": ["Die", "eit\u00b7le", "Ei\u00b7tel\u00b7keit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was Gott bewacht, ist wohl bewacht", "tokens": ["Was", "Gott", "be\u00b7wacht", ",", "ist", "wohl", "be\u00b7wacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "NN", "VVPP", "$,", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hier und in Ewigkeit.", "tokens": ["Hier", "und", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gegangen ist das Sonnenlicht,", "tokens": ["Ge\u00b7gan\u00b7gen", "ist", "das", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "p