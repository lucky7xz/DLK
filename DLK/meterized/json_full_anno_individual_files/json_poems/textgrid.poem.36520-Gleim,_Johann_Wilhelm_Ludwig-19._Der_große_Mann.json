{"textgrid.poem.36520": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "19. Der gro\u00dfe Mann", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Mann, der Gott, und sich, und Gottes Welt studierte,", "tokens": ["Ein", "Mann", ",", "der", "Gott", ",", "und", "sich", ",", "und", "Got\u00b7tes", "Welt", "stu\u00b7dier\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "KON", "PRF", "$,", "KON", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sa\u00df auf dem Gotthard, sah sich um,", "tokens": ["Sa\u00df", "auf", "dem", "Got\u00b7thard", ",", "sah", "sich", "um", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NE", "$,", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und fand um sich herum,", "tokens": ["Und", "fand", "um", "sich", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auf zwanzig Meilen ins Gevierte,", "tokens": ["Auf", "zwan\u00b7zig", "Mei\u00b7len", "ins", "Ge\u00b7vier\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wohin er sah, auf Esel oder Pferd,", "tokens": ["Wo\u00b7hin", "er", "sah", ",", "auf", "E\u00b7sel", "o\u00b7der", "Pferd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word",