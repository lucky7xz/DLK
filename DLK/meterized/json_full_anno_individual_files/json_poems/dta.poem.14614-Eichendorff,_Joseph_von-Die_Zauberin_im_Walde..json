{"dta.poem.14614": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Zauberin im Walde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eund von ihrem Hals behende", "tokens": ["\u201e", "und", "von", "ih\u00b7rem", "Hals", "be\u00b7hen\u00b7de"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "APPR", "PPOSAT", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Th\u00e4t sie I\u00f6sen eine Kette,", "tokens": ["Th\u00e4t", "sie", "I\u00b7\u00f6\u00b7sen", "ei\u00b7ne", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Reicht' mit ihren wei\u00dfen H\u00e4nden", "tokens": ["Reicht'", "mit", "ih\u00b7ren", "wei\u00b7\u00dfen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir die allersch\u00f6nste Perle.\u201c", "tokens": ["Mir", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Per\u00b7le", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}