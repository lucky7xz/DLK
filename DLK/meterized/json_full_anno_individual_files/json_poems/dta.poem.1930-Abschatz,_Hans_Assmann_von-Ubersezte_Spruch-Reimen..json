{"dta.poem.1930": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wer das g\u00f6ldne Mittel liebt/ steckt nicht unter Spinnen-", "tokens": ["Wer", "das", "g\u00f6ld\u00b7ne", "Mit\u00b7tel", "liebt", "/", "steckt", "nicht", "un\u00b7ter", "Spin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$(", "VVFIN", "PTKNEG", "APPR", "TRUNC"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "weben", "tokens": ["we\u00b7ben"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "chwartzer H\u00fctte stets verdeckt/ wei\u00df auch ohne Neyd zu leben", "tokens": ["chwart\u00b7zer", "H\u00fct\u00b7te", "stets", "ver\u00b7deckt", "/", "wei\u00df", "auch", "oh\u00b7ne", "Neyd", "zu", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$(", "VVFIN", "ADV", "APPR", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Der bey stoltzen H\u00f6fen herrscht/ hofft bey rauher Winde", "tokens": ["Der", "bey", "stolt\u00b7zen", "H\u00f6\u00b7fen", "herrscht", "/", "hofft", "bey", "rau\u00b7her", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$(", "VVFIN", "APPR", "ADJA", "NN"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Br\u00fcllen/", "tokens": ["Br\u00fcl\u00b7len", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "F\u00fcrcht den Wechsel des Gel\u00fccks/ wenn ihm alles geht nach", "tokens": ["F\u00fcrcht", "den", "Wech\u00b7sel", "des", "Ge\u00b7l\u00fccks", "/", "wenn", "ihm", "al\u00b7les", "geht", "nach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN", "$(", "KOUS", "PPER", "PIS", "VVFIN", "APPR"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Willen.", "tokens": ["Wil\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Denn der Winter k\u00f6mmt und weicht/ Freude wechselt mit Be-", "tokens": ["Denn", "der", "Win\u00b7ter", "k\u00f6mmt", "und", "weicht", "/", "Freu\u00b7de", "wech\u00b7selt", "mit", "Be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "$(", "NN", "VVFIN", "APPR", "TRUNC"], "meter": "--+-+-++-+--+", "measure": "iambic.hexa.chol"}, "line.9": {"text": "schwerden/", "tokens": ["schwer\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Gehts izt schlimm/ so bleibt der Trost/ da\u00df es bald wird besser", "tokens": ["Gehts", "izt", "schlimm", "/", "so", "bleibt", "der", "Trost", "/", "da\u00df", "es", "bald", "wird", "bes\u00b7ser"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "$(", "ADV", "VVFIN", "ART", "NN", "$(", "KOUS", "PPER", "ADV", "VAFIN", "ADJD"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "werden.", "tokens": ["wer\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Cynthius f\u00fchrt nicht allzeit bey sich den gespannten Bogen/", "tokens": ["Cyn\u00b7thi\u00b7us", "f\u00fchrt", "nicht", "all\u00b7zeit", "bey", "sich", "den", "ge\u00b7spann\u00b7ten", "Bo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ADV", "APPR", "PRF", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.13": {"text": "Unterweilen hat er auch linde S\u00e4iten auffgezogen.", "tokens": ["Un\u00b7ter\u00b7wei\u00b7len", "hat", "er", "auch", "lin\u00b7de", "S\u00e4i\u00b7ten", "auff\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Schaue da\u00df man dich allzeit im Gedrangen muttig finde/", "tokens": ["Schau\u00b7e", "da\u00df", "man", "dich", "all\u00b7zeit", "im", "Ge\u00b7dran\u00b7gen", "mut\u00b7tig", "fin\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "PIS", "PRF", "ADV", "APPRART", "NN", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Aber halt die Segel ein bey dem allerbesten Winde.", "tokens": ["A\u00b7ber", "halt", "die", "Se\u00b7gel", "ein", "bey", "dem", "al\u00b7ler\u00b7bes\u00b7ten", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}