{"dta.poem.18930": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "9.  \n  An die Fraw A. Hahnin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Botz Creutz/ wie ist (O schand/ O schmach!)", "tokens": ["Botz", "Creutz", "/", "wie", "ist", "(", "O", "schand", "/", "O", "schmach", "!", ")"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "$(", "KOKOM", "VAFIN", "$(", "NE", "VVFIN", "$(", "NE", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Heurat ein verd\u00f6cktes essen!", "tokens": ["Der", "Heu\u00b7rat", "ein", "ver\u00b7d\u00f6ck\u00b7tes", "es\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O Hertzlayd nimmer zu vergessen!", "tokens": ["O", "Hertz\u00b7layd", "nim\u00b7mer", "zu", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seuftzen die Fraw Hanin sprach:", "tokens": ["Mit", "seuft\u00b7zen", "die", "Fraw", "Ha\u00b7nin", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich hoffet/ als man jhn mir gab/", "tokens": ["Ich", "hof\u00b7fet", "/", "als", "man", "jhn", "mir", "gab", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PIS", "PPER", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df einen Hanen Ich genommen/", "tokens": ["Da\u00df", "ei\u00b7nen", "Ha\u00b7nen", "Ich", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So hab ich/ (ach weh da\u00df ich hab!)", "tokens": ["So", "hab", "ich", "/", "(", "ach", "weh", "da\u00df", "ich", "hab", "!", ")"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$(", "$(", "ITJ", "ADV", "KOUS", "PPER", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nur einen Copaunen bekommen.", "tokens": ["Nur", "ei\u00b7nen", "Co\u00b7pau\u00b7nen", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}