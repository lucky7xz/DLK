{"dta.poem.9800": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Engelland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein zustand gehet mir mehr als du denckst zu hertzen/", "tokens": ["Dein", "zu\u00b7stand", "ge\u00b7het", "mir", "mehr", "als", "du", "denckst", "zu", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "KOUS", "PPER", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Gleichwie mein steiffer bund der liebe zeugni\u00df ist;", "tokens": ["Gleich\u00b7wie", "mein", "steif\u00b7fer", "bund", "der", "lie\u00b7be", "zeug\u00b7ni\u00df", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch la\u00df auch dieses wort ietzt lindern deine schmertzen:", "tokens": ["Doch", "la\u00df", "auch", "die\u00b7ses", "wort", "ietzt", "lin\u00b7dern", "dei\u00b7ne", "schmert\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "PDAT", "NN", "ADV", "VVFIN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df dein Arminius den hafen hat erkie\u00dft.", "tokens": ["Da\u00df", "dein", "Ar\u00b7mi\u00b7nius", "den", "ha\u00b7fen", "hat", "er\u00b7kie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VAFIN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Das gl\u00fccke/ so vor ihm selbst seine seegel streichet/", "tokens": ["Das", "gl\u00fc\u00b7cke", "/", "so", "vor", "ihm", "selbst", "sei\u00b7ne", "see\u00b7gel", "strei\u00b7chet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "ADV", "APPR", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ja sein gefehrte stets geschworen hat zu seyn/", "tokens": ["Ja", "sein", "ge\u00b7fehr\u00b7te", "stets", "ge\u00b7schwo\u00b7ren", "hat", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "ADJA", "ADV", "VVPP", "VAFIN", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hat dieses theure pfand/ vom himmel \u00fcberreichet/", "tokens": ["Hat", "die\u00b7ses", "theu\u00b7re", "pfand", "/", "vom", "him\u00b7mel", "\u00fc\u00b7berr\u00b7ei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "$(", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ins k\u00f6niges palast mit lust gef\u00fchret ein.", "tokens": ["Ins", "k\u00f6\u00b7ni\u00b7ges", "pa\u00b7last", "mit", "lust", "ge\u00b7f\u00fch\u00b7ret", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich kenne sein verdienst und seine wunder-thaten/", "tokens": ["Ich", "ken\u00b7ne", "sein", "ver\u00b7dienst", "und", "sei\u00b7ne", "wun\u00b7der\u00b7tha\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die bey der ewigkeit stets aufgezeichnet steh\u2019n;", "tokens": ["Die", "bey", "der", "e\u00b7wig\u00b7keit", "stets", "auf\u00b7ge\u00b7zeich\u00b7net", "steh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich wei\u00df es/ da\u00df er nie an seinen feind gerathen/", "tokens": ["Ich", "wei\u00df", "es", "/", "da\u00df", "er", "nie", "an", "sei\u00b7nen", "feind", "ge\u00b7ra\u00b7then", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df man nicht auch den sieg von ihm erlang\u2019t geseh\u2019n.", "tokens": ["Da\u00df", "man", "nicht", "auch", "den", "sieg", "von", "ihm", "er\u00b7lang't", "ge\u00b7seh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PIS", "PTKNEG", "ADV", "ART", "NN", "APPR", "PPER", "VVFIN", "NE", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Donau hat er offt mit T\u00fcrcken-blut gef\u00e4rbet/", "tokens": ["Die", "Do\u00b7nau", "hat", "er", "offt", "mit", "T\u00fcr\u00b7cken\u00b7blut", "ge\u00b7f\u00e4r\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Constantinopel ist noch ietzt bey sich best\u00fcrtzt/", "tokens": ["Con\u00b7stan\u00b7ti\u00b7no\u00b7pel", "ist", "noch", "ietzt", "bey", "sich", "be\u00b7st\u00fcrtzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADV", "APPR", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da\u00df bey Salankemen er ihre macht verderbet/", "tokens": ["Da\u00df", "bey", "Sa\u00b7lan\u00b7ke\u00b7men", "er", "ih\u00b7re", "macht", "ver\u00b7der\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "PPOSAT", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und ihres Divans rath durch tapferkeit verk\u00fcrtzt.", "tokens": ["Und", "ih\u00b7res", "Di\u00b7vans", "rath", "durch", "tap\u00b7fer\u00b7keit", "ver\u00b7k\u00fcrtzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}