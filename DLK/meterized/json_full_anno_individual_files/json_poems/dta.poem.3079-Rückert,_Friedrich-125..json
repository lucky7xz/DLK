{"dta.poem.3079": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "125.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und wenn ihr fragt, warum wir euch kein Ganzes geben?", "tokens": ["Und", "wenn", "ihr", "fragt", ",", "wa\u00b7rum", "wir", "euch", "kein", "Gan\u00b7zes", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir geben euch es so wie wirs empfangen eben.", "tokens": ["Wir", "ge\u00b7ben", "euch", "es", "so", "wie", "wirs", "emp\u00b7fan\u00b7gen", "e\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "ADV", "KOKOM", "PIS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mir zur Erquickung gab in einzlen Augenblicken", "tokens": ["Mir", "zur", "Er\u00b7quic\u00b7kung", "gab", "in", "einz\u00b7len", "Au\u00b7gen\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es Gott, und also m\u00f6g' es einzeln euch erquicken.", "tokens": ["Es", "Gott", ",", "und", "al\u00b7so", "m\u00f6g'", "es", "ein\u00b7zeln", "euch", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "KON", "ADV", "VMFIN", "PPER", "VVFIN", "PPER", "VVINF", "$.