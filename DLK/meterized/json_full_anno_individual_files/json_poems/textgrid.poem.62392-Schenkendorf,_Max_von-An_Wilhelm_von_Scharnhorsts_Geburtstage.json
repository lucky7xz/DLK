, "line.3": {"text": "Die Leiden wie die Wonnen", "tokens": ["Die", "Lei\u00b7den", "wie", "die", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bereiten dir Gewinn.", "tokens": ["Be\u00b7rei\u00b7ten", "dir", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was alte Lieder singen,", "tokens": ["Was", "al\u00b7te", "Lie\u00b7der", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und manches liebe Bild,", "tokens": ["Und", "man\u00b7ches", "lie\u00b7be", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und was die Becher klingen,", "tokens": ["Und", "was", "die", "Be\u00b7cher", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wird Alles noch erf\u00fcllt.", "tokens": ["Wird", "Al\u00b7les", "noch", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "La\u00df uns die Blicke lenken", "tokens": ["La\u00df", "uns", "die", "Bli\u00b7cke", "len\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinauf zum Himmelsschlo\u00df,", "tokens": ["Hin\u00b7auf", "zum", "Him\u00b7mels\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Vaters la\u00df uns denken,", "tokens": ["Des", "Va\u00b7ters", "la\u00df", "uns", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der gern sein Blut vergo\u00df;", "tokens": ["Der", "gern", "sein", "Blut", "ver\u00b7go\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn weil in deinen S\u00e4ften", "tokens": ["Denn", "weil", "in", "dei\u00b7nen", "S\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Blut des Helden quillt,", "tokens": ["Das", "Blut", "des", "Hel\u00b7den", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bist du so stark in Kr\u00e4ften", "tokens": ["Bist", "du", "so", "stark", "in", "Kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und bist so fromm und mild.", "tokens": ["Und", "bist", "so", "fromm", "und", "mild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "O heil'ger, heil'ger Boden,", "tokens": ["O", "heil'\u00b7ger", ",", "heil'\u00b7ger", "Bo\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O theures Vaterland,", "tokens": ["O", "theu\u00b7res", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie selig ruhn die Todten", "tokens": ["Wie", "se\u00b7lig", "ruhn", "die", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In deinem k\u00fchlen Sand;", "tokens": ["In", "dei\u00b7nem", "k\u00fch\u00b7len", "Sand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie schallen helle Lieder", "tokens": ["Wie", "schal\u00b7len", "hel\u00b7le", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch deine Felder weit,", "tokens": ["Durch", "dei\u00b7ne", "Fel\u00b7der", "weit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie sind die wackern Br\u00fcder", "tokens": ["Wie", "sind", "die", "wa\u00b7ckern", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu k\u00fchner That bereit!", "tokens": ["Zu", "k\u00fch\u00b7ner", "That", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Den heute wir beschlie\u00dfen,", "tokens": ["Den", "heu\u00b7te", "wir", "be\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Bund soll stets gedeihn,", "tokens": ["Der", "Bund", "soll", "stets", "ge\u00b7deihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So lang die Mosel flie\u00dfen", "tokens": ["So", "lang", "die", "Mo\u00b7sel", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird in den gr\u00fcnen Rhein,", "tokens": ["Wird", "in", "den", "gr\u00fc\u00b7nen", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So lang noch Traubenh\u00fcgel", "tokens": ["So", "lang", "noch", "Trau\u00b7ben\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Hauptquartier erfreun,", "tokens": ["Ein", "Haupt\u00b7quar\u00b7tier", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und unserm Geiste Fl\u00fcgel", "tokens": ["Und", "un\u00b7serm", "Geis\u00b7te", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verleiht der edle Wein!", "tokens": ["Ver\u00b7leiht", "der", "ed\u00b7le", "Wein", "!"], "token_info": ["word", "word", "word", "w