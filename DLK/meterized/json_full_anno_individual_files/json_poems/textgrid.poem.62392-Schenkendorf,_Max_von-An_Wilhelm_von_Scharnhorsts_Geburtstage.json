{"textgrid.poem.62392": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "An Wilhelm von Scharnhorsts Geburtstage", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wir uns hier gefunden", "tokens": ["Wie", "wir", "uns", "hier", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In diesem holden Thal,", "tokens": ["In", "die\u00b7sem", "hol\u00b7den", "Thal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So bleiben wir verbunden", "tokens": ["So", "blei\u00b7ben", "wir", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In einem heil'gen Strahl.", "tokens": ["In", "ei\u00b7nem", "heil'\u00b7gen", "Strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir freuen uns der Flammen,", "tokens": ["Wir", "freu\u00b7en", "uns", "der", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die unsre Brust gen\u00e4hrt,", "tokens": ["Die", "uns\u00b7re", "Brust", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Flammen alle stammen", "tokens": ["Die", "Flam\u00b7men", "al\u00b7le", "stam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von einem gro\u00dfen Herd.", "tokens": ["Von", "ei\u00b7nem", "gro\u00b7\u00dfen", "Herd", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir gr\u00fc\u00dfen dich in Treue,", "tokens": ["Wir", "gr\u00fc\u00b7\u00dfen", "dich", "in", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du treues Heldenkind,", "tokens": ["Du", "treu\u00b7es", "Hel\u00b7den\u00b7kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und bleiben ohne Reue", "tokens": ["Und", "blei\u00b7ben", "oh\u00b7ne", "Reu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir immer wohlgesinnt.", "tokens": ["Dir", "im\u00b7mer", "wohl\u00b7ge\u00b7sinnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wolauf, mit frischem Herzen", "tokens": ["Wo\u00b7lauf", ",", "mit", "fri\u00b7schem", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PAV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zeuch fr\u00f6hlich durch die Welt;", "tokens": ["Zeuch", "fr\u00f6h\u00b7lich", "durch", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Wehmuth und die Schmerzen", "tokens": ["Die", "Weh\u00b7muth", "und", "die", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Beschleichen doch dein Zelt.", "tokens": ["Be\u00b7schlei\u00b7chen", "doch", "dein", "Zelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun hat ein Jahr begonnen,", "tokens": ["Nun", "hat", "ein", "Jahr", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es flie\u00dfe selig hin,", "tokens": ["Es", "flie\u00b7\u00dfe", "se\u00b7lig", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Leiden wie die Wonnen", "tokens": ["Die", "Lei\u00b7den", "wie", "die", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bereiten dir Gewinn.", "tokens": ["Be\u00b7rei\u00b7ten", "dir", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was alte Lieder singen,", "tokens": ["Was", "al\u00b7te", "Lie\u00b7der", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und manches liebe Bild,", "tokens": ["Und", "man\u00b7ches", "lie\u00b7be", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und was die Becher klingen,", "tokens": ["Und", "was", "die", "Be\u00b7cher", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wird Alles noch erf\u00fcllt.", "tokens": ["Wird", "Al\u00b7les", "noch", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00df uns die Blicke lenken", "tokens": ["La\u00df", "uns", "die", "Bli\u00b7cke", "len\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinauf zum Himmelsschlo\u00df,", "tokens": ["Hin\u00b7auf", "zum", "Him\u00b7mels\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Vaters la\u00df uns denken,", "tokens": ["Des", "Va\u00b7ters", "la\u00df", "uns", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der gern sein Blut vergo\u00df;", "tokens": ["Der", "gern", "sein", "Blut", "ver\u00b7go\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn weil in deinen S\u00e4ften", "tokens": ["Denn", "weil", "in", "dei\u00b7nen", "S\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Blut des Helden quillt,", "tokens": ["Das", "Blut", "des", "Hel\u00b7den", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bist du so stark in Kr\u00e4ften", "tokens": ["Bist", "du", "so", "stark", "in", "Kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und bist so fromm und mild.", "tokens": ["Und", "bist", "so", "fromm", "und", "mild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O heil'ger, heil'ger Boden,", "tokens": ["O", "heil'\u00b7ger", ",", "heil'\u00b7ger", "Bo\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O theures Vaterland,", "tokens": ["O", "theu\u00b7res", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie selig ruhn die Todten", "tokens": ["Wie", "se\u00b7lig", "ruhn", "die", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In deinem k\u00fchlen Sand;", "tokens": ["In", "dei\u00b7nem", "k\u00fch\u00b7len", "Sand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie schallen helle Lieder", "tokens": ["Wie", "schal\u00b7len", "hel\u00b7le", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch deine Felder weit,", "tokens": ["Durch", "dei\u00b7ne", "Fel\u00b7der", "weit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie sind die wackern Br\u00fcder", "tokens": ["Wie", "sind", "die", "wa\u00b7ckern", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu k\u00fchner That bereit!", "tokens": ["Zu", "k\u00fch\u00b7ner", "That", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Den heute wir beschlie\u00dfen,", "tokens": ["Den", "heu\u00b7te", "wir", "be\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Bund soll stets gedeihn,", "tokens": ["Der", "Bund", "soll", "stets", "ge\u00b7deihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So lang die Mosel flie\u00dfen", "tokens": ["So", "lang", "die", "Mo\u00b7sel", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird in den gr\u00fcnen Rhein,", "tokens": ["Wird", "in", "den", "gr\u00fc\u00b7n