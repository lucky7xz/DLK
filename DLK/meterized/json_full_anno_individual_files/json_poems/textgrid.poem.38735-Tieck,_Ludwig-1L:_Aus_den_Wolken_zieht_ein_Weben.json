{"textgrid.poem.38735": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus den Wolken zieht ein Weben", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus den Wolken zieht ein Weben", "tokens": ["Aus", "den", "Wol\u00b7ken", "zieht", "ein", "We\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Labend \u00fcber W\u00e4lder hin,", "tokens": ["La\u00b7bend", "\u00fc\u00b7ber", "W\u00e4l\u00b7der", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es f\u00fchlt der Mensch sein Leben,", "tokens": ["Und", "es", "f\u00fchlt", "der", "Mensch", "sein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Still erwacht sein innrer Sinn.", "tokens": ["Still", "er\u00b7wacht", "sein", "inn\u00b7rer", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie der Strom sich niedersenket", "tokens": ["Wie", "der", "Strom", "sich", "nie\u00b7der\u00b7sen\u00b7ket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die S\u00fc\u00dfe von sich giebt,", "tokens": ["Und", "die", "S\u00fc\u00b7\u00dfe", "von", "sich", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird die matte Brust getr\u00e4nket,", "tokens": ["Wird", "die", "mat\u00b7te", "Brust", "ge\u00b7tr\u00e4n\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie f\u00fchlet, da\u00df sie liebt.", "tokens": ["Und", "sie", "f\u00fch\u00b7let", ",", "da\u00df", "sie", "liebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Durch die weiten Sternenr\u00e4ume", "tokens": ["Durch", "die", "wei\u00b7ten", "Ster\u00b7nen\u00b7r\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dringt der liebevolle Sinn,", "tokens": ["Dringt", "der", "lie\u00b7be\u00b7vol\u00b7le", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie Engel steigen Tr\u00e4ume", "tokens": ["Und", "wie", "En\u00b7gel", "stei\u00b7gen", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der Leiter her und hin.", "tokens": ["Auf", "der", "Lei\u00b7ter", "her", "und", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ferne glimmt auf goldnen Bahnen", "tokens": ["Fer\u00b7ne", "glimmt", "auf", "gold\u00b7nen", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch ein heller Funke sch\u00f6n,", "tokens": ["Noch", "ein", "hel\u00b7ler", "Fun\u00b7ke", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein sehnsuchtvolles Ahnen", "tokens": ["Und", "ein", "sehn\u00b7sucht\u00b7vol\u00b7les", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will zur Lichtesbl\u00fcthe gehn.", "tokens": ["Will", "zur", "Lich\u00b7tes\u00b7bl\u00fct\u00b7he", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Berg und Waldung, Strom und Fluren", "tokens": ["Berg", "und", "Wal\u00b7dung", ",", "Strom", "und", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Thaun als Wolke um ihn her,", "tokens": ["Thaun", "als", "Wol\u00b7ke", "um", "ihn", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ausgel\u00f6scht sind alle Spuren,", "tokens": ["Aus\u00b7ge\u00b7l\u00f6scht", "sind", "al\u00b7le", "Spu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er vergeht im w\u00fcsten Meer.", "tokens": ["Er", "ver\u00b7geht", "im", "w\u00fcs\u00b7ten", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und kein Halt will ihm erscheinen,", "tokens": ["Und", "kein", "Halt", "will", "ihm", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles flieht und zieht so weit,", "tokens": ["Al\u00b7les", "flieht", "und", "zieht", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Quellen f\u00fchlt er weinen,", "tokens": ["Al\u00b7le", "Quel\u00b7len", "f\u00fchlt", "er", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsam steht er in der Zeit.", "tokens": ["Ein\u00b7sam", "steht", "er", "in", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach, wo bleiben meine Freuden,", "tokens": ["Ach", ",", "wo", "blei\u00b7ben", "mei\u00b7ne", "Freu\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Gespielen meiner Lust?", "tokens": ["Die", "Ge\u00b7spie\u00b7len", "mei\u00b7ner", "Lust", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollt ihr alle mich vermeiden?", "tokens": ["Wollt", "ihr", "al\u00b7le", "mich", "ver\u00b7mei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klagt er, und verschlie\u00dft die Brust.", "tokens": ["Klagt", "er", ",", "und", "ver\u00b7schlie\u00dft", "die", "Brust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Alles will um mich zerrinnen,", "tokens": ["Al\u00b7les", "will", "um", "mich", "zer\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir entfliehet die Gestalt,", "tokens": ["Mir", "ent\u00b7flie\u00b7het", "die", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steigt in meine tiefsten Sinnen", "tokens": ["Steigt", "in", "mei\u00b7ne", "tiefs\u00b7ten", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon die H\u00f6lle schwarz und kalt.", "tokens": ["Schon", "die", "H\u00f6l\u00b7le", "schwarz", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Alles, was mir freundlich bl\u00fchte,", "tokens": ["Al\u00b7les", ",", "was", "mir", "freund\u00b7lich", "bl\u00fch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist in sich zur\u00fcck geflohn,", "tokens": ["Ist", "in", "sich", "zu\u00b7r\u00fcck", "ge\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PRF", "PTKVZ", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was mein Busen kindlich gl\u00fchte;", "tokens": ["Was", "mein", "Bu\u00b7sen", "kind\u00b7lich", "gl\u00fch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ausgel\u00f6scht ist jeder Ton.", "tokens": ["Aus\u00b7ge\u00b7l\u00f6scht", "ist", "je\u00b7der", "Ton", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wo ich ehemals gel\u00fcstend", "tokens": ["Wo", "ich", "e\u00b7he\u00b7mals", "ge\u00b7l\u00fcs\u00b7tend"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ordnung und die Liebe sah,", "tokens": ["Ord\u00b7nung", "und", "die", "Lie\u00b7be", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steht das Chaos jetzt verw\u00fcstend", "tokens": ["Steht", "das", "Chaos", "jetzt", "ver\u00b7w\u00fcs\u00b7tend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Meiner bangen Seele nah.", "tokens": ["Mei\u00b7ner", "ban\u00b7gen", "See\u00b7le", "nah", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Fern und ferner zieht das Sehnen", "tokens": ["Fern", "und", "fer\u00b7ner", "zieht", "das", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der entflohnen Liebe nach,", "tokens": ["Der", "ent\u00b7floh\u00b7nen", "Lie\u00b7be", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kaum geblieben sind die Thr\u00e4nen,", "tokens": ["Kaum", "ge\u00b7blie\u00b7ben", "sind", "die", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch des Busens tiefes Ach! \u2013", "tokens": ["Noch", "des", "Bu\u00b7sens", "tie\u00b7fes", "Ach", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und er wend't sich mit den Blicken", "tokens": ["Und", "er", "wend't", "sich", "mit", "den", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die schwarze Wolkennacht,", "tokens": ["In", "die", "schwar\u00b7ze", "Wol\u00b7ken\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Finstre wird ein Z\u00fccken", "tokens": ["In", "der", "Finst\u00b7re", "wird", "ein", "Z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Blitzen angefacht.", "tokens": ["Wie", "ein", "Blit\u00b7zen", "an\u00b7ge\u00b7facht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und aus den Gew\u00e4ndern dunkel,", "tokens": ["Und", "aus", "den", "Ge\u00b7w\u00e4n\u00b7dern", "dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aus den Wolken, Berg und Wald,", "tokens": ["Aus", "den", "Wol\u00b7ken", ",", "Berg", "und", "Wald", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaut mit heimlichem Gefunkel", "tokens": ["Schaut", "mit", "heim\u00b7li\u00b7chem", "Ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu ihm her ein Auge bald.", "tokens": ["Zu", "ihm", "her", "ein", "Au\u00b7ge", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Und sein Herze wird ein Blicken", "tokens": ["Und", "sein", "Her\u00b7ze", "wird", "ein", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Auges ew'ges Bild,", "tokens": ["In", "des", "Au\u00b7ges", "ew'\u00b7ges", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichts kann ihm den Wink entr\u00fccken,", "tokens": ["Nichts", "kann", "ihm", "den", "Wink", "ent\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Alle Sehnsucht ist gestillt.", "tokens": ["Al\u00b7le", "Sehn\u00b7sucht", "ist", "ge\u00b7stillt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Nun ist ihm die Welt entschwunden,", "tokens": ["Nun", "ist", "ihm", "die", "Welt", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ewig blickt das Auge s\u00fc\u00df,", "tokens": ["E\u00b7wig", "blickt", "das", "Au\u00b7ge", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dessen Locken er empfunden,", "tokens": ["Des\u00b7sen", "Lo\u00b7cken", "er", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sein Herz ist ihm gewi\u00df.", "tokens": ["Und", "sein", "Herz", "ist", "ihm", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Dieser fragt nach keinen K\u00fcnsten,", "tokens": ["Die\u00b7ser", "fragt", "nach", "kei\u00b7nen", "K\u00fcns\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihm Welt und Zeit verhei\u00dft,", "tokens": ["Die", "ihm", "Welt", "und", "Zeit", "ver\u00b7hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er verschmacht't in Liebesbr\u00fcnsten,", "tokens": ["Er", "ver\u00b7schmacht't", "in", "Lie\u00b7bes\u00b7br\u00fcns\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in Gott entfleu\u00dft der Geist.", "tokens": ["Und", "in", "Gott", "ent\u00b7fleu\u00dft", "der", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Aus den Wolken zieht ein Weben", "tokens": ["Aus", "den", "Wol\u00b7ken", "zieht", "ein", "We\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Labend \u00fcber W\u00e4lder hin,", "tokens": ["La\u00b7bend", "\u00fc\u00b7ber", "W\u00e4l\u00b7der", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es f\u00fchlt der Mensch sein Leben,", "tokens": ["Und", "es", "f\u00fchlt", "der", "Mensch", "sein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Still erwacht sein innrer Sinn.", "tokens": ["Still", "er\u00b7wacht", "sein", "inn\u00b7rer", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Wie der Strom sich niedersenket", "tokens": ["Wie", "der", "Strom", "sich", "nie\u00b7der\u00b7sen\u00b7ket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die S\u00fc\u00dfe von sich giebt,", "tokens": ["Und", "die", "S\u00fc\u00b7\u00dfe", "von", "sich", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird die matte Brust getr\u00e4nket,", "tokens": ["Wird", "die", "mat\u00b7te", "Brust", "ge\u00b7tr\u00e4n\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie f\u00fchlet, da\u00df sie liebt.", "tokens": ["Und", "sie", "f\u00fch\u00b7let", ",", "da\u00df", "sie", "liebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Durch die weiten Sternenr\u00e4ume", "tokens": ["Durch", "die", "wei\u00b7ten", "Ster\u00b7nen\u00b7r\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dringt der liebevolle Sinn,", "tokens": ["Dringt", "der", "lie\u00b7be\u00b7vol\u00b7le", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie Engel steigen Tr\u00e4ume", "tokens": ["Und", "wie", "En\u00b7gel", "stei\u00b7gen", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der Leiter her und hin.", "tokens": ["Auf", "der", "Lei\u00b7ter", "her", "und", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Ferne glimmt auf goldnen Bahnen", "tokens": ["Fer\u00b7ne", "glimmt", "auf", "gold\u00b7nen", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch ein heller Funke sch\u00f6n,", "tokens": ["Noch", "ein", "hel\u00b7ler", "Fun\u00b7ke", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein sehnsuchtvolles Ahnen", "tokens": ["Und", "ein", "sehn\u00b7sucht\u00b7vol\u00b7les", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will zur Lichtesbl\u00fcthe gehn.", "tokens": ["Will", "zur", "Lich\u00b7tes\u00b7bl\u00fct\u00b7he", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Berg und Waldung, Strom und Fluren", "tokens": ["Berg", "und", "Wal\u00b7dung", ",", "Strom", "und", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Thaun als Wolke um ihn her,", "tokens": ["Thaun", "als", "Wol\u00b7ke", "um", "ihn", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ausgel\u00f6scht sind alle Spuren,", "tokens": ["Aus\u00b7ge\u00b7l\u00f6scht", "sind", "al\u00b7le", "Spu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er vergeht im w\u00fcsten Meer.", "tokens": ["Er", "ver\u00b7geht", "im", "w\u00fcs\u00b7ten", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Und kein Halt will ihm erscheinen,", "tokens": ["Und", "kein", "Halt", "will", "ihm", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles flieht und zieht so weit,", "tokens": ["Al\u00b7les", "flieht", "und", "zieht", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Quellen f\u00fchlt er weinen,", "tokens": ["Al\u00b7le", "Quel\u00b7len", "f\u00fchlt", "er", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsam steht er in der Zeit.", "tokens": ["Ein\u00b7sam", "steht", "er", "in", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Ach, wo bleiben meine Freuden,", "tokens": ["Ach", ",", "wo", "blei\u00b7ben", "mei\u00b7ne", "Freu\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Gespielen meiner Lust?", "tokens": ["Die", "Ge\u00b7spie\u00b7len", "mei\u00b7ner", "Lust", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollt ihr alle mich vermeiden?", "tokens": ["Wollt", "ihr", "al\u00b7le", "mich", "ver\u00b7mei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klagt er, und verschlie\u00dft die Brust.", "tokens": ["Klagt", "er", ",", "und", "ver\u00b7schlie\u00dft", "die", "Brust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "Alles will um mich zerrinnen,", "tokens": ["Al\u00b7les", "will", "um", "mich", "zer\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir entfliehet die Gestalt,", "tokens": ["Mir", "ent\u00b7flie\u00b7het", "die", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steigt in meine tiefsten Sinnen", "tokens": ["Steigt", "in", "mei\u00b7ne", "tiefs\u00b7ten", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon die H\u00f6lle schwarz und kalt.", "tokens": ["Schon", "die", "H\u00f6l\u00b7le", "schwarz", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "Alles, was mir freundlich bl\u00fchte,", "tokens": ["Al\u00b7les", ",", "was", "mir", "freund\u00b7lich", "bl\u00fch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist in sich zur\u00fcck geflohn,", "tokens": ["Ist", "in", "sich", "zu\u00b7r\u00fcck", "ge\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PRF", "PTKVZ", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was mein Busen kindlich gl\u00fchte;", "tokens": ["Was", "mein", "Bu\u00b7sen", "kind\u00b7lich", "gl\u00fch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ausgel\u00f6scht ist jeder Ton.", "tokens": ["Aus\u00b7ge\u00b7l\u00f6scht", "ist", "je\u00b7der", "Ton", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Wo ich ehemals gel\u00fcstend", "tokens": ["Wo", "ich", "e\u00b7he\u00b7mals", "ge\u00b7l\u00fcs\u00b7tend"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ordnung und die Liebe sah,", "tokens": ["Ord\u00b7nung", "und", "die", "Lie\u00b7be", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steht das Chaos jetzt verw\u00fcstend", "tokens": ["Steht", "das", "Chaos", "jetzt", "ver\u00b7w\u00fcs\u00b7tend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Meiner bangen Seele nah.", "tokens": ["Mei\u00b7ner", "ban\u00b7gen", "See\u00b7le", "nah", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.27": {"line.1": {"text": "Fern und ferner zieht das Sehnen", "tokens": ["Fern", "und", "fer\u00b7ner", "zieht", "das", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der entflohnen Liebe nach,", "tokens": ["Der", "ent\u00b7floh\u00b7nen", "Lie\u00b7be", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kaum geblieben sind die Thr\u00e4nen,", "tokens": ["Kaum", "ge\u00b7blie\u00b7ben", "sind", "die", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch des Busens tiefes Ach! \u2013", "tokens": ["Noch", "des", "Bu\u00b7sens", "tie\u00b7fes", "Ach", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.28": {"line.1": {"text": "Und er wend't sich mit den Blicken", "tokens": ["Und", "er", "wend't", "sich", "mit", "den", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die schwarze Wolkennacht,", "tokens": ["In", "die", "schwar\u00b7ze", "Wol\u00b7ken\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Finstre wird ein Z\u00fccken", "tokens": ["In", "der", "Finst\u00b7re", "wird", "ein", "Z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Blitzen angefacht.", "tokens": ["Wie", "ein", "Blit\u00b7zen", "an\u00b7ge\u00b7facht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.29": {"line.1": {"text": "Und aus den Gew\u00e4ndern dunkel,", "tokens": ["Und", "aus", "den", "Ge\u00b7w\u00e4n\u00b7dern", "dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aus den Wolken, Berg und Wald,", "tokens": ["Aus", "den", "Wol\u00b7ken", ",", "Berg", "und", "Wald", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaut mit heimlichem Gefunkel", "tokens": ["Schaut", "mit", "heim\u00b7li\u00b7chem", "Ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu ihm her ein Auge bald.", "tokens": ["Zu", "ihm", "her", "ein", "Au\u00b7ge", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.30": {"line.1": {"text": "Und sein Herze wird ein Blicken", "tokens": ["Und", "sein", "Her\u00b7ze", "wird", "ein", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Auges ew'ges Bild,", "tokens": ["In", "des", "Au\u00b7ges", "ew'\u00b7ges", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichts kann ihm den Wink entr\u00fccken,", "tokens": ["Nichts", "kann", "ihm", "den", "Wink", "ent\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Alle Sehnsucht ist gestillt.", "tokens": ["Al\u00b7le", "Sehn\u00b7sucht", "ist", "ge\u00b7stillt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.31": {"line.1": {"text": "Nun ist ihm die Welt entschwunden,", "tokens": ["Nun", "ist", "ihm", "die", "Welt", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ewig blickt das Auge s\u00fc\u00df,", "tokens": ["E\u00b7wig", "blickt", "das", "Au\u00b7ge", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dessen Locken er empfunden,", "tokens": ["Des\u00b7sen", "Lo\u00b7cken", "er", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sein Herz ist ihm gewi\u00df.", "tokens": ["Und", "sein", "Herz", "ist", "ihm", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.32": {"line.1": {"text": "Dieser fragt nach keinen K\u00fcnsten,", "tokens": ["Die\u00b7ser", "fragt", "nach", "kei\u00b7nen", "K\u00fcns\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihm Welt und Zeit verhei\u00dft,", "tokens": ["Die", "ihm", "Welt", "und", "Zeit", "ver\u00b7hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er verschmacht't in Liebesbr\u00fcnsten,", "tokens": ["Er", "ver\u00b7schmacht't", "in", "Lie\u00b7bes\u00b7br\u00fcns\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in Gott entfleu\u00dft der Geist.", "tokens": ["Und", "in", "Gott", "ent\u00b7fleu\u00dft", "der", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}