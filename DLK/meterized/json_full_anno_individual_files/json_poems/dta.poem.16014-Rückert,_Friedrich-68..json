{"dta.poem.16014": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei Sonnenstrale, von der Sonne ausgegangen,", "tokens": ["Zwei", "Son\u00b7nen\u00b7stra\u00b7le", ",", "von", "der", "Son\u00b7ne", "aus\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verga\u00dfen unterwegs, vonwannen sie entsprangen.", "tokens": ["Ver\u00b7ga\u00b7\u00dfen", "un\u00b7ter\u00b7wegs", ",", "von\u00b7wan\u00b7nen", "sie", "ent\u00b7spran\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und h\u00e4tten sie es nicht vergessen, w\u00e4ren sie", "tokens": ["Und", "h\u00e4t\u00b7ten", "sie", "es", "nicht", "ver\u00b7ges\u00b7sen", ",", "w\u00e4\u00b7ren", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPER", "PTKNEG", "VVPP", "$,", "VAFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur Sonne heimgekehrt, gelangt zur Erde nie.", "tokens": ["Zur", "Son\u00b7ne", "heim\u00b7ge\u00b7kehrt", ",", "ge\u00b7langt", "zur", "Er\u00b7de", "nie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "VVPP", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zur Welt gelangten sie, und wirkten da gesch\u00e4ftig,", "tokens": ["Zur", "Welt", "ge\u00b7lang\u00b7ten", "sie", ",", "und", "wirk\u00b7ten", "da", "ge\u00b7sch\u00e4f\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonnenvergessen zwar, wirkten sie sonnenkr\u00e4ftig.", "tokens": ["Son\u00b7nen\u00b7ver\u00b7ges\u00b7sen", "zwar", ",", "wirk\u00b7ten", "sie", "son\u00b7nen\u00b7kr\u00e4f\u00b7tig", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}, "stanza.4": {"line.1": {"text": "Da kamen sie sich nah in ihrem Wirkungskreise;", "tokens": ["Da", "ka\u00b7men", "sie", "sich", "nah", "in", "ih\u00b7rem", "Wir\u00b7kungs\u00b7krei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer bist du und woher? befragten sie sich leise.", "tokens": ["Wer", "bist", "du", "und", "wo\u00b7her", "?", "be\u00b7frag\u00b7ten", "sie", "sich", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "KON", "PWAV", "$.", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich wei\u00df es nicht, allein du scheinst ein Fremdling mir;", "tokens": ["Ich", "wei\u00df", "es", "nicht", ",", "al\u00b7lein", "du", "scheinst", "ein", "Fremd\u00b7ling", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "PPER", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So bin ich einer auch, ich f\u00fchls, ich gleiche dir.", "tokens": ["So", "bin", "ich", "ei\u00b7ner", "auch", ",", "ich", "f\u00fchls", ",", "ich", "glei\u00b7che", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADV", "$,", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und sind wir Fremdlinge, wo ist die Heimat nun?", "tokens": ["Und", "sind", "wir", "Fremd\u00b7lin\u00b7ge", ",", "wo", "ist", "die", "Hei\u00b7mat", "nun", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "$,", "PWAV", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dahin zusammen la\u00df uns doch die Reise thun. \u2014", "tokens": ["Da\u00b7hin", "zu\u00b7sam\u00b7men", "la\u00df", "uns", "doch", "die", "Rei\u00b7se", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "ADV", "VVIMP", "PPER", "ADV", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der Sonn' Erinnrung gieng in beiden Stralen auf,", "tokens": ["Der", "Sonn'", "E\u00b7rinn\u00b7rung", "gieng", "in", "bei\u00b7den", "Stra\u00b7len", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und freudig Hand in Hand nahmen sie heim den Lauf,", "tokens": ["Und", "freu\u00b7dig", "Hand", "in", "Hand", "nah\u00b7men", "sie", "heim", "den", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "APPR", "NN", "VVFIN", "PPER", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}}, "stanza.8": {"line.1": {"text": "Sich denkend unterwegs, da\u00df jeder das gefunden", "tokens": ["Sich", "den\u00b7kend", "un\u00b7ter\u00b7wegs", ",", "da\u00df", "je\u00b7der", "das", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "ADV", "$,", "KOUS", "PIS", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Blick des andern, was ihm selber war geschwunden.", "tokens": ["Im", "Blick", "des", "an\u00b7dern", ",", "was", "ihm", "sel\u00b7ber", "war", "ge\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "$,", "PWS", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Wie sollten sie vereint zur Sonne nicht gelangen,", "tokens": ["Wie", "soll\u00b7ten", "sie", "ver\u00b7eint", "zur", "Son\u00b7ne", "nicht", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVPP", "APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die hier dem einen schon im andern aufgegangen?", "tokens": ["Die", "hier", "dem", "ei\u00b7nen", "schon", "im", "an\u00b7dern", "auf\u00b7ge\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ART", "ADV", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}