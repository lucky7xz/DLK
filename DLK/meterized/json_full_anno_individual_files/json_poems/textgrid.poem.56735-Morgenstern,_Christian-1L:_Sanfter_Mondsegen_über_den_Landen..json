{"textgrid.poem.56735": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sanfter Mondsegen \u00fcber den Landen.", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sanfter Mondsegen \u00fcber den Landen.", "tokens": ["Sanf\u00b7ter", "Mond\u00b7se\u00b7gen", "\u00fc\u00b7ber", "den", "Lan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Schlafstumm Berge, W\u00e4lder, Tale.", "tokens": ["Schlaf\u00b7stumm", "Ber\u00b7ge", ",", "W\u00e4l\u00b7der", ",", "Ta\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "NN", "$,", "NN", "$,", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den H\u00fctten erstorben die Herde;", "tokens": ["In", "den", "H\u00fct\u00b7ten", "ers\u00b7tor\u00b7ben", "die", "Her\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "an den Herden eingenickte Gro\u00dfm\u00fctter,", "tokens": ["an", "den", "Her\u00b7den", "ein\u00b7ge\u00b7nick\u00b7te", "Gro\u00df\u00b7m\u00fct\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "zu deren Knieen offne Enkel-M\u00e4ulerchen", "tokens": ["zu", "de\u00b7ren", "Kni\u00b7e\u00b7en", "off\u00b7ne", "En\u00b7kel\u00b7M\u00e4u\u00b7ler\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ADJA", "NN"], "meter": "----+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.6": {"text": "unter verh\u00e4ngten \u00c4uglein atmen.", "tokens": ["un\u00b7ter", "ver\u00b7h\u00e4ng\u00b7ten", "\u00c4ug\u00b7lein", "at\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Auf Daunen und Strohsack", "tokens": ["Auf", "Dau\u00b7nen", "und", "Stroh\u00b7sack"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "schnarchendes Laster, schnarchende Tugend.", "tokens": ["schnar\u00b7chen\u00b7des", "Las\u00b7ter", ",", "schnar\u00b7chen\u00b7de", "Tu\u00b7gend", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Wachend allein: Diebe, Dichter,", "tokens": ["Wa\u00b7chend", "al\u00b7lein", ":", "Die\u00b7be", ",", "Dich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "$.", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "W\u00e4chter der Nacht, und auf Gassen, in G\u00e4rten", "tokens": ["W\u00e4ch\u00b7ter", "der", "Nacht", ",", "und", "auf", "Gas\u00b7sen", ",", "in", "G\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "KON", "APPR", "NN", "$,", "APPR", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.11": {"text": "und in verschwiegenen Kammern", "tokens": ["und", "in", "ver\u00b7schwie\u00b7ge\u00b7nen", "Kam\u00b7mern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "lispelnde Liebe.", "tokens": ["lis\u00b7peln\u00b7de", "Lie\u00b7be", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Sanfter Mond! du ", "tokens": ["Sanf\u00b7ter", "Mond", "!", "du"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$.", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "weil du nichts andres kannst.", "tokens": ["weil", "du", "nichts", "and\u00b7res", "kannst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PIS", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aber am Herzen", "tokens": ["A\u00b7ber", "am", "Her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "zehren dir Neid und Groll,", "tokens": ["zeh\u00b7ren", "dir", "Neid", "und", "Groll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "weil die Menschen dich also mi\u00dfachten,", "tokens": ["weil", "die", "Men\u00b7schen", "dich", "al\u00b7so", "mi\u00df\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-++--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "da\u00df sie zu Bett gehn, wenn du kommst.", "tokens": ["da\u00df", "sie", "zu", "Bett", "gehn", ",", "wenn", "du", "kommst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00c4rgerlich ziehn sie die Vorh\u00e4nge zu:", "tokens": ["\u00c4r\u00b7ger\u00b7lich", "ziehn", "sie", "die", "Vor\u00b7h\u00e4n\u00b7ge", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "und du stehst drau\u00dfen", "tokens": ["und", "du", "stehst", "drau\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "und \u2013 segnest milde deine Ver\u00e4chter.", "tokens": ["und", "\u2013", "seg\u00b7nest", "mil\u00b7de", "dei\u00b7ne", "Ver\u00b7\u00e4ch\u00b7ter", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Sanfter Mond! manchmal auch", "tokens": ["Sanf\u00b7ter", "Mond", "!", "manch\u00b7mal", "auch"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "ADV", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "lugen Herrschergel\u00fcste gef\u00e4hrlich vor", "tokens": ["lu\u00b7gen", "Herr\u00b7scher\u00b7ge\u00b7l\u00fcs\u00b7te", "ge\u00b7f\u00e4hr\u00b7lich", "vor"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJD", "PTKVZ"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "unter deiner Demut.", "tokens": ["un\u00b7ter", "dei\u00b7ner", "De\u00b7mut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Dann rufst du in vertr\u00e4umte Gehirne;", "tokens": ["Dann", "rufst", "du", "in", "ver\u00b7tr\u00e4um\u00b7te", "Ge\u00b7hir\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbauf! auf!", "tokens": ["\u00bb", "auf", "!", "auf", "!"], "token_info": ["punct", "word", "punct", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "PTKVZ", "$."], "meter": "++", "measure": "spondeus"}, "line.3": {"text": "Ich bin die Sonne!", "tokens": ["Ich", "bin", "die", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Kommt: es ist Tag!\u00ab", "tokens": ["Kommt", ":", "es", "ist", "Tag", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$.", "PPER", "VAFIN", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und der bl\u00f6den Schl\u00e4fer", "tokens": ["Und", "der", "bl\u00f6\u00b7den", "Schl\u00e4\u00b7fer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "glaubt es dir mancher", "tokens": ["glaubt", "es", "dir", "man\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PIAT"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "und steigt ernsthaft", "tokens": ["und", "steigt", "ernst\u00b7haft"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "aus seinen Kissen", "tokens": ["aus", "sei\u00b7nen", "Kis\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "und geht gravit\u00e4tisch", "tokens": ["und", "geht", "gra\u00b7vi\u00b7t\u00e4\u00b7tisch"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "\u00fcber die D\u00e4cher.", "tokens": ["\u00fc\u00b7ber", "die", "D\u00e4\u00b7cher", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Scheel sehen die Kater ihn an.", "tokens": ["Scheel", "se\u00b7hen", "die", "Ka\u00b7ter", "ihn", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Er aber wandelt und klettert,", "tokens": ["Er", "a\u00b7ber", "wan\u00b7delt", "und", "klet\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "als h\u00e4tt ihm sein Arzt", "tokens": ["als", "h\u00e4tt", "ihm", "sein", "Arzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOKOM", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "die Alpen verschrieben.", "tokens": ["die", "Al\u00b7pen", "ver\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Wie? Freundchen!", "tokens": ["Wie", "?", "Freund\u00b7chen", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["PWAV", "$.", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "H\u00e4tt ich dich heut gar ertappt?", "tokens": ["H\u00e4tt", "ich", "dich", "heut", "gar", "er\u00b7tappt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir d\u00fcnkt, da unten", "tokens": ["Mir", "d\u00fcnkt", ",", "da", "un\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "k\u00e4m solch ein Wandler!", "tokens": ["k\u00e4m", "solch", "ein", "Wand\u00b7ler", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Armer Fremdling,", "tokens": ["Ar\u00b7mer", "Fremd\u00b7ling", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "\u2013 besser: Hemdling \u2013,", "tokens": ["\u2013", "bes\u00b7ser", ":", "Hemd\u00b7ling", "\u2013", ","], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$.", "NE", "$(", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "wer bist du?", "tokens": ["wer", "bist", "du", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Welchem Bette entflohst du?", "tokens": ["Wel\u00b7chem", "Bet\u00b7te", "ent\u00b7flohst", "du", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Opferlamm", "tokens": ["Op\u00b7fer\u00b7lamm"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "mondlicher L\u00fcsternheit,", "tokens": ["mond\u00b7li\u00b7cher", "L\u00fcs\u00b7tern\u00b7heit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "meilenweit mu\u00dft du gewandert sein!", "tokens": 