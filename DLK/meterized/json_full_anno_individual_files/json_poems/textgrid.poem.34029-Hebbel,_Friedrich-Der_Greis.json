{"textgrid.poem.34029": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Greis", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bin ich wieder genesen, und glaubte, sicher zu sterben?", "tokens": ["Bin", "ich", "wie\u00b7der", "ge\u00b7ne\u00b7sen", ",", "und", "glaub\u00b7te", ",", "si\u00b7cher", "zu", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,", "KON", "VVFIN", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Dank dir, g\u00fctiger Tod, da\u00df du ein Umseh'n mir noch", "tokens": ["Dank", "dir", ",", "g\u00fc\u00b7ti\u00b7ger", "Tod", ",", "da\u00df", "du", "ein", "Um\u00b7seh'n", "mir", "noch"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "ADV"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wolltest verg\u00f6nnen, ein letztes! Zu lange werd' ich nicht z\u00f6gern!", "tokens": ["Woll\u00b7test", "ver\u00b7g\u00f6n\u00b7nen", ",", "ein", "letz\u00b7tes", "!", "Zu", "lan\u00b7ge", "werd'", "ich", "nicht", "z\u00f6\u00b7gern", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ART", "ADJA", "$.", "APPR", "ADV", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Einen einzigen Blick! Erde, wie bist du so sch\u00f6n!", "tokens": ["Ei\u00b7nen", "ein\u00b7zi\u00b7gen", "Blick", "!", "Er\u00b7de", ",", "wie", "bist", "du", 