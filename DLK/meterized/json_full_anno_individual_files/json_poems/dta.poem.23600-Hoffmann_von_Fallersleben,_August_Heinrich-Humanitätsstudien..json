{"dta.poem.23600": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Humanit\u00e4tsstudien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dies Geschlecht, das in Vokabeln", "tokens": ["Dies", "Ge\u00b7schlecht", ",", "das", "in", "Vo\u00b7ka\u00b7beln"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-++--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Wie der Ochs' im Joche zieht,", "tokens": ["Wie", "der", "Ochs'", "im", "Jo\u00b7che", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das vor grauen G\u00f6tterfabeln", "tokens": ["Das", "vor", "grau\u00b7en", "G\u00f6t\u00b7ter\u00b7fa\u00b7beln"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine Gegenwart mehr sieht \u2014", "tokens": ["Kei\u00b7ne", "Ge\u00b7gen\u00b7wart", "mehr", "sieht"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dies Geschlecht, es schien geboren", "tokens": ["Dies", "Ge\u00b7schlecht", ",", "es", "schien", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "NN", "$,", "PPER", "VVFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur in Rom und in Athen,", "tokens": ["Nur", "in", "Rom", "und", "in", "A\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "KON", "APPR", "NE", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und wie Deutschland ging verloren,", "tokens": ["Und", "wie", "Deutschland", "ging", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "VVFIN", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Lie\u00dfen sie es gern geschehn.", "tokens": ["Lie\u00b7\u00dfen", "sie", "es", "gern", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn nur G\u00f6tterruh und Frieden", "tokens": ["Wenn", "nur", "G\u00f6t\u00b7ter\u00b7ruh", "und", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre matte Seele fand,", "tokens": ["Ih\u00b7re", "mat\u00b7te", "See\u00b7le", "fand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun, das war f\u00fcr sie hienieden", "tokens": ["Nun", ",", "das", "war", "f\u00fcr", "sie", "hien\u00b7ie\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PDS", "VAFIN", "APPR", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mehr als je ein Vaterland.", "tokens": ["Mehr", "als", "je", "ein", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wirbst auch ", "tokens": ["Wirbst", "auch"], "token_info": ["word", "word"], "pos": ["VAFIN", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "In der todten Wissenschaft?", "tokens": ["In", "der", "tod\u00b7ten", "Wis\u00b7sen\u00b7schaft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weihst auch ", "tokens": ["Weihst", "auch"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Deines Lebens F\u00fcll' und Kraft?", "tokens": ["Dei\u00b7nes", "Le\u00b7bens", "F\u00fcll'", "und", "Kraft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Deutsche Jugend, du von heute,", "tokens": ["Deut\u00b7sche", "Ju\u00b7gend", ",", "du", "von", "heu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Voll von Griechisch und Latein,", "tokens": ["Voll", "von", "Grie\u00b7chisch", "und", "La\u00b7tein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Wirst du ", "tokens": ["Wirst", "du"], "token_info": ["word", "word"], "pos": ["VAFIN", "PPER"], "meter": "-+", "measure": "iambic.single"}}, "stanza.6": {"line.1": {"text": "Ein Geschlecht, das in Vokabeln", "tokens": ["Ein", "Ge\u00b7schlecht", ",", "das", "in", "Vo\u00b7ka\u00b7beln"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-++--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Wie der Ochs' im Joche zieht,", 