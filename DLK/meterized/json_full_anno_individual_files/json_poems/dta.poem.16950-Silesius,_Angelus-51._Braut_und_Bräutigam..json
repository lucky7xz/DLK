{"dta.poem.16950": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "51.  Braut und Br\u00e4utigam.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Br\u00e4utgam seyn ist viel: noch mehr der Braut geniss\u1ebd/", "tokens": ["Ein", "Br\u00e4ut\u00b7gam", "seyn", "ist", "viel", ":", "noch", "mehr", "der", "Braut", "ge\u00b7ni\u00b7ss\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAINF", "VAFIN", "ADV", "$.", "ADV", "ADV", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Und jhren s\u00fcssen Mund mit Hertzer-Liebe k\u00fcssen:", "tokens": ["Und", "jhren", "s\u00fcs\u00b7sen", "Mund", "mit", "Hert\u00b7zer\u00b7Lie\u00b7be", "k\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Ich aber liebe mehr die Hochzeit/ da ich Braut", "tokens": ["Ich", "a\u00b7ber", "lie\u00b7be", "mehr", "die", "Hoch\u00b7zeit", "/", "da", "ich", "Braut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "NN", "$(", "KOUS", "PPER", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gott meinem Br\u00e4utigam werd\u2019 innig eingetraut.", "tokens": ["Gott", "mei\u00b7nem", "Br\u00e4u\u00b7ti\u00b7gam", "werd'", "in\u00b7nig", "ein\u00b7ge\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "