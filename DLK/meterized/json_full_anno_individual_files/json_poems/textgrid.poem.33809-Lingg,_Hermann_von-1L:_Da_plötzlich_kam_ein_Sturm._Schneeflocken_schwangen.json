{"textgrid.poem.33809": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da pl\u00f6tzlich kam ein Sturm. Schneeflocken schwangen", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da pl\u00f6tzlich kam ein Sturm. Schneeflocken schwangen", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "kam", "ein", "Sturm", ".", "Schnee\u00b7flo\u00b7cken", "schwan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$.", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In unsre Bl\u00fcten sich; mit kalter Schneide", "tokens": ["In", "uns\u00b7re", "Bl\u00fc\u00b7ten", "sich", ";", "mit", "kal\u00b7ter", "Schnei\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zerri\u00df ein Eisstrom unser Krongeschmeide,", "tokens": ["Zer\u00b7ri\u00df", "ein", "Eis\u00b7strom", "un\u00b7ser", "Kron\u00b7ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und unser Jugendtraum, er war vergangen!", "tokens": ["Und", "un\u00b7ser", "Ju\u00b7gend\u00b7traum", ",", "er", "war", "ver\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Seit jener Nacht sind bleich der Lilie Wangen;", "tokens": ["Seit", "je\u00b7ner", "Nacht", "sind", "bleich", "der", "Li\u00