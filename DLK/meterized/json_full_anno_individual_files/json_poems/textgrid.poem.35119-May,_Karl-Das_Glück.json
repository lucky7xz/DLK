{"textgrid.poem.35119": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Das Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sagst, du k\u00f6nnest nicht fassen,", "tokens": ["Du", "sagst", ",", "du", "k\u00f6n\u00b7nest", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Was du zu fassen hast.", "tokens": ["Was", "du", "zu", "fas\u00b7sen", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du brauchsts nur wirken zu lassen,", "tokens": ["Du", "brauchsts", "nur", "wir\u00b7ken", "zu", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So hast du es gefa\u00dft.", "tokens": ["So", "hast", "du", "es", "ge\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es kommt genau wie die Sonne;", "tokens": ["Es", "kommt", "ge\u00b7nau", "wie", "die", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auch sie ergreifst du nicht", "tokens": ["Auch", "sie", "er\u00b7greifst", "du", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gr\u00fc\u00dfest sie doch mit Wonne", "tokens": ["Und", "gr\u00fc\u00b7\u00dfest", "sie", "doch", "mit", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und lebst in ihrem Licht.", "tokens": ["Und", "lebst", "in", "ih\u00b7rem", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nur darfst du dich nicht entziehen", "tokens": ["Nur", "darfst", "du", "dich", "nicht", "ent\u00b7zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dem oft verkannten Gl\u00fcck.", "tokens": ["Dem", "oft", "ver\u00b7kann\u00b7ten", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer eilig ist, es zu fliehen,", "tokens": ["Wer", "ei\u00b7lig", "ist", ",", "es", "zu", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dem kehrts wohl kaum zur\u00fcck.", "tokens": ["Dem", "kehrts", "wohl", "kaum", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du sagst, du k\u00f6n