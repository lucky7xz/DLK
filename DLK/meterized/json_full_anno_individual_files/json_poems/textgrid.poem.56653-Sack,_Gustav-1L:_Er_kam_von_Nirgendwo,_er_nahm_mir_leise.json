{"textgrid.poem.56653": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er kam von Nirgendwo, er nahm mir leise", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er kam von Nirgendwo, er nahm mir leise", "tokens": ["Er", "kam", "von", "Nir\u00b7gend\u00b7wo", ",", "er", "nahm", "mir", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$,", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der Dinge Meterma\u00df und Stundenglas", "tokens": ["der", "Din\u00b7ge", "Me\u00b7ter\u00b7ma\u00df", "und", "Stun\u00b7den\u00b7glas"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und gab mir, was ich lange schon verga\u00df,", "tokens": ["und", "gab", "mir", ",", "was", "ich", "lan\u00b7ge", "schon", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "zur\u00fcck in wundersam verzerrter Weise:", "tokens": ["zu\u00b7r\u00fcck", "in", "wun\u00b7der\u00b7sam", "ver\u00b7zerr\u00b7ter", "Wei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was einst ich stammelnd schrieb zu deinem Preise,", "tokens": ["Was", "einst", "ich", "stam\u00b7melnd", "schrieb", "zu", "dei\u00b7nem", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "w