{"textgrid.poem.31616": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "Uber das kleine wolbekandte Bl\u00fcmlein: Vergi\u00df mein nicht", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nes Bl\u00fcmlein! deine Farbe / zeigt des H\u00f6chsten Hoheit an /", "tokens": ["Sch\u00f6\u00b7nes", "Bl\u00fcm\u00b7lein", "!", "dei\u00b7ne", "Far\u00b7be", "/", "zeigt", "des", "H\u00f6chs\u00b7ten", "Ho\u00b7heit", "an", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "NN", "$(", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "als spr\u00e4ch sie: vergi\u00df mein nicht / du / dem also hoch beliebet", "tokens": ["als", "spr\u00e4ch", "sie", ":", "ver\u00b7gi\u00df", "mein", "nicht", "/", "du", "/", "dem", "al\u00b7so", "hoch", "be\u00b7lie\u00b7bet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "$.", "VVIMP", "PPOSAT", "PTKNEG", "$(", "PPER", "$(", "ART", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "dieser Erden Eitelkeit / die doch endlich nur betr\u00fcbet.", "tokens": ["die\u00b7ser", "Er\u00b7den", "Ei\u00b7tel\u00b7keit", "/", "die", "doch", "end\u00b7lich", "nur", "be\u00b7tr\u00fc\u00b7bet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$(", "PRELS", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wisse / da\u00df man / meiner denkend / wol vergn\u00fc