{"dta.poem.1146": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "32. Die Sch\u00f6ne Wundm\u00e4hlige.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Welch unbemenschtes Hertz/ und was vor Tiger-Klauen ", "tokens": ["Welch", "un\u00b7be\u00b7menschtes", "Hertz", "/", "und", "was", "vor", "Ti\u00b7ger\u00b7Klau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$(", "KON", "PWS", "APPR", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sinds/ die der zarte Leib zu seinem Hencker hat?", "tokens": ["Sinds", "/", "die", "der", "zar\u00b7te", "Leib", "zu", "sei\u00b7nem", "Hen\u00b7cker", "hat", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PRELS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach deine Sch\u00f6nheit ist der Ursprung dieser That!", "tokens": ["Ach", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "ist", "der", "Ur\u00b7sprung", "die\u00b7ser", "That", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "VAFIN", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dir schadet deine Zier/ der Federn Schmuck dem Pfauen:", "tokens": ["Dir", "scha\u00b7det", "dei\u00b7ne", "Zier", "/", "der", "Fe\u00b7dern", "Schmuck", "dem", "Pfau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "ART", "NN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der M\u00f6rder wolte bald/ da aus dem hellen Blauen", "tokens": ["Der", "M\u00f6r\u00b7der", "wol\u00b7te", "bald", "/", "da", "aus", "dem", "hel\u00b7len", "Blau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "$(", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein blitzend Augen-Strahl ihm an die Seele trat/", "tokens": ["Ein", "blit\u00b7zend", "Au\u00b7gen\u00b7Strahl", "ihm", "an", "die", "See\u00b7le", "trat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Wolcken deiner Brust zertrennen/ und die Stat", "tokens": ["Die", "Wol\u00b7cken", "dei\u00b7ner", "Brust", "zer\u00b7tren\u00b7nen", "/", "und", "die", "Stat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$(", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Des Himmels selbst/ den Sitz des edlen Geistes schauen.", "tokens": ["Des", "Him\u00b7mels", "selbst", "/", "den", "Sitz", "des", "ed\u00b7len", "Geis\u00b7tes", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$(", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach h\u00e4tt\u2019 er doch geglaubt/ eh du so viel erlitten!", "tokens": ["Ach", "h\u00e4tt'", "er", "doch", "ge\u00b7glaubt", "/", "eh", "du", "so", "viel", "er\u00b7lit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ADV", "VVPP", "$(", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das mindste/ was er fand/ war Perlen und Rubin/", "tokens": ["Das", "minds\u00b7te", "/", "was", "er", "fand", "/", "war", "Per\u00b7len", "und", "Ru\u00b7bin", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "PWS", "PPER", "VVFIN", "$(", "VAFIN", "NN", "KON", "NE", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Die aus den Augen und der Wunde flossen hin.", "tokens": ["Die", "aus", "den", "Au\u00b7gen", "und", "der", "Wun\u00b7de", "flos\u00b7sen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Am sch\u00f6nsten Orte wird der Zeug entzwey geschnitten:", "tokens": ["Am", "sch\u00f6ns\u00b7ten", "Or\u00b7te", "wird", "der", "Zeug", "ent\u00b7zwey", "ge\u00b7schnit\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ART", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wo g\u00f6ldnes Haar sticht durch/ trennt man der Haube De-", "tokens": ["Wo", "g\u00f6ld\u00b7nes", "Haar", "sticht", "durch", "/", "trennt", "man", "der", "Hau\u00b7be", "De"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "APPR", "$(", "VVFIN", "PIS", "ART", "NN", "TRUNC"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "cken/ ", "tokens": ["cken", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.15": {"text": "Der Handschuch wird zertheilt/ wo Stein und Ringe ste-", "tokens": ["Der", "Hand\u00b7schuch", "wird", "zer\u00b7theilt", "/", "wo", "Stein", "und", "Rin\u00b7ge", "ste"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "PWAV", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "cken. ", "tokens": ["cken", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}}}}}