{"dta.poem.10812": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Vorsorge GOttes f\u00fcr die Thie-  \n re, an den zur Winterszeit im W\u00e4l-  \n dern gr\u00fcnenden Stauden be-  \n merket.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts geschicht von Ohngefehr, alles hat\ndas h\u00f6chste Wesen,", "tokens": ["Nichts", "ge\u00b7schicht", "von", "Ohn\u00b7ge\u00b7fehr", ",", "al\u00b7les", "hat", "das", "h\u00f6chs\u00b7te", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVPP", "APPR", "ADV", "$,", "PIS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Auf der Welt zu seinem Zwek weis-", "tokens": ["Auf", "der", "Welt", "zu", "sei\u00b7nem", "Zwek", "weis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Diese Warheit fiel mir ein, als ich", "tokens": ["Die\u00b7se", "War\u00b7heit", "fiel", "mir", "ein", ",", "als", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wo der wilden Thiere Bahn, und ihr stiller Auffent-", "tokens": ["Wo", "der", "wil\u00b7den", "Thie\u00b7re", "Bahn", ",", "und", "ihr", "stil\u00b7ler", "Auf\u00b7fent"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN", "$,", "KON", "PPOSAT", "ADJA", "TRUNC"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Einst zur Winters Zeit ansah, da\u00df viel Stauden", "tokens": ["Einst", "zur", "Win\u00b7ters", "Zeit", "an\u00b7sah", ",", "da\u00df", "viel", "Stau\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "NN", "VVFIN", "$,", "KOUS", "PIAT", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Da in Garten durch den Schnee aller gr\u00fcner", "tokens": ["Da", "in", "Gar\u00b7ten", "durch", "den", "Schnee", "al\u00b7ler", "gr\u00fc\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "APPR", "ART", "NN", "PIAT", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ich sah hier Wacholdern bl\u00fchn, M\u00e4usdorn, Stech-", "tokens": ["Ich", "sah", "hier", "Wa\u00b7chol\u00b7dern", "bl\u00fchn", ",", "M\u00e4us\u00b7dorn", ",", "Stech"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN", "VVINF", "$,", "NN", "$,", "TRUNC"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Deren Laub im Schnee ganz frisch, wie im war-", "tokens": ["De\u00b7ren", "Laub", "im", "Schnee", "ganz", "frisch", ",", "wie", "im", "wa\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "NN", "APPRART", "NN", "ADV", "ADJD", "$,", "PWAV", "APPRART", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Weil ihr Safft ein klebricht Oel, der der K\u00e4lte wi-", "tokens": ["Weil", "ihr", "Safft", "ein", "kleb\u00b7richt", "O\u00b7el", ",", "der", "der", "K\u00e4l\u00b7te", "wi"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "TRUNC"], "meter": "--+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Und das Laub ganz frisch erh\u00e4lt, das sonst d\u00fcrre", "tokens": ["Und", "das", "Laub", "ganz", "frisch", "er\u00b7h\u00e4lt", ",", "das", "sonst", "d\u00fcr\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "B\u00e4ume die uns deine G\u00fct, in die G\u00e4rten hat gesezt?", "tokens": ["B\u00e4u\u00b7me", "die", "uns", "dei\u00b7ne", "G\u00fct", ",", "in", "die", "G\u00e4r\u00b7ten", "hat", "ge\u00b7sezt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PPER", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+---+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Warum sind die Stauden frei, die in denen W\u00e4l-", "tokens": ["Wa\u00b7rum", "sind", "die", "Stau\u00b7den", "frei", ",", "die", "in", "de\u00b7nen", "W\u00e4l"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "APPR", "PRELS", "TRUNC"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Deren Schmuk verborgen bleibt und die wenigsten", "tokens": ["De\u00b7ren", "Schmuk", "ver\u00b7bor\u00b7gen", "bleibt", "und", "die", "we\u00b7nigs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "VVPP", "VVFIN", "KON", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.15": {"text": "Indem ich dies \u00fcberdacht, lief ein Rehbok gleich daher,", "tokens": ["In\u00b7dem", "ich", "dies", "\u00fc\u00b7berd\u00b7acht", ",", "lief", "ein", "Reh\u00b7bok", "gleich", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVPP", "$,", "VVFIN", "ART", "NN", "ADV", "PAV", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Fras das Laub der Stauden ab; und der Haasen", "tokens": ["Fras", "das", "Laub", "der", "Stau\u00b7den", "ab", ";", "und", "der", "Haa\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ART", "NN", "PTKVZ", "$.", "KON", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Nahm es auch zur Nahrung weg: da sah ich, wozu", "tokens": ["Nahm", "es", "auch", "zur", "Nah\u00b7rung", "weg", ":", "da", "sah", "ich", ",", "wo\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "$,", "PWAV"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.18": {"text": "Und warum die Vorsehung dieses gr\u00fcne Laub be-", "tokens": ["Und", "wa\u00b7rum", "die", "Vor\u00b7se\u00b7hung", "die\u00b7ses", "gr\u00fc\u00b7ne", "Laub", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "PDAT", "ADJA", "NN", "TRUNC"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "W\u00fcrde nicht ohn dieses Laub, das im Wald ver-", "tokens": ["W\u00fcr\u00b7de", "nicht", "ohn", "die\u00b7ses", "Laub", ",", "das", "im", "Wald", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "APPR", "PDAT", "NN", "$,", "PRELS", "APPRART", "NN", "TRUNC"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "L\u00f6we, Tieger, B\u00e4r und Schwein, alles das nach", "tokens": ["L\u00f6\u00b7we", ",", "Tie\u00b7ger", ",", "B\u00e4r", "und", "Schwein", ",", "al\u00b7les", "das", "nach"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADJD", "$,", "NN", "KON", "NN", "$,", "PIS", "ART", "APPR"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.21": {"text": "In dem Winter untergehn; zwischen Schnee und", "tokens": ["In", "dem", "Win\u00b7ter", "un\u00b7ter\u00b7gehn", ";", "zwi\u00b7schen", "Schnee", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF", "$.", "APPR", "NN", "KON"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Wenn nicht ", "tokens": ["Wenn", "nicht"], "token_info": ["word", "word"], "pos": ["KOUS", "PTKNEG"], "meter": "-+", "measure": "iambic.single"}, "line.23": {"text": "So lehrt uns der W\u00e4lder Gr\u00fcn, und der Stau-", "tokens": ["So", "lehrt", "uns", "der", "W\u00e4l\u00b7der", "Gr\u00fcn", ",", "und", "der", "Stau"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,", "KON", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "In der kalten Winters Zeit: GOtt versorgt auch je-", "tokens": ["In", "der", "kal\u00b7ten", "Win\u00b7ters", "Zeit", ":", "Gott", "ver\u00b7sorgt", "auch", "je"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$.", "NN", "VVFIN", "ADV", "TRUNC"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Er verschafft dem wilden Vieh, auf so wunderbah-", "tokens": ["Er", "ver\u00b7schafft", "dem", "wil\u00b7den", "Vieh", ",", "auf", "so", "wun\u00b7der\u00b7bah"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "ADV", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Da er es im Pelz verh\u00fcllt, durch das Laub auch sei-", "tokens": ["Da", "er", "es", "im", "Pelz", "ver\u00b7h\u00fcllt", ",", "durch", "das", "Laub", "auch", "sei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "VVPP", "$,", "APPR", "ART", "NN", "ADV", "TRUNC"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}