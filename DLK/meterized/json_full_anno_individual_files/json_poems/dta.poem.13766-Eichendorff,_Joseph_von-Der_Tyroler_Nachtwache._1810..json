{"dta.poem.13766": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Tyroler Nachtwache.  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "In stiller Bucht, bei finstrer Nacht,", "tokens": ["In", "stil\u00b7ler", "Bucht", ",", "bei", "finst\u00b7rer", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00e4ft tief die Welt im Grunde,", "tokens": ["Schl\u00e4ft", "tief", "die", "Welt", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Berge rings steh'n auf der Wacht,", "tokens": ["Die", "Ber\u00b7ge", "rings", "steh'n", "auf", "der", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Himmel macht die Runde,", "tokens": ["Der", "Him\u00b7mel", "macht", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Geht um und um", "tokens": ["Geht", "um", "und", "um"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "KOUI"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ums Land herum", "tokens": ["Ums", "Land", "he\u00b7rum"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "NN", "APZR"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mit seinen goldnen Schaaren", "tokens": ["Mit", "sei\u00b7nen", "gold\u00b7nen", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Frommen zu bewahren.", "tokens": ["Die", "From\u00b7men", "zu", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}