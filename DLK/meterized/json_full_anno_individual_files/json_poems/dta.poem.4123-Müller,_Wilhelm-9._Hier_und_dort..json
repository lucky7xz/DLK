{"dta.poem.4123": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "9. Hier und dort.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.71", "tl:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Sang mir behagt!", "tokens": ["Dein", "Sang", "mir", "be\u00b7hagt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ach, wenn ich doch selber", "tokens": ["Ach", ",", "wenn", "ich", "doch", "sel\u00b7ber"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ein Lied gleich w\u00e4r',", "tokens": ["Ein", "Lied", "gleich", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Meinem Sch\u00e4tzchen zu Ehr'!", "tokens": ["Mei\u00b7nem", "Sch\u00e4tz\u00b7chen", "zu", "Ehr'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Da wollt' ich mich schreiben", "tokens": ["Da", "wollt'", "ich", "mich", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Auf seid'nes Papier,", "tokens": ["Auf", "sei\u00b7d'\u00b7nes", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und wollte mich schicken", "tokens": ["Und", "woll\u00b7te", "mich", "schi\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Per Post zu ihr.", "tokens": ["Per", "Post", "zu", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Flugs th\u00e4t' sie erbrechen", "tokens": ["Flugs", "th\u00e4t'", "sie", "er\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Das Briefchen so fein,", "tokens": ["Das", "Brief\u00b7chen", "so", "fein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und schaute schnurgrade", "tokens": ["Und", "schau\u00b7te", "schnur\u00b7gra\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "In's Herz mir hinein.", "tokens": ["In's", "Herz", "mir", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Und s\u00e4he und h\u00f6rte,", "tokens": ["Und", "s\u00e4\u00b7he", "und", "h\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Wie gut ich ihr bin,", "tokens": ["Wie", "gut", "ich", "ihr", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPER", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Und wie ich ihr diene", "tokens": ["Und", "wie", "ich", "ihr", "die\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PPER", "PDS"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Mit stetigem Sinn.", "tokens": ["Mit", "ste\u00b7ti\u00b7gem", "Sinn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Und Liebchen th\u00e4t' sagen:", "tokens": ["Und", "Lieb\u00b7chen", "th\u00e4t'", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Du thust mir behagen!", "tokens": ["Du", "thust", "mir", "be\u00b7ha\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Und sagte und s\u00e4nge", "tokens": ["Und", "sag\u00b7te", "und", "s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Und spielte nur mich,", "tokens": ["Und", "spiel\u00b7te", "nur", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Und tr\u00fcg' im Mund, im Kopf, im Herzen", "tokens": ["Und", "tr\u00fcg'", "im", "Mund", ",", "im", "Kopf", ",", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Mich ewiglich.", "tokens": ["Mich", "e\u00b7wig\u00b7lich", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "H\u00e4tt' Gott mich gefragt,", "tokens": ["H\u00e4tt'", "Gott", "mich", "ge\u00b7fragt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Als die Welt er gemacht,", "tokens": ["Als", "die", "Welt", "er", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "So h\u00e4tt' ich ein Liebchen,", "tokens": ["So", "h\u00e4tt'", "ich", "ein", "Lieb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das w\u00e4re fein hier,", "tokens": ["Das", "w\u00e4\u00b7re", "fein", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "ADV", "$,"], "meter": "-+---", "measure": "dactylic.init"}, "line.5": {"text": "Und w\u00e4r' sie wo anders,", "tokens": ["Und", "w\u00e4r'", "sie", "wo", "an\u00b7ders", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PWAV", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "So w\u00e4r' ich bei ihr.", "tokens": ["So", "w\u00e4r'", "ich", "bei", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Dies Lied hat gesungen", "tokens": ["Dies", "Lied", "hat", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "NN", "VAFIN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ein Wandrer vom Rhein.", "tokens": ["Ein", "Wand\u00b7rer", "vom", "Rhein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Hier trinkt er das Wasser,", "tokens": ["Hier", "trinkt", "er", "das", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dort trank er den Wein.", "tokens": ["Dort", "trank", "er", "den", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}