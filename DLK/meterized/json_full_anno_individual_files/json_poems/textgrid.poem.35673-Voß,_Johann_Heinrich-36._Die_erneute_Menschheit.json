{"textgrid.poem.35673": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "36. Die erneute Menschheit", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stille herrsch', Andacht, und der Seel' Erhebung,", "tokens": ["Stil\u00b7le", "herr\u00b7sch'", ",", "An\u00b7dacht", ",", "und", "der", "Seel'", "Er\u00b7he\u00b7bung", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "NN", "$,", "KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Rings umher! Fern sei, was befleckt von S\u00fcnd' ist,", "tokens": ["Rings", "um\u00b7her", "!", "Fern", "sei", ",", "was", "be\u00b7fleckt", "von", "S\u00fcnd'", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "NN", "VAFIN", "$,", "PWS", "VVFIN", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was dem Staub anhaftet, zu klein der Menschheit", "tokens": ["Was", "dem", "Staub", "an\u00b7haf\u00b7tet", ",", "zu", "klein", "der", "Menschheit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "PTKA", "ADJD", "ART", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "H\u00f6herem Aufschwung!", "tokens": ["H\u00f6\u00b7he\u00b7rem", "Auf\u00b7schwung", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Dem die Weltkreis' all in den Sonnenhimmeln", "tokens": ["Dem", "die", "Welt\u00b7kreis'", "all", "in", "den", "Son\u00b7nen\u00b7him\u00b7meln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "PIAT", "APPR", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Staub sind; dem Weltjahre wie Augenblicke;", "tokens": ["Staub", "sind", ";", "dem", "Welt\u00b7jah\u00b7re", "wie", "Au\u00b7gen\u00b7bli\u00b7cke", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$.", "ART", "NN", "KOKOM", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Dem, gesamt aufstrebend, der Geister Tiefsinn", "tokens": ["Dem", ",", "ge\u00b7samt", "auf\u00b7stre\u00b7bend", ",", "der", "Geis\u00b7ter", "Tief\u00b7sinn"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "VVPP", "VVPP", "$,", "ART", "NN", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Nur ein Gedank' ist;", "tokens": ["Nur", "ein", "Ge\u00b7dank'", "ist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Dessen Macht kein Ma\u00df der Erschaffnen ausmi\u00dft;", "tokens": ["Des\u00b7sen", "Macht", "kein", "Ma\u00df", "der", "Er\u00b7schaff\u00b7nen", "aus\u00b7mi\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dessen fernhin d\u00e4mmerndes Licht Begeist'rung", "tokens": ["Des\u00b7sen", "fern\u00b7hin", "d\u00e4m\u00b7mern\u00b7des", "Licht", "Be\u00b7geist'\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADJA", "NN", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Kaum erreicht, hochfliegend: den Geist der Geister!", "tokens": ["Kaum", "er\u00b7reicht", ",", "hoch\u00b7flie\u00b7gend", ":", "den", "Geist", "der", "Geis\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VVPP", "$.", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Betet ihn an! Gott!", "tokens": ["Be\u00b7tet", "ihn", "an", "!", "Gott", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Nicht der Lipp' Anbetung ist wert der Gottheit,", "tokens": ["Nicht", "der", "Lipp'", "An\u00b7be\u00b7tung", "ist", "wert", "der", "Got\u00b7theit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NN", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nicht Gepr\u00e4ng' abb\u00fc\u00dfendes Tempeldienstes,", "tokens": ["Nicht", "Ge\u00b7pr\u00e4ng'", "ab\u00b7b\u00fc\u00b7\u00dfen\u00b7des", "Tem\u00b7pel\u00b7diens\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Nicht Gel\u00fcbd', noch Faste; nur That gekl\u00e4rter", "tokens": ["Nicht", "Ge\u00b7l\u00fcbd'", ",", "noch", "Fas\u00b7te", ";", "nur", "That", "ge\u00b7kl\u00e4r\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "ADV", "NN", "$.", "ADV", "NN", "ADJA"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Menschlichkeit ehrt ihn!", "tokens": ["Menschlich\u00b7keit", "ehrt", "ihn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$."], "meter": "+---", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Dich allein, Abglanz von der Gottheit Urlicht,", "tokens": ["Dich", "al\u00b7lein", ",", "Ab\u00b7glanz", "von", "der", "Got\u00b7theit", "Ur\u00b7licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Menschlichkeit! Dich sah der entz\u00fcckte Denker,", "tokens": ["Menschlich\u00b7keit", "!", "Dich", "sah", "der", "ent\u00b7z\u00fcck\u00b7te", "Den\u00b7ker", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Bebt' in Wollust, rang, wie zur Braut der J\u00fcngling,", "tokens": ["Bebt'", "in", "Wol\u00b7lust", ",", "rang", ",", "wie", "zur", "Braut", "der", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "VVFIN", "$,", "PWAV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Ach! und umschlo\u00df dich!", "tokens": ["Ach", "!", "und", "um\u00b7schlo\u00df", "dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "VVFIN", "PPER", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Flog mit dir aufw\u00e4rts, und vernahm in Demut", "tokens": ["Flog", "mit", "dir", "auf\u00b7w\u00e4rts", ",", "und", "ver\u00b7nahm", "in", "De\u00b7mut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "$,", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "N\u00e4her Gott! \u2013 Allvater, erbarm' dich unser!", "tokens": ["N\u00e4\u00b7her", "Gott", "!", "\u2013", "All\u00b7va\u00b7ter", ",", "er\u00b7barm'", "dich", "un\u00b7ser", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "$(", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Fleht' er auf: Allvater, unendlich gro\u00df, un-", "tokens": ["Fleht'", "er", "auf", ":", "All\u00b7va\u00b7ter", ",", "un\u00b7end\u00b7lich", "gro\u00df", ",", "un"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "NN", "$,", "ADJD", "ADJD", "$,", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "endlicher G\u00fcte!", "tokens": ["end\u00b7li\u00b7cher", "G\u00fc\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Flehn auch wir: Allvater, erbarm' dich ihrer,", "tokens": ["Flehn", "auch", "wir", ":", "All\u00b7va\u00b7ter", ",", "er\u00b7barm'", "dich", "ih\u00b7rer", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "$.", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "(als sie thun's unkundig!) die: Gott der Heerschar!", "tokens": ["(", "als", "sie", "thun's", "un\u00b7kun\u00b7dig", "!", ")", "die", ":", "Gott", "der", "Heer\u00b7schar", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "VVFIN", "ADJD", "$.", "$(", "ART", "$.", "NN", "ART", "NN", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Uns nur Gott! aufrufen, der Rache Zorn dir", "tokens": ["Uns", "nur", "Gott", "!", "auf\u00b7ru\u00b7fen", ",", "der", "Ra\u00b7che", "Zorn", "dir"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "NN", "$.", "VVIZU", "$,", "ART", "NN", "NN", "PPER"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "L\u00f6schend in S\u00fchnblut!", "tokens": ["L\u00f6\u00b7schend", "in", "S\u00fchn\u00b7blut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Gott, sie nahn lobsingend, vom Blut der Br\u00fcder", "tokens": ["Gott", ",", "sie", "nahn", "lob\u00b7sin\u00b7gend", ",", "vom", "Blut", "der", "Br\u00fc\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "ADJD", "VVPP", "$,", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Wild, die fromm dir dienten den Dienst der Heimat,", "tokens": ["Wild", ",", "die", "fromm", "dir", "dien\u00b7ten", "den", "Dienst", "der", "Hei\u00b7mat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Anders nur dich, Gro\u00dfer, den Engeln selbst Viel-", "tokens": ["An\u00b7ders", "nur", "dich", ",", "Gro\u00b7\u00dfer", ",", "den", "En\u00b7geln", "selbst", "Viel"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "$,", "NN", "$,", "ART", "NN", "ADV", "TRUNC"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "namiger! nennend!", "tokens": ["na\u00b7mi\u00b7ger", "!", "nen\u00b7nend", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$.", "VVPP", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.9": {"line.1": {"text": "H\u00f6chstes Gut allstets, und des Guten Geber!", "tokens": ["H\u00f6chs\u00b7tes", "Gut", "alls\u00b7tets", ",", "und", "des", "Gu\u00b7ten", "Ge\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Ihm, der Raubwild jagt in der Eichelwaldung;", "tokens": ["Ihm", ",", "der", "Raub\u00b7wild", "jagt", "in", "der", "Ei\u00b7chel\u00b7wal\u00b7dung", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Ihm, der Feind' abwehrt mit Gescho\u00df und Harnisch,", "tokens": ["Ihm", ",", "der", "Feind'", "ab\u00b7wehrt", "mit", "Ge\u00b7scho\u00df", "und", "Har\u00b7nisch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Froh des Gemeinwohls:", "tokens": ["Froh", "des", "Ge\u00b7mein\u00b7wohls", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Oder ihm, des Seel', in das All sich schwingend,", "tokens": ["O\u00b7der", "ihm", ",", "des", "Seel'", ",", "in", "das", "All", "sich", "schwin\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "$,", "APPR", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mit der Grundursachen Gewicht und Ma\u00dfe,", "tokens": ["Mit", "der", "Grund\u00b7ur\u00b7sa\u00b7chen", "Ge\u00b7wicht", "und", "Ma\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Harmonie wahrnimmt, aus Verbl\u00fchn Erschaffung,", "tokens": ["Har\u00b7mo\u00b7nie", "wahr\u00b7nimmt", ",", "aus", "Ver\u00b7bl\u00fchn", "Er\u00b7schaf\u00b7fung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "APPR", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Leben aus Tode!", "tokens": ["Le\u00b7ben", "aus", "To\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Ob wie tot auch starre der Geist der Menschheit,", "tokens": ["Ob", "wie", "tot", "auch", "star\u00b7re", "der", "Geist", "der", "Menschheit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ADJD", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Durch der Willk\u00fcr Zwang und gebotnen Wahnsinn;", "tokens": ["Durch", "der", "Will\u00b7k\u00fcr", "Zwang", "und", "ge\u00b7bot\u00b7nen", "Wahn\u00b7sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Doch erringt siegreich auch der Geist der Menschheit", "tokens": ["Doch", "er\u00b7ringt", "sieg\u00b7reich", "auch", "der", "Geist", "der", "Menschheit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN"], "meter": "--++-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Neue Belebung.", "tokens": ["Neu\u00b7e", "Be\u00b7le\u00b7bung", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.12": {"line.1": {"text": "Zwar er schlief Jahrhunderte, dumpf in Fesseln,", "tokens": ["Zwar", "er", "schlief", "Jahr\u00b7hun\u00b7der\u00b7te", ",", "dumpf", "in", "Fes\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Todesschlaf, seit himmelempor die Freiheit", "tokens": ["To\u00b7des\u00b7schlaf", ",", "seit", "him\u00b7mel\u00b7em\u00b7por", "die", "Frei\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "NN", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Vor den Zwingherrn floh und des G\u00f6tzenpriesters", "tokens": ["Vor", "den", "Zwing\u00b7herrn", "floh", "und", "des", "G\u00f6t\u00b7zen\u00b7pries\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "KON", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Laurendem Bannstrahl.", "tokens": ["Lau\u00b7ren\u00b7dem", "Bann\u00b7strahl", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.13": {"line.1": {"text": "Luther kam: auf schauert' im Schlaf der Geist ihm,", "tokens": ["Lu\u00b7ther", "kam", ":", "auf", "schau\u00b7ert'", "im", "Schlaf", "der", "Geist", "ihm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "APPR", "VVFIN", "APPRART", "NN", "ART", "NN", "PPER", "$,"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Blickt' umher, schlo\u00df wieder das Aug' in Ohnmacht,", "tokens": ["Blickt'", "um\u00b7her", ",", "schlo\u00df", "wie\u00b7der", "das", "Aug'", "in", "Ohn\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und vernahm leis ahndend den Laut aus Tr\u00fcmmern", "tokens": ["Und", "ver\u00b7nahm", "leis", "ahn\u00b7dend", "den", "Laut", "aus", "Tr\u00fcm\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ADJD", "ART", "APPR", "APPR", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Attischer Weisheit.", "tokens": ["At\u00b7ti\u00b7scher", "Weis\u00b7heit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.14": {"line.1": {"text": "Bald, wie Glut fortglimmt in der Asch', am Windhauch", "tokens": ["Bald", ",", "wie", "Glut", "fort\u00b7glimmt", "in", "der", "Asch'", ",", "am", "Wind\u00b7hauch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "PWAV", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "APPRART", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcnkchen hellt, rot wird, und in Feuerflammen", "tokens": ["F\u00fcnk\u00b7chen", "hellt", ",", "rot"