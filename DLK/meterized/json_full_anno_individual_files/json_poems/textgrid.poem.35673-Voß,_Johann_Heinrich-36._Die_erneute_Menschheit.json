 "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "$,", "KON", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Bl\u00fchten mit Heil auf!", "tokens": ["Bl\u00fch\u00b7ten", "mit", "Heil", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.16": {"line.1": {"text": "Stille herrsch', Andacht, und der Seel' Erhebung,", "tokens": ["Stil\u00b7le", "herr\u00b7sch'", ",", "An\u00b7dacht", ",", "und", "der", "Seel'", "Er\u00b7he\u00b7bung", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "NN", "$,", "KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Rings umher! Fern sei, was befleckt von S\u00fcnd' ist,", "tokens": ["Rings", "um\u00b7her", "!", "Fern", "sei", ",", "was", "be\u00b7fleckt", "von", "S\u00fcnd'", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "NN", "VAFIN", "$,", "PWS", "VVFIN", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was dem Staub anhaftet, zu klein der Menschheit", "tokens": ["Was", "dem", "Staub", "an\u00b7haf\u00b7tet", ",", "zu", "klein", "der", "Menschheit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "PTKA", "ADJD", "ART", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "H\u00f6herem Aufschwung!", "tokens": ["H\u00f6\u00b7he\u00b7rem", "Auf\u00b7schwung", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.17": {"l