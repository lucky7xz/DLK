{"textgrid.poem.33087": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eines wei\u00df ich, ob ihr mir auch grollt,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eines wei\u00df ich, ob ihr mir auch grollt,", "tokens": ["Ei\u00b7nes", "wei\u00df", "ich", ",", "ob", "ihr", "mir", "auch", "grollt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df ich stets das Beste nur gewollt!", "tokens": ["Da\u00df", "ich", "stets", "das", "Bes\u00b7te", "nur", "ge\u00b7wollt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "ADV", "VMPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sprecht, warum war ich euch denn einst lieb,", "tokens": ["Sprecht", ",", "wa\u00b7rum", "war", "ich", "euch", "denn", "einst", "lieb", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "VAFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "+-----+-+", "measure": "dactylic.init"}, "line.4": {"text": "Welch ein Reiz war's, der euch zu mir trieb?", "tokens": ["Welch", "ein", "Reiz", "wa\u00b7r's", ",", "der", "euch", "zu", "mir", "trieb", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Sch\u00f6nheit blieb mir fern und Reichthum fehlt,", "tokens": ["Sch\u00f6n\u00b7heit", "blieb", "mir", "fern", "und", "Reicht\u00b7hum", "fehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Witz und Geist ist Andern mehr erw\u00e4hlt,", "tokens": ["Witz", "und", "Geist", "ist", "An\u00b7dern", "mehr", "er\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADJA", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Doch ein treues Herz und fester Muth", "tokens": ["Doch", "ein", "treu\u00b7es", "Herz", "und", "fes\u00b7ter", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "F\u00fcr das Rechte und der Wahrheit Gut,", "tokens": ["F\u00fcr", "das", "Rech\u00b7te", "und", "der", "Wahr\u00b7heit", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Liebe zu der Menschheit, die da klagt,", "tokens": ["Lie\u00b7be", "zu", "der", "Menschheit", ",", "die", "da", "klagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Und ein Geist, der nicht vor M\u00e4cht'gen zagt \u2013", "tokens": ["Und", "ein", "Geist", ",", "der", "nicht", "vor", "M\u00e4cht'\u00b7gen", "zagt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PTKNEG", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Dies allein ist's, was mich liebenswerth", "tokens": ["Dies", "al\u00b7lein", "ist's", ",", "was", "mich", "lie\u00b7bens\u00b7werth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "VAFIN", "$,", "PWS", "PPER", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Machen konnte und zum Freund begehrt!", "tokens": ["Ma\u00b7chen", "konn\u00b7te", "und", "zum", "Freund", "be\u00b7gehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VMFIN", "KON", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Und nun wundert ihr euch, da\u00df ich hei\u00df", "tokens": ["Und", "nun", "wun\u00b7dert", "ihr", "euch", ",", "da\u00df", "ich", "hei\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Gl\u00fche f\u00fcr des Lebens h\u00f6chsten Preis,", "tokens": ["Gl\u00fc\u00b7he", "f\u00fcr", "des", "Le\u00b7bens", "h\u00f6chs\u00b7ten", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Und ihr scheltet, wenn ich laut und frei", "tokens": ["Und", "ihr", "schel\u00b7tet", ",", "wenn", "ich", "laut", "und", "frei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "Rede gegen L\u00fcg' und Tyrannei,", "tokens": ["Re\u00b7de", "ge\u00b7gen", "L\u00fcg'", "und", "Ty\u00b7ran\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Scheltet, wenn mein Herz von Gram bewegt", "tokens": ["Schel\u00b7tet", ",", "wenn", "mein", "Herz", "von", "Gram", "be\u00b7wegt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN", "APPR", "NE", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "F\u00fcr der Menschheit ew'ge Rechte schl\u00e4gt,", "tokens": ["F\u00fcr", "der", "Menschheit", "ew'\u00b7ge", "Rech\u00b7te", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "--++-+-+", "measure": "anapaest.init"}, "line.19": {"text": "Wenn es mitk\u00e4mpft in dem heil'gen Krieg", "tokens": ["Wenn", "es", "mit\u00b7k\u00e4mpft", "in", "dem", "heil'\u00b7gen", "Krieg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.20": {"text": "F\u00fcr der ", "tokens": ["F\u00fcr", "der"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Was als Wahrheit ich erkannte rein,", "tokens": ["Was", "als", "Wahr\u00b7heit", "ich", "er\u00b7kann\u00b7te", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "KOUS", "NN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.22": {"text": "Mu\u00df in's Leben tragen ich hinein,", "tokens": ["Mu\u00df", "in's", "Le\u00b7ben", "tra\u00b7gen", "ich", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["