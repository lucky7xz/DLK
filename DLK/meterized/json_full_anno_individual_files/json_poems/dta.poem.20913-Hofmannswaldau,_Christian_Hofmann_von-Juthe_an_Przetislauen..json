RT", "NN", "VVFIN", "PPER", "PIS", "$(", "ART", "NN", "VVFIN", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.99": {"text": "tzen/", "tokens": ["tzen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.100": {"text": "Ein Blick/ wie scharf er ist/ dringt warlich nicht dahin/", "tokens": ["Ein", "Blick", "/", "wie", "scharf", "er", "ist", "/", "dringt", "war\u00b7lich", "nicht", "da\u00b7hin", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "VVFIN", "PPER", "VAFIN", "$(", "VVFIN", "ADV", "PTKNEG", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.101": {"text": "Ich kann ohn alle Schuld zugleich mit Worten scher-", "tokens": ["Ich", "kann", "ohn", "al\u00b7le", "Schuld", "zu\u00b7gleich", "mit", "Wor\u00b7ten", "scher"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "ADV", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.102": {"text": "tzen/", "tokens": ["tzen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.103": {"text": "Und dencken da\u00df ich hier als eine Nonne bin;", "tokens": ["Und", "den\u00b7cken", "da\u00df", "ich", "hier", "als", "ei\u00b7ne", "Non\u00b7ne", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "PPER", "ADV", "KOUS", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.104": {"text": "Ich bin (doch unger\u00fchmt) mit Keuschheit so umschlos-", "tokens": ["Ich", "bin", "(", "doch", "un\u00b7ge\u00b7r\u00fchmt", ")", "mit", "Keuschheit", "so", "um\u00b7schlos"], "t