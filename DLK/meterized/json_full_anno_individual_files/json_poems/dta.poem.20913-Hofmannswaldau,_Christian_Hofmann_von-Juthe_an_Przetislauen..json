{"dta.poem.20913": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Juthe an Przetislauen.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Empfindst du/ da\u00df mein Brief dir nicht nach", "tokens": ["Emp\u00b7findst", "du", "/", "da\u00df", "mein", "Brief", "dir", "nicht", "nach"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Rosen schmecket/", "tokens": ["Ro\u00b7sen", "schme\u00b7cket", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ist dieses schlechte Blat nicht Biesemkuchen", "tokens": ["Ist", "die\u00b7ses", "schlech\u00b7te", "Blat", "nicht", "Bie\u00b7sem\u00b7ku\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "PTKNEG", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "voll/", "tokens": ["voll", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "So dencke nur es ist mit Klosterstaub bedecket/", "tokens": ["So", "den\u00b7cke", "nur", "es", "ist", "mit", "Klos\u00b7ter\u00b7staub", "be\u00b7de\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "VAFIN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wer verstohlen schreibt/ schreibt selten allzuwohl.", "tokens": ["Und", "wer", "ver\u00b7stoh\u00b7len", "schreibt", "/", "schreibt", "sel\u00b7ten", "all\u00b7zu\u00b7wohl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVINF", "VVFIN", "$(", "VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich bin gantz ungewohnt die Feder recht zuf\u00fchren/", "tokens": ["Ich", "bin", "gantz", "un\u00b7ge\u00b7wohnt", "die", "Fe\u00b7der", "recht", "zu\u00b7f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich kenne noch den Marckt der sch\u00f6nen Worte nicht/", "tokens": ["Ich", "ken\u00b7ne", "noch", "den", "Marckt", "der", "sch\u00f6\u00b7nen", "Wor\u00b7te", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es wei\u00df kein runder Spruch mein Schreiben recht", "tokens": ["Es", "wei\u00df", "kein", "run\u00b7der", "Spruch", "mein", "Schrei\u00b7ben", "recht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "zu ziehren/", "tokens": ["zu", "zieh\u00b7ren", "/"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Weil mir der Anfang auch des Schreibens fast ge-", "tokens": ["Weil", "mir", "der", "An\u00b7fang", "auch", "des", "Schrei\u00b7bens", "fast", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "bricht.", "tokens": ["bricht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Was aber sagest du? ich stehle hier die hertzen/", "tokens": ["Was", "a\u00b7ber", "sa\u00b7gest", "du", "?", "ich", "steh\u00b7le", "hier", "die", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und hette deines selbst zu dem Altar gethan?", "tokens": ["Und", "het\u00b7te", "dei\u00b7nes", "selbst", "zu", "dem", "Al\u00b7tar", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es scheint der F\u00fcrst hat Lust mit seiner Magd zu scher-", "tokens": ["Es", "scheint", "der", "F\u00fcrst", "hat", "Lust", "mit", "sei\u00b7ner", "Magd", "zu", "scher"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "tzen/", "tokens": ["tzen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.17": {"text": "Mein Finger r\u00fchret nichts als nur den Psalter an.", "tokens": ["Mein", "Fin\u00b7ger", "r\u00fch\u00b7ret", "nichts", "als", "nur", "den", "Psal\u00b7ter", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "KOKOM", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Bethe lieget itzt allein in meinen H\u00e4nden/", "tokens": ["Die", "Be\u00b7the", "lie\u00b7get", "itzt", "al\u00b7lein", "in", "mei\u00b7nen", "H\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "In diese hab\u2019 ich noch kein weltlich Buch gebracht/", "tokens": ["In", "die\u00b7se", "hab'", "ich", "noch", "kein", "welt\u00b7lich", "Buch", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VAFIN", "PPER", "ADV", "PIAT", "ADJD", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mein Auge wei\u00df sich nur auf das Altar zuwenden/", "tokens": ["Mein", "Au\u00b7ge", "wei\u00df", "sich", "nur", "auf", "das", "Al\u00b7tar", "zu\u00b7wen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.21": {"text": "Und ist nunmehr auf nichts als nur auff Gott bedacht.", "tokens": ["Und", "ist", "nun\u00b7mehr", "auf", "nichts", "als", "nur", "auff", "Gott", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PIS", "KOKOM", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ich kenne fast nicht recht di\u00df/ was mein F\u00fcrst begehret/", "tokens": ["Ich", "ken\u00b7ne", "fast", "nicht", "recht", "di\u00df", "/", "was", "mein", "F\u00fcrst", "be\u00b7ge\u00b7hret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADJD", "PDS", "$(", "PWS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Er ha\u00dft mein Element/ darinn ich leben mu\u00df/", "tokens": ["Er", "ha\u00dft", "mein", "E\u00b7le\u00b7ment", "/", "da\u00b7rinn", "ich", "le\u00b7ben", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PAV", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Er sagt/ ich werde hier nur durch mich selbst verzehret/", "tokens": ["Er", "sagt", "/", "ich", "wer\u00b7de", "hier", "nur", "durch", "mich", "selbst", "ver\u00b7zeh\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "ADV", "ADV", "APPR", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Er tadelt meinen Gang/ und st\u00f6ret meinen Fu\u00df.", "tokens": ["Er", "ta\u00b7delt", "mei\u00b7nen", "Gang", "/", "und", "st\u00f6\u00b7ret", "mei\u00b7nen", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Er will die Bethe mir aus meinen H\u00e4nden bringen/", "tokens": ["Er", "will", "die", "Be\u00b7the", "mir", "aus", "mei\u00b7nen", "H\u00e4n\u00b7den", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Er macht die Kloster Pflicht zu seinem Possen Spiel/", "tokens": ["Er", "macht", "die", "Klos\u00b7ter", "Pflicht", "zu", "sei\u00b7nem", "Pos\u00b7sen", "Spiel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Er hat mich nie geh\u00f6rt und tadelt doch mein singen/", "tokens": ["Er", "hat", "mich", "nie", "ge\u00b7h\u00f6rt", "und", "ta\u00b7delt", "doch", "mein", "sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und meint ich th\u00e4te nicht/ was Bluth und Jugend will.", "tokens": ["Und", "meint", "ich", "th\u00e4\u00b7te", "nicht", "/", "was", "Bluth", "und", "Ju\u00b7gend", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "PTKNEG", "$(", "PWS", "NN", "KON", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Di\u00df und was ferner folgt/ das seyn mir neue Sachen/", "tokens": ["Di\u00df", "und", "was", "fer\u00b7ner", "folgt", "/", "das", "seyn", "mir", "neu\u00b7e", "Sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KON", "PWS", "ADV", "VVFIN", "$(", "PDS", "VAFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich kenn es nicht vielmehr/ als Schrifft der frembden", "tokens": ["Ich", "kenn", "es", "nicht", "viel\u00b7mehr", "/", "als", "Schrifft", "der", "fremb\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$(", "KOUS", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Welt/", "tokens": ["Welt", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.33": {"text": "Ich wei\u00df nicht was ich soll aus deinen Worte\u0303 machen/", "tokens": ["Ich", "wei\u00df", "nicht", "was", "ich", "soll", "aus", "dei\u00b7nen", "Wor\u00b7t\u1ebd", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Doch wo mich recht bedeucht/ hier ist verboten Geld.", "tokens": ["Doch", "wo", "mich", "recht", "be\u00b7deucht", "/", "hier", "ist", "ver\u00b7bo\u00b7ten", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADJD", "$(", "ADV", "VAFIN", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Du zeigst mir/ wie es scheint/ gar viel verf\u00e4lschte Wah-", "tokens": ["Du", "zeigst", "mir", "/", "wie", "es", "scheint", "/", "gar", "viel", "ver\u00b7f\u00e4lschte", "Wah"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PWAV", "PPER", "VVFIN", "$(", "ADV", "PIAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.36": {"text": "ren/", "tokens": ["ren", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.37": {"text": "Auf die des H\u00f6chsten Zorn das Feuer hat gesetzt/", "tokens": ["Auf", "die", "des", "H\u00f6chs\u00b7ten", "Zorn", "das", "Feu\u00b7er", "hat", "ge\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ich kan nicht allzuwohl die Gr\u00e4ntzen \u00fcberfahren/", "tokens": ["Ich", "kan", "nicht", "all\u00b7zu\u00b7wohl", "die", "Gr\u00e4nt\u00b7zen", "\u00fc\u00b7berf\u00b7ah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Nach dem der Himmel mich des Klosters werth ge-", "tokens": ["Nach", "dem", "der", "Him\u00b7mel", "mich", "des", "Klos\u00b7ters", "werth", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "PPER", "ART", "NN", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "sch\u00e4tzt.", "tokens": ["sch\u00e4tzt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.41": {"text": "Und da\u00df ich endlich nun hier sonder F\u00fcrhang spiele/", "tokens": ["Und", "da\u00df", "ich", "end\u00b7lich", "nun", "hier", "son\u00b7der", "F\u00fcr\u00b7hang", "spie\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und ohne Ma\u00dfque dir nur zeige/ was ich bin/", "tokens": ["Und", "oh\u00b7ne", "Ma\u00df\u00b7que", "dir", "nur", "zei\u00b7ge", "/", "was", "ich", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "ADV", "VVFIN", "$(", "PWS", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Mein Auge steht itzund nicht weit von deinem Ziele/", "tokens": ["Mein", "Au\u00b7ge", "steht", "it\u00b7zund", "nicht", "weit", "von", "dei\u00b7nem", "Zie\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ich kenne deinen Wunsch und sp\u00fcre deinen Sinn.", "tokens": ["Ich", "ken\u00b7ne", "dei\u00b7nen", "Wunsch", "und", "sp\u00fc\u00b7re", "dei\u00b7nen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Des Briefes Wolcke hat ein guter Freund vertrieben/", "tokens": ["Des", "Brie\u00b7fes", "Wol\u00b7cke", "hat", "ein", "gu\u00b7ter", "Freund", "ver\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Er hat/ was Nebel war/