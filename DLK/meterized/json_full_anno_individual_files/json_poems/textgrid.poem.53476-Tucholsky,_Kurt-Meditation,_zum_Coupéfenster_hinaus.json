{"textgrid.poem.53476": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Meditation, zum Coup\u00e9fenster hinaus", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die langen Telegrafenstangen", "tokens": ["Wie", "die", "lan\u00b7gen", "Te\u00b7le\u00b7gra\u00b7fen\u00b7stan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "jene schwarzen, d\u00fcnnen Dr\u00e4hte, die", "tokens": ["je\u00b7ne", "schwar\u00b7zen", ",", "d\u00fcn\u00b7nen", "Dr\u00e4h\u00b7te", ",", "die"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PDS", "VVINF", "$,", "ADJA", "NN", "$,", "PRELS"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "grad sich zu erheben angefangen,", "tokens": ["grad", "sich", "zu", "er\u00b7he\u00b7ben", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "immer wieder niedergehen, wie", "tokens": ["im\u00b7mer", "wie\u00b7der", "nie\u00b7der\u00b7ge\u00b7hen", ",", "wie"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "VVINF", "$,", "PWAV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "diese dunkeln regelm\u00e4\u00dfigen St\u00e4be,", "tokens": ["die\u00b7se", "dun\u00b7keln", "re\u00b7gel\u00b7m\u00e4\u00b7\u00dfi\u00b7gen", "St\u00e4\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "die das Auf und Ab und Auf und Ab",