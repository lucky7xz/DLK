{"dta.poem.9394": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Als H. H. Hermanns liebes Ehgemahl  \n F. Clara Sophia Schultzin todes  \n verblichen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Stellet das gr\u00e4men und hermen bey seite/", "tokens": ["Stel\u00b7let", "das", "gr\u00e4\u00b7men", "und", "her\u00b7men", "bey", "sei\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "KON", "VVFIN", "APPR", "PPOSAT", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Eure Gemahlin ist g\u00e4ntzlich nicht todt/", "tokens": ["Eu\u00b7re", "Ge\u00b7mah\u00b7lin", "ist", "g\u00e4ntz\u00b7lich", "nicht", "todt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "PTKNEG", "ADJD", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "ob sie schon l\u00e4sset die zeitliche noth/", "tokens": ["ob", "sie", "schon", "l\u00e4s\u00b7set", "die", "zeit\u00b7li\u00b7che", "noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "ob sie schon euere gegenwart heute", "tokens": ["ob", "sie", "schon", "eu\u00b7e\u00b7re", "ge\u00b7gen\u00b7wart", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "l\u00e4sset und erbet die ewige Beute/", "tokens": ["l\u00e4s\u00b7set", "und", "er\u00b7bet", "die", "e\u00b7wi\u00b7ge", "Beu\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "da sie wird schauen den s\u00fc\u00dfesten Soot/", "tokens": ["da", "sie", "wird", "schau\u00b7en", "den", "s\u00fc\u00b7\u00dfes\u00b7ten", "Soot", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVINF", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nectar und Manna/ das himlische Brodt/", "tokens": ["Nec\u00b7tar", "und", "Man\u00b7na", "/", "das", "him\u00b7li\u00b7sche", "Brodt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und die verkl\u00e4resten weisesten Leuthe/", "tokens": ["und", "die", "ver\u00b7kl\u00e4\u00b7res\u00b7ten", "wei\u00b7ses\u00b7ten", "Leu\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "ja mit der kl\u00e4resten Wei\u00dfheit begabt/", "tokens": ["ja", "mit", "der", "kl\u00e4\u00b7res\u00b7ten", "Wei\u00df\u00b7heit", "be\u00b7gabt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ADJD", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "die Sie auf erden nur etwas gehabt.", "tokens": ["die", "Sie", "auf", "er\u00b7den", "nur", "et\u00b7was", "ge\u00b7habt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADV", "PIS", "VAPP", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Die Cherubinen empfangen Sie sch\u00f6ne/", "tokens": ["Die", "Che\u00b7ru\u00b7bi\u00b7nen", "emp\u00b7fan\u00b7gen", "Sie", "sch\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "der Adamantine Himlische Saal/", "tokens": ["der", "A\u00b7da\u00b7man\u00b7ti\u00b7ne", "Him\u00b7li\u00b7sche", "Saal", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "zieht Jhr entgegen mit gro\u00dfer anzahl;", "tokens": ["zieht", "Ihr", "ent\u00b7ge\u00b7gen", "mit", "gro\u00b7\u00dfer", "an\u00b7zahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Ach wer auch h\u00f6rte das sch\u00f6ne geth\u00f6ne?", "tokens": ["Ach", "wer", "auch", "h\u00f6r\u00b7te", "das", "sch\u00f6\u00b7ne", "ge\u00b7th\u00f6\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}