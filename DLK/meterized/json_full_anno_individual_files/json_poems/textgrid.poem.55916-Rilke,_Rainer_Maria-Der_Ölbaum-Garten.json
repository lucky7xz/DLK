{"textgrid.poem.55916": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der \u00d6lbaum-Garten", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er ging hinauf unter dem grauen Laub", "tokens": ["Er", "ging", "hin\u00b7auf", "un\u00b7ter", "dem", "grau\u00b7en", "Laub"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "ganz grau und aufgel\u00f6st im \u00d6lgel\u00e4nde", "tokens": ["ganz", "grau", "und", "auf\u00b7ge\u00b7l\u00f6st", "im", "\u00d6l\u00b7ge\u00b7l\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und legte seine Stirne voller Staub", "tokens": ["und", "leg\u00b7te", "sei\u00b7ne", "Stir\u00b7ne", "vol\u00b7ler", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "tief in das Staubigsein der hei\u00dfen H\u00e4nde.", "tokens": ["tief", "in", "das", "Stau\u00b7big\u00b7sein", "der", "hei\u00b7\u00dfen", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nach allem dies. Und dieses war der Schlu\u00df.", "tokens": ["Nach", "al\u00b7lem", "dies", ".", "Und", "die\u00b7ses", "war", "der", "Schlu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["