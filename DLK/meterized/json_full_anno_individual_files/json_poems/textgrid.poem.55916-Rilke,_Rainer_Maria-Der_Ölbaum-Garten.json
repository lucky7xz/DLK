ord", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "$.", "PTKNEG", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich finde Dich nicht mehr. Ich bin allein.", "tokens": ["Ich", "fin\u00b7de", "Dich", "nicht", "mehr", ".", "Ich", "bin", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$.", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich bin allein mit aller Menschen Gram,", "tokens": ["Ich", "bin", "al\u00b7lein", "mit", "al\u00b7ler", "Men\u00b7schen", "Gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PIAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "den ich durch Dich zu lindern unternahm,", "tokens": ["den", "ich", "durch", "Dich", "zu", "lin\u00b7dern", "un\u00b7ter\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der Du nicht bist. O namenlose Scham...", "tokens": ["der", "Du", "nicht", "bist", ".", "O", "na\u00b7men\u00b7lo\u00b7se", "Scham", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VAFIN", "$.", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sp\u00e4ter erz\u00e4hlte man: ein Engel kam \u2013.", "tokens": ["Sp\u00e4\u00b7ter", "er\u00b7z\u00e4hl\u00b7te", "man", ":", "ein", "En\u00b7gel", "kam", "\u2013", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", 