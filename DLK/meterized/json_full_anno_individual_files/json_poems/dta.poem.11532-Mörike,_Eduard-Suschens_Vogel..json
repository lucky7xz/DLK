{"dta.poem.11532": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Suschens Vogel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hatt' ein V\u00f6glein, ach wie fein!", "tokens": ["Ich", "hatt'", "ein", "V\u00f6\u00b7glein", ",", "ach", "wie", "fein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein sch\u00f6ners mag wohl nimmer seyn:", "tokens": ["Kein", "sch\u00f6\u00b7ners", "mag", "wohl", "nim\u00b7mer", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VMFIN", "ADV", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00e4tt' auf der Brust ein Herzlein roth,", "tokens": ["H\u00e4tt'", "auf", "der", "Brust", "ein", "Herz\u00b7lein", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sung und sung sich schier zu todt.", "tokens": ["Und", "sung", "und", "sung", "sich", "schier", "zu", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PRF", "ADJD", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Herzvogel mein, so wundersch\u00f6n,", "tokens": ["Herz\u00b7vo\u00b7gel", "mein", ",", "so", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jezt sollt du mit zu Markte gehn! \u2014", "tokens": ["Jezt", "sollt", "du", "mit", "zu", "Mark\u00b7te", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "APPR", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und da ich durch das St\u00e4dtlein kam,", "tokens": ["Und", "da", "ich", "durch", "das", "St\u00e4dt\u00b7lein", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es sa\u00df auf meiner Achsel zahm;", "tokens": ["Es", "sa\u00df", "auf", "mei\u00b7ner", "Ach\u00b7sel", "zahm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und als ich ging am Haus vorbei", "tokens": ["Und", "als", "ich", "ging", "am", "Haus", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Knaben, dem ich brach die Treu,", "tokens": ["Des", "Kna\u00b7ben", ",", "dem", "ich", "brach", "die", "Treu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Knab' just aus dem Fenster sah,", "tokens": ["Der", "Knab'", "just", "aus", "dem", "Fens\u00b7ter", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinem Finger schnalzt er da:", "tokens": ["Mit", "sei\u00b7nem", "Fin\u00b7ger", "schnalzt", "er", "da", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie horchet gleich mein Vogel auf!", "tokens": ["Wie", "hor\u00b7chet", "gleich", "mein", "Vo\u00b7gel", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Knaben fliegt er husch! hinauf;", "tokens": ["Zum", "Kna\u00b7ben", "fliegt", "er", "husch", "!", "hin\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "$.", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der koset ihn so lieb und hold,", "tokens": ["Der", "ko\u00b7set", "ihn", "so", "lieb", "und", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wu\u00dft nicht, was ich machen sollt,", "tokens": ["Ich", "wu\u00dft", "nicht", ",", "was", "ich", "ma\u00b7chen", "sollt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und stund, im Herzen so erschreckt,", "tokens": ["Und", "stund", ",", "im", "Her\u00b7zen", "so", "er\u00b7schreckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit H\u00e4nden mein Gesichte deckt',", "tokens": ["Mit", "H\u00e4n\u00b7den", "mein", "Ge\u00b7sich\u00b7te", "deckt'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und schlich davon und weinet' sehr,", "tokens": ["Und", "schlich", "da\u00b7von", "und", "wei\u00b7net'", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PAV", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir war, als rief' es hinterher:", "tokens": ["Mir", "war", ",", "als", "rie\u00b7f'", "es", "hin\u00b7ter\u00b7her", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "\u201edu falsche Maid, beh\u00fct' dich Gott,", "tokens": ["\u201e", "du", "fal\u00b7sche", "Maid", ",", "be\u00b7h\u00fct'", "dich", "Gott", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hab' doch wieder mein Herzlein roth!\u201c", "tokens": ["Ich", "hab'", "doch", "wie\u00b7der", "mein", "Herz\u00b7lein", "roth", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "ADJD", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}