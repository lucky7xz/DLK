{"dta.poem.11015": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Absicht unsers Hierseyns.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu welchem Endzweck meynst du wohl,", "tokens": ["Zu", "wel\u00b7chem", "End\u00b7zweck", "meynst", "du", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df doch das menschliche Geschlecht auf Erden?", "tokens": ["Da\u00df", "doch", "das", "menschli\u00b7che", "Ge\u00b7schlecht", "auf", "Er\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gewi\u00dflich nicht, um reich zu werden.", "tokens": ["Ge\u00b7wi\u00df\u00b7lich", "nicht", ",", "um", "reich", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$,", "KOUI", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn ob gleich du,", "tokens": ["Denn", "ob", "gleich", "du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Aus vielen Beuteln, Geld in deinen Beutel legest:", "tokens": ["Aus", "vie\u00b7len", "Beu\u00b7teln", ",", "Geld", "in", "dei\u00b7nen", "Beu\u00b7tel", "le\u00b7gest", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So h\u00f6rt es alles doch, wenn du es recht erwegest,", "tokens": ["So", "h\u00f6rt", "es", "al\u00b7les", "doch", ",", "wenn", "du", "es", "recht", "er\u00b7we\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADV", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dem Sch\u00f6pfer ja nicht minder zu,", "tokens": ["Dem", "Sch\u00f6p\u00b7fer", "ja", "nicht", "min\u00b7der", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als es ihm erst geh\u00f6rt, wie es bey andern war.", "tokens": ["Als", "es", "ihm", "erst", "ge\u00b7h\u00f6rt", ",", "wie", "es", "bey", "an\u00b7dern", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,", "PWAV", "PPER", "APPR", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was in der Erde liegt, was mir geh\u00f6rt, was dein,", "tokens": ["Was", "in", "der", "Er\u00b7de", "liegt", ",", "was", "mir", "ge\u00b7h\u00f6rt", ",", "was", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "PRELS", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ist alles sein:", "tokens": ["Ist", "al\u00b7les", "sein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "So ist dein Reichthum nicht sein Endzweck, das ist klar.", "tokens": ["So", "ist", "dein", "Reicht\u00b7hum", "nicht", "sein", "End\u00b7zweck", ",", "das", "ist", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PTKNEG", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Soll ich dir aber hier des Sch\u00f6pfers Absicht zeigen,", "tokens": ["Soll", "ich", "dir", "a\u00b7ber", "hier", "des", "Sch\u00f6p\u00b7fers", "Ab\u00b7sicht", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die er mit uns gehabt: So will ichs nicht verschweigen,", "tokens": ["Die", "er", "mit", "uns", "ge\u00b7habt", ":", "So", "will", "ichs", "nicht", "ver\u00b7schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VAPP", "$.", "ADV", "VMFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du sollt, wie Gottes Werk so sch\u00f6n,", "tokens": ["Du", "sollt", ",", "wie", "Got\u00b7tes", "Werk", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "NN", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "In deiner Lust, zu seinen Ehren,", "tokens": ["In", "dei\u00b7ner", "Lust", ",", "zu", "sei\u00b7nen", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Empfinden, schmecken, h\u00f6ren, sehn,", "tokens": ["Emp\u00b7fin\u00b7den", ",", "schme\u00b7cken", ",", "h\u00f6\u00b7ren", ",", "sehn", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVINF", "$,", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und dergestalt sein herrlich Lob vermehren.", "tokens": ["Und", "der\u00b7ge\u00b7stalt", "sein", "herr\u00b7lich", "Lob", "ver\u00b7meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}