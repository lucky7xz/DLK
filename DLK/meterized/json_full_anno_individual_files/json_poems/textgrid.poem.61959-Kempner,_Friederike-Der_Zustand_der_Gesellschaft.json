{"textgrid.poem.61959": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Der Zustand der Gesellschaft", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Erde bebte, gro\u00df, gewaltig wird ihr inn'res W\u00fcten,", "tokens": ["Die", "Er\u00b7de", "beb\u00b7te", ",", "gro\u00df", ",", "ge\u00b7wal\u00b7tig", "wird", "ihr", "inn'\u00b7res", "W\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "$,", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Und schwarz und finster war's und keine Sonnen gl\u00fchten.", "tokens": ["Und", "schwarz", "und", "fins\u00b7ter", "wa\u00b7r's", "und", "kei\u00b7ne", "Son\u00b7nen", "gl\u00fch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAFIN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ach, keine Bl\u00fcten und kein Rauschen, und kein Fr\u00fchlingswehen,", "tokens": ["Ach", ",", "kei\u00b7ne", "Bl\u00fc\u00b7ten", "und", "kein", "Rau\u00b7schen", ",", "und", "kein", "Fr\u00fch\u00b7lings\u00b7we\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "NN", "KON", "PIAT", "NN", "$,", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Die gro\u00dfe Nacht war d\u00fcster, schauerlich mit anzusehen.", "tokens": ["Die", "gro\u00b7\u00dfe", "Nacht", "war", "d\u00fcs\u00b7ter", ",", "schau\u00b7er\u00b7lich", "mit", "an\u00b7zu\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,", "ADJD", "APPR", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Da schallt des Donners Stimme und erweckt die stumme Nacht.", "tokens": ["Da", "schallt", "des", "Don\u00b7ners", "Stim\u00b7me", "und", "er\u00b7weckt", "die", "stum\u00b7me", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Des Blitzes Schein erhellt die Erde, die Menschheit sie erwacht.", "tokens": ["Des", "Blit\u00b7zes", "Schein", "er\u00b7hellt", "die", "Er\u00b7de", ",", "die", "Menschheit", "sie", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie \u00f6ffnet halb das m\u00fcde Auge, vom Schein zur\u00fcckgeschreckt.", "tokens": ["Sie", "\u00f6ff\u00b7net", "halb", "das", "m\u00fc\u00b7de", "Au\u00b7ge", ",", "vom", "Schein", "zu\u00b7r\u00fcck\u00b7ge\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Und schl\u00e4frig bleibt die Wimper liegen, die ihr das Licht versteckt.", "tokens": ["Und", "schl\u00e4f\u00b7rig", "bleibt", "die", "Wim\u00b7per", "lie\u00b7gen", ",", "die", "ihr", "das", "Licht", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.5": {"line.1": {"text": "Doch durch die zarten, kleinen H\u00e4rchen der gro\u00dfe Lichtstrahl dringt,", "tokens": ["Doch", "durch", "die", "zar\u00b7ten", ",", "klei\u00b7nen", "H\u00e4r\u00b7chen", "der", "gro\u00b7\u00dfe", "Licht\u00b7strahl", "dringt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Und golden es dem langen Schl\u00e4fer in's tr\u00fcbe Auge blinkt.", "tokens": ["Und", "gol\u00b7den", "es", "dem", "lan\u00b7gen", "Schl\u00e4\u00b7fer", "in's", "tr\u00fc\u00b7be", "Au\u00b7ge", "blinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.6": {"line.1": {"text": "Es folgt ein Blitz dem ersten Strahle, mit voller Blitzeskraft,", "tokens": ["Es", "folgt", "ein", "Blitz", "dem", "ers\u00b7ten", "Strah\u00b7le", ",", "mit", "vol\u00b7ler", "Blit\u00b7zes\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Die ganze Welt, sie steht in Flammen und hat sich aufgerafft.", "tokens": ["Die", "gan\u00b7ze", "Welt", ",", "sie", "steht", "in", "Flam\u00b7men", "und", "hat", "sich", "auf\u00b7ge\u00b7rafft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "KON", "VAFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.7": {"line.1": {"text": "Die Menschheit mit den edlen Z\u00fcgen, sie sieht den jungen Tag.", "tokens": ["Die", "Menschheit", "mit", "den", "ed\u00b7len", "Z\u00fc\u00b7gen", ",", "sie", "sieht", "den", "jun\u00b7gen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und macht sich auf vom finster'n Lager, wo sie im Schlafe lag.", "tokens": ["Und", "macht", "sich", "auf", "vom", "fins\u00b7ter'n", "La\u00b7ger", ",", "wo", "sie", "im", "Schla\u00b7fe", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "APPRART", "ADJA", "NN", "$,", "PWAV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.8": {"line.1": {"text": "Noch f\u00fchlt sie nicht den Rausch der Wonne, sie schreckt die Gegenwart,", "tokens": ["Noch", "f\u00fchlt", "sie", "nicht", "den", "Rausch", "der", "Won\u00b7ne", ",", "sie", "schreckt", "die", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Sie f\u00fchlt sich schwach, denn sie ist feige und ahnt, was ihrer harrt.", "tokens": ["Sie", "f\u00fchlt", "sich", "schwach", ",", "denn", "sie", "ist", "fei\u00b7ge", "und", "ahnt", ",", "was", "ih\u00b7rer", "harrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "$,", "KON", "PPER", "VAFIN", "ADJD", "KON", "VVFIN", "$,", "PRELS", "PPOSAT", "VVFIN", "$."], "meter": "-+-+---+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Sie konnt' das Finstre ja nicht schauen, was tat es ihr zu Leid?", "tokens": ["Sie", "konnt'", "das", "Finst\u00b7re", "ja", "nicht", "schau\u00b7en", ",", "was", "tat", "es", "ihr", "zu", "Leid", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$,", "PWS", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Jetzt sieht sie es, vom Licht erhellet und sieht es weit und breit.", "tokens": ["Jetzt", "sieht", "sie", "es", ",", "vom", "Licht", "er\u00b7hel\u00b7let", "und", "sieht", "es", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbich soll die Finsternis verscheuchen\u00ab, so ruft der Mensch und weint,", "tokens": ["\u00bb", "ich", "soll", "die", "Fins\u00b7ter\u00b7nis", "ver\u00b7scheu\u00b7chen", "\u00ab", ",", "so", "ruft", "der", "Mensch", "und", "weint", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "$(", "$,", "ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Die Finsternis wird gro\u00df und gr\u00f6\u00dfer, je n\u00e4her sie erscheint!", "tokens": ["Die", "Fins\u00b7ter\u00b7nis", "wird", "gro\u00df", "und", "gr\u00f6\u00b7\u00dfer", ",", "je", "n\u00e4\u00b7her", "sie", "er\u00b7scheint", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.11": {"line.1": {"text": "Ich will ihr nicht in's Auge sehen, der schwarzen H\u00f6llenbraut! \u2013", "tokens": ["Ich", "will", "ihr", "nicht", "in's", "Au\u00b7ge", "se\u00b7hen", ",", "der", "schwar\u00b7zen", "H\u00f6l\u00b7len\u00b7braut", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "In diesen Abgrund, der verzehret, wenn man hinunter schaut! \u2013", "tokens": ["In", "die\u00b7sen", "Ab\u00b7grund", ",", "der", "ver\u00b7zeh\u00b7ret", ",", "wenn", "man", "hin\u00b7un\u00b7ter", "schaut", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "VVFIN", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.12": {"line.1": {"text": "Die Menschheit m\u00f6chte wieder schlafen, und dr\u00fcckt ein Auge zu,", "tokens": ["Die", "Menschheit", "m\u00f6ch\u00b7te", "wie\u00b7der", "schla\u00b7fen", ",", "und", "dr\u00fcckt", "ein", "Au\u00b7ge", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVINF", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch auch im Herzen brennt die Flamme und ihr wird keine Ruh'!", "tokens": ["Doch", "auch", "im", "Her\u00b7zen", "brennt", "die", "Flam\u00b7me", "und", "ihr", "wird", "kei\u00b7ne", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "KON", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.13": {"line.1": {"text": "Die Erde bebte, gro\u00df, gewaltig wird ihr inn'res W\u00fcten,", "tokens": ["Die", "Er\u00b7de", "beb\u00b7te", ",", "gro\u00df", ",", "ge\u00b7wal\u00b7tig", "wird", "ihr", "inn'\u00b7res", "W\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "$,", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Und schwarz und finster war's und keine Sonnen gl\u00fchten.", "tokens": ["Und", "schwarz", "und", "fins\u00b7ter", "wa\u00b7r's", "und", "kei\u00b7ne", "Son\u00b7nen", "gl\u00fch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAFIN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "Ach, keine Bl\u00fcten und kein Rauschen, und kein Fr\u00fchlingswehen,", "tokens": ["Ach", ",", "kei\u00b7ne", "Bl\u00fc\u00b7ten", "und", "kein", "Rau\u00b7schen", ",", "und", "kein", "Fr\u00fch\u00b7lings\u00b7we\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "NN", "KON", "PIAT", "NN", "$,", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Die gro\u00dfe Nacht war d\u00fcster, schauerlich mit anzusehen.", "tokens": ["Die", "gro\u00b7\u00dfe", "Nacht", "war", "d\u00fcs\u00b7ter", ",", "schau\u00b7er\u00b7lich", "mit", "an\u00b7zu\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,", "ADJD", "APPR", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.15": {"line.1": {"text": "Da schallt des Donners Stimme und erweckt die stumme Nacht.", "tokens": ["Da", "schallt", "des", "Don\u00b7ners", "Stim\u00b7me", "und", "er\u00b7weckt", "die", "stum\u00b7me", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Des Blitzes Schein erhellt die Erde, die Menschheit sie erwacht.", "tokens": ["Des", "Blit\u00b7zes", "Schein", "er\u00b7hellt", "die", "Er\u00b7de", ",", "die", "Menschheit", "sie", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.16": {"line.1": {"text": "Sie \u00f6ffnet halb das m\u00fcde Auge, vom Schein zur\u00fcckgeschreckt.", "tokens": ["Sie", "\u00f6ff\u00b7net", "halb", "das", "m\u00fc\u00b7de", "Au\u00b7ge", ",", "vom", "Schein", "zu\u00b7r\u00fcck\u00b7ge\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Und schl\u00e4frig bleibt die Wimper liegen, die ihr das Licht versteckt.", "tokens": ["Und", "schl\u00e4f\u00b7rig", "bleibt", "die", "Wim\u00b7per", "lie\u00b7gen", ",", "die", "ihr", "das", "Licht", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.17": {"line.1": {"text": "Doch durch die zarten, kleinen H\u00e4rchen der gro\u00dfe Lichtstrahl dringt,", "tokens": ["Doch", "durch", "die", "zar\u00b7ten", ",", "klei\u00b7nen", "H\u00e4r\u00b7chen", "der", "gro\u00b7\u00dfe", "Licht\u00b7strahl", "dringt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Und golden es dem langen Schl\u00e4fer in's tr\u00fcbe Auge blinkt.", "tokens": ["Und", "gol\u00b7den", "es", "dem", "lan\u00b7gen", "Schl\u00e4\u00b7fer", "in's", "tr\u00fc\u00b7be", "Au\u00b7ge", "blinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.18": {"line.1": {"text": "Es folgt ein Blitz dem ersten Strahle, mit voller Blitzeskraft,", "tokens": ["Es", "folgt", "ein", "Blitz", "dem", "ers\u00b7ten", "Strah\u00b7le", ",", "mit", "vol\u00b7ler", "Blit\u00b7zes\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Die ganze Welt, sie steht in Flammen und hat sich aufgerafft.", "tokens": ["Die", "gan\u00b7ze", "Welt", ",", "sie", "steht", "in", "Flam\u00b7men", "und", "hat", "sich", "auf\u00b7ge\u00b7rafft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "KON", "VAFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.19": {"line.1": {"text": "Die Menschheit mit den edlen Z\u00fcgen, sie sieht den jungen Tag.", "tokens": ["Die", "Menschheit", "mit", "den", "ed\u00b7len", "Z\u00fc\u00b7gen", ",", "sie", "sieht", "den", "jun\u00b7gen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und macht sich auf vom finster'n Lager, wo sie im Schlafe lag.", "tokens": ["Und", "macht", "sich", "auf", "vom", "fins\u00b7ter'n", "La\u00b7ger", ",", "wo", "sie", "im", "Schla\u00b7fe", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "APPRART", "ADJA", "NN", "$,", "PWAV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.20": {"line.1": {"text": "Noch f\u00fchlt sie nicht den Rausch der Wonne, sie schreckt die Gegenwart,", "tokens": ["Noch", "f\u00fchlt", "sie", "nicht", "den", "Rausch", "der", "Won\u00b7ne", ",", "sie", "schreckt", "die", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Sie f\u00fchlt sich schwach, denn sie ist feige und ahnt, was ihrer harrt.", "tokens": ["Sie", "f\u00fchlt", "sich", "schwach", ",", "denn", "sie", "ist", "fei\u00b7ge", "und", "ahnt", ",", "was", "ih\u00b7rer", "harrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "$,", "KON", "PPER", "VAFIN", "ADJD", "KON", "VVFIN", "$,", "PRELS", "PPOSAT", "VVFIN", "$."], "meter": "-+-+---+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.21": {"line.1": {"text": "Sie konnt' das Finstre ja nicht schauen, was tat es ihr zu Leid?", "tokens": ["Sie", "konnt'", "das", "Finst\u00b7re", "ja", "nicht", "schau\u00b7en", ",", "was", "tat", "es", "ihr", "zu", "Leid", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$,", "PWS", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Jetzt sieht sie es, vom Licht erhellet und sieht es weit und breit.", "tokens": ["Jetzt", "sieht", "sie", "es", ",", "vom", "Licht", "er\u00b7hel\u00b7let", "und", "sieht", "es", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.22": {"line.1": {"text": "\u00bbich soll die Finsternis verscheuchen\u00ab, so ruft der Mensch und weint,", "tokens": ["\u00bb", "ich", "soll", "die", "Fins\u00b7ter\u00b7nis", "ver\u00b7scheu\u00b7chen", "\u00ab", ",", "so", "ruft", "der", "Mensch", "und", "weint", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "$(", "$,", "ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Die Finsternis wird gro\u00df und gr\u00f6\u00dfer, je n\u00e4her sie erscheint!", "tokens": ["Die", "Fins\u00b7ter\u00b7nis", "wird", "gro\u00df", "und", "gr\u00f6\u00b7\u00dfer", ",", "je", "n\u00e4\u00b7her", "sie", "er\u00b7scheint", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.23": {"line.1": {"text": "Ich will ihr nicht in's Auge sehen, der schwarzen H\u00f6llenbraut! \u2013", "tokens": ["Ich", "will", "ihr", "nicht", "in's", "Au\u00b7ge", "se\u00b7hen", ",", "der", "schwar\u00b7zen", "H\u00f6l\u00b7len\u00b7braut", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "In diesen Abgrund, der verzehret, wenn man hinunter schaut! \u2013", "tokens": ["In", "die\u00b7sen", "Ab\u00b7grund", ",", "der", "ver\u00b7zeh\u00b7ret", ",", "wenn", "man", "hin\u00b7un\u00b7ter", "schaut", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "VVFIN", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.24": {"line.1": {"text": "Die Menschheit m\u00f6chte wieder schlafen, und dr\u00fcckt ein Auge zu,", "tokens": ["Die", "Menschheit", "m\u00f6ch\u00b7te", "wie\u00b7der", "schla\u00b7fen", ",", "und", "dr\u00fcckt", "ein", "Au\u00b7ge", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVINF", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch auch im Herzen brennt die Flamme und ihr wird keine Ruh'!", "tokens": ["Doch", "auch", "im", "Her\u00b7zen", "brennt", "die", "Flam\u00b7me", "und", "ihr", "wird", "kei\u00b7ne", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "KON", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}}}}