{"textgrid.poem.47066": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich tr\u00e4umt', ich w\u00e4r' ein V\u00f6gelein und fl\u00f6ge", "tokens": ["Ich", "tr\u00e4umt'", ",", "ich", "w\u00e4r'", "ein", "V\u00f6\u00b7ge\u00b7lein", "und", "fl\u00f6\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinaus zu ihr mit einer Schar von Ammern,", "tokens": ["Hin\u00b7aus", "zu", "ihr", "mit", "ei\u00b7ner", "Schar", "von", "Am\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die drau\u00dfen jetzt vor ihrem Fenster jammern,", "tokens": ["Die", "drau\u00b7\u00dfen", "jetzt", "vor", "ih\u00b7rem", "Fens\u00b7ter", "jam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis sie mit L\u00e4cheln ihnen f\u00fcllt die Tr\u00f6ge.", "tokens": ["Bis", "sie", "mit", "L\u00e4\u00b7cheln", "ih\u00b7nen", "f\u00fcllt", "die", "Tr\u00f6\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wenn der Schwarm ges\u00e4ttigt weiterz\u00f6ge,", "tokens": ["Und", "wenn", "der", "Schwarm", "ge\u00b7s\u00e4t\u00b7tigt", "wei\u00b7ter\u00b7z\u00f6\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Blieb' ich, um an ihr Kleid mich anzuklammern,", "tokens": ["Blieb'", "ich", ",", "um", "an", "ihr", "Kleid", "mich", "an\u00b7zu\u00b7klam\u00b7mern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUI", "APPR", "PPOSAT", "NN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis sie, sich mein erbarmend, in die Kammern", "tokens": ["Bis", "sie", ",", "sich", "mein", "er\u00b7bar\u00b7mend", ",", "in", "die", "Kam\u00b7mern"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PRF", "PPOSAT", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich mit sich n\u00e4hme und mich drinnen pfl\u00f6ge.", "tokens": ["Mich", "mit", "sich", "n\u00e4h\u00b7me", "und", "mich", "drin\u00b7nen", "pfl\u00f6\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PRF", "VVFIN", "KON", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dann th\u00e4t' ich so erfroren und erstarret,", "tokens": ["Dann", "th\u00e4t'", "ich", "so", "er\u00b7fro\u00b7ren", "und", "er\u00b7star\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sie aus Mitleid in den Busen nieder", "tokens": ["Da\u00df", "s