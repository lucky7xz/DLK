{"dta.poem.14012": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachtigall .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Nach den sch\u00f6nen Fr\u00fchlingstagen,", "tokens": ["Nach", "den", "sch\u00f6\u00b7nen", "Fr\u00fch\u00b7lings\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die blauen L\u00fcfte wehen,", "tokens": ["Wenn", "die", "blau\u00b7en", "L\u00fcf\u00b7te", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcnsche mit dem Fl\u00fcgel schlagen", "tokens": ["W\u00fcn\u00b7sche", "mit", "dem", "Fl\u00fc\u00b7gel", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und im Gr\u00fcnen Amor zielt,", "tokens": ["Und", "im", "Gr\u00fc\u00b7nen", "A\u00b7mor", "zielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bleibt ein Jauchzen auf den H\u00f6hen;", "tokens": ["Bleibt", "ein", "Jauch\u00b7zen", "auf", "den", "H\u00f6\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ein Wetterleuchten spielt", "tokens": ["Und", "ein", "Wet\u00b7ter\u00b7leuch\u00b7ten", "spielt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Aus der Ferne durch die B\u00e4ume", "tokens": ["Aus", "der", "Fer\u00b7ne", "durch", "die", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wunderbar die ganze Nacht,", "tokens": ["Wun\u00b7der\u00b7bar", "die", "gan\u00b7ze", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df die Nachtigall erwacht", "tokens": ["Da\u00df", "die", "Nach\u00b7ti\u00b7gall", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Von den irren Widerscheinen,", "tokens": ["Von", "den", "ir\u00b7ren", "Wi\u00b7der\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und durch alle seel'ge Gr\u00fcnde", "tokens": ["Und", "durch", "al\u00b7le", "seel'\u00b7ge", "Gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "In der Einsamkeit verk\u00fcnde,", "tokens": ["In", "der", "Ein\u00b7sam\u00b7keit", "ver\u00b7k\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Was sie alle, alle meinen;", "tokens": ["Was", "sie", "al\u00b7le", ",", "al\u00b7le", "mei\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "PIS", "$,", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Dieses Rauschen in den B\u00e4umen", "tokens": ["Die\u00b7ses", "Rau\u00b7schen", "in", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und der Mensch in dunkeln Tr\u00e4umen.", "tokens": ["Und", "der", "Mensch", "in", "dun\u00b7keln", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}