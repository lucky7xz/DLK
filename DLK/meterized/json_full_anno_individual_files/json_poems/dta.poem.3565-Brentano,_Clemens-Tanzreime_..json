{"dta.poem.3565": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Tanzreime .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich weis nicht wo's V\u00f6glein ist,               ", "tokens": ["Ich", "weis", "nicht", "wo's", "V\u00f6\u00b7glein", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "PTKNEG", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich wei\u00df nicht wo's pfeift;", "tokens": ["Ich", "wei\u00df", "nicht", "wo's", "pfeift", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NE", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Hinterm kleinen L\u00e4delein,", "tokens": ["Hin\u00b7term", "klei\u00b7nen", "L\u00e4\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00e4tzlein wo leist?", "tokens": ["Sch\u00e4tz\u00b7lein", "wo", "leist", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PWAV", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Es sitzt ja das V\u00f6glein", "tokens": ["Es", "sitzt", "ja", "das", "V\u00f6\u00b7glein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nicht alleweil im Nest;", "tokens": ["Nicht", "al\u00b7le\u00b7weil", "im", "Nest", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schwingt seine Fl\u00fcgelein,", "tokens": ["Schwingt", "sei\u00b7ne", "Fl\u00fc\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "H\u00fcpft auf die Aest.", "tokens": ["H\u00fcpft", "auf", "die", "A\u00b7est", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wo ich gelegen bin,", "tokens": ["Wo", "ich", "ge\u00b7le\u00b7gen", "bin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Darf ich wohl sagen;", "tokens": ["Darf", "ich", "wohl", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Hinterm gr\u00fcn N\u00e4geleinstock", "tokens": ["Hin\u00b7term", "gr\u00fcn", "N\u00e4\u00b7ge\u00b7lein\u00b7stock"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Zwischen zwei Knaben.", "tokens": ["Zwi\u00b7schen", "zwei", "Kna\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}