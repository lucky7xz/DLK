{"dta.poem.5269": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es pr\u00e4gt der Sonnen heitrer Schein", "tokens": ["Es", "pr\u00e4gt", "der", "Son\u00b7nen", "hei\u00b7trer", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jm Lentzen ein recht l\u00e4chelnd Wesen", "tokens": ["Jm", "Lent\u00b7zen", "ein", "recht", "l\u00e4\u00b7chelnd", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJD", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Fast einem jeden Vorwurf ein,", "tokens": ["Fast", "ei\u00b7nem", "je\u00b7den", "Vor\u00b7wurf", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und was man nah und fern erblickt", "tokens": ["Und", "was", "man", "nah", "und", "fern", "er\u00b7blickt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PIS", "ADJD", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Scheint uns zur Anmuht neu geschm\u00fcckt.", "tokens": ["Scheint", "uns", "zur", "An\u00b7muht", "neu", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der reinen L\u00fcfte blaue Glut", "tokens": ["Der", "rei\u00b7nen", "L\u00fcf\u00b7te", "blau\u00b7e", "Glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00e4rbt lieblich blau die glatte Flut,", "tokens": ["F\u00e4rbt", "lieb\u00b7lich", "blau", "die", "glat\u00b7te", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein gr\u00fcner Glantz bedeckt die W\u00e4lder,", "tokens": ["Ein", "gr\u00fc\u00b7ner", "Glantz", "be\u00b7deckt", "die", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ein bunter, die bebl\u00fchmten Felder;", "tokens": ["Ein", "bun\u00b7ter", ",", "die", "be\u00b7bl\u00fchm\u00b7ten", "Fel\u00b7der", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Es schwimmt jetzt \u00fcberall, zur Anmuth unsrer Brust,", "tokens": ["Es", "schwimmt", "jetzt", "\u00fc\u00b7be\u00b7rall", ",", "zur", "An\u00b7muth", "uns\u00b7rer", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der rege Blick in lauter Lust.", "tokens": ["Der", "re\u00b7ge", "Blick", "in", "lau\u00b7ter", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Man sieht jetzt, durch die hell- und reine Heiterkeit", "tokens": ["Man", "sieht", "jetzt", ",", "durch", "die", "hell", "und", "rei\u00b7ne", "Hei\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "$,", "APPR", "ART", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der hell- und reinen Sonnen Strahlen,", "tokens": ["Der", "hell", "und", "rei\u00b7nen", "Son\u00b7nen", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Worin, zumahl zur Fr\u00fchling Zeit,", "tokens": ["Wo\u00b7rin", ",", "zu\u00b7mahl", "zur", "Fr\u00fch\u00b7ling", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ein jeder Vorwurf gleichsam schwimmt,", "tokens": ["Ein", "je\u00b7der", "Vor\u00b7wurf", "gleich\u00b7sam", "schwimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wodurch fast jeder Vorwurf glimmt,", "tokens": ["Wo\u00b7durch", "fast", "je\u00b7der", "Vor\u00b7wurf", "glimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Sich jeden Vorwurf herrlich mahlen.", "tokens": ["Sich", "je\u00b7den", "Vor\u00b7wurf", "herr\u00b7lich", "mah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Es blickt (nicht hie und da, ein Vorwurf nur)", "tokens": ["Es", "blickt", "(", "nicht", "hie", "und", "da", ",", "ein", "Vor\u00b7wurf", "nur", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PTKNEG", "ADV", "KON", "ADV", "$,", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Die gantze sichtbare Natur", "tokens": ["Die", "gant\u00b7ze", "sicht\u00b7ba\u00b7re", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Uns \u00fcberall jetzt gleichsam l\u00e4chelnd an.", "tokens": ["Uns", "\u00fc\u00b7be\u00b7rall", "jetzt", "gleich\u00b7sam", "l\u00e4\u00b7chelnd", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ach, la\u00dft doch denn auch uns, durchs Lachen unsrer Erden,", "tokens": ["Ach", ",", "la\u00dft", "doch", "denn", "auch", "uns", ",", "durchs", "La\u00b7chen", "uns\u00b7rer", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "ADV", "ADV", "ADV", "PPER", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ein Lachen zugerichtet werden!", "tokens": ["Ein", "La\u00b7chen", "zu\u00b7ge\u00b7rich\u00b7tet", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Ach, m\u00f6chte doch, wenn man die Welt so sch\u00f6n geschm\u00fcckt,", "tokens": ["Ach", ",", "m\u00f6ch\u00b7te", "doch", ",", "wenn", "man", "die", "Welt", "so", "sch\u00f6n", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "ADV", "$,", "KOUS", "PIS", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mit Andacht und mit Lust, zur Fr\u00fchlings-Zeit erblickt,", "tokens": ["Mit", "An\u00b7dacht", "und", "mit", "Lust", ",", "zur", "Fr\u00fch\u00b7lings\u00b7Zeit", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Auch unser Geist dadurch geschm\u00fccket werden,", "tokens": ["Auch", "un\u00b7ser", "Geist", "da\u00b7durch", "ge\u00b7schm\u00fc\u00b7cket", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PAV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Da\u00df uns der Erden Pracht und Schmuck und Sch\u00f6nheit", "tokens": ["Da\u00df", "uns", "der", "Er\u00b7den", "Pracht", "und", "Schmuck", "und", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "r\u00fchren", "tokens": ["r\u00fch\u00b7ren"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.28": {"text": "Und uns zu ihrer Quell, zum grossen Sch\u00f6pfer, f\u00fchren!", "tokens": ["Und", "uns", "zu", "ih\u00b7rer", "Quell", ",", "zum", "gros\u00b7sen", "Sch\u00f6p\u00b7fer", ",", "f\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "$,", "APPRART", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ach, m\u00f6chte doch, durchs Gr\u00fcne dieser Erden,", "tokens": ["Ach", ",", "m\u00f6ch\u00b7te", "doch", ",", "durchs", "Gr\u00fc\u00b7ne", "die\u00b7ser", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "ADV", "$,", "APPRART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Der Hofnung geistig gr\u00fcn, die frohe Seele zieren!", "tokens": ["Der", "Hof\u00b7nung", "geis\u00b7tig", "gr\u00fcn", ",", "die", "fro\u00b7he", "See\u00b7le", "zie\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}