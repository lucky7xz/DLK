{"dta.poem.21899": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "2. Die Ewige Ruhest\u00e4dt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Es mag ein andrer sich umb sein Begr\u00e4bni\u00df kr\u00e4nken/", "tokens": ["Es", "mag", "ein", "an\u00b7drer", "sich", "umb", "sein", "Be\u00b7gr\u00e4b\u00b7ni\u00df", "kr\u00e4n\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und seinen Madensak mit st\u00f6ltzen Bau bedenken.", "tokens": ["Und", "sei\u00b7nen", "Ma\u00b7den\u00b7sak", "mit", "st\u00b7\u00f6lt\u00b7zen", "Bau", "be\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich Sorge nicht daf\u00fcr: Mein Grab/ mein Fel\u00df und", "tokens": ["Ich", "Sor\u00b7ge", "nicht", "da\u00b7f\u00fcr", ":", "Mein", "Grab", "/", "mein", "Fel\u00df", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "PTKNEG", "PAV", "$.", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In dem ich ewig Ruh/ sol\u2019s Hertze JEsu seyn. (schrein", "tokens": ["In", "dem", "ich", "e\u00b7wig", "Ruh", "/", "sol's", "Hert\u00b7ze", "Je\u00b7su", "seyn", ".", "(", "schrein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word