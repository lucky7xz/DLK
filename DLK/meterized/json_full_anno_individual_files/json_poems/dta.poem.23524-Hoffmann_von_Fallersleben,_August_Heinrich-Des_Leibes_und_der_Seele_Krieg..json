{"dta.poem.23524": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Des Leibes und der Seele Krieg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nach Seelen wird die Z\u00e4hlung nur gemacht,", "tokens": ["Nach", "See\u00b7len", "wird", "die", "Z\u00e4h\u00b7lung", "nur", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nach K\u00f6pfen wird die Steuer aufgebracht.", "tokens": ["Nach", "K\u00f6p\u00b7fen", "wird", "die", "Steu\u00b7er", "auf\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da dachtet ihr, der Leib hat seine Rechte", "tokens": ["Da", "dach\u00b7tet", "ihr", ",", "der", "Leib", "hat", "sei\u00b7ne", "Rech\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wie man ihn in Reih' und Glied wohl br\u00e4chte.", "tokens": ["Und", "wie", "man", "ihn", "in", "Reih'", "und", "Glied", "wohl", "br\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "PPER", "APPR", "NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da fing mit einem Mal das Turnen an,", "tokens": ["Da", "fing", "mit", "ei\u00b7nem", "Mal", "das", "Tur\u00b7nen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wer nicht turnte, war kein biderb Mann;", "tokens": ["Und", "wer", "nicht", "turn\u00b7te", ",", "war", "kein", "bi\u00b7derb", "Mann", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "VVFIN", "$,", "VAFIN", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man sang vom Barrn, Rung, Reck und Schwingel Lieder", "tokens": ["Man", "sang", "vom", "Barrn", ",", "Rung", ",", "Reck", "und", "Schwin\u00b7gel", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Deutschland hallte freudig alles wieder.", "tokens": ["Und", "Deutschland", "hall\u00b7te", "freu\u00b7dig", "al\u00b7les", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ADJD", "PIS", "ADV", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Da kam die Polizei euch auf den Leib:", "tokens": ["Da", "kam", "die", "Po\u00b7li\u00b7zei", "euch", "auf", "den", "Leib", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was soll der demagogische Zeitvertreib?", "tokens": ["Was", "soll", "der", "de\u00b7ma\u00b7go\u00b7gi\u00b7sche", "Zeit\u00b7ver\u00b7treib", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Staat will K\u00f6pf' und Seelen, doch mit Nichten", "tokens": ["Der", "Staat", "will", "K\u00f6pf'", "und", "See\u00b7len", ",", "doch", "mit", "Nich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "NE", "KON", "NN", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Turnleiber, so die Steuer nicht entrichten.", "tokens": ["Turn\u00b7lei\u00b7ber", ",", "so", "die", "Steu\u00b7er", "nicht", "ent\u00b7rich\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Staat beschr\u00e4nkte drum das Turnen nur", "tokens": ["Der", "Staat", "be\u00b7schr\u00e4nk\u00b7te", "drum", "das", "Tur\u00b7nen", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PAV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf edle fromme geistige Dressur.", "tokens": ["Auf", "ed\u00b7le", "from\u00b7me", "geis\u00b7ti\u00b7ge", "Dres\u00b7sur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was lerntet ihr doch auch vom Schwingen, Recken?", "tokens": ["Was", "lern\u00b7tet", "ihr", "doch", "auch", "vom", "Schwin\u00b7gen", ",", "Re\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr lerntet nur euch nach der Decke strecken.", "tokens": ["Ihr", "lern\u00b7tet", "nur", "euch", "nach", "der", "De\u00b7cke", "stre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}