ine.16": {"text": "F\u00fchrt' uns zusammen, und schnell fesselt der spielende Gott.", "tokens": ["F\u00fchrt'", "uns", "zu\u00b7sam\u00b7men", ",", "und", "schnell", "fes\u00b7selt", "der", "spie\u00b7len\u00b7de", "Gott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.17": {"text": "Damals nannt' ich dich Gn\u00e4dige noch, doch ahnte mein Herz schon,", "tokens": ["Da\u00b7mals", "nannt'", "ich", "dich", "Gn\u00e4\u00b7di\u00b7ge", "noch", ",", "doch", "ahn\u00b7te", "mein", "Herz", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "NN", "ADV", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "--+-++--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Freundliche w\u00fcrd' ich dich einst nennen und reizende Frau.", "tokens": ["Freund\u00b7li\u00b7che", "w\u00fcrd'", "ich", "dich", "einst", "nen\u00b7nen", "und", "rei\u00b7zen\u00b7de", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PRF", "ADV", "VVINF", "KON", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.19": {"text": "Schnell wie die Bien' um den rosigen Kelch, so spielt um den Mund dir", "tokens": ["Schnell", "wie", "die", "Bi\u00b7en'", "um", "den", "ro\u00b7si\u00b7gen", "Kelch", ",", "so", "spielt", "um", "den", "Mund", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "APPR", "ART", "NN", "PPER"], "meter": "-+--+--+--+-+--