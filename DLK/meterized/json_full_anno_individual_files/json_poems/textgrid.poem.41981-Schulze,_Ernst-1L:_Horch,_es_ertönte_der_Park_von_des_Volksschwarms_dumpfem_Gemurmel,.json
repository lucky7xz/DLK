{"textgrid.poem.41981": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Horch, es ert\u00f6nte der Park von des Volksschwarms dumpfem Gemurmel,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Horch, es ert\u00f6nte der Park von des Volksschwarms dumpfem Gemurmel,", "tokens": ["Horch", ",", "es", "er\u00b7t\u00f6n\u00b7te", "der", "Park", "von", "des", "Volks\u00b7schwarms", "dum\u00b7pfem", "Ge\u00b7mur\u00b7mel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-++-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Rasch in den G\u00e4ngen des Hains wogte die Welle der Lust;", "tokens": ["Rasch", "in", "den", "G\u00e4n\u00b7gen", "des", "Hains", "wog\u00b7te", "die", "Wel\u00b7le", "der", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Freund' und Freunde begegneten hier sich im bunten Gedr\u00e4nge:", "tokens": ["Freund'", "und", "Freun\u00b7de", "be\u00b7ge\u00b7gne\u00b7ten", "hier", "sich", "im", "bun\u00b7ten", "Ge\u00b7dr\u00e4n\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Traulich vereinte das Fest Jene, die nimmer sich sahn.", "tokens": ["Trau\u00b7lich", "ver\u00b7ein\u00b7te", "das", "Fest", "Je\u00b7ne", ",", "die", "nim\u00b7mer", "sich", "sahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PDAT", "$,", "PRELS", "ADV", "PRF", "VVFIN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Rauschend streifte das seidne Gewand an dem leinenen Kleid hin,", "tokens": ["Rau\u00b7schend", "streif\u00b7te", "das", "seid\u00b7ne", "Ge\u00b7wand", "an", "dem", "lei\u00b7ne\u00b7nen", "Kleid", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Gegen die Perle von Glas blitzte der goldene Schmuck;", "tokens": ["Ge\u00b7gen", "die", "Per\u00b7le", "von", "Glas", "blitz\u00b7te", "der", "gol\u00b7de\u00b7ne", "Schmuck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Was sich gepflegt in der Sonne des Gl\u00fccks, was bang' in des Ungl\u00fccks", "tokens": ["Was", "sich", "ge\u00b7pflegt", "in", "der", "Son\u00b7ne", "des", "Gl\u00fccks", ",", "was", "bang'", "in", "des", "Un\u00b7gl\u00fccks"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "VVPP", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Frosthauch bebte, vereint prangt' es im Kranze der Lust.", "tokens": ["Frost\u00b7hauch", "beb\u00b7te", ",", "ver\u00b7eint", "prangt'", "es", "im", "Kran\u00b7ze", "der", "Lust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVPP", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Magisch umflo\u00df das romantische Bild hellschimmernd der Lampen", "tokens": ["Ma\u00b7gisch", "um\u00b7flo\u00df", "das", "ro\u00b7man\u00b7ti\u00b7sche", "Bild", "hell\u00b7schim\u00b7mernd", "der", "Lam\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "VVPP", "ART", "NN"], "meter": "---+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Irrender Schein, durchs Gr\u00fcn gaukelte golden der Glanz;", "tokens": ["Ir\u00b7ren\u00b7der", "Schein", ",", "durchs", "Gr\u00fcn", "gau\u00b7kel\u00b7te", "gol\u00b7den", "der", "Glanz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPRART", "NN", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.11": {"text": "Ringsum schien's als s\u00e4nken zum irdischen Pfad die Gestirne,", "tokens": ["Ring\u00b7sum", "schien's", "als", "s\u00e4n\u00b7ken", "zum", "ir\u00b7di\u00b7schen", "Pfad", "die", "Ge\u00b7stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Da\u00df sich dem Himmel genaht w\u00e4hnte das selige Herz.", "tokens": ["Da\u00df", "sich", "dem", "Him\u00b7mel", "ge\u00b7naht", "w\u00e4hn\u00b7te", "das", "se\u00b7li\u00b7ge", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVPP", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Beide durchwandelten wir des Gestr\u00e4uchs dufthauchende D\u00e4mmrung,", "tokens": ["Bei\u00b7de", "durch\u00b7wan\u00b7del\u00b7ten", "wir", "des", "Ge\u00b7str\u00e4uchs", "duf\u00b7thau\u00b7chen\u00b7de", "D\u00e4mm\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Traulich schmiegte dein Arm weich an den meinigen sich;", "tokens": ["Trau\u00b7lich", "schmieg\u00b7te", "dein", "Arm", "weich", "an", "den", "mei\u00b7ni\u00b7gen", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "ADJD", "APPR", "ART", "PPOSS", "PRF", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Kaum erst kannten wir uns seit wenigen Stunden, der Zufall", "tokens": ["Kaum", "erst", "kann\u00b7ten", "wir", "uns", "seit", "we\u00b7ni\u00b7gen", "Stun\u00b7den", ",", "der", "Zu\u00b7fall"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "$,", "ART", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "F\u00fchrt' uns zusammen, und schnell fesselt der spielende Gott.", "tokens": ["F\u00fchrt'", "uns", "zu\u00b7sam\u00b7men", ",", "und", "schnell", "fes\u00b7selt", "der", "spie\u00b7len\u00b7de", "Gott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.17": {"text": "Damals nannt' ich dich Gn\u00e4dige noch, doch ahnte mein Herz schon,", "tokens": ["Da\u00b7mals", "nannt'", "ich", "dich", "Gn\u00e4\u00b7di\u00b7ge", "noch", ",", "doch", "ahn\u00b7te", "mein", "Herz", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "NN", "ADV", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "--+-++--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Freundliche w\u00fcrd' ich dich einst nennen und reizende Frau.", "tokens": ["Freund\u00b7li\u00b7che", "w\u00fcrd'", "ich", "dich", "einst", "nen\u00b7nen", "und", "rei\u00b7zen\u00b7de", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PRF", "ADV", "VVINF", "KON", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.19": {"text": "Schnell wie die Bien' um den rosigen Kelch, so spielt um den Mund dir", "tokens": ["Schnell", "wie", "die", "Bi\u00b7en'", "um", "den", "ro\u00b7si\u00b7gen", "Kelch", ",", "so", "spielt", "um", "den", "Mund", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "APPR", "ART", "NN", "PPER"], "meter": "-+--+--+--+-+--+-", "measure": "amphibrach.tetra.plus"}, "line.20": {"text": "T\u00e4ndelnder Witz, doch er hielt friedlich den Stachel zur\u00fcck.", "tokens": ["T\u00e4n\u00b7deln\u00b7der", "Witz", ",", "doch", "er", "hielt", "fried\u00b7lich", "den", "Sta\u00b7chel", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Fr\u00f6hlich entschwamm auf dem Silbergew\u00f6lk des beweglichen Leichtsinns,", "tokens": ["Fr\u00f6h\u00b7lich", "ent\u00b7schwamm", "auf", "dem", "Sil\u00b7ber\u00b7ge\u00b7w\u00f6lk", "des", "be\u00b7weg\u00b7li\u00b7chen", "Leicht\u00b7sinns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Haschend die wechselnde Lust, rasch der befl\u00fcgelte Geist:", "tokens": ["Ha\u00b7schend", "die", "wech\u00b7seln\u00b7de", "Lust", ",", "rasch", "der", "be\u00b7fl\u00fc\u00b7gel\u00b7te", "Geist", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.23": {"text": "Doch oft kettet' ihn s\u00fc\u00df das Gef\u00fchl, und den luftigen Fl\u00fcchtling", "tokens": ["Doch", "oft", "ket\u00b7tet'", "ihn", "s\u00fc\u00df", "das", "Ge\u00b7f\u00fchl", ",", "und", "den", "luf\u00b7ti\u00b7gen", "Fl\u00fccht\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "F\u00fchrt' in des zarteren Gl\u00fccks stillere Lauben die Huld.", "tokens": ["F\u00fchrt'", "in", "des", "zar\u00b7te\u00b7ren", "Gl\u00fccks", "stil\u00b7le\u00b7re", "Lau\u00b7ben", "die", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Vielfach wandest du mir um den Sinn die bezaubernde Fessel,", "tokens": ["Viel\u00b7fach", "wan\u00b7dest", "du", "mir", "um", "den", "Sinn", "die", "be\u00b7zau\u00b7bern\u00b7de", "Fes\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "M\u00e4chtiger ward dein Reiz stets mit des Sieges Begier,", "tokens": ["M\u00e4ch\u00b7ti\u00b7ger", "ward", "dein", "Reiz", "stets", "mit", "des", "Sie\u00b7ges", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.27": {"text": "L\u00e4chelnd sahst du mich an, und es schwebt' in dem Blick der Triumph dir,", "tokens": ["L\u00e4\u00b7chelnd", "sahst", "du", "mich", "an", ",", "und", "es", "schwebt'", "in", "dem", "Blick", "der", "Tri\u00b7umph", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PPER", "$,"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "Da\u00df du gesiegt, und leicht schien dir der spielende Kampf.", "tokens": ["Da\u00df", "du", "ge\u00b7siegt", ",", "und", "leicht", "schien", "dir", "der", "spie\u00b7len\u00b7de", "Kampf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.29": {"text": "Aber dich trog dein Wahn, Schlausinnige; sieh, es ermannte", "tokens": ["A\u00b7ber", "dich", "trog", "dein", "Wahn", ",", "Schlau\u00b7sin\u00b7ni\u00b7ge", ";", "sieh", ",", "es", "er\u00b7mann\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "NN", "$.", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Rasch sich der Geist, vom Netz wand der Gefangne sich los.", "tokens": ["Rasch", "sich", "der", "Geist", ",", "vom", "Netz", "wand", "der", "Ge\u00b7fang\u00b7ne", "sich", "los", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "ART", "NN", "$,", "APPRART", "NN", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.31": {"text": "Keck nun hob er das eigne Panier, und es k\u00e4mpfte mit Leichtsinn", "tokens": ["Keck", "nun", "hob", "er", "das", "eig\u00b7ne", "Pa\u00b7nier", ",", "und", "es", "k\u00e4mpf\u00b7te", "mit", "Leicht\u00b7sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "APPR", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.32": {"text": "Leichtsinn, und mit dem Stolz rang der erwachende Stolz.", "tokens": ["Leicht\u00b7sinn", ",", "und", "mit", "dem", "Stolz", "rang", "der", "er\u00b7wa\u00b7chen\u00b7de", "Stolz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.33": {"text": "Was du mich lehrtest, ich gab es zur\u00fcck, und ein sch\u00fctzender Gott lieh", "tokens": ["Was", "du", "mich", "lehr\u00b7test", ",", "ich", "gab", "es", "zu\u00b7r\u00fcck", ",", "und", "ein", "sch\u00fct\u00b7zen\u00b7der", "Gott", "lieh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "PRF", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.34": {"text": "Geist mir und Wort und der List t\u00e4uschendes Gaukelgewand.", "tokens": ["Geist", "mir", "und", "Wort", "und", "der", "List", "t\u00e4u\u00b7schen\u00b7des", "Gau\u00b7kel\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "NN", "KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.35": {"text": "Doch es entstahl der Idalische Gott schlau sp\u00e4hend des Schicksals", "tokens": ["Doch", "es", "ent\u00b7stahl", "der", "I\u00b7da\u00b7li\u00b7sche", "Gott", "schlau", "sp\u00e4\u00b7hend", "des", "Schick\u00b7sals"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ART", "ADJA", "NN", "ADJD", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.36": {"text": "Wage dem Zeus, und warf doppelte Loose hinein;", "tokens": ["Wa\u00b7ge", "dem", "Zeus", ",", "und", "warf", "dop\u00b7pel\u00b7te", "Loo\u00b7se", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "$,", "KON", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.37": {"text": "Hoch nun hob er sie auf; doch sie hing gradlinig, und Keinem", "tokens": ["Ho