{"textgrid.poem.32984": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alles l\u00e4ngst nun, l\u00e4ngst vor\u00fcber!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles l\u00e4ngst nun, l\u00e4ngst vor\u00fcber!", "tokens": ["Al\u00b7les", "l\u00e4ngst", "nun", ",", "l\u00e4ngst", "vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADV", "$,", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcnfmal schon ward's Winter dr\u00fcber!", "tokens": ["F\u00fcnf\u00b7mal", "schon", "ward's", "Win\u00b7ter", "dr\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "NN", "PAV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "immer andres dr\u00e4ngte her!", "tokens": ["im\u00b7mer", "and\u00b7res", "dr\u00e4ng\u00b7te", "her", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Neue Jahre, neue Ziele!", "tokens": ["Neu\u00b7e", "Jah\u00b7re", ",", "neu\u00b7e", "Zie\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Selten spiel ich jene Spiele", "tokens": ["Sel\u00b7ten", "spiel", "ich", "je\u00b7ne", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und noch seltener sing ich mehr!", "tokens": ["und", "noch", "sel\u00b7te\u00b7ner", "sing", "ich", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Wie die Zeit es eben \u00e4ndert:", "tokens": ["Wie", "die", "Zeit", "es", "e\u00b7ben", "\u00e4n\u00b7dert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "jener landet, dieser kentert,", "tokens": ["je\u00b7ner", "lan\u00b7det", ",", "die\u00b7ser", "ken\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "der liegt windstill wo auf See ...", "tokens": ["der", "liegt", "wind\u00b7still", "wo", "auf", "See", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADJD", "PWAV", "APPR", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "bleibt man nur auf seinem Posten", "tokens": ["bleibt", "man", "nur", "auf", "sei\u00b7nem", "Pos\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und l\u00e4\u00dft Kopf und Herz nicht rosten,", "tokens": ["und", "l\u00e4\u00dft", "Kopf", "und", "Herz", "nicht", "ros\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "geh es immer, wie es geh!", "tokens": ["geh", "es", "im\u00b7mer", ",", "wie", "es", "geh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Meist wohl ist's ja dummer Plunder;", "tokens": ["Meist", "wohl", "ist's", "ja", "dum\u00b7mer", "Plun\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "manchmal doch gl\u00fcckt auch ein Wunder,", "tokens": ["manch\u00b7mal", "doch", "gl\u00fcckt", "auch", "ein", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "noch viel eher aber f\u00e4llt's!", "tokens": ["noch", "viel", "e\u00b7her", "a\u00b7ber", "f\u00e4llt's", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was drum rinnt, la\u00df ruhig rinnen!", "tokens": ["Was", "drum", "rinnt", ",", "la\u00df", "ru\u00b7hig", "rin\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PAV", "VVFIN", "$,", "VVIMP", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "nur wer aush\u00e4lt, wird gewinnen ...", "tokens": ["nur", "wer", "aus\u00b7h\u00e4lt", ",", "wird", "ge\u00b7win\u00b7nen", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "$,", "VAFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "nicht ein jeder freilich h\u00e4lt's!", "tokens": ["nicht", "ein", "je\u00b7der", "frei\u00b7lich", "h\u00e4lt's", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"