{"textgrid.poem.49382": {"metadata": {"author": {"name": "Ball, Hugo", "birth": "N.A.", "death": "N.A."}, "title": "Das Gespenst", "genre": "verse", "period": "N.A.", "pub_year": 1906, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gew\u00f6hnlich kommt es, wenn die Lichter brennen.", "tokens": ["Ge\u00b7w\u00f6hn\u00b7lich", "kommt", "es", ",", "wenn", "die", "Lich\u00b7ter", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es poltert mit den Tellern und den Tassen.", "tokens": ["Es", "pol\u00b7tert", "mit", "den", "Tel\u00b7lern", "und", "den", "Tas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf roten Schuhen schlurrt es in den nassen", "tokens": ["Auf", "ro\u00b7ten", "Schu\u00b7hen", "schlurrt", "es", "in", "den", "nas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geschwenkten N\u00e4chten und man h\u00f6rt sein Flennen.", "tokens": ["Ge\u00b7schwenk\u00b7ten", "N\u00e4ch\u00b7ten", "und", "man", "h\u00f6rt", "sein", "Flen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von Zeit zu Zeit scheint es umherzurennen", "tokens": ["Von", "Zeit", "zu", "Zeit", "scheint", "es", "um\u00b7her\u00b7zu\u00b7ren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Trumpf, Atout und ausgespielten Assen.", "tokens": ["Mit", "Trumpf", ",", "At\u00b7out", "und", "aus\u00b7ge\u00b7spiel\u00b7ten", "As\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf Seil und R\u00e4der scheint es aufzupassen", "tokens": ["Auf", "Seil", "und", "R\u00e4\u00b7der", "scheint", "es", "auf\u00b7zu\u00b7pas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ist an seinem L\u00e4rmen zu erkennen.", "tokens": ["Und", "ist", "an", "sei\u00b7nem", "L\u00e4r\u00b7men", "zu", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es ist besch\u00e4ftigt in der G\u00e4ngelschwemme", "tokens": ["Es", "ist", "be\u00b7sch\u00e4f\u00b7tigt", "in", "der", "G\u00e4n\u00b7gel\u00b7schwem\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und hochweis weht dann seine erzene Haube,", "tokens": ["Und", "hoch\u00b7weis", "weht", "dann", "sei\u00b7ne", "er\u00b7ze\u00b7ne", "Hau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"]