{"textgrid.poem.47220": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die gn\u00e4dige Frau", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir haben eine gn\u00e4d'ge Frau,", "tokens": ["Wir", "ha\u00b7ben", "ei\u00b7ne", "gn\u00e4d'\u00b7ge", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ist so weich von Herzen,", "tokens": ["Die", "ist", "so", "weich", "von", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Augen stehn ihr gleich voll Tau,", "tokens": ["Die", "Au\u00b7gen", "stehn", "ihr", "gleich", "voll", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn andre munter scherzen.", "tokens": ["Wenn", "and\u00b7re", "mun\u00b7ter", "scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und h\u00e4tte sie mehr Nadelgeld,", "tokens": ["Und", "h\u00e4t\u00b7te", "sie", "mehr", "Na\u00b7del\u00b7geld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie heilte alle Schmerzen", "tokens": ["Sie", "heil\u00b7te", "al\u00b7le", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Im Dorf und in der ganzen Welt.", "tokens": ["Im", "Dorf", "und", "in", "der", "gan\u00b7zen", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie kann, zu knicken einen Floh,", "tokens": ["Sie", "kann", ",", "zu", "kni\u00b7cken", "ei\u00b7nen", "Floh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PTKZU", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00dcber das Herz nicht bringen,", "tokens": ["\u00dc\u00b7ber", "das", "Herz", "nicht", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sie l\u00e4\u00dft ihn lieber frei und froh", "tokens": ["Sie", "l\u00e4\u00dft", "ihn", "lie\u00b7ber", "frei", "und", "froh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hinaus zum Fenster springen.", "tokens": ["Hin\u00b7aus", "zum", "Fens\u00b7ter", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch wenn er sich ein Bein verstaucht,", "tokens": ["Doch", "wenn", "er", "sich", "ein", "Bein", "ver\u00b7staucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie wird sie es erschwingen", "tokens": ["Wie", "wird", "sie", "es", "er\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zu zahlen den Bader, den er braucht?", "tokens": ["Zu", "zah\u00b7len", "den", "Ba\u00b7der", ",", "den", "er", "braucht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Wir haben eine gn\u00e4d'ge Frau,", "tokens": ["Wir", "ha\u00b7ben", "ei\u00b7ne", "gn\u00e4d'\u00b7ge", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ist so weich von Herzen,", "tokens": ["Die", "ist", "so", "weich", "von", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Augen stehn ihr gleich voll Tau,", "tokens": ["Die", "Au\u00b7gen", "stehn", "ihr", "gleich", "voll", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn andre munter scherzen.", "tokens": ["Wenn", "and\u00b7re", "mun\u00b7ter", "scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und h\u00e4tte sie mehr Nadelgeld,", "tokens": ["Und", "h\u00e4t\u00b7te", "sie", "mehr", "Na\u00b7d