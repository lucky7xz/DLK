{"textgrid.poem.46227": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Widerum an wolermelten herren", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tod ist Gustav der gro\u00df, sigreich, un\u00fcberwunden,", "tokens": ["Tod", "ist", "Gus\u00b7tav", "der", "gro\u00df", ",", "sig\u00b7reich", ",", "un\u00b7\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "NE", "ART", "ADJD", "$,", "ADJD", "$,", "ADJA", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "er, dessen starke faust, er dessen gro\u00dfer mut", "tokens": ["er", ",", "des\u00b7sen", "star\u00b7ke", "faust", ",", "er", "des\u00b7sen", "gro\u00b7\u00dfer", "mut"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELAT", "ADJA", "NN", "$,", "PPER", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ein schrecken seines feinds und seiner freinden hut,", "tokens": ["ein", "schre\u00b7cken", "sei\u00b7nes", "feinds", "und", "sei\u00b7ner", "frein\u00b7den", "hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ihm jene mit zagheit, dise mit lieb verbunden.", "tokens": ["ihm", "je\u00b7ne", "mit", "zag\u00b7heit", ",", "di\u00b7se", "mit", "lieb", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "APPR", "NN", "$,", "PDS", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Tod ist Gustav der gro\u00df; von uns ist er verschwunden,", "tokens": ["Tod", "ist", "Gus\u00b7tav", "der", "gro\u00df", ";", "von", "uns", "ist", "er", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NE", "ART", "ADJD", "$.", "APPR", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "nachdem f\u00fcr sein haupt, hand und herz (stets weis, stark, gut)", "tokens": ["nach\u00b7dem", "f\u00fcr", "sein", "haupt", ",", "hand", "und", "herz", "(", "stets", "weis", ",", "stark", ",", "gut", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$(", "ADV", "PTKVZ", "$,", "ADJD", "$,", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "er unsern undank gro\u00df, zu schlecht der feinden wut", "tokens": ["er", "un\u00b7sern", "un\u00b7dank", "gro\u00df", ",", "zu", "schlecht", "der", "fein\u00b7den", "wut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "ADJD", "$,", "PTKA", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und dan die ganze welt zu schlim und eng befunden.", "tokens": ["und", "dan", "die", "gan\u00b7ze", "welt", "zu", "schlim", "und", "eng", "be\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "PTKA", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"