{"textgrid.poem.26057": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Veilchenzeit wird mir Liedermeister", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die welschen Bildergedanken verwehen,", "tokens": ["Die", "wel\u00b7schen", "Bil\u00b7der\u00b7ge\u00b7dan\u00b7ken", "ver\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn du und ich auf den H\u00fcgeln in Franken", "tokens": ["Wenn", "du", "und", "ich", "auf", "den", "H\u00fc\u00b7geln", "in", "Fran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KON", "PPER", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "An der Landstra\u00df' dem M\u00e4rzwind entgegenstehen.", "tokens": ["An", "der", "Land\u00b7stra\u00df'", "dem", "M\u00e4r\u00b7zwind", "ent\u00b7ge\u00b7gen\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "--+--++-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mehr als goldne Pagoden gilt Heimatgras.", "tokens": ["Mehr", "als", "gold\u00b7ne", "Pa\u00b7go\u00b7den", "gilt", "Hei\u00b7mat\u00b7gras", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ADJA", "NN", "VVFIN", "NE", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Wenn \u00fcber den k\u00fchligen Vorfr\u00fchlingsboden", "tokens": ["Wenn", "\u00fc\u00b7ber", "den", "k\u00fch\u00b7li\u00b7gen", "Vor\u00b7fr\u00fch\u00b7lings\u00b7bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die D\u00fcfte der Veilchen umgehen wie Geister,", "tokens": ["Die", "D\u00fcf\u00b7te", "der", "Veil\u00b7chen", "um\u00b7ge\u00b7hen", "wie", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "KOKOM", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Nicht l\u00e4nger ich dann mehr die Fremde begehre,", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "ich", "dann", "mehr", "die", "Frem\u00b7de", "be\u00b7geh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PPER", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Nicht Tropenerde, die feuerbergschwere,", "tokens": ["Nicht", "Tro\u00b7pe\u00b7ner\u00b7de", ",", "die", "feu\u00b7er\u00b7berg\u00b7schwe\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Die Veilchenzeit wird mir Liedermeister.", "tokens": ["Die", "Veil\u00b7chen\u00b7zeit", "wird", "mir", "Lie\u00b7der\u00b7meis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die welschen Bildergedanken verwehen,", "tokens": ["Die", "wel\u00b7schen", "Bil\u00b7der\u00b7ge\u00b7dan\u00b7ken", "ver\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed