{"textgrid.poem.46164": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An Marggrafen Karln von Baden", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Karl, dessen weises haupt und dessen k\u00fchne hand", "tokens": ["Karl", ",", "des\u00b7sen", "wei\u00b7ses", "haupt", "und", "des\u00b7sen", "k\u00fch\u00b7ne", "hand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELAT", "ADJA", "NN", "KON", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die himmel mehr und mehr mit ihrem schatz bereichen,", "tokens": ["die", "him\u00b7mel", "mehr", "und", "mehr", "mit", "ih\u00b7rem", "schatz", "be\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df euch an dapferkeit, an weisheit und verstand", "tokens": ["da\u00df", "euch", "an", "dap\u00b7fer\u00b7keit", ",", "an", "weis\u00b7heit", "und", "ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,", "APPR", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mars und Apollon selbs in kurzem sollen weichen.", "tokens": ["Mars", "und", "A\u00b7pol\u00b7lon", "selbs", "in", "kur\u00b7zem", "sol\u00b7len", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "ADV", "APPR", "ADJA", "VMFIN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Euch, prinz, des h\u00f6chsten gnad, die nu mit schwerer straf", "tokens": ["Euch", ",", "prinz", ",", "des", "h\u00f6chs\u00b7ten", "gnad", ",", "die", "nu", "mit", "schwe\u00b7rer", "straf"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NE", "$,", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "vertreibet des reichs ruh, damit es lang beflecket,", "tokens": ["ver\u00b7trei\u00b7bet", "des", "reichs", "ruh", ",", "da\u00b7mit", "es", "lang", "be\u00b7fle\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "hat schon in mancher not, auf da\u00df ihr des lands schlaf", "tokens": ["hat", "schon", "in", "man\u00b7cher", "not", ",", "auf", "da\u00df", "ihr", "des", "lands", "schlaf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "PIAT", "NN", "$,", "APPR", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.8": {"text": "vertreibet mit dem schwert, mit seinem schild bedecket.", "tokens": ["ver\u00b7trei\u00b7bet", "mit", "dem", "schwert", ",", "mit", "sei\u00b7nem", "schild", "be\u00b7de\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "VVFIN", "$,", "APPR", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wolan das neue jahr, in welchem unsre freud", "tokens": ["Wo\u00b7lan", "das", "neu\u00b7e", "jahr", ",", "in", "wel\u00b7chem", "uns\u00b7re", "freud"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "APPR", "PWAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "und gl\u00fcck erneuern wird, ist fertig umzuschlagen", "tokens": ["und", "gl\u00fcck", "er\u00b7neu\u00b7ern", "wird", ",", "ist", "fer\u00b7tig", "um\u00b7zu\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVINF", "VAFIN", "$,", "VAFIN", "ADJD", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "und will durch neuen zeug und zug das alte leid,", "tokens": ["und", "will", "durch", "neu\u00b7en", "zeug", "und", "zug", "das", "al\u00b7te", "leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ADJA", "NN", "KON", "NN", "ART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "und nach dem alten jahr selbs unsren feind verjagen.", "tokens": ["und", "nach", "dem", "al\u00b7ten", "jahr", "selbs", "un\u00b7sren", "feind", "ver\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So stellet euch auch ein nu bei dem dapfern heer,", "tokens": ["So", "stel\u00b7let", "euch", "auch", "ein", "nu", "bei", "dem", "dap\u00b7fern", "heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "besch\u00fctzend die freiheit, den krieg recht fortzuf\u00fchren;", "tokens": ["be\u00b7sch\u00fct\u00b7zend", "die", "frei\u00b7heit", ",", "den", "krieg", "recht", "fort\u00b7zu\u00b7f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "dan got, der eurer faust geliefert hat die wehr,", "tokens": ["dan", "got", ",", "der", "eu\u00b7rer", "faust", "ge\u00b7lie\u00b7fert", "hat", "die", "wehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.16": {"text": "der wird auch euer haupt mit lorberzweigen zieren.", "tokens": ["der", "wird", "auch", "eu\u00b7er", "haupt", "mit", "lor\u00b7ber\u00b7zwei\u00b7gen", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "ADV", "PPOSAT", "NN", "APPR", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Also soll euer arm umsunst nicht einen streich,", "tokens": ["Al\u00b7so", "soll", "eu\u00b7er", "arm", "um\u00b7sunst", "nicht", "ei\u00b7nen", "streich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJD", "ADV", "PTKNEG", "ART", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "also soll euer mund umsonst kein wort verrichten,", "tokens": ["al\u00b7so", "soll", "eu\u00b7er", "mund", "um\u00b7sonst", "kein", "wort", "ver\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.19": {"text": "darab der k\u00fchnste feind nicht alsobald erbleich", "tokens": ["da\u00b7rab", "der", "k\u00fchns\u00b7te", "feind", "nicht", "al\u00b7so\u00b7bald", "er\u00b7bleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "und welches nicht k\u00f6nd bald freind und feind euch verpflichten.", "tokens": ["und", "wel\u00b7ches", "nicht", "k\u00f6nd", "bald", "freind", "und", "feind", "euch", "ver\u00b7pflich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "VMFIN", "ADV", "ADJD", "KON", "NN", "PPER", "VVINF", "$."], "meter": "-+-++-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Also sigreich solt ihr, an seel und leib schadlos,", "tokens": ["Al\u00b7so", "sig\u00b7reich", "solt", "ihr", ",", "an", "seel", "und", "leib", "schad\u00b7los", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "$,", "APPR", "NE", "KON", "NN", "ADJD", "$,"], "meter": "-----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.22": {"text": "den stolz treulosen feind sampt allen seinen g\u00f6tzen", "tokens": ["den", "stolz", "treu\u00b7lo\u00b7sen", "feind", "sampt", "al\u00b7len", "sei\u00b7nen", "g\u00f6t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "(so gro\u00df, ja gr\u00f6\u00dfer noch hierdurch als Karl der gro\u00df)", "tokens": ["(", "so", "gro\u00df", ",", "ja", "gr\u00f6\u00b7\u00dfer", "noch", "hier\u00b7durch", "als", "Karl", "der", "gro\u00df", ")"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "$,", "ADV", "ADJD", "ADV", "PAV", "KOUS", "NE", "ART", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "aus euerm eigentum in das verderben hetzen.", "tokens": ["aus", "eu\u00b7erm", "ei\u00b7gen\u00b7tum", "in", "das", "ver\u00b7der\u00b7ben", "het\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "APPR", "PDS", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Also, gleichwie das jahr, weil sein end und anfang", "tokens": ["Al\u00b7so", ",", "gleich\u00b7wie", "das", "jahr", ",", "weil", "sein", "end", "und", "an\u00b7fang"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KON", "ART", "NN", "$,", "KOUS", "PPOSAT", "VVPP", "KON", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "in sich umwendet rund, sich niemal gar kan enden,", "tokens": ["in", "sich", "um\u00b7wen\u00b7det", "rund", ",", "sich", "nie\u00b7mal", "gar", "kan", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVFIN", "ADJD", "$,", "PRF", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "m\u00f6g euer lob endlos mit rundendem umgang", "tokens": ["m\u00f6g", "eu\u00b7er", "lob", "end\u00b7los", "mit", "run\u00b7den\u00b7dem", "um\u00b7gang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "sich \u00fcber die welt stets mit neuem ton umwenden.", "tokens": ["sich", "\u00fc\u00b7ber", "die", "welt", "stets", "mit", "neu\u00b7em", "ton", "um\u00b7wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Karl, dessen weises haupt und dessen k\u00fchne hand", "tokens": ["Karl", ",", "des\u00b7sen", "wei\u00b7ses", "haupt", "und", "des\u00b7sen", "k\u00fch\u00b7ne", "hand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELAT", "ADJA", "NN", "KON", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die himmel mehr und mehr mit ihrem schatz bereichen,", "tokens": ["die", "him\u00b7mel", "mehr", "und", "mehr", "mit", "ih\u00b7rem", "schatz", "be\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df euch an dapferkeit, an weisheit und verstand", "tokens": ["da\u00df", "euch", "an", "dap\u00b7fer\u00b7keit", ",", "an", "weis\u00b7heit", "und", "ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,", "APPR", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mars und Apollon selbs in kurzem sollen weichen.", "tokens": ["Mars", "und", "A\u00b7pol\u00b7lon", "selbs", 