ine.7": {"text": "Doch da er h\u00f6ret, da\u00df dem Lieben", "tokens": ["Doch", "da", "er", "h\u00f6\u00b7ret", ",", "da\u00df", "dem", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hier heut der erste Fleischtag sey,", "tokens": ["Hier", "heut", "der", "ers\u00b7te", "Fleischtag", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "So schimmert er mit frohem Lichte,", "tokens": ["So", "schim\u00b7mert", "er", "mit", "fro\u00b7hem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und weiset dieser Fluhr kein holprichtes Gesichte.", "tokens": ["Und", "wei\u00b7set", "die\u00b7ser", "Fluhr", "kein", "hol\u00b7prich\u00b7tes", "Ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Zwey theure V\u00e4ter sind vergn\u00fcgt,", "tokens": ["Zwey", "theu\u00b7re", "V\u00e4\u00b7ter", "sind", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Mutterherz f\u00fchlt zarte Triebe,", "tokens": ["Ein", "Mut\u00b7ter\u00b7herz", "f\u00fchlt", "zar\u00b7te", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ob dem, was GOtt zusammen f\u00fcgt,", "tokens": ["Ob", "dem", ",", "was", "Gott", "zu\u00b7sam\u00b7men", "f\u00fcgt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "PWS", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bey so werther Kinder-Liebe!", "tokens": ["Und", "bey", "so", "wert\u00b7her", "Kin\u00b7der\u00b7Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! schickte sich mein niedrig Lied,", "tokens": ["Ach", "!", "schick\u00b7te", "sich", "mein", "nied\u00b7rig", "Lied", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PRF", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So m\u00fc\u00dft ich auch von zweyen Helden,", "tokens": ["So", "m\u00fc\u00dft", "ich", "auch", "von", "zwe\u00b7yen", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und ihren grossen Thaten melden,", "tokens": ["Und", "ih\u00b7ren", "gros\u00b7sen", "Tha\u00b7ten", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die man hier h\u00f6chsterfreuet sieht,", "tokens": ["Die", "man", "hier", "h\u00f6chs\u00b7ter\u00b7freu\u00b7et", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Doch halt! die Poesie vermeynet,", "tokens": ["Doch", "halt", "!", "die", "Poe\u00b7sie", "ver\u00b7mey\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Da\u00df dermaleinst f\u00fcr sie noch ein Horaz erscheinet.", "tokens": ["Da\u00df", "der\u00b7ma\u00b7leinst", "f\u00fcr", "sie", "noch", "ein", "Ho\u00b7raz", "er\u00b7schei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}}, "stanza.12": {"line.1": {"text": "O Himmel! ja! du wollest nur", "tokens": ["O", "Him\u00b7mel", "!", "ja", "!", "du", "wol\u00b7lest", "nur"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PTKANT", "$.", "PPER", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein solch Verb\u00fcndni\u00df benedeyen,", "tokens": ["Ein", "solch", "Ver\u00b7b\u00fcn\u00b7dni\u00df", "be\u00b7ne\u00b7de\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Jhr Sch\u00e4fer dieser stillen Fluhr!", "tokens": ["Ihr", "Sch\u00e4\u00b7fer", "die\u00b7ser", "stil\u00b7len", "Fluhr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch heute m\u00fc\u00dftet ihr im Reyhen,", "tokens": ["Noch", "heu\u00b7te", "m\u00fc\u00df\u00b7tet", "ihr", "im", "Rey\u00b7hen", ","], "token_info": ["word",