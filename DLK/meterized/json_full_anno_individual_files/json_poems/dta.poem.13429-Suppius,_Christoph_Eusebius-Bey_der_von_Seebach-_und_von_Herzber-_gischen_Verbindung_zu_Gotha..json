{"dta.poem.13429": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Bey der  \n von Seebach- und von Herzber-  \n gischen Verbindung  \n zu Gotha.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Poeten wird es \u00f6fters schwer,", "tokens": ["Po\u00b7et\u00b7en", "wird", "es", "\u00f6f\u00b7ters", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein wohlgef\u00e4llig Werk zu schmieden,", "tokens": ["Ein", "wohl\u00b7ge\u00b7f\u00e4l\u00b7lig", "Werk", "zu", "schmie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie haben keine Venus mehr,", "tokens": ["Sie", "ha\u00b7ben", "kei\u00b7ne", "Ve\u00b7nus", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und endlich altern die Cupiden,", "tokens": ["Und", "end\u00b7lich", "al\u00b7tern", "die", "Cu\u00b7pi\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ja zu Vermehrung ihrer Noth", "tokens": ["Ja", "zu", "Ver\u00b7meh\u00b7rung", "ih\u00b7rer", "Noth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sind keine G\u00f6tter mehr vorhanden,", "tokens": ["Sind", "kei\u00b7ne", "G\u00f6t\u00b7ter", "mehr", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die ihnen sonsten beygestanden,", "tokens": ["Die", "ih\u00b7nen", "sons\u00b7ten", "bey\u00b7ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Herr Ph\u00f6bus ist erbla\u00dft und todt,", "tokens": ["Herr", "Ph\u00f6\u00b7bus", "ist", "er\u00b7bla\u00dft", "und", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Verjaget sind die Pierinnen,", "tokens": ["Ver\u00b7ja\u00b7get", "sind", "die", "Pie\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Wo soll die Poesie nun neue Zeuge spinnen?", "tokens": ["Wo", "soll", "die", "Poe\u00b7sie", "nun", "neu\u00b7e", "Zeu\u00b7ge", "spin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Jhr Feuer hab ich nie gef\u00fchlt,", "tokens": ["Ihr", "Feu\u00b7er", "hab", "ich", "nie", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir hat noch keine Hipokrene", "tokens": ["Mir", "hat", "noch", "kei\u00b7ne", "Hi\u00b7po\u00b7kre\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die halbe Raserey gek\u00fchlt,", "tokens": ["Die", "hal\u00b7be", "Ra\u00b7se\u00b7rey", "ge\u00b7k\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich kenne nur die schlechten T\u00f6ne,", "tokens": ["Ich", "ken\u00b7ne", "nur", "die", "schlech\u00b7ten", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die ruf ich auf dem Haber-Rohr,", "tokens": ["Die", "ruf", "ich", "auf", "dem", "Ha\u00b7ber\u00b7Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf Bergen, blumenreichen Hayden,", "tokens": ["Auf", "Ber\u00b7gen", ",", "blu\u00b7men\u00b7rei\u00b7chen", "Hay\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bey Fl\u00fcssen, wo die Heerden weyden,", "tokens": ["Bey", "Fl\u00fcs\u00b7sen", ",", "wo", "die", "Heer\u00b7den", "wey\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Durch die Natur gereizt, hervor,", "tokens": ["Durch", "die", "Na\u00b7tur", "ge\u00b7reizt", ",", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da h\u00f6ren denn die Westenwinde", "tokens": ["Da", "h\u00f6\u00b7ren", "denn", "die", "Wes\u00b7ten\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In meinen Liedern an, wo ich die Tugend finde.", "tokens": ["In", "mei\u00b7nen", "Lie\u00b7dern", "an", ",", "wo", "ich", "die", "Tu\u00b7gend", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und denen will ich heute noch", "tokens": ["Und", "de\u00b7nen", "will", "ich", "heu\u00b7te", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VMFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von einer Hochzeitfreude singen,", "tokens": ["Von", "ei\u00b7ner", "Hoch\u00b7zeit\u00b7freu\u00b7de", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Tugend Lob ist mir zu hoch,", "tokens": ["Der", "Tu\u00b7gend", "Lob", "ist", "mir", "zu", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das kann nur aus Trompeten klingen;", "tokens": ["Das", "kann", "nur", "aus", "Trom\u00b7pe\u00b7ten", "klin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein adlich Paar verm\u00e4hlt sich hier,", "tokens": ["Ein", "ad\u00b7lich", "Paar", "ver\u00b7m\u00e4hlt", "sich", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wird sich selbsten zur Belohnung,", "tokens": ["Und", "wird", "sich", "selbs\u00b7ten", "zur", "Be\u00b7loh\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dort, seht es in der Tugend Wohnung,", "tokens": ["Dort", ",", "seht", "es", "in", "der", "Tu\u00b7gend", "Woh\u00b7nung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gl\u00fcckseeliges! der HErr mit Dir!", "tokens": ["Gl\u00fcck\u00b7see\u00b7li\u00b7ges", "!", "der", "Herr", "mit", "Dir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wer sieht es, wer h\u00f6rt von ihm heute,", "tokens": ["Wer", "sieht", "es", ",", "wer", "h\u00f6rt", "von", "ihm", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "APPR", "PPER", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Der nicht zugleich auch ruft: o zwey begl\u00fcckte Leute!", "tokens": ["Der", "nicht", "zu\u00b7gleich", "auch", "ruft", ":", "o", "zwey", "be\u00b7gl\u00fcck\u00b7te", "Leu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "ADV", "VVFIN", "$.", "FM", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ganz recht! die\u00df auserw\u00e4hlte Paar", "tokens": ["Ganz", "recht", "!", "die\u00df", "au\u00b7ser\u00b7w\u00e4hl\u00b7te", "Paar"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$.", "PDS", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat allbereits sein Ja gegeben,", "tokens": ["Hat", "all\u00b7be\u00b7reits", "sein", "Ja", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es will von nun an immerdar", "tokens": ["Es", "will", "von", "nun", "an", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADV", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einander zu gefallen leben;", "tokens": ["Ein\u00b7an\u00b7der", "zu", "ge\u00b7fal\u00b7len", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zufriedenheit, die K\u00f6nigin", "tokens": ["Zu\u00b7frie\u00b7den\u00b7heit", ",", "die", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erhabner Herzen und Gem\u00fcther,", "tokens": ["Er\u00b7hab\u00b7ner", "Her\u00b7zen", "und", "Ge\u00b7m\u00fc\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er\u00f6ffnet ihnen wahre G\u00fcter,", "tokens": ["Er\u00b7\u00f6ff\u00b7net", "ih\u00b7nen", "wah\u00b7re", "G\u00fc\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und giebt sich ihnen selbsten hin,", "tokens": ["Und", "giebt", "sich", "ih\u00b7nen", "selbs\u00b7ten", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Um heut in Beyderseits Vertrauen", "tokens": ["Um", "heut", "in", "Bey\u00b7der\u00b7seits", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "APPR", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Auf ihre Lebenszeit ein Eden zu erbauen.", "tokens": ["Auf", "ih\u00b7re", "Le\u00b7bens\u00b7zeit", "ein", "E\u00b7den", "zu", "er\u00b7bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "In einer reinen Gluth entbrannt,", "tokens": ["In", "ei\u00b7ner", "rei\u00b7nen", "Gluth", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die erst von oben angez\u00fcndet,", "tokens": ["Die", "erst", "von", "o\u00b7ben", "an\u00b7ge\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcgt itzt die treue Hand in Hand,", "tokens": ["F\u00fcgt", "itzt", "die", "treu\u00b7e", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die holde Eintracht fest verbindet,", "tokens": ["Die", "hol\u00b7de", "Ein\u00b7tracht", "fest", "ver\u00b7bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein immerw\u00e4hrend Wohlergehn", "tokens": ["Ein", "im\u00b7mer\u00b7w\u00e4h\u00b7rend", "Woh\u00b7ler\u00b7gehn"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird durch die Folge langer Zeiten", "tokens": ["Wird", "durch", "die", "Fol\u00b7ge", "lan\u00b7ger", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie durch manch Jubeljahr begleiten,", "tokens": ["Sie", "durch", "manch", "Ju\u00b7bel\u00b7jahr", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Ursprung ist ja viel zu sch\u00f6n,", "tokens": ["Der", "Ur\u00b7sprung", "ist", "ja", "viel", "zu", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Als da\u00df ihn nicht die Ehrfurcht priese,", "tokens": ["Als", "da\u00df", "ihn", "nicht", "die", "Ehr\u00b7furcht", "prie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Er und die Menschlichkeit stammt aus dem Paradiese.", "tokens": ["Er", "und", "die", "Menschlich\u00b7keit", "stammt", "aus", "dem", "Pa\u00b7ra\u00b7die\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "So h\u00f6re mich denn, We stenwind!", "tokens": ["So", "h\u00f6\u00b7re", "mich", "denn", ",", "We", "sten\u00b7wind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr stillen Th\u00e4ler! milde Fluhren!", "tokens": ["Ihr", "stil\u00b7len", "Th\u00e4\u00b7ler", "!", "mil\u00b7de", "Fluh\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Fried und Eintracht Freunde sind,", "tokens": ["Wo", "Fried", "und", "Ein\u00b7tracht", "Freun\u00b7de", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und von den g\u00fcldnen Zeiten Spuhren;", "tokens": ["Und", "von", "den", "g\u00fcld\u00b7nen", "Zei\u00b7ten", "Spuh\u00b7ren", ";"], "token_info": ["word", "word", "word", "wo