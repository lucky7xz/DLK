, "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo jeder Fr\u00fchling sie verj\u00fcngt,", "tokens": ["Wo", "je\u00b7der", "Fr\u00fch\u00b7ling", "sie", "ver\u00b7j\u00fcngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sommer Erndten prophezeyen,", "tokens": ["Die", "Som\u00b7mer", "Ernd\u00b7ten", "pro\u00b7phe\u00b7ze\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die Herbste sie mit Frucht erfreuen,", "tokens": ["Die", "Herbs\u00b7te", "sie", "mit", "Frucht", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Winter neue Wonne bringt,", "tokens": ["Der", "Win\u00b7ter", "neu\u00b7e", "Won\u00b7ne", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Indem sie an zu z\u00e4hlen fangen,", "tokens": ["In\u00b7dem", "sie", "an", "zu", "z\u00e4h\u00b7len", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Seit wenn ihr B\u00fcndni\u00df nun zum ersten eingegangen.", "tokens": ["Seit", "wenn", "ihr", "B\u00fcn\u00b7dni\u00df", "nun", "zum", "ers\u00b7ten", "ein\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPOSAT", "NN", "ADV", "APPR