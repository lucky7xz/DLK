{"dta.poem.11294": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uber das grab des herrn baron  \n von Abschatz.  \n  E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Griechen machen sich mit ihrem Pindus breit,", "tokens": ["Die", "Grie\u00b7chen", "ma\u00b7chen", "sich", "mit", "ih\u00b7rem", "Pin\u00b7dus", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und r\u00fchmen den Apoll\u2019, als eine g\u00f6ttlichkeit;", "tokens": ["Und", "r\u00fch\u00b7men", "den", "A\u00b7poll'", ",", "als", "ei\u00b7ne", "g\u00f6tt\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein sie m\u00f6gen nur mit ihrem pralen schweigen:", "tokens": ["Al\u00b7lein", "sie", "m\u00f6\u00b7gen", "nur", "mit", "ih\u00b7rem", "pra\u00b7len", "schwei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Di\u00df enge grab kan mehr, als jene berge, zeigen.", "tokens": ["Di\u00df", "en\u00b7ge", "grab", "kan", "mehr", ",", "als", "je\u00b7ne", "ber\u00b7ge", ",", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VMFIN", "A