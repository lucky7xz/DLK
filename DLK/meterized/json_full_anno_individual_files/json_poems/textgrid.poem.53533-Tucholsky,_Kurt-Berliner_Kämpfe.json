{"textgrid.poem.53533": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Berliner K\u00e4mpfe", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Revolution? Aber kein Gedanke!", "tokens": ["Re\u00b7vo\u00b7lu\u00b7ti\u00b7on", "?", "A\u00b7ber", "kein", "Ge\u00b7dan\u00b7ke", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "PIAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Es brodelt im Hexenkessel der Panke,", "tokens": ["Es", "bro\u00b7delt", "im", "He\u00b7xen\u00b7kes\u00b7sel", "der", "Pan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+++-+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "es hupen die Autos, es knattern die Flinten,", "tokens": ["es", "hu\u00b7pen", "die", "Au\u00b7tos", ",", "es", "knat\u00b7tern", "die", "Flin\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Demonstrationen vorne und hinten \u2013", "tokens": ["De\u00b7monst\u00b7ra\u00b7ti\u00b7o\u00b7nen", "vor\u00b7ne", "und", "hin\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "KON", "ADV", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Tun sie auch so wie die Menschenfresser:", "tokens": ["Tun", "sie", "auch", "so", "wie", "die", "Men\u00b7schen\u00b7fres\u00b7ser", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "die Panke war stets ein stilles Gew\u00e4sser.", "tokens": ["die", "Pan\u00b7ke", "war", "stets", "ein", "stil\u00b7les", "Ge\u00b7w\u00e4s\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Jahrelang \u2013 b\u00e4ngliches Z\u00f6gern und Drehen.", "tokens": ["Jah\u00b7re\u00b7lang", "\u2013", "b\u00e4ng\u00b7li\u00b7ches", "Z\u00f6\u00b7gern", "und", "Dre\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "NN", "KON", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Jahrelang \u2013 wir werden ja sehen!", "tokens": ["Jah\u00b7re\u00b7lang", "\u2013", "wir", "wer\u00b7den", "ja", "se\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VAFIN", "ADV", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Jahrelang \u2013 Krupp und Tirpitz sollen leben!", "tokens": ["Jah\u00b7re\u00b7lang", "\u2013", "Krupp", "und", "Tir\u00b7pitz", "sol\u00b7len", "le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "KON", "NE", "VMFIN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.10": {"text": "Jahrelang \u2013 rin in die Sch\u00fctzengr\u00e4ben!", "tokens": ["Jah\u00b7re\u00b7lang", "\u2013", "rin", "in", "die", "Sch\u00fct\u00b7zen\u00b7gr\u00e4\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Jahrelang \u2013 Reklamiertenschi\u00df.", "tokens": ["Jah\u00b7re\u00b7lang", "\u2013", "Re\u00b7kla\u00b7mier\u00b7ten\u00b7schi\u00df", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Kompromi\u00df . . . und Kompromi\u00df . . .", "tokens": ["Kom\u00b7pro\u00b7mi\u00df", ".", ".", ".", "und", "Kom\u00b7pro\u00b7mi\u00df", ".", ".", "."], "token_info": ["word", "punct", "punct", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "$.", "$.", "$.", "KON", "NN", "$.", "$.", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Jahrelang \u2013 Ausverk\u00e4ufe an Sieg . . .", "tokens": ["Jah\u00b7re\u00b7lang", "\u2013", "Aus\u00b7ver\u00b7k\u00e4u\u00b7fe", "an", "Sieg", ".", ".", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "$(", "NN", "APPR", "NN", "$.", "$.", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Sozialisierung? Krieg ist Krieg.", "tokens": ["So\u00b7zi\u00b7a\u00b7li\u00b7sie\u00b7rung", "?", "Krieg", "ist", "Krieg", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und nun ist auf einmal Friede auf Erden.", "tokens": ["Und", "nun", "ist", "auf", "ein\u00b7mal", "Frie\u00b7de", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "APPR", "ADV", "NN", "APPR", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und nun soll das alles anders werden.", "tokens": ["Und", "nun", "soll", "das", "al\u00b7les", "an\u00b7ders", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ART", "PIS", "ADV", "VAINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wir hassen den bauchigen Kassenschrein.", "tokens": ["Wir", "has\u00b7sen", "den", "bau\u00b7chi\u00b7gen", "Kas\u00b7sen\u00b7schrein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wir wollen alle gl\u00fccklich sein!", "tokens": ["Wir", "wol\u00b7len", "al\u00b7le", "gl\u00fcck\u00b7lich", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Man kann sich \u00fcber das Tempo zanken.", "tokens": ["Man", "kann", "sich", "\u00fc\u00b7ber", "das", "Tem\u00b7po", "zan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nicht so bei uns an der blauen Panken.", "tokens": ["Nicht", "so", "bei", "uns", "an", "der", "blau\u00b7en", "Pan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn die Regierung einen wie Liebknecht h\u00e4tt!", "tokens": ["Wenn", "die", "Re\u00b7gie\u00b7rung", "ei\u00b7nen", "wie", "Lieb\u00b7knecht", "h\u00e4tt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "KOKOM", "NN", "VAFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Regierung aber sitzt auf dem Klosett", "tokens": ["Die", "Re\u00b7gie\u00b7rung", "a\u00b7ber", "sitzt", "auf", "dem", "Klo\u00b7sett"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "und ber\u00e4t wie fr\u00fcher in der Reichskanzlei,", "tokens": ["und", "be\u00b7r\u00e4t", "wie", "fr\u00fc\u00b7her", "in", "der", "Reichs\u00b7kanz\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "was nunmehr und ob es zu tun sei.", "tokens": ["was", "nun\u00b7mehr", "und", "ob", "es", "zu", "tun", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KON", "KOUS", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Es erinnert an schlechteste alte Zeiten:", "tokens": ["Es", "e\u00b7rin\u00b7nert", "an", "schlech\u00b7tes\u00b7te", "al\u00b7te", "Zei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "das Gesellschaftsspiel der Verantwortlichkeiten,", "tokens": ["das", "Ge\u00b7sell\u00b7schafts\u00b7spiel", "der", "Ver\u00b7ant\u00b7wort\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "der deutsche Streit um die Kompetenz \u2013", "tokens": ["der", "deut\u00b7sche", "Streit", "um", "die", "Kom\u00b7pe\u00b7tenz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "der alte politische Zirkus Renz.", "tokens": ["der", "al\u00b7te", "po\u00b7li\u00b7ti\u00b7sche", "Zir\u00b7kus", "Renz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Unterdessen schwillt der Spartakus", "tokens": ["Un\u00b7ter\u00b7des\u00b7sen", "schwillt", "der", "Spar\u00b7ta\u00b7kus"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "zur Macht empor, weil er will und mu\u00df.", "tokens": ["zur", "Macht", "em\u00b7por", ",", "weil", "er", "will", "und", "mu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und der B\u00fcrger? Du liebe G\u00fcte!", "tokens": ["Und", "der", "B\u00fcr\u00b7ger", "?", "Du", "lie\u00b7be", "G\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "PPER", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Es wackeln im Wind die Zylinderh\u00fcte.", "tokens": ["Es", "wa\u00b7ckeln", "im", "Wind", "die", "Zy\u00b7lin\u00b7der\u00b7h\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Er ist gegen jede Volksemp\u00f6rung.", "tokens": ["Er", "ist", "ge\u00b7gen", "je\u00b7de", "Volks\u00b7em\u00b7p\u00f6\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Politik ist gesch\u00e4ftliche St\u00f6rung.", "tokens": ["Po\u00b7li\u00b7tik", "ist", "ge\u00b7sch\u00e4ft\u00b7li\u00b7che", "St\u00f6\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Spartakus will seine Kasse bedrohn?", "tokens": ["Spar\u00b7ta\u00b7kus", "will", "sei\u00b7ne", "Kas\u00b7se", "be\u00b7drohn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Das geht zu weit mit der Revolution.", "tokens": ["Das", "geht", "zu", "weit", "mit", "der", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKA", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und wenn der B\u00fcrger noch zuschlagen wollte!", "tokens": ["Und", "wenn", "der", "B\u00fcr\u00b7ger", "noch", "zu\u00b7schla\u00b7gen", "woll\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es schl\u00e4ft Tante Minchen, es schl\u00e4ft Onkel Nolte . . .", "tokens": ["Es", "schl\u00e4ft", "Tan\u00b7te", "Min\u00b7chen", ",", "es", "schl\u00e4ft", "On\u00b7kel", "Nol\u00b7te", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "NN", "NN", "$,", "PPER", "VVFIN", "NN", "NE", "$.", "$.", "$."], "meter": "-++-+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.9": {"text": "Spartakus packt die Geschichte beim Schopf.", "tokens": ["Spar\u00b7ta\u00b7kus", "packt", "die", "Ge\u00b7schich\u00b7te", "beim", "Schopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.10": {"text": "Der B\u00fcrger wackelt emp\u00f6rt mit dem Kopf.", "tokens": ["Der", "B\u00fcr\u00b7ger", "wa\u00b7ckelt", "em\u00b7p\u00f6rt", "mit", "dem", "Kopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und so stehn wir am Anfang und stehn am Ende.", "tokens": ["Und", "so", "stehn", "wir", "am", "An\u00b7fang", "und", "stehn", "am", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Deutsches Blut flo\u00df \u00fcber deutsche H\u00e4nde.", "tokens": ["Deut\u00b7sches", "Blut", "flo\u00df", "\u00fc\u00b7ber", "deut\u00b7sche", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bblumpen! Deserteure! Proleten!\u00ab", "tokens": ["\u00bb", "lum\u00b7pen", "!", "De\u00b7ser\u00b7teu\u00b7re", "!", "Pro\u00b7le\u00b7ten", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VVINF", "$.", "NN", "$.", "NN", "$.", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "So kann man dem Ding nicht entgegentreten.", "tokens": ["So", "kann", "man", "dem", "Ding", "nicht", "ent\u00b7ge\u00b7gen\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Ist Ruhe die erste B\u00fcrgerpflicht,", "tokens": ["Ist", "Ru\u00b7he", "die", "ers\u00b7te", "B\u00fcr\u00b7ger\u00b7pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "die von Emp\u00f6rern ist es nicht.", "tokens": ["die", "von", "Em\u00b7p\u00f6\u00b7rern", "ist", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gewalt gegen Gewalt, Kraft gegen Kraft:", "tokens": ["Ge\u00b7walt", "ge\u00b7gen", "Ge\u00b7walt", ",", "Kraft", "ge\u00b7gen", "Kraft", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "das ist die alte Wissenschaft.", "tokens": ["das", "ist", "die", "al\u00b7te", "Wis\u00b7sen\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wei\u00dft du, Deutscher, wie die neue hei\u00dft?", "tokens": ["Wei\u00dft", "du", ",", "Deut\u00b7scher", ",", "wie", "die", "neu\u00b7e", "hei\u00dft", "?"], "token_info": ["word", "word", "punct", "word", "punct