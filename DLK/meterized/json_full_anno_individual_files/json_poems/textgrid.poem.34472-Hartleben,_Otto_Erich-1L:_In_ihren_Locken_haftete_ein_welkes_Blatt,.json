{"textgrid.poem.34472": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: In ihren Locken haftete ein welkes Blatt,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In ihren Locken haftete ein welkes Blatt,", "tokens": ["In", "ih\u00b7ren", "Lo\u00b7cken", "haf\u00b7te\u00b7te", "ein", "wel\u00b7kes", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "als ich mit ihr den alten Berg hernieder stieg", "tokens": ["als", "ich", "mit", "ihr", "den", "al\u00b7ten", "Berg", "her\u00b7nie\u00b7der", "stieg"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ART", "ADJA", "NN", "PTKVZ", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "zum letzten Mal. Verstohlne Freude war es mir,", "tokens": ["zum", "letz\u00b7ten", "Mal", ".", "Ver\u00b7stohl\u00b7ne", "Freu\u00b7de", "war", "es", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "ADJA", "NN", "VAFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "das braune Blatt im wirren braunen Haar zu sehn,", "tokens": ["das", "brau\u00b7ne", "Blatt", "im", "wir\u00b7ren", "brau\u00b7nen", "Haar", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "den stillen Zeugen stillgenossner, heiliger Lust,", "tokens": ["den", "stil\u00b7len", "Zeu\u00b7gen", "still\u00b7ge\u00b7noss\u00b7ner", ",", "hei\u00b7li\u00b7ger", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "und heimlich, gl\u00fccklich l\u00e4chelnd schritt ich neben ihr,", "tokens": ["und", "heim\u00b7lich", ",", "gl\u00fcck\u00b7lich", "l\u00e4\u00b7chelnd", "schritt", "ich", "ne\u00b7ben", "ihr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "ADJD", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "indes ein schwellend S\u00e4useln durch die Kronen ging.", "tokens": ["in\u00b7des", "ein", "schwel\u00b7lend", "S\u00e4u\u00b7seln", "durch", "die", "Kro\u00b7nen", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch eh wir noch das erste Haus der Stadt erreicht,", "tokens": ["Doch", "eh", "wir", "noch", "das", "ers\u00b7te", "Haus", "der", "Stadt", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "stahl ich ihr sacht das braune Blatt vom stolzen Haupt.", "tokens": ["stahl", "ich", "ihr", "sacht", "das", "brau\u00b7ne", "Blatt", "vom", "stol\u00b7zen", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da ich nun nach ihren lieben Augen sah,", "tokens": ["Und", "da", "ich", "nun", "nach", "ih\u00b7ren", "lie\u00b7ben", "Au\u00b7gen", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die ehrsam schon und sittig wieder schauten drein,", "tokens": ["die", "ehr\u00b7sam", "schon", "und", "sit\u00b7tig", "wie\u00b7der", "schau\u00b7ten", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "KON", "ADJD", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "hob fragend sie den Blick empor: was nahmst du da?", "tokens": ["hob", "fra\u00b7gend", "sie", "den", "Blick", "em\u00b7por", ":", "was", "nahmst", "du", "da", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "NN", "PTKVZ", "$.", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Ich zeigt es schweigend. Eine dunkle Welle Bluts", "tokens": ["Ich", "zeigt", "es", "schwei\u00b7gend", ".", "Ei\u00b7ne", "dunk\u00b7le", "Wel\u00b7le", "Bluts"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$.", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "floss \u00fcber ihr schamhaftes Antlitz. Aber dann", "tokens"