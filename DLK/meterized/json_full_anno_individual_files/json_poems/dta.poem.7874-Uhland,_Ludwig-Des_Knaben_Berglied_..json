{"dta.poem.7874": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Des Knaben Berglied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin vom Berg der Hirtenknab,", "tokens": ["Ich", "bin", "vom", "Berg", "der", "Hir\u00b7ten\u00b7knab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seh\u2019 auf die Schl\u00f6sser all herab.", "tokens": ["Seh'", "auf", "die", "Schl\u00f6s\u00b7ser", "all", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PIAT", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sonne stralt am ersten hier,", "tokens": ["Die", "Son\u00b7ne", "stralt", "am", "ers\u00b7ten", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am l\u00e4ngsten weilet sie bei mir.", "tokens": ["Am", "l\u00e4ngs\u00b7ten", "wei\u00b7let", "sie", "bei", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin der Knab vom Berge!", "tokens": ["Ich", "bin", "der", "Knab", "vom", "Ber\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hier ist des Stromes Mutterhaus,", "tokens": ["Hier", "ist", "des", "Stro\u00b7mes", "Mut\u00b7ter\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich trink\u2019 ihn frisch vom Stein heraus;", "tokens": ["Ich", "trink'", "ihn", "frisch", "vom", "Stein", "he\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er braust vom Fels in wildem Lauf,", "tokens": ["Er", "braust", "vom", "Fels", "in", "wil\u00b7dem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich fang\u2019 ihn mit den Armen auf.", "tokens": ["Ich", "fang'", "ihn", "mit", "den", "Ar\u00b7men", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin der Knab vom Berge!", "tokens": ["Ich", "bin", "der", "Knab", "vom", "Ber\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Berg, der ist mein Eigenthum,", "tokens": ["Der", "Berg", ",", "der", "ist", "mein", "Ei\u00b7gen\u00b7thum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ziehn die St\u00fcrme rings herum,", "tokens": ["Da", "ziehn", "die", "St\u00fcr\u00b7me", "rings", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und heulen sie von Nord und S\u00fcd,", "tokens": ["Und", "heu\u00b7len", "sie", "von", "Nord", "und", "S\u00fcd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So \u00fcberschallt sie doch mein Lied:", "tokens": ["So", "\u00fc\u00b7bersc\u00b7hallt", "sie", "doch", "mein", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin der Knab vom Berge!", "tokens": ["Ich", "bin", "der", "Knab", "vom", "Ber\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sind Blitz und Donner unter mir,", "tokens": ["Sind", "Blitz", "und", "Don\u00b7ner", "un\u00b7ter", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So steh\u2019 ich hoch im Blauen hier;", "tokens": ["So", "steh'", "ich", "hoch", "im", "Blau\u00b7en", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich kenne sie und rufe zu:", "tokens": ["Ich", "ken\u00b7ne", "sie", "und", "ru\u00b7fe", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00dft meines Vaters Haus in Ruh!", "tokens": ["La\u00dft", "mei\u00b7nes", "Va\u00b7ters", "Haus", "in", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin der Knab vom Berge!", "tokens": ["Ich", "bin", "der", "Knab", "vom", "Ber\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wann die Sturmglock\u2019 einst erschallt,", "tokens": ["Und", "wann", "die", "Sturm\u00b7glock'", "einst", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Manch Feuer auf den Bergen wallt,", "tokens": ["Manch", "Feu\u00b7er", "auf", "den", "Ber\u00b7gen", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann steig\u2019 ich nieder, tret\u2019 in\u2019s Glied,", "tokens": ["Dann", "steig'", "ich", "nie\u00b7der", ",", "tret'", "in's", "Glied", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schwing\u2019 mein Schwerdt, und sing\u2019 mein Lied:", "tokens": ["Und", "schwing'", "mein", "Schwerdt", ",", "und", "sing'", "mein", "Lied", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVIMP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin der Knab vom Berge!", "tokens": ["Ich", "bin", "der", "Knab", "vom", "Ber\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}