{"textgrid.poem.65155": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ganz hatt' ich mich dir hingegeben", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ganz hatt' ich mich dir hingegeben", "tokens": ["Ganz", "hatt'", "ich", "mich", "dir", "hin\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit echter Freundschaft Heldenmut,", "tokens": ["Mit", "ech\u00b7ter", "Freund\u00b7schaft", "Hel\u00b7den\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein inn'res wie mein \u00e4u\u00df'res Leben,", "tokens": ["Mein", "inn'\u00b7res", "wie", "mein", "\u00e4u\u00df'\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KOKOM", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich selbst befohlen deiner Hut,", "tokens": ["Mich", "selbst", "be\u00b7foh\u00b7len", "dei\u00b7ner", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus meines Herzens dunklem Stollen", "tokens": ["Aus", "mei\u00b7nes", "Her\u00b7zens", "dunk\u00b7lem", "Stol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das reinste Gold dir dargereicht, \u2013", "tokens": ["Das", "reins\u00b7te", "Gold", "dir", "dar\u00b7ge\