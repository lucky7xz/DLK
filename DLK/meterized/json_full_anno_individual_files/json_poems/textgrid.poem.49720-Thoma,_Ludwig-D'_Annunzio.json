{"textgrid.poem.49720": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "D' Annunzio", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonst ein \u00c4sthet, der honigs\u00fc\u00dfen Maules", "tokens": ["Sonst", "ein", "\u00c4s\u00b7thet", ",", "der", "ho\u00b7nig\u00b7s\u00fc\u00b7\u00dfen", "Mau\u00b7les"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und wiederkauend gab ein Wortgepr\u00e4ng ...", "tokens": ["Und", "wie\u00b7der\u00b7kau\u00b7end", "gab", "ein", "Wort\u00b7ge\u00b7pr\u00e4ng", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kam Pfeffer in den Hintern deines Gaules,", "tokens": ["Kam", "Pfef\u00b7fer", "in", "den", "Hin\u00b7tern", "dei\u00b7nes", "Gau\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df er so wiehernd st\u00fcrzt in das Gedr\u00e4ng?", "tokens": ["Da\u00df", "er", "so", "wie\u00b7hernd", "st\u00fcrzt", "in", "das", "Ge\u00b7dr\u00e4ng", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Du Dichter samtner Pf\u00fchle, seidner Fransen,", "tokens": ["Du", "Dich\u00b7ter", "samt\u00b7ner", "Pf\u00fch\u00b7le", ",", "seid\u00b7ner", "Fran\u00b7sen", ","], "token_info": [