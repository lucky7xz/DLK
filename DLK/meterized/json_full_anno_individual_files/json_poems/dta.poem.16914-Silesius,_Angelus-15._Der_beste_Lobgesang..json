{"dta.poem.16914": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "15.  Der beste Lobgesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Singt singt jhr Engel singt: mit h\u0169dert tausend Zungen", "tokens": ["Singt", "singt", "jhr", "En\u00b7gel", "singt", ":", "mit", "h\u0169\u00b7dert", "tau\u00b7send", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "VVFIN", "$.", "APPR", "VVFIN", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird dieses wehrte Kind nicht w\u00fcrdiglich besungen.", "tokens": ["Wird", "die\u00b7ses", "wehr\u00b7te", "Kind", "nicht", "w\u00fcr\u00b7dig\u00b7lich", "be\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach m\u00f6cht\u2019 ich ohne Zung/ und ohne Stimme seyn!", "tokens": ["Ach", "m\u00f6cht'", "ich", "oh\u00b7ne", "Zung", "/", "und", "oh\u00b7ne", "Stim\u00b7me", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "NN", "$(", "KON", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich wei\u00df ich s\u00e4ng\u2019 jhm straks das liebste Liedelein.", "tokens": ["Ich", "wei\u00df", "ich", "s\u00e4ng'", "jhm", "straks", "das", "liebs\u00b7te", "Lie\u00b7de\u00b7lein", "."], "token_info": ["word", "word"