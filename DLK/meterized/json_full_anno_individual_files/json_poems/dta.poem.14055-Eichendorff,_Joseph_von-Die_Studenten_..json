{"dta.poem.14055": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Studenten .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ade nun, Kind, und nicht geweint!", "tokens": ["A\u00b7de", "nun", ",", "Kind", ",", "und", "nicht", "ge\u00b7weint", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "$,", "KON", "PTKNEG", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Schon gehen Stimmen da und dort,", "tokens": ["Schon", "ge\u00b7hen", "Stim\u00b7men", "da", "und", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "KON", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Hoch \u00fcbern Wald Aurora scheint,", "tokens": ["Hoch", "\u00fc\u00b7bern", "Wald", "Au\u00b7ro\u00b7ra", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und die Studenten reisen fort.", "tokens": ["Und", "die", "Stu\u00b7den\u00b7ten", "rei\u00b7sen", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}