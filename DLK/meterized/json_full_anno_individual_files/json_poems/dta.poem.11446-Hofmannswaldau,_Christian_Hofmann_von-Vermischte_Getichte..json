{"dta.poem.11446": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte Getichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das gl\u00fccke kan uns nicht die wahre hoheit geben:", "tokens": ["Das", "gl\u00fc\u00b7cke", "kan", "uns", "nicht", "die", "wah\u00b7re", "ho\u00b7heit", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VMFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn dieses goldne flies erfordert eigne m\u00fch.", "tokens": ["Denn", "die\u00b7ses", "gold\u00b7ne", "flies", "er\u00b7for\u00b7dert", "eig\u00b7ne", "m\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "VVFIN", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die cron erh\u00f6ht uns nicht; Nein! wir erh\u00f6hen sie,", "tokens": ["Die", "cron", "er\u00b7h\u00f6ht", "uns", "nicht", ";", "Nein", "!", "wir", "er\u00b7h\u00f6\u00b7hen", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "PTKANT", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indem wir ihre last auf unser haupt erheben.", "tokens": ["In\u00b7dem", "wir", "ih\u00b7re", "last", "auf", "un\u00b7ser", "haupt", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein zwerg h\u00f6rt in der h\u00f6h nicht auf, ein zwerg zu seyn:", "tokens": ["Ein", "zwerg", "h\u00f6rt", "in", "der", "h\u00f6h", "nicht", "auf", ",", "ein", "zwerg", "zu", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKNEG", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und in der tieffen wird kein grosser riese klein;", "tokens": ["Und", "in", "der", "tief\u00b7fen", "wird", "kein", "gros\u00b7ser", "rie\u00b7se", "klein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "VAFIN", "PIAT", "ADJA", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch kan ein ries\u2019 ein zwerg durch knechtische geb\u00e4rden,", "tokens": ["Doch", "kan", "ein", "ries'", "ein", "zwerg", "durch", "knech\u00b7ti\u00b7sche", "ge\u00b7b\u00e4r\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "VVFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie durch verstand und muth, ein zwerg, ein riese werden.", "tokens": ["Wie", "durch", "ver\u00b7stand", "und", "muth", ",", "ein", "zwerg", ",", "ein", "rie\u00b7se", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "VVFIN", "KON", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}