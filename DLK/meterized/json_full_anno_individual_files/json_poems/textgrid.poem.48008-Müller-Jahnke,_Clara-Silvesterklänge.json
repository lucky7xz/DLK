{"textgrid.poem.48008": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Silvesterkl\u00e4nge", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eisnebel dr\u00e4ngen vom grauen Meer", "tokens": ["Eis\u00b7ne\u00b7bel", "dr\u00e4n\u00b7gen", "vom", "grau\u00b7en", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "gespenstisch \u00fcber die D\u00fcnen her", "tokens": ["ge\u00b7spens\u00b7tisch", "\u00fc\u00b7ber", "die", "D\u00fc\u00b7nen", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "APZR"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "und h\u00fcllen in fr\u00fchen D\u00e4mmerschein", "tokens": ["und", "h\u00fcl\u00b7len", "in", "fr\u00fc\u00b7hen", "D\u00e4m\u00b7mer\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "die schneelichtleuchtenden Weiten ein", "tokens": ["die", "schnee\u00b7licht\u00b7leuch\u00b7ten\u00b7den", "Wei\u00b7ten", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "und ziehen die schimmernden Spinneweben", "tokens": ["und", "zie\u00b7hen", "die", "schim\u00b7mern\u00b7den", "Spin\u00b7ne\u00b7we\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u00fcber des Waldes erstarrtes Leben.", "tokens": ["\u00fc\u00b7ber", "des", "Wal\u00b7des", "er\u00b7starr\u00b7tes", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "\u2013 Einsam schreit ich im tiefen Hag \u2013", "tokens": ["\u2013", "Ein\u00b7sam", "schreit", "ich", "im", "tie\u00b7fen", "Hag", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "ein Rabe mit lautlosem Fl\u00fcgelschlag", "tokens": ["ein", "Ra\u00b7be", "mit", "laut\u00b7lo\u00b7sem", "Fl\u00fc\u00b7gel\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "streift vom aufschnellenden Tannenast", "tokens": ["streift", "vom", "auf\u00b7schnel\u00b7len\u00b7den", "Tan\u00b7ne\u00b7nast"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "die wei\u00dfe, st\u00e4ubende Winterlast;", "tokens": ["die", "wei\u00b7\u00dfe", ",", "st\u00e4u\u00b7ben\u00b7de", "Win\u00b7ter\u00b7last", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "und durch die L\u00fcfte, verd\u00e4mmernd weit,", "tokens": ["und", "durch", "die", "L\u00fcf\u00b7te", ",", "ver\u00b7d\u00e4m\u00b7mernd", "weit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "VVPP", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "schwimmen die Stimmen der Einsamkeit . . . . .", "tokens": ["schwim\u00b7men", "die", "Stim\u00b7men", "der", "Ein\u00b7sam\u00b7keit", ".", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$.", "$.", "$.", "$.", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Sie fl\u00fcstern heimlich wie Fr\u00fchlingswind,", "tokens": ["Sie", "fl\u00fcs\u00b7tern", "heim\u00b7lich", "wie", "Fr\u00fch\u00b7lings\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "wenn rings der Saft in den Zweigen rinnt,", "tokens": ["wenn", "rings", "der", "Saft", "in", "den", "Zwei\u00b7gen", "rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "sie raunen z\u00e4rtlich wie Liebesgru\u00df,", "tokens": ["sie", "rau\u00b7nen", "z\u00e4rt\u00b7lich", "wie", "Lie\u00b7bes\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "wie ein wonneschauernder Brautnachtku\u00df,", "tokens": ["wie", "ein", "won\u00b7ne\u00b7schau\u00b7ern\u00b7der", "Braut\u00b7nacht\u00b7ku\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "sie weinen schmerzlich wie Klagesang", "tokens": ["sie", "wei\u00b7nen", "schmerz\u00b7lich", "wie", "Kla\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "ADJD", "KOKOM", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und sie schwellen zum hellen Glockenklang \u2013 \u2013", "tokens": ["und", "sie", "schwel\u00b7len", "zum", "hel\u00b7len", "Glo\u00b7cken\u00b7klang", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$(", "$("], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "von allen T\u00fcrmen gr\u00fc\u00dfen, locken \u2013", "tokens": ["von", "al\u00b7len", "T\u00fcr\u00b7men", "gr\u00fc\u00b7\u00dfen", ",", "lo\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "l\u00e4uten und st\u00fcrmen Silvesterglocken!", "tokens": ["l\u00e4u\u00b7ten", "und", "st\u00fcr\u00b7men", "Sil\u00b7ves\u00b7ter\u00b7glo\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.11": {"line.1": {"text": "Ein blutiges Rot im Westen bl\u00fcht,", "tokens": ["Ein", "blu\u00b7ti\u00b7ges", "Rot", "im", "Wes\u00b7ten", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ein brausender Windsto\u00df kommt aus S\u00fcd,", "tokens": ["ein", "brau\u00b7sen\u00b7der", "Wind\u00b7sto\u00df", "kommt", "aus", "S\u00fcd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "und der Schnee st\u00e4ubt auf \u2013 und es will auf Erden", "tokens": ["und", "der", "Schnee", "st\u00e4ubt", "auf", "\u2013", "und", "es", "will", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "$(", "KON", "PPER", "VMFIN", "APPR", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ein neues Jahr geboren werden.", "tokens": ["ein", "neu\u00b7es", "Jahr", "ge\u00b7bo\u00b7ren", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Ein neues Jahr, eine neue Zeit . . . . . .", "tokens": ["Ein", "neu\u00b7es", "Jahr", ",", "ei\u00b7ne", "neu\u00b7e", "Zeit", ".", ".", ".", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$.", "$.", "$.", "$.", "$.", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus der schweigenden Schneeeinsamkeit", "tokens": ["Aus", "der", "schwei\u00b7gen\u00b7den", "Schnee\u00b7e\u00b7in\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.14": {"line.1": {"text": "kehre ich heim; da glei\u00dft und bricht", "tokens": ["keh\u00b7re", "ich", "heim", ";", "da", "glei\u00dft", "und", "bricht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "KON", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "aus breiten Fenstern ein Strom von Licht", "tokens": ["aus", "brei\u00b7ten", "Fens\u00b7tern", "ein", "Strom", "von", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.15": {"line.1": {"text": "und t\u00f6nt ein Lachen und Gl\u00e4serklingen:", "tokens": ["und", "t\u00f6nt", "ein", "La\u00b7chen", "und", "Gl\u00e4\u00b7ser\u00b7klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "sie feiern Silvester mit Scherz und Singen.", "tokens": ["sie", "fei\u00b7ern", "Sil\u00b7ves\u00b7ter", "mit", "Scherz", "und", "Sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.16": {"line.1": {"text": "Vor\u00fcber an prunkender Villen Geheg", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "an", "prun\u00b7ken\u00b7der", "Vil\u00b7len", "Ge\u00b7heg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "durch schmutzige Gassen f\u00fchrt mein Weg.", "tokens": ["durch", "schmut\u00b7zi\u00b7ge", "Gas\u00b7sen", "f\u00fchrt", "mein", "Weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.17": {"line.1": {"text": "Hier t\u00f6nt nur Fluchen; ein tr\u00fcbes Licht", "tokens": ["Hier", "t\u00f6nt", "nur", "Flu\u00b7chen", ";", "ein", "tr\u00fc\u00b7bes", "Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "tr\u00e4g durch befrorene Scheiben bricht.", "tokens": ["tr\u00e4g", "durch", "be\u00b7fro\u00b7re\u00b7ne", "Schei\u00b7ben", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.18": {"line.1": {"text": "Das Elend h\u00fctet des Hauses Schwelle, \u2013", "tokens": ["Das", "E\u00b7lend", "h\u00fc\u00b7tet", "des", "Hau\u00b7ses", "Schwel\u00b7le", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "an der erkalteten Feuerstelle", "tokens": ["an", "der", "er\u00b7kal\u00b7te\u00b7ten", "Feu\u00b7er\u00b7stel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.19": {"line.1": {"text": "hockt die Verzweiflung und stiert und lacht", "tokens": ["hockt", "die", "Ver\u00b7zwei\u00b7flung", "und", "stiert", "und", "lacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "gell auf in der eisigen Winternacht . . . . .", "tokens": ["gell", "auf", "in", "der", "ei\u00b7si\u00b7gen", "Win\u00b7ter\u00b7nacht", ".", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["ADJD", "APPR", "APPR", "ART", "ADJA", "NN", "$.", "$.", "$.", "$.", "$."], "meter": "----+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.20": {"line.1": {"text": "Da, horch: aus den L\u00fcften ein Glockenchor!", "tokens": ["Da", ",", "horch", ":", "aus", "den", "L\u00fcf\u00b7ten", "ein", "Glo\u00b7cken\u00b7chor", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVIMP", "$.", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da, schau: aus des G\u00e4\u00dfchens niedrigem Tor", "tokens": ["Da", ",", "schau", ":", "aus", "des", "G\u00e4\u00df\u00b7chens", "nied\u00b7ri\u00b7gem", "Tor"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PTKVZ", "$.", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.21": {"line.1": {"text": "tritt wei\u00dfgewandet ein leuchtend Kind,", "tokens": ["tritt", "wei\u00df\u00b7ge\u00b7wan\u00b7det", "ein", "leuch\u00b7tend", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "so zart und hold, wie die Engel sind.", "tokens": ["so", "zart", "und", "hold", ",", "wie", "die", "En\u00b7gel", "sind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "PWAV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.22": {"line.1": {"text": "Mit blo\u00dfen F\u00fc\u00dfchen im kalten Schnee", "tokens": ["Mit", "blo\u00b7\u00dfen", "F\u00fc\u00df\u00b7chen", "im", "kal\u00b7ten", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "es l\u00e4chelt sonnig: ihm tut's nicht weh \u2013", "tokens": ["es", "l\u00e4\u00b7chelt", "son\u00b7nig", ":", "ihm", "tut's", "nicht", "weh", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.23": {"line.1": {"text": "kommt es die Stra\u00dfe heraufgeschritten", "tokens": ["kommt", "es", "die", "Stra\u00b7\u00dfe", "her\u00b7auf\u00b7ge\u00b7schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "und steht vor dem Haus in des G\u00e4\u00dfchens Mitten", "tokens": ["und", "steht", "vor", "dem", "Haus", "in", "des", "G\u00e4\u00df\u00b7chens", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.24": {"line.1": {"text": "und pocht so leise wie Nachtgespenster", "tokens": ["und", "pocht", "so", "lei\u00b7se", "wie", "Nacht\u00b7ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "KOKOM", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "mit der leuchtenden Hand ans Kammerfenster.", "tokens": ["mit", "der", "leuch\u00b7ten\u00b7den", "Hand", "ans", "Kam\u00b7mer\u00b7fens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.25": {"line.1": {"text": "Und wie der Klang durch die Stube hallt,", "tokens": ["Und", "wie", "der", "Klang", "durch", "die", "Stu\u00b7be", "hallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "A