{"textgrid.poem.40394": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Ein Goldgr\u00e4ber", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann ein Goldgr\u00e4ber werden", "tokens": ["Ich", "kann", "ein", "Gold\u00b7gr\u00e4\u00b7ber", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "und des Erdgrunds Sch\u00e4tze sch\u00fcrfen.", "tokens": ["und", "des", "Erd\u00b7grunds", "Sch\u00e4t\u00b7ze", "sch\u00fcr\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mutter Erde spendet immer mehr,", "tokens": ["Mut\u00b7ter", "Er\u00b7de", "spen\u00b7det", "im\u00b7mer", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "je mehr die Menschen bed\u00fcrfen;", "tokens": ["je", "mehr", "die", "Men\u00b7schen", "be\u00b7d\u00fcr\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "mein Lehrer hat's gesagt.", "tokens": ["mein", "Leh\u00b7rer", "hat's", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wohl kostet's Schwei\u00df in Str\u00f6men,", "tokens": ["Wohl", "kos\u00b7tet's", "Schwei\u00df", "in", "Str\u00f6\u00b7men", ","], "token_info": 