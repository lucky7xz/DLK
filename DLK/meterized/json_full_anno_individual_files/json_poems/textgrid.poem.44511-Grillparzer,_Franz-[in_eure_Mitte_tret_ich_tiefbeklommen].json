{"textgrid.poem.44511": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[in eure Mitte tret ich tiefbeklommen]", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In eure Mitte tret ich tiefbeklommen,", "tokens": ["In", "eu\u00b7re", "Mit\u00b7te", "tret", "ich", "tief\u00b7be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Wehmut Wolk umd\u00fcstert meinen Geist,", "tokens": ["Der", "Weh\u00b7mut", "Wolk", "um\u00b7d\u00fcs\u00b7tert", "mei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Augenblick des Scheidens ist gekommen,", "tokens": ["Der", "Au\u00b7gen\u00b7blick", "des", "Schei\u00b7dens", "ist", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum zweitenmale f\u00fchl ich mich verwaist.", "tokens": ["Zum", "zwei\u00b7ten\u00b7ma\u00b7le", "f\u00fchl", "ich", "mich", "ver\u00b7waist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verlassen soll ich diese trauten W\u00e4nde,", "tokens": ["Ver\u00b7las\u00b7sen", "soll", "ich", "die\u00b7se", "trau\u00b7ten", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo mich der Hauch der Kunst zuerst ber\u00fchrt,", "tokens": ["Wo", "mich", "der", "Hauch", "der", "Kunst", "zu\u00b7erst", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo liebevoll so treubefli\u00dfne H\u00e4nde", "tokens": ["Wo", "lie\u00b7be\u00b7voll", "so", "treu\u00b7be\u00b7fli\u00df\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ins Reich des Klangs zuerst mich eingef\u00fchrt.", "tokens": ["Ins", "Reich", "des", "Klangs", "zu\u00b7erst", "mich", "ein\u00b7ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wo mein Gem\u00fct, erweckt zu sch\u00f6nerm Streben,", "tokens": ["Wo", "mein", "Ge\u00b7m\u00fct", ",", "er\u00b7weckt", "zu", "sch\u00f6\u00b7nerm", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Keim empfangen, der, mit Gott, gepflegt,", "tokens": ["Den", "Keim", "emp\u00b7fan\u00b7gen", ",", "der", ",", "mit", "Gott", ",", "ge\u00b7pflegt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "$,", "APPR", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gedeihn und bl\u00fchn und wachsen soll im Leben,", "tokens": ["Ge\u00b7deihn", "und", "bl\u00fchn", "und", "wach\u00b7sen", "soll", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "KON", "VVINF", "VMFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis er einst lohnend s\u00fc\u00dfe Frucht mir tr\u00e4gt.", "tokens": ["Bis", "er", "einst", "loh\u00b7nend", "s\u00fc\u00b7\u00dfe", "Frucht", "mir", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Drum la\u00dft mich danken, eh ich trauernd gehe;", "tokens": ["Drum", "la\u00dft", "mich", "dan\u00b7ken", ",", "eh", "ich", "trau\u00b7ernd", "ge\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor allen, Dank dir, gute Meisterin!", "tokens": ["Vor", "al\u00b7len", ",", "Dank", "dir", ",", "gu\u00b7te", "Meis\u00b7te\u00b7rin", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "APPR", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was ich gewann, durch deine Lehr und N\u00e4he,", "tokens": ["Was", "ich", "ge\u00b7wann", ",", "durch", "dei\u00b7ne", "Lehr", "und", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach! unverge\u00dflich lebts in meinem Sinn.", "tokens": ["Ach", "!", "un\u00b7ver\u00b7ge\u00df\u00b7lich", "lebts", "in", "mei\u00b7nem", "Sinn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du gabst mir nicht blo\u00df hohle Wort und T\u00f6ne,", "tokens": ["Du", "gabst", "mir", "nicht", "blo\u00df", "hoh\u00b7le", "Wort", "und", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Herzen sprachst du liebend an mein Herz;", "tokens": ["Vom", "Her\u00b7zen", "sprachst", "du", "lie\u00b7bend", "an", "mein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und zeigtest mir, wie nur das wahre Sch\u00f6ne,", "tokens": ["Und", "zeig\u00b7test", "mir", ",", "wie", "nur", "das", "wah\u00b7re", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die reine Seele fl\u00fcgelt himmelw\u00e4rts.", "tokens": ["Die", "rei\u00b7ne", "See\u00b7le", "fl\u00fc\u00b7gelt", "him\u00b7mel\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Drum habe Dank! Dank auch den Edlen allen,", "tokens": ["Drum", "ha\u00b7be", "Dank", "!", "Dank", "auch", "den", "Ed\u00b7len", "al\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "$.", "APPR", "ADV", "ART", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die dieser Anstalt ihre Sorge weihn,", "tokens": ["Die", "die\u00b7ser", "An\u00b7stalt", "ih\u00b7re", "Sor\u00b7ge", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nie wird ihr Nam in meinem Ohr verhallen,", "tokens": ["Nie", "wird", "ihr", "Nam", "in", "mei\u00b7nem", "Ohr", "ver\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob fern auch, werden sie mir nahe sein.", "tokens": ["Ob", "fern", "auch", ",", "wer\u00b7den", "sie", "mir", "na\u00b7he", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "$,", "VAFIN", "PPER", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Sie werden stets im Geiste mich begleiten,", "tokens": ["Sie", "wer\u00b7den", "stets", "im", "Geis\u00b7te", "mich", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohin auch wechselnd mir das Schicksal winkt,", "tokens": ["Wo\u00b7hin", "auch", "wech\u00b7selnd", "mir", "das", "Schick\u00b7sal", "winkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Bild wird tr\u00f6stend mir zur Seite schreiten,", "tokens": ["Ihr", "Bild", "wird", "tr\u00f6s\u00b7tend", "mir", "zur", "Sei\u00b7te", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn mir der Mut bisweilen zagend sinkt.", "tokens": ["Wenn", "mir", "der", "Mut", "bis\u00b7wei\u00b7len", "za\u00b7gend", "sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und so, ger\u00fcstet mit dem besten Willen,", "tokens": ["Und", "so", ",", "ge\u00b7r\u00fcs\u00b7tet", "mit", "dem", "bes\u00b7ten", "Wil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beseelt vom Danke, scheid ich nun von hier.", "tokens": ["Be\u00b7seelt", "vom", "Dan\u00b7ke", ",", "scheid", "ich", "nun", "von", "hier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "PTKANT", "$,", "VVFIN", "PPER", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gott m\u00f6ge meinen Wunsch f\u00fcr euch erf\u00fcllen,", "tokens": ["Gott", "m\u00f6\u00b7ge", "mei\u00b7nen", "Wunsch", "f\u00fcr", "euch", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und euer bester Segen folge mir.", "tokens": ["Und", "eu\u00b7er", "bes\u00b7ter", "Se\u00b7gen", "fol\u00b7ge", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Und sollt ich einst die Heimat wiedersehen,", "tokens": ["Und", "sollt", "ich", "einst", "die", "Hei\u00b7mat", "wie\u00b7der\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor euch erprobend, was ihr selbst mir gabt,", "tokens": ["Vor", "euch", "er\u00b7pro\u00b7bend", ",", "was", "ihr", "selbst", "mir", "gabt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "PWS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So m\u00f6gt ihr freundlich lohnend mir gestehen,", "tokens": ["So", "m\u00f6gt", "ihr", "freund\u00b7lich", "loh\u00b7nend", "mir", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVPP", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ihr euch meiner nicht zu sch\u00e4men habt.", "tokens": ["Da\u00df", "ihr", "euch", "mei\u00b7ner", "nicht", "zu", "sch\u00e4\u00b7men", "habt", "."], "token