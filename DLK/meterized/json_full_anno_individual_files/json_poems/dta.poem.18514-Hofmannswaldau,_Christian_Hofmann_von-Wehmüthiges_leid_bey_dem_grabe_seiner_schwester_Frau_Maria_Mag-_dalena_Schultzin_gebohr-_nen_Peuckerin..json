{"dta.poem.18514": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Wehm\u00fcthiges leid bey dem grabe  \n seiner schwester/ Frau Maria Mag-  \n dalena Schultzin/ gebohr-  \n nen Peuckerin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr/ die ihr viel verdru\u00df/ ja viel gefahr und wunden", "tokens": ["Ihr", "/", "die", "ihr", "viel", "ver\u00b7dru\u00df", "/", "ja", "viel", "ge\u00b7fahr", "und", "wun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$(", "PRELS", "PPER", "PIAT", "NN", "$(", "ADV", "PIAT", "NN", "KON", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff langen reisen euch zu wege habt gebracht/", "tokens": ["Auff", "lan\u00b7gen", "rei\u00b7sen", "euch", "zu", "we\u00b7ge", "habt", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "PPER", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sagt/ ob ihr iemahls wohl so schweren schmertz empfunden/", "tokens": ["Sagt", "/", "ob", "ihr", "ie\u00b7mahls", "wohl", "so", "schwe\u00b7ren", "schmertz", "emp\u00b7fun\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "PPER", "ADV", "ADV", "ADV", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dergleichen neulich mir ein kurtzer weg gemacht?", "tokens": ["Derg\u00b7lei\u00b7chen", "neu\u00b7lich", "mir", "ein", "kurt\u00b7zer", "weg", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "PPER", "ART", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich hatte mir den Rhein zu sehen vorgenommen/", "tokens": ["Ich", "hat\u00b7te", "mir", "den", "Rhein", "zu", "se\u00b7hen", "vor\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NE", "PTKZU", "VVINF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der einst mein thr\u00e4nen-saltz hat in die see gef\u00fchrt/", "tokens": ["Der", "einst", "mein", "thr\u00e4\u00b7nen\u00b7saltz", "hat", "in", "die", "see", "ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und wolte noch zuvor nach unserm Torgau kommen/", "tokens": ["Und", "wol\u00b7te", "noch", "zu\u00b7vor", "nach", "un\u00b7serm", "Tor\u00b7gau", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Allwo mein fu\u00df zuerst hat diese welt ber\u00fchrt;", "tokens": ["All\u00b7wo", "mein", "fu\u00df", "zu\u00b7erst", "hat", "die\u00b7se", "welt", "be\u00b7r\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "VAFIN", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hier solte meine brust sich mit der schwester letzen/", "tokens": ["Hier", "sol\u00b7te", "mei\u00b7ne", "brust", "sich", "mit", "der", "schwes\u00b7ter", "let\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PRF", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die von geschwistern mir GOTT gantz allein geg\u00f6nnt/", "tokens": ["Die", "von", "ge\u00b7schwis\u00b7tern", "mir", "GoTT", "gantz", "al\u00b7lein", "ge\u00b7g\u00f6nnt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "PPER", "NE", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hier wolt ich mich mit ihr und ihrem mann erg\u00f6tzen/", "tokens": ["Hier", "wolt", "ich", "mich", "mit", "ihr", "und", "ih\u00b7rem", "mann", "er\u00b7g\u00f6t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den meine vater-stadt bey vielen w\u00fcrden kennt.", "tokens": ["Den", "mei\u00b7ne", "va\u00b7ter\u00b7stadt", "bey", "vie\u00b7len", "w\u00fcr\u00b7den", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PIS", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und freylich ward ich dort mit freudigkeit empfangen.", "tokens": ["Und", "frey\u00b7lich", "ward", "ich", "dort", "mit", "freu\u00b7dig\u00b7keit", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die schwester/ wie sie sonst bey ieden freundlich war/", "tokens": ["Die", "schwes\u00b7ter", "/", "wie", "sie", "sonst", "bey", "ie\u00b7den", "freund\u00b7lich", "war", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "PWAV", "PPER", "ADV", "APPR", "PIAT", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Empfieng uns allerseits mit sehnlichem verlangen/", "tokens": ["Emp\u00b7fi\u00b7eng", "uns", "al\u00b7ler\u00b7seits", "mit", "sehn\u00b7li\u00b7chem", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "VVINF", "$("], "meter": "+---+-+-+-+-+-", "measure": "dactylic.init"}, "line.16": {"text": "Man bot uns alle treu und alle dienste dar.", "tokens": ["Man", "bot", "uns", "al\u00b7le", "treu", "und", "al\u00b7le", "diens\u00b7te", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PIS", "ADJD", "KON", "PIAT", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch/ als die nacht erschien/ (o nacht voll angst und schrecken)", "tokens": ["Doch", "/", "als", "die", "nacht", "er\u00b7schien", "/", "(", "o", "nacht", "voll", "angst", "und", "schre\u00b7cken", ")"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "ART", "NN", "VVFIN", "$(", "$(", "FM", "NN", "ADJD", "VVPP", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "O nacht/ an die ich nicht ohn thr\u00e4nen dencken kan!)", "tokens": ["O", "nacht", "/", "an", "die", "ich", "nicht", "ohn", "thr\u00e4\u00b7nen", "den\u00b7cken", "kan", "!", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$(", "APPR", "PRELS", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Gieng meine schwester hin mein lager mir zu decken/", "tokens": ["Gieng", "mei\u00b7ne", "schwes\u00b7ter", "hin", "mein", "la\u00b7ger", "mir", "zu", "de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "ADV", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und macht\u2019 ihr leider! selbst zur letzten ruh die bahn.", "tokens": ["Und", "macht'", "ihr", "lei\u00b7der", "!", "selbst", "zur", "letz\u00b7ten", "ruh", "die", "bahn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$.", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Denn als sie von der h\u00f6h zur\u00fccke sich gewendet/", "tokens": ["Denn", "als", "sie", "von", "der", "h\u00f6h", "zu\u00b7r\u00fc\u00b7cke", "sich", "ge\u00b7wen\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "ADJD", "VVFIN", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Blie\u00df ihr ein starcker wind das licht in h\u00e4nden aus.", "tokens": ["Blie\u00df", "ihr", "ein", "star\u00b7cker", "wind", "das", "licht", "in", "h\u00e4n\u00b7den", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "VAFIN", "ART", "NN", "APPR", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Durch diesen sturm ward auch ihr lebens-licht geendet:", "tokens": ["Durch", "die\u00b7sen", "sturm", "ward", "auch", "ihr", "le\u00b7bens\u00b7licht", "ge\u00b7en\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Es brachte sie ein fall ins finstre todten-hau\u00df.", "tokens": ["Es", "brach\u00b7te", "sie", "ein", "fall", "ins", "finst\u00b7re", "tod\u00b7ten\u00b7hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.25": {"text": "Der abend-stern/ der sie noch gantz gesund gesehen/", "tokens": ["Der", "a\u00b7ben\u00b7ds\u00b7tern", "/", "der", "sie", "noch", "gantz", "ge\u00b7sund", "ge\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Erblickte sie bereits am morgen auff der baar.", "tokens": ["Er\u00b7blick\u00b7te", "sie", "be\u00b7reits", "am", "mor\u00b7gen", "auff", "der", "baar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADV", "APPR", "ART", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und wenn mein geist bedenckt/ wie damals mir geschehen/", "tokens": ["Und", "wenn", "mein", "geist", "be\u00b7denckt", "/", "wie", "da\u00b7mals", "mir", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVFIN", "$(", "KOKOM", "ADV", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So starret mir das hertz/ so zittert haupt und haar.", "tokens": ["So", "star\u00b7ret", "mir", "das", "hertz", "/", "so", "zit\u00b7tert", "haupt", "und", "haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$(", "ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Es traf nur leider ein/ was zeichen/ fall und heulen/", "tokens": ["Es", "traf", "nur", "lei\u00b7der", "ein", "/", "was", "zei\u00b7chen", "/", "fall", "und", "heu\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "$(", "PWS", "VVFIN", "$(", "NN", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und manche weheklag zuvor hat angedeut/", "tokens": ["Und", "man\u00b7che", "we\u00b7he\u00b7klag", "zu\u00b7vor", "hat", "an\u00b7ge\u00b7deut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ja/ was sie selber offt zur nachricht wolt ertheilen/", "tokens": ["Ja", "/", "was", "sie", "sel\u00b7ber", "offt", "zur", "nach\u00b7richt", "wolt", "er\u00b7thei\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PWS", "PPER", "ADV", "ADV", "APPRART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wenn sie den nahen tod ihr mehrmahls prophezeyt.", "tokens": ["Wenn", "sie", "den", "na\u00b7hen", "tod", "ihr", "mehr\u00b7mahls", "pro\u00b7phe\u00b7zeyt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ach schwester/ liebstes hertz! ach da\u00df ich dich soll missen/", "tokens": ["Ach", "schwes\u00b7ter", "/", "liebs\u00b7tes", "hertz", "!", "ach", "da\u00df", "ich", "dich", "soll", "mis\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJD", "$(", "ADJA", "NN", "$.", "XY", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die du mir auff der welt so manche freud erweckt/", "tokens": ["Die", "du", "mir", "auff", "der", "welt", "so", "man\u00b7che", "freud", "er\u00b7weckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "APPR", "ART", "NN", "ADV", "PIS", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ach da\u00df ich dich itzt soll mit sand bedecket wissen/", "tokens": ["Ach", "da\u00df", "ich", "dich", "itzt", "soll", "mit", "sand", "be\u00b7de\u00b7cket", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "PRF", "ADV", "VMFIN", "APPR", "NE", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da vormals dich und mich ein mutter-hertz bedeckt!", "tokens": ["Da", "vor\u00b7mals", "dich", "und", "mich", "ein", "mut\u00b7ter\u00b7hertz", "be\u00b7deckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "KON", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Was aber will ich thun? ich mu\u00df mich damit tr\u00f6sten/", "tokens": ["Was", "a\u00b7ber", "will", "ich", "thun", "?", "ich", "mu\u00df", "mich", "da\u00b7mit", "tr\u00f6s\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPER", "VVINF", "$.", "PPER", "VMFIN", "PRF", "PAV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Da\u00df du dich schon vorl\u00e4ngst zum sterben hast geschickt/", "tokens": ["Da\u00df", "du", "dich", "schon", "vor\u00b7l\u00e4ngst", "zum", "ster\u00b7ben", "hast", "ge\u00b7schickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "APPRART", "VVINF", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und da\u00df du zu der zahl und menge der erl\u00f6sten", "tokens": ["Und", "da\u00df", "du", "zu", "der", "zahl", "und", "men\u00b7ge", "der", "er\u00b7l\u00f6s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "KON", "NN", "ART", "ADJA"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.40": {"text": "Bist ohne langen schmertz mit seel und geist ger\u00fcckt.", "tokens": ["Bist", "oh\u00b7ne", "lan\u00b7gen", 