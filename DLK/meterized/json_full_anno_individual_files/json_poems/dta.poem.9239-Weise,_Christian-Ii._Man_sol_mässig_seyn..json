{"dta.poem.9239": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Ii.  \n Man sol m\u00e4ssig seyn.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Zuviel/ zuviel/ zuviel/ was hat euch so besessen/", "tokens": ["Zu\u00b7viel", "/", "zu\u00b7viel", "/", "zu\u00b7viel", "/", "was", "hat", "euch", "so", "be\u00b7ses\u00b7sen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PIS", "$(", "PIS", "$(", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ihr nichts anders schertzt/", "tokens": ["Da\u00df", "ihr", "nichts", "an\u00b7ders", "schertzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADV", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als da\u00df ihr immer hertzt/", "tokens": ["Als", "da\u00df", "ihr", "im\u00b7mer", "hertzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wo ihr einander wolt in einer woche fressen/", "tokens": ["Wo", "ihr", "ein\u00b7an\u00b7der", "wolt", "in", "ei\u00b7ner", "wo\u00b7che", "fres\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VMFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wird ein blosses kinderspiel", "tokens": ["So", "wird", "ein", "blos\u00b7ses", "kin\u00b7der\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zuviel/ zuviel/ zuviel.", "tokens": ["Zu\u00b7viel", "/", "zu\u00b7viel", "/", "zu\u00b7viel", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "PIS", "$(", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "2. Zuviel/ zuviel/ zuviel/ wolt ihr euch so vergn\u00fcgen", "tokens": ["Zu\u00b7viel", "/", "zu\u00b7viel", "/", "zu\u00b7viel", "/", "wolt", "ihr", "euch", "so", "ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "PIS", "$(", "PIS", "$(", "VMFIN", "PPER", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr habts zwar gute macht/", "tokens": ["Ihr", "habts", "zwar", "gu\u00b7te", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In dessen seyd bedacht/", "tokens": ["In", "des\u00b7sen", "seyd", "be\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VAFIN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie mancher hat den wein selbst in den keller liegen", "tokens": ["Wie", "man\u00b7cher", "hat", "den", "wein", "selbst", "in", "den", "kel\u00b7ler", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VAFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und trinck doch nicht so offt er wil/", "tokens": ["Und", "trinck", "doch", "nicht", "so", "offt", "er", "wil", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "PTKNEG", "ADV", "ADV", "PPER", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zuviel/ zuviel/ zuviel.", "tokens": ["Zu\u00b7viel", "/", "zu\u00b7viel", "/", "zu\u00b7viel", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "PIS", "$(", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "3. Zuviel/ zuviel/ zuviel/ was wird ein g\u00e4rtner sprechen/", "tokens": ["Zu\u00b7viel", "/", "zu\u00b7viel", "/", "zu\u00b7viel", "/", "was", "wird", "ein", "g\u00e4rt\u00b7ner", "spre\u00b7chen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PIS", "$(", "PIS", "$(", "PWS", "VAFIN", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann man zu keiner zeit/", "tokens": ["Wann", "man", "zu", "kei\u00b7ner", "zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ohn allen unterscheid", "tokens": ["Ohn", "al\u00b7len", "un\u00b7ter\u00b7scheid"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die angenehme lust der blumen wolte brechen.", "tokens": ["Die", "an\u00b7ge\u00b7neh\u00b7me", "lust", "der", "blu\u00b7men", "wol\u00b7te", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum geht und trefft das rechte ziel/", "tokens": ["Drum", "geht", "und", "trefft", "das", "rech\u00b7te", "ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zuviel/ zuviel/ zuviel/", "tokens": ["Zu\u00b7viel", "/", "zu\u00b7viel", "/", "zu\u00b7viel", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "PIS", "$(", "PIS", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "4. Zuviel/ zuviel/ zuviel/ ein koch/ der alle sachen", "tokens": ["Zu\u00b7viel", "/", "zu\u00b7viel", "/", "zu\u00b7viel", "/", "ein", "koch", "/", "der", "al\u00b7le", "sa\u00b7chen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "PIS", "$(", "PIS", "$(", "ART", "NE", "$(", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In lauter zucker setzt/", "tokens": ["In", "lau\u00b7ter", "zu\u00b7cker", "setzt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kan mehrentheils zu letzt/", "tokens": ["Kan", "meh\u00b7ren\u00b7theils", "zu", "letzt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So gut es erstlich schm\u00e4ckt/ den gr\u00f6sten eckel machen/", "tokens": ["So", "gut", "es", "erst\u00b7lich", "schm\u00e4ckt", "/", "den", "gr\u00f6s\u00b7ten", "ec\u00b7kel", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADJD", "VVFIN", "$(", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum braucht einmahl ein ander spiel/", "tokens": ["Drum", "braucht", "ein\u00b7mahl", "ein", "an\u00b7der", "spiel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ART", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zuviel/ zuviel/ zuviel.", "tokens": ["Zu\u00b7viel", "/", "zu\u00b7viel", "/", "zu\u00b7viel", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "PIS", "$(", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "5. Zuviel/ zuviel/ zuviel/ ihr m\u00fcst euch selbst verst\u00f6ren/", "tokens": ["Zu\u00b7viel", "/", "zu\u00b7viel", "/", "zu\u00b7viel", "/", "ihr", "m\u00fcst", "euch", "selbst", "ver\u00b7st\u00f6\u00b7ren", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PIS", "$(", "PIS", "$(", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ihr die finger leckt/", "tokens": ["Weil", "ihr", "die", "fin\u00b7ger", "leckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und weils am besten schmeckt/", "tokens": ["Und", "weils", "am", "bes\u00b7ten", "schmeckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PTKA", "ADJD", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So ist es allezeit am besten aufzuh\u00f6ren/", "tokens": ["So", "ist", "es", "al\u00b7le\u00b7zeit", "am", "bes\u00b7ten", "auf\u00b7zu\u00b7h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo man die lust erhalten wil/", "tokens": ["Wo", "man", "die", "lust", "er\u00b7hal\u00b7ten", "wil", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zuviel/ zuviel/ zuviel.", "tokens": ["Zu\u00b7viel", "/", "zu\u00b7viel", "/", "zu\u00b7viel", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "PIS", "$(", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "6. Zuviel/ zuviel/ zuviel/ ihr m\u00fcst euch etwas zecken/", "tokens": ["Zu\u00b7viel", "/", "zu\u00b7viel", "/", "zu\u00b7viel", "/", "ihr", "m\u00fcst", "euch", "et\u00b7was", "ze\u00b7cken", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PIS", "$(", "PIS", "$(", "PPER", "VMFIN", "PPER", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr m\u00fcsset/ auff den schein/", "tokens": ["Ihr", "m\u00fcs\u00b7set", "/", "auff", "den", "schein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein bi\u00dfgen b\u00f6se seyn/", "tokens": ["Ein", "bi\u00df\u00b7gen", "b\u00f6\u00b7se", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da m\u00fcst ihr sauer sehn/ da m\u00fcst ihr euch verstecken/", "tokens": ["Da", "m\u00fcst", "ihr", "sau\u00b7er", "sehn", "/", "da", "m\u00fcst", "ihr", "euch", "ver\u00b7ste\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$(", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So spiel man sonst das liebe-spiel/", "tokens": ["So", "spiel", "man", "sonst", "das", "lie\u00b7be\u00b7spiel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zuviel/ zuviel/ zuviel.", "tokens": ["Zu\u00b7viel", "/", "zu\u00b7viel", "/", "zu\u00b7viel", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "PIS", "$(", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}