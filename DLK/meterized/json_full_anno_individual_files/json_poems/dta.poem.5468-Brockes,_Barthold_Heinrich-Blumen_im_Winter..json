{"dta.poem.5468": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Blumen im Winter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist es m\u00f6glich, schon anjetzt, in der h\u00e4rtften Winter-Zeit,", "tokens": ["Ist", "es", "m\u00f6g\u00b7lich", ",", "schon", "an\u00b7jetzt", ",", "in", "der", "h\u00e4rtf\u00b7ten", "Win\u00b7ter\u00b7Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "ADV", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Da die Fluht mit Eis beleget, Garten, Feld und", "tokens": ["Da", "die", "Fluht", "mit", "Eis", "be\u00b7le\u00b7get", ",", "Gar\u00b7ten", ",", "Feld", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,", "NN", "$,", "NN", "KON"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Wald beschneit,", "tokens": ["Wald", "be\u00b7schneit", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Lieblich riechende Eyrenen, funckelnde Gentianellen,", "tokens": ["Lieb\u00b7lich", "rie\u00b7chen\u00b7de", "Ey\u00b7re\u00b7nen", ",", "fun\u00b7ckeln\u00b7de", "Gen\u00b7ti\u00b7a\u00b7nel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Crocos, Lilien-Convalljen, ja die sch\u00f6nste Pfirschen-Bl\u00fcth,", "tokens": ["Cro\u00b7cos", ",", "Li\u00b7li\u00b7en\u00b7Con\u00b7vall\u00b7jen", ",", "ja", "die", "sch\u00f6ns\u00b7te", "Pfir\u00b7schen\u00b7B\u00b7l\u00fcth", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Tulpen, Hyaeinth, Terzetten, wie man hier bewundernd", "tokens": ["Tul\u00b7pen", ",", "Hy\u00b7a\u00b7e\u00b7inth", ",", "Ter\u00b7zet\u00b7ten", ",", "wie", "man", "hier", "be\u00b7wun\u00b7dernd"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NE", "$,", "NN", "$,", "PWAV", "PIS", "ADV", "VVPP"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.7": {"text": "sieht,", "tokens": ["sieht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Zu erblicken, ja da\u00df solches m\u00f6glich, sich nur vorzustellen!", "tokens": ["Zu", "er\u00b7bli\u00b7cken", ",", "ja", "da\u00df", "sol\u00b7ches", "m\u00f6g\u00b7lich", ",", "sich", "nur", "vor\u00b7zu\u00b7stel\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADV", "KOUS", "PIS", "ADJD", "$,", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Ach, mein GOtt, durch deine G\u00fcte, nehm\u2019 ich in", "tokens": ["Ach", ",", "mein", "Gott", ",", "durch", "dei\u00b7ne", "G\u00fc\u00b7te", ",", "nehm'", "ich", "in"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPR"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "derselben Pracht,", "tokens": ["der\u00b7sel\u00b7ben", "Pracht", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Mit Vergn\u00fcgen, deine Weisheit, deine Wunder, deine", "tokens": ["Mit", "Ver\u00b7gn\u00fc\u00b7gen", ",", "dei\u00b7ne", "Weis\u00b7heit", ",", "dei\u00b7ne", "Wun\u00b7der", ",", "dei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "Macht,", "tokens": ["Macht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Die durch dich stets regen Kr\u00e4fte der Natur auch jetzt in", "tokens": ["Die", "durch", "dich", "stets", "re\u00b7gen", "Kr\u00e4f\u00b7te", "der", "Na\u00b7tur", "auch", "jetzt", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "ADV", "ADJA", "NN", "ART", "NN", "ADV", "ADV", "APPR"], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "acht,", "tokens": ["acht", ","], "token_info": ["word", "punct"], "pos": ["CARD", "$,"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Und, durch ihre holde Sch\u00f6nheit, auch im Frost recht", "tokens": ["Und", ",", "durch", "ih\u00b7re", "hol\u00b7de", "Sch\u00f6n\u00b7heit", ",", "auch", "im", "Frost", "recht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ADV", "APPRART", "NN", "ADJD"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.16": {"text": "angelacht,", "tokens": ["an\u00b7ge\u00b7lacht", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "F\u00fchl\u2019 ich, wie mein innerstes recht gelabet, recht erqvicket,", "tokens": ["F\u00fchl'", "ich", ",", "wie", "mein", "in\u00b7ners\u00b7tes", "recht", "ge\u00b7la\u00b7bet", ",", "recht", "er\u00b7qvic\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOKOM", "PPOSAT", "ADJA", "NN", "VVPP", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.18": {"text": "Ja, durch \u00e4msige Betrachtung ihres Schmucks, fast selbst", "tokens": ["Ja", ",", "durch", "\u00e4m\u00b7si\u00b7ge", "Be\u00b7trach\u00b7tung", "ih\u00b7res", "Schmucks", ",", "fast", "selbst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "ADJA", "NN", "PPOSAT", "NN", "$,", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.19": {"text": "geschm\u00fccket,", "tokens": ["ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Zu dir hingezogen wird. Diese Pracht, die sie erblickt,", "tokens": ["Zu", "dir", "hin\u00b7ge\u00b7zo\u00b7gen", "wird", ".", "Die\u00b7se", "Pracht", ",", "die", "sie", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "$.", "PDAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.21": {"text": "Wird ihr gleichsam zugeeignet, und, wie wir, noch einst", "tokens": ["Wird", "ihr", "gleich\u00b7sam", "zu\u00b7ge\u00b7eig\u00b7net", ",", "und", ",", "wie", "wir", ",", "noch", "einst"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$,", "KON", "$,", "PWAV", "PPER", "$,", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.22": {"text": "so sch\u00f6n,", "tokens": ["so", "sch\u00f6n", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.23": {"text": "Einer Sch\u00f6nen zarte Haut, bey ihr nahen Blumen, sehn;", "tokens": ["Ei\u00b7ner", "Sch\u00f6\u00b7nen", "zar\u00b7te", "Haut", ",", "bey", "ihr", "na\u00b7hen", "Blu\u00b7men", ",", "sehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Stellt sich eine frohe Seele, wenn sie Blumen so besieht,", "tokens": ["Stellt", "sich", "ei\u00b7ne", "fro\u00b7he", "See\u00b7le", ",", "wenn", "sie", "Blu\u00b7men", "so", "be\u00b7sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.25": {"text": "In selbst bl\u00fchenden Jdeen w\u00fcrcklich selbst versch\u00f6nert mir,", "tokens": ["In", "selbst", "bl\u00fc\u00b7hen\u00b7den", "Jdeen", "w\u00fcrck\u00b7lich", "selbst", "ver\u00b7sch\u00f6\u00b7nert", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "ADJD", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.26": {"text": "Bey der weissen Hyacinth und der rohten Pfirsich-Bl\u00fct\u2019,", "tokens": ["Bey", "der", "weis\u00b7sen", "Hya\u00b7cin\u00b7th", "und", "der", "roh\u00b7ten", "Pfir\u00b7sich\u00b7B\u00b7l\u00fct'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.27": {"text": "R\u00f6ther noch an Lieb und Andacht, weisser noch an Un-", "tokens": ["R\u00f6\u00b7ther", "noch", "an", "Lieb", "und", "An\u00b7dacht", ",", "weis\u00b7ser", "noch", "an", "Un"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "NN", "KON", "NN", "$,", "PRELS", "ADV", "APPR", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.28": {"text": "schuld, f\u00fcr.", "tokens": ["schuld", ",", "f\u00fcr", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "$."], "meter": "-+", "measure": "iambic.single"}}}}}