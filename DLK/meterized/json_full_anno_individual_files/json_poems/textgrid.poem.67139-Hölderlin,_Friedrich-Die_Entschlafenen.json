{"textgrid.poem.67139": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Entschlafenen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen verg\u00e4nglichen Tag lebt ich und wuchs mit den Meinen,", "tokens": ["Ei\u00b7nen", "ver\u00b7g\u00e4ng\u00b7li\u00b7chen", "Tag", "lebt", "ich", "und", "wuchs", "mit", "den", "Mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Eins ums andere schon schl\u00e4ft mir und fliehet dahin.", "tokens": ["Eins", "ums", "an\u00b7de\u00b7re", "schon", "schl\u00e4ft", "mir", "und", "flie\u00b7het", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "PIS", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "PAV", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Doch ihr Schlafenden wacht am Herzen mir, in verwandter", "tokens": ["Doch", "ihr", "Schla\u00b7fen\u00b7den", "wacht", "am", "Her\u00b7zen", "mir", ",", "in", "ver\u00b7wand\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "PPER", "$,", "APPR", "ADJA"], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Seele ruhet von euch mir das entfliehende Bild.", "tokens": ["See\u00b7le", "ru\u00b7het", "von", "euch", "mir", "das", "ent\u00b7flie\u00b7hen\u00b7de", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER"