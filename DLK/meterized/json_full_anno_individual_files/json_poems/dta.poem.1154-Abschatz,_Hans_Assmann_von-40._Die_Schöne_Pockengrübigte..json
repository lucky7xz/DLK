{"dta.poem.1154": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "40. Die Sch\u00f6ne Pockengr\u00fcbigte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Sind Amors Auffenthalt zwey angenehme Gr\u00fcbgen/ ", "tokens": ["Sind", "A\u00b7mors", "Auf\u00b7fent\u00b7halt", "zwey", "an\u00b7ge\u00b7neh\u00b7me", "Gr\u00fcb\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "CARD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die in das sch\u00f6ne Wang\u2019 ein s\u00fcsses L\u00e4cheln dr\u00fcckt/", "tokens": ["Die", "in", "das", "sch\u00f6\u00b7ne", "Wang'", "ein", "s\u00fcs\u00b7ses", "L\u00e4\u00b7cheln", "dr\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So sag ich/ wenn man der bey dir so viel erblickt/", "tokens": ["So", "sag", "ich", "/", "wenn", "man", "der", "bey", "dir", "so", "viel", "er\u00b7blickt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PIS", "ART", "APPR", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Antlitz hegt und birgt wohl tausend Venus-B\u00fcbgen.", "tokens": ["Dein", "Ant\u00b7litz", "hegt", "und", "birgt", "wohl", "tau\u00b7send", "Ve\u00b7nus\u00b7B\u00fcb\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du prangst/ o Himmels-Kind/ mit diesen Stich und Hibgen/", "tokens": ["Du", "prangst", "/", "o", "Him\u00b7mels\u00b7Kind", "/", "mit", "die\u00b7sen", "Stich", "und", "Hib\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "FM", "NN", "$(", "APPR", "PDAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als wie der Himmel mit viel Sternen ausgeschm\u00fcckt/", "tokens": ["Als", "wie", "der", "Him\u00b7mel", "mit", "viel", "Ster\u00b7nen", "aus\u00b7ge\u00b7schm\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den Hertzen/ die dein Brand gep\u00fclvert und zerst\u00fcckt", "tokens": ["Den", "Hert\u00b7zen", "/", "die", "dein", "Brand", "ge\u00b7p\u00fcl\u00b7vert", "und", "zer\u00b7st\u00fcckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "PPOSAT", "NN", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durchbohrt Cupido hier ein Rosenblatt zum Siebgen.", "tokens": ["Durch\u00b7bohrt", "Cu\u00b7pi\u00b7do", "hier", "ein", "Ro\u00b7sen\u00b7blatt", "zum", "Sieb\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die L\u00f6cher gehn nicht durch/ ich trage keine Sorgen", "tokens": ["Die", "L\u00f6\u00b7cher", "gehn", "nicht", "durch", "/", "ich", "tra\u00b7ge", "kei\u00b7ne", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "$(", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was unter dieser Schrifft der Wangen liegt verborgen.", "tokens": ["Was", "un\u00b7ter", "die\u00b7ser", "Schrifft", "der", "Wan\u00b7gen", "liegt", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PDAT", "NN", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "H\u00e4lt nur das Hertz den Stich/ wer fraget nach der Haut.", "tokens": ["H\u00e4lt", "nur", "das", "Hertz", "den", "Stich", "/", "wer", "fra\u00b7get", "nach", "der", "Haut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "$(", "PWS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Durchf\u00e4hrt man nicht den Ros/ wenn man will Honig haben?", "tokens": ["Durch\u00b7f\u00e4hrt", "man", "nicht", "den", "Ros", "/", "wenn", "man", "will", "Ho\u00b7nig", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "ART", "NN", "$(", "KOUS", "PIS", "VMFIN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jemehr das Erdreich wird bepfl\u00fcgt und umgegraben/", "tokens": ["Je\u00b7mehr", "das", "Er\u00b7dreich", "wird", "be\u00b7pf\u00b7l\u00fcgt", "und", "um\u00b7ge\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVPP", "KON", "VVPP", "$("], "meter": "-+--++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Jemehr man Edelstein\u2019 und sch\u00f6ne Fr\u00fcchte schaut.", "tokens": ["Je\u00b7mehr", "man", "E\u00b7del\u00b7stein'", "und", "sch\u00f6\u00b7ne", "Fr\u00fcch\u00b7te", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}