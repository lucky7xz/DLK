{"textgrid.poem.33878": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Schmetterling", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein R\u00e4uplein sa\u00df auf kleinem Blatt,", "tokens": ["Ein", "R\u00e4up\u00b7lein", "sa\u00df", "auf", "klei\u00b7nem", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es sa\u00df nicht hoch, doch a\u00df es satt", "tokens": ["Es", "sa\u00df", "nicht", "hoch", ",", "doch", "a\u00df", "es", "satt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "$,", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und war auch wohl geborgen;", "tokens": ["Und", "war", "auch", "wohl", "ge\u00b7bor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da ward das kleine Raupending", "tokens": ["Da", "ward", "das", "klei\u00b7ne", "Rau\u00b7pen\u00b7ding"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Schmetterling,", "tokens": ["Zum", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "An einem sch\u00f6nen Morgen", "tokens": ["An", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+