ns": ["Die", "Fl\u00fc\u00b7gel", "al\u00b7le", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Wind rei\u00dft sie entzwei.", "tokens": ["Der", "Wind", "rei\u00dft", "sie", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er taumelt, ach! so matt, so matt,", "tokens": ["Er", "tau\u00b7melt", ",", "ach", "!", "so", "matt", ",", "so", "matt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ITJ", "$.", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur\u00fcck nun auf das kleine Blatt,", "tokens": ["Zu\u00b7r\u00fcck", "nun", "auf", "das", "klei\u00b7ne", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ihn ern\u00e4hrt als Raupe.", "tokens": ["Das", "ihn", "er\u00b7n\u00e4hrt", "als", "Rau\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "KOUS", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O weh', o weh', du armes Ding!", "tokens": ["O", "weh'", ",", "o", "weh'", ",", "du", "ar\u00b7mes", "Ding", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "FM", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Schmetterling,", "tokens": ["Ein", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der n\u00e4hrt sich nicht vom Laube \u2013", "tokens": ["Der", "n\u00e4hrt", "sich", "nicht", "vom", "Lau\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "PTKNEG", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du armer Schmetterling!", "tokens": ["Du", "ar\u00b7mer", "Schmet\u00b7ter\u00b7ling", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihm ist das Blatt jetzt eine Gruft,", "tokens": ["Ihm", "ist", "das", "Blatt", "jetzt", "ei\u00b7ne", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn letzt nur Blumensaft und Duft,", "tokens": ["Ihn", "letzt", "nur", "Blu\u00b7men\u00b7saft", "und", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die kann er nicht erlangen,", "tokens": ["Die", "kann", "er", "nicht", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und eh' noch kommt das Abendroth,", "tokens": ["Und", "eh'", "noch", "kommt", "das", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sieht man ihn todt", "tokens": ["Sieht", "m