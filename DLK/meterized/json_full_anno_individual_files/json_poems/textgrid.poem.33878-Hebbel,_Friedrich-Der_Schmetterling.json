xt": "O weh', o weh', du armes Ding!", "tokens": ["O", "weh'", ",", "o", "weh'", ",", "du", "ar\u00b7mes", "Ding", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "FM", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Schmetterling,", "tokens": ["Ein", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der n\u00e4hrt sich nicht vom Laube \u2013", "tokens": ["Der", "n\u00e4hrt", "sich", "nicht", "vom", "Lau\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "PTKNEG", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du armer Schmetterling!", "tokens": ["Du", "ar\u00b7mer", "Schmet\u00b7ter\u00b7ling", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ihm ist das Blatt jetzt eine Gruft,", "tokens": ["Ihm", "ist", "das", "Blatt", "jetzt", "ei\u00b7ne", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn letzt nur Blumensaft und Duft,", "tokens": ["Ihn", "letzt", "nur", "Blu\u00b7men\u00b7saft", "und", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die kann er nicht erlangen,", "tokens": ["Die", "kann", "er", "nicht", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und eh' noch kommt das Abendroth,", "tokens": ["Und", "eh'", "noch", "kommt", "das", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sieht man ihn todt", "tokens": ["Sieht", "man", "ihn", "todt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPER", "ADJD"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "An seinem Bl\u00e4ttlein hangen,", "tokens": ["An", "sei\u00b7nem", "Bl\u00e4tt\u00b7lein", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach kalt, erstarrt und todt!", "tokens": ["Ach", "kalt", ",", "er\u00b7starrt", "und", "todt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJD", "$,", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}