{"textgrid.poem.46048": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[in jenen \u00e4u\u00dfersten Stunden]", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In jenen \u00e4u\u00dfersten Stunden,", "tokens": ["In", "je\u00b7nen", "\u00e4u\u00b7\u00dfers\u00b7ten", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nachts, in des \u00d6lbergs Grunde", "tokens": ["Nachts", ",", "in", "des", "\u00d6l\u00b7bergs", "Grun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Schwitzt' ich, von \u00c4ngsten umwunden,", "tokens": ["Schwitzt'", "ich", ",", "von", "\u00c4ngs\u00b7ten", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Blutige Str\u00f6me f\u00fcr dich.", "tokens": ["Blu\u00b7ti\u00b7ge", "Str\u00f6\u00b7me", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Weh, und wer wei\u00df, ob wohl je", "tokens": ["Weh", ",", "und", "wer", "wei\u00df", ",", "ob", "wohl", "je"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "PWS", "VVFIN", "$,", "KOUS", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du auch nur denkest an mich!", "tokens": ["Du", "auch", "nur", "den\u00b7kest", "an", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie ich, von Gei\u00dfeln zerschlagen,", "tokens": ["Wie", "ich", ",", "von", "Gei\u00b7\u00dfeln", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wunde an Wunde ertragen,", "tokens": ["Wun\u00b7de", "an", "Wun\u00b7de", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "La\u00df von den Engeln dir klagen,", "tokens": ["La\u00df", "von", "den", "En\u00b7geln", "dir", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wie viele Wunden um dich!", "tokens": ["Wie", "vie\u00b7le", "Wun\u00b7den", "um", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Weh, und wer wei\u00df, ob wohl je", "tokens": ["Weh", ",", "und", "wer", "wei\u00df", ",", "ob", "wohl", "je"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "PWS", "VVFIN", "$,", "KOUS", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du auch nur denkest an mich.", "tokens": ["Du", "auch", "nur", "den\u00b7kest", "an", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Stach mich von Dornen die Krone,", "tokens": ["Stach", "mich", "von", "Dor\u00b7nen", "die", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Gab man mir Scherben zum Throne,", "tokens": ["Gab", "man", "mir", "Scher\u00b7ben", "zum", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Reicht man ein Rohr mir zum Hohne,", "tokens": ["Reicht", "man", "ein", "Rohr", "mir", "zum", "Hoh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Ach, da gedacht' ich an dich.", "tokens": ["Ach", ",", "da", "ge\u00b7dacht'", "ich", "an", "dich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Weh, und wer wei\u00df, ob wohl je", "tokens": ["Weh", ",", "und", "wer", "wei\u00df", ",", "ob", "wohl", "je"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "PWS", "VVFIN", "$,", "KOUS", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du auch nur denkest an mich!", "tokens": ["Du", "auch", "nur", "den\u00b7kest", "an", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, und zum Tode geschicket,", "tokens": ["Ach", ",", "und", "zum", "To\u00b7de", "ge\u00b7schi\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Peinlich vom Dornkranz umstricket,", "tokens": ["Pein\u00b7lich", "vom", "Dorn\u00b7kranz", "um\u00b7stri\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Unter der Kreuzlast geb\u00fccket", "tokens": ["Un\u00b7ter", "der", "Kreuz\u00b7last", "ge\u00b7b\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Schleppt' ich zum Berg mich f\u00fcr dich!", "tokens": ["Schleppt'", "ich", "zum", "Berg", "mich", "f\u00fcr", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PRF", "APPR", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Weh, und wer wei\u00df, ob wohl je", "tokens": ["Weh", ",", "und", "wer", "wei\u00df", ",", "ob", "wohl", "je"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "PWS", "VVFIN", "$,", "KOUS", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du auch nur denkest an mich!", "tokens": ["Du", "auch", "nur", "den\u00b7kest", "an", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sieh, an ein Holz fest geschlagen,", "tokens": ["Sieh", ",", "an", "ein", "Holz", "fest", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Eiserne N\u00e4gel mich tragen;", "tokens": ["Ei\u00b7ser\u00b7ne", "N\u00e4\u00b7gel", "mich", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "In einem Meere von Plagen", "tokens": ["In", "ei\u00b7nem", "Mee\u00b7re", "von", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wollte ich sterben f\u00fcr dich.", "tokens": ["Woll\u00b7te", "ich", "ster\u00b7ben", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Weh, und wer wei\u00df, ob wohl je", "tokens": ["Weh", ",", "und", "wer", "wei\u00df", ",", "ob", "wohl", "je"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "PWS", "VVFIN", "$,", "KOUS", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du auch nur denkest an mich!", "tokens": ["Du", "auch", "nur", "den\u00b7kest", "an", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00d6ffnet der Speer bis zum Grunde", "tokens": ["\u00d6ff\u00b7net", "der", "Speer", "bis", "zum", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "APPRART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Grausam ins Herz mir die Wunde,", "tokens": ["Grau\u00b7sam", "ins", "Herz", "mir", "die", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Quillt draus all Tag und all Stunde", "tokens": ["Quillt", "draus", "all", "Tag", "und", "all", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PAV", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wasser des Lebens f\u00fcr dich.", "tokens": ["Was\u00b7ser", "des", "Le\u00b7bens", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Weh, und wer wei\u00df, ob wohl je", "tokens": ["Weh", ",", "und", "wer", "wei\u00df", ",", "ob", "wohl", "je"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "PWS", "VVFIN", "$,", "KOUS", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du auch nur denkest an mich!", "tokens": ["Du", "auch", "nur", "den\u00b7kest", "an", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sieh alle Wunden erschlossen,", "tokens": ["Sieh", "al\u00b7le", "Wun\u00b7den", "er\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sieh all mein Blut hingeflossen:", "tokens": ["Sieh", "all", "mein", "Blut", "hin\u00b7ge\u00b7flos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Jegliches Tr\u00f6pflein vergossen", "tokens": ["Jeg\u00b7li\u00b7ches", "Tr\u00f6pf\u00b7lein", "ver\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Hab' ich aus Liebe f\u00fcr dich.", "tokens": ["Hab'", "ich", "aus", "Lie\u00b7be", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "APPR", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Weh, und wer wei\u00df, ob wohl je", "tokens": ["Weh", ",", "und", "wer", "wei\u00df", ",", "ob", "wohl", "je"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "PWS", "VVFIN", "$,", "KOUS", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du auch nur denkest an mich!", "tokens": ["Du", "auch", "nur", "den\u00b7kest", "an", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Betend zum Vater im Sterben", "tokens": ["Be\u00b7tend", "zum", "Va\u00b7ter", "im", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPRART", "NN", "APPRART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Fleht' ich, dir Heil zu erwerben,", "tokens": ["Fleht'", "ich", ",", "dir", "Heil", "zu", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Setzte dich, S\u00fcnder, zum Erben,", "tokens": ["Setz\u00b7te", "dich", ",", "S\u00fcn\u00b7der", ",", "zum", "Er\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Lie\u00df selbst die Mutter f\u00fcr dich.", "tokens": ["Lie\u00df", "selbst", "die", "Mut\u00b7ter", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Weh, und wer wei\u00df, ob wohl je", "tokens": ["Weh", ",", "und", "wer", "wei\u00df", ",", "ob", "wohl", "je"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "PWS", "VVFIN", "$,", "KOUS", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du auch nur denkest an mich!", "tokens": ["Du", "auch", "nur", "den\u00b7kest", "an", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Himmel und Erd' hat's durchdrungen,", "tokens": ["Him\u00b7mel", "und", "Erd'", "hat's", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Nacht hat die Sonne umschlungen,", "tokens": ["Nacht", "hat", "die", "Son\u00b7ne", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "wor