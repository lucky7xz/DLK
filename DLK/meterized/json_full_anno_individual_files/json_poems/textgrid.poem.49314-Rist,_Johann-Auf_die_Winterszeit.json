{"textgrid.poem.49314": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Winterszeit", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Winter hat sich angefangen,", "tokens": ["Der", "Win\u00b7ter", "hat", "sich", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schnee bedeckt das ganze Land,", "tokens": ["Der", "Schnee", "be\u00b7deckt", "das", "gan\u00b7ze", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sommer ist hinweggegangen,", "tokens": ["Der", "Som\u00b7mer", "ist", "hin\u00b7weg\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wald hat sich in Reif verwandt.", "tokens": ["Der", "Wald", "hat", "sich", "in", "Reif", "ver\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Wiesen sind von Frost versehret,", "tokens": ["Die", "Wie\u00b7sen", "sind", "von", "Frost", "ver\u00b7seh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Felder gl\u00e4nzen wie Metall;", "tokens": ["Die", "Fel\u00b7der", "gl\u00e4n\u00b7zen", "wie", "Me\u00b7tall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Blumen sind in Eis verkehret,", "tokens": ["Die", "Blu\u00b7men", "sind", "in", "Eis", "ver\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Fl\u00fcsse stehn wie harter Stahl.", "tokens": ["Die", "Fl\u00fcs\u00b7se", "stehn", "wie", "har\u00b7ter", "Stahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wolan, wir wollen von uns jagen", "tokens": ["Wo\u00b7lan", ",", "wir", "wol\u00b7len", "von", "uns", "ja\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VMFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Durchs Feur das kalte Winterkleid;", "tokens": ["Durchs", "Feur", "das", "kal\u00b7te", "Win\u00b7ter\u00b7kleid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Komt, la\u00dft uns Holz zum Herde tragen", "tokens": ["Komt", ",", "la\u00dft", "uns", "Holz", "zum", "Her\u00b7de", "tra\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVIMP", "PPER", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und Kohlen dran, jetzt ist es Zeit.", "tokens": ["Und", "Koh\u00b7len", "dran", ",", "jetzt", "ist", "es", "Zeit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "La\u00dft uns den F\u00fcrnewein hergeben", "tokens": ["La\u00dft", "uns", "den", "F\u00fcr\u00b7ne\u00b7wein", "her\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Dort unten aus dem gro\u00dfen Fa\u00df!", "tokens": ["Dort", "un\u00b7ten", "aus", "dem", "gro\u00b7\u00dfen", "Fa\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Das ist das rechte Winterleben:", "tokens": ["Das", "ist", "das", "rech\u00b7te", "Win\u00b7ter\u00b7le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ein' hei\u00dfe Stub' und k\u00fchles Glas.", "tokens": ["Ein'", "hei\u00b7\u00dfe", "Stub'", "und", "k\u00fch\u00b7les", "Glas", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wolan, wir wollen musicieren", "tokens": ["Wo\u00b7lan", ",", "wir", "wol\u00b7len", "mu\u00b7si\u00b7cie\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Bei warmer Luft und k\u00fchlen Wein;", "tokens": ["Bei", "war\u00b7mer", "Luft", "und", "k\u00fch\u00b7len", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ein ander mag sein' Klagen f\u00fchren,", "tokens": ["Ein", "an\u00b7der", "mag", "sein'", "Kla\u00b7gen", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Den Mammon nie l\u00e4\u00dft fr\u00f6lich sein.", "tokens": ["Den", "Mam\u00b7mon", "nie", "l\u00e4\u00dft", "fr\u00f6\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Wir wollen spielen, scherzen, essen,", "tokens": ["Wir", "wol\u00b7len", "spie\u00b7len", ",", "scher\u00b7zen", ",", "es\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Solang' uns noch kein Geld gebricht,", "tokens": ["So\u00b7lang'", "uns", "noch", "kein", "Geld", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Doch auch der Sch\u00f6nsten nicht vergessen,", "tokens": ["Doch", "auch", "der", "Sch\u00f6ns\u00b7ten", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Denn wer nicht liebt, der lebet nicht.", "tokens": ["Denn", "wer", "nicht", "liebt", ",", "der", "le\u00b7bet", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "VVFIN", "$,", "PRELS", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Wir haben dennoch gnug zu sorgen,", "tokens": ["Wir", "ha\u00b7ben", "den\u00b7noch", "gnug", "zu", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Wann nun das Alter komt heran;", "tokens": ["Wann", "nun", "das", "Al\u00b7ter", "komt", "he\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Es wei\u00df doch keiner, was ihm morgen", "tokens": ["Es", "wei\u00df", "doch", "kei\u00b7ner", ",", "was", "ihm", "mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$,", "PWS", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Noch vor ein Gl\u00fcck begegnen kan.", "tokens": ["Noch", "vor", "ein", "Gl\u00fcck", "be\u00b7geg\u00b7nen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Drum wil ich ohne Sorgen leben,", "tokens": ["Drum", "wil", "ich", "oh\u00b7ne", "Sor\u00b7gen", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Mit meinen Br\u00fcdern fr\u00f6lich sein.", "tokens": ["Mit", "mei\u00b7nen", "Br\u00fc\u00b7dern", "fr\u00f6\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Nach Ehr' und Tugend thu' ich streben,", "tokens": ["Nach", "Ehr'", "und", "Tu\u00b7gend", "thu'", "ich", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Den Rest befehl' ich Gott allein.", "tokens": ["Den", "Rest", "be\u00b7fehl'", "ich", "Gott", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Winter hat sich angefangen,", "tokens": ["Der", "Win\u00b7ter", "hat", "sich", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schnee bedeckt das ganze Land,", "tokens": ["Der", "Schnee", "be\u00b7deckt", "das", "gan\u00b7ze", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sommer ist hinweggegangen,", "tokens": ["Der", "Som\u00b7mer", "ist", "hin\u00b7weg\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wald hat sich in Reif verwandt.", "tokens": ["Der", "Wald", "hat", "sich", "in", "Reif", "ver\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART",