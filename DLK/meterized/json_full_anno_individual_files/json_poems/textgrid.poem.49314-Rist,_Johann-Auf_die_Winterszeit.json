+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Doch auch der Sch\u00f6nsten nicht vergessen,", "tokens": ["Doch", "auch", "der", "Sch\u00f6ns\u00b7ten", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Denn wer nicht liebt, der lebet nicht.", "tokens": ["Denn", "wer", "nicht", "liebt", ",", "der", "le\u00b7bet", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "VVFIN", "$,", "PRELS", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Wir haben dennoch gnug zu sorgen,", "tokens": ["Wir", "ha\u00b7ben", "den\u00b7noch", "gnug", "zu", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Wann nun das Alter komt heran;", "tokens": ["Wann", "nun", "das", "Al\u00b7ter", "komt", "he\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Es wei\u00df doch keiner, was ihm morgen", "tokens": ["Es", "wei\u00df", "doch", "kei\u00b7ner", ",", "was", "ihm", "mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$,", "PWS", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Noch vor ein Gl\u00fcck begegnen kan.", "tokens": ["Noch", "vor", "ein", "Gl\u00fcck", "be\u00b7geg\u00b7nen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Drum wil ich ohne Sorgen leben,", "tokens": ["Drum", "wil", "ich", "oh\u00b7ne", "Sor\u00b7gen", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Mit meinen Br\u00fcdern fr\u00f6lich sein.", "tokens": ["Mit", "mei\u00b7nen", "Br\u00fc\u00b7dern", "fr\u00f6\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Nach Ehr' und Tugend thu' ich streben,", "tokens": ["Nach", "Ehr'", "und", "Tu\u00b7gend", "thu'", "ich", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Den Rest befehl' ich Gott allein.", "tokens": ["Den", "Rest", "be\u00b7fehl'", "ich", "Gott", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}