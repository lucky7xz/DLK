{"dta.poem.4254": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Sch\u00f6nheit der Welt zur  \n Abend-Zeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sehe, mit erstaunten Blicken,", "tokens": ["Ich", "se\u00b7he", ",", "mit", "er\u00b7staun\u00b7ten", "Bli\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die wunderreiche Herrlichkeit,", "tokens": ["Die", "wun\u00b7der\u00b7rei\u00b7che", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Womit, zu dieser Abend-Zeit,", "tokens": ["Wo\u00b7mit", ",", "zu", "die\u00b7ser", "A\u00b7ben\u00b7dZeit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich, durch das Licht, die C\u00f6rper schm\u00fccken.", "tokens": ["Sich", ",", "durch", "das", "Licht", ",", "die", "C\u00f6r\u00b7per", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "APPR", "ART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es heben noch die langen Schatten,", "tokens": ["Es", "he\u00b7ben", "noch", "die", "lan\u00b7gen", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da sie sich mit dem Lichte gatten,", "tokens": ["Da", "sie", "sich", "mit", "dem", "Lich\u00b7te", "gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das untergeht, den Glanz noch mehr:", "tokens": ["Das", "un\u00b7ter\u00b7geht", ",", "den", "Glanz", "noch", "mehr", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So da\u00df, worauf die Sonn\u2019 itzt strahlet,", "tokens": ["So", "da\u00df", ",", "wo\u00b7rauf", "die", "Sonn'", "itzt", "strah\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "$,", "PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sich recht, als wie mit Golde, mahlet.", "tokens": ["Sich", "recht", ",", "als", "wie", "mit", "Gol\u00b7de", ",", "mah\u00b7let", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ADJD", "$,", "KOUS", "KOKOM", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein GOtt! wie flammt, zu Deiner Ehr\u2019,", "tokens": ["Mein", "Gott", "!", "wie", "flammt", ",", "zu", "Dei\u00b7ner", "Ehr'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die\u00df g\u00fcldne Licht auf dunklem Gr\u00fcnen!", "tokens": ["Die\u00df", "g\u00fcld\u00b7ne", "Licht", "auf", "dunk\u00b7lem", "Gr\u00fc\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Stellen, die dadurch beschienen,", "tokens": ["Die", "Stel\u00b7len", ",", "die", "da\u00b7durch", "be\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die scheinen itzt im ird\u2019schen nicht,", "tokens": ["Die", "schei\u00b7nen", "itzt", "im", "ird'\u00b7schen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "ADJA", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In einem wahren Himmels-Licht,", "tokens": ["In", "ei\u00b7nem", "wah\u00b7ren", "Him\u00b7mels\u00b7Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durchs Abend-Roht noch einst so sch\u00f6n,", "tokens": ["Durchs", "A\u00b7ben\u00b7dRoht", "noch", "einst", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als an dem sch\u00f6nsten Tag, zu steh\u2019n.", "tokens": ["Als", "an", "dem", "sch\u00f6ns\u00b7ten", "Tag", ",", "zu", "steh'", "n."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Erde scheint aufs neu belebt,", "tokens": ["Die", "Er\u00b7de", "scheint", "aufs", "neu", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Unglaublich ist, wie, bey dem Dunkeln", "tokens": ["Un\u00b7glaub\u00b7lich", "ist", ",", "wie", ",", "bey", "dem", "Dun\u00b7keln"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "$,", "PWAV", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der nahen Schatten, sich das Funkeln", "tokens": ["Der", "na\u00b7hen", "Schat\u00b7ten", ",", "sich", "das", "Fun\u00b7keln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der rohten Strahlen mehrt und hebt.", "tokens": ["Der", "roh\u00b7ten", "Strah\u00b7len", "mehrt", "und", "hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die niedern Felder, Thal und Matten", "tokens": ["Die", "nie\u00b7dern", "Fel\u00b7der", ",", "Thal", "und", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Bedeckt bereits ein gr\u00fcner Schatten,", "tokens": ["Be\u00b7deckt", "be\u00b7reits", "ein", "gr\u00fc\u00b7ner", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wie auch den untern Theil der H\u00f6h\u2019n,", "tokens": ["Wie", "auch", "den", "un\u00b7tern", "Theil", "der", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Inzwischen, da\u00df der B\u00e4ume Wipfel,", "tokens": ["I\u00b7nzwi\u00b7schen", ",", "da\u00df", "der", "B\u00e4u\u00b7me", "Wip\u00b7fel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und der erhabnen Berge Gipfel,", "tokens": ["Und", "der", "er\u00b7hab\u00b7nen", "Ber\u00b7ge", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "In Rosen- farbnem Golde steh\u2019n.", "tokens": ["In", "Ro\u00b7sen", "farb\u00b7nem", "Gol\u00b7de", "steh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "TRUNC", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Schatten dunkle Nachbarschaft", "tokens": ["Der", "Schat\u00b7ten", "dunk\u00b7le", "Nach\u00b7bar\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vermehrt des g\u00fcldnen Lichtes Kraft.", "tokens": ["Ver\u00b7mehrt", "des", "g\u00fcld\u00b7nen", "Lich\u00b7tes", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "In der sonst rein- und heitern Luft", "tokens": ["In", "der", "sonst", "rein", "und", "hei\u00b7tern", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwebt hie und da ein zarter Duft,", "tokens": ["Schwebt", "hie", "und", "da", "ein", "zar\u00b7ter", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An welchem, wenn er sich vereint,", "tokens": ["An", "wel\u00b7chem", ",", "wenn", "er", "sich", "ver\u00b7eint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hin und wieder Wolken bildet,", "tokens": ["Und", "hin", "und", "wie\u00b7der", "Wol\u00b7ken", "bil\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "KON", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sonne so vortreflich scheint,", "tokens": ["Die", "Son\u00b7ne", "so", "vor\u00b7tre\u00b7flich", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df sie, recht in der That verg\u00fcldet,", "tokens": ["Da\u00df", "sie", ",", "recht", "in", "der", "That", "ver\u00b7g\u00fcl\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ein wirklich Meer vom Golde zeigen,", "tokens": ["Ein", "wirk\u00b7lich", "Meer", "vom", "Gol\u00b7de", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Aus welchem Strahlen abwerts steigen.", "tokens": ["Aus", "wel\u00b7chem", "Strah\u00b7len", "ab\u00b7werts", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So hell, so feurig ist ihr Schein,", "tokens": ["So", "hell", ",", "so", "feu\u00b7rig", "ist", "ihr", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df, s\u00e4he man die Sonne nicht,", "tokens": ["Da\u00df", ",", "s\u00e4\u00b7he", "man", "die", "Son\u00b7ne", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "VVFIN", "PIS", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Jhr Schimmer selbst ein Sonnen-Licht,", "tokens": ["Ihr", "Schim\u00b7mer", "selbst", "ein", "Son\u00b7nen\u00b7Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und eine Licht-Quell k\u00f6nnte seyn.", "tokens": ["Und", "ei\u00b7ne", "Licht\u00b7Quell", "k\u00f6nn\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Es scheint auch, da\u00df auf unsrer Erde,", "tokens": ["Es", "scheint", "auch", ",", "da\u00df", "auf", "uns\u00b7rer", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.14": {"text": "Vom Strahl, der sich an ihnen bricht,", "tokens": ["Vom", "Strahl", ",", "der", "sich", "an", "ih\u00b7nen", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Das sonst schon sch\u00f6ne Sonnen-Licht,", "tokens": ["Das", "sonst", "schon", "sch\u00f6\u00b7ne", "Son\u00b7nen\u00b7Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der rohte Glanz, vermehret werde,", "tokens": ["Der", "roh\u00b7te", "Glanz", ",", "ver\u00b7meh\u00b7ret", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und da\u00df sichs noch zu andern f\u00fcge.", "tokens": ["Und", "da\u00df", "sichs", "noch", "zu", "an\u00b7dern", "f\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Man bildet sich von diesem Schein", "tokens": ["Man", "bil\u00b7det", "sich", "von", "die\u00b7sem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Unm\u00f6glich fast was anders ein,", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "fast", "was", "an\u00b7ders", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PWS", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Als da\u00df vom Himmels-Abend-Roht", "tokens": ["Als", "da\u00df", "vom", "Him\u00b7mels\u00b7A\u00b7ben\u00b7dRoht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPRART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Auch etwas auf der Erden liege.", "tokens": ["Auch", "et\u00b7was", "auf", "der", "Er\u00b7den", "lie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Die Sch\u00f6nheit r\u00fchrte meine Brust", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "r\u00fchr\u00b7te", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Mit einer ganz besondern Lust,", "tokens": ["Mit", "ei\u00b7ner", "ganz", "be\u00b7son\u00b7dern", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Es ward durch die so sch\u00f6ne Pracht", "tokens": ["Es", "ward", "durch", "die", "so", "sch\u00f6\u00b7ne", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Ein Andacht-Feuer angefacht,", "tokens": ["Ein", "An\u00b7dacht\u00b7Feu\u00b7er", "an\u00b7ge\u00b7facht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Es fing so kr\u00e4ftig 