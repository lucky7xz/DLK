{"textgrid.poem.63031": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vor des Chroniden Thron erschienen", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor des Chroniden Thron erschienen", "tokens": ["Vor", "des", "Chro\u00b7ni\u00b7den", "Thron", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fuchs, der Sp\u00fcrhund und der Luchs.", "tokens": ["Der", "Fuchs", ",", "der", "Sp\u00fcr\u00b7hund", "und", "der", "Luchs", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ART", "NN", "KON", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie baten ihn mit demuthsvollen Mienen", "tokens": ["Sie", "ba\u00b7ten", "ihn", "mit", "de\u00b7muths\u00b7vol\u00b7len", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Um ein Geh\u00f6r. Der Redner war der Fuchs:", "tokens": ["Um", "ein", "Ge\u00b7h\u00f6r", ".", "Der", "Red\u00b7ner", "war", "der", "Fuchs", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$.", "ART", "NN", "VAFIN", "ART", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir kennen, Herr, den Werth der hohen Gaben,", "tokens": ["Wir", "ken\u00b7nen", ",", "Herr", ",", "den", "Werth", "der", "ho\u00b7hen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die wir von deiner Huld empfangen haben;", "tokens": ["Die", "wir", "von", "dei\u00b7ner", "Huld", "emp\u00b7fan\u00b7gen", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Kein Adler hat den Blick, den sie dem Luchs verlieh;", "tokens": ["Kein", "Ad\u00b7ler", "hat", "den", "Blick", ",", "den", "sie", "dem", "Luchs", "ver\u00b7lieh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Sp\u00fcrhund riecht das Wild auf viele hundert Schritte,", "tokens": ["Der", "Sp\u00fcr\u00b7hund", "riecht", "das", "Wild", "auf", "vie\u00b7le", "hun\u00b7dert", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "PIAT", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und mich erhobst du zum Genie:", "tokens": ["Und", "mich", "er\u00b7hobst", "du", "zum", "Ge\u00b7nie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Indessen w\u00fcrden wir, und die\u00df ist unsre Bitte,", "tokens": ["In\u00b7des\u00b7sen", "w\u00fcr\u00b7den", "wir", ",", "und", "die\u00df", "ist", "uns\u00b7re", "Bit\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KON", "PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch alle drey noch weit vollkommner seyn,", "tokens": ["Doch", "al\u00b7le", "drey", "noch", "weit", "voll\u00b7komm\u00b7ner", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "CARD", "ADV", "ADJD", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wenn jeden unter uns auch die Talente zierten,", "tokens": ["Wenn", "je\u00b7den", "un\u00b7ter", "uns", "auch", "die", "Ta\u00b7len\u00b7te", "zier\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-++----+-", "measure": "unknown.measure.penta"}, "line.13": {"text": "Die du den andern gabst. Den Vorschlag geh ich ein,", "tokens": ["Die", "du", "den", "an\u00b7dern", "gabst", ".", "Den", "Vor\u00b7schlag", "geh", "ich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "VVFIN", "$.", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Erwiederte Herr Zevs den Alliirten;", "tokens": ["Er\u00b7wie\u00b7der\u00b7te", "Herr", "Zevs", "den", "Al\u00b7li\u00b7ir\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Doch will des Schicksals ernster Schlu\u00df,", "tokens": ["Doch", "will", "des", "Schick\u00b7sals", "erns\u00b7ter", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Da\u00df jeder seinem Freund von seinem eignen Pfunde", "tokens": ["Da\u00df", "je\u00b7der", "sei\u00b7nem", "Freund", "von", "sei\u00b7nem", "eig\u00b7nen", "Pfun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein gleiches Maas ersetzen mu\u00df,", "tokens": ["Ein", "glei\u00b7ches", "Maas", "er\u00b7set\u00b7zen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Als er von ihm erh\u00e4lt. Mit frohem Munde", "tokens": ["Als", "er", "von", "ihm", "er\u00b7h\u00e4lt", ".", "Mit", "fro\u00b7hem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und einem tiefen Knicks nahm das Triumvirat", "tokens": ["Und", "ei\u00b7nem", "tie\u00b7fen", "Knicks", "nahm", "das", "Tri\u00b7um\u00b7vi\u00b7rat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Klausel an; und Zevs mit Sch\u00f6pfersblicke", "tokens": ["Die", "Klau\u00b7sel", "an", ";", "und", "Zevs", "mit", "Sch\u00f6p\u00b7fers\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "KON", "NE", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Best\u00e4tigte den Tauschtraktat:", "tokens": ["Be\u00b7st\u00e4\u00b7tig\u00b7te", "den", "Tauschtrak\u00b7tat", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Nun, sprach er, kehrt zur Br\u00fcderschaar zur\u00fccke,", "tokens": ["Nun", ",", "sprach", "er", ",", "kehrt", "zur", "Br\u00fc\u00b7der\u00b7schaar", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Und sagt ihr, was der Vater der Geschicke", "tokens": ["Und", "sagt", "ihr", ",", "was", "der", "Va\u00b7ter", "der", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "F\u00fcr euern k\u00fchnen Ehrgeitz that.", "tokens": ["F\u00fcr", "eu\u00b7ern", "k\u00fch\u00b7nen", "Ehr\u00b7geitz", "that", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Die Bande k\u00fc\u00dft entz\u00fcckt dem Gotte die Sandale,", "tokens": ["Die", "Ban\u00b7de", "k\u00fc\u00dft", "ent\u00b7z\u00fcckt", "dem", "Got\u00b7te", "die", "San\u00b7da\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und wie ein junger Arzt, der sich zum erstenmale", "tokens": ["Und", "wie", "ein", "jun\u00b7ger", "Arzt", ",", "der", "sich", "zum", "ers\u00b7ten\u00b7ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Dem Volk als Doktor zeigt, so steif, so naseweis", "tokens": ["Dem", "Volk", "als", "Dok\u00b7tor", "zeigt", ",", "so", "steif", ",", "so", "na\u00b7se\u00b7weis"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "KOUS", "NN", "VVFIN", "$,", "ADV", "ADJD", "$,", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dr\u00e4ngt jeder sich in seiner Br\u00fcder Kreis", "tokens": ["Dr\u00e4ngt", "je\u00b7der", "sich", "in", "sei\u00b7ner", "Br\u00fc\u00b7der", "Kreis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PRF", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Und predigt seine mystische Geschichte.", "tokens": ["Und", "pre\u00b7digt", "sei\u00b7ne", "mys\u00b7ti\u00b7sche", "Ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Erstaunt vernahmen sie die prahlenden Berichte.", "tokens": ["Er\u00b7staunt", "ver\u00b7nah\u00b7men", "sie", "die", "prah\u00b7len\u00b7den", "Be\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Doch ehe noch ein Tag verschlichen war,", "tokens": ["Doch", "e\u00b7he", "noch", "ein", "Tag", "ver\u00b7schli\u00b7chen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Hie\u00df es, der Fuchs ist vor den Kopf geschlagen,", "tokens": ["Hie\u00df", "es", ",", "der", "Fuchs", "ist", "vor", "den", "Kopf", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NE", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Der Sp\u00fcrhund taugt nicht mehr zum Jagen,", "tokens": ["Der", "Sp\u00fcr\u00b7hund", "taugt", "nicht", "mehr", "zum", "Ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Und Argus Luchs bek\u00f6mmt den Staar.", "tokens": ["Und", "Ar\u00b7gus", "Luchs", "be\u00b7k\u00f6mmt", "den", "Staar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Geliebte, die Ihr theils mit fr\u00f6hlichem Get\u00fcmmel", "tokens": ["Ge\u00b7lieb\u00b7te", ",", "die", "Ihr", "theils", "mit", "fr\u00f6h\u00b7li\u00b7chem", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie holde Scherze mich umschwebt,", "tokens": ["Wie", "hol\u00b7de", "Scher\u00b7ze", "mich", "um\u00b7schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Theils weit von mir zerstreut, auch unter fremdem Himmel", "tokens": ["Theils", "weit", "von", "mir", "zer\u00b7streut", ",", "auch", "un\u00b7ter", "frem\u00b7dem", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "PPER", "VVPP", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Noch stets in meinem Herzen lebt!", "tokens": ["Noch", "stets", "in", "mei\u00b7nem", "Her\u00b7zen", "lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Glaubt Eurem besten Freund auf Erden,", "tokens": ["Glaubt", "Eu\u00b7rem", "bes\u00b7ten", "Freund", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wer alles werden will, wird nie was Rechtes werden.", "tokens": ["Wer", "al\u00b7les", "wer\u00b7den", "will", ",", "wird", "nie", "was", "Rech\u00b7tes", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VAINF", "VMFIN", "$,", "VAFIN", "ADV", "PWS", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vor des Chroniden Thron erschienen", "tokens": ["Vor", "des", "Chro\u00b7ni\u00b7den", "Thron", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fuchs, der Sp\u00fcrhund und der Luchs.", "tokens