{"textgrid.poem.55643": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "[die Jahre nahmen dir, du sagst, so vieles]", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdie Jahre nahmen dir, du sagst, so vieles:", "tokens": ["\u00bb", "die", "Jah\u00b7re", "nah\u00b7men", "dir", ",", "du", "sagst", ",", "so", "vie\u00b7les", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "$,", "ADV", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die eigentliche Lust des Sinnespieles,", "tokens": ["Die", "ei\u00b7gent\u00b7li\u00b7che", "Lust", "des", "Sin\u00b7ne\u00b7spie\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Erinnerung des allerliebsten Tandes", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", "des", "al\u00b7ler\u00b7liebs\u00b7ten", "Tan\u00b7des"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von gestern, weit und breiten Landes", "tokens": ["Von", "ge\u00b7stern", ",", "weit", "und", "brei\u00b7ten", "Lan\u00b7des"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "$,", "ADJD", "KON", "ADJA", "NN"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Durchschweifen frommt nicht mehr; selbst nicht von oben", "tokens": ["Durch\u00b7schwei\u00b7fen", "frommt", "nicht", "mehr", ";", "selbst", "nicht", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKNEG", "ADV", "$.", "ADV", "PTKNEG", "APPR", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Ehren anerkannte Zier, das Loben,", "tokens": ["Der", "Eh\u00b7ren", "an\u00b7er\u00b7kann\u00b7te", "Zier", ",", "das", "Lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Erfreulich sonst. Aus eignem Tun Behagen", "tokens": ["Er\u00b7freu\u00b7lich", "sonst", ".", "Aus", "eig\u00b7nem", "Tun", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "$.", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Quillt nicht mehr auf, dir fehlt ein dreistes Wagen!", "tokens": ["Quillt", "nicht", "mehr", "auf", ",", "dir", "fehlt", "ein", "dreis\u00b7tes", "Wa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nun w\u00fc\u00dft ich nicht, was dir Besondres bliebe?\u00ab", "tokens": ["Nun", "w\u00fc\u00dft", "ich", "nicht", ",", "was", "dir", "Be\u00b7sond\u00b7res", "blie\u00b7be", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "PWS", "PPER", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mir bleibt genug! Es bleibt Idee und Liebe!", "tokens": ["Mir", "bleibt", "ge\u00b7nug", "!", "Es", "bleibt", "I\u00b7dee", "und", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word",