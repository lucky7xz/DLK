{"textgrid.poem.47580": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nimm dir ein Exempel an den Rosen!]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm dir ein Exempel an den Rosen!", "tokens": ["Nimm", "dir", "ein", "Ex\u00b7em\u00b7pel", "an", "den", "Ro\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Auf der Sonne klares Angesicht,", "tokens": ["Auf", "der", "Son\u00b7ne", "kla\u00b7res", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Morgenthau und s\u00fc\u00dfer Oste Kosen", "tokens": ["Mor\u00b7gent\u00b7hau", "und", "s\u00fc\u00b7\u00dfer", "Os\u00b7te", "Ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Thun sie nun und nimmermehr Verzicht.", "tokens": ["Thun", "sie", "nun", "und", "nim\u00b7mer\u00b7mehr", "Ver\u00b7zicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "ADV", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Siehe, wie sie lachen, diese losen!", "tokens": ["Sie\u00b7he", ",", "wie", "sie", "la\u00b7chen", ",", "die\u00b7se", "lo\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "PPER", "VVINF", "$,", "PDS", "VVINF", "$."], "meter": "+-+-+-+-+-", "meas