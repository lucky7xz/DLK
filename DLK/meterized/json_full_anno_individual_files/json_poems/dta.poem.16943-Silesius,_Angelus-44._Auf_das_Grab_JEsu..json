{"dta.poem.16943": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "44.  Auf das Grab JEsu.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Hier ", "tokens": ["Hier"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Ein Held/ der seinen Feind mit Leyden kan ermorden.", "tokens": ["Ein", "Held", "/", "der", "sei\u00b7nen", "Feind", "mit", "Ley\u00b7den", "kan", "er\u00b7mor\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PPOSAT", "NN", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wiltu ihm werden gleich/ und Uberwinder seyn/", "tokens": ["Wil\u00b7tu", "ihm", "wer\u00b7den", "gleich", "/", "und", "U\u00b7ber\u00b7win\u00b7der", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VAFIN", "ADV", "$(", "KON", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So leyd/ meid/ fleuch und stirb/ inn Wolust und in Pein.", "tokens": ["So", "leyd", "/", "meid", "/", "fleuch", "und", "stirb", "/", "inn", "Wo\u00b7lust", "und", "in", "Pein", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "VVFIN", "$(", "VVIMP", "KON", "VVFIN", "$(", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weistu nicht wer Er ist? so merke diese Drey/", "tokens": ["Weis\u00b7tu", "nicht", "wer", "Er", "ist", "?", "so", "mer\u00b7ke", "die\u00b7se", "Drey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "PWS", "PPER", "VAFIN", "$.", "ADV", "VVFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df er ein Mensch und GOtt/ und dein Erl\u00f6ser sey.", "tokens": ["Da\u00df", "er", "ein", "Mensch", "und", "Gott", "/", "und", "dein", "Er\u00b7l\u00f6\u00b7ser", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "NN", "$(", "KON", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}