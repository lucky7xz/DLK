{"dta.poem.4125": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Prager Musikant.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit dem Kappel in der Hand,", "tokens": ["Mit", "dem", "Kap\u00b7pel", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziehn wir Prager Musikanten", "tokens": ["Ziehn", "wir", "Pra\u00b7ger", "Mu\u00b7si\u00b7kan\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch das weite Christenland.", "tokens": ["Durch", "das", "wei\u00b7te", "Chris\u00b7ten\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unser Schutzpatron im Himmel", "tokens": ["Un\u00b7ser", "Schutz\u00b7pat\u00b7ron", "im", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00dft der heil'ge Nepomuk,", "tokens": ["Hei\u00dft", "der", "heil'\u00b7ge", "Ne\u00b7po\u00b7muk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steht mit seinem Sternenkr\u00e4nzel", "tokens": ["Steht", "mit", "sei\u00b7nem", "Ster\u00b7nen\u00b7kr\u00e4n\u00b7zel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mitten auf der Prager Bruck.", "tokens": ["Mit\u00b7ten", "auf", "der", "Pra\u00b7ger", "Bruck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Als ich da hinausgewandert,", "tokens": ["Als", "ich", "da", "hin\u00b7aus\u00b7ge\u00b7wan\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' ich Reverenz gemacht,", "tokens": ["Hab'", "ich", "Re\u00b7ve\u00b7renz", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein Gebet ihm aus dem Kopfe", "tokens": ["Ein", "Ge\u00b7bet", "ihm", "aus", "dem", "Kop\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Recht bed\u00e4chtig hergesagt.", "tokens": ["Recht", "be\u00b7d\u00e4ch\u00b7tig", "her\u00b7ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Steht also in keinem B\u00fcchel,", "tokens": ["Steht", "al\u00b7so", "in", "kei\u00b7nem", "B\u00fc\u00b7chel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie man's auf dem Herzen hat:", "tokens": ["Wie", "man's", "auf", "dem", "Her\u00b7zen", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wanderschaft mit leerem Beutel,", "tokens": ["Wan\u00b7der\u00b7schaft", "mit", "lee\u00b7rem", "Beu\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein Sch\u00e4tzel in der Stadt.", "tokens": ["Und", "ein", "Sch\u00e4t\u00b7zel", "in", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn das M\u00e4del singen k\u00f6nnte,", "tokens": ["Wenn", "das", "M\u00e4\u00b7del", "sin\u00b7gen", "k\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4r's gezogen mit hinaus,", "tokens": ["W\u00e4r's", "ge\u00b7zo\u00b7gen", "mit", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "APZR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch nun hat's 'ne heis're Kehle,", "tokens": ["Doch", "nun", "hat's", "'ne", "heis'\u00b7re", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Mu\u00dft' es lassen drum zu Haus.", "tokens": ["Mu\u00dft'", "es", "las\u00b7sen", "drum", "zu", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PAV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ei, da gab es nasse Augen,", "tokens": ["Ei", ",", "da", "gab", "es", "nas\u00b7se", "Au\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "'s war mir selbst nicht einerlei:", "tokens": ["'s", "war", "mir", "selbst", "nicht", "ei\u00b7ner\u00b7lei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sprach itzt: 'S ist ja nicht f\u00fcr ewig,", "tokens": ["Sprach", "itzt", ":", "'s", "ist", "ja", "nicht", "f\u00fcr", "e\u00b7wig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "NE", "VAFIN", "ADV", "PTKNEG", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sch\u00f6nstes Nannerl, la\u00df mich frei!", "tokens": ["Sch\u00f6ns\u00b7tes", "Nan\u00b7nerl", ",", "la\u00df", "mich", "frei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und ich schl\u00fcpft' aus ihren Armen,", "tokens": ["Und", "ich", "schl\u00fcpft'", "aus", "ih\u00b7ren", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Pforte, aus dem Haus,", "tokens": ["Aus", "der", "Pfor\u00b7te", ",", "aus", "dem", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Konnt' nicht wieder r\u00fcckw\u00e4rts schauen,", "tokens": ["Konnt'", "nicht", "wie\u00b7der", "r\u00fcck\u00b7w\u00e4rts", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis ich war zur Stadt hinaus.", "tokens": ["Bis", "ich", "war", "zur", "Stadt", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Da hab' ich dies Lied gesungen,", "tokens": ["Da", "hab'", "ich", "dies", "Lied", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hab' die Fiedel zu gespielt,", "tokens": ["Hab'", "die", "Fie\u00b7del", "zu", "ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKZU", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis ich in den Morgenl\u00fcften", "tokens": ["Bis", "ich", "in", "den", "Mor\u00b7gen\u00b7l\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf' der Brust mich leicht gef\u00fchlt.", "tokens": ["Auf'", "der", "Brust", "mich", "leicht", "ge\u00b7f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Manches V\u00f6glein hat's vernommen:", "tokens": ["Man\u00b7ches", "V\u00f6\u00b7glein", "hat's", "ver\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fl\u00f6g' nur eins an Liebchens Ohr,", "tokens": ["Fl\u00f6g'", "nur", "eins", "an", "Lieb\u00b7chens", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIS", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00e4ng' ihr, wenn sie weinen wollte,", "tokens": ["S\u00e4ng'", "ihr", ",", "wenn", "sie", "wei\u00b7nen", "woll\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieses frische Liedel vor!", "tokens": ["Die\u00b7ses", "fri\u00b7sche", "Lie\u00b7del", "vor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wenn ich aus der Fremde komme,", "tokens": ["Wenn", "ich", "aus", "der", "Frem\u00b7de", "kom\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spiel' ich auf aus anderm Ton,", "tokens": ["Spiel'", "ich", "auf", "aus", "an\u00b7derm", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Abends unter ihrem Fenster:", "tokens": ["A\u00b7bends", "un\u00b7ter", "ih\u00b7rem", "Fens\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00e4tzel, Sch\u00e4tzel, schl\u00e4fst du schon?", "tokens": ["Sch\u00e4t\u00b7zel", ",", "Sch\u00e4t\u00b7zel", ",", "schl\u00e4fst", "du", "schon", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Hoch geschwenkt den vollen Beutel,", "tokens": ["Hoch", "ge\u00b7schwenkt", "den", "vol\u00b7len", "Beu\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das giebt eine Musika!", "tokens": ["Das", "giebt", "ei\u00b7ne", "Mu\u00b7si\u00b7ka", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "'s Fenster klirrt, es rauscht der Laden,", "tokens": ["'s", "Fens\u00b7ter", "klirrt", ",", "es", "rauscht", "der", "La\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heilige C\u00e4cilia!", "tokens": ["Hei\u00b7li\u00b7ge", "C\u00e4\u00b7ci\u00b7lia", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NE", "$."], "meter": "+-+---", "measure": "unknown.measure.di"}}, "stanza.12": {"line.1": {"text": "All' ihr Prager Musikanten,", "tokens": ["All'", "ihr", "Pra\u00b7ger", "Mu\u00b7si\u00b7kan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf, heraus mit Horn und Ba\u00df,", "tokens": ["Auf", ",", "he\u00b7raus", "mit", "Horn", "und", "Ba\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "ADV", "APPR", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spielt den sch\u00f6nsten Hochzeitreigen!", "tokens": ["Spielt", "den", "sch\u00f6ns\u00b7ten", "Hoch\u00b7zei\u00b7trei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgen leeren wir ein Fa\u00df.", "tokens": ["Mor\u00b7gen", "lee\u00b7ren", "wir", "ein", "Fa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}