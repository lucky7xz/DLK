{"dta.poem.7156": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach GOTT! so viel Secunden und Minuten,", "tokens": ["Ach", "GoTT", "!", "so", "viel", "Se\u00b7cun\u00b7den", "und", "Mi\u00b7nu\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als im verwichnen Jahr gewesen und verflossen,", "tokens": ["Als", "im", "ver\u00b7wich\u00b7nen", "Jahr", "ge\u00b7we\u00b7sen", "und", "ver\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "VAPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So viele Quellen auch von tausendfachem Guten,", "tokens": ["So", "vie\u00b7le", "Quel\u00b7len", "auch", "von", "tau\u00b7send\u00b7fa\u00b7chem", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So, nebst den Meinigen, auch ich genossen,", "tokens": ["So", ",", "nebst", "den", "Mei\u00b7ni\u00b7gen", ",", "auch", "ich", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "PPOSS", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So Du, o HERR! auf uns gelencket,", "tokens": ["So", "Du", ",", "o", "HeRR", "!", "auf", "uns", "ge\u00b7len\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "FM", "NN", "$.", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sind, ich erkenn\u2019 es wol, mir blo\u00df durch Dich geschencket:", "tokens": ["Sind", ",", "ich", "er\u00b7kenn'", "es", "wol", ",", "mir", "blo\u00df", "durch", "Dich", "ge\u00b7schen\u00b7cket", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn jeder kleiner Augenblick", "tokens": ["Denn", "je\u00b7der", "klei\u00b7ner", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist ja von unsrer Zeit ein wesentliches St\u00fcck,", "tokens": ["Ist", "ja", "von", "uns\u00b7rer", "Zeit", "ein", "we\u00b7sent\u00b7li\u00b7ches", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Worinn uns GOTT nicht nur das Leben,", "tokens": ["Wo\u00b7rinn", "uns", "GoTT", "nicht", "nur", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NE", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Auch alles das zugleich, was Er auf dieser Welt,", "tokens": ["Auch", "al\u00b7les", "das", "zu\u00b7gleich", ",", "was", "Er", "auf", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ART", "ADV", "$,", "PRELS", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "An Gut, Bequemlichkeit, Gesundheit uns gegeben,", "tokens": ["An", "Gut", ",", "Be\u00b7quem\u00b7lich\u00b7keit", ",", "Ge\u00b7sund\u00b7heit", "uns", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Recht wunderbar erhalten, noch erh\u00e4lt.", "tokens": ["Recht", "wun\u00b7der\u00b7bar", "er\u00b7hal\u00b7ten", ",", "noch", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}