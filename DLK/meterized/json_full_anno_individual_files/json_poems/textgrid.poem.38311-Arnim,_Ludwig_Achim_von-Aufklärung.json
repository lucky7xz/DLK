{"textgrid.poem.38311": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Aufkl\u00e4rung", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was soll ich thun, was soll ich glauben?", "tokens": ["Was", "soll", "ich", "thun", ",", "was", "soll", "ich", "glau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und was ist meine Zuversicht?", "tokens": ["Und", "was", "ist", "mei\u00b7ne", "Zu\u00b7ver\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will man mir meine Zuflucht rauben,", "tokens": ["Will", "man", "mir", "mei\u00b7ne", "Zu\u00b7flucht", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die mir des H\u00f6chsten Wort verspricht?", "tokens": ["Die", "mir", "des", "H\u00f6chs\u00b7ten", "Wort", "ver\u00b7spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So ist mein Leben Gram und Leid", "tokens": ["So", "ist", "mein", "Le\u00b7ben", "Gram", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In dieser aufgekl\u00e4rten Zeit.", "tokens": ["In", "die\u00b7ser", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ten", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein jeder schnitzt sich nach Belieben", "tokens": ["Ein", "je\u00b7der", "schnitzt", "sich", "nach", "Be\u00b7lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jezt selber die Religion;", "tokens": ["Jezt", "sel\u00b7ber", "die", "Re\u00b7li\u00b7gi\u00b7on", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Teufel, hei\u00dft es, ist vertrieben,", "tokens": ["Der", "Teu\u00b7fel", ",", "hei\u00dft", "es", ",", "ist", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Christus ist nicht Gottessohn;", "tokens": ["Und", "Chris\u00b7tus", "ist", "nicht", "Got\u00b7tes\u00b7sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nichts gilt mehr Dreyeinigkeit,", "tokens": ["Und", "nichts", "gilt", "mehr", "Drey\u00b7ei\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PIAT", "NN", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "In dieser aufgekl\u00e4rten Zeit.", "tokens": ["In", "die\u00b7ser", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ten", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Taufe, das Kommunicieren,", "tokens": ["Die", "Tau\u00b7fe", ",", "das", "Kom\u00b7mu\u00b7ni\u00b7cie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist f\u00fcr die aufgekl\u00e4rte Welt", "tokens": ["Ist", "f\u00fcr", "die", "auf\u00b7ge\u00b7kl\u00e4r\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur Thorheit wie das Kopulieren,", "tokens": ["Nur", "Thor\u00b7heit", "wie", "das", "Ko\u00b7pu\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bringet nur den Priestern Geld,", "tokens": ["Und", "brin\u00b7get", "nur", "den", "Pries\u00b7tern", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Kluge nimmt ein Weib und freyt", "tokens": ["Der", "Klu\u00b7ge", "nimmt", "ein", "Weib", "und", "freyt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach Art der aufgekl\u00e4rten Zeit.", "tokens": ["Nach", "Art", "der", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ten", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Ehebruch ist keine S\u00fcnde,", "tokens": ["Der", "E\u00b7heb\u00b7ruch", "ist", "kei\u00b7ne", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch weniger die Hurerey;", "tokens": ["Noch", "we\u00b7ni\u00b7ger", "die", "Hu\u00b7re\u00b7rey", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und obs gleich in der Bibel st\u00fcnde,", "tokens": ["Und", "obs", "gleich", "in", "der", "Bi\u00b7bel", "st\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Steht doch der Galgen nicht dabey.", "tokens": ["Steht", "doch", "der", "Gal\u00b7gen", "nicht", "da\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKNEG", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum ists galante Sittlichkeit", "tokens": ["Drum", "ists", "ga\u00b7lan\u00b7te", "Sitt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In dieser aufgekl\u00e4rten Zeit.", "tokens": ["In", "die\u00b7ser", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ten", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Aufgekl\u00e4rte folgt den Trieben,", "tokens": ["Der", "Auf\u00b7ge\u00b7kl\u00e4r\u00b7te", "folgt", "den", "Trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und diese sind ihm Glaubenslehr;", "tokens": ["Und", "die\u00b7se", "sind", "ihm", "Glau\u00b7bens\u00b7lehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was Gottes Wort ihm vorgeschrieben,", "tokens": ["Was", "Got\u00b7tes", "Wort", "ihm", "vor\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das deucht ihm fabelhaft und schwer.", "tokens": ["Das", "deucht", "ihm", "fa\u00b7bel\u00b7haft", "und", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem P\u00f6bel ist es nur geweiht", "tokens": ["Dem", "P\u00f6\u00b7bel", "ist", "es", "nur", "ge\u00b7weiht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nicht der aufgekl\u00e4rten Zeit.", "tokens": ["Und", "nicht", "der", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ten", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Tugend sucht man zwar zu preisen,", "tokens": ["Die", "Tu\u00b7gend", "sucht", "man", "zwar", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als die alleine selig macht;", "tokens": ["Als", "die", "al\u00b7lei\u00b7ne", "se\u00b7lig", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nur den Glauben zu verweisen,", "tokens": ["Doch", "nur", "den", "Glau\u00b7ben", "zu", "ver\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil der uns unsre Laster sagt.", "tokens": ["Weil", "der", "uns", "uns\u00b7re", "Las\u00b7ter", "sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Laster suchet man nicht weit", "tokens": ["Und", "Las\u00b7ter", "su\u00b7chet", "man", "nicht", "weit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PIS", "PTKNEG", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In dieser aufgekl\u00e4rten Zeit.", "tokens": ["In", "die\u00b7ser", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ten", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So liegt nun in dem S\u00fcndenschlafe", "tokens": ["So", "liegt", "nun", "in", "dem", "S\u00fcn\u00b7den\u00b7schla\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das ganze aufgekl\u00e4rte Land;", "tokens": ["Das", "gan\u00b7ze", "auf\u00b7ge\u00b7kl\u00e4r\u00b7te", "Land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil auch die ewge H\u00f6llenstrafe", "tokens": ["Weil", "auch", "die", "ew\u00b7ge", "H\u00f6l\u00b7len\u00b7stra\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist gl\u00fccklich aus der Welt verbannt.", "tokens": ["Ist", "gl\u00fcck\u00b7lich", "aus", "der", "Welt", "ver\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn jeder hofft Barmherzigkeit", "tokens": ["Denn", "je\u00b7der", "hofft", "Barm\u00b7her\u00b7zig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In dieser und in jener Zeit.", "tokens": ["In", "die\u00b7ser", "und", "in", "je\u00b7ner", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "KON", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "So schreiben alle Antichristen,", "tokens": ["So", "schrei\u00b7ben", "al\u00b7le", "An\u00b7ti\u00b7ch\u00b7ris\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Weil es dem Leichtsinn wohlgef\u00e4llt;", "tokens": ["Weil", "es", "dem", "Leicht\u00b7sinn", "wohl\u00b7ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn diese sind als Kanzelisten", "tokens": ["Denn", "die\u00b7se", "sind", "als", "Kan\u00b7ze\u00b7lis\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Satan selber angestellt:", "tokens": ["Vom", "Sa\u00b7tan", "sel\u00b7ber", "an\u00b7ge\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch sie gewinnt der Teufel mehr,", "tokens": ["Durch", "sie", "ge\u00b7winnt", "der", "Teu\u00b7fel", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als wenn er selbst zugegen w\u00e4r.", "tokens": ["Als", "wenn", "er", "selbst", "zu\u00b7ge\u00b7gen", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O la\u00dft mich doch bei meiner Bibel,", "tokens": ["O", "la\u00dft", "mich", "doch", "bei", "mei\u00b7ner", "Bi\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00dft mich in meiner Dunkelheit:", "tokens": ["La\u00dft", "mich", "in", "mei\u00b7ner", "Dun\u00b7kel\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn ohne Hoffnung wird mir \u00fcbel,", "tokens": ["Denn", "oh\u00b7ne", "Hoff\u00b7nung", "wird", "mir", "\u00fc\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bei dieser aufgekl\u00e4rten Zeit;", "tokens": ["Bei", "die\u00b7ser", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ten", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ohne Hoffnung bin ich hier", "tokens": ["Und", "oh\u00b7ne", "Hoff\u00b7nung", "bin", "ich", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein elend aufgekl\u00e4rtes Thier.", "tokens": ["Ein", "e\u00b7lend", "auf\u00b7ge\u00b7kl\u00e4r\u00b7tes", "Thier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Drum Thoren sprecht, ich mag nichts h\u00f6ren,", "tokens": ["Drum", "Tho\u00b7ren", "sprecht", ",", "ich", "mag", "nichts", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "VVFIN", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verschonet mich mit eurem Gift;", "tokens": ["Ver\u00b7scho\u00b7net", "mich", "mit", "eu\u00b7rem", "Gift", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gesetzt, wenn es auch 