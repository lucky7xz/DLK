{"dta.poem.6020": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie h\u00e4tt' sie in der Welt doch einen andern Ort,", "tokens": ["Wie", "h\u00e4tt'", "sie", "in", "der", "Welt", "doch", "ei\u00b7nen", "an\u00b7dern", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als in der Mitte, kriegen k\u00f6nnen?", "tokens": ["Als", "in", "der", "Mit\u00b7te", ",", "krie\u00b7gen", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Daselbst nur hat ihr Feur den Heerd, sie schickt von dort", "tokens": ["Da\u00b7selbst", "nur", "hat", "ihr", "Feur", "den", "Heerd", ",", "sie", "schickt", "von", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Strahlen heisse Gluht, die in der R\u00fcnde brennen.", "tokens": ["Der", "Strah\u00b7len", "heis\u00b7se", "Gluht", ",", "die", "in", "der", "R\u00fcn\u00b7de", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Daselbst, indem sie um sich selbst sich dreht,", "tokens": ["Da\u00b7selbst", ",", "in\u00b7dem", "sie", "um", "sich", "selbst", "sich", "dreht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "APPR", "PRF", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wirfft ihr geschwinder Druck, und die gedrengte Krafft,", "tokens": ["Wirfft", "ihr", "ge\u00b7schwin\u00b7der", "Druck", ",", "und", "die", "ge\u00b7dreng\u00b7te", "Krafft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zusammt den feur\u2019gen Schwall, so stets in ihr entsteht,", "tokens": ["Zu\u00b7sammt", "den", "feur'\u00b7gen", "Schwall", ",", "so", "stets", "in", "ihr", "ent\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von allen Seiten aus die Lebensreiche Hitze,", "tokens": ["Von", "al\u00b7len", "Sei\u00b7ten", "aus", "die", "Le\u00b7bens\u00b7rei\u00b7che", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wodurch sie der Natur so unentbehrlich n\u00fctze.", "tokens": ["Wo\u00b7durch", "sie", "der", "Na\u00b7tur", "so", "un\u00b7ent\u00b7behr\u00b7lich", "n\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}