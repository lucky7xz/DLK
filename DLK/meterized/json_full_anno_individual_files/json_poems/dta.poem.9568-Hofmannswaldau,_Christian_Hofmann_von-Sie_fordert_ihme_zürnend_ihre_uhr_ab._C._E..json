{"dta.poem.9568": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie fordert ihme z\u00fcrnend ihre uhr ab.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geh hin/ verha\u00dfte uhr/ zur trotzigen Melinden", "tokens": ["Geh", "hin", "/", "ver\u00b7ha\u00df\u00b7te", "uhr", "/", "zur", "trot\u00b7zi\u00b7gen", "Me\u00b7lin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$(", "ADJA", "NN", "$(", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lieb du mir zuvor/ so feind bin ich dir itzt/", "tokens": ["So", "lieb", "du", "mir", "zu\u00b7vor", "/", "so", "feind", "bin", "ich", "dir", "itzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "ADV", "$(", "ADV", "ADJD", "VAFIN", "PPER", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nachdem du ihren grimm hast wider mich erhitzt/", "tokens": ["Nach\u00b7dem", "du", "ih\u00b7ren", "grimm", "hast", "wi\u00b7der", "mich", "er\u00b7hitzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "VAFIN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sie mit rach und zorn wilt gegen mich entz\u00fcnden.", "tokens": ["Und", "sie", "mit", "rach", "und", "zorn", "wilt", "ge\u00b7gen", "mich", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NE", "KON", "NN", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weg nur/ ich mag dich nicht! wer kan dich auch ergr\u00fcnden?", "tokens": ["Weg", "nur", "/", "ich", "mag", "dich", "nicht", "!", "wer", "kan", "dich", "auch", "er\u00b7gr\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$(", "PPER", "VMFIN", "PRF", "PTKNEG", "$.", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer wei\u00df/ da\u00df solch ein ha\u00df in deinen r\u00e4dern sitzt/", "tokens": ["Wer", "wei\u00df", "/", "da\u00df", "solch", "ein", "ha\u00df", "in", "dei\u00b7nen", "r\u00e4\u00b7dern", "sitzt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "PIAT", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wodurch ihr unmuths-feur so grimmig auff mich blitzt?", "tokens": ["Wo\u00b7durch", "ihr", "un\u00b7muths\u00b7feur", "so", "grim\u00b7mig", "auff", "mich", "blitzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "An deinem zeiger h\u00e4tt\u2019 ichs ewig nicht kont finden.", "tokens": ["An", "dei\u00b7nem", "zei\u00b7ger", "h\u00e4tt'", "ichs", "e\u00b7wig", "nicht", "kont", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIS", "ADJD", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch/ was verfolg ich dich? es ist Melindens art/", "tokens": ["Doch", "/", "was", "ver\u00b7folg", "ich", "dich", "?", "es", "ist", "Me\u00b7lin\u00b7dens", "art", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PWS", "VVFIN", "PPER", "PRF", "$.", "PPER", "VAFIN", "NE", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie hat wohl ehmahls schon das spiel mir so gekart/", "tokens": ["Sie", "hat", "wohl", "eh\u00b7mahls", "schon", "das", "spiel", "mir", "so", "ge\u00b7kart", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "PDS", "VVFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und nichts/ als nur verdru\u00df und pein mir wollen g\u00f6nnen;", "tokens": ["Und", "nichts", "/", "als", "nur", "ver\u00b7dru\u00df", "und", "pein", "mir", "wol\u00b7len", "g\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$(", "KOKOM", "ADV", "NN", "KON", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die unruh in der uhr ist ihrer sinnen bild/", "tokens": ["Die", "un\u00b7ruh", "in", "der", "uhr", "ist", "ih\u00b7rer", "sin\u00b7nen", "bild", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die will sie etwan sehn? Jhr wunsch sey dann erf\u00fcllt!", "tokens": ["Die", "will", "sie", "et\u00b7wan", "sehn", "?", "Ihr", "wunsch", "sey", "dann", "er\u00b7f\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$.", "PPER", "ADJD", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie sch\u00f6n wird sie sich nun darinn nicht spiegeln k\u00f6nnen.", "tokens": ["Wie", "sch\u00f6n", "wird", "sie", "sich", "nun", "da\u00b7rinn", "nicht", "spie\u00b7geln", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PRF", "ADV", "PAV", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}