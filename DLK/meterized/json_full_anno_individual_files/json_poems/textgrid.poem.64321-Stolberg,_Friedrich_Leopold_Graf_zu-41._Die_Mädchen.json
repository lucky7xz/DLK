{"textgrid.poem.64321": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "41. Die M\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sehe mit Schmerzen,", "tokens": ["Ich", "se\u00b7he", "mit", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Du kennest die Herzen", "tokens": ["Du", "ken\u00b7nest", "die", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Kupidens noch nicht;", "tokens": ["Ku\u00b7pi\u00b7dens", "noch", "nicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Du hoffest mit Herzen", "tokens": ["Du", "hof\u00b7fest", "mit", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Der M\u00e4dchen zu scherzen,", "tokens": ["Der", "M\u00e4d\u00b7chen", "zu", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Es reizet die Rose dich, ehe sie sticht.", "tokens": ["Es", "rei\u00b7zet", "die", "Ro\u00b7se", "dich", ",", "e\u00b7he", "sie", "sticht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Zu spielen mit Rosen,", "tokens": ["Zu", "spie\u00b7len", "mit", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und M\u00e4dchen zu kosen", "tokens": ["Und", "M\u00e4d\u00b7chen", "zu", "ko\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "L\u00e4\u00dft lieblich und fein!", "tokens": ["L\u00e4\u00dft", "lieb\u00b7lich", "und", "fein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Du trauest den Losen?", "tokens": ["Du", "trau\u00b7est", "den", "Lo\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Sie lachen und sto\u00dfen", "tokens": ["Sie", "la\u00b7chen", "und", "sto\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ganz freundlich den Dolch in das Herz uns hinein!", "tokens": ["Ganz", "freund\u00b7lich", "den", "Dolch", "in", "das", "Herz", "uns", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "O J\u00fcngling, dann m\u00fcssen", "tokens": ["O", "J\u00fcng\u00b7ling", ",", "dann", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "ADV", "VMFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit Thr\u00e4nen wir b\u00fc\u00dfen,", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "wir", "b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mit brennendem Schmerz!", "tokens": ["Mit", "bren\u00b7nen\u00b7dem", "Schmerz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Es fliehen die S\u00fc\u00dfen", "tokens": ["Es", "flie\u00b7hen", "die", "S\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zu andern, und k\u00fcssen", "tokens": ["Zu", "an\u00b7dern", ",", "und", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "PIS", "$,", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Auch ihnen Verzweiflung ins wehrlose Herz.", "tokens": ["Auch", "ih\u00b7nen", "Ver\u00b7zwei\u00b7flung", "ins", "wehr\u00b7lo\u00b7se", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Sie k\u00f6nnen mit Blicken", "tokens": ["Sie", "k\u00f6n\u00b7nen", "mit", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Herzen bestricken,", "tokens": ["Die", "Her\u00b7zen", "be\u00b7stri\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und scheinen so gut!", "tokens": ["Und", "schei\u00b7nen", "so", "gut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Kaum kehrst du den R\u00fccken,", "tokens": ["Kaum", "kehrst", "du", "den", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "So winken und nicken", "tokens": ["So", "win\u00b7ken", "und", "ni\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Falschen, und freun sich der wachsenden Glut!", "tokens": ["Die", "Fal\u00b7schen", ",", "und", "freun", "sich", "der", "wach\u00b7sen\u00b7den", "Glut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn endlich dich eine", "tokens": ["Wenn", "end\u00b7lich", "dich", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "ART"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Von K\u00fcnsten noch reine", "tokens": ["Von", "K\u00fcns\u00b7ten", "noch", "rei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mit Innigkeit liebt,", "tokens": ["Mit", "In\u00b7nig\u00b7keit", "liebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "So wisse, der kleine", "tokens": ["So", "wis\u00b7se", ",", "der", "klei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Kupido hat seine", "tokens": ["Ku\u00b7pi\u00b7do", "hat", "sei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Geheimeren R\u00e4nke, wodurch er betr\u00fcbt.", "tokens": ["Ge\u00b7hei\u00b7me\u00b7ren", "R\u00e4n\u00b7ke", ",", "wo\u00b7durch", "er", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Oft spinnet er F\u00e4dchen", "tokens": ["Oft", "spin\u00b7net", "er", "F\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Am goldenen R\u00e4dchen,", "tokens": ["Am", "gol\u00b7de\u00b7nen", "R\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wie Haare so fein;", "tokens": ["Wie", "Haa\u00b7re", "so", "fein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Kaum glaubst du am Dr\u00e4htchen", "tokens": ["Kaum", "glaubst", "du", "am", "Dr\u00e4ht\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zu halten dein M\u00e4dchen,", "tokens": ["Zu", "hal\u00b7ten", "dein", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "So rei\u00dft es und l\u00e4\u00dft dich Beth\u00f6rten allein!", "tokens": ["So", "rei\u00dft", "es", "und", "l\u00e4\u00dft", "dich", "Be\u00b7th\u00f6r\u00b7ten", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "NN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Viel hab ich gelitten,", "tokens": ["Viel", "hab", "ich", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Hab dreimal gestritten", "tokens": ["Hab", "drei\u00b7mal", "ge\u00b7strit\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "F\u00fcr Thr\u00e4nen zum Sold!", "tokens": ["F\u00fcr", "Thr\u00e4\u00b7nen", "zum", "Sold", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Bei d\u00f6rflichen Sitten,", "tokens": ["Bei", "d\u00f6rf\u00b7li\u00b7chen", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "In moosigen H\u00fctten,", "tokens": ["In", "moo\u00b7si\u00b7gen", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Dort wohnet die Liebe noch lauter wie Gold!", "tokens": ["Dort", "woh\u00b7net", "die", "Lie\u00b7be", "noch", "lau\u00b7ter", "wie", "Gold", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PIAT", "KOKOM", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Ich sehe mit Schmerzen,", "tokens": ["Ich", "se\u00b7he", "mit", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Du kennest die Herzen", "tokens": ["Du", "ken\u00b7nest", "die", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Kupidens noch nicht;", "tokens": ["Ku\u00b7pi\u00b7dens", "noch", "nicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Du hoffest mit Herzen", "tokens": ["Du", "hof\u00b7fest", "mit", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Der M\u00e4dchen zu scherzen,", "tokens": ["Der", "M\u00e4d\u00b7chen", "zu", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Es reizet die Rose dich, ehe sie sticht.", "tokens": ["Es", "rei\u00b7zet", "die", "Ro\u00b7se", "dich", ",", "e\u00b7he", "sie", "sticht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {