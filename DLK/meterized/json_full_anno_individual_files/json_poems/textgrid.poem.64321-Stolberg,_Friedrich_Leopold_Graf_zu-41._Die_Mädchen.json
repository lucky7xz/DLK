: "Und scheinen so gut!", "tokens": ["Und", "schei\u00b7nen", "so", "gut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Kaum kehrst du den R\u00fccken,", "tokens": ["Kaum", "kehrst", "du", "den", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "So winken und nicken", "tokens": ["So", "win\u00b7ken", "und", "ni\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Falschen, und freun sich der wachsenden Glut!", "tokens": ["Die", "Fal\u00b7schen", ",", "und", "freun", "sich", "der", "wach\u00b7sen\u00b7den", "Glut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.12": {"line.1": {"text": "Wenn endlich dich eine", "tokens": ["Wenn", "end\u00b7lich", "dich", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "ART"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Von K\u00fcnsten noch reine", "tokens": ["Von", "K\u00fcns\u00b7ten", "noch", "rei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mit Innigkeit liebt,", "tokens": ["Mit", "In\u00b7nig\u00b7keit", "liebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "So wisse, der kleine", "tokens": ["So", "wis\u00b7se", ",", "der", "klei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Kupido hat seine", "tokens": ["Ku\u00b7pi\u00b7do", "hat", "sei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Geheimeren R\u00e4nke, wodurch er betr\u00fcbt.", "tokens": ["Ge\u00b7hei\u00b7me\u00b7ren", "R\u00e4n\u00b7ke", ",", "wo\u00b7durch", "er", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.13": {"line.1": {"text": "Oft spinnet er F\u00e4dchen", "tokens": ["Oft", "spin\u00b7net", "er", "F\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Am goldenen R\u00e4dchen,", "tokens": ["Am", "gol\u00b7de\u00b7nen", "R\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wie Haare so fein;", "tokens": ["Wie", "Haa\u00b7re", "so", "fein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Kaum glaubst du am Dr\u00e4htchen", "tokens": ["Kaum", "glaubst", "du", "am", "Dr\u00e4ht\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zu halten dein M\u00e4dchen,", "tokens": ["Zu", "hal\u00b7ten", "dein", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "So rei\u00dft es und l\u00e4\u00dft dich Beth\u00f6rten allein!", "tokens": ["So", "rei\u00dft", "es", "und", "l\u00e4\u00dft", "dich", "Be\u00b7th\u00f6r\u00b7ten", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "NN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.14": {"line.1": {"text": "Viel hab ich gelitten,", "tokens": ["Viel", "hab", "ich", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Hab dreimal gestritten", "tokens": ["Hab", "drei\u00b7mal", "ge\u00b7strit\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "F\u00fcr Thr\u00e4nen zum Sold!", "tokens": ["F\u00fcr", "Thr\u00e4\u00b7nen", "zum", "Sold", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Bei d\u00f6rflichen Sitten,", "tokens": ["Bei", "d\u00f6rf\u00b7li\u00b7chen", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "In moosigen H\u00fctten,", "tokens": ["In", "moo\u00b7si\u00b7gen", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Dort wohnet die Liebe noch lauter wie Gold!", "tokens": ["Dort", "woh\u00b7net", "die", "Lie\u00b7be", "noch", "lau\u00b7ter", "wie", "Gold", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PIAT", "KOKOM", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}