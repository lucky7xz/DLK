{"dta.poem.6959": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die R\u00f6hren, die erf\u00fcllt von Flammen, Lufft und Wind:", "tokens": ["Die", "R\u00f6h\u00b7ren", ",", "die", "er\u00b7f\u00fcllt", "von", "Flam\u00b7men", ",", "Lufft", "und", "Wind", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dies ist was Epicur der Menschen Seelen nennet,", "tokens": ["Dies", "ist", "was", "E\u00b7pi\u00b7cur", "der", "Men\u00b7schen", "See\u00b7len", "nen\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dies ist, was gr\u00f6sser wird, in D\u00fcnsten sich verd\u00fcnnt,", "tokens": ["Dies", "ist", ",", "was", "gr\u00f6s\u00b7ser", "wird", ",", "in", "D\u00fcns\u00b7ten", "sich", "ver\u00b7d\u00fcnnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und was mit unsrem Blut zugleich von hinnen rinnt.", "tokens": ["Und", "was", "mit", "uns\u00b7rem", "Blut", "zu\u00b7gleich", "von", "hin\u00b7nen", "rinnt", "."], "token_info": ["word", "word", "word", "