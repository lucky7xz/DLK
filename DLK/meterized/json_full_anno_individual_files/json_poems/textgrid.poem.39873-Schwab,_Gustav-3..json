{"textgrid.poem.39873": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Enkel w\u00e4chst mit Lust heran,", "tokens": ["Der", "En\u00b7kel", "w\u00e4chst", "mit", "Lust", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Nordlands Knaben bl\u00fchen;", "tokens": ["Wie", "Nord\u00b7lands", "Kna\u00b7ben", "bl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Um wenig Jahre sei es noch,", "tokens": ["Um", "we\u00b7nig", "Jah\u00b7re", "sei", "es", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist er zum Mann gediehen.", "tokens": ["Ist", "er", "zum", "Mann", "ge\u00b7die\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Stunden, die flogen schnell dahin,", "tokens": ["Die", "Stun\u00b7den", ",", "die", "flo\u00b7gen", "schnell", "da\u00b7hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ADJD", "PAV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie man ein Liedlein singet;", "tokens": ["Wie", "man", "ein", "Lied\u00b7lein", "sin\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Feld gedieh, das Haus stieg auf,", "tokens": ["Das", "Feld", "ge\u00b7dieh", ",", "das", "Haus", "stieg", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Greis sa\u00df wie verj\u00fcnget.", "tokens": ["Der", "Greis", "sa\u00df", "wie", "ver\u00b7j\u00fcn\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es hing ihm eine Wolke wohl", "tokens": ["Es", "hing", "ihm", "ei\u00b7ne", "Wol\u00b7ke", "wohl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In seiner Stirne Falten;", "tokens": ["In", "sei\u00b7ner", "Stir\u00b7ne", "Fal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der J\u00fcngling fragt nicht, dient so treu,", "tokens": ["Der", "J\u00fcng\u00b7ling", "fragt", "nicht", ",", "dient", "so", "treu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis er erfreute den Alten.", "tokens": ["Bis", "er", "er\u00b7freu\u00b7te", "den", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch wie die Zeit nun schneller ging,", "tokens": ["Doch", "wie", "die", "Zeit", "nun", "schnel\u00b7ler", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah man ihn stille sitzen,", "tokens": ["Sah", "man", "ihn", "stil\u00b7le", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "VVFIN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und aus den hohlen Augen war's,", "tokens": ["Und", "aus", "den", "hoh\u00b7len", "Au\u00b7gen", "wa\u00b7r's", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Als wollt' ein Feuer blitzen.", "tokens": ["Als", "wollt'", "ein", "Feu\u00b7er", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zuletzt das Schweigen doch er brach,", "tokens": ["Zu\u00b7letzt", "das", "Schwei\u00b7gen", "doch", "er", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das manchen Tag gedauert.", "tokens": ["Das", "man\u00b7chen", "Tag", "ge\u00b7dau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er sprach: \u00bbStellt mir den Enkel her!\u00ab", "tokens": ["Er", "sprach", ":", "\u00bb", "Stellt", "mir", "den", "En\u00b7kel", "her", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VVIMP", "PPER", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er rief's von Schmerz durchschauert.", "tokens": ["Er", "rie\u00b7f's", "von", "Schmerz", "durch\u00b7schau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbgro\u00dfvater, la\u00df nicht f\u00fchren mich!", "tokens": ["\u00bb", "gro\u00df\u00b7va\u00b7ter", ",", "la\u00df", "nicht", "f\u00fch\u00b7ren", "mich", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "VVIMP", "PTKNEG", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch Fr\u00fchling wird's im Norden,", "tokens": ["Auch", "Fr\u00fch\u00b7ling", "wird's", "im", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du siehst nicht, wie ich gewachsen bin,", "tokens": ["Du", "siehst", "nicht", ",", "wie", "ich", "ge\u00b7wach\u00b7sen", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ich bin ein J\u00fcngling worden.\u00ab", "tokens": ["Ich", "bin", "ein", "J\u00fcng\u00b7ling", "wor\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VAPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der schlanke Knabe, der eilt herzu,", "tokens": ["Der", "schlan\u00b7ke", "Kna\u00b7be", ",", "der", "eilt", "her\u00b7zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihn fa\u00dft der Greis mit Zittern,", "tokens": ["Ihn", "fa\u00dft", "der", "Greis", "mit", "Zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbja,\u00ab ruft er, \u00bbSommer im Norden ward's,", "tokens": ["\u00bb", "ja", ",", "\u00ab", "ruft", "er", ",", "\u00bb", "Som\u00b7mer", "im", "Nor\u00b7den", "ward's", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "NN", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich horche den Ungewittern!", "tokens": ["Ich", "hor\u00b7che", "den", "Un\u00b7ge\u00b7wit\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Weh mir, es sprosset ihm schon der Bart,", "tokens": ["Weh", "mir", ",", "es", "spros\u00b7set", "ihm", "schon", "der", "Bart", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+--++-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Es schwellen die Glieder, die Knochen,", "tokens": ["Es", "schwel\u00b7len", "die", "Glie\u00b7der", ",", "die", "Kno\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Er ist ein Mann geworden und hat", "tokens": ["Er", "ist", "ein", "Mann", "ge\u00b7wor\u00b7den", "und", "hat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VAPP", "KON", "VAFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Den Vater noch nicht gerochen! \u2013", "tokens": ["Den", "Va\u00b7ter", "noch", "nicht", "ge\u00b7ro\u00b7chen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "VVPP", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Blutrache, heilig, alt Gesetz,", "tokens": ["Blu\u00b7tra\u00b7che", ",", "hei\u00b7lig", ",", "alt", "Ge\u00b7setz", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie unsre G\u00f6tter und Eichen,", "tokens": ["Wie", "uns\u00b7re", "G\u00f6t\u00b7ter", "und", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vor dir mu\u00df unsers Hauses Fried'", "tokens": ["Vor", "dir", "mu\u00df", "un\u00b7sers", "Hau\u00b7ses", "Fried'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Liebe mir heut erbleichen!", "tokens": ["Und", "Lie\u00b7be", "mir", "heut", "er\u00b7blei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Seht ihr es nicht? mir d\u00e4ucht, ich seh's \u2013", "tokens": ["Seht", "ihr", "es", "nicht", "?", "mir", "d\u00e4ucht", ",", "ich", "seh's", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "$,", "PPER", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bin doch blind so lange \u2013", "tokens": ["Und", "bin", "doch", "blind", "so", "lan\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "ADV", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie seine Augen funkeln wild!", "tokens": ["Wie", "sei\u00b7ne", "Au\u00b7gen", "fun\u00b7keln", "wild", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du dort, ist dir nicht bange?", "tokens": ["Du", "dort", ",", "ist", "dir", "nicht", "ban\u00b7ge", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "O weh', du hast mir gedient so fromm,", "tokens": ["O", "weh'", ",", "du", "hast", "mir", "ge\u00b7dient", "so", "fromm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "VAFIN", "PPER", "VVPP", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hast's wie ein Sohn getrieben!", "tokens": ["Hast's", "wie", "ein", "Sohn", "ge\u00b7trie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du solltest f\u00fchren in's neue Haus", "tokens": ["Du", "soll\u00b7test", "f\u00fch\u00b7ren", "in's", "neu\u00b7e", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Braut, die dir treu geblieben.", "tokens": ["Die", "Braut", ",", "die", "dir", "treu", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Jetzt kannst du bei mir nicht bau'n dein Haus,", "tokens": ["Jetzt", "kannst", "du", "bei", "mir", "nicht", "bau'n", "dein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Bei mir dein Weib nicht freien \u2013", "tokens": ["Bei", "mir", "dein", "Weib", "nicht", "frei\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie soll in seinem Angesicht", "tokens": ["Wie", "soll", "in", "sei\u00b7nem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir dein Geschlecht gedeihen?", "tokens": ["Dir", "dein", "Ge\u00b7schlecht", "ge\u00b7dei\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Nimm dir aus Kammer und Stall ein Theil,", "tokens": ["Nimm", "dir", "aus", "Kam\u00b7mer", "und", "Stall", "ein", "Theil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was mir der Sohn sollt' erben!", "tokens": ["Was", "mir", "der", "Sohn", "sollt'", "er\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So lange die Rach' in dem Knaben schl\u00e4ft,", "tokens": ["So", "lan\u00b7ge", "die", "Rach'", "in", "dem", "Kna\u00b7ben", "schl\u00e4ft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Fleuch, fleuch! du sollst mir nicht sterben!", "tokens": ["Fleuch", ",", "fleuch", "!", "du", "sollst", "mir", "nicht", "ster\u00b7ben", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.14": {"line.1": {"text": "Zur fernsten Orkneysinsel zeuch!", "tokens": ["Zur", "ferns\u00b7ten", "Ork\u00b7ney\u00b7sin\u00b7sel", "zeuch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVIMP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dort, hinter der Fluten Walle,", "tokens": ["Dort", ",", "hin\u00b7ter", "der", "Flu\u00b7ten", "Wal\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dort bau von meinem Gute dir", "tokens": ["Dort", "bau", "von", "mei\u00b7nem", "Gu\u00b7te", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eine feste, helle Halle!", "tokens": ["Ei\u00b7ne", "fes\u00b7te", ",", "hel\u00b7le", "Hal\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Dort lebe sicher und zeug' ein Kind", "tokens": ["Dort", "le\u00b7be", "si\u00b7cher", "und", "zeug'", "ein", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "VVFIN", "ART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "F\u00fcr deines Alters Tage!", "tokens": ["F\u00fcr", "dei\u00b7nes", "Al\u00b7ters", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und keiner sei \u2013 nimm hin den Wunsch \u2013", "tokens": ["Und", "kei\u00b7ner", "sei", "\u2013", "nimm", "hin", "den", "Wunsch", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$(", "VVIMP", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der dir den Sohn erschlage!\u00ab", "tokens": ["Der", "dir", "den", "Sohn", "er\u00b7schla\u00b7ge", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Der Enkel w\u00e4chst mit Lust heran,", "tokens": ["Der", "En\u00b7kel", "w\u00e4chst", "mit", "Lust", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Nordlands Knaben bl\u00fchen;", "tokens": ["Wie", "Nord\u00b7lands", "Kna\u00b7ben", "bl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Um wenig Jahre sei es noch,", "tokens": ["Um", "we\u00b7nig", "Jah\u00b7re", "sei", "es", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist er zum Mann gediehen.", "tokens": ["Ist", "er", "zum", "Mann", "ge\u00b7die\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Die Stunden, die flogen schnell dahin,", "tokens": ["Die", "Stun\u00b7den", ",", "die", "flo\u00b7gen", "schnell", "da\u00b7hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ADJD", "PAV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie man ein Liedlein singet;", "tokens": ["Wie", "man", "ein", "Lied\u00b7lein", "sin\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Feld gedieh, das Haus stieg auf,", "tokens": ["Das", "Feld", "ge\u00b7dieh", ",", "das", "Haus", "stieg", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Greis sa\u00df wie verj\u00fcnget.", "tokens": ["Der", "Greis", "sa\u00df", "wie", "ver\u00b7j\u00fcn\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Es hing ihm eine Wolke wohl", "tokens": ["Es", "hing", "ihm", "ei\u00b7ne", "Wol\u00b7ke", "wohl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In seiner Stirne Falten;", "tokens": ["In", "sei\u00b7ner", "Stir\u00b7ne", "Fal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der J\u00fcngling fragt nicht, dient so treu,", "tokens": ["Der", "J\u00fcng\u00b7ling", "fragt", "nicht", ",", "dient", "so", "treu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis er erfreute den Alten.", "tokens": ["Bis", "er", "er\u00b7freu\u00b7te", "den", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.19": {"line.1": {"text": "Doch wie die Zeit nun schneller ging,", "tokens": ["Doch", "wie", "die", "Zeit", "nun", "schnel\u00b7ler", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah man ihn stille sitzen,", "tokens": ["Sah", "man", "ihn", "stil\u00b7le", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "VVFIN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und aus den hohlen Augen war's,", "tokens": ["Und", "aus", "den", "hoh\u00b7len", "Au\u00b7gen", "wa\u00b7r's", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Als wollt' ein Feuer blitzen.", "tokens": ["Als", "wollt'", "ein", "Feu\u00b7er", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Zuletzt das Schweigen doch er brach,", "tokens": ["Zu\u00b7letzt", "das", "Schwei\u00b7gen", "doch", "er", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das manchen Tag gedauert.", "tokens": ["Das", "man\u00b7chen", "Tag", "ge\u00b7dau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er sprach: \u00bbStellt mir den Enkel her!\u00ab", "tokens": ["Er", "sprach", ":", "\u00bb", "Stellt", "mir", "den", "En\u00b7kel", "her", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VVIMP", "PPER", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er rief's von Schmerz durchschauert.", "tokens": ["Er", "rie\u00b7f's", "von", "Schmerz", "durch\u00b7schau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.21": {"line.1": {"text": "\u00bbgro\u00dfvater, la\u00df nicht f\u00fchren mich!", "tokens": ["\u00bb", "gro\u00df\u00b7va\u00b7ter", ",", "la\u00df", "nicht", "f\u00fch\u00b7ren", "mich", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "VVIMP", "PTKNEG", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch Fr\u00fchling wird's im Norden,", "tokens": ["Auch", "Fr\u00fch\u00b7ling", "wird's", "im", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du siehst nicht, wie ich gewachsen bin,", "tokens": ["Du", "siehst", "nicht", ",", "wie", "ich", "ge\u00b7wach\u00b7sen", "bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ich bin ein J\u00fcngling worden.\u00ab", "tokens": ["Ich", "bin", "ein", "J\u00fcng\u00b7ling", "wor\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VAPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Der schlanke Knabe, der eilt herzu,", "tokens": ["Der", "schlan\u00b7ke", "Kna\u00b7be", ",", "der", "eilt", "her\u00b7zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihn fa\u00dft der Greis mit Zittern,", "tokens": ["Ihn", "fa\u00dft", "der", "Greis", "mit", "Zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbja,\u00ab ruft er, \u00bbSommer im Norden ward's,", "tokens": ["\u00bb", "ja", ",", "\u00ab", "ruft", "er", ",", "\u00bb", "Som\u00b7mer", "im", "Nor\u00b7den", "ward's", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "NN", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich horche den Ungewittern!", "tokens": ["Ich", "hor\u00b7che", "den", "Un\u00b7ge\u00b7wit\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.23": {"line.1": {"text": "Weh mir, es sprosset ihm schon der Bart,", "tokens": ["Weh", "mir", ",", "es", "spros\u00b7set", "ihm", "schon", "der", "Bart", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+--++-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Es schwellen die Glieder, die Knochen,", "tokens": ["Es", "schwel\u00b7len", "die", "Glie\u00b7der", ",", "die", "Kno\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Er ist ein Mann geworden und hat", "tokens": ["Er", "ist", "ein", "Mann", "ge\u00b7wor\u00b7den", "und", "hat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VAPP", "KON", "VAFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Den Vater noch nicht gerochen! \u2013", "tokens": ["Den", "Va\u00b7ter", "noch", "nicht", "ge\u00b7ro\u00b7chen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "VVPP", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.24": {"line.1": {"text": "Blutrache, heilig, alt Gesetz,", "tokens": ["Blu\u00b7tra\u00b7che", ",", "hei\u00b7lig", ",", "alt", "Ge\u00b7setz", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie unsre G\u00f6tter und Eichen,", "tokens": ["Wie", "uns\u00b7re", "G\u00f6t\u00b7ter", "und", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vor dir mu\u00df unsers Hauses Fried'", "tokens": ["Vor", "dir", "mu\u00df", "un\u00b7sers", "Hau\u00b7ses", "Fried'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Liebe mir heut erbleichen!", "tokens": ["Und", "Lie\u00b7be", "mir", "heut", 