], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wie silberfarb flittern", "tokens": ["Wie", "sil\u00b7ber\u00b7farb", "flit\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VVFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die Wiesen, wie zittern", "tokens": ["Die", "Wie\u00b7sen", ",", "wie", "zit\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Tausend Sonnen im perlenden Tau!", "tokens": ["Tau\u00b7send", "Son\u00b7nen", "im", "per\u00b7len\u00b7den", "Tau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "In s\u00e4uselnder K\u00fchle", "tokens": ["In", "s\u00e4u\u00b7seln\u00b7der", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Beginnen die Spiele", "tokens": ["Be\u00b7gin\u00b7nen", "die", "Spie\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der jungen Natur,", "tokens": ["Der", "jun\u00b7gen", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die Zephire kosen", "tokens": ["Die", "Ze\u00b7phi\u00b7re", "ko\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und schmeicheln um Rosen,", "tokens": ["Und", "schmei\u00b7cheln", "um", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und D\u00fcfte bestr\u00f6men die lachende Flur.", "tokens": ["Und", "D\u00fcf\u00b7te", "be\u00b7str\u00f6\u00b7men", "die", "la\u00b7chen\u00b7de", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Wie hoch aus den St\u00e4dten die Rauchwolken dampfen,", "tokens": ["Wie", "hoch", "aus", "den", "St\u00e4d\u00b7ten", "die", "Rauch\u00b7wol\u00b7ken", "damp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Laut wiehern und schnauben und knirschen und strampfen", "tokens": ["Laut", "wie\u00b7hern", "und", "schnau\u00b7ben", "und", "knir\u00b7schen", "und", "stramp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die Rosse, die Farren,", "tokens": ["Die", "Ros\u00b7se", ",", "die", "Far\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Wagen erknarren", "tokens": ["Die", "Wa\u00b7gen", "er\u00b7knar\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ins \u00e4chzende Tal.", "tokens": ["Ins", "\u00e4ch\u00b7zen\u00b7de", "Tal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Die Waldungen leben", "tokens": ["Die", "Wal\u00b7dun\u00b7gen", "le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und Adler und Falken und Habichte schweben,", "tokens": ["Und", "Ad\u00b7ler", "und", "Fal\u00b7ken", "und", "Ha\u00b7bich\u00b7te", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und wiegen die Fl\u00fcgel im blendenden Strahl.", "tokens": ["Und", "wie\u00b7gen", "die", "Fl\u00fc\u00b7gel", "im", "blen\u00b7den\u00b7den", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Den Frieden zu finden,", "tokens": ["Den", "Frie\u00b7den", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wohin soll ich wenden", "tokens": ["Wo\u00b7hin", "soll", "ich", "wen\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Am elenden Stab?", "tokens": ["Am", "e\u00b7len\u00b7den", "Stab", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die lachende Erde", "tokens": ["Die", "la\u00b7chen\u00b7de", "Er\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Mit J\u00fcnglingsgeb\u00e4rde", "tokens": ["Mit", "J\u00fcng\u00b7lings\u00b7ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "F\u00fcr mich nur ein Grab!", "tokens": ["F\u00fcr", "mich", "nur", "ein", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Steig empor, o Morgenrot, und r\u00f6te", "tokens": ["Steig", "em\u00b7por", ",", "o", "Mor\u00b7gen\u00b7rot", ",", "und", "r\u00f6\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "FM", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit purpurnem Kusse Hain und Feld.", "tokens": ["Mit", "pur\u00b7pur\u00b7nem", "Kus\u00b7se", "Hain", "und", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "S\u00e4usle nieder, Abendrot, und fl\u00f6te", "tokens": ["S\u00e4us\u00b7le", "nie\u00b7der", ",", "A\u00b7ben\u00b7drot", ",", "und", "fl\u00f6\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sanft in Schlummer die erstorbne Welt.", "tokens": ["Sanft", "in", "Schlum\u00b7mer", "die", "ers\u00b7torb\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", 