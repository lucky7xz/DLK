{"textgrid.poem.26683": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Morgenphantasie", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frisch atmet des Morgens lebendiger Hauch,", "tokens": ["Frisch", "at\u00b7met", "des", "Mor\u00b7gens", "le\u00b7ben\u00b7di\u00b7ger", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Purpurisch zuckt durch d\u00fcstre Tannenritzen", "tokens": ["Pur\u00b7pu\u00b7risch", "zuckt", "durch", "d\u00fcst\u00b7re", "Tan\u00b7nen\u00b7rit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Das junge Licht und \u00e4ugelt aus dem Strauch,", "tokens": ["Das", "jun\u00b7ge", "Licht", "und", "\u00e4u\u00b7gelt", "aus", "dem", "Strauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In goldnen Flammen blitzen", "tokens": ["In", "gold\u00b7nen", "Flam\u00b7men", "blit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Berge Wolkenspitzen,", "tokens": ["Der", "Ber\u00b7ge", "Wol\u00b7ken\u00b7spit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit freudig melodisch gewirbeltem Lied", "tokens": ["Mit", "freu\u00b7dig", "me\u00b7lo\u00b7di