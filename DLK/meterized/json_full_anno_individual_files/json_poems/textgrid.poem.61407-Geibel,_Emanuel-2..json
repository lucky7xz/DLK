{"textgrid.poem.61407": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00dfes vermag der Verstand, er ersinnt und bildet und ordnet,", "tokens": ["Gro\u00b7\u00dfes", "ver\u00b7mag", "der", "Ver\u00b7stand", ",", "er", "er\u00b7sinnt", "und", "bil\u00b7det", "und", "ord\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "PPER", "VVPP", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Aber das Kunstwerk schweigt, aber die Ordnung ist tot.", "tokens": ["A\u00b7ber", "das", "Kunst\u00b7werk", "schweigt", ",", "a\u00b7ber", "die", "Ord\u00b7nung", "ist", "tot", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "ADV", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Prangt auch hehr das Gebild' in der Glieder entz\u00fcckendem Gleichma\u00df:", "tokens": ["Prangt", "auch", "hehr", "das", "Ge\u00b7bild'", "in", "der", "Glie\u00b7der", "ent\u00b7z\u00fc\u00b7cken\u00b7dem", "Gleich\u00b7ma\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Nimmer vom Marmorgestell springt es err\u00f6tend herab,", "tokens": ["Nim\u00b7mer", "vom", "Mar\u00b7mor\u00b7ge\u00b7stell", "springt", "es", "er\u00b7r\u00f6\u00b7tend", "her\u00b7ab", ","], "token_info": ["word", "word", "word",