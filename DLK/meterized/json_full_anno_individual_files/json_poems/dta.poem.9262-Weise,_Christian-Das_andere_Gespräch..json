{"dta.poem.9262": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das andere Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Die lufft ver\u00e4ndert nichts/ die klugheit mu\u00df in reisen", "tokens": ["Die", "lufft", "ver\u00b7\u00e4n\u00b7dert", "nichts", "/", "die", "klug\u00b7heit", "mu\u00df", "in", "rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "$(", "ART", "NN", "VMFIN", "APPR", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Compa\u00df und ruder seyn/ sie mu\u00df den anfurt weisen/", "tokens": ["Com\u00b7pa\u00df", "und", "ru\u00b7der", "seyn", "/", "sie", "mu\u00df", "den", "an\u00b7furt", "wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAINF", "$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie mu\u00df die rechnung thun/ und welcher das vergist/", "tokens": ["Sie", "mu\u00df", "die", "rech\u00b7nung", "thun", "/", "und", "wel\u00b7cher", "das", "ver\u00b7gist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$(", "KON", "PRELS", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der k\u00f6mmet wieder heim wie er gewesen ist.", "tokens": ["Der", "k\u00f6m\u00b7met", "wie\u00b7der", "heim", "wie", "er", "ge\u00b7we\u00b7sen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKVZ", "KOKOM", "PPER", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es ist ein schlechtes werck sich etwas tieffer b\u00fccken/", "tokens": ["Es", "ist", "ein", "schlech\u00b7tes", "werck", "sich", "et\u00b7was", "tief\u00b7fer", "b\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PRF", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der alten sprache brauch mit neuen w\u00f6rtern sticken/", "tokens": ["Der", "al\u00b7ten", "spra\u00b7che", "brauch", "mit", "neu\u00b7en", "w\u00f6r\u00b7tern", "sti\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In fremb der kleidung gehn: und wer sich in der welt", "tokens": ["In", "fremb", "der", "klei\u00b7dung", "gehn", ":", "und", "wer", "sich", "in", "der", "welt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "VVINF", "$.", "KON", "PWS", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um sonsten nichs bem\u00fcht/ verspielet zeit und geld.", "tokens": ["Um", "sons\u00b7ten", "nichs", "be\u00b7m\u00fcht", "/", "ver\u00b7spie\u00b7let", "zeit", "und", "geld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "PIS", "VVFIN", "$(", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wem ist damit gedient/ und wenn man haar-klein w\u00fcste/", "tokens": ["Wem", "ist", "da\u00b7mit", "ge\u00b7dient", "/", "und", "wenn", "man", "haa\u00b7rklein", "w\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PAV", "VVPP", "$(", "KON", "KOUS", "PIS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie scharff ein ander trinckt/ wie mancher seine l\u00fcste/", "tokens": ["Wie", "scharff", "ein", "an\u00b7der", "trinckt", "/", "wie", "man\u00b7cher", "sei\u00b7ne", "l\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJD", "VVFIN", "$(", "KOKOM", "PIAT", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit b\u00f6sem wasser l\u00f6scht/ wie einer schertzt und spielt/", "tokens": ["Mit", "b\u00f6\u00b7sem", "was\u00b7ser", "l\u00f6scht", "/", "wie", "ei\u00b7ner", "schertzt", "und", "spielt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$(", "PWAV", "PIS", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie jener springt und tantzt/ wie dieser raubt und stielt.", "tokens": ["Wie", "je\u00b7ner", "springt", "und", "tantzt", "/", "wie", "die\u00b7ser", "raubt", "und", "stielt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "KON", "VVFIN", "$(", "KOKOM", "PDS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es ist nur eitelkeit/ man h\u00f6rt von solchen dingen/", "tokens": ["Es", "ist", "nur", "ei\u00b7tel\u00b7keit", "/", "man", "h\u00f6rt", "von", "sol\u00b7chen", "din\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$(", "PIS", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lernt sie endlich selbst in die gewohnheit bringen", "tokens": ["Und", "lernt", "sie", "end\u00b7lich", "selbst", "in", "die", "ge\u00b7wohn\u00b7heit", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So da\u00df ein solcher mensch viel gr\u00f6sser unheil stifft", "tokens": ["So", "da\u00df", "ein", "sol\u00b7cher", "mensch", "viel", "gr\u00f6s\u00b7ser", "un\u00b7heil", "stifft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "PIAT", "NN", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als welcher einen brunn verderbet und vergifft.", "tokens": ["Als", "wel\u00b7cher", "ei\u00b7nen", "brunn", "ver\u00b7der\u00b7bet", "und", "ver\u00b7gifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein kluges kauffmans-schiff k\u00f6mmt nimmermehr gefahren/", "tokens": ["Ein", "klu\u00b7ges", "kauff\u00b7mans\u00b7schiff", "k\u00f6mmt", "nim\u00b7mer\u00b7mehr", "ge\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sucht Arabien um liederliche waaren:", "tokens": ["Und", "sucht", "A\u00b7ra\u00b7bi\u00b7en", "um", "lie\u00b7der\u00b7li\u00b7che", "waa\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "ADJA", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Das gold wird au\u00dfgesucht/ der weirauch zugericht/", "tokens": ["Das", "gold", "wird", "au\u00df\u00b7ge\u00b7sucht", "/", "der", "wei\u00b7rauch", "zu\u00b7ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die myrrhen eingepackt: den sand begehrt man nicht.", "tokens": ["Die", "myr\u00b7rhen", "ein\u00b7ge\u00b7packt", ":", "den", "sand", "be\u00b7gehrt", "man", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "ART", "NN", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wolan du junges volck! weil ihr von eurem leben", "tokens": ["Wo\u00b7lan", "du", "jun\u00b7ges", "volck", "!", "weil", "ihr", "von", "eu\u00b7rem", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADJA", "NN", "$.", "KOUS", "PPER", "APPR", "PPOSAT", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem hohen alter sollt einmahl die zinse geben.", "tokens": ["Dem", "ho\u00b7hen", "al\u00b7ter", "sollt", "ein\u00b7mahl", "die", "zin\u00b7se", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So reiset mit vernunfft/ und nehmet di\u00df in acht/", "tokens": ["So", "rei\u00b7set", "mit", "ver\u00b7nunfft", "/", "und", "neh\u00b7met", "di\u00df", "in", "acht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$(", "KON", "VVFIN", "PDS", "APPR", "CARD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was von der klugheit kom\u0303t/ un\u0303 euch zu menschen macht.", "tokens": ["Was", "von", "der", "klug\u00b7heit", "kom\u0303t", "/", "u\u00f1", "euch", "zu", "men\u00b7schen", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$(", "KON", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}