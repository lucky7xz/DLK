{"textgrid.poem.41282": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Trostgr\u00fcne", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Sophron, nichts geschieht vergebens.", "tokens": ["Mein", "Sop\u00b7hron", ",", "nichts", "ge\u00b7schieht", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PIS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Uns witziget, uns \u00fcbt die Widerw\u00e4rtigkeit", "tokens": ["Uns", "wit\u00b7zi\u00b7get", ",", "uns", "\u00fcbt", "die", "Wi\u00b7der\u00b7w\u00e4r\u00b7tig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Pr\u00fcfungsstande dieses Lebens.", "tokens": ["Im", "Pr\u00fc\u00b7fungs\u00b7stan\u00b7de", "die\u00b7ses", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seele siegt nicht ohne Streit.", "tokens": ["Die", "See\u00b7le", "siegt", "nicht", "oh\u00b7ne", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn wir auch nicht den Sieg erwerben;", "tokens": ["Wenn", "wir", "auch", "nicht", "den", "Sieg", "er\u00b7wer\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So hat dennoch das Ungl\u00fcck seinen Werth,", "tokens": ["So", "hat", "den\u00b7noch", "das", "Un\u00b7gl\u00fcck", "sei\u00b7nen", "Werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Weil es die gr\u00f6\u00dfte Kunst uns lehrt:", "tokens": ["Weil", "es", "die", "gr\u00f6\u00df\u00b7te", "Kunst", "uns", "lehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die, Gl\u00fccklichen so schwere, Kunst zu sterben.", "tokens": ["Die", ",", "Gl\u00fcck\u00b7li\u00b7chen", "so", "schwe\u00b7re", ",", "Kunst", "zu", "ster\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "NN", "ADV", "ADJA", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Sophron, nichts geschieht vergebens.", "tokens": ["Mein", "Sop\u00b7hron", ",", "nichts", "ge\u00b7schieht", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PIS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Uns witziget, uns \u00fcbt die Widerw\u00e4rtigkeit", "tokens": ["Uns", "wit\u00b7zi\u00b7get", ",", "uns", "\u00fcbt", "die", "Wi\u00b7der\u00b7w\u00e4r\u00b7tig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Pr\u00fcfungsstande dieses Lebens.", "tokens": 