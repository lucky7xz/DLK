{"dta.poem.16671": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "30.  Zufall und Wesen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch werde wesentlich: denn wann die Welt vergeht/", "tokens": ["Mensch", "wer\u00b7de", "we\u00b7sent\u00b7lich", ":", "denn", "wann", "die", "Welt", "ver\u00b7geht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$.", "KON", "PWAV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So f\u00e4llt der Zufall weg/ das wesen das besteht.", "tokens": ["So", "f\u00e4llt", "der", "Zu\u00b7fall", "weg", "/", "das", "we\u00b7sen", "das", "be\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$(", "PDS", "VVFIN", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}