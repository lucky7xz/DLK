{"dta.poem.8775": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Am Grabe Peter Ulrich Kernell's.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Um der Jugend Rest betrogen,", "tokens": ["Um", "der", "Ju\u00b7gend", "Rest", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lasset uns den Freund bestatten,", "tokens": ["Las\u00b7set", "uns", "den", "Freund", "be\u00b7stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den wir, wenn auch fern erzogen,", "tokens": ["Den", "wir", ",", "wenn", "auch", "fern", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "KOUS", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieb, wie einen Bruder, hatten.", "tokens": ["Lieb", ",", "wie", "ei\u00b7nen", "Bru\u00b7der", ",", "hat\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "$,", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, es lockten heim'sche Bande,", "tokens": ["Ach", ",", "es", "lock\u00b7ten", "heim'\u00b7sche", "Ban\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lockten aus Hesperiens Eden,", "tokens": ["Lock\u00b7ten", "aus", "Hes\u00b7pe\u00b7ri\u00b7ens", "E\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Vom erhabnen Tiberstrande,", "tokens": ["Vom", "er\u00b7hab\u00b7nen", "Ti\u00b7ber\u00b7stran\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wieder ihn in's theure Schweden,", "tokens": ["Wie\u00b7der", "ihn", "in's", "theu\u00b7re", "Schwe\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "ADJA", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nach dem frommen Vaterlande!", "tokens": ["Nach", "dem", "from\u00b7men", "Va\u00b7ter\u00b7lan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber, eilendes Verderben,", "tokens": ["A\u00b7ber", ",", "ei\u00b7len\u00b7des", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du verg\u00f6nntest nicht dem Armen,", "tokens": ["Du", "ver\u00b7g\u00f6nn\u00b7test", "nicht", "dem", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um das lezte Gl\u00fcck zu werben,", "tokens": ["Um", "das", "lez\u00b7te", "Gl\u00fcck", "zu", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den schwesterlichen Armen,", "tokens": ["In", "den", "schwes\u00b7ter\u00b7li\u00b7chen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An der Mutter Brust zu sterben!", "tokens": ["An", "der", "Mut\u00b7ter", "Brust", "zu", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schauernd in der Morgenstunde,", "tokens": ["Schau\u00b7ernd", "in", "der", "Mor\u00b7gen\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bey dem Schalle fremder Glocken,", "tokens": ["Bey", "dem", "Schal\u00b7le", "frem\u00b7der", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Senken hier wir ihn zu Grunde,", "tokens": ["Sen\u00b7ken", "hier", "wir", "ihn", "zu", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Senden, ach! nur wen'ge Locken", "tokens": ["Sen\u00b7den", ",", "ach", "!", "nur", "wen'\u00b7ge", "Lo\u00b7cken"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ITJ", "$.", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nach dem allzufernen Sunde.", "tokens": ["Nach", "dem", "all\u00b7zu\u00b7fer\u00b7nen", "Sun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bess'res l\u00e4\u00dft sich nichts gew\u00e4hren", "tokens": ["Bess'\u00b7res", "l\u00e4\u00dft", "sich", "nichts", "ge\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jenen, die so viel ertragen:", "tokens": ["Je\u00b7nen", ",", "die", "so", "viel", "er\u00b7tra\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Sehnsucht quillt in Z\u00e4hren,", "tokens": ["Ih\u00b7re", "Sehn\u00b7sucht", "quillt", "in", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwillt in Seufzern, st\u00fcrmt in Klagen,", "tokens": ["Schwillt", "in", "Seuf\u00b7zern", ",", "st\u00fcrmt", "in", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die sich ewig neu geb\u00e4ren!", "tokens": ["Die", "sich", "e\u00b7wig", "neu", "ge\u00b7b\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Eh' der Lenz dir Frist gegeben,", "tokens": ["Eh'", "der", "Lenz", "dir", "Frist", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lie\u00df, o Freund, dein allzukarges", "tokens": ["Lie\u00df", ",", "o", "Freund", ",", "dein", "all\u00b7zu\u00b7kar\u00b7ges"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "FM", "NN", "$,", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebensloos dich uns entschweben,", "tokens": ["Le\u00b7bens\u00b7loos", "dich", "uns", "ent\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den Deckel deines Sarges", "tokens": ["Und", "den", "De\u00b7ckel", "dei\u00b7nes", "Sar\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zieren Rosen ohne Leben.", "tokens": ["Zie\u00b7ren", "Ro\u00b7sen", "oh\u00b7ne", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "O wie zog es dich nach jenen", "tokens": ["O", "wie", "zog", "es", "dich", "nach", "je\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "PRF", "APPR", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tagen hin, wo laue Winde", "tokens": ["Ta\u00b7gen", "hin", ",", "wo", "lau\u00b7e", "Win\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weichgepflaumte Fl\u00fcgel dehnen!", "tokens": ["Weich\u00b7ge\u00b7pf\u00b7laum\u00b7te", "Fl\u00fc\u00b7gel", "deh\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Nach der ersten Knospenrinde", "tokens": ["Nach", "der", "ers\u00b7ten", "Knos\u00b7pen\u00b7rin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lockte dich dein leztes Sehnen!", "tokens": ["Lock\u00b7te", "dich", "dein", "lez\u00b7tes", "Seh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Noch bey seinem mattern Pochen", "tokens": ["Noch", "bey", "sei\u00b7nem", "mat\u00b7tern", "Po\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat vielleicht das Herz des Kranken,", "tokens": ["Hat", "viel\u00b7leicht", "das", "Herz", "des", "Kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh' der starre Blick gebrochen,", "tokens": ["Eh'", "der", "star\u00b7re", "Blick", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unaussprechliche Gedanken", "tokens": ["Un\u00b7aus\u00b7sprech\u00b7li\u00b7che", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit den Seinen still gesprochen!", "tokens": ["Mit", "den", "Sei\u00b7nen", "still", "ge\u00b7spro\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSS", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Diese Lieben zu ermuthen,", "tokens": ["Die\u00b7se", "Lie\u00b7ben", "zu", "er\u00b7mu\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00e4uselt aus dem Schoos der Gr\u00fcfte", "tokens": ["S\u00e4u\u00b7selt", "aus", "dem", "Schoos", "der", "Gr\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch ein Lebewohl des Guten:", "tokens": ["Noch", "ein", "Le\u00b7be\u00b7wohl", "des", "Gu\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Haschet es, ihr Fr\u00fchlingsl\u00fcfte,", "tokens": ["Ha\u00b7schet", "es", ",", "ihr", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tragt es \u00fcber Land und Fluthen!", "tokens": ["Tragt", "es", "\u00fc\u00b7ber", "Land", "und", "Flut\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}