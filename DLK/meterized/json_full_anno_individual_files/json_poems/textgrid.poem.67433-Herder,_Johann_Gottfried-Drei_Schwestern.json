{"textgrid.poem.67433": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Drei Schwestern", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoffnung und Liebe sind des Lebens fr\u00f6hliche Schwestern;", "tokens": ["Hoff\u00b7nung", "und", "Lie\u00b7be", "sind", "des", "Le\u00b7bens", "fr\u00f6h\u00b7li\u00b7che", "Schwes\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Jene flieget voran, Diese regieret den Flug,", "tokens": ["Je\u00b7ne", "flie\u00b7get", "vo\u00b7ran", ",", "Die\u00b7se", "re\u00b7gie\u00b7ret", "den", "Flug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$,", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Tr\u00e4gt auf ihren Schwingen und weht der leidenden Seele", "tokens": ["Tr\u00e4gt", "auf", "ih\u00b7ren", "Schwin\u00b7gen", "und", "weht", "der", "lei\u00b7den\u00b7den", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "K\u00fchlenden Athem zu, hebt und erquicket sie sanft.", "tokens": ["K\u00fch\u00b7len\u00b7den", "A\u00b7them", "zu", ",", "hebt", "und", "er\u00b7quic\u00b7ket", "sie", "sanft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$,", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Untrennbare! verla\u00dft mich nimmer, Ihr lieblichen Schwestern!", "tokens": ["Un\u00b7trenn\u00b7ba\u00b7re", "!", "ver\u00b7la\u00dft", "mich", "nim\u00b7mer", ",", "Ihr", "lieb\u00b7li\u00b7chen", "Schwes\u00b7tern", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ohne die Hoffnung sind Leben und Liebe dahin.", "tokens": ["Oh\u00b7ne", "die", "Hoff\u00b7nung", "sind", "Le\u00b7ben", "und", "Lie\u00b7be", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "NN", "KON", "NN", "PAV", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}, "stanza.2": {"line.1": {"text": "Hoffnung und Liebe sind des Lebens fr\u00f6hliche Schwestern;", "tokens": ["Hoff\u00b7nung", "und", "Lie\u00b7be", "sind", "des", "Le\u00b7bens", "fr\u00f6h\u00b7li\u00b7che", "Schwes\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Jene flieget voran, Diese regieret den Flug,", "tokens": ["Je\u00b7ne", "flie\u00b7get", "vo\u00b7ran", ",", "Die\u00b7se", "re\u00b7gie\u00b7ret", "den", "Flug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$,", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Tr\u00e4gt auf ihren Schwingen und weht der leidenden Seele", "tokens": ["Tr\u00e4gt", "auf", "ih\u00b7ren", "Schwin\u00b7gen", "und", "weht", "der", "lei\u00b7den\u00b7den", "See\u00b7le"]