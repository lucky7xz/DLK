{"textgrid.poem.60244": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einzige Tochter des sorgenden Paares,", "genre": "verse", "period": "N.A.", "pub_year": 1756, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einzige Tochter des sorgenden Paares,", "tokens": ["Ein\u00b7zi\u00b7ge", "Toch\u00b7ter", "des", "sor\u00b7gen\u00b7den", "Paa\u00b7res", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Welches dich n\u00e4hret, lehret und sch\u00fctzt,", "tokens": ["Wel\u00b7ches", "dich", "n\u00e4h\u00b7ret", ",", "leh\u00b7ret", "und", "sch\u00fctzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und in dir ein s\u00fc\u00dfes, wahres", "tokens": ["Und", "in", "dir", "ein", "s\u00fc\u00b7\u00dfes", ",", "wah\u00b7res"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "PPER", "ART", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wiedergeschenktes Vergn\u00fcgen besitzt \u2013", "tokens": ["Wie\u00b7der\u00b7ge\u00b7schenk\u00b7tes", "Ver\u00b7gn\u00fc\u00b7gen", "be\u00b7sitzt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Danke mit deinem zuk\u00fcnftigen Leben,", "tokens": ["Dan\u00b7ke", "mit", "dei\u00b7nem", "zu\u00b7k\u00fcnf\u00b7ti\u00b7gen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Lobe mit jeder Empfindung die Macht", "tokens": ["Lo\u00b7be", "mit", "je\u00b7der", "Emp\u00b7fin\u00b7dung", "die", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Welche dem Tode Befehle gegeben,", "tokens": ["Wel\u00b7che", "dem", "To\u00b7de", "Be\u00b7feh\u00b7le", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Nicht mit ewiger Nacht", "tokens": ["Nicht", "mit", "e\u00b7wi\u00b7ger", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Dein schon zitterndes Auge zu decken.", "tokens": ["Dein", "schon", "zit\u00b7tern\u00b7des", "Au\u00b7ge", "zu", "de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+--", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Ach, dein keuchender Busen empfand", "tokens": ["Ach", ",", "dein", "keu\u00b7chen\u00b7der", "Bu\u00b7sen", "emp\u00b7fand"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Schon des Grabes gewaltige Schrecken,", "tokens": ["Schon", "des", "Gra\u00b7bes", "ge\u00b7wal\u00b7ti\u00b7ge", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Und gen Himmel gewandt", "tokens": ["Und", "gen", "Him\u00b7mel", "ge\u00b7wandt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVPP"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "War das Auge der Mutter, mit Thr\u00e4nen", "tokens": ["War", "das", "Au\u00b7ge", "der", "Mut\u00b7ter", ",", "mit", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,", "APPR", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Ueber und \u00fcber benetzt.", "tokens": ["Ue\u00b7ber", "und", "\u00fc\u00b7ber", "be\u00b7netzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "APPR", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.15": {"text": "Durch dein winselndes St\u00f6hnen", "tokens": ["Durch", "dein", "win\u00b7seln\u00b7des", "St\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.16": {"text": "Ward ihr liebendes Herze verletzt.", "tokens": ["Ward", "ihr", "lie\u00b7ben\u00b7des", "Her\u00b7ze", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.17": {"text": "Mit ihr weinte, von Kummer durchdrungen,", "tokens": ["Mit", "ihr", "wein\u00b7te", ",", "von", "Kum\u00b7mer", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Auch der minder weichliche Mann,", "tokens": ["Auch", "der", "min\u00b7der", "weich\u00b7li\u00b7che", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.19": {"text": "Den noch keiner zu Thr\u00e4nen gezwungen,", "tokens": ["Den", "noch", "kei\u00b7ner", "zu", "Thr\u00e4\u00b7nen", "ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "APPR", "NN", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.20": {"text": "Der sonst jedem Ungl\u00fcck trotzen kann,", "tokens": ["Der", "sonst", "je\u00b7dem", "Un\u00b7gl\u00fcck", "trot\u00b7zen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Wann es ihn bedrohen sollte \u2013", "tokens": ["Wann", "es", "ihn", "be\u00b7dro\u00b7hen", "soll\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Diesem bef\u00fcrchtenden Vater entfiel", "tokens": ["Die\u00b7sem", "be\u00b7f\u00fcrch\u00b7ten\u00b7den", "Va\u00b7ter", "ent\u00b7fiel"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "NE"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.23": {"text": "Aller Muth, wenn er dich tr\u00f6sten wollte,", "tokens": ["Al\u00b7ler", "Muth", ",", "wenn", "er", "dich", "tr\u00f6s\u00b7ten", "woll\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Denn er sahe dich am Ziel", "tokens": ["Denn", "er", "sa\u00b7he", "dich", "am", "Ziel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Deines kaum begonnenen Lebens:", "tokens": ["Dei\u00b7nes", "kaum", "be\u00b7gon\u00b7ne\u00b7nen", "Le\u00b7bens", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.26": {"text": "Sein Gedanke wiederholte tausendmal:", "tokens": ["Sein", "Ge\u00b7dan\u00b7ke", "wie\u00b7der\u00b7hol\u00b7te", "tau\u00b7send\u00b7mal", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.27": {"text": "Armes M\u00e4dchen, ach! du strebst vergebens", "tokens": ["Ar\u00b7mes", "M\u00e4d\u00b7chen", ",", "ach", "!", "du", "strebst", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ITJ", "$.", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Hier zu bleiben, deiner Jahre Zahl", "tokens": ["Hier", "zu", "blei\u00b7ben", ",", "dei\u00b7ner", "Jah\u00b7re", "Zahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.29": {"text": "Ist vollendet, wie die Zahl der Monden", "tokens": ["Ist", "voll\u00b7en\u00b7det", ",", "wie", "die", "Zahl", "der", "Mon\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Von dem jugendlichbl\u00fchenden Stahl,", "tokens": ["Von", "dem", "ju\u00b7gend\u00b7lich\u00b7bl\u00fc\u00b7hen\u00b7den", "Stahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.31": {"text": "Dem die Grazien zu Londen", "tokens": ["Dem", "die", "Gra\u00b7zi\u00b7en", "zu", "Lon\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Rosen auf sein Grab gestreut,", "tokens": ["Ro\u00b7sen", "auf", "sein", "Grab", "ge\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Und dabey voll Mitleid sangen:", "tokens": ["Und", "da\u00b7bey", "voll", "Mit\u00b7leid", "san\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADJD", "NN", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.34": {"text": "Deutscher J\u00fcngling, deine Redlichkeit", "tokens": ["Deut\u00b7scher", "J\u00fcng\u00b7ling", ",", "dei\u00b7ne", "Red\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.35": {"text": "L\u00e4chelte von deinen Wangen", "tokens": ["L\u00e4\u00b7chel\u00b7te", "von", "dei\u00b7nen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Und von offener Stirne herab,", "tokens": ["Und", "von", "of\u00b7fe\u00b7ner", "Stir\u00b7ne", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.37": {"text": "Und der Mann, der sie dir erblich gab,", "tokens": ["Und", "der", "Mann", ",", "der", "sie", "dir", "er\u00b7blich", "gab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.38": {"text": "Wird umsonst nach dir verlangen,", "tokens": ["Wird", "um\u00b7sonst", "nach", "dir", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Ruft umsonst den einzigen Sohn,", "tokens": ["Ruft", "um\u00b7sonst", "den", "ein\u00b7zi\u00b7gen", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.40": {"text": "Den die b\u00f6se Krankheit weggerissen,", "tokens": ["Den", "die", "b\u00f6\u00b7se", "Krank\u00b7heit", "weg\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.41": {"text": "Welcher vom br\u00e4utlichen Lager und Thron", "tokens": ["Wel\u00b7cher", "vom", "br\u00e4ut\u00b7li\u00b7chen", "La\u00b7ger", "und", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "APPRART", "ADJA", "NN", "KON", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.42": {"text": "Oft die F\u00fcrstenkinder folgen m\u00fcssen.", "tokens": ["Oft", "die", "F\u00fcrs\u00b7ten\u00b7kin\u00b7der", "fol\u00b7gen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.43": {"text": "Fremdes Erdreich deckt dein sch\u00f6nes Haupt,", "tokens": ["Frem\u00b7des", "Er\u00b7dreich", "deckt", "dein", "sch\u00f6\u00b7nes", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.44": {"text": "Deinen Schwestern, deinen Spielgesellen", "tokens": ["Dei\u00b7nen", "Schwes\u00b7tern", ",", "dei\u00b7nen", "Spiel\u00b7ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.45": {"text": "Ist nichts weiter vom Schicksal erlaubt,", "tokens": ["Ist", "nichts", "wei\u00b7ter", "vom", "Schick\u00b7sal", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.46": {"text": "Als im Geist sich um die Gruft zu stellen,", "tokens": ["Als", "im", "Geist", "sich", "um", "die", "Gruft", "zu", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.47": {"text": "Die dich, Blume der Jugend, geraubt.", "tokens": ["Die", "dich", ",", "Blu\u00b7me", "der", "Ju\u00b7gend", ",", "ge\u00b7raubt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "NN", "ART", "NN", "$,", "VVPP", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Einzige Tochter des sorgenden Paares,", "tokens": ["Ein\u00b7zi\u00b7ge", "Toch\u00b7ter", "des", "sor\u00b7gen\u00b7den", "Paa\u00b7res", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$,"], "me