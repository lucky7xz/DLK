{"textgrid.poem.31645": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "17. Herr wie lang wiltu mein so gar vergessen", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ged\u00e4chtnus! ist es dir auch m\u00fcglich zu vergessen?", "tokens": ["Ge\u00b7d\u00e4cht\u00b7nus", "!", "ist", "es", "dir", "auch", "m\u00fcg\u00b7lich", "zu", "ver\u00b7ges\u00b7sen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ja gar? und also lang? ach denk' an deine G\u00fct.", "tokens": ["ja", "gar", "?", "und", "al\u00b7so", "lang", "?", "ach", "denk'", "an", "dei\u00b7ne", "G\u00fct", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "KON", "ADV", "ADJD", "$.", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bistu doch eine Krafft im G\u00f6ttlichen Gem\u00fct.", "tokens": ["Bis\u00b7tu", "doch", "ei\u00b7ne", "Krafft", "im", "G\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7m\u00fct", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Solt einen Fehler man dem h\u00f6chsten Gut zumessen.", "tokens": ["Solt", "ei\u00b7nen", "Feh\u00b7ler", "man", "dem", "h\u00f6chs\u00b7ten", "Gut", "zu\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", 