{"textgrid.poem.52360": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr Kreihenbom up Holtenhagen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Kreihenbom up Holtenhagen,", "tokens": ["Herr", "Krei\u00b7hen\u00b7bom", "up", "Hol\u00b7ten\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "De r\u00f6ppt sick Korl Smidten 'ran", "tokens": ["De", "r\u00f6ppt", "sick", "Korl", "Smid\u00b7ten", "'ran"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "NE", "NE", "NE"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Un seggt: \u00bbKorl, in den negsten Dagen,", "tokens": ["Un", "seggt", ":", "\u00bb", "Korl", ",", "in", "den", "negs\u00b7ten", "Da\u00b7gen", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "$(", "NE", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Denn fang wi an tau decken an,", "tokens": ["Denn", "fang", "wi", "an", "tau", "de\u00b7cken", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "APPR", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Weitst du woll nich kein Deckelweden?\u00ab", "tokens": ["Weitst", "du", "woll", "nich", "kein", "De\u00b7ckel\u00b7we\u00b7den", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "VMFIN", "PTKNEG", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Korl was bekannt bi all un jeden", "tokens": ["Korl", "was", "be\u00b7kannt", "bi", "all", "un", "je\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWS", "ADJD", "APPR", "PIAT", "FM", "PIAT"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "As so'ne Ort von Holtscheni;", "tokens": ["As", "so'\u00b7ne", "Ort", "von", "Holt\u00b7sche\u00b7ni", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hei arbeit't woll, doch je nah dem", "tokens": ["Hei", "ar\u00b7beit't", "woll", ",", "doch", "je", "nah", "dem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "$,", "ADV", "ADV", "ADJD", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Bedrew hei en Gesch\u00e4ft dorbi", "tokens": ["Be\u00b7drew", "hei", "en", "Ge\u00b7sch\u00e4ft", "dor\u00b7bi"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Mit Hakenkr\u00fcmmel, Ledderb\u00f6m,", "tokens": ["Mit", "Ha\u00b7ken\u00b7kr\u00fcm\u00b7mel", ",", "Led\u00b7der\u00b7b\u00f6m", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mit Bandst\u00f6ck un mit Bohnensch\u00e4cht,", "tokens": ["Mit", "Band\u00b7st\u00f6ck", "un", "mit", "Boh\u00b7nen\u00b7sch\u00e4cht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "FM", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Un w\u00fcrd'n in Fr\u00fchjohr Arwten leggt,", "tokens": ["Un", "w\u00fcrd'n", "in", "Fr\u00fch\u00b7johr", "Arw\u00b7ten", "leggt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Denn k\u00fcnn de Utsat nich gelingen,", "tokens": ["Denn", "k\u00fcnn", "de", "Ut\u00b7sat", "nich", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "NE", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Korl m\u00fc\u00dft de Arwtstr\u00fck d\u00f6rtau bringen.", "tokens": ["Korl", "m\u00fc\u00dft", "de", "A\u00b7rwtstr\u00fck", "d\u00f6r\u00b7tau", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "NE", "NE", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "\u00bbje\u00ab, seggt hei, \u00bbHerr, von Deckelweden,", "tokens": ["\u00bb", "je", "\u00ab", ",", "seggt", "hei", ",", "\u00bb", "Herr", ",", "von", "De\u00b7ckel\u00b7we\u00b7den", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "$(", "$,", "VVFIN", "PTKVZ", "$,", "$(", "NN", "$,", "APPR", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Dor is hir 'r\u00fcm nich vel tau reden,", "tokens": ["Dor", "is", "hir", "'r\u00fcm", "nich", "vel", "tau", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.17": {"text": "Up Ehren Fell'n, dor stahn jo kein.\u00ab", "tokens": ["Up", "Eh\u00b7ren", "Fell'n", ",", "dor", "stahn", "jo", "kein", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "NE", "$,", "ADV", "VVFIN", "NE", "PIAT", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "\u00bbdat weit ick\u00ab, seggt Herr Kreihenbom,", "tokens": ["\u00bb", "dat", "weit", "ick", "\u00ab", ",", "seggt", "Herr", "Krei\u00b7hen\u00b7bom", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJD", "PPER", "$(", "$,", "VVFIN", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "\u00bbdoch heww ick nilig fluggs weck seihn", "tokens": ["\u00bb", "doch", "heww", "ick", "ni\u00b7lig", "fluggs", "weck", "seihn"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Tau Golchen, l\u00e4ngs den Wischensom,", "tokens": ["Tau", "Gol\u00b7chen", ",", "l\u00e4ngs", "den", "Wi\u00b7schen\u00b7som", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Du weitst woll, bi den Oberf\u00f6rster \u2013", "tokens": ["Du", "weitst", "woll", ",", "bi", "den", "O\u00b7ber\u00b7f\u00f6rs\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Doch, Korl, h\u00fcrst du, ick segg dat man,", "tokens": ["Doch", ",", "Korl", ",", "h\u00fcrst", "du", ",", "ick", "segg", "dat", "man", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NE", "$,", "VVFIN", "PPER", "$,", "FM.nl", "FM.nl", "FM.nl", "PIS", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.23": {"text": "Wat gell'n uns fr\u00f6mde Weden an!", "tokens": ["Wat", "gell'n", "uns", "fr\u00f6m\u00b7de", "We\u00b7den", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Ick gew f\u00f6r't Dusend di s\u00f6s Gr\u00f6schen,", "tokens": ["Ick", "gew", "f\u00f6r't", "Du\u00b7send", "di", "s\u00f6s", "Gr\u00f6\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.25": {"text": "Un's Winters kannst du bi mi d\u00f6schen.", "tokens": ["Un's", "Win\u00b7ters", "kannst", "du", "bi", "mi", "d\u00f6\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "NE", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Na, seih doch mal so'n beten r\u00fcm.\u00ab", "tokens": ["Na", ",", "seih", "doch", "mal", "so'n", "be\u00b7ten", "r\u00fcm", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "VVIMP", "ADV", "ADV", "ADV", "VVFIN", "NE", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.27": {"text": "\u00bbje\u00ab, seggt Korl Smidt, \u00bbdat is woll slimm\u00ab,", "tokens": ["\u00bb", "je", "\u00ab", ",", "seggt", "Korl", "Smidt", ",", "\u00bb", "dat", "is", "woll", "slimm", "\u00ab", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$(", "$,", "VVFIN", "NE", "NE", "$,", "$(", "ART", "FM", "ADV", "ADJD", "$(", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "Un geiht. Inde\u00df den annern Morgen", "tokens": ["Un", "geiht", ".", "In\u00b7de\u00df", "den", "an\u00b7nern", "Mor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Ward hei en Dusend ran besorgen.", "tokens": ["Ward", "hei", "en", "Du\u00b7send", "ran", "be\u00b7sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "FM", "FM", "FM", "FM", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Na, sch\u00f6n! Herr Kreihenbom, de seggt:", "tokens": ["Na", ",", "sch\u00f6n", "!", "Herr", "Krei\u00b7hen\u00b7bom", ",", "de", "seggt", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJD", "$.", "NN", "NE", "$,", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "\u00bbnimm man din Wedenbund un legg't", "tokens": ["\u00bb", "nimm", "man", "din", "We\u00b7den\u00b7bund", "un", "leg\u00b7g't"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.32": {"text": "Herinne in dat Wagenschur,", "tokens": ["He\u00b7rin\u00b7ne", "in", "dat", "Wa\u00b7gen\u00b7schur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Un h\u00fcrst du, Korl, dat't keiner s\u00fcht!\u00ab", "tokens": ["Un", "h\u00fcrst", "du", ",", "Korl", ",", "dat't", "kei\u00b7ner", "s\u00fcht", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NE", "$,", "VVFIN", "PIS", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "So geiht dat nu 'ne ganze Tid.", "tokens": ["So", "geiht", "dat", "nu", "'ne", "gan\u00b7ze", "Tid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "All Morgens dr\u00f6ggt herin in't Dur", "tokens": ["All", "Mor\u00b7gens", "dr\u00f6ggt", "he\u00b7rin", "in't", "Dur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADV", "VVFIN", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Uns' Korl en frisches Bund un stellt", "tokens": ["Un\u00b7s'", "Korl", "en", "fri\u00b7sches", "Bund", "un", "stellt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "ADJA", "NN", "FM", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.37": {"text": "Dat rinne in dat Wagenschur;", "tokens": ["Dat", "rin\u00b7ne", "in", "dat", "Wa\u00b7gen\u00b7schur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "All Morgens kriggt hei ok sin Geld.", "tokens": ["All", "Mor\u00b7gens", "kriggt", "hei", "ok", "sin", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADV", "VVFIN", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "\u00bbnu is dat naug! Nu k\u00e4n wi decken,", "tokens": ["\u00bb", "nu", "is", "dat", "naug", "!", "Nu", "k\u00e4n", "wi", "de\u00b7cken", ","], "token_info": ["punct", "word", "word", "word", "word", "p