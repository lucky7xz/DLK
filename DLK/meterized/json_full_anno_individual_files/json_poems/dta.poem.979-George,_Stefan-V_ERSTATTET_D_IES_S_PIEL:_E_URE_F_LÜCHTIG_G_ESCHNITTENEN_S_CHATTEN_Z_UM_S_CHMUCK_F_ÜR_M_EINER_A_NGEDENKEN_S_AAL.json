{"dta.poem.979": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "V ERSTATTET  D IES  S PIEL:  E URE  F L\u00dcCHTIG  \n  G ESCHNITTENEN  S CHATTEN  Z UM  S CHMUCK  \n  F \u00dcR  M EINER  A NGEDENKEN  S AAL", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Angenehm flossen bei dir unsre n&#228;chtlichen stunden               ", "tokens": ["An\u00b7ge\u00b7nehm", "flos\u00b7sen", "bei", "dir", "uns\u00b7re", "n", "&#228;", "cht\u00b7li\u00b7chen", "stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "PPER", "PPOSAT", "XY", "$(", "ADJA", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Dass wir der ampel vergassen doch dir zum gewinn nicht &#183;               ", "tokens": ["Dass", "wir", "der", "am\u00b7pel", "ver\u00b7gas\u00b7sen", "doch", "dir", "zum", "ge\u00b7winn", "nicht", "&#183;"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "XML_entity"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "KON", "PPER", "APPRART", "NN", "PTKNEG", "$("], "meter": "---+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Trieb dich verblendung mit misslicher wende zu reden               ", "tokens": ["Trieb", "dich", "ver\u00b7blen\u00b7dung", "mit", "miss\u00b7li\u00b7cher", "wen\u00b7de", "zu", "re\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+--+--+--+-+--", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Was mir zu h\u00f6ren nicht noch zu erwidern verg\u00f6nnt ist?               ", "tokens": ["Was", "mir", "zu", "h\u00f6\u00b7ren", "nicht", "noch", "zu", "er\u00b7wi\u00b7dern", "ver\u00b7g\u00f6nnt", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKZU", "VVINF", "PTKNEG", "ADV", "PTKZU", "VVINF", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Kannst du bed&#228;chtige sprache nicht weiter erfinden               ", "tokens": ["Kannst", "du", "bed", "&#228;", "ch\u00b7ti\u00b7ge", "spra\u00b7che", "nicht", "wei\u00b7ter", "er\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "$(", "ADJA", "VVFIN", "PTKNEG", "ADV", "VVINF"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.6": {"text": "Meide mich! so nicht mein schmerzlich erstaunen dich zwinge               ", "tokens": ["Mei\u00b7de", "mich", "!", "so", "nicht", "mein", "schmerz\u00b7lich", "er\u00b7stau\u00b7nen", "dich", "zwin\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "ADV", "PTKNEG", "PPOSAT", "ADJD", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Lenke die eigne verachtung ob m&#252;ssigen werbens               ", "tokens": ["Len\u00b7ke", "die", "eig\u00b7ne", "ver\u00b7ach\u00b7tung", "ob", "m", "&#252;", "ssi\u00b7gen", "wer\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "KOUS", "NE", "$(", "ADJA", "NN"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.8": {"text": "Und die gel&#228;chter von deiner zerknitterten seele.             ", "tokens": ["Und", "die", "gel", "&#228;", "ch\u00b7ter", "von", "dei\u00b7ner", "zer\u00b7knit\u00b7ter\u00b7ten", "see\u00b7le", "."], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}}}}}