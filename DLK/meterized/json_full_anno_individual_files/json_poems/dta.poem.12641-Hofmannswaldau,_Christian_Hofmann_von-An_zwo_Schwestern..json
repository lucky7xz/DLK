{"dta.poem.12641": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An zwo Schwestern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein ungewaschen mund soll unsern thau begr\u00fc\u00dfen?", "tokens": ["Ein", "un\u00b7ge\u00b7wa\u00b7schen", "mund", "soll", "un\u00b7sern", "thau", "be\u00b7gr\u00fc\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der frevel mu\u00df gewi\u00df nicht sonder strafe seyn:", "tokens": ["Der", "fre\u00b7vel", "mu\u00df", "ge\u00b7wi\u00df", "nicht", "son\u00b7der", "stra\u00b7fe", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PTKNEG", "ADV", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein fleck-und kleck-papier soll lilgen-h\u00e4nde k\u00fcssen?", "tokens": ["Ein", "fleck\u00b7\u00b7und", "kleck\u00b7pa\u00b7pier", "soll", "lil\u00b7gen\u00b7h\u00e4n\u00b7de", "k\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Di\u00df ist vor g\u00f6ttinnen gewi\u00df was ungemein.", "tokens": ["Di\u00df", "ist", "vor", "g\u00f6t\u00b7tin\u00b7nen", "ge\u00b7wi\u00df", "was", "un\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "ADV", "PWS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der reichthum unsrer gunst steht, sclave! dir verschlossen,", "tokens": ["Der", "reicht\u00b7hum", "uns\u00b7rer", "gunst", "steht", ",", "scla\u00b7ve", "!", "dir", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "$.", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vor solche, gleich wie du, sind unsre perlen nicht:", "tokens": ["Vor", "sol\u00b7che", ",", "gleich", "wie", "du", ",", "sind", "uns\u00b7re", "per\u00b7len", "nicht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "ADV", "KOKOM", "PPER", "$,", "VAFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der g\u00f6tter alicant wird nicht dahin gegossen,", "tokens": ["Der", "g\u00f6t\u00b7ter", "a\u00b7li\u00b7cant", "wird", "nicht", "da\u00b7hin", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo den gef\u00e4\u00dfen glantz und kostbar ertzt gebricht.", "tokens": ["Wo", "den", "ge\u00b7f\u00e4\u00b7\u00dfen", "glantz", "und", "kost\u00b7bar", "ertzt", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "KON", "ADJD", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dein eisen reimt sich nicht vor unsre zug-magneten,", "tokens": ["Dein", "ei\u00b7sen", "reimt", "sich", "nicht", "vor", "uns\u00b7re", "zug\u00b7ma\u00b7gne\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PRF", "PTKNEG", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wir ziehen nur den sirahl der g\u00f6ttlichkeit uns zu,", "tokens": ["Wir", "zie\u00b7hen", "nur", "den", "si\u00b7rahl", "der", "g\u00f6tt\u00b7lich\u00b7keit", "uns", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Wer nicht durch tugend kan die groben sitten t\u00f6den,", "tokens": ["Wer", "nicht", "durch", "tu\u00b7gend", "kan", "die", "gro\u00b7ben", "sit\u00b7ten", "t\u00f6\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "APPR", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und f\u00fcrstlich uns bedient, hat vor uns gute ruh;", "tokens": ["Und", "f\u00fcrst\u00b7lich", "uns", "be\u00b7dient", ",", "hat", "vor", "uns", "gu\u00b7te", "ruh", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVPP", "$,", "VAFIN", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und du, du kahler knecht! der arm von allen sch\u00e4tzen,", "tokens": ["Und", "du", ",", "du", "kah\u00b7ler", "knecht", "!", "der", "arm", "von", "al\u00b7len", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "ADJA", "NN", "$.", "ART", "ADJD", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die sonsten die natur den andern beygelegt,", "tokens": ["Die", "sons\u00b7ten", "die", "na\u00b7tur", "den", "an\u00b7dern", "bey\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "ART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du darffst dich unterstehn den fu\u00df zu uns zu setzen,", "tokens": ["Du", "darffst", "dich", "un\u00b7ter\u00b7stehn", "den", "fu\u00df", "zu", "uns", "zu", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ART", "PTKVZ", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wo sch\u00f6nheit und verstand zusammen sind gepr\u00e4gt?", "tokens": ["Wo", "sch\u00f6n\u00b7heit", "und", "ver\u00b7stand", "zu\u00b7sam\u00b7men", "sind", "ge\u00b7pr\u00e4gt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "VVFIN", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Kein bl\u00f6des auge kan der strahlen gluth vertragen,", "tokens": ["Kein", "bl\u00f6\u00b7des", "au\u00b7ge", "kan", "der", "strah\u00b7len", "gluth", "ver\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ein ungemeiner blitz verblendet das gesicht,", "tokens": ["Ein", "un\u00b7ge\u00b7mei\u00b7ner", "blitz", "ver\u00b7blen\u00b7det", "das", "ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PDS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und deiner reime wachs will sich zur sonnen wagen?", "tokens": ["Und", "dei\u00b7ner", "rei\u00b7me", "wachs", "will", "sich", "zur", "son\u00b7nen", "wa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "PIS", "VMFIN", "PRF", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Erwehle dir davor ein dunckel schwefel-licht.", "tokens": ["Er\u00b7weh\u00b7le", "dir", "da\u00b7vor", "ein", "dun\u00b7ckel", "schwe\u00b7fel\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So f\u00fchret ihr vielleicht, ihr edlen Venus-t\u00f6chter!", "tokens": ["So", "f\u00fch\u00b7ret", "ihr", "viel\u00b7leicht", ",", "ihr", "ed\u00b7len", "Ve\u00b7nus\u00b7t\u00f6ch\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Aus rechter eifersucht mich und mein schreiben an;", "tokens": ["Aus", "rech\u00b7ter", "ei\u00b7fer\u00b7sucht", "mich", "und", "mein", "schrei\u00b7ben", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "KON", "PPOSAT", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Doch eure demuth macht das z\u00f6rnen zum gel\u00e4chter,", "tokens": ["Doch", "eu\u00b7re", "de\u00b7muth", "macht", "das", "z\u00f6r\u00b7nen", "zum", "ge\u00b7l\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und ist deu th\u00e4lern so, wie bergen, zugethan.", "tokens": ["Und", "ist", "deu", "th\u00e4\u00b7lern", "so", ",", "wie", "ber\u00b7gen", ",", "zu\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "VVFIN", "ADV", "$,", "PWAV", "VVPP", "$,", "ADJD", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Der hochmuth ist euch gifft, die hoffart lauter galle,", "tokens": ["Der", "hoch\u00b7muth", "ist", "euch", "gifft", ",", "die", "hof\u00b7fart", "lau\u00b7ter", "gal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Jhr seyd, wie g\u00f6tter thun, dem ehr-geitz schlangen feind,", "tokens": ["Ihr", "seyd", ",", "wie", "g\u00f6t\u00b7ter", "thun", ",", "dem", "ehr\u00b7geitz", "schlan\u00b7gen", "feind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "ADJD", "VVINF", "$,", "PRELS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Doch also, da\u00df euch auch zuweilen wohlgefalle,", "tokens": ["Doch", "al\u00b7so", ",", "da\u00df", "euch", "auch", "zu\u00b7wei\u00b7len", "wohl\u00b7ge\u00b7fal\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wenn man euch sonne nennt, die vor dem p\u00f6bel scheint.", "tokens": ["Wenn", "man", "euch", "son\u00b7ne", "nennt", ",", "die", "vor", "dem", "p\u00f6\u00b7bel", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und das ist r\u00fchmenswerth, wer selbst nichts von sich halten,", "tokens": ["Und", "das", "ist", "r\u00fch\u00b7mens\u00b7werth", ",", "wer", "selbst", "nichts", "von", "sich", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADJD", "$,", "PWS", "ADV", "PIS", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Nicht den verdienten prei\u00df der tugend achten will,", "tokens": ["Nicht", "den", "ver\u00b7dien\u00b7ten", "prei\u00df", "der", "tu\u00b7gend", "ach\u00b7ten", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Mu\u00df endlich unterm ei\u00df gemeiner leut erkalten:", "tokens": ["Mu\u00df", "end\u00b7lich", "un\u00b7term", "ei\u00df", "ge\u00b7mei\u00b7ner", "leut", "er\u00b7kal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sind waaren wo gemein, da gelten sie nicht viel.", "tokens": ["Sind", "waa\u00b7ren", "wo", "ge\u00b7mein", ",", "da", "gel\u00b7ten", "sie", "nicht", "viel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "PWAV", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich unterstehe mich, gesteh ich, grosser sachen,", "tokens": ["Ich", "un\u00b7ter\u00b7ste\u00b7he", "mich", ",", "ge\u00b7steh", "ich", ",", "gros\u00b7ser", "sa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Indem mein schlechter brief, der kein gew\u00fcrtze kennt,", "tokens": ["In\u00b7dem", "mein", "schlech\u00b7ter", "brief", ",", "der", "kein", "ge\u00b7w\u00fcrt\u00b7ze", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und den kein balsam-fach noch rosen riechend machen,", "tokens": ["Und", "den", "kein", "bal\u00b7sam\u00b7fach", "noch", "ro\u00b7sen", "rie\u00b7chend", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "ADV", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Blind hin, unangesagt auf eure tafel rennt.", "tokens": ["Blind", "hin", ",", "un\u00b7an\u00b7ge\u00b7sagt", "auf", "eu\u00b7re", "ta\u00b7fel", "rennt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.37": {"text": "Jhr aber habt ihm selbst den zaum der gunst gegeben,", "tokens": ["Ihr", "a\u00b7ber", "habt", "ihm", "selbst", "den", "zaum", "der", "gunst", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Durch eure freundlichkeit wird er so keck gef\u00fchrt,", "tokens": ["Durch", "eu\u00b7re", "freund\u00b7lich\u00b7keit", "wird", "er", "so", "keck", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.39": {"text": "Der gnaden-fl\u00fcgel paar kan ihn vom staub erheben,", "tokens": ["Der", "gna\u00b7den\u00b7fl\u00fc\u00b7gel", "paar", "kan", "ihn", "vom", "staub", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Durch eurer sch\u00f6nheit farb ist er genug geziert;", "tokens": ["Durch", "eu\u00b7rer", "sch\u00f6n\u00b7heit", "farb", "ist", "er", "ge\u00b7nug", "ge\u00b7ziert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Drauf halt ich unverzagt huld und auch schuld zusammen,", "tokens": ["Drauf", "halt", "ich", "un\u00b7ver\u00b7zagt", "huld", "und", "auch", "schuld", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "NN", "KON", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.42": {"text": "Dem feuer-gla\u00df ist die, der sonnen jene gleich:", "tokens": ["Dem", "feu\u00b7er\u00b7gla\u00df", "ist", "die", ",", "der", "son\u00b7nen", "je\u00b7ne", "gleich", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "$,", "PRELS", "ADV", "PDS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Die schuld f\u00e4ngt von der huld die mitgetheilten flammen,", "tokens": ["Die", "schuld", "f\u00e4ngt", "von", "der", "huld", "die", "mit\u00b7ge\u00b7theil\u00b7ten", "flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Wird wie Vefuvius und Aetna feuer-reich.", "tokens": ["Wird", "wie", "Ve\u00b7fu\u00b7vius", "und", "A\u00b7et\u00b7na", "feu\u00b7er\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NE", "KON", "NE", "ADJD", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.45": {"text": "Hier z\u00fcndet nun der danck auf eurem zucht-altare,", "tokens": ["Hier", "z\u00fcn\u00b7det", "nun", "der", "danck", "auf", "eu\u00b7rem", "zucht\u00b7al\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Das opffer freudig an, so von dem hertzen k\u00f6mmt,", "tokens": ["Das", "opf\u00b7fer", "freu\u00b7dig", "an", ",", "so", "von", "dem", "hert\u00b7zen", "k\u00f6mmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Er braucht zu solchem fest kein kraut noch r\u00e4ucher-waare,", "tokens": ["Er", "braucht", "zu", "sol\u00b7chem", "fest", "kein", "kraut", "noch", "r\u00e4u\u00b7cher\u00b7waa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "ADJD", "PIAT", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Sein weyrauch ist ein wunsch der f\u00fcr euch nymfen glimmt.", "tokens": ["Sein", "wey\u00b7rauch", "ist", "ein", "wunsch", "der", "f\u00fcr", "euch", "nym\u00b7fen", "glimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJD", "ART", "APPR", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Theil, himmel! meinen rauch in hundert tausend st\u00fccke,", "tokens": ["Theil", ",", "him\u00b7mel", "!", "mei\u00b7nen", "rauch", "in", "hun\u00b7dert", "tau\u00b7send", "st\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "PPOSAT", "NN", "APPR", "CARD", "CARD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Nimm durch gekl\u00e4rte lufft auch jedes st\u00e4ubgen an,", "tokens": ["Nimm", "durch", "ge\u00b7kl\u00e4r\u00b7te", "lufft", "auch", "je\u00b7des", "st\u00e4ub\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Schick aber gl\u00fcckes-blick ohn alle t\u00fcck zur\u00fccke,", "tokens": ["Schick", "a\u00b7ber", "gl\u00fc\u00b7ckes\u00b7blick", "ohn", "al\u00b7le", "t\u00fcck", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADJD", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Und schwing um diese zwo die rothe freuden-fahn.", "tokens": ["Und", "schwing", "um", "die\u00b7se", "zwo", "die", "ro\u00b7the", "freu\u00b7den\u00b7fahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "CARD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "La\u00df fix-und irre-stern: Es leben beyde! ruffen,", "tokens": ["La\u00df", "fix\u00b7\u00b7und", "ir\u00b7re\u00b7stern", ":", "Es", "le\u00b7ben", "bey\u00b7de", "!", "ruf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "$.", "PPER", "VVFIN", "PIS", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Wirff ihnen ro\u00dfmarin und bisam-blumen zu,", "tokens": ["Wirff", "ih\u00b7nen", "ro\u00df\u00b7ma\u00b7rin", "und", "bi\u00b7sam\u00b7blu\u00b7men", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Lust sey der beyden kost, vergn\u00fcgung beyder hoffen,", "tokens": ["Lust", "sey", "der", "bey\u00b7den", "kost", ",", "ver\u00b7gn\u00fc\u00b7gung", "bey\u00b7der", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "PIAT", "NN", "$,", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Und ihrer sch\u00e4tze schatz, o himmel! bleibe du!", "tokens": ["Und", "ih\u00b7rer", "sch\u00e4t\u00b7ze", "schatz", ",", "o", "him\u00b7mel", "!", "blei\u00b7be", "du", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,", "FM", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Mein opffer ist verricht, die asch ist noch zu sehen,", "tokens": ["Mein", "opf\u00b7fer", "ist", "ver\u00b7richt", ",", "die", "asch", "ist", "noch", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "PRELS", "ADJD", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Daraus des wunsches ziel als ph\u00f6nix werden soll,", "tokens": ["Da\u00b7raus", "des", "wun\u00b7sches", "ziel", "als", "ph\u00f6\u00b7nix", "wer\u00b7den", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "KOKOM", "ADJD", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Ich wei\u00df, GOtt selbst wird geist und athen in ihn wehen;", "tokens": ["Ich", "wei\u00df", ",", "Gott", "selbst", "wird", "geist", "und", "at\u00b7hen", "in", "ihn", "we\u00b7hen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "ADV", "VAFIN", "VVPP", "KON", "VVFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Wo so der wunsch bekleibt, so lebt, ihr schwestern, wohl.", "tokens": ["Wo", "so", "der", "wunsch", "be\u00b7kleibt", ",", "so", "lebt", ",", "ihr", "schwes\u00b7tern", ",", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJD", "VVFIN", "$,", "ADV", "VVFIN", "$,", "PPER", "VVINF", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}