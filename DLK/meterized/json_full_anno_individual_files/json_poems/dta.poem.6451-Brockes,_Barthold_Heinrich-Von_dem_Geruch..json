{"dta.poem.6451": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Theilchen, so sich immer regen,", "tokens": ["Die", "Theil\u00b7chen", ",", "so", "sich", "im\u00b7mer", "re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PRF", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und die dasjenige, was riecht, formiren,", "tokens": ["Und", "die", "das\u00b7je\u00b7ni\u00b7ge", ",", "was", "riecht", ",", "for\u00b7mi\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "PWS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wird man verschiedentlich versp\u00fcren,", "tokens": ["Wird", "man", "ver\u00b7schie\u00b7dent\u00b7lich", "ver\u00b7sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nachdem das Werckzeug selbst geschickt sich zu bewegen.", "tokens": ["Nach\u00b7dem", "das", "Wer\u00b7ck\u00b7zeug", "selbst", "ge\u00b7schickt", "sich", "zu", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}