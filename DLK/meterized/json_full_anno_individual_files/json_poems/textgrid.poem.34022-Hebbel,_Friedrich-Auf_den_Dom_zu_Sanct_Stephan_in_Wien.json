{"textgrid.poem.34022": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Dom zu Sanct Stephan in Wien", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Altehrw\u00fcrd'ges Symbol der wahren Einheit und Eintracht,", "tokens": ["Alt\u00b7ehr\u00b7w\u00fcrd'\u00b7ges", "Sym\u00b7bol", "der", "wah\u00b7ren", "Ein\u00b7heit", "und", "Ein\u00b7tracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+++", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Welch ein gewaltiges Bild stellst du mir hin vor den Geist!", "tokens": ["Welch", "ein", "ge\u00b7wal\u00b7ti\u00b7ges", "Bild", "stellst", "du", "mir", "hin", "vor", "den", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "VVFIN", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--++-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Mehr erhebt es mich fast, dich werden zu seh'n in Gedanken,", "tokens": ["Mehr", "er\u00b7hebt", "es", "mich", "fast", ",", "dich", "wer\u00b7den", "zu", "seh'n", "in", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$,", "PPER", "VAINF", "PTKZU", "VVINF", "APPR", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Als mich, ges\u00e4ttigten Blicks, deiner Vollendung zu freu'n.", "tokens": ["Als", "mich", ",", "ge\u00b7s\u00e4t\u00b7tig\u00b7ten", "Blicks", ",", "dei\u00b7ner", "Vol\u00b7len\u00b7dung", "zu", "freu'", "n."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPER", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "APPR", "NE", "NE"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Welch ein harmonisches Leben! Welch fr\u00f6hlicher Austausch der Kr\u00e4fte!", "tokens": ["Welch", "ein", "har\u00b7mo\u00b7ni\u00b7sches", "Le\u00b7ben", "!", "Welch", "fr\u00f6h\u00b7li\u00b7cher", "Aus\u00b7tausch", "der", "Kr\u00e4f\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$.", "PIAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "----+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und von Geschlecht zu Geschlecht schlingt sich das heilige Band.", "tokens": ["Und", "von", "Ge\u00b7schlecht", "zu", "Ge\u00b7schlecht", "schlingt", "sich", "das", "hei\u00b7li\u00b7ge", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Kaum entfaltet der Meister, des Genius irdischer Herold,", "tokens": ["Kaum", "ent\u00b7fal\u00b7tet", "der", "Meis\u00b7ter", ",", "des", "Ge\u00b7nius", "ir\u00b7di\u00b7scher", "He\u00b7rold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Fromm und begeistert den Plan, als sich auch Alles ihm beugt:", "tokens": ["Fromm", "und", "be\u00b7geis\u00b7tert", "den", "Plan", ",", "als", "sich", "auch", "Al\u00b7les", "ihm", "beugt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "ART", "NN", "$,", "KOUS", "PRF", "ADV", "PIS", "PPER", "VVFIN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "Nicht das Handwerk blo\u00df, das nur durch's Dienen sich adelt,", "tokens": ["Nicht", "das", "Hand\u00b7werk", "blo\u00df", ",", "das", "nur", "durch's", "Die\u00b7nen", "sich", "a\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADV", "$,", "PRELS", "ADV", "APPRART", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Auch die stolzere Kunst horcht nur auf seinen Befehl.", "tokens": ["Auch", "die", "stol\u00b7ze\u00b7re", "Kunst", "horcht", "nur", "auf", "sei\u00b7nen", "Be\u00b7fehl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Einer greift zur Kelle, der And're zum Mei\u00dfel, und freudig", "tokens": ["Ei\u00b7ner", "greift", "zur", "Kel\u00b7le", ",", "der", "An\u00b7d'\u00b7re", "zum", "Mei\u00b7\u00dfel", ",", "und", "freu\u00b7dig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,", "KON", "ADJD"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "F\u00e4ngt nun Jeglicher an, was er nicht endigen soll.", "tokens": ["F\u00e4ngt", "nun", "Jeg\u00b7li\u00b7cher", "an", ",", "was", "er", "nicht", "en\u00b7di\u00b7gen", "soll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "PTKVZ", "$,", "PW