{"textgrid.poem.26066": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Leben hei\u00dft Sehnsucht verehren", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber den leeren m\u00e4chtigen B\u00e4umen", "tokens": ["\u00dc\u00b7ber", "den", "lee\u00b7ren", "m\u00e4ch\u00b7ti\u00b7gen", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "H\u00e4ngen die schm\u00e4chtigen Sterne,", "tokens": ["H\u00e4n\u00b7gen", "die", "schm\u00e4ch\u00b7ti\u00b7gen", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Umdr\u00e4ngen den Mond im Kreise.", "tokens": ["Um\u00b7dr\u00e4n\u00b7gen", "den", "Mond", "im", "Krei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sehns\u00fcchte leben auch in den pr\u00e4chtigen Himmelsr\u00e4umen,", "tokens": ["Sehn\u00b7s\u00fcch\u00b7te", "le\u00b7ben", "auch", "in", "den", "pr\u00e4ch\u00b7ti\u00b7gen", "Him\u00b7mels\u00b7r\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und auch Gestirne kommen aus ihrem Geleise.", "tokens": ["Und", "auch", "Ge\u00b7stir\u00b7ne", "kom\u00b7men", "aus", "ih\u00b7rem", "Ge\u00b7lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Keine Sonne, kein Stern kann sich der Sehnsucht erwehren,", "tokens": ["Kei\u00b7ne", "Son\u00b7ne", ",", "kein", "Stern", "kann", "sich", "der", "Sehn\u00b7sucht", "er\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Alle Leben leiden und lachen auf gleiche Weise.", "tokens": ["Al\u00b7le", "Le\u00b7ben", "lei\u00b7den", "und", "la\u00b7chen", "auf", "glei\u00b7che", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Leben hei\u00dft Sehnsucht verehren;", "tokens": ["Le\u00b7ben", "hei\u00dft", "Sehn\u00b7sucht", "ver\u00b7eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Niemals der Tod, die Geliebte allein kann dir Ruhe bescheren.", "tokens": ["Nie\u00b7mals", "der", "Tod", ",", "die", "Ge\u00b7lieb\u00b7te", "al\u00b7lein", "kann", "dir", "Ru\u00b7he", "be\u00b7sche\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "ADV", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}}, "stanza.2": {"line.1": {"text": "\u00dcber den leeren m\u00e4chtigen B\u00e4umen", "tokens": ["\u00dc\u00b7ber", "den", "lee\u00b7ren", "m\u00e4ch\u00b7ti\u00b7gen", "B\u00e4u\u00b7men"