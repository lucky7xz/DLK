{"textgrid.poem.41154": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies Bild von einem Hunnenk\u00f6nig", "tokens": ["Dies", "Bild", "von", "ei\u00b7nem", "Hun\u00b7nen\u00b7k\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kam aus der Tinte heut heraus,", "tokens": ["Kam", "aus", "der", "Tin\u00b7te", "heut", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gebetet hat der Alte wenig,", "tokens": ["Ge\u00b7be\u00b7tet", "hat", "der", "Al\u00b7te", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jedoch ver\u00fcbt manch argen Graus;", "tokens": ["Je\u00b7doch", "ver\u00b7\u00fcbt", "manch", "ar\u00b7gen", "Graus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer ihm nicht ganz war untert\u00e4nig,", "tokens": ["Wer", "ihm", "nicht", "ganz", "war", "un\u00b7ter\u00b7t\u00e4\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem stach er selbst die Augen aus.", "tokens": ["Dem", "stach", "er", "selbst", "die", "Au\u00b7gen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nun sitzt er in des Hades Schauer,", "tokens": ["Nun", "sitzt", "er", "in", "des", "Ha\u00b7des", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bis seine Herrscherwut gestillt,", "tokens": ["Bis", "sei\u00b7ne", "Herr\u00b7scher\u00b7wut", "ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Aus seinen Augen ihm, o Schauer!", "tokens": ["Aus", "sei\u00b7nen", "Au\u00b7gen", "ihm", ",", "o", "Schau\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "$,", "FM", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ein ganzer Bach von Tr\u00e4nen quillt.", "tokens": ["Ein", "gan\u00b7zer", "Bach", "von", "Tr\u00e4\u00b7nen", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Im Mondenlicht, wann gehn Gespenster,", "tokens": ["Im", "Mon\u00b7den\u00b7licht", ",", "wann", "gehn", "Ge\u00b7spens\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sich malet oft von selbst ans Fenster", "tokens": ["Sich", "ma\u00b7let", "oft", "von", "selbst", "ans", "Fens\u00b7ter"], "token_info": ["word", "word", "word"