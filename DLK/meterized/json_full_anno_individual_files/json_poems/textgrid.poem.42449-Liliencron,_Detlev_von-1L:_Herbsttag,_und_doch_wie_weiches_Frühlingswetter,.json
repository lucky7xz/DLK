{"textgrid.poem.42449": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herbsttag, und doch wie weiches Fr\u00fchlingswetter,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herbsttag, und doch wie weiches Fr\u00fchlingswetter,", "tokens": ["Herbst\u00b7tag", ",", "und", "doch", "wie", "wei\u00b7ches", "Fr\u00fch\u00b7lings\u00b7wet\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADV", "KOKOM", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ich schlenderte langseits der Friedhofshecke,", "tokens": ["Ich", "schlen\u00b7der\u00b7te", "lang\u00b7seits", "der", "Fried\u00b7hofs\u00b7he\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein Sarg schien unter Gramgel\u00e4ut zu sinken,", "tokens": ["Ein", "Sarg", "schien", "un\u00b7ter", "Gram\u00b7ge\u00b7l\u00e4ut", "zu", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann bog ich auf dem Wege um die Ecke.", "tokens": ["Dann", "bog", "ich", "auf", "dem", "We\u00b7ge", "um", "die", "E\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da kamst du, keine T\u00e4uschung, mir entgegen,", "tokens": ["Da", "kamst", "du", ",", "kei\u00b7ne", "T\u00e4u\u00b7schung", ",", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PIAT", "NN", "$,", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir hatten gestern Abschied schon genommen,", "tokens": ["Wir", "hat\u00b7ten", "ge\u00b7stern", "Ab\u00b7schied", "schon", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du gingst zur Bahn, begleitet von Geschwistern,", "tokens": ["Du", "gingst", "zur", "Bahn", ",", "be\u00b7glei\u00b7tet", "von", "Ge\u00b7schwis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was mu\u00dfte noch einmal die Marter kommen.", "tokens": ["Was", "mu\u00df\u00b7te", "noch", "ein\u00b7mal", "die", "Mar\u00b7ter", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich gr\u00fc\u00dfte dich, und sah dein freundlich Danken,", "tokens": ["Ich", "gr\u00fc\u00df\u00b7te", "dich", ",", "und", "sah", "dein", "freund\u00b7lich", "Dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mit dir schritten, haben's nicht beachtet.", "tokens": ["Die", "mit", "dir", "schrit\u00b7ten", ",", "ha\u00b7ben's", "nicht", "be\u00b7ach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Und ich blieb stehn, du wandtest dich verstohlen,", "tokens": ["Und", "ich", "blieb", "stehn", ",", "du", "wand\u00b7test", "dich", "ver\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "VVINF", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Leid war meine Seele dicht umnachtet.", "tokens": ["Von", "Leid", "war", "mei\u00b7ne", "See\u00b7le", "dicht", "um\u00b7nach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Schmerz grub ich die Linke in den Dornbusch,", "tokens": ["Im", "Schmerz", "grub", "ich", "die", "Lin\u00b7ke", "in", "den", "Dorn\u00b7busch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und lie\u00df die Stacheln tief in's Fleisch mir dringen,", "tokens": ["Und", "lie\u00df", "die", "Sta\u00b7cheln", "tief", "in's", "Fleisch", "mir", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein letzter Gru\u00df von dir, von mir \u2