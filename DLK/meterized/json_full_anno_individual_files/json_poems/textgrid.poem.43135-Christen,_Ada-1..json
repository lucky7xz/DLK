{"textgrid.poem.43135": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie mit einem einz'gen Schlag", "tokens": ["Wie", "mit", "ei\u00b7nem", "einz'\u00b7gen", "Schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist die Welt um mich verwandelt,", "tokens": ["Ist", "die", "Welt", "um", "mich", "ver\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4chelnd knixet Tag um Tag,", "tokens": ["L\u00e4\u00b7chelnd", "kni\u00b7xet", "Tag", "um", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was mich einst so schlecht behandelt.", "tokens": ["Was", "mich", "einst", "so", "schlecht", "be\u00b7han\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Heute reichet mir die Hand,", "tokens": ["Heu\u00b7te", "rei\u00b7chet", "mir", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was einst Schm\u00e4hungen gesendet,", "tokens": ["Was", "einst", "Schm\u00e4\u00b7hun\u00b7gen", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Heute naht im Festgewand,", "tokens": ["Heu\u00b7te", "naht", "im", "Fest\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Was sich einst von mir gewendet.", "tokens": ["Was", "sich", "einst", "von", "mir", "ge\u00b7wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Euer Ha\u00df war die Gewalt,", "tokens": ["Eu\u00b7er", "Ha\u00df", "war", "die", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Die mich einst hinausgetrieben \u2013", "tokens": ["Die", "mich", "einst", "hin\u00b7aus\u00b7ge\u00b7trie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Aber unbewegt und kalt", "tokens": ["A\u00b7ber", "un\u00b7be\u00b7wegt", "und", "kalt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "L\u00e4\u00dft mich heute Euer Lieben!", "tokens": ["L\u00e4\u00dft", "mich", "heu\u00b7te", "Eu\u00b7er", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie mit einem einz'gen Schlag", "tokens": ["Wie", "mit", "ei\u00b7nem", "einz'\u00b7gen", "Schlag"], "token_info": ["word