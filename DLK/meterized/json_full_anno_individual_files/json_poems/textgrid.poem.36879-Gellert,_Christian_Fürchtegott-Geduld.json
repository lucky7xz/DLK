{"textgrid.poem.36879": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Geduld", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Herz, o Gott! in Leid und Kreuz geduldig,", "tokens": ["Ein", "Herz", ",", "o", "Gott", "!", "in", "Leid", "und", "Kreuz", "ge\u00b7dul\u00b7dig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM", "NN", "$.", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das bin ich dir und meinem Heile schuldig.", "tokens": ["Das", "bin", "ich", "dir", "und", "mei\u00b7nem", "Hei\u00b7le", "schul\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PPER", "KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df mich die Pflicht, die wir so oft vergessen,", "tokens": ["La\u00df", "mich", "die", "Pflicht", ",", "die", "wir", "so", "oft", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "T\u00e4glich ermessen.", "tokens": ["T\u00e4g\u00b7lich", "er\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Bin ich nicht Staub, wie alle meine V\u00e4ter?", "tokens": ["Bin", "ich", "nicht", "Staub", ",", "wie", "al\u00b7le", "mei\u00b7ne", "V\u00e4\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "$,", "PWAV", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bin ich vor dir, Herr, nicht ein \u00dcbertreter?", "tokens": ["Bin", "ich", "vor", "dir", ",", "Herr", ",", "nicht", "ein", "\u00dc\u00b7bert\u00b7re\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "$,", "NN", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tu ich zu viel, wenn ich die schweren Tage", "tokens": ["Tu", "ich", "zu", "viel", ",", "wenn", "ich", "die", "schwe\u00b7ren", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PTKA", "PIS", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Standhaft ertrage?", "tokens": ["Stand\u00b7haft", "er\u00b7tra\u00b7ge", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wie oft, o Gott! wenn wir das B\u00f6se dulden,", "tokens": ["Wie", "oft", ",", "o", "Gott", "!", "wenn", "wir", "das", "B\u00f6\u00b7se", "dul\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "FM", "NN", "$.", "KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erdulden wir nur unsrer Torheit Schulden,", "tokens": ["E\u00b7rdul\u00b7den", "wir", "nur", "uns\u00b7rer", "Tor\u00b7heit", "Schul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nennen Lohn, den wir verdient bekommen,", "tokens": ["Und", "nen\u00b7nen", "Lohn", ",", "den", "wir", "ver\u00b7dient", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tr\u00fcbsal der Frommen!", "tokens": ["Tr\u00fcb\u00b7sal", "der", "From\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ist D\u00fcrftigkeit, in der die Tr\u00e4gen klagen,", "tokens": ["Ist", "D\u00fcrf\u00b7tig\u00b7keit", ",", "in", "der", "die", "Tr\u00e4\u00b7gen", "kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind Ha\u00df und Pein, die Stolz und Wollust tragen,", "tokens": ["Sind", "Ha\u00df", "und", "Pein", ",", "die", "Stolz", "und", "Wol\u00b7lust", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$,", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Schwelgers Schmerz, des Neids vermi\u00dfte Freuden,", "tokens": ["Des", "Schwel\u00b7gers", "Schmerz", ",", "des", "Neids", "ver\u00b7mi\u00df\u00b7te", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Christliches Leiden?", "tokens": ["Christ\u00b7li\u00b7ches", "Lei\u00b7den", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Ist deren Qual, die deinen Rat verachtet,", "tokens": ["Ist", "de\u00b7ren", "Qual", ",", "die", "dei\u00b7nen", "Rat", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach Gottesfurcht und Glauben nie getrachtet,", "tokens": ["Nach", "Got\u00b7tes\u00b7furcht", "und", "Glau\u00b7ben", "nie", "ge\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und die sich itzt in finstrer Schwermut qu\u00e4len,", "tokens": ["Und", "die", "sich", "itzt", "in", "finst\u00b7rer", "Schwer\u00b7mut", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRF", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Pr\u00fcfung der Seelen?", "tokens": ["Pr\u00fc\u00b7fung", "der", "See\u00b7len", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Doch selbst, o Gott, in Strafen unsrer S\u00fcnden", "tokens": ["Doch", "selbst", ",", "o", "Gott", ",", "in", "Stra\u00b7fen", "uns\u00b7rer", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "FM", "NN", "$,", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "L\u00e4\u00dft du den Weg zu unserm Heil uns finden,", "tokens": ["L\u00e4\u00dft", "du", "den", "Weg", "zu", "un\u00b7serm", "Heil", "uns", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn wir sie uns, die Missetat zu hassen,", "tokens": ["Wenn", "wir", "sie", "uns", ",", "die", "Mis\u00b7se\u00b7tat", "zu", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Z\u00fcchtigen lassen.", "tokens": ["Z\u00fcch\u00b7ti\u00b7gen", "las\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Jag ich nur nach dem Frieden im Gewissen:", "tokens": ["Jag", "ich", "nur", "nach", "dem", "Frie\u00b7den", "im", "Ge\u00b7wis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird alles mir zum Besten dienen m\u00fcssen.", "tokens": ["Wird", "al\u00b7les", "mir", "zum", "Bes\u00b7ten", "die\u00b7nen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPER", "APPRART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du, Herr, regierst, und ewig wirkt dein Wille", "tokens": ["Du", ",", "Herr", ",", "re\u00b7gierst", ",", "und", "e\u00b7wig", "wirkt", "dein", "Wil\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gutes die F\u00fclle.", "tokens": ["Gu\u00b7tes", "die", "F\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Ich bin ein Gast und Pilger auf der Erden,", "tokens": ["Ich", "bin", "ein", "Gast", "und", "Pil\u00b7ger", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht hier, erst dort, dort soll ich gl\u00fccklich werden;", "tokens": ["Nicht", "hier", ",", "erst", "dort", ",", "dort", "soll", "ich", "gl\u00fcck\u00b7lich", "wer\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "ADV", "ADV", "$,", "ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und gegen euch, was sind, ihr ewgen Freuden,", "tokens": ["Und", "ge\u00b7gen", "euch", ",", "was", "sind", ",", "ihr", "ew\u00b7gen", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "PWS", "VAFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dieser Zeit Leiden?", "tokens": ["Die\u00b7ser", "Zeit", "Lei\u00b7den", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Wenn ich nur nicht mein Elend selbst verschulde;", "tokens": ["Wenn", "ich", "nur", "nicht", "mein", "E\u00b7lend", "selbst", "ver\u00b7schul\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich als Mensch, als Christ, hier leid und dulde:", "tokens": ["Wenn", "ich", "als", "Mensch", ",", "als", "Christ", ",", "hier", "leid", "und", "dul\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "$,", "KOUS", "NN", "$,", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So kann ich mich der H\u00fclfe der Erl\u00f6sten", "tokens": ["So", "kann", "ich", "mich", "der", "H\u00fcl\u00b7fe", "der", "Er\u00b7l\u00f6s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sicher getr\u00f6sten.", "tokens": ["Si\u00b7cher", "ge\u00b7tr\u00f6s\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Ich bin ein Mensch, und Leiden m\u00fcssen kr\u00e4nken;", "tokens": ["Ich", "bin", "ein", "Mensch", ",", "und", "Lei\u00b7den", "m\u00fcs\u00b7sen", "kr\u00e4n\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch in der Not an seinen Sch\u00f6pfer denken,", "tokens": ["Doch", "in", "der", "Not", "an", "sei\u00b7nen", "Sch\u00f6p\u00b7fer", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ihm vertraun, dies st\u00e4rket unsre Herzen", "tokens": ["Und", "ihm", "ver\u00b7traun", ",", "dies", "st\u00e4r\u00b7ket", "uns\u00b7re", "Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$,", "PDS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mitten in Schmerzen.", "tokens": ["Mit\u00b7ten", "in", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Schau \u00fcber dich! Wer tr\u00e4gt der Himmel Heere?", "tokens": ["Schau", "\u00fc\u00b7ber", "dich", "!", "Wer", "tr\u00e4gt", "der", "Him\u00b7mel", "Hee\u00b7re", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$.", "PWS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Merk auf! Wer spricht: Bis hieher! zu dem Meere?", "tokens": ["Merk", "auf", "!", "Wer", "spricht", ":", "Bis", "hie\u00b7her", "!", "zu", "dem", "Mee\u00b7re", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "PWS", "VVFIN", "$.", "APPR", "PAV", "$.", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist er nicht auch dein Helfer und Berater,", "tokens": ["Ist", "er", "nicht", "auch", "dein", "Hel\u00b7fer", "und", "Be\u00b7ra\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Ewig dein Vater?", "tokens": ["E\u00b7wig", "dein", "Va\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.12": {"line.1": {"text": "Willst du so viel, als der Allweise, wissen?", "tokens": ["Willst", "du", "so", "viel", ",", "als", "der", "All\u00b7wei\u00b7se", ",", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Itzt wei\u00dft du nicht, warum du leiden m\u00fcssen;", "tokens": ["Itzt", "wei\u00dft", "du", "nicht", ",", "wa\u00b7rum", "du", "lei\u00b7den", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Allein du wirst, was seine Wege waren,", "tokens": ["Al\u00b7lein", "du", "wirst", ",", "was", "sei\u00b7ne", "We\u00b7ge", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nachmals erfahren.", "tokens": ["Nach\u00b7mals", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.13": {"line.1": {"text": "Er z\u00fcchtigt uns, damit wir zu ihm nahen,", "tokens": ["Er", "z\u00fcch\u00b7tigt", "uns", ",", "da\u00b7mit", "wir", "zu", "ihm", "na\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Heiligung des Geistes zu empfahen,", "tokens": ["Die", "Hei\u00b7li\u00b7gung", "des", "Geis\u00b7tes", "zu", "em\u00b7pfa\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mit dem Trost der H\u00fclfe, die wir merken,", "tokens": ["Und", "mit", "dem", "Trost", "der", "H\u00fcl\u00b7fe", ",", "die", "wir", "mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Andre zu st\u00e4rken.", "tokens": ["And\u00b7re", "zu", "st\u00e4r\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.14": {"line.1": {"text": "Das Kreuz des Herrn wirkt Weisheit und Erfahrung;", "tokens": ["Das", "Kreuz", "des", "Herrn", "wirkt", "Weis\u00b7heit", "und", "Er\u00b7fah\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erfahrung gibt dem Glauben Mut und Nahrung.", "tokens": ["Er\u00b7fah\u00b7rung", "gibt", "dem", "Glau\u00b7ben", "Mut", "und", "Nah\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein starkes Herz steht in der Not noch feste.", "tokens": ["Ein", "star\u00b7kes", "Herz", "steht", "in", "der", "Not", "noch", "fes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hoffe das Beste!", "tokens": ["Hof\u00b7fe", "das", "Bes\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.15": {"line.1": {"text": "Ein Herz, o Gott! in Leid und Kreuz geduldig,", "tokens": ["Ein", "Herz", ",", "o", "Gott", "!", "in", "Leid", "und", "Kreuz", "ge\u00b7dul\u00b7dig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM", "NN", "$.", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das bin ich dir und meinem Heile schuldig.", "tokens": ["Das", "bin", "ich", "dir", "und", "mei\u00b7nem", "Hei\u00b7le", "schul\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PPER", "KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df mich die Pflicht, die wir so oft vergessen,", "tokens": ["La\u00df", "mich", "die", "Pflicht", ",", "die", "wir", "so", "oft", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "T\u00e4glich ermessen.", "tokens": ["T\u00e4g\u00b7lich", "er\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.16": {"line.1": {"text": "Bin ich nicht Staub, wie alle meine V\u00e4ter?", "tokens": ["Bin", "ich", "nicht", "Staub", ",", "wie", "al\u00b7le", "mei\u00b7ne", "V\u00e4\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "$,", "PWAV", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bin ich vor dir, Herr, nicht ein \u00dcbertreter?", "tokens": ["Bin", "ich", "vor", "dir", ",", "Herr", ",", "nicht", "ein", "\u00dc\u00b7bert\u00b7re\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "$,", "NN", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tu ich zu viel, wenn ich die schweren Tage", "tokens": ["Tu", "ich", "zu", "viel", ",", "wenn", "ich", "die", "schwe\u00b7ren", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PTKA", "PIS", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Standhaft ertrage?", "tokens": ["Stand\u00b7haft", "er\u00b7tra\u00b7ge", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.17": {"line.1": {"text": "Wie oft, o Gott! wenn wir das B\u00f6se dulden,", "tokens": ["Wie", "oft", ",", "o", "Gott", "!", "wenn", "wir", "das", "B\u00f6\u00b7se", "dul\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "FM", "NN", "$.", "KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erdulden wir nur unsrer Torheit Schulden,", "tokens": ["E\u00b7rdul\u00b7den", "wir", "nur", "uns\u00b7rer", "Tor\u00b7heit", "Schul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nennen Lohn, den wir verdient bekommen,", "tokens": ["Und", "nen\u00b7nen", "Lohn", ",", "den", "wir", "ver\u00b7dient", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tr\u00fcbsal der Frommen!", "tokens": ["Tr\u00fcb\u00b7sal", "der", "From\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.18": {"line.1": {"text": "Ist D\u00fcrftigkeit, in der die Tr\u00e4gen klagen,", "tokens": ["Ist", "D\u00fcrf\u00b7tig\u00b7keit", ",", "in", "der", "die", "Tr\u00e4\u00b7gen", "kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind Ha\u00df und Pein, die Stolz und Wollust tragen,", "tokens": ["Sind", "Ha\u00df", "und", "Pein", ",", "die", "Stolz", "und", "Wol\u00b7lust", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$,", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Schwelgers Schmerz, des Neids vermi\u00dfte Freuden,", "tokens": ["Des", "Schwel\u00b7gers", "Schmerz", ",", "des", "Neids", "ver\u00b7mi\u00df\u00b7te", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Christliches Leiden?", "tokens": ["Christ\u00b7li\u00b7ches", "Lei\u00b7den", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.19": {"line.1": {"text": "Ist deren Qual, die deinen Rat verachtet,", "tokens": ["Ist", "de\u00b7ren", "Qual", ",", "die", "dei\u00b7nen", "Rat", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach Gottesfurcht und Glauben nie getrachtet,", "tokens": ["Nach", "Got\u00b7tes\u00b7furcht", "und", "Glau\u00b7ben", "nie", "ge\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und die sich itzt in finstrer Schwermut qu\u00e4len,", "tokens": ["Und", "die", "sich", "itzt", "in", "finst\u00b7rer", "Schwer\u00b7mut", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRF", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Pr\u00fcfung der Seelen?", "tokens": ["Pr\u00fc\u00b7fung", "der", "See\u00b7len", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.20": {"line.1": {"text": "Doch selbst, o Gott, in Strafen unsrer S\u00fcnden", "tokens": ["Doch", "selbst", ",", "o", "Gott", ",", "in", "Stra\u00b7fen", "uns\u00b7rer", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "FM", "NN", "$,", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "L\u00e4\u00dft du den Weg zu unserm Heil uns finden,", "tokens": ["L\u00e4\u00dft", "du", "den", "Weg", "zu", "un\u00b7serm", "Heil", "uns", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn wir sie uns, die Missetat zu hassen,", "tokens": ["Wenn", "wir", "sie", "uns", ",", "die", "Mis\u00b7se\u00b7tat", "zu", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Z\u00fcchtigen lassen.", "tokens": ["Z\u00fcch\u00b7ti\u00b7gen", "las\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.21": {"line.1": {"text": "Jag ich nur nach dem Frieden im Gewissen:", "tokens": ["Jag", "ich", "nur", "nach", "dem", "Frie\u00b7den", "im", "Ge\u00b7wis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird alles mir zum Besten dienen m\u00fcssen.", "tokens": ["Wird", "al\u00b7les", "mir", "zum", "Bes\u00b7ten", "die\u00b7nen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPER", "APPRART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du, Herr, regierst, und ewig wirkt dein Wille", "tokens": ["Du", ",", "Herr", ",", "re\u00b7gierst", ",", "und", "e\u00b7wig", "wirkt", "dein", "Wil\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gutes die F\u00fclle.", "tokens": ["Gu\u00b7tes", "die", "F\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.22": {"line.1": {"text": "Ich bin ein Gast und Pilger auf der Erden,", "tokens": ["Ich", "bin", "ein", "Gast", "und", "Pil\u00b7ger", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht hier, erst dort, dort soll ich gl\u00fccklich werden;", "tokens": ["Nicht", "hier", ",", "erst", "dort", ",", "dort", "soll", "ich", "gl\u00fcck\u00b7lich", "wer\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "ADV", "ADV", "$,", "ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und gegen euch, was sind, ihr ewgen Freuden,", "tokens": ["Und", "ge\u00b7gen", "euch", ",", "was", "sind", ",", "ihr", "ew\u00b7gen", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "PWS", "VAFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dieser Zeit Leiden?", "tokens": ["Die\u00b7ser", "Zeit", "Lei\u00b7den", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.23": {"line.1": {"text": "Wenn ich nur nicht mein Elend selbst verschulde;", "tokens": ["Wenn", "ich", "nur", "nicht", "mein", "E\u00b7lend", "selbst", "ver\u00b7schul\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich als Mensch, als Christ, hier leid und dulde:", "tokens": ["Wenn", "ich", "als", "Mensch", ",", "als", "Christ", ",", "hier", "leid", "und", "dul\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "$,", "KOUS", "NN", "$,", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So kann ich mich der H\u00fclfe der Erl\u00f6sten", "tokens": ["So", "kann", "ich", "mich", "der", "H\u00fcl\u00b7fe", "der", "Er\u00b7l\u00f6s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sicher getr\u00f6sten.", "tokens": ["Si\u00b7cher", "ge\u00b7tr\u00f6s\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.24": {"line.1": {"text": "Ich bin ein Mensch, und Leiden m\u00fcssen kr\u00e4nken;", "tokens": ["Ich", "bin", "ein", "Mensch", ",", "und", "Lei\u00b7den", "m\u00fcs\u00b7sen", "kr\u00e4n\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch in der Not an seinen Sch\u00f6pfer denken,", "tokens": ["Doch", "in", "der", "Not", "an", "sei\u00b7nen", "Sch\u00f6p\u00b7fer", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ihm vertraun, dies st\u00e4rket unsre Herzen", "tokens": ["Und", "ihm", "ver\u00b7traun", ",", "dies", "st\u00e4r\u00b7ket", "uns\u00b7re", "Her\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$,", "PDS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mitten in Schmerzen.", "tokens": ["Mit\u00b7ten", "in", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.25": {"line.1": {"text": "Schau \u00fcber dich! Wer tr\u00e4gt der Himmel Heere?", "tokens": ["Schau", "\u00fc\u00b7ber", "dich", "!", "Wer", "tr\u00e4gt", "der", "Him\u00b7mel", "Hee\u00b7re", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$.", "PWS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Merk auf! Wer spricht: Bis hieher! zu dem Meere?", "tokens": ["Merk", "auf", "!", "Wer", "spricht", ":", "Bis", "hie\u00b7her", "!", "zu", "dem", "Mee\u00b7re", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "PWS", "VVFIN", "$.", "APPR", "PAV", "$.", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist er nicht auch dein Helfer und Berater,", "tokens": ["Ist", "er", "nicht", "auch", "dein", "Hel\u00b7fer", "und", "Be\u00b7ra\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Ewig dein Vater?", "tokens": ["E\u00b7wig", "dein", "Va\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.26": {"line.1": {"text": "Willst du so viel, als der Allweise, wissen?", "tokens": ["Willst", "du", "so", "viel", ",", "als", "der", "All\u00b7wei\u00b7se", ",", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Itzt wei\u00dft du nicht, warum du leiden m\u00fcssen;", "tokens": ["Itzt", "wei\u00dft", "du", "nicht", ",", "wa\u00b7rum", "du", "lei\u00b7den", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VVINF", "VMINF", "$."], "me