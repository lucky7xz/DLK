{"textgrid.poem.32754": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schlaf deinen letzten Schlummer, Tyrann! Mit ihm", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlaf deinen letzten Schlummer, Tyrann! Mit ihm", "tokens": ["Schlaf", "dei\u00b7nen", "letz\u00b7ten", "Schlum\u00b7mer", ",", "Ty\u00b7rann", "!", "Mit", "ihm"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,", "NE", "$.", "APPR", "PPER"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Fleucht Ruh auf ewig! Tr\u00e4ume zum letztenmal", "tokens": ["Fleucht", "Ruh", "auf", "e\u00b7wig", "!", "Tr\u00e4u\u00b7me", "zum", "letz\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "ADJD", "$.", "NN", "APPRART", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dich gl\u00fccklich! Ha! Du lachst! Erschien dir,", "tokens": ["Dich", "gl\u00fcck\u00b7lich", "!", "Ha", "!", "Du", "lachst", "!", "Er\u00b7schien", "dir", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$.", "ITJ", "$.", "PPER", "VVFIN", "$.", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch ungeboren, ein neuer Frevel?", "tokens": ["Noch", "un\u00b7ge\u00b7bo\u00b7ren", ",", "ein", "neu\u00b7er", "Fre\u00b7vel", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Lach nur, und zwing die H\u00f6lle zum Lachen mit!", "tokens": ["Lach", "nur", ",", "und", "zwing", "die", "H\u00f6l\u00b7le", "zum", "La\u00b7chen", "mit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "KON", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bald wird sie heulen! st\u00fcrzen vom Throne bald,", "tokens": ["Bald", "wird", "sie", "heu\u00b7len", "!", "st\u00fcr\u00b7zen", "vom", "Thro\u00b7ne", "bald", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$.", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den du, mit ihr im Bund', auf Sch\u00e4deln", "tokens": ["Den", "du", ",", "mit", "ihr", "im", "Bund'", ",", "auf", "Sch\u00e4\u00b7deln"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "$,", "APPR", "PPOSAT", "APPRART", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freier, verratener V\u00f6lker bautest!", "tokens": ["Frei\u00b7er", ",", "ver\u00b7ra\u00b7te\u00b7ner", "V\u00f6l\u00b7ker", "bau\u00b7test", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Ihr Blut zu trinken, zogst du die Menschheit aus,", "tokens": ["Ihr", "Blut", "zu", "trin\u00b7ken", ",", "zogst", "du", "die", "Menschheit", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schufst Tier' aus Menschen, da\u00df sie dir huldigten!", "tokens": ["Schufst", "Tier'", "aus", "Men\u00b7schen", ",", "da\u00df", "sie", "dir", "hul\u00b7dig\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-++-+--", "measure": "unknown.measure.penta"}, "line.3": {"text": "Da krochen um den Thron sie, bebten", "tokens": ["Da", "kro\u00b7chen", "um", "den", "Thron", "sie", ",", "beb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "PPER", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor des erschaffenen Gottes Allmacht!", "tokens": ["Vor", "des", "er\u00b7schaf\u00b7fe\u00b7nen", "Got\u00b7tes", "All\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie Meeresstrudel gierig das Schiff verschlingt,", "tokens": ["Wie", "Mee\u00b7res\u00b7stru\u00b7del", "gie\u00b7rig", "das", "Schiff", "ver\u00b7schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und wieder ausspeit: Also versammelte", "tokens": ["Und", "wie\u00b7der", "aus\u00b7speit", ":", "Al\u00b7so", "ver\u00b7sam\u00b7mel\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "PTKVZ", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Thron die Laster, Str\u00f6me st\u00fcrzten", "tokens": ["Dein", "Thron", "die", "Las\u00b7ter", ",", "Str\u00f6\u00b7me", "st\u00fcrz\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich in den hallenden weiten Abgrund!", "tokens": ["Sich", "in", "den", "hal\u00b7len\u00b7den", "wei\u00b7ten", "Ab\u00b7grund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Da\u00df deine Burg ein brausender Becher ward", "tokens": ["Da\u00df", "dei\u00b7ne", "Burg", "ein", "brau\u00b7sen\u00b7der", "Be\u00b7cher", "ward"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Voll hei\u00dfer L\u00fcste, der sich mit wildem Strom", "tokens": ["Voll", "hei\u00b7\u00dfer", "L\u00fcs\u00b7te", ",", "der", "sich", "mit", "wil\u00b7dem", "Strom"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "$,", "PRELS", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In alle Land' ergo\u00df, da\u00df thr\u00e4nend", "tokens": ["In", "al\u00b7le", "Land'", "er\u00b7go\u00df", ",", "da\u00df", "thr\u00e4\u00b7nend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Engel ihr heiliges Antlitz wandten!", "tokens": ["En\u00b7gel", "ihr", "hei\u00b7li\u00b7ges", "Ant\u00b7litz", "wand\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Von dir vergiftet, schleichen Gerippe dort", "tokens": ["Von", "dir", "ver\u00b7gif\u00b7tet", ",", "schlei\u00b7chen", "Ge\u00b7rip\u00b7pe", "dort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "$,", "ADJA", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf allen Stra\u00dfen, fluchen im Tode dir,", "tokens": ["Auf", "al\u00b7len", "Stra\u00b7\u00dfen", ",", "flu\u00b7chen", "im", "To\u00b7de", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "VVFIN", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenn meine Br\u00fcder sie zur Rache", "tokens": ["Wenn", "mei\u00b7ne", "Br\u00fc\u00b7der", "sie", "zur", "Ra\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchren in deiner Vertrauten Abgrund!", "tokens": ["F\u00fch\u00b7ren", "in", "dei\u00b7ner", "Ver\u00b7trau\u00b7ten", "Ab\u00b7grund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Schau, vor den Mauren schmachtet das Land umher;", "tokens": ["Schau", ",", "vor", "den", "Mau\u00b7ren", "schmach\u00b7tet", "das", "Land", "um\u00b7her", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Verdorrte B\u00e4ume starren gen Himmel auf;", "tokens": ["Ver\u00b7dorr\u00b7te", "B\u00e4u\u00b7me", "star\u00b7ren", "gen", "Him\u00b7mel", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Weinberg stehn verwaiste St\u00e4be,", "tokens": ["Im", "Wein\u00b7berg", "stehn", "ver\u00b7wais\u00b7te", "St\u00e4\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bl\u00fchende Disteln auf \u00f6dem Fruchtfeld!", "tokens": ["Bl\u00fc\u00b7hen\u00b7de", "Dis\u00b7teln", "auf", "\u00f6\u00b7dem", "Frucht\u00b7feld", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Denn unterm Ro\u00df des J\u00e4gers erstirbt die Saat;", "tokens": ["Denn", "un\u00b7term", "Ro\u00df", "des", "J\u00e4\u00b7gers", "er\u00b7stirbt", "die", "Saat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und was der Huf des rasenden Heers verschont,", "tokens": ["Und", "was", "der", "Huf", "des", "ra\u00b7sen\u00b7den", "Heers", "ver\u00b7schont", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zerw\u00fchlt das Wild, das deiner Mordlust", "tokens": ["Zer\u00b7w\u00fchlt", "das", "Wild", ",", "das", "dei\u00b7ner", "Mord\u00b7lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du f\u00fcr den kommenden Morgen hegtest!", "tokens": ["Du", "f\u00fcr", "den", "kom\u00b7men\u00b7den", "Mor\u00b7gen", "heg\u00b7test", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "In leeren H\u00fctten schmachtet \u2013 du raubtest ihr", "tokens": ["In", "lee\u00b7ren", "H\u00fct\u00b7ten", "schmach\u00b7tet", "\u2013", "du", "raub\u00b7test", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$(", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Mann \u2013 die Witwe; weinende Kinder flehn", "tokens": ["Den", "Mann", "\u2013", "die", "Wit\u00b7we", ";", "wei\u00b7nen\u00b7de", "Kin\u00b7der", "flehn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "NN", "$.", "ADJA", "NN", "VVINF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Um Brot, das du, dein Vieh zu n\u00e4hren,", "tokens": ["Um", "Brot", ",", "das", "du", ",", "dein", "Vieh", "zu", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "PRELS", "PPER", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr aus den zitternden H\u00e4nden rissest!", "tokens": ["Ihr", "aus", "den", "zit\u00b7tern\u00b7den", "H\u00e4n\u00b7den", "ris\u00b7sest", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Dort jammern nackte Pfl\u00fcger am rostenden,", "tokens": ["Dort", "jam\u00b7mern", "nack\u00b7te", "Pfl\u00fc\u00b7ger", "am", "ros\u00b7ten\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Stierlosen Pfluge! Jener mit fremdem Stier", "tokens": ["Stier\u00b7lo\u00b7sen", "Pflu\u00b7ge", "!", "Je\u00b7ner", "mit", "frem\u00b7dem", "Stier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$.", "PDAT", "APPR", "ADJA", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Pfl\u00fcgt schaudernd seines Sohns Gebein auf,", "tokens": ["Pfl\u00fcgt", "schau\u00b7dernd", "sei\u00b7nes", "Sohns", "Ge\u00b7bein", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den in der rasenden Schlacht du w\u00fcrgtest! \u2013", "tokens": ["Den", "in", "der", "ra\u00b7sen\u00b7den", "Schlacht", "du", "w\u00fcrg\u00b7test", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PPER", "VAFIN", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Wie oft, Tyrann, erhub ich mein Schert! Wie oft", "tokens": ["Wie", "oft", ",", "Ty\u00b7rann", ",", "er\u00b7hub", "ich", "mein", "Schert", "!", "Wie", "oft"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "$,", "NE", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "PWAV", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gab ich's dem Sohn der Freiheit! Er blutete!", "tokens": ["Gab", "ich's", "dem", "Sohn", "der", "Frei\u00b7heit", "!", "Er", "blu\u00b7te\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In Wolken h\u00fcllt' ich mich, und blickte", "tokens": ["In", "Wol\u00b7ken", "h\u00fcllt'", "ich", "mich", ",", "und", "blick\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sehnend der s\u00e4umenden Rach' entgegen!", "tokens": ["Seh\u00b7nend", "der", "s\u00e4u\u00b7men\u00b7den", "Rach'", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Sie k\u00f6mmt! sie k\u00f6mmt! Erwache! Schon ist sie da!", "tokens": ["Sie", "k\u00f6mmt", "!", "sie", "k\u00f6mmt", "!", "Er\u00b7wa\u00b7che", "!", "Schon", "ist", "sie", "da", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Laut hebt im Himmel blutige Klage sich!", "tokens": ["Laut", "hebt", "im", "Him\u00b7mel", "blu\u00b7ti\u00b7ge", "Kla\u00b7ge", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPRART", "NN", "ADJA", "NN", "PRF", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}