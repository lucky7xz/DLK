{"textgrid.poem.24731": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will den Schatten einziger Geschicke", "tokens": ["Ich", "will", "den", "Schat\u00b7ten", "ein\u00b7zi\u00b7ger", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gro\u00df an den Boden der Gedichte legen,", "tokens": ["Gro\u00df", "an", "den", "Bo\u00b7den", "der", "Ge\u00b7dich\u00b7te", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der jungen Helden ungeheure Blicke", "tokens": ["Der", "jun\u00b7gen", "Hel\u00b7den", "un\u00b7ge\u00b7heu\u00b7re", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und andre G\u00f6tter, die den Sinn bewegen.", "tokens": ["Und", "and\u00b7re", "G\u00f6t\u00b7ter", ",", "die", "den", "Sinn", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Erst aber la\u00dft uns von den Fr\u00fcchten essen:", "tokens": ["Erst", "a\u00b7ber", "la\u00dft", "uns", "von", "den", "Fr\u00fcch\u00b7ten", "es\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie kommen aus den Bergen, aus dem Meer,", "tokens": ["Sie", "kom\u00b7men", "aus", "den", "Ber\u00b7gen", ",", "aus", "dem", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus schlummerlosen K\u00f6nigsgr\u00e4bern her,", "tokens": ["Aus", "schlum\u00b7mer\u00b7lo\u00b7sen", "K\u00f6\u00b7nigs\u00b7gr\u00e4\u00b7bern", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wir wollen ihren Ursprung nicht vergessen", "tokens": ["Wir", "wol\u00b7len", "ih\u00b7ren", "Ur\u00b7sprung", "nicht", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und nicht, da\u00df sie von Blut Geschwister sind", "tokens": ["Und", "nicht", ",", "da\u00df", "sie", "von", "Blut", "Ge\u00b7schwis\u00b7ter", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "NN", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit uns und all den anderen Gesch\u00f6pfen", "tokens": ["Mit", "uns", "und", "all", "den", "an\u00b7de\u00b7ren", "Ge\u00b7sch\u00f6p\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des gro\u00dfen Grabes, die den Abendwind", "