{"dta.poem.19397": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "130.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Lebens Sorge l\u00e4\u00dft dir wenig Zeit zu denken", "tokens": ["Des", "Le\u00b7bens", "Sor\u00b7ge", "l\u00e4\u00dft", "dir", "we\u00b7nig", "Zeit", "zu", "den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PIAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An dich, und deinen Sinn aufs Ewige zu lenken.", "tokens": ["An", "dich", ",", "und", "dei\u00b7nen", "Sinn", "aufs", "E\u00b7wi\u00b7ge", "zu", "len\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KON", "PPOSAT", "NN", "APPRART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Lang sorgst du, sorgenlos zu haben eine Stunde;", "tokens": ["Lang", "sorgst", "du", ",", "sor\u00b7gen\u00b7los", "zu", "ha\u00b7ben", "ei\u00b7ne", "Stun\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJD", "PTKZU", "VAINF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann wird, der Sorgen los, zu lang dir die Sekunde.", "tokens": ["Dann", "wird", ",", "der", "Sor\u00b7gen", "los", ",", "zu", "lang", "dir", "die", "Se\u00b7kun\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "ART", "NN", "PTKVZ", "$,", "PTKA", "ADJD", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du gehst auf Zeitvertreib, auf Unterhaltung aus,", "tokens": ["Du", "gehst", "auf", "Zeit\u00b7ver\u00b7treib", ",", "auf", "Un\u00b7ter\u00b7hal\u00b7tung", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und statt der Sammlung suchst du der Zerstreuung Braus.", "tokens": ["Und", "statt", "der", "Samm\u00b7lung", "suchst", "du", "der", "Zer\u00b7streu\u00b7ung", "Braus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du findest wol nach Wunsch dein Innres nicht bestellt,", "tokens": ["Du", "fin\u00b7dest", "wol", "nach", "Wunsch", "dein", "Inn\u00b7res", "nicht", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "PPOSAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und w\u00fcnschest lieber nicht zu sehn was dir misf\u00e4llt.", "tokens": ["Und", "w\u00fcn\u00b7schest", "lie\u00b7ber", "nicht", "zu", "sehn", "was", "dir", "mis\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du siehst ein weites Leer, und wei\u00dft es nicht zu f\u00fcllen,", "tokens": ["Du", "siehst", "ein", "wei\u00b7tes", "Leer", ",", "und", "wei\u00dft", "es", "nicht", "zu", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und willst mit holem Schein der Luft es \u00fcberh\u00fcllen.", "tokens": ["Und", "willst", "mit", "ho\u00b7lem", "Schein", "der", "Luft", "es", "\u00fc\u00b7berh\u00b7\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ADJA", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Vertreibe denn die Zeit, bis dich die Zeit vertreibt;", "tokens": ["Ver\u00b7trei\u00b7be", "denn", "die", "Zeit", ",", "bis", "dich", "die", "Zeit", "ver\u00b7treibt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zerstreue dich, bis nichts an dir zu sammeln bleibt;", "tokens": ["Zer\u00b7streu\u00b7e", "dich", ",", "bis", "nichts", "an", "dir", "zu", "sam\u00b7meln", "bleibt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PIS", "APPR", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Bis wieder sammelt einst des Lebens Herr und Meister", "tokens": ["Bis", "wie\u00b7der", "sam\u00b7melt", "einst", "des", "Le\u00b7bens", "Herr", "und", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "ADV", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Deine in alle Welt zerstreuten Lebensgeister.", "tokens": ["Dei\u00b7ne", "in", "al\u00b7le", "Welt", "zer\u00b7streu\u00b7ten", "Le\u00b7bens\u00b7geis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.8": {"line.1": {"text": "Er wird nicht schwerer auch sie bringen zum Vereine", "tokens": ["Er", "wird", "nicht", "schwe\u00b7rer", "auch", "sie", "brin\u00b7gen", "zum", "Ver\u00b7ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "ADV", "PPER", "VVINF", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als unsere zu Staub zerstreuten Todtenbeine.", "tokens": ["Als", "un\u00b7se\u00b7re", "zu", "Staub", "zer\u00b7streu\u00b7ten", "Tod\u00b7ten\u00b7bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}