{"dta.poem.23636": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Dichtertrost.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo ist die Zeit als Namen galten", "tokens": ["Wo", "ist", "die", "Zeit", "als", "Na\u00b7men", "gal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "KOUS", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Noch leben Dichter wie die alten,", "tokens": ["Noch", "le\u00b7ben", "Dich\u00b7ter", "wie", "die", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KOKOM", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch Ruhm und Minnesold ist fort.", "tokens": ["Doch", "Ruhm", "und", "Min\u00b7ne\u00b7sold", "ist", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Einst war sie hoch und sch\u00f6n gepriesen,", "tokens": ["Einst", "war", "sie", "hoch", "und", "sch\u00f6n", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Dichtung laute sel'ge Lust \u2014", "tokens": ["Der", "Dich\u00b7tung", "lau\u00b7te", "sel'\u00b7ge", "Lust"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie ist verbannt und heimgewiesen", "tokens": ["Sie", "ist", "ver\u00b7bannt", "und", "heim\u00b7ge\u00b7wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In jedes edlen Dichters Brust.", "tokens": ["In", "je\u00b7des", "ed\u00b7len", "Dich\u00b7ters", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In diesem stillen Heiligthume", "tokens": ["In", "die\u00b7sem", "stil\u00b7len", "Hei\u00b7lig\u00b7thu\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tr\u00e4umt sie in Selbstgen\u00fcgsamkeit", "tokens": ["Tr\u00e4umt", "sie", "in", "Selbst\u00b7ge\u00b7n\u00fcg\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Minnegl\u00fcck, von Ehr' und Ruhme,", "tokens": ["Von", "Min\u00b7ne\u00b7gl\u00fcck", ",", "von", "Ehr'", "und", "Ruh\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von einer sch\u00f6nren k\u00fcnft'gen Zeit.", "tokens": ["Von", "ei\u00b7ner", "sch\u00f6n\u00b7ren", "k\u00fcnft'\u00b7gen", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}