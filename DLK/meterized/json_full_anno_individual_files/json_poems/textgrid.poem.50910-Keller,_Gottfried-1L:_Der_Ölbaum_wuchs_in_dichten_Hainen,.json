oken_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Trat er hervor, mit Gott allein im Bunde;", "tokens": ["Trat", "er", "her\u00b7vor", ",", "mit", "Gott", "al\u00b7lein", "im", "Bun\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "APPR", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Hand mit wei\u00dfen Steinen sch\u00e4rfend", "tokens": ["Die", "Hand", "mit", "wei\u00b7\u00dfen", "Stei\u00b7nen", "sch\u00e4r\u00b7fend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus eines B\u00e4chleins hellem Silbergrunde,", "tokens": ["Aus", "ei\u00b7nes", "B\u00e4ch\u00b7leins", "hel\u00b7lem", "Sil\u00b7ber\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Tat er den Wurf; des Riesen Stirne klaffte,", "tokens": ["Tat", "er", "den", "Wurf", ";", "des", "Rie\u00b7sen", "Stir\u00b7ne", "klaff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$.", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Es war aus blauer Luft ein j\u00e4her Schlag!", "tokens": ["Es", "war", "aus", "blau\u00b7er", "Luft", "ein", "j\u00e4\u00b7her", "Schlag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie lacht' er sch\u00f6n, als der Erschlaffte", "tokens": ["Wie", "lacht'", "er", "sch\u00f6n", ",", "als", "der", "Er\u00b7schlaff\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Kopflos zu seinen F\u00fc\u00dfen lag!", "tokens": ["Kopf\u00b7los", "zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", "lag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Dank, den David hat empfangen,", "tokens": ["Der", "Dank", ",", "den", "Da\u00b7vid", "hat", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Steht in den alten Schwarten aufgeschrieben:", "tokens": ["Steht", "in", "den", "al\u00b7ten", "Schwar\u00b7ten", "auf\u00b7ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach seinem Tod ein toll Verlangen!", "tokens": ["Nach", "sei\u00b7nem", "Tod", "ein", "toll", "Ver\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Not und Irrsal ward er hingetrieben.", "tokens": ["In", "Not", "und", "Irr\u00b7sal", "ward", "er", "hin\u00b7ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sein Haupt zum Herren n\u00e4chtlich aufgewendet,", "tokens": ["Sein", "Haupt", "zum", "Her\u00b7ren", "n\u00e4cht\u00b7lich", "auf\u00b7ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sang er des Grames Lied ohn Unterla\u00df;", "tokens": ["Sang", "er", "des", "Gra\u00b7mes", "Lied", "ohn", "Un\u00b7ter\u00b7la\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Doch hat das Spiel noch gut geendet,", "tokens": ["Doch", "hat", "das", "Spiel", "noch", "gut", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als auf dem Thron der Feldhirt sa\u00df!", "tokens": ["Als", "auf", "dem", "Thron", "der", "Feld\u00b7hirt", "sa\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der \u00d6lbaum wuchs in dichten Hainen,", "tokens": ["Der", "\u00d6l\u00b7baum", "wuchs", "in", "dich\u00b7ten", "Hai\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An klaren B\u00e4chen wucherte die Rose,", "tokens": ["An", "kla\u00b7ren", "B\u00e4\u00b