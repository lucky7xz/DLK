{"textgrid.poem.37094": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Stern der Seelen,", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jenen freundlichen Stern, den Gespielen der Abendd\u00e4mmrung,", "tokens": ["Je\u00b7nen", "freund\u00b7li\u00b7chen", "Stern", ",", "den", "Ge\u00b7spie\u00b7len", "der", "A\u00b7bend\u00b7d\u00e4mm\u00b7rung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und Verk\u00fcnder der Ruh, bewohnen die Seelen der Menschen,", "tokens": ["Und", "Ver\u00b7k\u00fcn\u00b7der", "der", "Ruh", ",", "be\u00b7woh\u00b7nen", "die", "See\u00b7len", "der", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Eh der Allschaffende ruft, und die Seelen vom Schlummer erwachen,", "tokens": ["Eh", "der", "All\u00b7schaf\u00b7fen\u00b7de", "ruft", ",", "und", "die", "See\u00b7len", "vom", "Schlum\u00b7mer", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Vom halbwachenden Schlummer, den unter Blumen sie schliefen.", "tokens": ["Vom", "halb\u00b7wa\u00b7chen\u00b7den", "Schlum\u00b7mer", ",", "den", "un\u00b7ter", "Blu\u00b7men", "sie", "schlie\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Geu\u00df durch die Wipfel des Hayns, wo ich singe, sch\u00f6nster der Sterne,", "tokens": ["Geu\u00df", "durch", "die", "Wip\u00b7fel", "des", "Hayns", ",", "wo", "ich", "sin\u00b7ge", ",", "sch\u00f6ns\u00b7ter", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Hellres Licht! Dich beschwebt ich in meiner schlummernden Kindheit,", "tokens": ["Hell\u00b7res", "Licht", "!", "Dich", "be\u00b7schwebt", "ich", "in", "mei\u00b7ner", "schlum\u00b7mern\u00b7den", "Kind\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Und Jahrtausende tr\u00e4umt' ich in deinen Thalen vor\u00fcber.", "tokens": ["Und", "Jahr\u00b7tau\u00b7sen\u00b7de", "tr\u00e4umt'", "ich", "in", "dei\u00b7nen", "Tha\u00b7len", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "S\u00fc\u00dfes Gef\u00fchl der Erinnrung beschleicht die Bewohner des Erballs,", "tokens": ["S\u00fc\u00b7\u00dfes", "Ge\u00b7f\u00fchl", "der", "E\u00b7rinn\u00b7rung", "be\u00b7schleicht", "die", "Be\u00b7woh\u0