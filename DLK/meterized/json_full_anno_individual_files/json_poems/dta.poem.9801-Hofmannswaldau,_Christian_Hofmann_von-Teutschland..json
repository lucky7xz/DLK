{"dta.poem.9801": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Teutschland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So mu\u00df das gl\u00fccke dann das steuer-ruder f\u00fchren/", "tokens": ["So", "mu\u00df", "das", "gl\u00fc\u00b7cke", "dann", "das", "steu\u00b7er\u00b7ru\u00b7der", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PDS", "VVFIN", "ADV", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So offt ein C\u00e4sar sich der wilden see vertrau\u2019t?", "tokens": ["So", "offt", "ein", "C\u00e4\u00b7sar", "sich", "der", "wil\u00b7den", "see", "ver\u00b7trau't", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NE", "PRF", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und mein durchleuchter Held kan nichts als wohlseyn sp\u00fcren/", "tokens": ["Und", "mein", "durch\u00b7leuch\u00b7ter", "Held", "kan", "nichts", "als", "wohl\u00b7seyn", "sp\u00fc\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VMFIN", "PIS", "KOKOM", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wann bey den Britten er die Majest\u00e4ten schau\u2019t.", "tokens": ["Wann", "bey", "den", "Brit\u00b7ten", "er", "die", "Ma\u00b7jes\u00b7t\u00e4\u00b7ten", "schau'", "t."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des grossen Wilhelms gunst/ Mariens gnaden-blicke/", "tokens": ["Des", "gros\u00b7sen", "Wil\u00b7helms", "gunst", "/", "Ma\u00b7ri\u00b7ens", "gna\u00b7den\u00b7bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "NE", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die wie das morgen-licht die welt erfreuen kan/", "tokens": ["Die", "wie", "das", "mor\u00b7gen\u00b7licht", "die", "welt", "er\u00b7freu\u00b7en", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sind seiner tugend lohn/ ein k\u00f6stliches gesticke;", "tokens": ["Sind", "sei\u00b7ner", "tu\u00b7gend", "lohn", "/", "ein", "k\u00f6st\u00b7li\u00b7ches", "ge\u00b7sti\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn adler sehen nur der sonnen strahlen an.", "tokens": ["Denn", "ad\u00b7ler", "se\u00b7hen", "nur", "der", "son\u00b7nen", "strah\u00b7len", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "ART", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da wird sein treuer geist auch auff mein bestes dencken/", "tokens": ["Da", "wird", "sein", "treu\u00b7er", "geist", "auch", "auff", "mein", "bes\u00b7tes", "den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und \u00fcber meine ruh\u2019 offt mit zu rathe geh\u2019n/", "tokens": ["Und", "\u00fc\u00b7ber", "mei\u00b7ne", "ruh'", "offt", "mit", "zu", "ra\u00b7the", "geh'n", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADV", "APPR", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der H\u00f6chste/ dessen hand mir ihn hat wollen schencken/", "tokens": ["Der", "H\u00f6chs\u00b7te", "/", "des\u00b7sen", "hand", "mir", "ihn", "hat", "wol\u00b7len", "schen\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "VVFIN", "PPER", "PPER", "VAFIN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der la\u00df ihn mich vergn\u00fcgt in kurtzen wieder seh\u2019n.", "tokens": ["Der", "la\u00df", "ihn", "mich", "ver\u00b7gn\u00fcgt", "in", "kurt\u00b7zen", "wie\u00b7der", "seh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "VVIMP", "PPER", "PRF", "VVPP", "APPR", "ADJA", "ADV", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Euch aber/ die ihr ietzt dem gl\u00fcck im schosse sitzet/", "tokens": ["Euch", "a\u00b7ber", "/", "die", "ihr", "ietzt", "dem", "gl\u00fcck", "im", "schos\u00b7se", "sit\u00b7zet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "PRELS", "PPER", "ADV", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und aus der sichern ruh\u2019 der g\u00f6ldnen zeiten lacht/", "tokens": ["Und", "aus", "der", "si\u00b7chern", "ruh'", "der", "g\u00f6ld\u00b7nen", "zei\u00b7ten", "lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Beschwer ich bey der treu/ die unser b\u00fcndnis st\u00fctzet/", "tokens": ["Be\u00b7schwer", "ich", "bey", "der", "treu", "/", "die", "un\u00b7ser", "b\u00fcnd\u00b7nis", "st\u00fct\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "ADJD", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df meinen f\u00fcrsten ihr zu lieben seyd bedacht.", "tokens": ["Da\u00df", "mei\u00b7nen", "f\u00fcrs\u00b7ten", "ihr", "zu", "lie\u00b7ben", "seyd", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "PPER", "PTKZU", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}