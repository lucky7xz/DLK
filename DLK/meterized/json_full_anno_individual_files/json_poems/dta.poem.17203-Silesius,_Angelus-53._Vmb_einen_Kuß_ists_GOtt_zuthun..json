{"dta.poem.17203": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "53.  Vmb einen Ku\u00df ists GOtt zuthun.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Was wil doch GOttes Sohn da\u00df Er ins Elend k\u00f6mbt/", "tokens": ["Was", "wil", "doch", "Got\u00b7tes", "Sohn", "da\u00df", "Er", "ins", "E\u00b7lend", "k\u00f6mbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "NN", "NN", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ein solch schweres Kreutz auf seine Schultern nimbt?", "tokens": ["Und", "ein", "solch", "schwe\u00b7res", "Kreutz", "auf", "sei\u00b7ne", "Schul\u00b7tern", "nimbt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja da\u00df Er bi\u00df inn Tod sich \u00e4ngstet f\u00fcr und f\u00fcr?", "tokens": ["Ja", "da\u00df", "Er", "bi\u00df", "inn", "Tod", "sich", "\u00e4ngs\u00b7tet", "f\u00fcr", "und", "f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PPER", "ADV", "APPR", "NN", "PRF", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er suchet anders nichts als einen Ku\u00df von dir.", "tokens": ["Er", "su\u00b7chet", "an\u00b7ders", "nichts", "als", "ei\u00b7nen", "Ku\u00df