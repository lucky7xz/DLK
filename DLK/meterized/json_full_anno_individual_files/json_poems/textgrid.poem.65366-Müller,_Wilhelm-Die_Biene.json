{"textgrid.poem.65366": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Biene", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Biene, dich k\u00f6nnt' ich beneiden,", "tokens": ["Bie\u00b7ne", ",", "dich", "k\u00f6nnt'", "ich", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nnte Neid im Fr\u00fchling wachsen,", "tokens": ["K\u00f6nn\u00b7te", "Neid", "im", "Fr\u00fch\u00b7ling", "wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich dich versunken sehe,", "tokens": ["Wenn", "ich", "dich", "ver\u00b7sun\u00b7ken", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer leiser leiser summend,", "tokens": ["Im\u00b7mer", "lei\u00b7ser", "lei\u00b7ser", "sum\u00b7mend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In dem rosenrothen Kelche", "tokens": ["In", "dem", "ro\u00b7sen\u00b7ro\u00b7then", "Kel\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einer jungen Apfelbl\u00fcthe.", "tokens": ["Ei\u00b7ner", "jun\u00b7gen", "Ap\u00b7fel\u00b7bl\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Als die Knospe wollte springen", "tokens": ["Als", "die", "Knos\u00b7pe", "woll\u00b7te", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und versch\u00e4mt es noch nicht wagte,", "tokens": ["Und", "ver\u00b7sch\u00e4mt", "es", "noch", "nicht", "wag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "In die helle Welt zu schauen,", "tokens": ["In", "die", "hel\u00b7le", "Welt", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Jetzo kamst du hergeflogen", "tokens": ["Jet\u00b7zo", "kamst", "du", "her\u00b7ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und ersahest dir die Knospe;", "tokens": ["Und", "er\u00b7sa\u00b7hest", "dir", "die", "Knos\u00b7pe", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und noch eh' ein Strahl der Sonne", "tokens": ["Und", "noch", "eh'", "ein", "Strahl", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "ART", "NN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.13": {"text": "Und ein Flatterhauch des Zephyrs", "tokens": ["Und", "ein", "Flat\u00b7ter\u00b7hauch", "des", "Ze\u00b7phyrs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Ihren Kelch ber\u00fchren konnte,", "tokens": ["Ih\u00b7ren", "Kelch", "be\u00b7r\u00fch\u00b7ren", "konn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Hingest du daran und sogest.", "tokens": ["Hin\u00b7gest", "du", "da\u00b7ran", "und", "so\u00b7gest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "KON", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Sauge, sauge! \u2013 Schwer und m\u00fcde", "tokens": ["Sau\u00b7ge", ",", "sau\u00b7ge", "!", "\u2013", "Schwer", "und", "m\u00fc\u00b7de"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$.", "$(", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Fliegst du heim nach deiner Zelle:", "tokens": ["Fliegst", "du", "heim", "nach", "dei\u00b7ner", "Zel\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Hast dein Tagewerk vollendet,", "tokens": ["Hast", "dein", "Ta\u00b7ge\u00b7werk", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Hast gesorgt auch f\u00fcr den Winter!", "tokens": ["Hast", "ge\u00b7sorgt", "auch", "f\u00fcr", "den", "Win\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ADV"