{"textgrid.poem.43022": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "10. An Ihn", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kann ich lindern dieses Sehnen,", "tokens": ["Kann", "ich", "lin\u00b7dern", "die\u00b7ses", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das mich tr\u00e4umend Dir vereint?", "tokens": ["Das", "mich", "tr\u00e4u\u00b7mend", "Dir", "ver\u00b7eint", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir verha\u00dft sind diese Thr\u00e4nen,", "tokens": ["Dir", "ver\u00b7ha\u00dft", "sind", "die\u00b7se", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der blasse Kummer weint;", "tokens": ["Die", "der", "blas\u00b7se", "Kum\u00b7mer", "weint", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die ein Opfer des Geschickes", "tokens": ["Die", "ein", "Op\u00b7fer", "des", "Ge\u00b7schi\u00b7ckes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weint am Grab entschwund'nen Gl\u00fcckes! \u2013", "tokens": ["Weint", "am", "Grab", "ent\u00b7schwun\u00b7d'\u00b7nen", "Gl\u00fc\u00b7ckes", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "\u00bbihre Todten zu begraben,", "tokens": ["\u00bb", "ih\u00b7re", "Tod\u00b7ten", "zu", "be\u00b7gra\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "La\u00df' die Todten sich bem\u00fch'n!", "tokens": ["La\u00df'", "die", "Tod\u00b7ten", "sich", "be\u00b7m\u00fch'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Doch des Lebens reichste Gaben", "tokens": ["Doch", "des", "Le\u00b7bens", "reichs\u00b7te", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "M\u00f6gen den Lebend'gen bl\u00fch'n.", "tokens": ["M\u00f6\u00b7gen", "den", "Le\u00b7ben\u00b7d'\u00b7gen", "bl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["VMFIN", "ART", "NN", "VVFIN", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Ewig soll's im Herzen lenzen,", "tokens": ["E\u00b7wig", "soll's", "im", "Her\u00b7zen", "len\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neue Triebe, neue Kraft!", "tokens": ["Neu\u00b7e", "Trie\u00b7be", ",", "neu\u00b7e", "Kraft", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit frischen Bl\u00fcthenkr\u00e4nzen", "tokens": ["Und", "mit", "fri\u00b7schen", "Bl\u00fc\u00b7then\u00b7kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schm\u00fccke sich die Leidenschaft!", "tokens": ["Schm\u00fc\u00b7cke", "sich", "die", "Lei\u00b7den\u00b7schaft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was im Sturm der Zeit verloren,", "tokens": ["Was", "im", "Sturm", "der", "Zeit", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sei verj\u00fcngt und neugeboren!", "tokens": ["Sei", "ver\u00b7j\u00fcngt", "und", "neu\u00b7ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn der Sonne Glanz versunken,", "tokens": ["Wenn", "der", "Son\u00b7ne", "Glanz", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn vergl\u00fcht des Tages Pracht;", "tokens": ["Wenn", "ver\u00b7gl\u00fcht", "des", "Ta\u00b7ges", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Steige auf, von Wonne trunken,", "tokens": ["Stei\u00b7ge", "auf", ",", "von", "Won\u00b7ne", "trun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Gluterf\u00fcllte Liebesnacht!\u00ab \u2013", "tokens": ["Glu\u00b7ter\u00b7f\u00fcll\u00b7te", "Lie\u00b7bes\u00b7nacht", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und doch r\u00fchrt mich frisches Leben", "tokens": ["Und", "doch", "r\u00fchrt", "mich", "fri\u00b7sches", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht mit seinem Zauberstab.", "tokens": ["Nicht", "mit", "sei\u00b7nem", "Zau\u00b7ber\u00b7stab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00e4umende Gedanken schweben", "tokens": ["Tr\u00e4u\u00b7men\u00b7de", "Ge\u00b7dan\u00b7ken", "schwe\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Um entschwund'ner Zeiten Grab;", "tokens": ["Um", "ent\u00b7schwun\u00b7d'\u00b7ner", "Zei\u00b7ten", "Grab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und es gr\u00fc\u00dft die bange Klage", "tokens": ["Und", "es", "gr\u00fc\u00dft", "die", "ban\u00b7ge", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Abendroth versunk'ner Tage.", "tokens": ["A\u00b7ben\u00b7droth", "ver\u00b7sunk'\u00b7ner", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Will ich kr\u00e4ftig mich ermannen,", "tokens": ["Will", "ich", "kr\u00e4f\u00b7tig", "mich", "er\u00b7man\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fliehen der Erinn'rung Fluch;", "tokens": ["Flie\u00b7hen", "der", "Er\u00b7inn'\u00b7rung", "Fluch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Fehlt, die Geister fortzubannen,", "tokens": ["Fehlt", ",", "die", "Geis\u00b7ter", "fort\u00b7zu\u00b7ban\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mir der m\u00e4cht'ge Zauberspruch!", "tokens": ["Mir", "der", "m\u00e4cht'\u00b7ge", "Zau\u00b7ber\u00b7spruch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schau' umher ich tiefbek\u00fcmmert,", "tokens": ["Schau'", "um\u00b7her", "ich", "tief\u00b7be\u00b7k\u00fcm\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles wird zur Elegie;", "tokens": ["Al\u00b7les", "wird", "zur", "E\u00b7le\u00b7gie", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im Innersten zertr\u00fcmmert", "tokens": ["Und", "im", "In\u00b7ners\u00b7ten", "zer\u00b7tr\u00fcm\u00b7mert"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ist der Seele Harmonie;", "tokens": ["Ist", "der", "See\u00b7le", "Har\u00b7mo\u00b7nie", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Klagend in Erinnerungen,", "tokens": ["Kla\u00b7gend", "in", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine Glocke, die gesprungen!", "tokens": ["Ei\u00b7ne", "Glo\u00b7cke", ",", "die", "ge\u00b7sprun\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wer dem machterf\u00fcllten Beben", "tokens": ["Wer", "dem", "mach\u00b7ter\u00b7f\u00fcll\u00b7ten", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihrer T\u00f6ne einst gelauscht;", "tokens": ["Ih\u00b7rer", "T\u00f6\u00b7ne", "einst", "ge\u00b7lauscht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "H\u00f6rt, wie jetzt zerriss'nes Leben", "tokens": ["H\u00f6rt", ",", "wie", "jetzt", "zer\u00b7riss'\u00b7nes", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "In gebroch'nen Kl\u00e4ngen rauscht.", "tokens": ["In", "ge\u00b7bro\u00b7ch'\u00b7nen", "Kl\u00e4n\u00b7gen", "rauscht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Sch\u00f6ne Tage, kehret wieder!", "tokens": ["Sch\u00f6\u00b7ne", "Ta\u00b7ge", ",", "keh\u00b7ret", "wie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bringt das Herrliche zur\u00fcck!", "tokens": ["Bringt", "das", "Herr\u00b7li\u00b7che", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seiner Freiheit wilde Lieder;", "tokens": ["Sei\u00b7ner", "Frei\u00b7heit", "wil\u00b7de", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seiner Liebe mildes Gl\u00fcck!", "tokens": ["Sei\u00b7ner", "Lie\u00b7be", "mil\u00b7des", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, vergessen war mein Dulden,", "tokens": ["Ja", ",", "ver\u00b7ges\u00b7sen", "war", "mein", "Dul\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und vergeben mein Verschulden!", "tokens": ["Und", "ver\u00b7ge\u00b7ben", "mein", "Ver\u00b7schul\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Deiner Lehre treuer J\u00fcnger", "tokens": ["Dei\u00b7ner", "Leh\u00b7re", "treu\u00b7er", "J\u00fcn\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weint' ich keinem Gl\u00fccke nach,", "tokens": ["Weint'", "ich", "kei\u00b7nem", "Gl\u00fc\u00b7cke", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Denn ein neuer Freudenbringer", "tokens": ["Denn", "ein", "neu\u00b7er", "Freu\u00b7den\u00b7brin\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Stieg empor der neue Tag.", "tokens": ["Stieg", "em\u00b7por", "der", "neu\u00b7e", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sprach'st Du mir von M\u00e4nnerw\u00fcrde,", "tokens": ["Sprach'st", "Du", "mir", "von", "M\u00e4n\u00b7ner\u00b7w\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Freiheit Herrlichkeit,", "tokens": ["Von", "der", "Frei\u00b7heit", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warf ich eig'ner Sorgen B\u00fcrde", "tokens": ["Warf", "ich", "eig'\u00b7ner", "Sor\u00b7gen", "B\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In das weite Meer der Zeit.", "tokens": ["In", "das", "wei\u00b7te", "Meer", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Erst der Freiheit Sonne scheint;", "tokens": ["Erst", "der", "Frei\u00b7heit", "Son\u00b7ne", "scheint", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird das Weib mit gleichen Rechten", "tokens": ["Wird", "das", "Weib", "mit", "glei\u00b7chen", "Rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nimmer lausch' ich mehr dem Worte,", "tokens": ["Nim\u00b7mer", "lausch'", "ich", "mehr", "dem", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das mein Innerstes durchklang;", "tokens": ["Das", "mein", "In\u00b7ners\u00b7tes", "durch\u00b7klang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Pochend an der Zukunft Pforte", "tokens": ["Po\u00b7chend", "an", "der", "Zu\u00b7kunft", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Jugend Thatendrang,", "tokens": ["In", "der", "Ju\u00b7gend", "Tha\u00b7ten\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Raubend von des Himmels Heerde", "tokens": ["Rau\u00b7bend", "von", "des", "Him\u00b7mels", "Heer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Licht und Feuer f\u00fcr die Erde.", "tokens": ["Licht", "und", "Feu\u00b7er", "f\u00fcr", "die", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Solcher Liebe hei\u00dfes Werben", "tokens": ["Sol\u00b7cher", "Lie\u00b7be", "hei\u00b7\u00dfes", "Wer\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wurde rasch des Friedens Grab;", "tokens": ["Wur\u00b7de", "rasch", "des", "Frie\u00b7dens", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und in seliges Verderben", "tokens": ["Und", "in", "se\u00b7li\u00b7ges", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "St\u00fcrzt' ich freudig mich hinab. \u2013", "tokens": ["St\u00fcrzt'", "ich", "freu\u00b7dig", "mich", "hin\u00b7ab", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "PTKVZ", "$.", "$("], "meter": "+-