{"dta.poem.8880": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XiII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Die kalte Welt mit ihrem falschen Treiben?", "tokens": ["Die", "kal\u00b7te", "Welt", "mit", "ih\u00b7rem", "fal\u00b7schen", "Trei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kann sie es fesseln oder es vertreiben?", "tokens": ["Kann", "sie", "es", "fes\u00b7seln", "o\u00b7der", "es", "ver\u00b7trei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kann sie uns trennen oder uns verbinden?", "tokens": ["Kann", "sie", "uns", "tren\u00b7nen", "o\u00b7der", "uns", "ver\u00b7bin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir sehn die Dinge rings um uns verschwinden,", "tokens": ["Wir", "sehn", "die", "Din\u00b7ge", "rings", "um", "uns", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als Dinge, die die Liebe nur umschreiben;", "tokens": ["Als", "Din\u00b7ge", ",", "die", "die", "Lie\u00b7be", "nur", "um\u00b7schrei\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verborgen mu\u00df die wahre Liebe bleiben,", "tokens": ["Ver\u00b7bor\u00b7gen", "mu\u00df", "die", "wah\u00b7re", "Lie\u00b7be", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein Dritter darf zu dir und mir sich finden.", "tokens": ["Kein", "Drit\u00b7ter", "darf", "zu", "dir", "und", "mir", "sich", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "APPR", "PPER", "KON", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie, die uns wandeln sehn im bunten Schwarme,", "tokens": ["Sie", ",", "die", "uns", "wan\u00b7deln", "sehn", "im", "bun\u00b7ten", "Schwar\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht ahnen sollen sie, da\u00df in der Stille", "tokens": ["Nicht", "ah\u00b7nen", "sol\u00b7len", "sie", ",", "da\u00df", "in", "der", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wir uns verzehren im verliebten Harme.", "tokens": ["Wir", "uns", "ver\u00b7zeh\u00b7ren", "im", "ver\u00b7lieb\u00b7ten", "Har\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vergessen will ich jede fremde Grille,", "tokens": ["Ver\u00b7ges\u00b7sen", "will", "ich", "je\u00b7de", "frem\u00b7de", "Gril\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn dich umschlingen meine frohen Arme,", "tokens": ["Wenn", "dich", "um\u00b7schlin\u00b7gen", "mei\u00b7ne", "fro\u00b7hen", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dir allein beugt sich mein Eigenwille.", "tokens": ["Und", "dir", "al\u00b7lein", "beugt", "sich", "mein", "Ei\u00b7gen\u00b7wil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}