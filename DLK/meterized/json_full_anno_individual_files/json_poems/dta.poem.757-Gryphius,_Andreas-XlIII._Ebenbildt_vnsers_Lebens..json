{"dta.poem.757": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlIII.  Ebenbildt vnsers Lebens.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch das spiel der zeit/ spielt weil er alhie lebt. ", "tokens": ["Der", "Mensch", "das", "spiel", "der", "zeit", "/", "spielt", "weil", "er", "al\u00b7hie", "lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDS", "VVFIN", "ART", "NN", "$(", "VVFIN", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jm Schaw-platz dieser welt; er sitzt/ vnd doch nicht feste.", "tokens": ["Jm", "Scha\u00b7wplatz", "die\u00b7ser", "welt", ";", "er", "sitzt", "/", "vnd", "doch", "nicht", "fes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "$.", "PPER", "VVFIN", "$(", "KON", "ADV", "PTKNEG", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der steigt vnd jener f\u00e4lt/ der suchet die Pal\u00e4ste/ ", "tokens": ["Der", "steigt", "vnd", "je\u00b7ner", "f\u00e4lt", "/", "der", "su\u00b7chet", "die", "Pa\u00b7l\u00e4s\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "PDS", "VVFIN", "$(", "ART", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd der ein schlechtes dach/ der herscht vnd jener webt.", "tokens": ["Vnd", "der", "ein", "schlech\u00b7tes", "dach", "/", "der", "herscht", "vnd", "je\u00b7ner", "webt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "PAV", "$(", "ART", "NN", "KON", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was gestern war ist hin/ was jtzt das gl\u00fcck erhebt;", "tokens": ["Was", "ge\u00b7stern", "war", "ist", "hin", "/", "was", "jtzt", "das", "gl\u00fcck", "er\u00b7hebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "VAFIN", "ADV", "$(", "PWS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wirdt morgen vntergehn/ die vorhin gr\u00fcnen \u00e4ste", "tokens": ["Wirdt", "mor\u00b7gen", "vn\u00b7ter\u00b7gehn", "/", "die", "vor\u00b7hin", "gr\u00fc\u00b7nen", "\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "VVINF", "$(", "ART", "ADV", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sindt nunmehr d\u00fcr vnd todt/ wir armen sindt nur g\u00e4ste", "tokens": ["Sindt", "nun\u00b7mehr", "d\u00fcr", "vnd", "todt", "/", "wir", "ar\u00b7men", "sindt", "nur", "g\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADJD", "$(", "PPER", "ADJA", "VVFIN", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob den ein scharffes schwerdt an zarter seide schwebt.", "tokens": ["Ob", "den", "ein", "scharf\u00b7fes", "schwerdt", "an", "zar\u00b7ter", "sei\u00b7de", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wir sindt zwar gleich am fleisch/ doch nicht von gleichem", "tokens": ["Wir", "sindt", "zwar", "gleich", "am", "fleisch", "/", "doch", "nicht", "von", "glei\u00b7chem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKA", "ADJD", "$(", "ADV", "PTKNEG", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "stande", "tokens": ["stan\u00b7de"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der tr\u00e4gt ein purpur-kleidt/ vnd jener gr\u00e4bt im Sande/", "tokens": ["Der", "tr\u00e4gt", "ein", "pur\u00b7pur\u00b7kleidt", "/", "vnd", "je\u00b7ner", "gr\u00e4bt", "im", "San\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJD", "$(", "KON", "PDS", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bi\u00df nach entraubtem schmuck/ der Tod vns gleiche macht.", "tokens": ["Bi\u00df", "nach", "en\u00b7traub\u00b7tem", "schmuck", "/", "der", "Tod", "vns", "glei\u00b7che", "macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "NN", "$(", "ART", "NN", "PPER", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Spilt denn dis ernste spiel: weil es die zeit noch leidet.", "tokens": ["Spilt", "denn", "dis", "erns\u00b7te", "spiel", ":", "weil", "es", "die", "zeit", "noch", "lei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "PDS", "ADJA", "VVFIN", "$.", "KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd lernt: das wenn man vom pancket des lebens scheidet:", "tokens": ["Vnd", "lernt", ":", "das", "wenn", "man", "vom", "pan\u00b7cket", "des", "le\u00b7bens", "schei\u00b7det", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ART", "KOUS", "PIS", "APPRART", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Kron/ weisheit/ st\u00e4rck vnd gut/ sey ein geborgter pracht.", "tokens": ["Kron", "/", "weis\u00b7heit", "/", "st\u00e4rck", "vnd", "gut", "/", "sey", "ein", "ge\u00b7borg\u00b7ter", "pracht", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "$(", "ADJD", "KON", "ADJD", "$(", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}