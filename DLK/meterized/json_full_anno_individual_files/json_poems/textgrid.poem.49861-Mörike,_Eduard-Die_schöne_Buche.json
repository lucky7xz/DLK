": "G\u00fcrtel, o Einsamkeit, f\u00fchlt ich und dachte nur dich!", "tokens": ["G\u00fcr\u00b7tel", ",", "o", "Ein\u00b7sam\u00b7keit", ",", "f\u00fchlt", "ich", "und", "dach\u00b7te", "nur", "dich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "KON", "VVFIN", "ADV", "PPER", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}