{"textgrid.poem.49861": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Die sch\u00f6ne Buche", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ganz verborgen im Wald kenn ich ein Pl\u00e4tzchen, da stehet", "tokens": ["Ganz", "ver\u00b7bor\u00b7gen", "im", "Wald", "kenn", "ich", "ein", "Pl\u00e4tz\u00b7chen", ",", "da", "ste\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVPP", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Eine Buche, man sieht sch\u00f6ner im Bilde sie nicht.", "tokens": ["Ei\u00b7ne", "Bu\u00b7che", ",", "man", "sieht", "sch\u00f6\u00b7ner", "im", "Bil\u00b7de", "sie", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIS", "VVFIN", "ADJD", "APPRART", "NN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Rein und glatt, in gediegenem Wuchs erhebt sie sich einzeln,", "tokens": ["Rein", "und", "glatt", ",", "in", "ge\u00b7die\u00b7ge\u00b7nem", "Wuchs", "er\u00b7hebt", "sie", "sich", "ein\u00b7zeln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Keiner der Nachbarn r\u00fchrt ihr an den seidenen Schmuck.", "tokens": ["Kei\u00b7ner", "der", "Nach\u00b7barn", "r\u00fchrt", "ihr", "an", "den", "sei\u00b7de\u00b7nen", "Schmuck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Rings, so weit sein Gezweig der stattliche Baum ausbreitet,", "tokens": ["Rings", ",", "so", "weit", "sein", "Ge\u00b7zweig", "der", "statt\u00b7li\u00b7che", "Baum", "aus\u00b7brei\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Gr\u00fcnet der Rasen, das Aug still zu erquicken, umher;", "tokens": ["Gr\u00fc\u00b7net", "der", "Ra\u00b7sen", ",", "das", "Aug", "still", "zu", "er\u00b7qui\u00b7cken", ",", "um\u00b7her", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Gleich nach allen Seiten umzirkt er den Stamm in der Mitte;", "tokens": ["Gleich", "nach", "al\u00b7len", "Sei\u00b7ten", "um\u00b7zirkt", "er", "den", "Stamm", "in", "der", "Mit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Kunstlos schuf die Natur selber dies liebliche Rund.", "tokens": ["Kunst\u00b7los", "schuf", "die", "Na\u00b7tur", "sel\u00b7ber", "dies", "lieb\u00b7li\u00b7che", "Rund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADV", "PDS", "ADJA", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Zartes Geb\u00fcsch umkr\u00e4nzet es erst; hochst\u00e4mmige B\u00e4ume,", "tokens": ["Zar\u00b7tes", "Ge\u00b7b\u00fcsch", "um\u00b7kr\u00e4n\u00b7zet", "es", "erst", ";", "hoch\u00b7st\u00e4m\u00b7mi\u00b7ge", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "$.", "ADJA", "NN", "$,"], "meter": "+--+-+---+-+-+-", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Folgend in dichtem Gedr\u00e4ng, wehren dem himmlischen Blau.", "tokens": ["Fol\u00b7gend", "in", "dich\u00b7tem", "Ge\u00b7dr\u00e4ng", ",", "weh\u00b7ren", "dem", "himm\u00b7li\u00b7schen", "Blau", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.11": {"text": "Neben der dunkleren F\u00fclle des Eichbaums wieget die Birke", "tokens": ["Ne\u00b7ben", "der", "dunk\u00b7le\u00b7ren", "F\u00fcl\u00b7le", "des", "Eich\u00b7baums", "wie\u00b7get", "die", "Bir\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Ihr jungfr\u00e4uliches Haupt sch\u00fcchtern im goldenen Licht.", "tokens": ["Ihr", "jung\u00b7fr\u00e4u\u00b7li\u00b7ches", "Haupt", "sch\u00fcch\u00b7tern", "im", "gol\u00b7de\u00b7nen", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Nur wo, verdeckt vom Felsen, der Fu\u00dfsteig j\u00e4h sich hinabschlingt,", "tokens": ["Nur", "wo", ",", "ver\u00b7deckt", "vom", "Fel\u00b7sen", ",", "der", "Fu\u00df\u00b7steig", "j\u00e4h", "sich", "hin\u00b7ab\u00b7schlingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "$,", "VVFIN", "APPRART", "NN", "$,", "ART", "NN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "L\u00e4sset die Hellung mich ahnen das offene Feld.", "tokens": ["L\u00e4s\u00b7set", "die", "Hel\u00b7lung", "mich", "ah\u00b7nen", "das", "of\u00b7fe\u00b7ne", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.15": {"text": "\u2013 Als ich unl\u00e4ngst einsam, von neuen Gestalten des Sommers", "tokens": ["\u2013", "Als", "ich", "un\u00b7l\u00e4ngst", "ein\u00b7sam", ",", "von", "neu\u00b7en", "Ge\u00b7stal\u00b7ten", "des", "Som\u00b7mers"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "PPER", "ADV", "ADJD", "$,", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Ab dem Pfade gelockt, dort im Geb\u00fcsch mich verlor,", "tokens": ["Ab", "dem", "Pfa\u00b7de", "ge\u00b7lockt", ",", "dort", "im", "Ge\u00b7b\u00fcsch", "mich", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "F\u00fchrt' ein freundlicher Geist, des Hains auflauschende Gottheit,", "tokens": ["F\u00fchrt'", "ein", "freund\u00b7li\u00b7cher", "Geist", ",", "des", "Hains", "auf\u00b7lau\u00b7schen\u00b7de", "Got\u00b7theit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Hier mich zum erstenmal, pl\u00f6tzlich, den Staunenden, ein.", "tokens": ["Hier", "mich", "zum", "ers\u00b7ten\u00b7mal", ",", "pl\u00f6tz\u00b7lich", ",", "den", "Stau\u00b7nen\u00b7den", ",", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "ADV", "$,", "ADJD", "$,", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Welch Entz\u00fccken! Es war um die hohe Stunde des Mittags,", "tokens": ["Welch", "Ent\u00b7z\u00fc\u00b7cken", "!", "Es", "war", "um", "die", "ho\u00b7he", "Stun\u00b7de", "des", "Mit\u00b7tags", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Lautlos alles, es schwieg selber der Vogel im Laub.", "tokens": ["Laut\u00b7los", "al\u00b7les", ",", "es", "schwieg", "sel\u00b7ber", "der", "Vo\u00b7gel", "im", "Laub", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIS", "$,", "PPER", "VVFIN", "ADV", "ART", "NE", "APPRART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Und ich zauderte noch, auf den zierlichen Teppich zu treten;", "tokens": ["Und", "ich", "zau\u00b7der\u00b7te", "noch", ",", "auf", "den", "zier\u00b7li\u00b7chen", "Tep\u00b7pich", "zu", "tre\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "--+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Festlich empfing er den Fu\u00df, leise beschritt ich ihn nur.", "tokens": ["Fest\u00b7lich", "emp\u00b7fing", "er", "den", "Fu\u00df", ",", "lei\u00b7se", "be\u00b7schritt", "ich", "ihn", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,", "ADJD", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Jetzo, gelehnt an den Stamm (er tr\u00e4gt sein breites Gew\u00f6lbe", "tokens": ["Jet\u00b7zo", ",", "ge\u00b7lehnt", "an", "den", "Stamm", "(", "er", "tr\u00e4gt", "sein", "brei\u00b7tes", "Ge\u00b7w\u00f6l\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVPP", "APPR", "ART", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Nicht zu hoch), lie\u00df ich rundum die Augen ergehn,", "tokens": ["Nicht", "zu", "hoch", ")", ",", "lie\u00df", "ich", "run\u00b7dum", "die", "Au\u00b7gen", "er\u00b7gehn", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKA", "ADJD", "$(", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.25": {"text": "Wo den beschatteten Kreis die feurig strahlende Sonne,", "tokens": ["Wo", "den", "be\u00b7schat\u00b7te\u00b7ten", "Kreis", "die", "feu\u00b7rig", "strah\u00b7len\u00b7de", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Fast gleich messend umher, s\u00e4umte mit blendendem Rand.", "tokens": ["Fast", "gleich", "mes\u00b7send", "um\u00b7her", ",", "s\u00e4um\u00b7te", "mit", "blen\u00b7den\u00b7dem", "Rand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PTKVZ", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Aber ich stand und r\u00fchrte mich nicht; d\u00e4monischer Stille,", "tokens": ["A\u00b7ber", "ich", "stand", "und", "r\u00fchr\u00b7te", "mich", "nicht", ";", "d\u00e4\u00b7mo\u00b7ni\u00b7scher", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "$.", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.28": {"text": "Unergr\u00fcndlicher Ruh lauschte mein innerer Sinn.", "tokens": ["Un\u00b7er\u00b7gr\u00fcnd\u00b7li\u00b7cher", "Ruh", "lauschte", "mein", "in\u00b7ne\u00b7rer", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Eingeschlossen mit dir in diesem sonnigen Zauber-", "tokens": ["Ein\u00b7ge\u00b7schlos\u00b7sen", "mit", "dir", "in", "die\u00b7sem", "son\u00b7ni\u00b7gen", "Zau\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "APPR", "PDAT", "ADJA", "TRUNC"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.30": {"text": "G\u00fcrtel, o Einsamkeit, f\u00fchlt ich und dachte nur dich!", "tokens": ["G\u00fcr\u00b7tel", ",", "o", "Ein\u00b7sam\u00b7keit", ",", "f\u00fchlt", "ich", "und", "dach\u00b7te", "nur", "dich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "KON", "VVFIN", "ADV", "PPER", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}, "stanza.2": {"line.1": {"text": "Ganz verborgen im Wald kenn ich ein Pl\u00e4tzchen, da stehet", "tokens": ["Ganz", "ver\u00b7bor\u00b7gen", "im", "Wald", "kenn", "ich", "ein", "Pl\u00e4tz\u00b7chen", ",", "da", "ste\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVPP", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Eine Buche, man sieht sch\u00f6ner im Bilde sie nicht.", "tokens": ["Ei\u00b7ne", "Bu\u00b7che", ",", "man", "sieht", "sch\u00f6\u00b7ner", "im", "Bil\u00b7de", "sie", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIS", "VVFIN", "ADJD", "APPRART", "NN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Rein und glatt, in gediegenem Wuchs erhebt sie sich einzeln,", "tokens": ["Rein", "und", "glatt", ",", "in", "ge\u00b7die\u00b7ge\u00b7nem", "Wuchs", "er\u00b7hebt", "sie", "sich", "ein\u00b7zeln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Keiner der Nachbarn r\u00fchrt ihr an den seidenen Schmuck.", "tokens": ["Kei\u00b7ner", "der", "Nach\u00b7barn", "r\u00fchrt", "ihr", "an", "den", "sei\u00b7de\u