 ["ADJD", "VVFIN", "ART", "NN", "ADV", "PDS", "ADJA", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Zartes Geb\u00fcsch umkr\u00e4nzet es erst; hochst\u00e4mmige B\u00e4ume,", "tokens": ["Zar\u00b7tes", "Ge\u00b7b\u00fcsch", "um\u00b7kr\u00e4n\u00b7zet", "es", "erst", ";", "hoch\u00b7st\u00e4m\u00b7mi\u00b7ge", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "$.", "ADJA", "NN", "$,"], "meter": "+--+-+---+-+-+-", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Folgend in dichtem Gedr\u00e4ng, wehren dem himmlischen Blau.", "tokens": ["Fol\u00b7gend", "in", "dich\u00b7tem", "Ge\u00b7dr\u00e4ng", ",", "weh\u00b7ren", "dem", "himm\u00b7li\u00b7schen", "Blau", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.11": {"text": "Neben der dunkleren F\u00fclle des Eichbaums wieget die Birke", "tokens": ["Ne\u00b7ben", "der", "dunk\u00b7le\u00b7ren", "F\u00fcl\u00b7le", "des", "Eich\u00b7baums", "wie\u00b7get", "die", "Bir\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Ihr jungfr\u00e4uliches Haupt sch\u00fcchtern im goldenen Licht.", "tokens": ["Ihr", "jung\u00b7fr\u00e4u\u00b7li\u00b7ches", "Haupt", "sch\u00fcch\u00b7tern", "im", "gol\u00b7de\u00b7nen", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Nur wo, verdeckt vom Felsen, der Fu\u00dfsteig j\u00e4h sich hinabschlingt,", "tokens": ["Nur", "wo", ",", "ver\u00b7deckt", "vom", "Fel\u00b7sen", ",", "der", "Fu\u00df\u00b7steig", "j\u00e4h", "sich", "hin\u00b7ab\u00b7schlingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "$,", "VVFIN", "APPRART", "NN", "$,", "ART", "NN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "L\u00e4sset die Hellung mich ahnen das offene Feld.", "tokens": ["L\u00e4s\u00b7set", "die", "Hel\u00b7lung", "mich", "ah\u00b7nen", "das", "of\u00b7fe\u00b7ne", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.15": {"text": "\u2013 Als ich unl\u00e4ngst einsam, von neuen Gestalten des Sommers", "tokens": ["\u2013", "Als", "ich", "un\u00b7l\u00e4ngst", "ein\u00b7sam", ",", "von", "neu\u00b7en", "Ge\u00b7stal\u00b7ten", "des", "Som\u00b7mers"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "PPER", "ADV", "ADJD", "$,", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Ab dem Pfade gelockt, dort im Geb\u00fcsch mich verlor,", "tokens": ["Ab", "dem", "Pfa\u00b7de", "ge\u00b7lockt", ",", "dort", "im", "Ge\u00b7b\u00fcsch", "mich", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "F\u00fchrt' ein freundlicher Geist, des Hains auflauschende Gottheit,", "tokens": ["F\u00fchrt'", "ein", "freund\u00b7li\u00b7cher", "Geist", ",", "des", "Hains", "auf\u00b7lau\u00b7schen\u00b7de", "Got\u00b7theit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Hier mich zum erstenmal, pl\u00f6tzlich, den Staunenden, ein.", "tokens": ["Hier", "mich", "zum", "ers\u00b7ten\u00b7mal", ",", "pl\u00f6tz\u00b7lich", ",", "den", "Stau\u00b7nen\u00b7den", ",", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "ADV", "$,", "ADJD", "$,", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Welch Entz\u00fccken! Es war um die hohe Stunde des Mittags,", "tokens": ["Welch", "Ent\u00b7z\u00fc\u00b7cken", "!", "Es", "war", "um", "die", "ho\u00b7he", "Stun\u00b7de", "des", "Mit\u00b7tags", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Lautlos alles, es schwieg selber der Vogel im Laub.", "tokens": ["Laut\u00b7los", "al\u00b7les", ",", "es", "schwieg", "sel\u00b7ber", "der", "Vo\u00b7gel", "im", "Laub", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIS", "$,", "PPER", "VVFIN", "ADV", "ART", "NE", "APPRART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Und ich zauderte noch, auf den zierlichen Teppich zu treten;", "tokens": ["Und", "ich", "zau\u00b7der\u00b7te", "noch", ",", "auf", "den", "zier\u00b7li\u00b7chen", "Tep\u00b7pich", "zu", "tre\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "--+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Festlich empfing er den Fu\u00df, leise beschritt ich ihn nur.", "tokens": ["Fest\u00b7lich", "emp\u00b7fing", "er", "den", "Fu\u00df", ",", "lei\u00b7se", "be\u00b7schritt", "ich", "ihn", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,", "ADJD", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Jetzo, gelehnt an den Stamm (er tr\u00e4gt sein breites Gew\u00f6lbe", "tokens": ["Jet\u00b7zo", ",", "ge\u00b7lehnt", "an", "den", "Stamm", "(", "er", "tr\u00e4gt", "sein", "brei\u00b7tes", "Ge\u00b7w\u00f6l\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVPP", "APPR", "ART", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Nicht zu hoch), lie\u00df ich rundum die Augen ergehn,", "tokens": ["Nicht", "zu", "hoch", ")", ",", "lie\u00df", "ich", "run\u00b7dum", "die", "Au\u00b7gen", "er\u00b7gehn", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKA", "ADJD", "$(", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.25": {"text": "Wo den beschatteten Kreis die feurig strahlende Sonne,", "tokens": ["Wo", "den", "be\u00b7schat\u00b7te\u00b7ten", "Kreis", "die", "feu\u00b7rig", "strah\u00b7len\u00b7de", "Son\u00b