{"textgrid.poem.67602": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdu singst, Frau Nachtigall (darum gef\u00e4llst Du mir),", "tokens": ["\u00bb", "du", "singst", ",", "Frau", "Nach\u00b7ti\u00b7gall", "(", "da\u00b7rum", "ge\u00b7f\u00e4llst", "Du", "mir", ")", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "NN", "NN", "$(", "PAV", "VVFIN", "PPER", "PPER", "$(", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ganze Jahr nicht mehr als wenig Wochen,\u00ab", "tokens": ["Das", "gan\u00b7ze", "Jahr", "nicht", "mehr", "als", "we\u00b7nig", "Wo\u00b7chen", ",", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "ADV", "KOKOM", "PIAT", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So kam der Kuckuk an die Th\u00fcr", "tokens": ["So", "kam", "der", "Kuc\u00b7kuk", "an", "die", "Th\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der S\u00e4ngerin sanft angesprochen.", "tokens": ["Der", "S\u00e4n\u00b7ge\u00b7rin", "sanft", "an\u00b7ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbund ich darinnen folge Dir!", "tokens": ["\u00bb", "und", "ich", "da\u00b7rin\u00b7nen", "fol\u00b7ge", "Dir", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich sing' auch kurze Zeit; warum? um sch\u00f6n zu singen.", "tokens": ["Ich", "sing'", "auch", "kur\u00b7ze", "Zeit", ";", "wa\u00b7rum", "?", "um", "sch\u00f6n", "zu", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$.", "PWAV", "$.", "APPR", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich folg' im Singen der Natur;", "tokens": ["Ich", "fol\u00b7g'", "im", "Sin\u00b7gen", "der", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Denn die Natur l\u00e4\u00dft sich mit Nichten zwingen;", "tokens": ["Denn", "die", "Na\u00b7tur", "l\u00e4\u00dft", "sich", "mit", "Nich\u00b7ten", "zwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So lange sie gebeut, so lange sing' ich nur,", "tokens": ["So", "lan\u00b7ge", "sie", "ge\u00b7beut", ",", "so", "lan\u00b7ge", "sing'", "ich", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und wenn sie nicht gebeut, so h\u00f6r' ich auf zu singen;", "tokens": ["Und", "wenn", "sie", "nicht", "ge\u00b7beut", ",", "so", "h\u