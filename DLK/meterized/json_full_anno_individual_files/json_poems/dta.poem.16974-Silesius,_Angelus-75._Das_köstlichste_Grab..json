{"dta.poem.16974": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "75.  Das k\u00f6stlichste Grab.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Grab ist k\u00f6stlicher bi\u00df heute zu gewesen/", "tokens": ["Kein", "Grab", "ist", "k\u00f6st\u00b7li\u00b7cher", "bi\u00df", "heu\u00b7te", "zu", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "APPR", "ADV", "APPR", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als was von ", "tokens": ["Als", "was", "von"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIS", "APPR"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Und doch verlang\u2019 ichs nicht: ich w\u00fcnsche mir allein", "tokens": ["Und", "doch", "ver\u00b7lang'", "ichs", "nicht", ":", "ich", "w\u00fcn\u00b7sche", "mir", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In meines Heylands Scho\u00df tief einversenkt zu seyn.", "tokens": ["In", "mei\u00b7nes", "Hey\u00b7lands", "Scho\u00df", "tief", "ein\u00b7ver\u00b7senkt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ADJD", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}