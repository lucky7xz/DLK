{"dta.poem.23305": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An die deutschen Frauen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seid mir gegr\u00fc\u00dft, ihr deutschen Frauen,", "tokens": ["Seid", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "ihr", "deut\u00b7schen", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der sch\u00f6nern Zukunft Morgenroth!", "tokens": ["Der", "sch\u00f6\u00b7nern", "Zu\u00b7kunft", "Mor\u00b7gen\u00b7roth", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Vaterland in seiner Noth?", "tokens": ["Das", "Va\u00b7ter\u00b7land", "in", "sei\u00b7ner", "Noth", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr kennt noch frohe deutsche Weise,", "tokens": ["Ihr", "kennt", "noch", "fro\u00b7he", "deut\u00b7sche", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch deutsche Zucht und Sittsamkeit;", "tokens": ["Noch", "deut\u00b7sche", "Zucht", "und", "Sitt\u00b7sam\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Euch blieb in eurem stillen Kreise", "tokens": ["Euch", "blieb", "in", "eu\u00b7rem", "stil\u00b7len", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch Frohsinn und Zufriedenheit.", "tokens": ["Noch", "Froh\u00b7sinn", "und", "Zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr tragt noch nicht die bunten B\u00e4nder,", "tokens": ["Ihr", "tragt", "noch", "nicht", "die", "bun\u00b7ten", "B\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die man dem Staatsverdienste weiht;", "tokens": ["Die", "man", "dem", "Staats\u00b7ver\u00b7diens\u00b7te", "weiht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Euch sind noch eure Hausgew\u00e4nder", "tokens": ["Euch", "sind", "noch", "eu\u00b7re", "Haus\u00b7ge\u00b7w\u00e4n\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mehr werth als ein Beamtenkleid.", "tokens": ["Mehr", "werth", "als", "ein", "Be\u00b7am\u00b7ten\u00b7kleid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr seid noch nicht verlocket worden", "tokens": ["Ihr", "seid", "noch", "nicht", "ver\u00b7lo\u00b7cket", "wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "VVPP", "VAPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Titel oder andern Tand;", "tokens": ["Durch", "Ti\u00b7tel", "o\u00b7der", "an\u00b7dern", "Tand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Euch kann noch sein der sch\u00f6nste Orden:", "tokens": ["Euch", "kann", "noch", "sein", "der", "sch\u00f6ns\u00b7te", "Or\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Liebe f\u00fcr das Vaterland.", "tokens": ["Die", "Lie\u00b7be", "f\u00fcr", "das", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wohlan! ihr sollt im Kind' erwecken", "tokens": ["Wo\u00b7hlan", "!", "ihr", "sollt", "im", "Kind'", "er\u00b7we\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "PPER", "VMFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Sinn f\u00fcr Vaterland und Recht,", "tokens": ["Den", "Sinn", "f\u00fcr", "Va\u00b7ter\u00b7land", "und", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr sollt erziehn zum Feindesschrecken", "tokens": ["Ihr", "sollt", "er\u00b7ziehn", "zum", "Fein\u00b7des\u00b7schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein freies biederes Geschlecht.", "tokens": ["Ein", "frei\u00b7es", "bie\u00b7de\u00b7res", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Euch mu\u00df vertrau'n, auf euch mu\u00df bauen", "tokens": ["Euch", "mu\u00df", "ver\u00b7trau'n", ",", "auf", "euch", "mu\u00df", "bau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "APPR", "PPER", "VMFIN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Das Vaterland in seiner Noth!", "tokens": ["Das", "Va\u00b7ter\u00b7land", "in", "sei\u00b7ner", "Noth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seid mir gegr\u00fc\u00dft, ihr deutschen Frauen,", "tokens": ["Seid", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "ihr", "deut\u00b7schen", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der sch\u00f6nern Zukunft Morgenroth!", "tokens": ["Der", "sch\u00f6\u00b7nern", "Zu\u00b7kunft", "Mor\u00b7gen\u00b7roth", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}