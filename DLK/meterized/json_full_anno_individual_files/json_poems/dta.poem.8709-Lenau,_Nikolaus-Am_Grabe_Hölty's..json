{"dta.poem.8709": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Am Grabe H\u00f6lty's.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6lty! dein Freund, der Fr\u00fchling ist gekommen!", "tokens": ["H\u00f6l\u00b7ty", "!", "dein", "Freund", ",", "der", "Fr\u00fch\u00b7ling", "ist", "ge\u00b7kom\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Klagend irrt er im Haine, dich zu finden;", "tokens": ["Kla\u00b7gend", "irrt", "er", "im", "Hai\u00b7ne", ",", "dich", "zu", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Doch umsonst! sein klagender Ruf verhallt in", "tokens": ["Doch", "um\u00b7sonst", "!", "sein", "kla\u00b7gen\u00b7der", "Ruf", "ver\u00b7hallt", "in"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Einsamen Schatten!", "tokens": ["Ein\u00b7sa\u00b7men", "Schat\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Nimmer entgegen t\u00f6nen ihm die Lieder", "tokens": ["Nim\u00b7mer", "ent\u00b7ge\u00b7gen", "t\u00f6\u00b7nen", "ihm", "die", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "VVFIN", "PPER", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Deiner z\u00e4rtlichen, sch\u00f6nen Seele, nimmer", "tokens": ["Dei\u00b7ner", "z\u00e4rt\u00b7li\u00b7chen", ",", "sch\u00f6\u00b7nen", "See\u00b7le", ",", "nim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,", "ADV"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Freust des ersten Veilchens du dich, des ersten", "tokens": ["Freust", "des", "ers\u00b7ten", "Veil\u00b7chens", "du", "dich", ",", "des", "ers\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "PPER", "PRF", "$,", "ART", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Taubengegirres!", "tokens": ["Tau\u00b7ben\u00b7ge\u00b7gir\u00b7res", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Ach, an den H\u00fcgel sinkt er deines Grabes,", "tokens": ["Ach", ",", "an", "den", "H\u00fc\u00b7gel", "sinkt", "er", "dei\u00b7nes", "Gra\u00b7bes", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und umarmet ihn sehnsuchtsvoll: \u201emein S\u00e4nger", "tokens": ["Und", "um\u00b7ar\u00b7met", "ihn", "sehn\u00b7suchts\u00b7voll", ":", "\u201e", "mein", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$.", "$(", "PPOSAT", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Todt!\u201c So klagt sein fl\u00fcsternder Hauch dahin durch", "tokens": ["Todt", "!", "\u201c", "So", "klagt", "sein", "fl\u00fcs\u00b7tern\u00b7der", "Hauch", "da\u00b7hin", "durch"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "$(", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "PAV", "APPR"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "S\u00e4uselnde Blumen.", "tokens": ["S\u00e4u\u00b7seln\u00b7de", "Blu\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}