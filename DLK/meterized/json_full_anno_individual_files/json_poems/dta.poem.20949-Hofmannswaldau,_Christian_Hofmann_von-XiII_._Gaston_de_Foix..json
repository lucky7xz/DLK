{"dta.poem.20949": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XiII .  \n Gaston de Foix.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab in kurtzer Zeit dreymahl den Feind geschla-", "tokens": ["Ich", "hab", "in", "kurt\u00b7zer", "Zeit", "drey\u00b7mahl", "den", "Feind", "ge\u00b7schla"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "ADV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gen/", "tokens": ["gen", "/"], "token_info": ["word", "punct"], "pos": ["APPR", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Mein gr\u00fcnes Lorberblat das muste Purpur tragen.", "tokens": ["Mein", "gr\u00fc\u00b7nes", "Lor\u00b7berb\u00b7lat", "das", "mus\u00b7te", "Pur\u00b7pur", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Feindes Hand beschrieb selbst meinen Helden", "tokens": ["Des", "Fein\u00b7des", "Hand", "be\u00b7schrieb", "selbst", "mei\u00b7nen", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Muth/", "tokens": ["Muth", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Die Feder war ein Spie\u00df/ die Tinte war das Blut.", "tokens": ["Die", "Fe\u00b7der", "war", "ein", "Spie\u00df", "/", "die", "Tin\u00b7te", "war", "das", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$(", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}