{"textgrid.poem.39146": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als j\u00fcngst im gr\u00fcnen Hage", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als j\u00fcngst im gr\u00fcnen Hage", "tokens": ["Als", "j\u00fcngst", "im", "gr\u00fc\u00b7nen", "Ha\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Schlaf sich ein Dichter geletzt,", "tokens": ["Am", "Schlaf", "sich", "ein", "Dich\u00b7ter", "ge\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da hat das Fr\u00e4ulein Sage", "tokens": ["Da", "hat", "das", "Fr\u00e4u\u00b7lein", "Sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich neben ihn hingesetzt.", "tokens": ["Sich", "ne\u00b7ben", "ihn", "hin\u00b7ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Es war ein schmuckes Pfl\u00e4nzchen,", "tokens": ["Es", "war", "ein", "schmu\u00b7ckes", "Pfl\u00e4nz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nur etwas sehr kokett;", "tokens": ["Nur", "et\u00b7was", "sehr", "ko\u00b7kett", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Haare das Efeukr\u00e4nzchen,", "tokens": ["Im", "Haa\u00b7re", "das", "E\u00b7feu\u00b7kr\u00e4nz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das stand ihr gar zu nett.", "tokens": ["Das", "stand", "ihr", "gar", "zu", "nett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr Haar war lang und flachsen,", "tokens": ["Ihr", "Haar", "war", "lang", "und", "flach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Nacken war superb,", "tokens": ["Ihr", "Na\u00b7cken", "war", "su\u00b7perb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie war recht gut gewachsen,", "tokens": ["Sie", "war", "recht", "gut", "ge\u00b7wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur etwas gar zu derb.", "tokens": ["Nur", "et\u00b7was", "gar", "zu", "derb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Von Schminken und Sch\u00f6nheitspflastern,", "tokens": ["Von", "Schmin\u00b7ken", "und", "Sch\u00f6n\u00b7heits\u00b7pflas\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da ward dem Dichter nichts kund;", "tokens": ["Da", "ward", "dem", "Dich\u00b7ter", "nichts", "kund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PIS", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ihr Busen war alabastern,", "tokens": ["Ihr", "Bu\u00b7sen", "war", "a\u00b7la\u00b7bas\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nur etwas gar zu rund.", "tokens": ["Nur", "et\u00b7was", "gar", "zu", "rund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr Aug' war tief und n\u00e4chtig,", "tokens": ["Ihr", "Aug'", "war", "tief", "und", "n\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nur etwas gar zu gro\u00df,", "tokens": ["Nur", "et\u00b7was", "gar", "zu", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie trug sich reich und pr\u00e4chtig,", "tokens": ["Sie", "trug", "sich", "reich", "und", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur etwas gar zu blo\u00df.", "tokens": ["Nur", "et\u00b7was", "gar", "zu", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie machten Wahlverwandtschaft,", "tokens": ["Sie", "mach\u00b7ten", "Wahl\u00b7ver\u00b7wandt\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Dichter war galant,", "tokens": ["Der", "Dich\u00b7ter", "war", "ga\u00b7lant", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie war bei n\u00e4hrer Bekanntschaft", "tokens": ["Sie", "war", "bei", "n\u00e4h\u00b7rer", "Be\u00b7kannt\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ausnehmend interessant.", "tokens": ["Aus\u00b7neh\u00b7mend", "in\u00b7ter\u00b7es\u00b7sant", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Viel Bilder, alt' und neue,", "tokens": ["Viel", "Bil\u00b7der", ",", "alt'", "und", "neu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADJA", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die malte sie frisch und gut,", "tokens": ["Die", "mal\u00b7te", "sie", "frisch", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Blaue mit Augenbl\u00e4ue,", "tokens": ["Das", "Blau\u00b7e", "mit", "Au\u00b7gen\u00b7bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Rote mit Heldenblut.", "tokens": ["Das", "Ro\u00b7te", "mit", "Hel\u00b7den\u00b7blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Das Gr\u00fcne mit Schmelz der Triften,", "tokens": ["Das", "Gr\u00fc\u00b7ne", "mit", "Schmelz", "der", "Trif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Goldne mit Sonnenpracht,", "tokens": ["Das", "Gold\u00b7ne", "mit", "Son\u00b7nen\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Helle mit Himmelsl\u00fcften,", "tokens": ["Das", "Hel\u00b7le", "mit", "Him\u00b7mel\u00b7sl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Dunkle mit Waldesnacht.", "tokens": ["Das", "Dunk\u00b7le", "mit", "Wal\u00b7des\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Sie erz\u00e4hlte lange Geschichten,", "tokens": ["Sie", "er\u00b7z\u00e4hl\u00b7te", "lan\u00b7ge", "Ge\u00b7schich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Geschichten von Lust und Weh,", "tokens": ["Ge\u00b7schich\u00b7ten", "von", "Lust", "und", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von den Nixen, ihren Nichten,", "tokens": ["Von", "den", "Ni\u00b7xen", ",", "ih\u00b7ren", "Nich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von ihrer Tante, der Fee.", "tokens": ["Von", "ih\u00b7rer", "Tan\u00b7te", ",", "der", "Fee", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Sie sprach mit vielem Geschnatter,", "tokens": ["Sie", "sprach", "mit", "vie\u00b7lem", "Ge\u00b7schnat\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nach echter Fr\u00e4uleinsart,", "tokens": ["Nach", "ech\u00b7ter", "Fr\u00e4u\u00b7lein\u00b7sart", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von dem Kobold, ihrem Gevatter,", "tokens": ["Von", "dem", "Ko\u00b7bold", ",", "ih\u00b7rem", "Ge\u00b7vat\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und seinem langen Bart.", "tokens": ["Und", "sei\u00b7nem", "lan\u00b7gen", "Bart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Vom Strommann im Flutkristalle", "tokens": ["Vom", "Strom\u00b7mann", "im", "Flut\u00b7kris\u00b7tal\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Erz\u00e4hlte sie Zauberwerk,", "tokens": ["Er\u00b7z\u00e4hl\u00b7te", "sie", "Zau\u00b7ber\u00b7werk", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In des Berges Rubinenhalle,", "tokens": ["In", "des", "Ber\u00b7ges", "Ru\u00b7bi\u00b7nen\u00b7hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Da kannte sie jeden Zwerg.", "tokens": ["Da", "kann\u00b7te", "sie", "je\u00b7den", "Zwerg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Mit der Heinzelm\u00e4nnchen Gelichter,", "tokens": ["Mit", "der", "Hein\u00b7zel\u00b7m\u00e4nn\u00b7chen", "Ge\u00b7lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da hatte sie oft getost;", "tokens": ["Da", "hat\u00b7te", "sie", "oft", "ge\u00b7tost", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein jeder der toten Dichter,", "tokens": ["Ein", "je\u00b7der", "der", "to\u00b7ten", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der hatte mit ihr gekost.", "tokens": ["Der", "hat\u00b7te", "mit", "ihr", "ge\u00b7kost", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Ein jeder der toten Ritter,", "tokens": ["Ein", "je\u00b7der", "der", "to\u00b7ten", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das war ein Buhle von ihr,", "tokens": ["Das", "war", "ein", "Buh\u00b7le", "von", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie folgt' ihm ins Kampfgewitter,", "tokens": ["Sie", "folgt'", "ihm", "ins", "Kampf\u00b7ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als Fr\u00e4ulein Avent\u00fcr'.", "tokens": ["Als", "Fr\u00e4u\u00b7lein", "A\u00b7ven\u00b7t\u00fcr'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Dem Dichter t\u00e4ten gefallen", "tokens": ["Dem", "Dich\u00b7ter", "t\u00e4\u00b7ten", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nicht ganz die M\u00e4rchen der Fee,", "tokens": ["Nicht", "ganz", "die", "M\u00e4r\u00b7chen", "der", "Fee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er vermi\u00dfte in dem allen", "tokens": ["Er", "ver\u00b7mi\u00df\u00b7te", "in", "dem", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die politische Grundidee.", "tokens": ["Die", "po\u00b7li\u00b7ti\u00b7sche", "Grun\u00b7di\u00b7dee", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Er frug mit \u00e4ngstlichem Fl\u00fcstern", "tokens": ["Er", "frug", "mit", "\u00e4ngst\u00b7li\u00b7chem", "Fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u2013 Die Sache war riskant \u2013", "tokens": ["\u2013", "Die", "Sa\u00b7che", "war", "ris\u00b7kant", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nach den Elfen, ihren Geschwistern,", "tokens": ["Nach", "den", "El\u00b7fen", ",", "ih\u00b7ren", "Ge\u00b7schwis\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und den Dingen aus Elfenland.", "tokens": ["Und", "den", "Din\u00b7gen", "aus", "El\u00b7fen\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NE", "$."], "meter": "--+--+-+", "measure": "