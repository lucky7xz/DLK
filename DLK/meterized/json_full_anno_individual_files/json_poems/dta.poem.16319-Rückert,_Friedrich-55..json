{"dta.poem.16319": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das beste Lebensgut ist leichter froher Sinn,", "tokens": ["Das", "bes\u00b7te", "Le\u00b7bens\u00b7gut", "ist", "leich\u00b7ter", "fro\u00b7her", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit ihm ist kein Verlust und ohn' ihn kein Gewinn.", "tokens": ["Mit", "ihm", "ist", "kein", "Ver\u00b7lust", "und", "ohn'", "ihn", "kein", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIAT", "NN", "KON", "APPR", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch, ward dirs nicht so leicht, und ist dein Wesen schwerer,", "tokens": ["Doch", ",", "ward", "dirs", "nicht", "so", "leicht", ",", "und", "ist", "dein", "We\u00b7sen", "schwe\u00b7rer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PIS", "PTKNEG", "ADV", "ADJD", "$,", "KON", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So tr\u00f6stet dich villeicht ein Wort von deinem Lehrer:", "tokens": ["So", "tr\u00f6s\u00b7tet", "dich", "vil\u00b7leicht", "ein", "Wort", "von", "dei\u00b7nem", "Leh\u00b7rer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die dunkle Nelke, nicht die bunte Tulp' hat Duft,", "tokens": ["Die", "dunk\u00b7le", "Nel\u00b7ke", ",", "nicht", "die", "bun\u00b7te", "Tul\u00b7p'", "hat", "Duft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PTKNEG", "ART", "ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Und auch zum Himmel geht der Weg nur durch die Gruft.", "tokens": ["Und", "auch", "zum", "Him\u00b7mel", "geht", "der", "Weg", "nur", "durch", "die", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O scheu nicht durch die Gruft den Weg zu deinem Himmel;", "tokens": ["O", "scheu", "nicht", "durch", "die", "Gruft", "den", "Weg", "zu", "dei\u00b7nem", "Him\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKNEG", "APPR", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und la\u00df wer gehn will gehn durchs bunte Weltgewimmel.", "tokens": ["Und", "la\u00df", "wer", "gehn", "will", "gehn", "durchs", "bun\u00b7te", "Welt\u00b7ge\u00b7wim\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PWS", "VVINF", "VMFIN", "VVINF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "O scheu nur durch die Gruft den Weg zum Himme nicht!", "tokens": ["O", "scheu", "nur", "durch", "die", "Gruft", "den", "Weg", "zum", "Him\u00b7me", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADV", "APPR", "ART", "NN", "ART", "NN", "APPRART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Herzen dunkler Duft, im Auge sanftes Licht.", "tokens": ["Im", "Her\u00b7zen", "dunk\u00b7ler", "Duft", ",", "im", "Au\u00b7ge", "sanf\u00b7tes", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Im Auge sanftes Licht, im Herzen dunkler Duft;", "tokens": ["Im", "Au\u00b7ge", "sanf\u00b7tes", "Licht", ",", "im", "Her\u00b7zen", "dunk\u00b7ler", "Duft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du gehst, o bange nicht, zum Himmel durch die Gruft.", "tokens": ["Du", "gehst", ",", "o", "ban\u00b7ge", "nicht", ",", "zum", "Him\u00b7mel", "durch", "die", "Gruft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "ADV", "PTKNEG", "$,", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}