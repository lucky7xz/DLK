{"textgrid.poem.55971": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Gassen haben einen sachten Gang", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Gassen haben einen sachten Gang", "tokens": ["Die", "Gas\u00b7sen", "ha\u00b7ben", "ei\u00b7nen", "sach\u00b7ten", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "(wie manchmal Menschen gehen im Genesen", "tokens": ["(", "wie", "manch\u00b7mal", "Men\u00b7schen", "ge\u00b7hen", "im", "Ge\u00b7ne\u00b7sen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KOKOM", "ADV", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.3": {"text": "nachdenkend: was ist fr\u00fcher hier gewesen?)", "tokens": ["nach\u00b7den\u00b7kend", ":", "was", "ist", "fr\u00fc\u00b7her", "hier", "ge\u00b7we\u00b7sen", "?", ")"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "$.", "PWS", "VAFIN", "ADJD", "ADV", "VAPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und die an Pl\u00e4tze kommen, warten lang", "tokens": ["und", "die", "an", "Pl\u00e4t\u00b7ze", "kom\u00b7men", ",", "war\u00b7ten", "lang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "APPR", "NN", "VVINF", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "auf eine andre, die mit einem Schritt", "tokens": ["auf", "ei\u00b7ne", "and\u00b7re", ",", "die", "mit", "ei\u00b7nem", "Schritt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00fcber das abendklare Wasser tritt,", "tokens": ["\u00fc\u00b7ber", "das", "a\u00b7bend\u00b7kla\u00b7re", "Was\u00b7ser", "tritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "darin, je mehr sich rings die Dinge mildern,", "tokens": ["da\u00b7rin", ",", "je", "mehr", "sich", "rings", "die", "Din\u00b7ge", "mil\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ADV", "ADV", "PRF", "ADV", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die eingeh\u00e4ngte Welt von Spiegelbildern", "tokens": ["die", "ein\u00b7ge\u00b7h\u00e4ng\u00b7te", "Welt", "von", "Spie\u00b7gel\u00b7bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "so wirklich wird wie diese Dinge nie.", "tokens": ["so", "wirk\u00b7lich", "wird", "wie", "die\u00b7se", "Din\u00b7ge", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "KOKOM", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Verging nicht diese Stadt? Nun siehst du, wie", "tokens": ["Ver\u00b7ging", "nicht", "die\u00b7se", "Stadt", "?", "Nun", "siehst", "du", ",", "wie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PTKNEG", "PDAT", "NN", "$.", "ADV", "VVFIN", "PPER", "$,", "PWAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "(nach e