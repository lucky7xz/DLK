{"dta.poem.947": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "W ALLER   I M   S CHNEE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.71", "af:0.28"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Die blume die ich mir am fenster hege               ", "tokens": ["Die", "blu\u00b7me", "die", "ich", "mir", "am", "fens\u00b7ter", "he\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PPER", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verwahrt vorm froste in der grauen scherbe               ", "tokens": ["Ver\u00b7wahrt", "vorm", "fros\u00b7te", "in", "der", "grau\u00b7en", "scher\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPRART", "ADJA", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Betr&#252;bt mich nur trotz meiner guten pflege               ", "tokens": ["Be\u00b7tr", "&#252;", "bt", "mich", "nur", "trotz", "mei\u00b7ner", "gu\u00b7ten", "pfle\u00b7ge"], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und h&#228;ngt das haupt als ob sie langsam sterbe             ", "tokens": ["Und", "h", "&#228;", "ngt", "das", "haupt", "als", "ob", "sie", "lang\u00b7sam", "ster\u00b7be"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$(", "VVFIN", "ART", "NN", "KOKOM", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Um ihrer fr&#252;hern bl&#252;henden geschicke               ", "tokens": ["Um", "ih\u00b7rer", "fr", "&#252;", "hern", "bl", "&#252;", "hen\u00b7den", "ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "XML_entity", "word", "word"], "pos": ["KOUI", "PPOSAT", "FM", "FM", "FM", "FM", "$(", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erinnerung aus meinem sinn zu merzen               ", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", "aus", "mei\u00b7nem", "sinn", "zu", "mer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erw&#228;hl ich scharfe waffen und ich knicke               ", "tokens": ["Erw", "&#228;", "hl", "ich", "schar\u00b7fe", "waf\u00b7fen", "und", "ich", "kni\u00b7cke"], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "VVFIN", "PPER", "VVFIN", "NN", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die blasse blume mit dem kranken herzen             ", "tokens": ["Die", "blas\u00b7se", "blu\u00b7me", "mit", "dem", "kran\u00b7ken", "her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was soll sie nur zur bitternis mir taugen?               ", "tokens": ["Was", "soll", "sie", "nur", "zur", "bit\u00b7ter\u00b7nis", "mir", "tau\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich w&#252;nschte dass vom fenster sie verschw&#228;nde &#183; &#183;               ", "tokens": ["Ich", "w", "&#252;", "nschte", "dass", "vom", "fens\u00b7ter", "sie", "ver\u00b7schw", "&#228;", "nde", "&#183;", "&#183;"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "XML_entity", "XML_entity"], "pos": ["PPER", "NE", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun heb ich wieder meine leeren augen               ", "tokens": ["Nun", "heb", "ich", "wie\u00b7der", "mei\u00b7ne", "lee\u00b7ren", "au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und in die leere nacht die leeren h&#228;nde.             ", "tokens": ["Und", "in", "die", "lee\u00b7re", "nacht", "die", "lee\u00b7ren", "h", "&#228;", "nde", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "VVFIN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}}}}}