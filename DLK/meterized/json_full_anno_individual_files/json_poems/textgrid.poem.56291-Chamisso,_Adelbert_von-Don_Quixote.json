{"textgrid.poem.56291": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Don Quixote", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch ein Abenteuer,", "tokens": ["Noch", "ein", "A\u00b7bent\u00b7eu\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Welches Ruhm verspricht;", "tokens": ["Wel\u00b7ches", "Ruhm", "ver\u00b7spricht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Siehst du auf dem H\u00fcgel", "tokens": ["Siehst", "du", "auf", "dem", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dort die Riesen nicht?", "tokens": ["Dort", "die", "Rie\u00b7sen", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Turmhoch, mi\u00dfgeschaffen,", "tokens": ["Turm\u00b7hoch", ",", "mi\u00df\u00b7ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Drohend in den Wind,", "tokens": ["Dro\u00b7hend", "in", "den", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Welche anzuschauen", "tokens": ["Wel\u00b7che", "an\u00b7zu\u00b7schau\u00b7en"], "token_info": ["word", "word"], "pos": ["PWAT", "VVIZU"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Fast wie M\u00fchlen sind?", "tokens": ["Fast", "wie", "M\u00fch\u00b7len", "sind", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NN", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Mit Vergunst, Herr Ritter,", "tokens": ["Mit", "Ver\u00b7gunst", ",", "Herr", "Rit\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Kann ich da nur sehn", "tokens": ["Kann", "ich", "da", "nur", "sehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "M\u00fchlen, die im Winde", "tokens": ["M\u00fch\u00b7len", ",", "die", "im", "Win\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Ihre Fl\u00fcgel drehn.", "tokens": ["Ih\u00b7re", "Fl\u00fc\u00b7gel", "drehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Seien, feiger Knappe,", "tokens": ["Sei\u00b7en", ",", "fei\u00b7ger", "Knap\u00b7pe", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Deinem stumpfen Sinn", "tokens": ["Dei\u00b7nem", "stump\u00b7fen", "Sinn"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Diese Ungeheuer", "tokens": ["Die\u00b7se", "Un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "M\u00fchlen immerhin;", "tokens": ["M\u00fch\u00b7len", "im\u00b7mer\u00b7hin", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "H\u00fclle sich mit Trugschein", "tokens": ["H\u00fcl\u00b7le", "sich", "mit", "Trug\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Zauberhaft der Graus,", "tokens": ["Zau\u00b7ber\u00b7haft", "der", "Graus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Findet doch der Ritter", "tokens": ["Fin\u00b7det", "doch", "der", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Sich die Riesen aus.", "tokens": ["Sich", "die", "Rie\u00b7sen", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Mit Vergunst, Herr Ritter,", "tokens": ["Mit", "Ver\u00b7gunst", ",", "Herr", "Rit\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Glaubt's mir, auf mein Wort,", "tokens": ["Glaubt's", "mir", ",", "auf", "mein", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Das sind echte M\u00fchlen,", "tokens": ["Das", "sind", "ech\u00b7te", "M\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Auf dem H\u00fcgel dort.", "tokens": ["Auf", "dem", "H\u00fc\u00b7gel", "dort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "D\u00fcrft ihr's euch erfrechen,", "tokens": ["D\u00fcrft", "ih\u00b7r's", "euch", "er\u00b7fre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Haltet mir nur Stand,", "tokens": ["Hal\u00b7tet", "mir", "nur", "Stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Strau\u00df mit euresgleichen", "tokens": ["Strau\u00df", "mit", "eu\u00b7res\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ist mir Kindertand.", "tokens": ["Ist", "mir", "Kin\u00b7der\u00b7tand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Einer gegen alle,", "tokens": ["Ei\u00b7ner", "ge\u00b7gen", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PIS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Falsche H\u00f6llenbrut,", "tokens": ["Fal\u00b7sche", "H\u00f6l\u00b7len\u00b7brut", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und die Erde trinkt bald", "tokens": ["Und", "die", "Er\u00b7de", "trinkt", "bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Eures Herzens Blut.", "tokens": ["Eu\u00b7res", "Her\u00b7zens", "Blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Mit Vergunst, Herr Ritter,", "tokens": ["Mit", "Ver\u00b7gunst", ",", "Herr", "Rit\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "H\u00f6rt mich doch nur an,", "tokens": ["H\u00f6rt", "mich", "doch", "nur", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "M\u00fchlen sind's, nur M\u00fchlen,", "tokens": ["M\u00fch\u00b7len", "sin\u00b7d's", ",", "nur", "M\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ADV", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.12": {"text": "Wie ich schw\u00f6ren kann.", "tokens": ["Wie", "ich", "schw\u00f6\u00b7ren", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "S\u00fc\u00dfe Dulcinea,", "tokens": ["S\u00fc\u00b7\u00dfe", "Dul\u00b7ci\u00b7nea", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Blick auf mich herab!", "tokens": ["Blick", "auf", "mich", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "So der wackre Ritter,", "tokens": ["So", "der", "wack\u00b7re", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Spornt den Gaul in Trab;", "tokens": ["Spornt", "den", "Gaul", "in", "Trab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Treibet auf den ersten,", "tokens": ["Trei\u00b7bet", "auf", "den", "ers\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Der da seiner harrt \u2013", "tokens": ["Der", "da", "sei\u00b7ner", "harrt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "VVFIN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und geschleudert st\u00fcrzt er", "tokens": ["Und", "ge\u00b7schleu\u00b7dert", "st\u00fcrzt", "er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVPP", "VVFIN", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Auf die Erde hart.", "tokens": ["Auf", "die", "Er\u00b7de", "hart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Lebt Ihr, guter Ritter,", "tokens": ["Lebt", "Ihr", ",", "gu\u00b7ter", "Rit\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Oder seid Ihr tot?", "tokens": ["O\u00b7der", "seid", "Ihr", "tot", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Aber tat's mit M\u00fchlen", "tokens": ["A\u00b7ber", "tat's", "mit", "M\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Euch zu raufen Not?", "tokens": ["Euch", "zu", "rau\u00b7fen", "Not", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVFIN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Sollte wer mich fragen,", "tokens": ["Soll\u00b7te", "wer", "mich", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PWS", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wie man vieles fragt,", "tokens": ["Wie", "man", "vie\u00b7les", "fragt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PIS", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ob es Riesen waren,", "tokens": ["Ob", "es", "Rie\u00b7sen", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wie der Herr es sagt,", "tokens": ["Wie", "der", "Herr", "es", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Oder blo\u00dfe M\u00fchlen,", "tokens": ["O\u00b7der", "blo\u00b7\u00dfe", "M\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wie es meint der Knecht;", "tokens": ["Wie", "es", "meint", "der", "Knecht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Geb ich unbed