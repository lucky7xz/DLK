{"dta.poem.5837": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch diese einfach-hell-und klare Regel nur", "tokens": ["Durch", "die\u00b7se", "ein\u00b7fach\u00b7hell\u00b7\u00b7und", "kla\u00b7re", "Re\u00b7gel", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ver\u00e4ndert alles sich in der Natur.", "tokens": ["Ver\u00b7\u00e4n\u00b7dert", "al\u00b7les", "sich", "in", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Durch die Bewegungs-Krafft allein,", "tokens": ["Durch", "die", "Be\u00b7we\u00b7gungs\u00b7Krafft", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind alle C\u00f6rper, die unsicht-und sichtbar seyn,", "tokens": ["Sind", "al\u00b7le", "C\u00f6r\u00b7per", ",", "die", "un\u00b7sicht\u00b7\u00b7und", "sicht\u00b7bar", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "PRELS", "ADJD", "ADJD", "VAINF", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Getrennt, vereint, verst\u00f6hrt,", "tokens": ["Ge\u00b7trennt", ",", "ver\u00b7eint", ",", "ver\u00b7st\u00f6hrt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und wiederum in vor\u2019gen Stand gekehrt.", "tokens": ["Und", "wie\u00b7de\u00b7rum", "in", "vor'\u00b7gen", "Stand", "ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}