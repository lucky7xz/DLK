{"dta.poem.20158": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Lieb-erfreute  \n  FJLAMON .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1666", "urn": "urn:nbn:de:kobv:b4-20424-1", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch-Historischer Lustgarten. Frankfurt (Main), 1666."}, "poem": {"stanza.1": {"line.1": {"text": "Filamon der mu\u00df verderben", "tokens": ["Fi\u00b7la\u00b7mon", "der", "mu\u00df", "ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "VMFIN", "VVFIN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "In der grossen Liebes-Pein.", "tokens": ["In", "der", "gros\u00b7sen", "Lie\u00b7bes\u00b7Pein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil er niemahl kan erwerben", "tokens": ["Weil", "er", "nie\u00b7mahl", "kan", "er\u00b7wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bellifloren M\u00fcndelein.", "tokens": ["Bell\u00b7if\u00b7lo\u00b7ren", "M\u00fcn\u00b7del\u00b7ein", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sinn und Hertz mu\u00df ihm verschwinden/", "tokens": ["Sinn", "und", "Hertz", "mu\u00df", "ihm", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo er nicht wird H\u00fclff\u2019 empfinden.", "tokens": ["Wo", "er", "nicht", "wird", "H\u00fclff'", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VAFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}