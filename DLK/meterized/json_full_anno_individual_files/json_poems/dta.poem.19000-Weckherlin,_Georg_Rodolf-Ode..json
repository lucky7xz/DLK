{"dta.poem.19000": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ode.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Wje vnverhinderlich ein Jahr", "tokens": ["Wie", "vn\u00b7ver\u00b7hin\u00b7der\u00b7lich", "ein", "Jahr"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schnell nach dem andern dahin fliehet!", "tokens": ["Schnell", "nach", "dem", "an\u00b7dern", "da\u00b7hin", "flie\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "PAV", "VVFIN", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie vnempfindlich vnser haar", "tokens": ["Wie", "vnemp\u00b7find\u00b7lich", "vn\u00b7ser", "haar"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich graw zu f\u00e4rben nicht verziehet!", "tokens": ["Sich", "graw", "zu", "f\u00e4r\u00b7ben", "nicht", "ver\u00b7zie\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PTKZU", "VVINF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vmbsunst die Frombkeit selbs/ die stirn", "tokens": ["Vmbsunst", "die", "Fromb\u00b7keit", "selbs", "/", "die", "stirn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV", "$(", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Von r\u00fcntzeln/ vnd von sorg das hirn", "tokens": ["Von", "r\u00fcnt\u00b7zeln", "/", "vnd", "von", "sorg", "das", "hirn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "$(", "KON", "APPR", "NE", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu freyhen sich bem\u00fchet.", "tokens": ["Zu", "frey\u00b7hen", "sich", "be\u00b7m\u00fc\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Lauff alle tag der Kirchen zu/", "tokens": ["Lauff", "al\u00b7le", "tag", "der", "Kir\u00b7chen", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd dien dem/ der allein Allm\u00e4chtig;", "tokens": ["Vnd", "di\u00b7en", "dem", "/", "der", "al\u00b7lein", "All\u00b7m\u00e4ch\u00b7tig", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "$(", "ART", "ADV", "ADJD", "$."], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Vnd ohn erquickung/ nahrung/ ruh/", "tokens": ["Vnd", "ohn", "er\u00b7quic\u00b7kung", "/", "nah\u00b7rung", "/", "ruh", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "NN", "$(", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erwei\u00df dich tag vnd nacht and\u00e4chtig", "tokens": ["Er\u00b7wei\u00df", "dich", "tag", "vnd", "nacht", "an\u00b7d\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd Christlich: So wirt endlich doch", "tokens": ["Vnd", "Christ\u00b7lich", ":", "So", "wirt", "end\u00b7lich", "doch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NE", "$.", "ADV", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das vnvermeidenliche Joch", "tokens": ["Das", "vn\u00b7ver\u00b7mei\u00b7den\u00b7li\u00b7che", "Joch"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Tods auch durch dich pr\u00e4chtig.", "tokens": ["Des", "Tods", "auch", "durch", "dich", "pr\u00e4ch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die s\u00fcnd/ die alle menschen gleich", "tokens": ["Die", "s\u00fcnd", "/", "die", "al\u00b7le", "men\u00b7schen", "gleich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "$(", "ART", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gemachet/ machet sie fortgehen/", "tokens": ["Ge\u00b7ma\u00b7chet", "/", "ma\u00b7chet", "sie", "fort\u00b7ge\u00b7hen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd lasset weder arm noch reich", "tokens": ["Vnd", "las\u00b7set", "we\u00b7der", "arm", "noch", "reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "ADJD", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich l\u00e4nger spreissen vnd stillstehen:", "tokens": ["Sich", "l\u00e4n\u00b7ger", "spreis\u00b7sen", "vnd", "still\u00b7ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Juncker/ Herr/ Graff vnd Monarch", "tokens": ["Ein", "Jun\u00b7cker", "/", "Herr", "/", "Graff", "vnd", "Mon\u00b7arch"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "NN", "$(", "NE", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wirt wie ein baur mit einem sarch", "tokens": ["Wirt", "wie", "ein", "baur", "mit", "ei\u00b7nem", "sarch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vnd einem grab versehen.", "tokens": ["Vnd", "ei\u00b7nem", "grab", "ver\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vmbsunst der forchtsam f\u00fcr ein weil", "tokens": ["Vmbsunst", "der", "forcht\u00b7sam", "f\u00fcr", "ein", "weil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJD", "APPR", "ART", "KOUS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem Meer/ dem Krieg/ der Pest entfliehet/", "tokens": ["Dem", "Meer", "/", "dem", "Krieg", "/", "der", "Pest", "ent\u00b7flie\u00b7het", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dan ja der Tod/ der dan in eyl/", "tokens": ["Dan", "ja", "der", "Tod", "/", "der", "dan", "in", "eyl", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$(", "ART", "ADV", "APPR", "NN", "$("], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Dan langsam ist/ nicht lang verziehet:", "tokens": ["Dan", "lang\u00b7sam", "ist", "/", "nicht", "lang", "ver\u00b7zie\u00b7het", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$(", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gleich ist jhm der klein vnd der gro\u00df/", "tokens": ["Gleich", "ist", "jhm", "der", "klein", "vnd", "der", "gro\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJD", "KON", "ART", "ADJD", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Vnd der gewafnet vnd der blo\u00df/", "tokens": ["Vnd", "der", "ge\u00b7waf\u00b7net", "vnd", "der", "blo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVPP", "KON", "ART", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der welck/ vnd der noch bl\u00fchet.", "tokens": ["Der", "welck", "/", "vnd", "der", "noch", "bl\u00fc\u00b7het", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "ART", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Vmbsunst sich setzet vngedult", "tokens": ["Vmbsunst", "sich", "set\u00b7zet", "vn\u00b7ge\u00b7dult"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PRF", "VVFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Forcht vnd geheul dem Tod entgegen:", "tokens": ["Forcht", "vnd", "ge\u00b7heul", "dem", "Tod", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es mu\u00df ein jeder dise schuld", "tokens": ["Es", "mu\u00df", "ein", "je\u00b7der", "di\u00b7se", "schuld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "PIS", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auff die bestimpte zeit abl\u00f6gen:", "tokens": ["Auff", "die", "be\u00b7stimp\u00b7te", "zeit", "ab\u00b7l\u00f6\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nichts kan den Tod/ vnser geschlecht", "tokens": ["Nichts", "kan", "den", "Tod", "/", "vn\u00b7ser", "ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "(von staub vnd aschen ein gem\u00e4cht)", "tokens": ["(", "von", "staub", "vnd", "asc\u00b7hen", "ein", "ge\u00b7m\u00e4cht", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJD", "KON", "VVFIN", "ART", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu sparen je bew\u00f6gen.", "tokens": ["Zu", "spa\u00b7ren", "je", "be\u00b7w\u00f6\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da m\u00fcssen wir dan alles gut", "tokens": ["Da", "m\u00fcs\u00b7sen", "wir", "dan", "al\u00b7les", "gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PIS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wir begehret vnd erfassen/", "tokens": ["So", "wir", "be\u00b7ge\u00b7hret", "vnd", "er\u00b7fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "KON", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Was vns mit hochmuht vnd vnmuht", "tokens": ["Was", "vns", "mit", "hoch\u00b7muht", "vnd", "vn\u00b7muht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jemahls zu lieben vnd zu hassen", "tokens": ["Je\u00b7mahls", "zu", "lie\u00b7ben", "vnd", "zu", "has\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Beliebet/ mit dem lieben leib/", "tokens": ["Be\u00b7lie\u00b7bet", "/", "mit", "dem", "lie\u00b7ben", "leib", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hau\u00df/ hoff/ spil/ kurtzweil/ kinder/ weib/", "tokens": ["Hau\u00df", "/", "hoff", "/", "spil", "/", "kurt\u00b7zweil", "/", "kin\u00b7der", "/", "weib", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "$(", "NE", "$(", "ADJD", "$(", "NN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vnd freind dahinden lassen.", "tokens": ["Vnd", "freind", "da\u00b7hin\u00b7den", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Hat einer (nichts mehr dan gestanck)", "tokens": ["Hat", "ei\u00b7ner", "(", "nichts", "mehr", "dan", "ge\u00b7stanck", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "$(", "PIS", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verlassend alles dan beschlossen/", "tokens": ["Ver\u00b7las\u00b7send", "al\u00b7les", "dan", "be\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PIS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erfolget daf\u00fcr schlechter danck", "tokens": ["Er\u00b7fol\u00b7get", "da\u00b7f\u00fcr", "schlech\u00b7ter", "danck"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PAV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vermischet mit spot/ schmach vnd bossen;", "tokens": ["Ver\u00b7mi\u00b7schet", "mit", "spot", "/", "schmach", "vnd", "bos\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "$(", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wan Er errungen vil alhie", "tokens": ["Wan", "Er", "er\u00b7run\u00b7gen", "vil", "al\u00b7hie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "ADV", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "F\u00fcr andre leut mit sorg vnd m\u00fch/", "tokens": ["F\u00fcr", "and\u00b7re", "leut", "mit", "sorg", "vnd", "m\u00fch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "APPR", "NN", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vnd (Narr) selbs nicht genossen.", "tokens": ["Vnd", "(", "Narr", ")", "selbs", "nicht", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "NE", "$(", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}