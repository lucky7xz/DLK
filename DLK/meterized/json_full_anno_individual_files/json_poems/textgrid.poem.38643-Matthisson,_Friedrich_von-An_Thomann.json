{"textgrid.poem.38643": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Thomann", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "St\u00fcrme sausen im Eichwald; seine Pfade", "tokens": ["St\u00fcr\u00b7me", "sau\u00b7sen", "im", "Eich\u00b7wald", ";", "sei\u00b7ne", "Pfa\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Deckt des rauschenden Laubes brauner Teppich;", "tokens": ["Deckt", "des", "rau\u00b7schen\u00b7den", "Lau\u00b7bes", "brau\u00b7ner", "Tep\u00b7pich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Einsam trauren die Pfeiler der zerst\u00f6rten", "tokens": ["Ein\u00b7sam", "trau\u00b7ren", "die", "Pfei\u00b7ler", "der", "zer\u00b7st\u00f6r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Schattengew\u00f6lbe.", "tokens": ["Schat\u00b7ten\u00b7ge\u00b7w\u00f6l\u00b7be", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenig k\u00fcmmert am Heerd, bey Hochheims Nektar,", "tokens": ["We\u00b7nig", "k\u00fcm\u00b7mert", "am", "Heerd", ",", "bey", "Hoch\u00b7heims", "Nek\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,", "APPR", "NE", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Uns der heulende Nord, wiewohl er Flocken", "tokens": ["Uns", "der", "heu\u00b7len\u00b7de", "Nord", ",", "wie\u00b7wohl", "er", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "St\u00fcrmt, die Quelle bepanzert und des Hainthals", "tokens": ["St\u00fcrmt", ",", "die", "Quel\u00b7le", "be\u00b7pan\u00b7zert", "und", "des", "Haint\u00b7hals"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "NN", "VVPP", "KON", "ART", "NN"], "meter": "+-+--++-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Tempe ver\u00f6det.", "tokens": ["Tem\u00b7pe", "ver\u00b7\u00f6\u00b7det", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Sind des kehrenden Lenzes wir doch sicher,", "tokens": ["Sind", "des", "keh\u00b7ren\u00b7den", "Len\u00b7zes", "wir", "doch", "si\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Der die Fluren verj\u00fcngt, den Hain bebl\u00fctet", "tokens": ["Der", "die", "Flu\u00b7ren", "ver\u00b7j\u00fcngt", ",", "den", "Hain", "be\u00b7bl\u00fc\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und aus himmlischer Urne des Entz\u00fcckens", "tokens": ["Und", "aus", "himm\u00b7li\u00b7scher", "Ur\u00b7ne", "des", "Ent\u00b7z\u00fc\u00b7ckens"], "token_i