{"dta.poem.14026": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Steckbrief .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Liebe geht durch die Luft,", "tokens": ["Lie\u00b7be", "geht", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Holt fern die Liebste ein;", "tokens": ["Holt", "fern", "die", "Liebs\u00b7te", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Fort \u00fcber Berg und Kluft!", "tokens": ["Fort", "\u00fc\u00b7ber", "Berg", "und", "Kluft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und Sie wird doch noch mein!", "tokens": ["Und", "Sie", "wird", "doch", "noch", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "PPOSAT", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}