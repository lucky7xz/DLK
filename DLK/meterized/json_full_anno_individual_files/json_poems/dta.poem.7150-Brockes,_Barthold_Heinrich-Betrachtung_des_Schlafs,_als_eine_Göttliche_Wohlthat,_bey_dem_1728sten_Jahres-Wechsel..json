{"dta.poem.7150": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Dancken aber fl\u00fcsst und fasst in sich", "tokens": ["Das", "Dan\u00b7cken", "a\u00b7ber", "fl\u00fcsst", "und", "fasst", "in", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "KON", "VVFIN", "APPR", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Lob, Inbrunst, Andacht, Frenden-Triebe,", "tokens": ["Lob", ",", "In\u00b7brunst", ",", "An\u00b7dacht", ",", "Fren\u00b7den\u00b7Trie\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Preis, Ehr, Erkenntlichkeit und Liebe.", "tokens": ["Preis", ",", "Ehr", ",", "Er\u00b7kennt\u00b7lich\u00b7keit", "und", "Lie\u00b7be", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ja, man empfindet inniglich", "tokens": ["Ja", ",", "man", "emp\u00b7fin\u00b7det", "in\u00b7nig\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PIS", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Recht eine kindliche Begier,", "tokens": ["Recht", "ei\u00b7ne", "kind\u00b7li\u00b7che", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Seele, Sinnen, Geist und Leben,", "tokens": ["Die", "See\u00b7le", ",", "Sin\u00b7nen", ",", "Geist", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "O ew\u2019ge Lieb\u2019 und Sch\u00f6pffer, Dir", "tokens": ["O", "ew'\u00b7ge", "Lieb'", "und", "Sch\u00f6pf\u00b7fer", ",", "Dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "ADJA", "NN", "KON", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gantz aufzuopffern, hinzugeben.", "tokens": ["Gantz", "auf\u00b7zuopf\u00b7fern", ",", "hin\u00b7zu\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVIZU", "$,", "VVIZU", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Wofern der Menschen Seele nun", "tokens": ["Wo\u00b7fern", "der", "Men\u00b7schen", "See\u00b7le", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Was GOTT-gef\u00e4lliges noch in sich heget;", "tokens": ["Was", "GoT\u00b7T\u00b7ge\u00b7f\u00e4l\u00b7li\u00b7ges", "noch", "in", "sich", "he\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So wird ein solches br\u00fcnstigs Wallen,", "tokens": ["So", "wird", "ein", "sol\u00b7ches", "br\u00fcns\u00b7tigs", "Wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Durch Lieb\u2019 und Danckbarkeit erreget,", "tokens": ["Durch", "Lieb'", "und", "Dan\u00b7ck\u00b7bar\u00b7keit", "er\u00b7re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Dem Grossen Vater nicht mi\u00dffallen.", "tokens": ["Dem", "Gros\u00b7sen", "Va\u00b7ter", "nicht", "mi\u00df\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}