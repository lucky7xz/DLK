{"dta.poem.14307": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ein alt Gemach voll sinn'ger Seltsamkeiten,", "tokens": ["Ein", "alt", "Ge\u00b7mach", "voll", "sinn'\u00b7ger", "Selt\u00b7sam\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Still' Blumen aufgestellt am Fensterbogen,", "tokens": ["Still'", "Blu\u00b7men", "auf\u00b7ge\u00b7stellt", "am", "Fens\u00b7ter\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gebirg' und L\u00e4nder drau\u00dfen blau gezogen,", "tokens": ["Ge\u00b7bir\u00b7g'", "und", "L\u00e4n\u00b7der", "drau\u00b7\u00dfen", "blau", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wo Str\u00f6me geh'n und Ritter ferne reiten.", "tokens": ["Wo", "Str\u00f6\u00b7me", "geh'n", "und", "Rit\u00b7ter", "fer\u00b7ne", "rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}