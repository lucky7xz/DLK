za.16": {"line.1": {"text": "Erscheint mir so der G\u00e4rtner in Gedanken,", "tokens": ["Er\u00b7scheint", "mir", "so", "der", "G\u00e4rt\u00b7ner", "in", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der J\u00e4ger auf dem luft'gen Felsenwege,", "tokens": ["Der", "J\u00e4\u00b7ger", "auf", "dem", "luft'\u00b7gen", "Fel\u00b7sen\u00b7we\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Fischer in des Wassers wildem Schwanken,", "tokens": ["Der", "Fi\u00b7scher", "in", "des", "Was\u00b7sers", "wil\u00b7dem", "Schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.17": {"line.1": {"text": "Und f\u00e4llt mir ein, worauf ich einst gebauet,", "tokens": ["Und", "f\u00e4llt", "mir", "ein", ",", "wo\u00b7rauf", "ich", "einst", "ge\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Lieb' und Treu' und Wort, so find' ich leider,", "tokens": ["Auf", "Lieb'", "und", "Treu'", "und", "Wort", ",", "so", "find'", "ich", "lei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich verloren, weil ich fest vertrauet.", "tokens": ["Da\u00df", "ich", "ver\u00b7lo\u00b7ren", ",", "weil", "ich", "fest", "ver\u00b7trau\u00b7et", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.18": {"line.1": {"text": "Den Gl\u00fccklichen ist alle Ruh beschieden,", "tokens": ["Den", "Gl\u00fcck\u00b7li\u00b7chen", "ist", "al\u00b7le", "Ruh", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich aber jage nur nach eitlem Ruhme,", "tokens": ["Ich", "a\u00b7ber", "ja\u00b7ge", "nur", "nach", "eit\u00b7lem", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So sah denn auch noch keiner mich zufrieden.", "tokens": ["So", "sah", "denn", "auch", "noch", "kei\u00b7ner", "mich", "zu\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADV", "PIS", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}