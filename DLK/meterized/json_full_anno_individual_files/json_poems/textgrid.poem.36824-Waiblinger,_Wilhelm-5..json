{"textgrid.poem.36824": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es baut der Mensch im wohlbepflanzten Garten,", "tokens": ["Es", "baut", "der", "Mensch", "im", "wohl\u00b7be\u00b7pflanz\u00b7ten", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und zieht der Rebe fruchtbares Gewinde", "tokens": ["Und", "zieht", "der", "Re\u00b7be", "frucht\u00b7ba\u00b7res", "Ge\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Baum zu Baum in freudigem Erwarten.", "tokens": ["Von", "Baum", "zu", "Baum", "in", "freu\u00b7di\u00b7gem", "Er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So gr\u00fcnt denn selbst, vom Menschenflei\u00df bebauet,", "tokens": ["So", "gr\u00fcnt", "denn", "selbst", ",", "vom", "Men\u00b7schen\u00b7flei\u00df", "be\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der kahle Fels, der aus dem Meere starret,", "tokens": ["Der", "kah\u00b7le", "Fels", ",", "der", "aus", "dem", "Mee\u00b7re", "star\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der G\u00e4rtner erndtet, weil er fest vertrauet.", "tokens": ["Der", "G\u00e4rt\u00b7ner", "ernd\u00b7tet", ",", "weil", "er", "fest", "ver\u00b7trau\u00b7et", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vom Vogelfange n\u00e4hret sich der Arme,", "tokens": ["Vom", "Vo\u00b7gel\u00b7fan\u00b7ge", "n\u00e4h\u00b7ret", "sich", "der", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die steilste Klippe wei\u00df er zu erklettern,", "tokens": ["Die", "steils\u00b7te", "Klip\u00b7pe", "wei\u00df", "er", "zu", "er\u00b7klet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und lauert k\u00fchn nach dem verborgnen Schwarme.", "tokens": ["Und", "lau\u00b7ert", "k\u00fchn", "nach", "dem", "ver\u00b7borg\u00b7nen", "Schwar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er zittert nicht, wenn er zum Abgrund schauet,", "tokens": ["Er", "zit\u00b7tert", "nicht", ",", "wenn", "er", "zum", "Ab\u00b7grund", "schau\u00b7et", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo tief die gr\u00fcne Meereswoge brandet,", "tokens": ["Wo", "tief", "die", "gr\u00fc\u00b7ne", "Mee\u00b7res\u00b7wo\u00b7ge", "bran\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erreicht die Beute, weil er fest vertrauet.", "tokens": ["Er\u00b7reicht", "die", "Beu\u00b7te", ",", "weil", "er", "fest", "ver\u00b7trau\u00b7et", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das Element des Fischers ist die Welle,", "tokens": ["Das", "E\u00b7le\u00b7ment", "des", "Fi\u00b7schers", "ist", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sein Boot ist sicher, und er achtet's wenig,", "tokens": ["Sein", "Boot", "ist", "si\u00b7cher", ",", "und", "er", "ach\u00b7tet's", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "PPER", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob's um ihn sch\u00e4um' und auf und nieder schwelle,", "tokens": ["Ob's", "um", "ihn", "sch\u00e4um'", "und", "auf", "und", "nie\u00b7der", "schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VVFIN", "KON", "APPR", "KON", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Er kennt die See, so wie sein Haus; ihm grauet", "tokens": ["Er", "kennt", "die", "See", ",", "so", "wie", "sein", "Haus", ";", "ihm", "grau\u00b7et"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "KOKOM", "PPOSAT", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor ihrer Falschheit nicht, er senkt die Netze,", "tokens": ["Vor", "ih\u00b7rer", "Falschheit", "nicht", ",", "er", "senkt", "die", "Net\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und er gewinnet, weil er fest vertrauet.", "tokens": ["Und", "er", "ge\u00b7win\u00b7net", ",", "weil", "er", "fest", "ver\u00b7trau\u00b7et", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Erscheint mir so der G\u00e4rtner in Gedanken,", "tokens": ["Er\u00b7scheint", "mir", "so", "der", "G\u00e4rt\u00b7ner", "in", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der J\u00e4ger auf dem luft'gen Felsenwege,", "tokens": ["Der", "J\u00e4\u00b7ger", "auf", "dem", "luft'\u00b7gen", "Fel\u00b7sen\u00b7we\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Fischer in des Wassers wildem Schwanken,", "tokens": ["Der", "Fi\u00b7scher", "in", "des", "Was\u00b7sers", "wil\u00b7dem", "Schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und f\u00e4llt mir ein, worauf ich einst gebauet,", "tokens": ["Und", "f\u00e4llt", "mir", "ein", ",", "wo\u00b7rauf", "ich", "einst", "ge\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Lieb' und Treu' und Wort, so find' ich leider,", "tokens": ["Auf", "Lieb'", "und", "Treu'", "und", "Wort", ",", "so", "find'", "ich", "lei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich verloren, weil ich fest vertrauet.", "tokens": ["Da\u00df", "ich", "ver\u00b7lo\u00b7ren", ",", "weil", "ich", "fest", "ver\u00b7trau\u00b7et", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Den Gl\u00fccklichen ist alle Ruh beschieden,", "tokens": ["Den", "Gl\u00fcck\u00b7li\u00b7chen", "ist", "al\u00b7le", "Ruh", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word"