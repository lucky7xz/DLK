{"dta.poem.1928": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die vier Alter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Knabe/ der nun Fu\u00df und Zunge brauchen kan/", "tokens": ["Ein", "Kna\u00b7be", "/", "der", "nun", "Fu\u00df", "und", "Zun\u00b7ge", "brau\u00b7chen", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "NN", "KON", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hebt gerne Kinderspiel mit seines gleichen an/", "tokens": ["Hebt", "ger\u00b7ne", "Kin\u00b7der\u00b7spiel", "mit", "sei\u00b7nes", "glei\u00b7chen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "APPR", "PPOSAT", "ADJA", "PTKVZ", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Z\u00fcrnt und vers\u00f6hnt sich leicht/ ist wandelbarer Sinnen/", "tokens": ["Z\u00fcrnt", "und", "ver\u00b7s\u00f6hnt", "sich", "leicht", "/", "ist", "wan\u00b7del\u00b7ba\u00b7rer", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PRF", "ADJD", "$(", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "W\u00fcnscht alle Stunden ihm was neues zu beginnen.", "tokens": ["W\u00fcnscht", "al\u00b7le", "Stun\u00b7den", "ihm", "was", "neu\u00b7es", "zu", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PPER", "PWS", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein J\u00fcngling ohne Barth in Freyheit nun gestellt/", "tokens": ["Ein", "J\u00fcng\u00b7ling", "oh\u00b7ne", "Barth", "in", "Frey\u00b7heit", "nun", "ge\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat Hund und Pferde lieb/ streicht durch das freye Feld/", "tokens": ["Hat", "Hund", "und", "Pfer\u00b7de", "lieb", "/", "streicht", "durch", "das", "frey\u00b7e", "Feld", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADJD", "$(", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zum B\u00f6sen weich/ als Wachs/ zum Straffen hart als Stahl/", "tokens": ["Zum", "B\u00f6\u00b7sen", "weich", "/", "als", "Wachs", "/", "zum", "Straf\u00b7fen", "hart", "als", "Stahl", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$(", "KOUS", "NN", "$(", "APPRART", "NN", "ADJD", "KOKOM", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Geneigt zum Geld-verthun/ thut langsam gutte Wahl;", "tokens": ["Ge\u00b7neigt", "zum", "Geld\u00b7ver\u00b7thun", "/", "thut", "lang\u00b7sam", "gut\u00b7te", "Wahl", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$(", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Will leichtlich oben aus/ ist voll Begier und Lust/", "tokens": ["Will", "leicht\u00b7lich", "o\u00b7ben", "aus", "/", "ist", "voll", "Be\u00b7gier", "und", "Lust", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "PTKVZ", "$(", "VAFIN", "ADJD", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es wechseln Lieb und Ha\u00df gar bald in seiner Brust.", "tokens": ["Es", "wech\u00b7seln", "Lieb", "und", "Ha\u00df", "gar", "bald", "in", "sei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bey nunmehr reiffem Mann ist alles umgewandt/", "tokens": ["Bey", "nun\u00b7mehr", "reif\u00b7fem", "Mann", "ist", "al\u00b7les", "um\u00b7ge\u00b7wandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VAFIN", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Er sucht durch Geld und Freund zu bessern seinen Stand/", "tokens": ["Er", "sucht", "durch", "Geld", "und", "Freund", "zu", "bes\u00b7sern", "sei\u00b7nen", "Stand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Strebt Ruhm und Ehren nach/ vermeidet zu begehn/", "tokens": ["Strebt", "Ruhm", "und", "Eh\u00b7ren", "nach", "/", "ver\u00b7mei\u00b7det", "zu", "be\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "APPR", "$(", "VVPP", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Woraus ihm sp\u00e4te Reu und Schande kan entstehn.", "tokens": ["Wo\u00b7raus", "ihm", "sp\u00e4\u00b7te", "Reu", "und", "Schan\u00b7de", "kan", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "NE", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Viel Ungelegenheit sieht man beym Alter bl\u00fchn;", "tokens": ["Viel", "Un\u00b7ge\u00b7le\u00b7gen\u00b7heit", "sieht", "man", "beym", "Al\u00b7ter", "bl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PIS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Es scharrt/ und will ihm doch selbst den ", "tokens": ["Es", "scharrt", "/", "und", "will", "ihm", "doch", "selbst", "den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KON", "VMFIN", "PPER", "ADV", "ADV", "ART"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Greifft alles furchtsam an und rechnet annoch weit/", "tokens": ["Greifft", "al\u00b7les", "furcht\u00b7sam", "an", "und", "rech\u00b7net", "an\u00b7noch", "weit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "PTKVZ", "KON", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ist tr\u00e4ge/ voll Verdru\u00df/ schiebt gern auff l\u00e4ngre Zeit/", "tokens": ["Ist", "tr\u00e4\u00b7ge", "/", "voll", "Ver\u00b7dru\u00df", "/", "schiebt", "gern", "auff", "l\u00e4ng\u00b7re", "Zeit", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$(", "ADJD", "NN", "$(", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Lobt seiner Jugend Thun/ und tadelt andrer Leben/", "tokens": ["Lobt", "sei\u00b7ner", "Ju\u00b7gend", "Thun", "/", "und", "ta\u00b7delt", "an\u00b7drer", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$(", "KON", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Pflegt ", "tokens": ["Pflegt"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}}}}}