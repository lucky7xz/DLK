{"dta.poem.10368": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "V.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "In stiller, wehmuthweicher Abendstunde,", "tokens": ["In", "stil\u00b7ler", ",", "weh\u00b7muth\u00b7wei\u00b7cher", "A\u00b7bends\u00b7tun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umklingen mich die l\u00e4ngst verscholl'nen Lieder,", "tokens": ["Um\u00b7klin\u00b7gen", "mich", "die", "l\u00e4ngst", "ver\u00b7scholl'\u00b7nen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Thr\u00e4nen rollen von der Wange nieder,", "tokens": ["Und", "Thr\u00e4\u00b7nen", "rol\u00b7len", "von", "der", "Wan\u00b7ge", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Blut entquillt der alten Herzenswunde.", "tokens": ["Und", "Blut", "ent\u00b7quillt", "der", "al\u00b7ten", "Her\u00b7zens\u00b7wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wie in eines Zauberspiegels Grunde", "tokens": ["Und", "wie", "in", "ei\u00b7nes", "Zau\u00b7ber\u00b7spie\u00b7gels", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Seh' ich das Bildni\u00df meiner Liebsten wieder;", "tokens": ["Seh'", "ich", "das", "Bild\u00b7ni\u00df", "mei\u00b7ner", "Liebs\u00b7ten", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie sitzt am Arbeitstisch', im rothen Mieder,", "tokens": ["Sie", "sitzt", "am", "Ar\u00b7beit\u00b7stisch'", ",", "im", "ro\u00b7then", "Mie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Stille herrscht in ihrer heilgen Runde.", "tokens": ["Und", "Stil\u00b7le", "herrscht", "in", "ih\u00b7rer", "heil\u00b7gen", "Run\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Doch pl\u00f6tzlich springt sie auf vom Stuhl und schneidet", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "springt", "sie", "auf", "vom", "Stuhl", "und", "schnei\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "APPRART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Von ihrem Haupt die sch\u00f6nste aller Locken,", "tokens": ["Von", "ih\u00b7rem", "Haupt", "die", "sch\u00f6ns\u00b7te", "al\u00b7ler", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und gibt sie mir, \u2014 vor Freud bin ich erschrocken", "tokens": ["Und", "gibt", "sie", "mir", ",", "vor", "Freud", "bin", "ich", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "$(", "APPR", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Mephisto hat die Freude mir verleidet.", "tokens": ["Me\u00b7phis\u00b7to", "hat", "die", "Freu\u00b7de", "mir", "ver\u00b7lei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Er spann ein festes Seil von jenen Haaren,", "tokens": ["Er", "spann", "ein", "fes\u00b7tes", "Seil", "von", "je\u00b7nen", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und schleift mich dran herum seit vielen Jahren.", "tokens": ["Und", "schleift", "mich", "dran", "he\u00b7rum", "seit", "vie\u00b7len", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PAV", "APZR", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}