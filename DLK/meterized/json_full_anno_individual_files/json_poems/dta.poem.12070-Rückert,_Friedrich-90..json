{"dta.poem.12070": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "90.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lust an Verg\u00e4nglichem kann nur verg\u00e4nglich seyn,", "tokens": ["Lust", "an", "Ver\u00b7g\u00e4ng\u00b7li\u00b7chem", "kann", "nur", "ver\u00b7g\u00e4ng\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VMFIN", "ADV", "ADJD", "VAINF", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und ewig ist die Lust am Ewigen allein.", "tokens": ["Und", "e\u00b7wig", "ist", "die", "Lust", "am", "E\u00b7wi\u00b7gen", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "APPRART", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du sagst dir das, und kannst dennoch der Lust nicht wehren,", "tokens": ["Du", "sagst", "dir", "das", ",", "und", "kannst", "den\u00b7noch", "der", "Lust", "nicht", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDS", "$,", "KON", "VMFIN", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was unbegehrenswerth du siehest, zu begehren.", "tokens": ["Was", "un\u00b7be\u00b7geh\u00b7rens\u00b7werth", "du", "sie\u00b7hest", ",", "zu", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Warum? weil in dir selbst ist ein Verg\u00e4ngliches,", "tokens": ["Wa\u00b7rum", "?", "weil", "in", "dir", "selbst", "ist", "ein", "Ver\u00b7g\u00e4ng\u00b7li\u00b7ches", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "APPR", "PPER", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Unverg\u00e4nglichkeit ganz unempf\u00e4ngliches.", "tokens": ["Der", "Un\u00b7ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "ganz", "un\u00b7emp\u00b7f\u00e4ng\u00b7li\u00b7ches", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch f\u00fchlest du in dir ein Andres unverg\u00e4nglich,", "tokens": ["Doch", "f\u00fch\u00b7lest", "du", "in", "dir", "ein", "And\u00b7res", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "ART", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem, was verg\u00e4nglich ist, erscheinet unzul\u00e4nglich.", "tokens": ["Dem", ",", "was", "ver\u00b7g\u00e4ng\u00b7lich", "ist", ",", "er\u00b7schei\u00b7net", "un\u00b7zu\u00b7l\u00e4ng\u00b7lich", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ADJD", "VAFIN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "In solchem Kampfe bleibt der Sieg nicht zweifelhaft,", "tokens": ["In", "sol\u00b7chem", "Kamp\u00b7fe", "bleibt", "der", "Sieg", "nicht", "zwei\u00b7fel\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sobald das Edlere gebrauchet seine Kraft.", "tokens": ["So\u00b7bald", "das", "Ed\u00b7le\u00b7re", "ge\u00b7brau\u00b7chet", "sei\u00b7ne", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Dir wird f\u00fcr ew'ge Lust jemehr Empf\u00e4nglichkeit,", "tokens": ["Dir", "wird", "f\u00fcr", "ew'\u00b7ge", "Lust", "je\u00b7mehr", "Emp\u00b7f\u00e4ng\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jemehr in deiner Brust reift Unverg\u00e4nglichkeit.", "tokens": ["Je\u00b7mehr", "in", "dei\u00b7ner", "Brust", "reift", "Un\u00b7ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}