{"dta.poem.10771": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Vff Herrn Ludwig Hebers Hochzeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.85", "fr:0.14"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Triumph/ du keusche Lieb/ triumph du nun solst singen/", "tokens": ["Tri\u00b7umph", "/", "du", "keu\u00b7sche", "Lieb", "/", "tri\u00b7umph", "du", "nun", "solst", "sin\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "ADJA", "NN", "$(", "VVFIN", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die gegenwertig Zeit mit Ehr vnd Frewd vollbringen.", "tokens": ["Die", "ge\u00b7gen\u00b7wer\u00b7tig", "Zeit", "mit", "Ehr", "vnd", "Frewd", "voll\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In beider Ehleuth Hertz r\u00fcst deinen Wagen au\u00df/", "tokens": ["In", "bei\u00b7der", "Eh\u00b7leuth", "Hertz", "r\u00fcst", "dei\u00b7nen", "Wa\u00b7gen", "au\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Fahr als ein Siegerin in das bekante Hau\u00df.", "tokens": ["Fahr", "als", "ein", "Sie\u00b7ge\u00b7rin", "in", "das", "be\u00b7kan\u00b7te", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dann du das sch\u00f6ne Joch/ darnach vor kurtzen Zeiten/", "tokens": ["Dann", "du", "das", "sch\u00f6\u00b7ne", "Joch", "/", "dar\u00b7nach", "vor", "kurt\u00b7zen", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN", "$(", "PAV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Bott de\u00df Todts getracht/ dasselbig au\u00dfzureutten/", "tokens": ["Der", "Bott", "de\u00df", "Todts", "ge\u00b7tracht", "/", "das\u00b7sel\u00b7big", "au\u00df\u00b7zu\u00b7reut\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$(", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auffs new erobert hast: da doch fast gantz vnd gar", "tokens": ["Auffs", "new", "er\u00b7o\u00b7bert", "hast", ":", "da", "doch", "fast", "gantz", "vnd", "gar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "VVPP", "VAFIN", "$.", "ADV", "ADV", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hinweg de\u00df lebens trost/ hinweg die hoffnung war.", "tokens": ["Hin\u00b7weg", "de\u00df", "le\u00b7bens", "trost", "/", "hin\u00b7weg", "die", "hoff\u00b7nung", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$(", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du hast die t\u00f6dlich hitz/ als sie so kranck da lagen/", "tokens": ["Du", "hast", "die", "t\u00f6d\u00b7lich", "hitz", "/", "als", "sie", "so", "kranck", "da", "la\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "$(", "KOUS", "PPER", "ADV", "ADJD", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df auch in sie der Todt sein letztes recht wolt wagen/", "tokens": ["Da\u00df", "auch", "in", "sie", "der", "Todt", "sein", "letz\u00b7tes", "recht", "wolt", "wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPER", "ART", "NN", "PPOSAT", "ADJA", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Verendert in ein hitz/ davon die Lieb sich nehrt/", "tokens": ["Ver\u00b7en\u00b7dert", "in", "ein", "hitz", "/", "da\u00b7von", "die", "Lieb", "sich", "nehrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$(", "PAV", "ART", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Gleich wie vom fetten Oel ein glut vnd flamm sich mehrt.", "tokens": ["Gleich", "wie", "vom", "fet\u00b7ten", "O\u00b7el", "ein", "glut", "vnd", "flamm", "sich", "mehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPRART", "ADJA", "NN", "ART", "NN", "KON", "VVFIN", "PRF", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "So hurtig kan die Lieb ein sch\u00f6nen wechsel finden/", "tokens": ["So", "hur\u00b7tig", "kan", "die", "Lieb", "ein", "sch\u00f6\u00b7nen", "wech\u00b7sel", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vnd von de\u00df Fiebers hitz jhr Fackel selbst anz\u00fcnden/", "tokens": ["Vnd", "von", "de\u00df", "Fie\u00b7bers", "hitz", "jhr", "Fa\u00b7ckel", "selbst", "an\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "PPOSAT", "NN", "ADV", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Davon krafft/ k\u00fchlung/ trost/ ein Hertz im ander/ hat/", "tokens": ["Da\u00b7von", "krafft", "/", "k\u00fch\u00b7lung", "/", "trost", "/", "ein", "Hertz", "im", "an\u00b7der", "/", "hat", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VVFIN", "$(", "NN", "$(", "VVFIN", "$(", "ART", "NN", "APPRART", "ADJD", "$(", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Vnd wird von steter Lieb doch nimmer m\u00fcth noch matt.", "tokens": ["Vnd", "wird", "von", "ste\u00b7ter", "Lieb", "doch", "nim\u00b7mer", "m\u00fcth", "noch", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "ADV", "ADV", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die kousche Lieb ist selbst jhr Seugam vnd ernehrer/", "tokens": ["Die", "kou\u00b7sche", "Lieb", "ist", "selbst", "jhr", "Seu\u00b7gam", "vnd", "er\u00b7neh\u00b7rer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "KON", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Vnd findetnimmer mehr kein Rauber noch Zerst\u00f6rer/", "tokens": ["Vnd", "fin\u00b7det\u00b7nim\u00b7mer", "mehr", "kein", "Rau\u00b7ber", "noch", "Zer\u00b7st\u00f6\u00b7rer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PIAT", "NN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wo fern sie beide nur in gleicher wage stehn/", "tokens": ["Wo", "fern", "sie", "bei\u00b7de", "nur", "in", "glei\u00b7cher", "wa\u00b7ge", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PIS", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wo fern sie beide nur in einem gleisse gehn.", "tokens": ["Wo", "fern", "sie", "bei\u00b7de", "nur", "in", "ei\u00b7nem", "gleis\u00b7se", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PIS", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Kraut/ Wurtzel/ Safft vnd Tranck/ hab ich gar offt geh\u00f6ret/", "tokens": ["Kraut", "/", "Wurt\u00b7zel", "/", "Safft", "vnd", "Tranck", "/", "hab", "ich", "gar", "offt", "ge\u00b7h\u00f6\u00b7ret", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "$(", "VAFIN", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Zu brauchen ist vmbsonst/ damit Lieb werd gemehret.", "tokens": ["Zu", "brau\u00b7chen", "ist", "vmbsonst", "/", "da\u00b7mit", "Lieb", "werd", "ge\u00b7meh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "ADV", "$(", "KOUS", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.23": {"text": "Dann eins dem andern offt/ so brauchen will die kunst/", "tokens": ["Dann", "eins", "dem", "an\u00b7dern", "offt", "/", "so", "brau\u00b7chen", "will", "die", "kunst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ART", "ADJA", "ADV", "$(", "ADV", "VVINF", "VMFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Den Todt zu trincken gibt/ an statt der Lieb vnd gunst.", "tokens": ["Den", "Todt", "zu", "trin\u00b7cken", "gibt", "/", "an", "statt", "der", "Lieb", "vnd", "gunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "$(", "APPR", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Lieb selbst der Liebtranck ist/ lieb selbst die Lieb gebieret/", "tokens": ["Lieb", "selbst", "der", "Lieb\u00b7tranck", "ist", "/", "lieb", "selbst", "die", "Lieb", "ge\u00b7bie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VAFIN", "$(", "ADJD", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.26": {"text": "Wie in eim Thal vom schall ein widerschall herr\u00fceret/", "tokens": ["Wie", "in", "eim", "Thal", "vom", "schall", "ein", "wi\u00b7der\u00b7schall", "her\u00b7r\u00fce\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Lieb durch Tranck gemacht/ mit ha\u00df doch endlich lohnt.", "tokens": ["Die", "Lieb", "durch", "Tranck", "ge\u00b7macht", "/", "mit", "ha\u00df", "doch", "end\u00b7lich", "lohnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$(", "APPR", "VVIMP", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die Lieb durch Lieb gemacht/ bey zweyen ewig wohnt.", "tokens": ["Die", "Lieb", "durch", "Lieb", "ge\u00b7macht", "/", "bey", "zwe\u00b7yen", "e\u00b7wig", "wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$(", "APPR", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Von diesem Liebtranck nun beid Ehleuth sollen trincken/", "tokens": ["Von", "die\u00b7sem", "Lieb\u00b7tranck", "nun", "beid", "Eh\u00b7leuth", "sol\u00b7len", "trin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "PIAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wann in dem hau\u00dfstand will das Gl\u00fcck bi\u00dfweilen hincken:", "tokens": ["Wann", "in", "dem", "hau\u00df\u00b7stand", "will", "das", "Gl\u00fcck", "bi\u00df\u00b7wei\u00b7len", "hin\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wans/ nach de\u00df Ehestands brauch/ nit/ wie jhr wollet/ geht/", "tokens": ["Wans", "/", "nach", "de\u00df", "E\u00b7hes\u00b7tands", "brauch", "/", "nit", "/", "wie", "jhr", "wol\u00b7let", "/", "geht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$(", "APPR", "ART", "NN", "VVFIN", "$(", "PTKNEG", "$(", "PWAV", "PPER", "VMFIN", "$(", "VVFIN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Wann nicht der Windt allzeit/ wie jhr begehret/ weht.", "tokens": ["Wann", "nicht", "der", "Windt", "all\u00b7zeit", "/", "wie", "jhr", "be\u00b7ge\u00b7hret", "/", "weht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "ADV", "$(", "PWAV", "PPER", "VVFIN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ein solchen Liebtranck wolt einander fleissig bringen/", "tokens": ["Ein", "sol\u00b7chen", "Lieb\u00b7tranck", "wolt", "ein\u00b7an\u00b7der", "fleis\u00b7sig", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VMFIN", "PRF", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Bi\u00df jrgend in eim Jahr wir also weiden singen:", "tokens": ["Bi\u00df", "jr\u00b7gend", "in", "eim", "Jahr", "wir", "al\u00b7so", "wei\u00b7den", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPR", "ART", "NN", "PPER", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Secht was der Liebtranck hat gewirckt in dieser Zeit.", "tokens": ["Secht", "was", "der", "Lieb\u00b7tranck", "hat", "ge\u00b7wirckt", "in", "die\u00b7ser", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "ART", "NN", "VAFIN", "VVPP", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Di\u00df wird alsdann euch sein die h\u00f6chste lust vnd frewd.", "tokens": ["Di\u00df", "wird", "als\u00b7dann", "euch", "sein", "die", "h\u00f6chs\u00b7te", "lust", "vnd", "frewd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPER", "PPOSAT", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}