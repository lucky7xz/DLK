{"textgrid.poem.53262": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel wollt' euch vns mit dem bedinge zeigen,", "tokens": ["Der", "Him\u00b7mel", "wollt'", "euch", "vns", "mit", "dem", "be\u00b7din\u00b7ge", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das, B\u00f6hminn, wenn die Welt an Euch ein Bildt erkant,", "tokens": ["Das", ",", "B\u00f6h\u00b7minn", ",", "wenn", "die", "Welt", "an", "Euch", "ein", "Bildt", "er\u00b7kant", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NE", "$,", "KOUS", "ART", "NN", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein edles Frawen-Bildt, Euch Gott ohn Wiederstandt", "tokens": ["Ein", "ed\u00b7les", "Fra\u00b7wen\u00b7Bildt", ",", "Euch", "Gott", "ohn", "Wie\u00b7der\u00b7standt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Macht heim zu holen hett'. Ihr habt nun ewer eigen", "tokens": ["Macht", "heim", "zu", "ho\u00b7len", "hett'", ".", "Ihr", "habt", "nun", "e\u00b7wer", "ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "PTKZU", "VVINF", "VAFIN", "$.", "PPER", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die\u00df einig lassen sein durch Demuth, Zucht vnd schweigen,", "tokens": ["Die\u00df", "ei\u00b7nig", "las\u00b7sen", "sein", "durch", "De\u00b7muth", ",", "Zucht", "vnd", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVINF", "PPOSAT", "APPR", "NN", "$,", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch niemals-falschen Sinn vnd Tugend allerhandt", "tokens": ["Durch", "nie\u00b7mals\u00b7fal\u00b7schen", "Sinn", "vnd", "Tu\u00b7gend", "al\u00b7ler\u00b7handt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Erden kunt zu thun, ihr wehret dieses Pfandt", "tokens": ["Der", "Er\u00b7den", "kunt", "zu", "thun", ",", "ihr", "weh\u00b7ret", "die\u00b7ses", "Pfandt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$,", "PPER", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Frawen h\u00f6chstes Ziel, das wenig sonst ersteigen.", "tokens": ["Der", "Fra\u00b7wen", "h\u00f6chs\u00b7tes", "Ziel", ",", "das", "we\u00b7nig", "sonst", "er\u00b7stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gott nimpt, was sein ist, hin; ob er denn vnrecht thut,", "tokens": ["Gott", "nimpt", ",", "was", "sein", "ist", ",", "hin", ";", "ob", "er", "denn", "vn\u00b7recht", "thut", ","], "token_in