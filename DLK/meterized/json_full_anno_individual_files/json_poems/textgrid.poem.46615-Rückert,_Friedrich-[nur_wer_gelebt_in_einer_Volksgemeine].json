{"textgrid.poem.46615": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nur wer gelebt in einer Volksgemeine]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur wer gelebt in einer Volksgemeine,", "tokens": ["Nur", "wer", "ge\u00b7lebt", "in", "ei\u00b7ner", "Volks\u00b7ge\u00b7mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sollt' in derselben werden auch begraben;", "tokens": ["Sollt'", "in", "der\u00b7sel\u00b7ben", "wer\u00b7den", "auch", "be\u00b7gra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PDAT", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So m\u00f6chten sie ihr Fest zusammen haben,", "tokens": ["So", "m\u00f6ch\u00b7ten", "sie", "ihr", "Fest", "zu\u00b7sam\u00b7men", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Mondschein nun, wie sonst im Sonnenscheine.", "tokens": ["Im", "Mond\u00b7schein", "nun", ",", "wie", "sonst", "im", "Son\u00b7nen\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "PWAV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Euch aber, dich, mein T\u00f6chterlein, das kleine,", "tokens": ["Euch", "a\u00b7ber", ",", "dich", ",", "mein", "T\u00f6ch\u00b7ter\u00b7lein", ",", "das", "klei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "$,", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und dich, o meinen nicht viel gr\u00f6\u00dfern Knaben,", "tokens": ["Und", "dich", ",", "o", "mei\u00b7nen", "nicht", "viel", "gr\u00f6\u00b7\u00dfern", "Kna\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "VVFIN", "PTKNEG", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Statt, da\u00df sie hier euch euer Pl\u00e4tzchen gaben,", "tokens": ["Statt", ",", "da\u00df", "sie", "hier", "euch", "eu\u00b7er", "Pl\u00e4tz\u00b7chen", "ga\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bestattet w\u00fcnscht' ich euch im stillen Haine.", "tokens": ["Be\u00b7stat\u00b7tet", "w\u00fcnscht'", "ich", "euch", "im", "stil\u00b7len", "Hai\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr wart noch unter Leute nicht gekommen", "tokens": ["Ihr", "wart", "noch", "un\u00b7ter", "Leu\u00b7te", "nicht", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und m\u00fc\u00dft' hier unter lauter Fremd' euch mischen;", "tokens": ["Und", "m\u00fc\u00dft'", "hier", "un\u00b7ter", "lau\u00b7ter", "Fremd'", "euch", "mi\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie werdet ihr von ihnen aufgenommen?", "tokens": ["Wie", "wer\u00b7det", "ihr", "von", "ih\u00b7nen", "auf\u00b7ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hier ruht kein Ahn, mit Muth euch zu erfrischen,", "tokens": ["Hier", "ruht", "kein", "Ahn", ",", "mit", "Muth", "euch", "zu", "er\u00b7fri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und euer Vater hat erst nachzukommen;", "tokens": ["Und", "eu\u00b7er", "Va\u00b7ter", "hat", "erst", "nach\u00b7zu\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So nehm' euch Gott in seine Hut inzwischen!", "tokens": ["So", "nehm'", "euch", "Gott", "in", "sei\u00b7ne", "Hut", "in\u00b7zwi\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nur wer gelebt in einer Volksgemeine,", "tokens": ["Nur", "wer", "ge\u00b7lebt", "in", "ei\u00b7ner", "Volks\u00b7ge\u00b7mei\u00b7ne", ","], "t