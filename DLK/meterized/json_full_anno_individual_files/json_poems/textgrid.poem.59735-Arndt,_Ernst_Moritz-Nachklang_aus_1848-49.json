, "?", "Sie", "sind", "zu", "leicht", "er\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$.", "PPER", "VAFIN", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihm gerufen und geschrien.", "tokens": ["Die", "ihm", "ge\u00b7ru\u00b7fen", "und", "ge\u00b7schri\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er liegt bis heute fest gebunden \u2013", "tokens": ["Er", "liegt", "bis", "heu\u00b7te", "fest", "ge\u00b7bun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So spricht der stumme Stein f\u00fcr ihn:", "tokens": ["So", "spricht", "der", "stum\u00b7me", "Stein", "f\u00fcr", "ihn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbwie? Wagt ihr mir den Schlaf zu strafen", "tokens": ["\u00bb", "wie", "?", "Wagt", "ihr", "mir", "den", "Schlaf", "zu", "stra\u00b7fen"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "$.", "VVFIN", "PPER", "PRF", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In eurer feigen Ungeduld?", "tokens": ["In", "eu\u00b7rer", "fei\u00b7gen", "Un\u00b7ge\u00b7duld", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wi\u00dft, weil ihr schnarchet, mu\u00df ich schlafen \u2013", "tokens": ["Wi\u00dft", ",", "weil", "ihr", "schnar\u00b7chet", ",", "mu\u00df", "ich", "schla\u00b7fen", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Straft eurer eignen Faulheit Schuld.\u00ab", "tokens": ["Straft", "eu\u00b7rer", "eig\u00b7nen", "Faul\u00b7heit", "Schuld", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}