{"textgrid.poem.54551": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So reist holdselges Kind der unverhoffte Tod", "tokens": ["So", "reist", "hold\u00b7sel\u00b7ges", "Kind", "der", "un\u00b7ver\u00b7hoff\u00b7te", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich von uns hin/ und macht kaum inner einem Lichte/", "tokens": ["Dich", "von", "uns", "hin", "/", "und", "macht", "kaum", "in\u00b7ner", "ei\u00b7nem", "Lich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "PTKVZ", "$(", "KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das liebreichst Ebenbild der Anmuth gantz zu nichte.", "tokens": ["Das", "lieb\u00b7reichst", "E\u00b7ben\u00b7bild", "der", "An\u00b7muth", "gantz", "zu", "nich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "ART", "NN", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach kaum erh\u00f6rter Fall! durch rauher schmertzen Noth.", "tokens": ["Ach", "kaum", "er\u00b7h\u00f6r\u00b7ter", "Fall", "!", "durch", "rau\u00b7her", "schmert\u00b7zen", "Noth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADJA", "NN", "$.", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "li