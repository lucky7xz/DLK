{"textgrid.poem.35362": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer in der Heimat keine Ruhe fand", "tokens": ["Wer", "in", "der", "Hei\u00b7mat", "kei\u00b7ne", "Ru\u00b7he", "fand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und drau\u00dfen auf der See sie auch nicht findet", "tokens": ["Und", "drau\u00b7\u00dfen", "auf", "der", "See", "sie", "auch", "nicht", "fin\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "PPER", "ADV", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nun sich nachts auf seinem Lager windet", "tokens": ["Und", "nun", "sich", "nachts", "auf", "sei\u00b7nem", "La\u00b7ger", "win\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dr\u00fcckt sein Ohr an der Kabine Wand", "tokens": ["Und", "dr\u00fcckt", "sein", "Ohr", "an", "der", "Ka\u00b7bi\u00b7ne", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und lauscht den gro\u00dfen Wassern, stets bewegt,", "tokens": ["Und", "lauscht", "den", "gro\u00b7\u00dfen", "Was\u00b7sern", ",", "stets", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und h\u00f6rt ihr Pl\u00e4tschern auf die leichten Bretter,", "tokens": ["Und", "h\u00f6rt", "ihr", "Pl\u00e4t\u00b7schern", "auf", "die", "leich\u00b7ten", "Bret\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der denkt gewi\u00df, beim sch\u00f6nsten Reisewetter,", "tokens": ["Der", "denkt", "ge\u00b7wi\u00df", ",", "beim", "sch\u00f6ns\u00b7ten", "Rei\u00b7se\u00b7wet\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df sich der Tod an seine Seite legt;", "tokens": ["Da\u00df", "sich", "der", "Tod", "an", "sei\u00b7ne", "Sei\u00b7te", "legt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und wenn es pl\u00f6tzlich hei\u00dft: Das Schiff ist leck,", "tokens": ["Und", "wenn", "es", "pl\u00f6tz\u00b7lich", "hei\u00dft", ":", "Das", "Schiff", "ist", "leck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und schwer, wie uns bed\u00fcnkt, das Loch zu stopfen \u2013", "tokens": ["Und", "schwer", ",", "wie", "uns", "be\u00b7d\u00fcnkt", ",", "das", "Loch", "zu", "stop\u00b7fen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wird freilich mehr als ein gelinder Schreck", "tokens": ["Wird", "frei\u00b7lich", "mehr", "als", "ein", "ge\u00b7lin\u00b7der", "Schreck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIAT", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Dem armen Manne auf die Beine klopfen.", "tokens": ["Dem", "ar\u00b7men", "Man\u00b7ne", "auf", "die", "Bei\u00b7ne", "klop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Doch wenn nun ein entfesselter Orkan", "tokens": ["Doch", "wenn", "nun", "ein", "ent\u00b7fes\u00b7sel\u00b7ter", "Or\u00b7kan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Sich t\u00e4ppisch heftet an den morschen Kasten,", "tokens": ["Sich", "t\u00e4p\u00b7pisch", "hef\u00b7tet", "an", "den", "mor\u00b7schen", "Kas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Da\u00df jede Planke st\u00f6hnt und da\u00df die Masten", "tokens": ["Da\u00df", "je\u00b7de", "Plan\u00b7ke", "st\u00f6hnt", "und", "da\u00df", "die", "Mas\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "KON", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Sich beugen m\u00fcssen vor dem Grobian,", "tokens": ["Sich", "beu\u00b7gen", "m\u00fcs\u00b7sen", "vor", "dem", "Gro\u00b7bi\u00b7an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Dann tobt das alte Meer in Donnerlauten,", "tokens": ["Dann", "tobt", "das", "al\u00b7te", "Meer", "in", "Don\u00b7ner\u00b7lau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Dann f\u00e4hrt durchs Takelwerk ein b\u00f6ser Pfiff,", "tokens": ["Dann", "f\u00e4hrt", "durchs", "Ta\u00b7kel\u00b7werk", "ein", "b\u00f6\u00b7ser", "Pfiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Dann wird zur H\u00f6lle das geplagte Schiff", "tokens": ["Dann", "wird", "zur", "H\u00f6l\u00b7le", "das", "ge\u00b7plag\u00b7te", "Schiff"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "F\u00fcr den mit seinem Wesen nicht Vertrauten.", "tokens": ["F\u00fcr", "den", "mit", "sei\u00b7nem", "We\u00b7sen", "nicht", "Ver\u00b7trau\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "PPOSAT", "NN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Mag ihren Schaum, den aufgew\u00fchlten, gelben,", "tokens": ["Mag", "ih\u00b7ren", "Schaum", ",", "den", "auf\u00b7ge\u00b7w\u00fchl\u00b7ten", ",", "gel\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Die z\u00fcgellose See zum Himmel spritzen;", "tokens": ["Die", "z\u00fc\u00b7gel\u00b7lo\u00b7se", "See", "zum", "Him\u00b7mel", "sprit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Mag sie im Diamantenschmucke blitzen", "tokens": ["Mag", "sie", "im", "Di\u00b7a\u00b7man\u00b7ten\u00b7schmu\u00b7cke", "blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Sobald die Nacht erscheint \u2013 es sind dieselben", "tokens": ["So\u00b7bald", "die", "Nacht", "er\u00b7scheint", "\u2013", "es", "sind", "die\u00b7sel\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$(", "PPER", "VAFIN", "PDAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Grausamen Spiele f\u00fcr den Passagier,", "tokens": ["Grau\u00b7sa\u00b7men", "Spie\u00b7le", "f\u00fcr", "den", "Pas\u00b7sa\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.26": {"text": "Der fr\u00fcher sich an Traumgebilden sonnte,", "tokens": ["Der", "fr\u00fc\u00b7her", "sich", "an", "Traum\u00b7ge\u00b7bil\u00b7den", "sonn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Die er beherrschen und ver\u00e4ndern konnte,", "tokens": ["Die", "er", "be\u00b7herr\u00b7schen", "und", "ver\u00b7\u00e4n\u00b7dern", "konn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Er, so beklommen und so kleinlaut hier! \u2013 \u2013", "tokens": ["Er", ",", "so", "be\u00b7klom\u00b7men", "und", "so", "klein\u00b7laut", "hier", "!", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD", "ADV", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Die Rettungsb\u00f6te rei\u00dft der Wogen Kamm", "tokens": ["Die", "Ret\u00b7tungs\u00b7b\u00f6\u00b7te", "rei\u00dft", "der", "Wo\u00b7gen", "Kamm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Hinunter vom Verdeck und auf die nackte", "tokens": ["Hin\u00b7un\u00b7ter", "vom", "Ver\u00b7deck", "und", "auf", "die", "nack\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "KON", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "H\u00fclflose Schale st\u00fcrzen Katarakte \u2013 \u2013", "tokens": ["H\u00fcl\u00b7flo\u00b7se", "Scha\u00b7le", "st\u00fcr\u00b7zen", "Ka\u00b7ta\u00b7rak\u00b7te", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVFIN", "NN", "$(", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.32": {"text": "Kaptain und Mannschaft bleiben dennoch stramm;", "tokens": ["Kap\u00b7ta\u00b7in", "und", "Mann\u00b7schaft", "blei\u00b7ben", "den\u00b7noch", "stramm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.33": {"text": "Der echte Seemann, nervenstark und z\u00e4he,", "tokens": ["Der", "ech\u00b7te", "See\u00b7mann", ",", "ner\u00b7ven\u00b7stark", "und", "z\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.34": {"text": "Entsetzt sich nicht vor des Verderbens N\u00e4he;", "tokens": ["Ent\u00b7setzt", "sich", "nicht", "vor", "des", "Ver\u00b7der\u00b7bens", "N\u00e4\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Er h\u00e4lt auf seinem Posten scharfe Wacht,", "tokens": ["Er", "h\u00e4lt", "auf", "sei\u00b7nem", "Pos\u00b7ten", "schar\u00b7fe", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Gefa\u00dft, doch mit dem Auge eines Falken;", "tokens": ["Ge\u00b7fa\u00dft", ",", "doch", "mit", "dem", "Au\u00b7ge", "ei\u00b7nes", "Fal\u00b7ken", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Und wenn das Fahrzeug auseinanderkracht,", "tokens": ["Und", "wenn", "das", "Fahr\u00b7zeug", "aus\u00b7ein\u00b7