{"textgrid.poem.25539": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ich gro\u00dfe Dichter lese,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich gro\u00dfe Dichter lese,", "tokens": ["Wenn", "ich", "gro\u00b7\u00dfe", "Dich\u00b7ter", "le\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie bescheiden", "tokens": ["Wie", "be\u00b7schei\u00b7den"], "token_info": ["word", "word"], "pos": ["PWAV", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Denk' ich von mir selber dann!", "tokens": ["Denk'", "ich", "von", "mir", "sel\u00b7ber", "dann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ich mich in ganzen N\u00e4chten", "tokens": ["Wie", "ich", "mich", "in", "gan\u00b7zen", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von den Freuden", "tokens": ["Von", "den", "Freu\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ihres Ruhms, nicht m\u00fcde sprechen kann!", "tokens": ["Ih\u00b7res", "Ruhms", ",", "nicht", "m\u00fc\u00b7de", "spre\u00b7chen", "kann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Les' ich aber deine Briefchen", "tokens": ["Les'", "ich", "a\u00b7ber", "dei\u00b7ne", "Brief\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und Ges\u00e4nge,", "tokens": ["Und", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Ha! wie werd' ich stolz und stumm!", "tokens": ["Ha", "!", "wie", "werd'", "ich", "stolz", "und", "stumm", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Alle gro\u00dfe M\u00e4nner tauschten", "tokens": ["Al\u00b7le", "gro\u00b7\u00dfe", "M\u00e4n\u00b7ner", "tauschten"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ihr Gepr\u00e4nge", "tokens": ["Ihr", "Ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Gegen deine Liebe mit mir um!", "tokens": ["Ge\u00b7gen", "dei\u00b7ne", "Lie\u00b7be", "mit", "mir", "um", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn ich gro\u00dfe Dichter lese,", "tokens": ["Wenn", "ich", "gro\u00b7\u00dfe", "Dich\u00b7ter", "le\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie besche