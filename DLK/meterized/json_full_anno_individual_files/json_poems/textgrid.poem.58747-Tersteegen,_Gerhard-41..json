{"textgrid.poem.58747": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin krank vor Liebe. Hoheslied 2, 5", "tokens": ["Ich", "bin", "krank", "vor", "Lie\u00b7be", ".", "Ho\u00b7hes\u00b7lied", "2", ",", "5"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "number", "punct", "number"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "NN", "$.", "NN", "CARD", "$,", "CARD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Du selber, Gotteslieb', hast mich ans Kreuz geschlagen,", "tokens": ["Du", "sel\u00b7ber", ",", "Got\u00b7tes\u00b7lieb'", ",", "hast", "mich", "ans", "Kreuz", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich kann mich dr\u00fcber nicht beklagen;", "tokens": ["Ich", "kann", "mich", "dr\u00fc\u00b7ber", "nicht", "be\u00b7kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PAV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fcrcht' es nicht, ich lieb' vielmehr die leichte Last,", "tokens": ["Ich", "f\u00fcrcht'", "es", "nicht", ",", "ich", "lieb'", "viel\u00b7mehr", "die", "leich\u00b7te", "Last", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.