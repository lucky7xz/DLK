{"textgrid.poem.34365": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: So soll ich dich verlassen, liebes Zimmer,", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So soll ich dich verlassen, liebes Zimmer,", "tokens": ["So", "soll", "ich", "dich", "ver\u00b7las\u00b7sen", ",", "lie\u00b7bes", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo in mein Herz der Himmel niedersank,", "tokens": ["Wo", "in", "mein", "Herz", "der", "Him\u00b7mel", "nie\u00b7der\u00b7sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den ich aus ihrem Blick, wie seelig, aus dem Schimmer", "tokens": ["Den", "ich", "aus", "ih\u00b7rem", "Blick", ",", "wie", "see\u00b7lig", ",", "aus", "dem", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Gottheit auf der Wange trank,", "tokens": ["Der", "Got\u00b7theit", "auf", "der", "Wan\u00b7ge", "trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn sich ihr Herz nach ihm, nach ihm emp\u00f6rte,", "tokens": ["Wenn", "sich", "ihr", "Herz", "nach", "ihm", ",", "nach", "ihm", "em\u00b7p\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "APPR", "PPER", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und ihr entz\u00fccktes Ohr der Sph\u00e4ren Wollaut h\u00f6rte,", "tokens": ["Und", "ihr", "ent\u00b7z\u00fcck\u00b7tes", "Ohr", "der", "Sph\u00e4\u00b7ren", "Wol\u00b7laut", "h\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn sie mit Shakespeare der ihren Geist umfieng", "tokens": ["Wenn", "sie", "mit", "Sha\u00b7ke\u00b7spe\u00b7a\u00b7re", "der", "ih\u00b7ren", "Geist", "um\u00b7fi\u00b7eng"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NE", "ART", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.8": {"text": "Ha zitternd oft f\u00fcr Furcht und Freude,", "tokens": ["Ha", "zit\u00b7ternd", "oft", "f\u00fcr", "Furcht", "und", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der Engel Lust im s\u00fc\u00dfen Unschuldskleide,", "tokens": ["Der", "En\u00b7gel", "Lust", "im", "s\u00fc\u00b7\u00dfen", "Un\u00b7schulds\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "In die Mysterien des hohen Schicksals gieng:", "tokens": ["In", "die", "Mys\u00b7te\u00b7ri\u00b7en", "des", "ho\u00b7hen", "Schick\u00b7sals", "gieng", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Auch ich sah ihren Pfad, auch mir", "tokens": ["Auch", "ich", "sah", "ih\u00b7ren", "Pfad", ",", "auch", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "War es verg\u00f6nnt ein R\u00f6schen drauf zu streuen,", "tokens": ["War", "es", "ver\u00b7g\u00f6nnt", "ein", "R\u00f6\u00b7schen", "drauf", "zu", "streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "ART", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Zur Priesterinn des Gottes sie zu weihen", "tokens": ["Zur", "Pries\u00b7te\u00b7rinn", "des", "Got\u00b7tes", "sie", "zu", "wei\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und hinzuknieen vor ihm und ihr.", "tokens": ["Und", "hin\u00b7zu\u00b7kni\u00b7e\u00b7en", "vor", "ihm", "und", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "KON", "PPER", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ach w\u00e4r ich nur so rein gewesen,", "tokens": ["Ach", "w\u00e4r", "ich", "nur", "so", "rein", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als die Erscheinung dieses Gl\u00fccks", "tokens": ["Als", "die", "Er\u00b7schei\u00b7nung", "die\u00b7ses", "Gl\u00fccks"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vorausgesezt! Ihr h\u00f6hern Wesen,", "tokens": ["Vor\u00b7aus\u00b7ge\u00b7sezt", "!", "Ihr", "h\u00f6\u00b7hern", "We\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verzeiht dem Strauchelnden, euch waren sie erlesen;", "tokens": ["Ver\u00b7zeiht", "dem", "Strau\u00b7cheln\u00b7den", ",", "euch", "wa\u00b7ren", "sie", "er\u00b7le\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch Ewigkeiten Lust sind Kranken, die genesen,", "tokens": ["Doch", "E\u00b7wig\u00b7kei\u00b7ten", "Lust", "sind", "Kran\u00b7ken", ",", "die", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nur Freuden eines Augenblicks.", "tokens": ["Nur", "Freu\u00b7den", "ei\u00b7nes", "Au\u00b7gen\u00b7blicks", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja es erwarten dich du Himmelskind! der Freuden", "tokens": ["Ja", "es", "er\u00b7war\u00b7ten", "dich", "du", "Him\u00b7mel\u00b7skind", "!", "der", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "PPER", "VVFIN", "PPER", "PPER", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Unz\u00e4hlige, durch selbstgemachte Leiden", "tokens": ["Un\u00b7z\u00e4h\u00b7li\u00b7ge", ",", "durch", "selbst\u00b7ge\u00b7mach\u00b7te", "Lei\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJA", "$,", "APPR", "ADJA", "NN"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Dir unbegreiflich, l\u00e4ngst erkauft,", "tokens": ["Dir", "un\u00b7be\u00b7greif\u00b7lich", ",", "l\u00e4ngst", "er\u00b7kauft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Tr\u00e4nen ingeheim getauft.", "tokens": ["Mit", "Tr\u00e4\u00b7nen", "in\u00b7ge\u00b7heim", "ge\u00b7tauft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja es erwartet dich, was du nicht l\u00f6sen k\u00f6nntest,", "tokens": ["Ja", "es", "er\u00b7war\u00b7tet", "dich", ",", "was", "du", "nicht", "l\u00f6\u00b7sen", "k\u00f6nn\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "PPER", "$,", "PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der R\u00e4zel Allentwickelung,", "tokens": ["Der", "R\u00e4\u00b7zel", "Al\u00b7lent\u00b7wi\u00b7cke\u00b7lung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"