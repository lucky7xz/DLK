{"dta.poem.13248": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201emir scheints, es fl\u00f6sse GOtt bereits in meine Sinnen:", "tokens": ["\u201e", "mir", "scheints", ",", "es", "fl\u00f6s\u00b7se", "Gott", "be\u00b7reits", "in", "mei\u00b7ne", "Sin\u00b7nen", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eer seegne seinen Wunsch und dieses sein Beginnen.", "tokens": ["\u201e", "er", "seeg\u00b7ne", "sei\u00b7nen", "Wunsch", "und", "die\u00b7ses", "sein", "Be\u00b7gin\u00b7nen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPOSAT", "NN", "KON", "PDS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ebewaffnet, r\u00fcstet euch! das Ende kr\u00f6nt den Sieg;", "tokens": ["\u201e", "be\u00b7waff\u00b7net", ",", "r\u00fcs\u00b7tet", "euch", "!", "das", "En\u00b7de", "kr\u00f6nt", "den", "Sieg", ";"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "PPER", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "210\u201dDie Krone kom\u0303t von GOtt; von ihm kom\u0303t Fried und Krieg.", "tokens": ["\"", "Die", "Kro\u00b7ne", "kom\u0303t", "von", "Gott", ";", "von", "ihm", "kom\u0303t", "Fried", "und", "Krieg", "."],