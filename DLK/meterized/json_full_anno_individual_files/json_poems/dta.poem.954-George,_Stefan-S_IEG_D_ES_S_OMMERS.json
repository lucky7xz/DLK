{"dta.poem.954": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "S IEG   D ES   S OMMERS", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Gemahnt dich noch das sch\u00f6ne bildnis dessen               ", "tokens": ["Ge\u00b7mahnt", "dich", "noch", "das", "sch\u00f6\u00b7ne", "bild\u00b7nis", "des\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "PPER", "ADV", "ART", "ADJA", "NN", "PDS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der nach den schluchten-rosen k&#252;hn gehascht               ", "tokens": ["Der", "nach", "den", "schluch\u00b7ten\u00b7ro\u00b7sen", "k", "&#252;", "hn", "ge\u00b7hascht"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$(", "ADJD", "VVPP"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der &#252;ber seiner jagd den tag vergessen               ", "tokens": ["Der", "&#252;", "ber", "sei\u00b7ner", "jagd", "den", "tag", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$(", "APPR", "PPOSAT", "NN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der von der dolden vollem seim genascht             ", "tokens": ["Der", "von", "der", "dol\u00b7den", "vol\u00b7lem", "seim", "ge\u00b7nascht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der nach dem parke sich zur ruhe wandte               ", "tokens": ["Der", "nach", "dem", "par\u00b7ke", "sich", "zur", "ru\u00b7he", "wand\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Trieb ihn ein fl&#252;gelschillern allzuweit               ", "tokens": ["Trieb", "ihn", "ein", "fl", "&#252;", "gel\u00b7schil\u00b7lern", "all\u00b7zu\u00b7weit"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "$(", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Der sinnend sass an jenes weihers kante               ", "tokens": ["Der", "sin\u00b7nend", "sass", "an", "je\u00b7nes", "wei\u00b7hers", "kan\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "PDAT", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und lauschte in die tiefe heimlichkeit &#183; &#183;             ", "tokens": ["Und", "lauschte", "in", "die", "tie\u00b7fe", "heim\u00b7lich\u00b7keit", "&#183;", "&#183;"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "XML_entity"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$(", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und von der insel moosgekr\u00f6nter steine               ", "tokens": ["Und", "von", "der", "in\u00b7sel", "moos\u00b7ge\u00b7kr\u00f6n\u00b7ter", "stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "FM", "FM", "FM"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verliess der schwan das spiel des wasserfalls               ", "tokens": ["Ver\u00b7liess", "der", "schwan", "das", "spiel", "des", "was\u00b7ser\u00b7falls"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "VVFIN", "PDS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und legte in die kinderhand die feine               ", "tokens": ["Und", "leg\u00b7te", "in", "die", "kin\u00b7der\u00b7hand", "die", "fei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die schmeichelnde den schlanken hals.             ", "tokens": ["Die", "schmei\u00b7cheln\u00b7de", "den", "schlan\u00b7ken", "hals", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}