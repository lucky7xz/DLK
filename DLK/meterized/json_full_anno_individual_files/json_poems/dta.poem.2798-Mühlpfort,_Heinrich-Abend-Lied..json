line.45": {"text": "Von dem Bette zu dem Grab/", "tokens": ["Von", "dem", "Bet\u00b7te", "zu", "dem", "Grab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Trett ich nun im Wechsel ab/", "tokens": ["Trett", "ich", "nun", "im", "Wech\u00b7sel", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Und di\u00df Vorbild lehrt mich eben", "tokens": ["Und", "di\u00df", "Vor\u00b7bild", "lehrt", "mich", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Munter meinen Geist aufgeben.", "tokens": ["Mun\u00b7ter", "mei\u00b7nen", "Geist", "auf\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Ich befehl in deine H\u00e4nde/", "tokens": ["Ich", "be\u00b7fehl", "in", "dei\u00b7ne", "H\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Diese Handvoll meiner Jahr.", "tokens": ["Die\u00b7se", "Hand\u00b7voll", "mei\u00b7ner", "Jahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Gieb mir ein vern\u00fcnfftig Ende/", "tokens": ["Gieb", "mir", "ein", "ver\u00b7n\u00fcnff\u00b7tig", "En\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJD", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Und da\u00df ich von hinnen fahr/", "tokens": ["Und", "da\u00df", "ich", "von", "hin\u00b7nen", "fahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "In die Freude die kein Ohr", "tokens": ["In", "die", "Freu\u00b7de", "die", "kein", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.54": {"text": "Nie geh\u00f6ret/ und zum Chor", "tokens": ["Nie", "ge\u00b7h\u00f6\u00b7ret", "/", "und", "zum", "Chor"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "KON", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Das kein einzig Mensch vernommen/", "tokens": ["Das", "kein", "ein\u00b7zig", "Mensch", "ver\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJD", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Bi\u00df er selber drein ist kommen.", "tokens": ["Bi\u00df", "er", "sel\u00b7ber", "drein", "ist", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Segne GOtt des Geistes Gaben/", "tokens": ["Seg\u00b7ne", "Gott", "des", "Geis\u00b7tes", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Schencke mir der Wei\u00dfheit Schatz/", "tokens": ["Schen\u00b7cke", "mir", "der", "Wei\u00df\u00b7heit", "Schatz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "Ich mag nicht gro\u00df Reichthum haben/", "tokens": ["Ich", "mag", "nicht", "gro\u00df", "Reicht\u00b7hum", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "NN", "VAFIN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.60": {"text": "Denn das macht den Lastern Platz.", "tokens": ["Denn", "das", "macht", "den", "Las\u00b7tern", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.61": {"text": "Gieb mir mein bescheiden Theil/", "tokens": ["Gieb", "mir", "mein", "be\u00b7schei\u00b7den", "Theil", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.62": {"text": "Und von oben Gl\u00fcck und Heil.", "tokens": ["Und", "von", "o\u00b7ben", "Gl\u00fcck", "und", "Heil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.63": {"text": "So wird nie mein Mund sich sperren/", "tokens": ["So", "wird", "nie", "mein", "Mund", "sich", "sper\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.64": {"text": "Dich zu preisen/ seinen HErren.", "tokens": ["Dich", "zu", "prei\u00b7sen", "/", "sei\u00b7nen", "Her\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$(", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}