{"dta.poem.2798": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Abend-Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott/ ich will dir itzt lobsingen/", "tokens": ["Gott", "/", "ich", "will", "dir", "itzt", "lob\u00b7sin\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mein Opffer tragen f\u00fcr/", "tokens": ["Und", "mein", "Opf\u00b7fer", "tra\u00b7gen", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herr/ O h\u00fclff/ und la\u00df gelingen/", "tokens": ["Herr", "/", "O", "h\u00fclff", "/", "und", "la\u00df", "ge\u00b7lin\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "NE", "$(", "KON", "VVIMP", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich dancke nach Geb\u00fchr/", "tokens": ["Da\u00df", "ich", "dan\u00b7cke", "nach", "Ge\u00b7b\u00fchr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deiner treuen Vater-Hand/", "tokens": ["Dei\u00b7ner", "treu\u00b7en", "Va\u00b7ter\u00b7Hand", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die Du stets auf mich gewandt/", "tokens": ["Die", "Du", "stets", "auf", "mich", "ge\u00b7wa