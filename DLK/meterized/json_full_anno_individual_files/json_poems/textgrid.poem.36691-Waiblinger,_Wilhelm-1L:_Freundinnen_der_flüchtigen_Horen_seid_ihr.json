{"textgrid.poem.36691": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freundinnen der fl\u00fcchtigen Horen seid ihr", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freundinnen der fl\u00fcchtigen Horen seid ihr", "tokens": ["Freun\u00b7din\u00b7nen", "der", "fl\u00fcch\u00b7ti\u00b7gen", "Ho\u00b7ren", "seid", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "VAFIN", "PPER"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "T\u00f6ne doch vor allen, geheim im Bunde", "tokens": ["T\u00f6\u00b7ne", "doch", "vor", "al\u00b7len", ",", "ge\u00b7heim", "im", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PIAT", "$,", "ADJD", "APPRART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Steht ihr, und das Sch\u00f6nste, die Seele nach dem", "tokens": ["Steht", "ihr", ",", "und", "das", "Sch\u00f6ns\u00b7te", ",", "die", "See\u00b7le", "nach", "dem"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "ART", "NN", "$,", "ART", "NN", "APPR", "ART"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Traurigen Tode", "tokens": ["Trau\u00b7ri\u00b7gen", "To\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Lassen jene Genien zur\u00fcck in eurer", "tokens": ["Las\u00b7sen", "je\u00b7ne", "Ge\u00b7ni\u00b7en", "zu\u00b7r\u00fcck", "in", "eu\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN", "PTKVZ", "APPR", "PPOSAT"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sanften unverg\u00e4nglichen Macht und Sch\u00f6ne,", "tokens": ["Sanf\u00b7ten", "un\u00b7ver\u00b7g\u00e4ng\u00b7li\u00b7chen", "Macht", "und", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Ja ihr weckt sie immer zu neuem Leben", "tokens": ["Ja", "ihr", "weckt", "sie", "im\u00b7mer", "zu", "neu\u00b7em", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Selbst aus dem Grab' auf.", "tokens": ["Selbst", "aus", "dem", "Grab'", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Meine Kindheit schlie\u00dft mir im Fl\u00f6tenklange", "tokens": ["Mei\u00b7ne", "Kind\u00b7heit", "schlie\u00dft", "mir", "im", "Fl\u00f6\u00b7ten\u00b7klan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Ihre Rosenwelt und den tiefen Kelch auf,", "tokens": ["Ih\u00b7re", "Ro\u00b7sen\u00b7welt", "und", "den", "tie\u00b7fen", "Kelch", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Dessen Duft einst, wie der Gedank' im Herzen,", "tokens": ["Des\u00b7sen", "Duft", "einst", ",", "wie", "der", "Ge\u00b7dank'", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Lange geschlummert.", "tokens": ["Lan\u00b7ge", "ge\u00b7schlum\u00b7mert", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie verm\u00f6cht' ich jenen Gesang, die Stimme", "tokens": ["Wie", "ver\u00b7m\u00f6cht'", "ich", "je\u00b7nen", "Ge\u00b7sang", ",", "die", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PDAT", "NN", "$,", "ART", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihrer hei\u00dfen Sehnsucht, der ersten Liebe", "tokens": ["Ih\u00b7rer", "hei\u00b7\u00dfen", "Sehn\u00b7sucht", ",", "der", "ers\u00b7ten", "Lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Klagelaut, und all' das uns\u00e4glich Zarte", "tokens": ["Kla\u00b7ge\u00b7laut", ",", "und", "all'", "das", "un\u00b7s\u00e4g\u00b7lich", "Zar\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "PIS", "ART", "ADJD", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Noch zu ertragen,", "tokens": ["Noch", "zu", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Wenn's einmal in rauschenden Melodien", "tokens": ["Wenn's", "ein\u00b7mal", "in", "rau\u00b7schen\u00b7den", "Me\u00b7lo\u00b7dien"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Freudejauchzend, ach aus so ganz verlornen", "tokens": ["Freu\u00b7de\u00b7jauch\u00b7zend", ",", "ach", "aus", "so", "ganz", "ver\u00b7lor\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "ADV", "APPR", "ADV", "ADV", "VVINF"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Blumentagen, jubelnd zur\u00fcck ins Herz kehrt,", "tokens": ["Blu\u00b7men\u00b7ta\u00b7gen", ",", "ju\u00b7belnd", "zu\u00b7r\u00fcck", "ins", "Herz", "kehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PTKVZ", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Wo es gestorben.", "tokens": ["Wo", "es", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Das, o T\u00f6ne, wie ich auch oft es f\u00fchle,", "tokens": ["Das", ",", "o", "T\u00f6\u00b7ne", ",", "wie", "ich", "auch", "oft", "es", "f\u00fch\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "FM", "NN", "$,", "PWAV", "PPER", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Das ertr\u00fcg' ich nicht. Denn der Freud' und Jugend", "tokens": ["Das", "er\u00b7tr\u00fcg'", "ich", "nicht", ".", "Denn", "der", "Freud'", "und", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "$.", "KON", "ART", "NN", "KON", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Schwand mir so viel, da\u00df die Erinn'rung nicht, nur", "tokens": ["Schwand", "mir", "so", "viel", ",", "da\u00df", "die", "Er\u00b7inn'\u00b7rung", "nicht", ",", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "ART", "NN", "PTKNEG", "$,", "ADV"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Lethe mich tr\u00f6stet.", "tokens": ["Le\u00b7the", "mich", "tr\u00f6s\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Eines aber lieb' ich, wenn meiner Leiden", "tokens": ["Ei\u00b7nes", "a\u00b7ber", "lieb'", "ich", ",", "wenn", "mei\u00b7ner", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Und Verluste schmerzlicher Seufzerlaut und", "tokens": ["Und", "Ver\u00b7lus\u00b7te", "schmerz\u00b7li\u00b7cher", "Seuf\u00b7zer\u00b7laut", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADJA", "NN", "KON"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "All' mein Weh, gleich Aeolus L\u00fcften, leise", "tokens": ["All'", "mein", "Weh", ",", "gleich", "A\u00b7e\u00b7o\u00b7lus", "L\u00fcf\u00b7ten", ",", "lei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "ADV", "NE", "NN", "$,", "ADJD"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Mir in des Herzens", "tokens": ["Mir", "in", "des", "Her\u00b7zens"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "D\u00fcstre tiefzerfallne Ruine spielet:", "tokens": ["D\u00fcst\u00b7re", "tief\u00b7zer\u00b7fall\u00b7ne", "Ru\u00b7i\u00b7ne", "spie\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Denn mir ist, als k\u00e4men die Geister meiner", "tokens": ["Denn", "mir", "ist", ",", "als", "k\u00e4\u00b7men", "die", "Geis\u00b7ter", "mei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOUS", "VVFIN", "ART", "NN", "PPOSAT"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Lieben schon von Jenseits zur\u00fcck in solchen", "tokens": ["Lie\u00b7ben", "schon", "von", "Jen\u00b7seits", "zu\u00b7r\u00fcck", "in", "sol\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "ADV", "APPR", "NE", "PTKVZ", "APPR", "PIAT"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Sel'gen Accorden.", "tokens": ["Sel'\u00b7gen", "Ac\u00b7cor\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Freundinnen der fl\u00fcchtigen Horen seid ihr", "tokens": ["Freun\u00b7din\u00b7nen", "der", "fl\u00fcch\u00b7ti\u00b7gen", "Ho\u00b7ren", "seid", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "VAFIN", "PPER"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "T\u00f6ne doch vor allen, geheim im Bunde", "tokens": ["T\u00f6\u00b7ne", "doch", "vor", "al\u00b7len", ",", "ge\u00b7heim", "im", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", 