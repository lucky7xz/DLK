{"dta.poem.7748": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich liebe nicht, da\u00df ihr des Himmels goldne Thronen", "tokens": ["Ich", "lie\u00b7be", "nicht", ",", "da\u00df", "ihr", "des", "Him\u00b7mels", "gold\u00b7ne", "Thro\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit K\u00f6nigsnamen auch besetzt wie Erdenzonen.", "tokens": ["Mit", "K\u00f6\u00b7nigs\u00b7na\u00b7men", "auch", "be\u00b7setzt", "wie", "Er\u00b7den\u00b7zo\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die M\u00e4cht'gen machen sich auf Erden breit genug,", "tokens": ["Die", "M\u00e4cht'\u00b7gen", "ma\u00b7chen", "sich", "auf", "Er\u00b7den", "breit", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Himmel ihnen auch zu r\u00e4umen ist nicht klug.", "tokens": ["Den", "Him\u00b7mel", "ih\u00b7nen", "auch", "zu", "r\u00e4u\u00b7men", "ist", "nicht", "klug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "PTKZU", "VVINF", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "La\u00dft dort nur ungest\u00f6rt Chim\u00e4ren und Zentauern,", "tokens": ["La\u00dft", "dort", "nur", "un\u00b7ge\u00b7st\u00f6rt", "Chi\u00b7m\u00e4\u00b7ren", "und", "Zen\u00b7tau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Unthier' und Ungeheur, einmal verewigt, dauern.", "tokens": ["Un\u00b7thier'", "und", "Un\u00b7ge\u00b7heur", ",", "ein\u00b7mal", "ve\u00b7re\u00b7wigt", ",", "dau\u00b7ern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADV", "VVPP", "$,", "VVINF", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Und wenn ihr f\u00fcllen wollt noch leer gebliebne Strecken,", "tokens": ["Und", "wenn", "ihr", "f\u00fcl\u00b7len", "wollt", "noch", "leer", "ge\u00b7blieb\u00b7ne", "Stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VMFIN", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schreibt deren Namen drein, die dort die Stern' entdecken;", "tokens": ["Schreibt", "de\u00b7ren", "Na\u00b7men", "drein", ",", "die", "dort", "die", "Stern'", "ent\u00b7de\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRELAT", "NN", "PTKVZ", "$,", "PRELS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Da\u00df es der Erde sei ein Zeichen jede Nacht,", "tokens": ["Da\u00df", "es", "der", "Er\u00b7de", "sei", "ein", "Zei\u00b7chen", "je\u00b7de", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df droben h\u00f6her gilt die Weisheit als die Macht.", "tokens": ["Da\u00df", "dro\u00b7ben", "h\u00f6\u00b7her", "gilt", "die", "Weis\u00b7heit", "als", "die", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}