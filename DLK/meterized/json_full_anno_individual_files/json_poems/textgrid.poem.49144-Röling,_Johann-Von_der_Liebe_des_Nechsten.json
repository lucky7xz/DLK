{"textgrid.poem.49144": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Von der Liebe des Nechsten", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist, da\u00df du, o Mensch, dich stellst,", "tokens": ["Was", "ist", ",", "da\u00df", "du", ",", "o", "Mensch", ",", "dich", "stellst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "KOUS", "PPER", "$,", "FM", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wenn du viel von Gott hie h\u00e4lst", "tokens": ["Als", "wenn", "du", "viel", "von", "Gott", "hie", "h\u00e4lst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "APPR", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ha\u00dfest dennoch deinen Nechsten?", "tokens": ["Und", "ha\u00b7\u00dfest", "den\u00b7noch", "dei\u00b7nen", "Nechs\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wenig Tag es sonder Licht,", "tokens": ["So", "we\u00b7nig", "Tag", "es", "son\u00b7der", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein guter Baum ohn gute Fr\u00fccht,", "tokens": ["Ein", "gu\u00b7ter", "Baum", "ohn", "gu\u00b7te", "Fr\u00fccht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So wenig liebest du den H\u00f6chsten,", "tokens": ["So", "we\u00b7nig", "lie\u00b7best", "du", "den", "H\u00f6chs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn dein bedr\u00fcckter Neben-Christ", "tokens": ["Wenn", "dein", "be\u00b7dr\u00fcck\u00b7ter", "Ne\u00b7ben\u00b7Christ"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht solcher Liebe theilhafft ist.", "tokens": ["Nicht", "sol\u00b7cher", "Lie\u00b7be", "theil\u00b7hafft", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sonst magstu Alles thun und seyn,", "tokens": ["Sonst", "mags\u00b7tu", "Al\u00b7les", "thun", "und", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVINF", "KON", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gib an die Armen All, was dein,", "tokens": ["Gib", "an", "die", "Ar\u00b7men", "All", ",", "was", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "NN", "$,", "PRELS", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sprich nach den hochberedten Engeln,", "tokens": ["Sprich", "nach", "den", "hoch\u00b7be\u00b7red\u00b7ten", "En\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dring selbst dich in das h\u00f6chste Licht,", "tokens": ["Dring", "selbst", "dich", "in", "das", "h\u00f6chs\u00b7te", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hast du dabei die Liebe nicht,", "tokens": ["Hast", "du", "da\u00b7bei", "die", "Lie\u00b7be", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So bistu reich an lauter M\u00e4ngeln,", "tokens": ["So", "bis\u00b7tu", "reich", "an", "lau\u00b7ter", "M\u00e4n\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein t\u00f6nend Ertz, ein leerer Schall,", "tokens": ["Ein", "t\u00f6\u00b7nend", "Ertz", ",", "ein", "lee\u00b7rer", "Schall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Ein lauter Nichtes \u00fcberall.", "tokens": ["Ein", "lau\u00b7ter", "Nich\u00b7tes", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie ist es, die uns nach der Zeit", "tokens": ["Sie", "ist", "es", ",", "die", "uns", "nach", "der", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hilfft bey der s\u00fc\u00dfen Ewigkeit", "tokens": ["Hilfft", "bey", "der", "s\u00fc\u00b7\u00dfen", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ungeendte Zeit vertreiben,", "tokens": ["Die", "un\u00b7ge\u00b7end\u00b7te", "Zeit", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie ist die Aehren-reiche Saat,", "tokens": ["Sie", "ist", "die", "A\u00b7eh\u00b7ren\u00b7rei\u00b7che", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die dort so herrlich Erndte hat,", "tokens": ["Die", "dort", "so", "herr\u00b7lich", "Ernd\u00b7te", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie ist das Probst\u00fcck, das wir schreiben,", "tokens": ["Sie", "ist", "das", "Prob\u00b7st\u00fcck", ",", "das", "wir", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu sehn, was unser Meister sey", "tokens": ["Zu", "sehn", ",", "was", "un\u00b7ser", "Meis\u00b7ter", "sey"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ob wir ihm recht kommen bey.", "tokens": ["Und", "ob", "wir", "ihm", "recht", "kom\u00b7men", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir Alle sind von einer Hand", "tokens": ["Wir", "Al\u00b7le", "sind", "von", "ei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und also von Natur verwandt;", "tokens": ["Und", "al\u00b7so", "von", "Na\u00b7tur", "ver\u00b7wandt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer thut nicht Gutes seinem Blute?", "tokens": ["Wer", "thut", "nicht", "Gu\u00b7tes", "sei\u00b7nem", "Blu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur ein Bild ist uns eingepr\u00e4gt,", "tokens": ["Nur", "ein", "Bild", "ist", "uns", "ein\u00b7ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Das unsern Sch\u00f6pffer uns vorlegt,", "tokens": ["Das", "un\u00b7sern", "Sch\u00f6pf\u00b7fer", "uns", "vor\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur ein Bad k\u00f6mmet uns zu Gute,", "tokens": ["Nur", "ein", "Bad", "k\u00f6m\u00b7met", "uns", "zu", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das solches wieder sauber w\u00e4scht,", "tokens": ["Das", "sol\u00b7ches", "wie\u00b7der", "sau\u00b7ber", "w\u00e4scht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So weit es Adams Wust verlescht.", "tokens": ["So", "weit", "es", "A\u00b7dams", "Wust", "ver\u00b7lescht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nur ein Glaub' ist, der uns verbindt,", "tokens": ["Nur", "ein", "Glaub'", "ist", ",", "der", "uns", "ver\u00b7bindt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur ein Geist, welcher uns entz\u00fcndt,", "tokens": ["Nur", "ein", "Geist", ",", "wel\u00b7cher", "uns", "ent\u00b7z\u00fcndt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Ein Vater, unter dem wir Br\u00fcder,", "tokens": ["Ein", "Va\u00b7ter", ",", "un\u00b7ter", "dem", "wir", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur ein Brodt, ein Leib, den er schenkt,", "tokens": ["Nur", "ein", "Brodt", ",", "ein", "Leib", ",", "den", "er", "schenkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Nur ein Wein, ein Blut, das uns tr\u00e4nkt,", "tokens": ["Nur", "ein", "Wein", ",", "ein", "Blut", ",", "das", "uns", "tr\u00e4nkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Nur ein Haupt, unter dem wir Glieder,", "tokens": ["Nur", "ein", "Haupt", ",", "un\u00b7ter", "dem", "wir", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Nur ein Reich kriegen wir dort ein:", "tokens": ["Nur", "ein", "Reich", "krie\u00b7gen", "wir", "dort", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.8": {"text": "Was wollen wir hie uneins seyn?", "tokens": ["Was", "wol\u00b7len", "wir", "hie", "un\u00b7eins", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gott liebet dich und darff nicht dein,", "tokens": ["Gott", "lie\u00b7bet", "dich", "und", "darff", "nicht", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "VMFIN", "PTKNEG", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Sohn hat so viel Angst und Pein", "tokens": ["Sein", "Sohn", "hat", "so", "viel", "Angst", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus Liebe blo\u00df f\u00fcr dich ertragen,", "tokens": ["Aus", "Lie\u00b7be", "blo\u00df", "f\u00fcr", "dich", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und fordert nichtes mehr von dir,", "tokens": ["Und", "for\u00b7dert", "nich\u00b7tes", "mehr", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als nur, da\u00df du ihm Dank daf\u00fcr", "tokens": ["Als", "nur", ",", "da\u00df", "du", "ihm", "Dank", "da\u00b7f\u00fcr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "$,", "KOUS", "PPER", "PPER", "APPR", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In seinen Freunden sollest sagen,", "tokens": ["In", "sei\u00b7nen", "Freun\u00b7den", "sol\u00b7lest", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und f\u00fcr zehn tausend Pfund und mehr", "tokens": ["Und", "f\u00fcr", "zehn", "tau\u00b7send", "Pfund", "und", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "CARD", "NN", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nur hundert Groschen gebest her.", "tokens": ["Nur", "hun\u00b7dert", "Gro\u00b7schen", "ge\u00b7best", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.7": {"line.1": {"text": "Thu, was du kanst; klagt wer sein Leid,", "tokens": ["Thu", ",", "was", "du", "kanst", ";", "klagt", "wer", "sein", "Leid", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "VMFIN", "$.", "VVFIN", "PWS", "PPOSAT", "NN", "$,"], "meter": "+--++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der hungrig, kranck, ohn Hau\u00df und Kleid,", "tokens": ["Der", "hung\u00b7rig", ",", "kranck", ",", "ohn", "Hau\u00df", "und", "Kleid", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADJD", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da denk, als wenn Gott vor dir stehe", "tokens": ["Da", "denk", ",", "als", "wenn", "Gott", "vor", "dir", "ste\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOKOM", "KOUS", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und Zins von seinen G\u00fctern nehm,", "tokens": ["Und", "Zins", "von", "sei\u00b7nen", "G\u00fc\u00b7tern", "nehm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und als wenn selbst dein Heyland k\u00e4m',", "tokens": ["Und", "als", "wenn", "selbst", "dein", "Hey\u00b7land", "k\u00e4m'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOKOM", "KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um Herberg dich und Kleider flehe,", "tokens": ["Um", "Her\u00b7berg", "dich", "und", "Klei\u00b7der", "fle\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PPER", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nach Speis' und Trank Verlangen trag'", "tokens": ["Nach", "Speis'", "und", "Trank", "Ver\u00b7lan\u00b7gen", "trag'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und dir von seiner Schwachheit sag'.", "tokens": ["Und", "dir", "von", "sei\u00b7ner", "Schwach\u00b7heit", "sag'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ach, denk, wie du um dich bem\u00fcht,", "tokens": ["Ach", ",", "denk", ",", "wie", "du", "um", "dich", "be\u00b7m\u00fcht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$,", "PWAV", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie labt und quickt sich dein Gebl\u00fct,", "tokens": ["Wie", "labt", "und", "quickt", "sich", "dein", "Ge\u00b7bl\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "KON", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie suchst und hegest du dir Freunde,", "tokens": ["Wie", "suchst", "und", "he\u00b7gest", "du", "dir", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "KON", "VVFIN", "PPER", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie baustu allen N\u00f6then f\u00fcr,", "tokens": ["Wie", "baus\u00b7tu", "al\u00b7len", "N\u00f6\u00b7then", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIAT", "NN", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie deckstu deine Fehl' an dir,", "tokens": ["Wie", "decks\u00b7tu", "dei\u00b7ne", "Fehl'", "an", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie streitstu wider deine Feinde!", "tokens": ["Wie", "streits\u00b7tu", "wi\u00b7der", "dei\u00b7ne", "Fein\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Selbst diese Gunst zu dir, die ist,", "tokens": ["Selbst", "die\u00b7se", "Gunst", "zu", "dir", ",", "die", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "APPR", "PPER", "$,", "PRELS", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die du dem Nechsten schuldig bist.", "tokens": ["Die", "du", "dem", "Nechs\u00b7ten", "schul\u00b7dig", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Erf\u00fclle, Jesu, mein Gem\u00fcth", "tokens": ["Er\u00b7f\u00fcl\u00b7le", ",", "Je\u00b7su", ",", "