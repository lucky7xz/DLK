"Denn", "wenn", "ich", "nich\u00b7tes", "von", "mir", "halt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So krieg' ich deines Bilds Gestalt.", "tokens": ["So", "krieg'", "ich", "dei\u00b7nes", "Bilds", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Was ist, da\u00df du, o Mensch, dich stellst,", "tokens": ["Was", "ist", ",", "da\u00df", "du", ",", "o", "Mensch", ",", "dich", "stellst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "KOUS", "PPER", "$,", "FM", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wenn du viel von Gott hie h\u00e4lst", "tokens": ["Als", "wenn", "du", "viel", "von", "Gott", "hie", "h\u00e4lst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "APPR", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ha\u00dfest dennoch deinen Nechsten?", "tokens": ["Und", "ha\u00b7\u00dfest", "den\u00b7noch", "dei\u00b7nen", "Nechs\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wenig Tag es sonder Licht,", "tokens": ["So", "we\u00b7nig", "Tag", "es", "son\u00b7der", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein guter Baum ohn gute Fr\u00fccht,", "tokens": ["Ein", "gu\u00b7ter", "Baum", "ohn", "gu\u00b7te", "Fr\u00fccht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So wenig liebest du den H\u00f6chsten,", "tokens": ["So", "we\u00b7nig", "lie\u00b7best", "du", "den", "H\u00f6chs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn dein bedr\u00fcckter Neben-Christ", "tokens": ["Wenn", "dein", "be\u00b7dr\u00fcck\u00b7ter", "Ne\u00b7ben\u00b7Christ"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht solcher Liebe theilhafft ist.", "tokens": ["Nicht", "sol\u00b7cher", "Lie\u00b7be", "theil\u00b7hafft", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Sonst magstu Alles thun und seyn,", "tokens": ["Sonst", "mags\u00b7tu", "Al\u00b7les", "thun", "und", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVINF", "KON", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gib an die Armen All, was dein,", "tokens": ["Gib", "an", "die", "Ar\u00b7men", "All", ",", "was", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "NN", "$,", "PRELS", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sprich nach den hochberedten Engeln,", "tokens": ["Sprich", "nach", "den", "hoch\u00b7be\u00b7red\u00b7ten", "En\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dring selbst dich in das h\u00f6chste Licht,", "tokens": ["Dring", "selbst", "dich", "in", "das", "h\u00f6chs\u00b7te", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hast du dabei die Liebe nicht,", "tokens": ["Hast", "du", "da\u00b7bei", "die", "Lie\u00b7be", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So bistu reich an lauter M\u00e4ngeln,", "tokens": ["So", "bis\u00b7tu", "reich", "an", "lau\u00b7ter", "M\u00e4n\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein t\u00f6nend Ertz, ein leerer Schall,", "tokens": ["Ein", "t\u00f6\u00b7nend", "Ertz", ",", "ein", "lee\u00b7rer", "Schall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Ein lauter Nichtes \u00fcberall.", "tokens": ["Ein", "lau\u00b7ter", "Nich\u00b7tes", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Sie ist es, die uns nach der Zeit", "tokens": ["Sie", "ist", "es", ",", "die", "uns", "nach", "der", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hilfft bey der s\u00fc\u00dfen Ewigkeit", "tokens": ["Hilfft", "bey", "der", "s\u00fc\u00b7\u00dfen", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ungeendte Zeit vertreiben,", "tokens": ["Die", "un\u00b7ge\u00b7end\u00b7te", "Zeit", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie ist die Aehren-reiche Saat,", "tokens": ["Sie", "ist", "die", "A\u00b7eh\u00b7ren\u00b7rei\u00b7che", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die dort so herrlich Erndte hat,", "tokens": ["Die", "dort", "so", "herr\u00b7lich", "Ernd\u00b7te", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie ist das Probst\u00fcck, das wir schreiben,", "tokens": ["Sie", "ist", "das", "Prob\u00b7st\u00fcck", ",", "das", "wir", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu sehn, was unser Meister sey", "tokens": ["Zu", "sehn", ",", "was", "un\u00b7ser", "Meis\u00b7ter", "sey"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ob wir ihm recht kommen bey.", "tokens": ["Und", "ob", "wir", "ihm", "recht", "kom\u00b7men", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Wir Alle sind von einer Hand", "tokens": ["Wir", "Al\u00b7le", "sind", "von", "ei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und also von Natur verwandt;", "tokens": ["Und", "al\u00b7so", "von", "Na\u00b7tur", "ver\u00b7wandt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer thut nicht Gutes seinem Blute?", "tokens": ["Wer", "thut", "nicht", "Gu\u00b7tes", "sei\u00b7nem", "Blu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur ein Bild ist uns eingepr\u00e4gt,", "tokens": ["Nur", "ein", "Bild", "ist", "uns", "ein\u00b7ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Das unsern Sch\u00f6pffer uns vorlegt,", "tokens": ["Das", "un\u00b7sern", "Sch\u00f6pf\u00b7fer", "uns", "vor\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": 