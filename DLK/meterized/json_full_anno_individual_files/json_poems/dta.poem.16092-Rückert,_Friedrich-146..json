{"dta.poem.16092": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "146.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.85", "tl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Sohn, wenn du gelangst zum Umgang sch\u00f6ner Frauen,", "tokens": ["Mein", "Sohn", ",", "wenn", "du", "ge\u00b7langst", "zum", "Um\u00b7gang", "sch\u00f6\u00b7ner", "Frau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVPP", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Andacht lerne sie, mit Ehrerbietung schauen.", "tokens": ["Mit", "An\u00b7dacht", "ler\u00b7ne", "sie", ",", "mit", "Ehr\u00b7er\u00b7bie\u00b7tung", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Leichtfertigkeit ver\u00fcbt am Heiligsten Verrath;", "tokens": ["Leicht\u00b7fer\u00b7tig\u00b7keit", "ver\u00b7\u00fcbt", "am", "Hei\u00b7ligs\u00b7ten", "Ver\u00b7rath", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Denk an die Mutter, Sohn, die dich geboren hat.", "tokens": ["Denk", "an", "die", "Mut\u00b7ter", ",", "Sohn", ",", "die", "dich", "ge\u00b7bo\u00b7ren", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "NN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zu solcher W\u00fcrde ist ein jedes Weib berufen;", "tokens": ["Zu", "sol\u00b7cher", "W\u00fcr\u00b7de", "ist", "ein", "je\u00b7des", "Weib", "be\u00b7ru\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Willst, kanst du, darfst du sie hinf\u00fchren zu den Stufen?", "tokens": ["Willst", ",", "kanst", "du", ",", "darfst", "du", "sie", "hin\u00b7f\u00fch\u00b7ren", "zu", "den", "Stu\u00b7fen", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "VMFIN", "PPER", "$,", "VMFIN", "PPER", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}