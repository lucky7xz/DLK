{"textgrid.poem.49758": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Indische Weisheit", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Will die Menschheit einen Dalai Lama,", "tokens": ["Will", "die", "Menschheit", "ei\u00b7nen", "Da\u00b7lai", "La\u00b7ma", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NE", "NE", "$,"], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Mu\u00df sie mit Geheimnis ihn umgeben.", "tokens": ["Mu\u00df", "sie", "mit", "Ge\u00b7heim\u00b7nis", "ihn", "um\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Besser ist es, wenn die Erdeng\u00f6tter", "tokens": ["Bes\u00b7ser", "ist", "es", ",", "wenn", "die", "Er\u00b7den\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hinter Wolken im Verborg'nen leben.", "tokens": ["Hin\u00b7ter", "Wol\u00b7ken", "im", "Ver\u00b7bor\u00b7g'\u00b7nen", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Auch die Seltenheit ist zu empfehlen", "tokens": ["Auch", "die", "Sel\u00b7ten\u00b7heit", "ist", "zu", "emp\u00b7feh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "PTKZU", "VVINF"], "meter": "--+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Gebrauch der hohen Lamaworte,", "tokens": ["Im", "Ge\u00b7brauch", "der", "ho\u00b7hen", "La\u00b7ma\u00b7wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Denn es leidet ihre Geltungsst\u00e4rke", "tokens": ["Denn", "es", "lei\u00b7det", "ih\u00b7re", "Gel\u00b7tungs\u00b7st\u00e4r\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Durch die H\u00e4ufigkeit der Mundespforte.", "tokens": ["Durch", "die", "H\u00e4u\u00b7fig\u00b7keit", "der", "Mun\u00b7des\u00b7pfor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Richtig ist, was uns die klugen Priester", "tokens": ["Rich\u00b7tig", "ist", ",", "was", "uns", "die", "klu\u00b7gen", "Pries\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "$,", "PRELS", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von der Weisheit Dalai Lama's lehren:", "tokens": ["Von", "der", "Weis\u00b7heit", "Da\u00b7lai", "La\u00b7ma's", "leh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "NE", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nur je weniger wir sie vernehmen,", "tokens": ["Nur", "je", "we\u00b7ni\u00b7ger", "wir", "sie", "ver\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+