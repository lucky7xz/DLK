{"textgrid.poem.32808": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da sehn wir's wieder hell und klar,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da sehn wir's wieder hell und klar,", "tokens": ["Da", "sehn", "wir's", "wie\u00b7der", "hell", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da Gott uns nie vergi\u00dft,", "tokens": ["Da", "Gott", "uns", "nie", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jede Zeit und Stund' im Jahr", "tokens": ["Und", "je\u00b7de", "Zeit", "und", "Stund'", "im", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der beste Vater ist.", "tokens": ["Der", "bes\u00b7te", "Va\u00b7ter", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da gie\u00dft nach langem Sonnenbrand,", "tokens": ["Da", "gie\u00dft", "nach", "lan\u00b7gem", "Son\u00b7nen\u00b7brand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Regen sich aufs d\u00fcrre Land.", "tokens": ["Sein", "Re\u00b7gen", "sich", "aufs", "d\u00fcr\u00b7re", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du lieber Gott, wie traurig sah's", "tokens": ["Du", "lie\u00b7ber", "Gott", ",", "wie", "trau\u00b7rig", "sah's"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "NN", "$,", "PWAV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei uns auf Wies' und Feld!", "tokens": ["Bei", "uns", "auf", "Wies'", "und", "Feld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verdorrt war Korn und Laub und Gras,", "tokens": ["Ver\u00b7dorrt", "war", "Korn", "und", "Laub", "und", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und von der Hitz entstellt;", "tokens": ["Und", "von", "der", "Hitz", "ent\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Matt schlich das Vieh zur Weid' hinaus,", "tokens": ["Matt", "schlich", "das", "Vieh", "zur", "Weid'", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und kehrte hungriger nach Haus.", "tokens": ["Und", "kehr\u00b7te", "hung\u00b7ri\u00b7ger", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die welken \u00c4hren neigten sich", "tokens": ["Die", "wel\u00b7ken", "\u00c4h\u00b7ren", "neig\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gelblich ward ihr Gr\u00fcn;", "tokens": ["Und", "gelb\u00b7lich", "ward", "ihr", "Gr\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der zarte Flachs schwand sichtbarlich", "tokens": ["Der", "zar\u00b7te", "Flachs", "schwand", "sicht\u00b7bar\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor unsern Augen hin;", "tokens": ["Vor", "un\u00b7sern", "Au\u00b7gen", "hin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Trespe nur und Distel stand,", "tokens": ["Die", "Tres\u00b7pe", "nur", "und", "Dis\u00b7tel", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wuchert' im erhitzten Land.", "tokens": ["Und", "wu\u00b7chert'", "im", "er\u00b7hitz\u00b7ten", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da stunden wir, vom Schwei\u00df ben\u00e4\u00dft,", "tokens": ["Da", "stun\u00b7den", "wir", ",", "vom", "Schwei\u00df", "be\u00b7n\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der von der Stirn uns rann,", "tokens": ["Der", "von", "der", "Stirn", "uns", "rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Herz, von Sorgen eingepre\u00dft,", "tokens": ["Das", "Herz", ",", "von", "Sor\u00b7gen", "ein\u00b7ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sahn den Jammer an,", "tokens": ["Und", "sahn", "den", "Jam\u00b7mer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und seufzten: Ach, du lieber Gott,", "tokens": ["Und", "seufz\u00b7ten", ":", "Ach", ",", "du", "lie\u00b7ber", "Gott", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ITJ", "$,", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer giebt uns auf den Winter Brot?", "tokens": ["Wer", "giebt", "uns", "auf", "den", "Win\u00b7ter", "Brot", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da schmolz \u2013 hab ewig Dank daf\u00fcr! \u2013", "tokens": ["Da", "schmolz", "\u2013", "hab", "e\u00b7wig", "Dank", "da\u00b7f\u00fcr", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "$(", "VAFIN", "ADJD", "APPR", "PAV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein milder Vatersinn;", "tokens": ["Dein", "mil\u00b7der", "Va\u00b7ter\u00b7sinn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sahn am fernen Himmel wir", "tokens": ["Da", "sahn", "am", "fer\u00b7nen", "Him\u00b7mel", "wir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gew\u00f6lk, wie Sch\u00e4fchen, ziehn,", "tokens": ["Ge\u00b7w\u00f6lk", ",", "wie", "Sch\u00e4f\u00b7chen", ",", "ziehn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und immer kam's in raschrem Lauf", "tokens": ["Und", "im\u00b7mer", "kam's", "in", "rasc\u00b7hrem", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schw\u00e4rzlicher zu uns herauf.", "tokens": ["Und", "schw\u00e4rz\u00b7li\u00b7cher", "zu", "uns", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da sank, in Dunkel eingeh\u00fcllt,", "tokens": ["Da", "sank", ",", "in", "Dun\u00b7kel", "ein\u00b7ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonn' am Wald hinab,", "tokens": ["Die", "Sonn'", "am", "Wald", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Regen rauschte k\u00fchl und mild", "tokens": ["Und", "Re\u00b7gen", "rauschte", "k\u00fchl", "und", "mild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aufs d\u00fcrre Feld herab;", "tokens": ["Aufs", "d\u00fcr\u00b7re", "Feld", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun freuet alles dankbarlich", "tokens": ["Nun", "freu\u00b7et", "al\u00b7les", "dank\u00b7bar\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Des neugeschenkten Lebens sich.", "tokens": ["Des", "neu\u00b7ge\u00b7schenk\u00b7ten", "Le\u00b7bens", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie frisch erhebt der Halm sein Haupt,", "tokens": ["Wie", "frisch", "er\u00b7hebt", "der", "Halm", "sein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie prangt der Flachs so blau!", "tokens": ["Wie", "prangt", "der", "Flachs", "so", "blau", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie steht der Baum so neubelaubt,", "tokens": ["Wie", "steht", "der", "Baum", "so", "neu\u00b7be\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gl\u00e4nzendgr\u00fcn die Au'!", "tokens": ["Und", "gl\u00e4n\u00b7zend\u00b7gr\u00fcn", "die", "Au'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und welch ein \u00fcbers\u00fc\u00dfer Duft", "tokens": ["Und", "welch", "ein", "\u00fc\u00b7ber\u00b7s\u00fc\u00b7\u00dfer", "Duft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcllt rings umher die k\u00fchle Luft.", "tokens": ["F\u00fcllt", "rings", "um\u00b7her", "die", "k\u00fch\u00b7le", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Das alles hast du, guter Gott,", "tokens": ["Das", "al\u00b7les", "hast", "du", ",", "gu\u00b7ter", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In kurzer Zeit gethan.", "tokens": ["In", "kur\u00b7zer", "Zeit", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo ist sie nun, die Hungersnot,", "tokens": ["Wo", "ist", "sie", "nun", ",", "die", "Hun\u00b7gers\u00b7no