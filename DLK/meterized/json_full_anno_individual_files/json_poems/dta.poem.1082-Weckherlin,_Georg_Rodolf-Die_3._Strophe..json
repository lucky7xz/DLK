{"dta.poem.1082": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die 3. Strophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aber seiner tugenden prob", "tokens": ["A\u00b7ber", "sei\u00b7ner", "tu\u00b7gen\u00b7den", "prob"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Au\u00df seiner V\u00e4tter grab zuziehen/", "tokens": ["Au\u00df", "sei\u00b7ner", "V\u00e4t\u00b7ter", "grab", "zu\u00b7zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df sich allein jener bem\u00fchen/", "tokens": ["Mu\u00df", "sich", "al\u00b7lein", "je\u00b7ner", "be\u00b7m\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "PDS", "VVINF", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "In dessen sehl kein aigenlob.", "tokens": ["In", "des\u00b7sen", "sehl", "kein", "ai\u00b7gen\u00b7lob", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht du/ der du gantz danckbarlich", "tokens": ["Nicht", "du", "/", "der", "du", "gantz", "dan\u00b7ck\u00b7bar\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "PPER", "$(", "PRELS", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Alle gaben von Got empfangen/", "tokens": ["Al\u00b7le", "ga\u00b7ben", "von", "Got", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "VVPP", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Welche die Tugent w\u00fcrdiglich", "tokens": ["Wel\u00b7che", "die", "Tu\u00b7gent", "w\u00fcr\u00b7dig\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Kan immer von dem Gl\u00fcck erlangen;", "tokens": ["Kan", "im\u00b7mer", "von", "dem", "Gl\u00fcck", "er\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nicht du/ der da niemand den lohn", "tokens": ["Nicht", "du", "/", "der", "da", "nie\u00b7mand", "den", "lohn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PPER", "$(", "ART", "KOUS", "PIS", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Seines verdiensts woltest verneinen/", "tokens": ["Sei\u00b7nes", "ver\u00b7diensts", "wol\u00b7test", "ver\u00b7nei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Begehrest/ klar gnug/ wie der Mohn", "tokens": ["Be\u00b7geh\u00b7rest", "/", "klar", "gnug", "/", "wie", "der", "Mohn"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "ADJD", "ADV", "$(", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mit entl\u00f6hnetem glantz zuscheinen.", "tokens": ["Mit", "ent\u00b7l\u00f6h\u00b7ne\u00b7tem", "glantz", "zu\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}