{"textgrid.poem.25738": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr Mops, der um das dritte Wort", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Mops, der um das dritte Wort", "tokens": ["Herr", "Mops", ",", "der", "um", "das", "drit\u00b7te", "Wort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Empfindsamkeit im Munde f\u00fchret,", "tokens": ["Emp\u00b7find\u00b7sam\u00b7keit", "im", "Mun\u00b7de", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn ein Grashalm ihm verdorrt,", "tokens": ["Und", "wenn", "ein", "Gras\u00b7halm", "ihm", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich einen Thr\u00e4nenstrom verlieret,", "tokens": ["Gleich", "ei\u00b7nen", "Thr\u00e4\u00b7nen\u00b7strom", "ver\u00b7lie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mops gr\u00fc\u00dft', als ein Romanenschmidt,", "tokens": ["Mops", "gr\u00fc\u00dft'", ",", "als", "ein", "Ro\u00b7ma\u00b7nen\u00b7schmidt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Autorhandwerk, mich denn mit.", "tokens": ["Das", "Au\u00b7tor\u00b7hand\u00b7werk", ",", "mich", "denn", "mit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit meinem Weibchen that er schier", "tokens": ["Mit", "mei\u00b7nem", "Weib\u00b7chen", "that", "er", "schier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gleich so bekannt, wie ein Franzose.", "tokens": ["Gleich", "so", "be\u00b7kannt", ",", "wie", "ein", "Fran\u00b7zo\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "All' Augenblicke bot er ihr", "tokens": ["All'", "Au\u00b7gen\u00b7bli\u00b7cke", "bot", "er", "ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Tobak aus eines Bettlers Dose,", "tokens": ["To\u00b7bak", "aus", "ei\u00b7nes", "Bett\u00b7lers", "Do\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Mit dem, am Zaun' im tiefen Schlaf',", "tokens": ["Mit", "dem", ",", "am", "Zaun'", "im", "tie\u00b7fen", "Schlaf'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Er einen Tausch, wie ", "tokens": ["Er", "ei\u00b7nen", "Tausch", ",", "wie"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "ART", "NN", "$,", "PWAV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Der Unempfindsamkeit zum Hohn'", "tokens": ["Der", "Un\u00b7emp\u00b7find\u00b7sam\u00b7keit", "zum", "Hohn'"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Hielt er auf eine M\u00fcck' im Glase", "tokens": ["Hielt", "er", "auf", "ei\u00b7ne", "M\u00fcck", "im", "Gla\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Beweglich einen Leichsermon.", "tokens": ["Be\u00b7weg\u00b7lich", "ei\u00b7nen", "Leich\u00b7ser\u00b7mon", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Purrt' eine Flieg' ihm an der Nase,", "tokens": ["Purrt'", "ei\u00b7ne", "Flieg'", "ihm", "an", "der", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Macht' er das Fenster auf, und sprach:", "tokens": ["Macht'", "er", "das", "Fens\u00b7ter", "auf", ",", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Zieh Oheim ", "tokens": ["Zieh", "O\u00b7heim"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Durch Mops ist wahrlich meine Magd", "tokens": ["Durch", "Mops", "ist", "wahr\u00b7lich", "mei\u00b7ne", "Magd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Nicht mehr bei Trost, nicht mehr bei Sinnen,", "tokens": ["Nicht", "mehr", "bei", "Trost", ",", "nicht", "mehr", "bei", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "$,", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "So sehr hat ihr sein Lob behagt,", "tokens": ["So", "sehr", "hat", "ihr", "sein", "Lob", "be\u00b7hagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Da\u00df sie empfindsam allen Spinnen", "tokens": ["Da\u00df", "sie", "emp\u00b7find\u00b7sam", "al\u00b7len", "Spin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "In meinem Hause, frank und frei", "tokens": ["In", "mei\u00b7nem", "Hau\u00b7se", ",", "frank", "und", "frei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Verstattet ihre Weberei.", "tokens": ["Ver\u00b7stat\u00b7tet", "ih\u00b7re", "We\u00b7be\u00b7rei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Er trat mein H\u00fcndchen auf das Bein:", "tokens": ["Er", "trat", "mein", "H\u00fcnd\u00b7chen", "auf", "das", "Bein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Hilf Himmel! Welch ein Lamentiren!", "tokens": ["Hilf", "Him\u00b7mel", "!", "Welch", "ein", "La\u00b7men\u00b7ti\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Es h\u00e4tte m\u00f6gen einen Stein", "tokens": ["Es", "h\u00e4t\u00b7te", "m\u00f6\u00b7gen", "ei\u00b7nen", "Stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Der Stra\u00dfe, zum Erbarmen r\u00fchren.", "tokens": ["Der", "Stra\u00b7\u00dfe", ",", "zum", "Er\u00b7bar\u00b7men", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Auch wedelt' ihm in einem Nu", "tokens": ["Auch", "we\u00b7delt'", "ihm", "in", "ei\u00b7nem", "Nu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Das H\u00fcndchen schon Vergebung zu.", "tokens": ["Das", "H\u00fcnd\u00b7chen", "schon", "Ver\u00b7ge\u00b7bung", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Ach! H\u00fcndchen, du besch\u00e4mst mich sehr!", "tokens": ["Ach", "!", "H\u00fcnd\u00b7chen", ",", "du", "be\u00b7sch\u00e4mst", "mich", "sehr", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Denn da\u00df mir Mops von meinem Leben", "tokens": ["Denn", "da\u00df", "mir", "Mops", "von", "mei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Drei Stunden stahl: wie schwer, wie schwer", "tokens": ["Drei", "Stun\u00b7den", "stahl", ":", "wie", "schwer", ",", "wie", "schwer"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "$.", "PWAV", "ADJD", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Wird's halten, das ihm zu vergeben.", "tokens": ["Wird's", "hal\u00b7ten", ",", "das", "ihm", "zu", "ver\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PRELS", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Denn Spinnen werden oben ein", "tokens": ["Denn", "Spin\u00b7nen", "wer\u00b7den", "o\u00b7ben", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "ADV", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Wohl gar noch meine M\u00f6rder seyn.", "tokens": ["Wohl", "gar", "noch", "mei\u00b7ne", "M\u00f6r\u00b7der", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Herr Mops, der um das dritte Wort", "tokens": ["Herr", "Mops", ",", "der", "um", "das", "drit\u00b7te", "Wort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "