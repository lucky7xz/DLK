{"textgrid.poem.50115": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "3. Auf Titrauchius", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist nicht ", "tokens": ["Ist", "nicht"], "token_info": ["word", "word"], "pos": ["VAFIN", "PTKNEG"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Er ", "tokens": ["Er"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Er liebt die, die mit ihm aus Lust nach Ungl\u00fcck lauffen,", "tokens": ["Er", "liebt", "die", ",", "die", "mit", "ihm", "aus", "Lust", "nach", "Un\u00b7gl\u00fcck", "lauf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "$,", "PRELS", "APPR", "PPER", "APPR", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und seiner Heimligkeit", "tokens": ["Und", "sei\u00b7ner", "Heim\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er liebt ein l\u00fcstern Weib, das von den Lastern lebt,", "tokens": ["Er", "liebt", "ein", "l\u00fcs\u00b7tern", "Weib", ",", "das", "von", "den", "Las\u00b7tern", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sich dem Mann zur Lust woll\u00fcstig weiss zu wenden,", "tokens": ["Und", "sich", "dem", "Mann", "zur", "Lust", "wol\u00b7l\u00fcs\u00b7tig", "weiss", "zu", "wen\u00b7den", ","], "t