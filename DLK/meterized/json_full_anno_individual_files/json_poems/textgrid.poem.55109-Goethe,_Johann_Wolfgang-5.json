{"textgrid.poem.55109": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "5", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als kleines art'ges Kind nach Feld und Auen", "tokens": ["Als", "klei\u00b7nes", "art'\u00b7ges", "Kind", "nach", "Feld", "und", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sprangst du mit mir, so manchen Fr\u00fchlingsmorgen.", "tokens": ["Sprangst", "du", "mit", "mir", ",", "so", "man\u00b7chen", "Fr\u00fch\u00b7lings\u00b7mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbf\u00fcr solch ein T\u00f6chterchen mit holden Sorgen", "tokens": ["\u00bb", "f\u00fcr", "solch", "ein", "T\u00f6ch\u00b7ter\u00b7chen", "mit", "hol\u00b7den", "Sor\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PIAT", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "M\u00f6cht ich als Vater segnend H\u00e4user bauen!\u00ab", "tokens": ["M\u00f6cht", "ich", "als", "Va\u00b7ter", "seg\u00b7nend", "H\u00e4u\u00b7ser", "bau\u00b7en", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "KOUS", "NN", "VVPP", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und als du anfingst, in die Welt zu schauen,", "tokens": ["Und", "als", "du", "an\u00