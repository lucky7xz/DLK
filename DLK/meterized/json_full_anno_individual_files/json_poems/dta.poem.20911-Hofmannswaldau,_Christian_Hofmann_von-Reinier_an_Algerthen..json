{"dta.poem.20911": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Reinier an Algerthen.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Algerthe schreibt zuviel! Ich kenne kein verja-", "tokens": ["Al\u00b7ger\u00b7the", "schreibt", "zu\u00b7viel", "!", "Ich", "ken\u00b7ne", "kein", "ver\u00b7ja"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIS", "$.", "PPER", "VVFIN", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gen/", "tokens": ["gen", "/"], "token_info": ["word", "punct"], "pos": ["APPR", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Mein Kleinoth ist das Reich/ und du sein", "tokens": ["Mein", "Klei\u00b7noth", "ist", "das", "Reich", "/", "und", "du", "sein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$(", "KON", "PPER", "VAINF"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "bester Stein/", "tokens": ["bes\u00b7ter", "Stein", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Es wird die Nachwelt mir nicht wissen nachzusagen/", "tokens": ["Es", "wird", "die", "Nach\u00b7welt", "mir", "nicht", "wis\u00b7sen", "nach\u00b7zu\u00b7sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "PTKNEG", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das deine Sch\u00f6nheit wird verstossen worden seyn.", "tokens": ["Das", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "wird", "ver\u00b7stos\u00b7sen", "wor\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VAFIN", "VVPP", "VAPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es soll dich meine Hand nicht au\u00df dem Lande trei-", "tokens": ["Es", "soll", "dich", "mei\u00b7ne", "Hand", "nicht", "au\u00df", "dem", "Lan\u00b7de", "trei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "PPOSAT", "NN", "PTKNEG", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "ben/", "tokens": ["ben", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Ich denck an deine treu/ und kenne meine Pflicht/", "tokens": ["Ich", "denck", "an", "dei\u00b7ne", "treu", "/", "und", "ken\u00b7ne", "mei\u00b7ne", "Pflicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJD", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du solt in meinen Reich und deinen Ehren bleiben/", "tokens": ["Du", "solt", "in", "mei\u00b7nen", "Reich", "und", "dei\u00b7nen", "Eh\u00b7ren", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn meine neue Brunst verlescht die alte nicht.", "tokens": ["Denn", "mei\u00b7ne", "neu\u00b7e", "Brunst", "ver\u00b7lescht", "die", "al\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich bin ein Held und wei\u00df dich Heldin auch zulieben/", "tokens": ["Ich", "bin", "ein", "Held", "und", "wei\u00df", "dich", "Hel\u00b7din", "auch", "zu\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VVFIN", "PPER", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich schau dich noch erhitzt und blutig vor mir stehn/", "tokens": ["Ich", "schau", "dich", "noch", "er\u00b7hitzt", "und", "blu\u00b7tig", "vor", "mir", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVFIN", "KON", "ADJD", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den abgematten Feind f\u00fcr deinen Augen gieben/", "tokens": ["Den", "ab\u00b7ge\u00b7mat\u00b7ten", "Feind", "f\u00fcr", "dei\u00b7nen", "Au\u00b7gen", "gie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und deinen strengen Fu\u00df durch blasse Leichen gehn.", "tokens": ["Und", "dei\u00b7nen", "stren\u00b7gen", "Fu\u00df", "durch", "blas\u00b7se", "Lei\u00b7chen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Mich deucht ich sp\u00fchre noch das B\u00f6ben deiner Br\u00fcste/", "tokens": ["Mich", "deucht", "ich", "sp\u00fch\u00b7re", "noch", "das", "B\u00f6\u00b7ben", "dei\u00b7ner", "Br\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Darauf der Schweden Bluth dir als Corallen stund/", "tokens": ["Da\u00b7rauf", "der", "Schwe\u00b7den", "Bluth", "dir", "als", "Co\u00b7ral\u00b7len", "stund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PPER", "KOUS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich wei\u00df was mir gefiel/ als ich dich erstlich k\u00fcste", "tokens": ["Ich", "wei\u00df", "was", "mir", "ge\u00b7fiel", "/", "als", "ich", "dich", "erst\u00b7lich", "k\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "VVFIN", "$(", "KOUS", "PPER", "PRF", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und in der neuen Lust befeuchte deinen Mund.", "tokens": ["Und", "in", "der", "neu\u00b7en", "Lust", "be\u00b7feuch\u00b7te", "dei\u00b7nen", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die s\u00fcsse K\u00fctzelung laufft noch durch meine Lenden/", "tokens": ["Die", "s\u00fcs\u00b7se", "K\u00fct\u00b7ze\u00b7lung", "laufft", "noch", "durch", "mei\u00b7ne", "Len\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Als ich die Erstlinge von deinen Bluhmen brach/", "tokens": ["Als", "ich", "die", "Erst\u00b7lin\u00b7ge", "von", "dei\u00b7nen", "Bluh\u00b7men", "brach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.22": {"text": "Und mein ", "tokens": ["Und", "mein"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}, "line.23": {"text": "Ja geht/ was Gunst betrifft/ gewi\u00dflich keiner nach.", "tokens": ["Ja", "geht", "/", "was", "Gunst", "be\u00b7tr\u00b7ifft", "/", "ge\u00b7wi\u00df\u00b7lich", "kei\u00b7ner", "nach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "$(", "PWS", "NN", "VVFIN", "$(", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Die Pf\u00e4nder so du mir hast auff die Welt gebohren/", "tokens": ["Die", "Pf\u00e4n\u00b7der", "so", "du", "mir", "hast", "auff", "die", "Welt", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die mich in s\u00fcsser Lust offt haben angelacht/", "tokens": ["Die", "mich", "in", "s\u00fcs\u00b7ser", "Lust", "offt", "ha\u00b7ben", "an\u00b7ge\u00b7lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Verj\u00fcngen mir itzund di\u00df/ was ich dir geschworen/", "tokens": ["Ver\u00b7j\u00fcn\u00b7gen", "mir", "it\u00b7zund", "di\u00df", "/", "was", "ich", "dir", "ge\u00b7schwo\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PDS", "$(", "PWS", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und st\u00e4rcken mir den Bund/ den ich mit dir gemacht.", "tokens": ["Und", "st\u00e4r\u00b7cken", "mir", "den", "Bund", "/", "den", "ich", "mit", "dir", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$(", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So redet die Natur/ doch mu\u00df Sie Sclavin werden/", "tokens": ["So", "re\u00b7det", "die", "Na\u00b7tur", "/", "doch", "mu\u00df", "Sie", "Scla\u00b7vin", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "ADV", "VMFIN", "PPER", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Man schauet wie sie sich offt meistern lassen mu\u00df/", "tokens": ["Man", "schau\u00b7et", "wie", "sie", "sich", "offt", "meis\u00b7tern", "las\u00b7sen", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOKOM", "PPER", "PRF", "ADV", "VVINF", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wie offt Sie zinsbar wird den zeitlichen Beschwer-", "tokens": ["Wie", "offt", "Sie", "zins\u00b7bar", "wird", "den", "zeit\u00b7li\u00b7chen", "Be\u00b7schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "ADJD", "VAFIN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "den/", "tokens": ["den", "/"], "token_info": ["word", "punct"], "pos": ["ART", "$("], "meter": "-", "measure": "single.down"}, "line.32": {"text": "Und durch den Zufalls Trieb ver\u00e4ndert Gang und Fu\u00df/", "tokens": ["Und", "durch", "den", "Zu\u00b7falls", "Trieb", "ver\u00b7\u00e4n\u00b7dert", "Gang", "und", "Fu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVPP", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Du weist wie K\u00f6nige ein schweres Eisen plaget/", "tokens": ["Du", "weist", "wie", "K\u00f6\u00b7ni\u00b7ge", "ein", "schwe\u00b7res", "Ei\u00b7sen", "pla\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wie Jhre Crone Sie in Fessel hat gebracht/", "tokens": ["Wie", "Ih\u00b7re", "Cro\u00b7ne", "Sie", "in", "Fes\u00b7sel", "hat", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wie offt ein hoher Schlu\u00df Sie aus sich selber jaget/", "tokens": ["Wie", "offt", "ein", "ho\u00b7her", "Schlu\u00df", "Sie", "aus", "sich", "sel\u00b7ber", "ja\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "PPER", "APPR", "PRF", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und stetig die Gefahr f\u00fcr Jhren Throne wacht.", "tokens": ["Und", "ste\u00b7tig", "die", "Ge\u00b7fahr", "f\u00fcr", "Ih\u00b7ren", "Thro\u00b7ne", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Di\u00df alles n\u00f6tigt mich auf ander Art zu dencken/", "tokens": ["Di\u00df", "al\u00b7les", "n\u00f6\u00b7tigt", "mich", "auf", "an\u00b7der", "Art", "zu", "den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PRF", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Als wol die Eigenschafft von meinen Hertzen will/", "tokens": ["Als", "wol", "die", "Ei\u00b7gen\u00b7schafft", "von", "mei\u00b7nen", "Hert\u00b7zen", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die Liebe heist mich zwar auf dich mein Auge lencken/", "tokens": ["Die", "Lie\u00b7be", "heist", "mich", "zwar", "auf", "dich", "mein", "Au\u00b7ge", "len\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Doch meines Reiches Nutz verr\u00fcckt mir Ma\u00df und", "tokens": ["Doch", "mei\u00b7nes", "Rei\u00b7ches", "Nutz", "ver\u00b7r\u00fcckt", "mir", "Ma\u00df", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Ziel.", "tokens": ["Ziel", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.42": {"text": "Ein Wetter so mit Blitz und harten Donner dreuet/", "tokens": ["Ein", "Wet\u00b7ter", "so", "mit", "Blitz", "und", "har\u00b7ten", "Don\u00b7ner", "dreu\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.43": {"text": "Begint ein neues Joch/ und meistert meinen Sinn/", "tokens": ["Be\u00b7gint", "ein", "neu\u00b7es", "Joch", "/", "und", "meis\u00b7tert", "mei\u00b7nen", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Was ich zuvor gethan/ hat mich zwar nicht gereuet/", "tokens": ["Was", "ich", "zu\u00b7vor", "ge\u00b7than", "/", "hat", "mich", "zwar", "nicht", "ge\u00b7reu\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$(", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Doch wird die Furcht forthin zu meiner Kuplerin;", "tokens": ["Doch",