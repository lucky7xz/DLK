{"dta.poem.4147": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Schifferreigen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich lass' mein Schifflein treiben", "tokens": ["Ich", "lass'", "mein", "Schif\u00b7flein", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinauf, hinab die Fluth;", "tokens": ["Hin\u00b7auf", ",", "hin\u00b7ab", "die", "Fluth", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob Wind und Woge schlafen,", "tokens": ["Ob", "Wind", "und", "Wo\u00b7ge", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Schiff sich nimmer ruht.", "tokens": ["Das", "Schiff", "sich", "nim\u00b7mer", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gieb mir mein Ruder wieder", "tokens": ["Gieb", "mir", "mein", "Ru\u00b7der", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und la\u00df das Spielen sein,", "tokens": ["Und", "la\u00df", "das", "Spie\u00b7len", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Diebin, oder nimm mich", "tokens": ["O", "Die\u00b7bin", ",", "o\u00b7der", "nimm", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "KON", "VVIMP", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In deinen Rachen ein!", "tokens": ["In", "dei\u00b7nen", "Ra\u00b7chen", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}