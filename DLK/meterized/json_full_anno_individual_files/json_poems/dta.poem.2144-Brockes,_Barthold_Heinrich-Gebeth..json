{"dta.poem.2144": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gebeth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gieb Herr und Sch\u00f6pfer dieser Welt,", "tokens": ["Gieb", "Herr", "und", "Sch\u00f6p\u00b7fer", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich auf meiner Lebensreise", "tokens": ["Da\u00df", "ich", "auf", "mei\u00b7ner", "Le\u00b7bens\u00b7rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dir diene, dich erheb\u2019 und preise,", "tokens": ["Dir", "die\u00b7ne", ",", "dich", "er\u00b7heb'", "und", "prei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "$,", "PPER", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und zwar auf eine solche Weise,", "tokens": ["Und", "zwar", "auf", "ei\u00b7ne", "sol\u00b7che", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die dir gef\u00e4llt!", "tokens": ["Die", "dir", "ge\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}