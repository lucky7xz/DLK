{"textgrid.poem.60319": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der traurige M\u00f6nch", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Schweden steht ein grauer Turm,", "tokens": ["In", "Schwe\u00b7den", "steht", "ein", "grau\u00b7er", "Turm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herbergend Eulen, Aare;", "tokens": ["Her\u00b7ber\u00b7gend", "Eu\u00b7len", ",", "Aa\u00b7re", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVPP", "NN", "$,", "NE", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Gespielt mit Regen, Blitz und Sturm", "tokens": ["Ge\u00b7spielt", "mit", "Re\u00b7gen", ",", "Blitz", "und", "Sturm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat er neunhundert Jahre;", "tokens": ["Hat", "er", "neun\u00b7hun\u00b7dert", "Jah\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "CARD", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Was je von Menschen hauste drin,", "tokens": ["Was", "je", "von", "Men\u00b7schen", "haus\u00b7te", "drin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Lust und Leid, ist l\u00e4ngst dahin.", "tokens": ["Mit", "Lust", "und", "Leid", ",", "ist", "l\u00e4ngst", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "VAFIN", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Regen str\u00f6mt, ein Reiter naht,", "tokens": ["Der", "Re\u00b7gen", "str\u00f6mt", ",", "ein", "Rei\u00b7ter", "naht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er spornt dem Ro\u00df die Flanken;", "tokens": ["Er", "spornt", "dem", "Ro\u00df", "die", "Flan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verloren hat er seinen Pfad", "tokens": ["Ver\u00b7lo\u00b7ren", "hat", "er", "sei\u00b7nen", "Pfad"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In D\u00e4mmrung und Gedanken;", "tokens": ["In", "D\u00e4mm\u00b7rung", "und", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es windet heulend sich im Wind", "tokens": ["Es", "win\u00b7det", "heu\u00b7lend", "sich", "im", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Wald, wie ein gepeitschtes Kind.", "tokens": ["Der", "Wald", ",", "wie", "ein", "ge\u00b7peitschtes", "Kind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Verrufen ist der Turm im Land,", "tokens": ["Ver\u00b7ru\u00b7fen", "ist", "der", "Turm", "im", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df nachts, bei hellem Lichte,", "tokens": ["Da\u00df", "nachts", ",", "bei", "hel\u00b7lem", "Lich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Geist dort spukt in M\u00f6nchsgewand,", "tokens": ["Ein", "Geist", "dort", "spukt", "in", "M\u00f6n\u00b7chs\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mit traurigem Gesichte;", "tokens": ["Mit", "trau\u00b7ri\u00b7gem", "Ge\u00b7sich\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wer dem M\u00f6nch ins Aug gesehn,", "tokens": ["Und", "wer", "dem", "M\u00f6nch", "ins", "Aug", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird traurig und will sterben gehn.", "tokens": ["Wird", "trau\u00b7rig", "und", "will", "ster\u00b7ben", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch ohne Schreck und Grauen tritt", "tokens": ["Doch", "oh\u00b7ne", "Schreck", "und", "Grau\u00b7en", "tritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Turmgew\u00f6lb der Reiter,", "tokens": ["Ins", "Turm\u00b7ge\u00b7w\u00f6lb", "der", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er f\u00fchrt herein den Rappen mit", "tokens": ["Er", "f\u00fchrt", "her\u00b7ein", "den", "Rap\u00b7pen", "mit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und scherzt zum R\u00f6\u00dflein heiter:", "tokens": ["Und", "scherzt", "zum", "R\u00f6\u00df\u00b7lein", "hei\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbgelt du, wir nehmens lieber auf", "tokens": ["\u00bb", "gelt", "du", ",", "wir", "neh\u00b7mens", "lie\u00b7ber", "auf"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "PPER", "$,", "PPER", "ADV", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Geistern als mit Wind und Trauf?\u00ab", "tokens": ["Mit", "Geis\u00b7tern", "als", "mit", "Wind", "und", "Trauf", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KOKOM", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Den Sattel und den nassen Zaum", "tokens": ["Den", "Sat\u00b7tel", "und", "den", "nas\u00b7sen", "Zaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entschnallt er seinem Pferde,", "tokens": ["Ent\u00b7schnallt", "er", "sei\u00b7nem", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er breitet sich im \u00f6den Raum", "tokens": ["Er", "brei\u00b7tet", "sich", "im", "\u00f6\u00b7den", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Mantel auf die Erde", "tokens": ["Den", "Man\u00b7tel", "auf", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und segnet noch den Aschenrest", "tokens": ["Und", "seg\u00b7net", "noch", "den", "A\u00b7schen\u00b7rest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der H\u00e4nde, die gebaut so fest.", "tokens": ["Der", "H\u00e4n\u00b7de", ",", "die", "ge\u00b7baut", "so", "fest", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und wie er schl\u00e4ft und wie er tr\u00e4umt", "tokens": ["Und", "wie", "er", "schl\u00e4ft", "und", "wie", "er", "tr\u00e4umt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "KON", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur mittern\u00e4chtgen Stunde,", "tokens": ["Zur", "mit\u00b7ter\u00b7n\u00e4cht\u00b7gen", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weckt ihn sein Pferd, es schnaubt und b\u00e4umt,", "tokens": ["Weckt", "ihn", "sein", "Pferd", ",", "es", "schnaubt", "und", "b\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hell ist die Turmesrunde,", "tokens": ["Hell", "ist", "die", "Tur\u00b7mes\u00b7run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Wand wie angez\u00fcndet glimmt;", "tokens": ["Die", "Wand", "wie", "an\u00b7ge\u00b7z\u00fcn\u00b7det", "glimmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Mann sein Herz zusammennimmt.", "tokens": ["Der", "Mann", "sein", "Herz", "zu\u00b7sam\u00b7men\u00b7nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Weit auf das Ro\u00df die N\u00fcstern rei\u00dft,", "tokens": ["Weit", "auf", "das", "Ro\u00df", "die", "N\u00fcs\u00b7tern", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es bleckt vor Angst die Z\u00e4hne,", "tokens": ["Es", "bleckt", "vor", "Angst", "die", "Z\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Rappe zitternd sieht den Geist", "tokens": ["Der", "Rap\u00b7pe", "zit\u00b7ternd", "sieht", "den", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und str\u00e4ubt empor die M\u00e4hne;", "tokens": ["Und", "str\u00e4ubt", "em\u00b7por", "die", "M\u00e4h\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nun schaut den Geist der Reiter auch", "tokens": ["Nun", "schaut", "den", "Geist", "der", "Rei\u00b7ter", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und kreuzet sich nach altem Brauch.", "tokens": ["Und", "kreu\u00b7zet", "sich", "nach", "al\u00b7tem", "Brauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der M\u00f6nch hat sich vor ihn gestellt,", "tokens": ["Der", "M\u00f6nch", "hat", "sich", "vor", "ihn", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "So klagend still, so schaurig,", "tokens": ["So", "kla\u00b7gend", "still", ",", "so", "schau\u00b7rig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als weine stumm aus ihm die Welt,", "tokens": ["Als", "wei\u00b7ne", "stumm", "aus", "ihm", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ADJD", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So traurig, o wie traurig!", "tokens": ["So", "trau\u00b7rig", ",", "o", "wie", "trau\u00b7rig", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "FM", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Wandrer schaut ihn unverwandt", "tokens": ["Der", "Wand\u00b7rer", "schaut", "ihn", "un\u00b7ver\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wird von Mitleid \u00fcbermannt.", "tokens": ["Und", "wird", "von", "Mit\u00b7leid", "\u00fc\u00b7berm\u00b7annt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der gro\u00dfe und geheime Schmerz,", "tokens": ["Der", "gro\u00b7\u00dfe", "und", "ge\u00b7hei\u00b7me", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der die Natur durchzittert,", "tokens": ["Der", "die", "Na\u00b7tur", "durch\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den ahnen mag ein blutend Herz,", "tokens": ["Den", "ah\u00b7nen", "mag", "ein", "blu\u00b7tend", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VMFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den die Verzweiflung wittert,", "tokens": ["Den", "die", "Ver\u00b7zwei\u00b7flung", "wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Doch nicht erreicht \u2013 ", "tokens": ["Doch", "nicht", "er\u00b7reicht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVPP", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Im Aug des M\u00f6nchs, der Reiter weint.", "tokens": ["Im", "Aug", "des", "M\u00f6nchs", ",", "der", "Rei\u00b7ter", "weint", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Er ruft: \u00bbO sage, was dich kr\u00e4nkt?", "tokens": ["Er", "ruft", ":", "\u00bb", "O", "sa\u00b7ge", ",", "was", "dich", "kr\u00e4nkt", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NE", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was dich so tief beweget?\u00ab", "tokens": ["Was", "dich", "so", "tief", "be\u00b7we\u00b7get", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch wie der M\u00f6nch das Antlitz senkt,", "tokens": ["Doch", "wie", "der