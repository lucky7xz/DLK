{"textgrid.poem.39539": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das Seil des Oknus", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Autoren giebt es, wie Sand am Meer,", "tokens": ["Au\u00b7to\u00b7ren", "giebt", "es", ",", "wie", "Sand", "am", "Meer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PWAV", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Censoren auch ein gro\u00dfes Heer.", "tokens": ["Cen\u00b7so\u00b7ren", "auch", "ein", "gro\u00b7\u00dfes", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Autor plagt sich mit Schreiben sehr;", "tokens": ["Der", "Au\u00b7tor", "plagt", "sich", "mit", "Schrei\u00b7ben", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Censor streicht ihm die Zeilen leer:", "tokens": ["Der", "Cen\u00b7sor", "streicht", "ihm", "die", "Zei\u00b7len", "leer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dann macht das Ober-Censurgericht", "tokens": ["Dann", "macht", "das", "O\u00b7ber\u00b7Cen\u00b7sur\u00b7ge\u00b7richt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Dem Censor viele Striche zunicht.", "tokens": ["Dem", "Cen\u00b7sor", "vie\u00b7le", "Stri\u00b7che", "zu\u00b7nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "So haben sie alle vollauf zu thun;", "tokens": ["So", "ha\u00b7ben", "sie", "al\u00b7le", "vol\u00b7lauf", "zu", "thun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der Leser will aber auch nicht ruhn.", "tokens": ["Der", "Le\u00b7ser", "will", "a\u00b7ber", "auch", "nicht", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Schafft ihm nur Futter, so ist er froh,", "tokens": ["Schafft", "ihm", "nur", "Fut\u00b7ter", ",", "so", "ist", "er", "froh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.10": {"text": "Und w\u00e4r' es auch H\u00e4cksel von altem Stroh.", "tokens": ["Und", "w\u00e4r'", "es", "auch", "H\u00e4ck\u00b7sel", "von", "al\u00b7tem", "Stroh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Autoren giebt es, wie Sand am Meer,", "tokens": ["Au\u00b7to\u00b7ren", "giebt", "es", ",", "wie", "Sand", "am", "Meer", ","], "token_info": ["word", "word", "word", "punct", "