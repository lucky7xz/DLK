{"textgrid.poem.35122": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Klarheit", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlie\u00df ab, schlie\u00df ab an jedem Tag des Lebens,", "tokens": ["Schlie\u00df", "ab", ",", "schlie\u00df", "ab", "an", "je\u00b7dem", "Tag", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und frage dich, zu welchem Zweck du lebst.", "tokens": ["Und", "fra\u00b7ge", "dich", ",", "zu", "wel\u00b7chem", "Zweck", "du", "lebst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPR", "PWAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Stets mu\u00dft du wissen, ob du wohl vergebens,", "tokens": ["Stets", "mu\u00dft", "du", "wis\u00b7sen", ",", "ob", "du", "wohl", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob mit Erfolg nach diesem Ziele strebst.", "tokens": ["Ob", "mit", "Er\u00b7folg", "nach", "die\u00b7sem", "Zie\u00b7le", "strebst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein kluger Mann will keine einz'ge Stunde", "tokens": ["Ein", "klu\u00b7ger", "Mann", "will", "kei\u00b7ne", "einz'\u00b7ge", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Zweifel \u00fcber seine Lage sein;", "tokens": ["Im", "Zwei\u00b7fel", "\u00fc\u00b7ber", "sei\u00b7ne", "La\u00b7ge", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er fordert von ihr klare, sichre Kunde", "tokens": ["Er", "for\u00b7dert", "von", "ihr", "kla\u00b7re", ",", "sich\u00b7re", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und pr\u00e4gt sich, was sie sagt, f\u00fcr immer ein.", "tokens": ["Und", "pr\u00e4gt", "sich", ",", "was", "sie", "sagt", ",", "f\u00fcr", "im\u00b7mer", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wer das nicht thut, der gleicht den armen Frauen,", "tokens": ["Wer", "das", "nicht", "thut", ",", "der", "gleicht", "den", "ar\u00b7men", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "PTKNEG", "VVFIN", "$,", "PRELS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ohne Oel und nie ger\u00fcstet sind.", "tokens": ["Die", "oh\u00b7ne", "O\u00b7el", "und", "nie", "ge\u00b7r\u00fcs\u00b7tet", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie schlafen fort im blinden Selbstvertrauen", "tokens": ["Sie", "schla\u00b7fen", "fort", "im", "blin\u00b7den", "Selbst\u00b7ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sind, wie dies Vertrauen, selbst auch blind.", "tokens": ["Und", "sind", ",", "wie", "dies", "Ver\u00b7trau\u00b7en", ",", "selbst", "auch", "blind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PWAV", "PDS", "NN", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Schlie\u00df ab, schlie\u00df ab an jedem Tag des Lebens,", "tokens": ["Schlie\u00df", "ab", ",", "schlie\u00df", "ab", "an", "je\u00b7dem", "Tag", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und frage dich, zu welchem Zweck du lebst.", "tokens": ["Und", "fra\u00b7ge", "dich", ",", "zu", "wel\u00b7chem", "Zweck", "du", "lebst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPR", "PWAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Stets mu\u00dft du wissen, ob du wohl vergebens,", "tokens": ["Stets", "mu\u00dft", "du", "wis\u00b7sen",