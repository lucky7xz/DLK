{"dta.poem.19354": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "87.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein altes Wort, ich will es dir entfalten:", "tokens": ["Es", "ist", "ein", "al\u00b7tes", "Wort", ",", "ich", "will", "es", "dir", "ent\u00b7fal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PPER", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In einem Zweifelsfall ists gut sich zu enthalten.", "tokens": ["In", "ei\u00b7nem", "Zwei\u00b7fels\u00b7fall", "ists", "gut", "sich", "zu", "ent\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADJD", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein Sohn, es gilt dis Wort, ich will es dir erkl\u00e4ren,", "tokens": ["Mein", "Sohn", ",", "es", "gilt", "dis", "Wort", ",", "ich", "will", "es", "dir", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PDS", "NN", "$,", "PPER", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In einer Sf\u00e4re nicht, es gilt in allen Sf\u00e4ren.", "tokens": ["In", "ei\u00b7ner", "Sf\u00e4\u00b7re", "nicht", ",", "es", "gilt", "in", "al\u00b7len", "Sf\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "$,", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es gilt im Rechtsgebiet: Wo zwischen Mein und Dein", "tokens": ["Es", "gilt", "im", "Rechts\u00b7ge\u00b7biet", ":", "Wo", "zwi\u00b7schen", "Mein", "und", "Dein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "PWAV", "APPR", "PPOSAT", "KON", "PPOSAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Zweifel waltet ob, sag barsch nicht: es ist mein!", "tokens": ["Ein", "Zwei\u00b7fel", "wal\u00b7tet", "ob", ",", "sag", "barsch", "nicht", ":", "es", "ist", "mein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOUS", "$,", "VVFIN", "ADJD", "PTKNEG", "$.", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}}, "stanza.4": {"line.1": {"text": "Es gilt im Sittlichen: Wo zwischen b\u00f6s' und gut", "tokens": ["Es", "gilt", "im", "Sitt\u00b7li\u00b7chen", ":", "Wo", "zwi\u00b7schen", "b\u00f6s'", "und", "gut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "PWAV", "APPR", "ADJD", "KON", "ADJD"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die That ist zweifelhaft, thut wohl, wer nicht sie thut.", "tokens": ["Die", "That", "ist", "zwei\u00b7fel\u00b7haft", ",", "thut", "wohl", ",", "wer", "nicht", "sie", "thut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "VVFIN", "ADV", "$,", "PWS", "PTKNEG", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Es gilt im Handel auch und Wandel: Ist Gewinn", "tokens": ["Es", "gilt", "im", "Han\u00b7del", "auch", "und", "Wan\u00b7del", ":", "Ist", "Ge\u00b7winn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "KON", "NN", "$.", "VAFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Schaden zweifelhaft, so leg den Handel hin.", "tokens": ["Und", "Scha\u00b7den", "zwei\u00b7fel\u00b7haft", ",", "so", "leg", "den", "Han\u00b7del", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "$,", "ADV", "NE", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Es gilt im Waffenspiel: Wo zweifelhaft der Sieg", "tokens": ["Es", "gilt", "im", "Waf\u00b7fen\u00b7spiel", ":", "Wo", "zwei\u00b7fel\u00b7haft", "der", "Sieg"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "PWAV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem klugen Feldherrn scheint, vermeidet er den Krieg.", "tokens": ["Dem", "klu\u00b7gen", "Feld\u00b7herrn", "scheint", ",", "ver\u00b7mei\u00b7det", "er", "den", "Krieg", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Es gilt im Wandern auch: Wo dir durch ein Geheg", "tokens": ["Es", "gilt", "im", "Wan\u00b7dern", "auch", ":", "Wo", "dir", "durch", "ein", "Ge\u00b7heg"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "$.", "PWAV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Weg unsicher scheint, bleib auf dem sichern Weg.", "tokens": ["Der", "Weg", "un\u00b7si\u00b7cher", "scheint", ",", "bleib", "auf", "dem", "si\u00b7chern", "Weg", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Es gilt im Wissen auch: Wo das kann seyn und dis,", "tokens": ["Es", "gilt", "im", "Wis\u00b7sen", "auch", ":", "Wo", "das", "kann", "seyn", "und", "dis", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "$.", "PWAV", "PDS", "VMFIN", "VAINF", "KON", "PDS", "$,"], "meter": "-+-+-----+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Sag nicht: das ist, dis nicht! sag: es ist ungewis.", "tokens": ["Sag", "nicht", ":", "das", "ist", ",", "dis", "nicht", "!", "sag", ":", "es", "ist", "un\u00b7ge\u00b7wis", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$.", "PDS", "VAFIN", "$,", "PDS", "PTKNEG", "$.", "VVFIN", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "So hab' ich dir erkl\u00e4rt dis Wort, um dir zu z\u00e4hmen", "tokens": ["So", "hab'", "ich", "dir", "er\u00b7kl\u00e4rt", "dis", "Wort", ",", "um", "dir", "zu", "z\u00e4h\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVFIN", "PDS", "NN", "$,", "KOUI", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den ungest\u00fcmen Sinn, doch nicht den Muth zu l\u00e4hmen.", "tokens": ["Den", "un\u00b7ge\u00b7st\u00fc\u00b7men", "Sinn", ",", "doch", "nicht", "den", "Muth", "zu", "l\u00e4h\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Solang ein Zweifel ist, la\u00df dich von ihm bedingen,", "tokens": ["So\u00b7lang", "ein", "Zwei\u00b7fel", "ist", ",", "la\u00df", "dich", "von", "ihm", "be\u00b7din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VAFIN", "$,", "VVIMP", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch da\u00df er nicht mehr sei, versuch' ihn zu bezwingen.", "tokens": ["Doch", "da\u00df", "er", "nicht", "mehr", "sei", ",", "ver\u00b7such'", "ihn", "zu", "be\u00b7zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "ADV", "VAFIN", "$,", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Verzweifle nicht an dir vor jedem Zweifelsfall;", "tokens": ["Ver\u00b7zweif\u00b7le", "nicht", "an", "dir", "vor", "je\u00b7dem", "Zwei\u00b7fels\u00b7fall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du mit Muth ihm stehst, siehst du des Zweifels Fall.", "tokens": ["Wenn", "du", "mit", "Muth", "ihm", "stehst", ",", "siehst", "du", "des", "Zwei\u00b7fels", "Fall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Gib dich gefangen nie in tr\u00e4ger Zweifel Haft!", "tokens": ["Gib", "dich", "ge\u00b7fan\u00b7gen", "nie", "in", "tr\u00e4\u00b7ger", "Zwei\u00b7fel", "Haft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "ADV", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In jedem Zweifelsfall r\u00e4th Gott unzweifelhaft.", "tokens": ["In", "je\u00b7dem", "Zwei\u00b7fels\u00b7fall", "r\u00e4\u00b7th", "Gott", "un\u00b7zwei\u00b7fel\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "NN", "ADJD", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}