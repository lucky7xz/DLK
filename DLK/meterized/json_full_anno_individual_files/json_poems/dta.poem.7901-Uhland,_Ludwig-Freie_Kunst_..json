{"dta.poem.7901": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Freie Kunst .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Singe, wem Gesang gegeben,", "tokens": ["Sin\u00b7ge", ",", "wem", "Ge\u00b7sang", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem deutschen Dichterwald!", "tokens": ["In", "dem", "deut\u00b7schen", "Dich\u00b7ter\u00b7wald", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das ist Freude, das ist Leben,", "tokens": ["Das", "ist", "Freu\u00b7de", ",", "das", "ist", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "PDS", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn\u2019s von allen Zweigen schallt.", "tokens": ["Wenn's", "von", "al\u00b7len", "Zwei\u00b7gen", "schallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht an wenig stolze Namen", "tokens": ["Nicht", "an", "we\u00b7nig", "stol\u00b7ze", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist die Liederkunst gebannt;", "tokens": ["Ist", "die", "Lie\u00b7der\u00b7kunst", "ge\u00b7bannt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ausgestreuet ist der Samen", "tokens": ["Aus\u00b7ge\u00b7streu\u00b7et", "ist", "der", "Sa\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber alles deutsche Land.", "tokens": ["Ue\u00b7ber", "al\u00b7les", "deut\u00b7sche", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Deines vollen Herzens Triebe,", "tokens": ["Dei\u00b7nes", "vol\u00b7len", "Her\u00b7zens", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gib sie keck im Klange frei!", "tokens": ["Gib", "sie", "keck", "im", "Klan\u00b7ge", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00e4uselnd wandle deine Liebe,", "tokens": ["S\u00e4u\u00b7selnd", "wand\u00b7le", "dei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Donnernd uns dein Zorn vorbei!", "tokens": ["Don\u00b7nernd", "uns", "dein", "Zorn", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Singst du nicht dein ganzes Leben,", "tokens": ["Singst", "du", "nicht", "dein", "gan\u00b7zes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sing doch in der Jugend Drang!", "tokens": ["Sing", "doch", "in", "der", "Ju\u00b7gend", "Drang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur im Bl\u00fcthenmond erheben", "tokens": ["Nur", "im", "Bl\u00fc\u00b7then\u00b7mond", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nachtigallen ihren Sang.", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len", "ih\u00b7ren", "Sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Kann man\u2019s nicht in B\u00fccher binden,", "tokens": ["Kann", "man's", "nicht", "in", "B\u00fc\u00b7cher", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was die Stunden dir verleihn:", "tokens": ["Was", "die", "Stun\u00b7den", "dir", "ver\u00b7leihn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gib ein fliegend Blatt den Winden,", "tokens": ["Gib", "ein", "flie\u00b7gend", "Blatt", "den", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJD", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Muntre Jugend hascht es ein.", "tokens": ["Mun\u00b7tre", "Ju\u00b7gend", "hascht", "es", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Fahret wohl, geheime Kunden,", "tokens": ["Fah\u00b7ret", "wohl", ",", "ge\u00b7hei\u00b7me", "Kun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nekromantik, Alchymie!", "tokens": ["Ne\u00b7kro\u00b7man\u00b7tik", ",", "Al\u00b7chy\u00b7mie", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Formel h\u00e4lt uns nicht gebunden,", "tokens": ["For\u00b7mel", "h\u00e4lt", "uns", "nicht", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre Kunst hei\u00dft Poesie.", "tokens": ["Uns\u00b7re", "Kunst", "hei\u00dft", "Poe\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Heilig achten wir die Geister,", "tokens": ["Hei\u00b7lig", "ach\u00b7ten", "wir", "die", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber Namen sind uns Dunst;", "tokens": ["A\u00b7ber", "Na\u00b7men", "sind", "uns", "Dunst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrdig ehren wir die Meister,", "tokens": ["W\u00fcr\u00b7dig", "eh\u00b7ren", "wir", "die", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber frei ist uns die Kunst.", "tokens": ["A\u00b7ber", "frei", "ist", "uns", "die", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nicht in kalten Marmorsteinen,", "tokens": ["Nicht", "in", "kal\u00b7ten", "Mar\u00b7mor\u00b7stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht in Tempeln, dumpf und todt:", "tokens": ["Nicht", "in", "Tem\u00b7peln", ",", "dumpf", "und", "todt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den frischen Eichenhainen", "tokens": ["In", "den", "fri\u00b7schen", "Ei\u00b7chen\u00b7hai\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Webt und rauscht der deutsche Gott.", "tokens": ["Webt", "und", "rauscht", "der", "deut\u00b7sche", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}