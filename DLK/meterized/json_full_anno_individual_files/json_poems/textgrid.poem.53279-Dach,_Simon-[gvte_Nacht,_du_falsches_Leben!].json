{"textgrid.poem.53279": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[gvte Nacht, du falsches Leben!]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gvte Nacht, du falsches Leben!", "tokens": ["Gv\u00b7te", "Nacht", ",", "du", "fal\u00b7sches", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gute Nacht, all eytle Lust!", "tokens": ["Gu\u00b7te", "Nacht", ",", "all", "eyt\u00b7le", "Lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An der Erden Koht vnd Wust", "tokens": ["An", "der", "Er\u00b7den", "Koht", "vnd", "Wust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mag ich nicht mehr kleben,", "tokens": ["Mag", "ich", "nicht", "mehr", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Meine zeit hab' ich vollbracht,", "tokens": ["Mei\u00b7ne", "zeit", "hab'", "ich", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meinen Lauff vollendet,", "tokens": ["Mei\u00b7nen", "Lauff", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Christus hat durch seine Macht", "tokens": ["Chris\u00b7tus", "hat", "durch", "sei\u00b7ne", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "All mein Leidt gewendet.", "tokens": ["All", "mein", "Leidt", "ge\u00b7wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Was ist vnser thun vnd treiben", "tokens": ["Was", "ist", "vn\u00b7ser", "thun", "vnd", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADJD", "VVINF", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Anders, als nur Eytelheit?", "tokens": ["An\u00b7ders", ",", "als", "nur", "Ey\u00b7tel\u00b7heit", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine gantze Lebens Zeit", "tokens": ["Mei\u00b7ne", "gant\u00b7ze", "Le\u00b7bens", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df in Eytelheit verkleiben;", "tokens": ["Mu\u00df", "in", "Ey\u00b7tel\u00b7heit", "ver\u00b7klei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was wir jmmer auff der Welt", "tokens": ["Was", "wir", "jm\u00b7mer", "auff", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcr gen\u00fcgen finden,", "tokens": ["F\u00fcr", "ge\u00b7n\u00fc\u00b7gen", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Dem ist bald sein Ziel gestelt,", "tokens": ["Dem", "ist", "bald", "sein", "Ziel", "ge\u00b7stelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df es mu\u00df verschwinden.", "tokens": ["Da\u00df", "es", "mu\u00df", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Weg mit solcher Lust vnd Frewde!", "tokens": ["Weg", "mit", "sol\u00b7cher", "Lust", "vnd", "Frew\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die nur Eytelheit gebiert,", "tokens": ["Die", "nur", "Ey\u00b7tel\u00b7heit", "ge\u00b7biert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd vns zum verderben f\u00fchrt,", "tokens": ["Vnd", "vns", "zum", "ver\u00b7der\u00b7ben", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Seelen Weide", "tokens": ["Mei\u00b7ner", "See\u00b7len", "Wei\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ist das wahre Gottes Lamm,", "tokens": ["Ist", "das", "wah\u00b7re", "Got\u00b7tes", "Lamm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das f\u00fcr mich gestorben,", "tokens": ["Das", "f\u00fcr", "mich", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Das mir an dem Creutzes-Stamm", "tokens": ["Das", "mir", "an", "dem", "Creut\u00b7zes\u00b7Stamm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ew'ges Heil erworben.", "tokens": ["Ew'\u00b7ges", "Heil", "er\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "In jhm sind ich mein vergn\u00fcgen,", "tokens": ["In", "jhm", "sind", "ich", "mein", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In jhm werd' ich blo\u00df ergetzt,", "tokens": ["In", "jhm", "werd'", "ich", "blo\u00df", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das mich endlich wird zuletzt", "tokens": ["Das", "mich", "end\u00b7lich", "wird", "zu\u00b7letzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ADV", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu den Frommen f\u00fcgen,", "tokens": ["Zu", "den", "From\u00b7men", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "O wie selig bin ich schon,", "tokens": ["O", "wie", "se\u00b7lig", "bin", "ich", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich mein verlangen,", "tokens": ["Da\u00df", "ich", "mein", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Meinen Trost vnd Gnadenthron", "tokens": ["Mei\u00b7nen", "Trost", "vnd", "Gna\u00b7dent\u00b7hron"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ewig sol vmbfangen!", "tokens": ["E\u00b7wig", "sol", "vmb\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Herr, wenn wird es denn geschehen,", "tokens": ["Herr", ",", "wenn", "wird", "es", "denn", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich aller Quaal vnd Pein", "tokens": ["Da\u00df", "ich", "al\u00b7ler", "Qua\u00b7al", "vnd", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Werd' einmal befreyet seyn?", "tokens": ["Werd'", "ein\u00b7mal", "be\u00b7fre\u00b7yet", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "VAINF", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Wenn werd' ich dich sehen?", "tokens": ["Wenn", "werd'", "ich", "dich", "se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Jesu, meine Zuversicht,", "tokens": ["Je\u00b7su", ",", "mei\u00b7ne", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hoffnung vnd vertrawen,", "tokens": ["Hoff\u00b7nung", "vnd", "ver\u00b7tra\u00b7wen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wenn werd' ich dein Angesicht", "tokens": ["Wenn", "werd'", "ich", "dein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seliglich anschawen?", "tokens": ["Se\u00b7lig\u00b7lich", "an\u00b7scha\u00b7wen", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Nun, ich bin bereit zu scheiden,", "tokens": ["Nun", ",", "ich", "bin", "be\u00b7reit", "zu", "schei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann du wilt, komm mit dem Todt,", "tokens": ["Wann", "du", "wilt", ",", "komm", "mit", "dem", "Todt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mach mich frey von aller Noht,", "tokens": ["Mach", "mich", "frey", "von", "al\u00b7ler", "Noht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeuch mich zu den Frewden;", "tokens": ["Zeuch", "mich", "zu", "den", "Frew\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "O mein Schatz vnd h\u00f6chstes Gutt,", "tokens": ["O", "mein", "Schatz", "vnd", "h\u00f6chs\u00b7tes", "Gutt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vnbegrieffnes Wesen,", "tokens": ["Vn\u00b7be\u00b7grieff\u00b7nes", "We\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "La\u00df mich durch dein thewres Blut", "tokens": ["La\u00df", "mich", "durch", "dein", "thew\u00b7res", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ewiglich genesen.", "tokens": ["E\u00b7wig\u00b7lich", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Gvte Nacht, du fa