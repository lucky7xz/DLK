{"dta.poem.3319": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Empfindungen  \n An einem Fr\u00fchlings-Morgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "O welche frische Luft haucht vom beb\u00fcschten H\u00fcgel!", "tokens": ["O", "wel\u00b7che", "fri\u00b7sche", "Luft", "haucht", "vom", "be\u00b7b\u00fcschten", "H\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAT", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Welch angenehmer West durchzieht", "tokens": ["Welch", "an\u00b7ge\u00b7neh\u00b7mer", "West", "durch\u00b7zieht"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit rauschendem bethauten Fl\u00fcgel", "tokens": ["Mit", "rau\u00b7schen\u00b7dem", "be\u00b7thau\u00b7ten", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die\u00df holde Thal, wo alles gr\u00fcnt und bl\u00fcht!", "tokens": ["Die\u00df", "hol\u00b7de", "Thal", ",", "wo", "al\u00b7les", "gr\u00fcnt", "und", "bl\u00fcht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,", "PWAV", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hier, wo die Grazien sich ihre Bluhmen hohlen,", "tokens": ["Hier", ",", "wo", "die", "Gra\u00b7zi\u00b7en", "sich", "ih\u00b7re", "Bluh\u00b7men", "hoh\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "PRF", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier seh ich, wie der Morgen lacht,", "tokens": ["Hier", "seh", "ich", ",", "wie", "der", "Mor\u00b7gen", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der unter d\u00fcftenden Violen", "tokens": ["Der", "un\u00b7ter", "d\u00fcf\u00b7ten\u00b7den", "Vi\u00b7o\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und beym Gesang der V\u00f6gel aufgewacht.", "tokens": ["Und", "beym", "Ge\u00b7sang", "der", "V\u00f6\u00b7gel", "auf\u00b7ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das kleinste Gr\u00e4schen blitzt vom farbenreiche\u0303 Thaue", "tokens": ["Das", "kleins\u00b7te", "Gr\u00e4sc\u00b7hen", "blitzt", "vom", "fa\u00b7rben\u00b7reich\u1ebd", "Thau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie himmlisch l\u00e4chelt die Natur,", "tokens": ["Wie", "himm\u00b7lisch", "l\u00e4\u00b7chelt", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohin ich um und bey mir schaue,", "tokens": ["Wo\u00b7hin", "ich", "um", "und", "bey", "mir", "schau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKVZ", "KON", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dort im Gestr\u00e4uch und hier auf gr\u00fcner Flur!", "tokens": ["Dort", "im", "Ge\u00b7str\u00e4uch", "und", "hier", "auf", "gr\u00fc\u00b7ner", "Flur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "KON", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die ganze Sch\u00f6pfung zeugt von weiser Gute H\u00e4nde\u0303;", "tokens": ["Die", "gan\u00b7ze", "Sch\u00f6p\u00b7fung", "zeugt", "von", "wei\u00b7ser", "Gu\u00b7te", "H\u00e4n\u00b7d\u1ebd", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Sch\u00f6nheit pranget unsre Welt.", "tokens": ["Mit", "Sch\u00f6n\u00b7heit", "pran\u00b7get", "uns\u00b7re", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df nur der Mensch die Sch\u00f6pfung sch\u00e4nden,", "tokens": ["Mu\u00df", "nur", "der", "Mensch", "die", "Sch\u00f6p\u00b7fung", "sch\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der sich so gern f\u00fcr ihre Zierde h\u00e4lt?", "tokens": ["Der", "sich", "so", "gern", "f\u00fcr", "ih\u00b7re", "Zier\u00b7de", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der Mensch darf sich nur sehn, damit er sich nicht", "tokens": ["Der", "Mensch", "darf", "sich", "nur", "sehn", ",", "da\u00b7mit", "er", "sich", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADV", "VVINF", "$,", "KOUS", "PPER", "PRF", "PTKNEG"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "br\u00fcste,", "tokens": ["br\u00fcs\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wie, an der Thorheit Brust ges\u00e4ugt,", "tokens": ["Wie", ",", "an", "der", "Thor\u00b7heit", "Brust", "ge\u00b7s\u00e4ugt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er sich im Taumel wilder L\u00fcste", "tokens": ["Er", "sich", "im", "Tau\u00b7mel", "wil\u00b7der", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PRF", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bald l\u00e4cherlich und bald abscheulich zeigt.", "tokens": ["Bald", "l\u00e4\u00b7cher\u00b7lich", "und", "bald", "ab\u00b7scheu\u00b7lich", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Um Tand und Puppenwerk vertauscht er seine Rechte", "tokens": ["Um", "Tand", "und", "Pup\u00b7pen\u00b7werk", "ver\u00b7tauscht", "er", "sei\u00b7ne", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "KON", "NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu gl\u00e4nzender Unsterblichkeit,", "tokens": ["Zu", "gl\u00e4n\u00b7zen\u00b7der", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Erniedrigt sich und sein Geschlechte,", "tokens": ["Er\u00b7nied\u00b7rigt", "sich", "und", "sein", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sucht kurze Lust und findet ewig Leid.", "tokens": ["Sucht", "kur\u00b7ze", "Lust", "und", "fin\u00b7det", "e\u00b7wig", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "KON", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ein denkendes Gesch\u00f6pf kann so verderblich w\u00e4hlen,", "tokens": ["Ein", "den\u00b7ken\u00b7des", "Ge\u00b7sch\u00f6pf", "kann", "so", "ver\u00b7derb\u00b7lich", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als w\u00e4r es nur zum Thier bestimmt?", "tokens": ["Als", "w\u00e4r", "es", "nur", "zum", "Thier", "be\u00b7stimmt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Herrscht solche Blindheit \u00fcber Seelen,", "tokens": ["Herrscht", "sol\u00b7che", "Blind\u00b7heit", "\u00fc\u00b7ber", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In welchen doch der Gottheit Funke glimmt?", "tokens": ["In", "wel\u00b7chen", "doch", "der", "Got\u00b7theit", "Fun\u00b7ke", "glimmt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Umsonst! weil dieser Strahl nur wenig Weisen funkelt!", "tokens": ["Um\u00b7sonst", "!", "weil", "die\u00b7ser", "Strahl", "nur", "we\u00b7nig", "Wei\u00b7sen", "fun\u00b7kelt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KOUS", "PDAT", "NN", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er wird von Leidenschaft und Wahn", "tokens": ["Er", "wird", "von", "Lei\u00b7den\u00b7schaft", "und", "Wahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In tausend Sterblichen verdunkelt,", "tokens": ["In", "tau\u00b7send", "Sterb\u00b7li\u00b7chen", "ver\u00b7dun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Oft eh er sich siegprangend kundgethan:", "tokens": ["Oft", "eh", "er", "sich", "sieg\u00b7pran\u00b7gend", "kund\u00b7ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Wie, wann die Sonne kaum dem Ocean entfliehet,", "tokens": ["Wie", ",", "wann", "die", "Son\u00b7ne", "kaum", "dem", "O\u00b7cean", "ent\u00b7flie\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des dunkeln Mondes Zwischenlauf", "tokens": ["Des", "dun\u00b7keln", "Mon\u00b7des", "Zwi\u00b7schen\u00b7lauf"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr flammend Antlitz uns entziehet:", "tokens": ["Ihr", "flam\u00b7mend", "Ant\u00b7litz", "uns", "ent\u00b7zie\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor ihrem Thron steigt schwarzer Schatten auf.", "tokens": ["Vor", "ih\u00b7rem", "Thron", "steigt", "schwar\u00b7zer", "Schat\u00b7ten", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Die V\u00f6gel hemmen schnell die angefangnen Lieder;", "tokens": ["Die", "V\u00f6\u00b7gel", "hem\u00b7men", "schnell", "die", "an\u00b7ge\u00b7fang\u00b7nen", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der halbverirrte Wandrer bebt,", "tokens": ["Der", "halb\u00b7ve\u00b7rirr\u00b7te", "Wand\u00b7rer", "bebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Inde\u00df mit schreckendem Gefieder", "tokens": ["In\u00b7de\u00df", "mit", "schre\u00b7cken\u00b7dem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die fr\u00fche Nacht um Erd und Himmel schwebt:", "tokens": ["Die", "fr\u00fc\u00b7he", "Nacht", "um", "Erd", "und", "Him\u00b7mel", "schwebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Bis Titans froher Blick, nach \u00fcberwundnen Schatten,", "tokens": ["Bis", "Ti\u00b7tans", "fro\u00b7her", "Blick", ",", "nach", "\u00fc\u00b7berw\u00b7und\u00b7nen", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jtzt wieder unverfinstert strahlt,", "tokens": ["Jtzt", "wie\u00b7der", "un\u00b7ver\u00b7fins\u00b7tert", "strahlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in den aufgehellten Matten", "tokens": ["Und", "in", "den", "auf\u00b7ge\u00b7hell\u00b7ten", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um Floren lacht und ihre Bluhmen mahlt.", "tokens": ["Um", "Flo\u00b7ren", "lacht", "und", "ih\u00b7re", "Bluh\u00b7men", "mahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "So strahlet unser Geist, mit angebohrnem Lichte,", "tokens": ["So", "strah\u00b7let", "un\u00b7ser", "Geist", ",", "mit", "an\u00b7ge\u00b7bohr\u00b7nem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch dicke Finsterni\u00df hervor,", "tokens": ["Durch", "di\u00b7cke", "Fins\u00b7ter\u00b7ni\u00df", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn vor der Weisheit Angesichte", "tokens": ["Wenn", "vor", "der", "Weis\u00b7heit", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Nebel fliehn, worinn er sich verlohr.", "tokens": ["Die", "Ne\u00b7bel", "fliehn", ",", "wo\u00b7rinn", "er", "sich", "ver\u00b7lohr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Geh auf mit vollem Tag, und herrsch' in Glanz und", "tokens": ["Geh", "auf", "mit", "vol\u00b7lem", "Tag", ",", "und", "herr\u00b7sch'", "in", "Glanz", "und"], "token_info": ["word", "word", "word", "wo