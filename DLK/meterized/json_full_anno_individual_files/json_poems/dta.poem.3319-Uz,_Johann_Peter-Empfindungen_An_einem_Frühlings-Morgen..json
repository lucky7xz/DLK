 "sich", "Thie\u00b7ren", "\u00e4hn\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "PRF", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.17": {"line.1": {"text": "Sie sucht und findet es in reiner Tugend Armen,", "tokens": ["Sie", "sucht", "und", "fin\u00b7det", "es", "in", "rei\u00b7ner", "Tu\u00b7gend", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sich f\u00fcr Andrer Wohl vergisst,", "tokens": ["Die", "sich", "f\u00fcr", "A\u00b7ndrer", "Wohl", "ver\u00b7gisst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, reich an g\u00f6ttlichem Erbarmen,", "tokens": ["Und", ",", "reich", "an", "g\u00f6tt\u00b7li\u00b7chem", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Himmel stammt, und selbst ein Himmel ist.", "tokens": ["Vom", "Him\u00b7mel", "stammt", ",", "und", "selbst", "ein", "Him\u00b7mel", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KON", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}