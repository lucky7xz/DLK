{"dta.poem.10078": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gegenwart des Guten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie wir, wann wir Aurora Rosen-Licht", "tokens": ["Wie", "wir", ",", "wann", "wir", "Au\u00b7ro\u00b7ra", "Ro\u00b7sen\u00b7Licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "PWAV", "PPER", "NE", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht mehr am Firmamente sehen,", "tokens": ["Nicht", "mehr", "am", "Fir\u00b7ma\u00b7men\u00b7te", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Selbst in Aurora Farben stehen,", "tokens": ["Selbst", "in", "Au\u00b7ro\u00b7ra", "Far\u00b7ben", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Merckt es gleich unser Auge nicht;", "tokens": ["Merckt", "es", "gleich", "un\u00b7ser", "Au\u00b7ge", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wie wir in den Abend-Stunden", "tokens": ["Und", "wie", "wir", "in", "den", "A\u00b7ben\u00b7dStun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Abend-Roth nicht eh\u2019 empfunden,", "tokens": ["Das", "A\u00b7ben\u00b7dRoth", "nicht", "eh'", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als bis es sich von uns entfernet, und vergeht;", "tokens": ["Als", "bis", "es", "sich", "von", "uns", "ent\u00b7fer\u00b7net", ",", "und", "ver\u00b7geht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So mercken wir das Gl\u00fcck, das GOTT uns g\u00f6nnet,", "tokens": ["So", "mer\u00b7cken", "wir", "das", "Gl\u00fcck", ",", "das", "GoTT", "uns", "g\u00f6n\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "In ferner Hoffnung nur, auch wann sichs von uns trennet;", "tokens": ["In", "fer\u00b7ner", "Hoff\u00b7nung", "nur", ",", "auch", "wann", "sichs", "von", "uns", "tren\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,", "ADV", "PWAV", "PIS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nur dann am wenigsten, wanns uns am n\u00e4chsten steht.", "tokens": ["Nur", "dann", "am", "we\u00b7nigs\u00b7ten", ",", "wanns", "uns", "am", "n\u00e4chs\u00b7ten", "steht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "PIS", "$,", "PWAV", "PPER", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-----+-+", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Ach liebste Menschen, lernet! lernet", "tokens": ["Ach", "liebs\u00b7te", "Men\u00b7schen", ",", "ler\u00b7net", "!", "ler\u00b7net"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ITJ", "ADJA", "NN", "$,", "VVFIN", "$.", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das gegenw\u00e4rtge Gl\u00fcck erkennen;", "tokens": ["Das", "ge\u00b7gen\u00b7w\u00e4rt\u00b7ge", "Gl\u00fcck", "er\u00b7ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht, wann es noch nicht da! nicht erst, wann sichs ent-", "tokens": ["Nicht", ",", "wann", "es", "noch", "nicht", "da", "!", "nicht", "erst", ",", "wann", "sichs", "ent"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "$,", "PWAV", "PPER", "ADV", "PTKNEG", "ADV", "$.", "PTKNEG", "ADV", "$,", "PWAV", "PIS", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Und nicht in Furcht und Hoffnung nur allein,", "tokens": ["Und", "nicht", "in", "Furcht", "und", "Hoff\u00b7nung", "nur", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "KON", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nein dann, wann ihr es seyd, vergn\u00fcgt zu seyn!", "tokens": ["Nein", "dann", ",", "wann", "ihr", "es", "seyd", ",", "ver\u00b7gn\u00fcgt", "zu", "seyn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "PWAV", "PPER", "PPER", "VAFIN", "$,", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So d\u00fcrftet ihr vielleicht, in wol empfundnen Freuden,", "tokens": ["So", "d\u00fcrf\u00b7tet", "ihr", "viel\u00b7leicht", ",", "in", "wol", "emp\u00b7fund\u00b7nen", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Aus blosser Danckbarkeit, die Laster meiden.", "tokens": ["Aus", "blos\u00b7ser", "Dan\u00b7ck\u00b7bar\u00b7keit", ",", "die", "Las\u00b7ter", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}