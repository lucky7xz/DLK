{"textgrid.poem.41803": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn hoch vom Turm die Glocken klingen,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn hoch vom Turm die Glocken klingen,", "tokens": ["Wenn", "hoch", "vom", "Turm", "die", "Glo\u00b7cken", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In mittern\u00e4chtlich ernster Stund'", "tokens": ["In", "mit\u00b7ter\u00b7n\u00e4cht\u00b7lich", "erns\u00b7ter", "Stund'"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Jahres Scheidegru\u00df zu bringen:", "tokens": ["Des", "Jah\u00b7res", "Schei\u00b7de\u00b7gru\u00df", "zu", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann lauschen wir, als werd' uns kund,", "tokens": ["Dann", "lau\u00b7schen", "wir", ",", "als", "werd'", "uns", "kund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOKOM", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was nun der neue Lauf der Horen", "tokens": ["Was", "nun", "der", "neu\u00b7e", "Lauf", "der", "Ho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Uns Erdenpilgern bieten mag \u2013", "tokens": ["Uns", "Er\u00b7den\u00b7pil\u00b7gern", "bie\u00b7ten", "mag", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Jahr ward neuverj\u00fcngt geboren", "tokens": ["Das", "Jahr", "ward", "neu\u00b7ver\u00b7j\u00fcngt", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und festlich gr\u00fc\u00dft sein erster Tag.", "tokens": ["Und", "fest\u00b7lich", "gr\u00fc\u00dft", "sein", "ers\u00b7ter", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch ist vergeblich alles Fragen,", "tokens": ["Doch", "ist", "ver\u00b7geb\u00b7lich", "al\u00b7les", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Antwort lautet immer gleich:", "tokens": ["Die", "Ant\u00b7wort", "lau\u00b7tet", "im\u00b7mer", "gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Propheten sind aus unsern Tagen", "tokens": ["Pro\u00b7phe\u00b7ten", "sind", "aus", "un\u00b7sern", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verbannt ins dunkle Sagenreich.", "tokens": ["Ver\u00b7bannt", "ins", "dunk\u00b7le", "Sa\u00b7gen\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Blick darf in die Werkstatt schweifen,", "tokens": ["Kein", "Blick", "darf", "in", "die", "Werk\u00b7statt", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In der des Menschen Los sich webt,", "tokens": ["In", "der", "des", "Men\u00b7schen", "Los", "sich", "webt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NE", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein Arm in das Getriebe greifen,", "tokens": ["Kein", "Arm", "in", "das", "Ge\u00b7trie\u00b7be", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das Schicksals-F\u00e4den senkt und hebt!", "tokens": ["Das", "Schick\u00b7sals\u00b7F\u00e4\u00b7den", "senkt", "und", "hebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das mu\u00dften alle wir erfahren", "tokens": ["Das", "mu\u00df\u00b7ten", "al\u00b7le", "wir", "er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PIS", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In unsrer Lieben engem Kreis \u2013", "tokens": ["In", "uns\u00b7rer", "Lie\u00b7ben", "en\u00b7gem", "Kreis", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gebrochen m\u00fcssen wir gewahren", "tokens": ["Ge\u00b7bro\u00b7chen", "m\u00fcs\u00b7sen", "wir", "ge\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Manch hoffnungsgr\u00fcne frisches Reis,", "tokens": ["Manch", "hoff\u00b7nungs\u00b7gr\u00fc\u00b7ne", "fri\u00b7sches", "Reis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wo wir's ahnend kaum vermutet,", "tokens": ["Und", "wo", "wir's", "ah\u00b7nend", "kaum", "ver\u00b7mu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da kam uns Rettung aus der Not,", "tokens": ["Da", "kam", "uns", "Ret\u00b7tung", "aus", "der", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Indessen dort ein Herz verblutet", "tokens": ["In\u00b7des\u00b7sen", "dort", "ein", "Herz", "ver\u00b7blu\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Weil ihm sein Liebstes nahm der Tod!", "tokens": ["Weil", "ihm", "sein", "Liebs\u00b7tes", "nahm", "der", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur eitel ist das ird'sche Hoffen,", "tokens": ["Nur", "ei\u00b7tel", "ist", "das", "ird'\u00b7sche", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das sich an \u00e4u\u00dfre Zeichen h\u00e4lt,", "tokens": ["Das", "sich", "an", "\u00e4u\u00df\u00b7re", "Zei\u00b7chen", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist nicht in uns ein Himmel offen,", "tokens": ["Ist", "nicht", "in", "uns", "ein", "Him\u00b7mel", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von dem kein Stern herunterf\u00e4llt.", "tokens": ["Von", "dem", "kein", "Stern", "her\u00b7un\u00b7ter\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie sehr auch Sturm und Donner wettert", "tokens": ["Wie", "sehr", "auch", "Sturm", "und", "Don\u00b7ner", "wet\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und frische Hoffnungssaat zerschl\u00e4gt", "tokens": ["Und", "fri\u00b7sche", "Hoff\u00b7nungs\u00b7saat", "zer\u00b7schl\u00e4gt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und alle Rosen uns entbl\u00e4ttert,", "tokens": ["Und", "al\u00b7le", "Ro\u00b7sen", "uns", "ent\u00b7bl\u00e4t\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie Staub in alle Winde tr\u00e4gt. \u2013", "tokens": ["Wie", "Staub", "in", "al\u00b7le", "Win\u00b7de", "tr\u00e4gt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "NN", "APPR", "PIAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein Himmel, den wir sicher schauen,", "tokens": ["Ein", "Him\u00b7mel", ",", "den", "wir", "si\u00b7cher", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sich der Blick nur ", "tokens": ["Wenn", "sich", "der", "Blick", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ein Himmel, den wir selber bauen,", "tokens": ["Ein", "Him\u00b7mel", ",", "den", "wir", "sel\u00b7ber", "bau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn wir zum h\u00f6chsten Ziel gestrebt,", "tokens": ["Wenn", "wir", "zum", "h\u00f6chs\u00b7ten", "Ziel", "ge\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Himmel, draus seit Ewigkeiten", "tokens": ["Ein", "Him\u00b7mel", ",", "draus", "seit", "E\u00b7wig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PAV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu uns die Sch\u00f6pfungsformel spricht,", "tokens": ["Zu", "uns", "die", "Sch\u00f6p\u00b7fungs\u00b7for\u00b7mel", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die heiligste f\u00fcr alle Zeiten:", "tokens": ["Die", "hei\u00b7ligs\u00b7te", "f\u00fcr", "al\u00b7le", "Zei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Kein Chaos mehr! \u2013 es werde Licht!", "tokens": ["Kein", "Chaos", "mehr", "!", "\u2013", "es", "wer\u00b7de", "Licht", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$.", "$(", "PPER", "VAFIN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Kein Chaos mehr \u2013 in unserm Leben,", "tokens": ["Kein", "Chaos", "mehr", "\u2013", "in", "un\u00b7serm", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$(", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Kein Chaos mehr im Vaterland!", "tokens": ["Kein", "Chaos", "mehr", "im", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es werde Licht, \u2013 dies unser Streben,", "tokens": ["Es", "wer\u00b7de", "Licht", ",", "\u2013", "dies", "un\u00b7ser", "Stre\u00b7ben", ","], "t