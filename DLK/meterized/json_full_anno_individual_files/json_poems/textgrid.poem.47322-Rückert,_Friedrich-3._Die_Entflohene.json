{"textgrid.poem.47322": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3. Die Entflohene", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die Sonne sinkt am Abend,", "tokens": ["Wie", "die", "Son\u00b7ne", "sinkt", "am", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich im goldnen Glanz begrabend;", "tokens": ["Sich", "im", "gold\u00b7nen", "Glanz", "be\u00b7gra\u00b7bend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Lenz vorm Herbste fl\u00fcchtet,", "tokens": ["Wie", "der", "Lenz", "vorm", "Herbs\u00b7te", "fl\u00fcch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Im Entfliehn mit Duft noch labend;", "tokens": ["Im", "Ent\u00b7fliehn", "mit", "Duft", "noch", "la\u00b7bend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "ADV", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wie die sch\u00f6ne Jugendg\u00f6ttin", "tokens": ["Wie", "die", "sch\u00f6\u00b7ne", "Ju\u00b7gend\u00b7g\u00f6t\u00b7tin"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf dem Ro\u00df der Zeit hintrabend;", "tokens": ["Auf", "dem", "Ro\u00df", "der", "Zeit", "hin\u00b7tra\u00b7bend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie das Leben, in den H\u00e4nden", "tokens": ["Wie", "das", "Le\u00b7ben", ",", "in", "den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unerf\u00fcllte W\u00fcnsche habend:", "tokens": ["Un\u00b7er\u00b7f\u00fcll\u00b7te", "W\u00fcn\u00b7sche", "ha\u00b7bend", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Also flohst du, Sonne, Fr\u00fchling,", "tokens": ["Al\u00b7so", "flohst", "du", ",", "Son\u00b7ne", ",", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Jugend, Leben, lustbegabend;", "tokens": ["Ju\u00b7gend", ",", "Le\u00b7ben", ",", "lust\u00b7be\u00b7ga\u00b7bend", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und Hafis, dir ferne, f\u00fchlet", "tokens": ["Und", "Ha\u00b7fis", ",", "dir", "fer\u00b7ne", ",", "f\u00fch\u00b7let"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "NE", "$,", "PPER", "ADV", "$,", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Sterben, Alter, Herbst und Abend.", "tokens": ["Ster\u00b7ben", ",", "Al\u00b7ter", ",", "Herbst", "und", "A\u00b7bend", "."], "token_info": ["word", "punct", "word", "punct", "word", "wor