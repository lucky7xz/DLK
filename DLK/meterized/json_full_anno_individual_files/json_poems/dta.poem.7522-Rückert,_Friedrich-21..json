{"dta.poem.7522": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gelobt sei jede Form, weich sei sie oder schroff;", "tokens": ["Ge\u00b7lobt", "sei", "je\u00b7de", "Form", ",", "weich", "sei", "sie", "o\u00b7der", "schroff", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "NN", "$,", "ADJD", "VAFIN", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn jede neue Form erzeuget neu den Stoff.", "tokens": ["Denn", "je\u00b7de", "neu\u00b7e", "Form", "er\u00b7zeu\u00b7get", "neu", "den", "Stoff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Geist, der einer ist und vielfach wird geboren,", "tokens": ["Der", "Geist", ",", "der", "ei\u00b7ner", "ist", "und", "viel\u00b7fach", "wird", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VAFIN", "KON", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sucht neuen Leib, wann er am alten Lust verloren.", "tokens": ["Sucht", "neu\u00b7en", "Leib", ",", "wann", "er", "am", "al\u00b7ten", "Lust", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,", "PWAV", "PPER", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er thut durch Ein Organ sich nur zur H\u00e4lfte kund,", "tokens": ["Er", "thut", "durch", "Ein", "Or\u00b7gan", "sich", "nur", "zur", "H\u00e4lf\u00b7te", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PRF", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verschweigt die H\u00e4lfte, bis er findet andern Mund.", "tokens": ["Ver\u00b7schweigt", "die", "H\u00e4lf\u00b7te", ",", "bis", "er", "fin\u00b7det", "an\u00b7dern", "Mund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was als Kristall er konnt', als Edelstein nicht spr\u00fchn,", "tokens": ["Was", "als", "Kris\u00b7tall", "er", "konnt'", ",", "als", "E\u00b7del\u00b7stein", "nicht", "spr\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "KOUS", "NN", "PPER", "VMFIN", "$,", "KOUS", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird er einmal als Pflanz', als Blum' einmal ausbl\u00fchn.", "tokens": ["Wird", "er", "ein\u00b7mal", "als", "Pflanz'", ",", "als", "Blum'", "ein\u00b7mal", "aus\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "KOUS", "NN", "$,", "KOUS", "NN", "ADV", "VVINF", "$."], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}}}}}