{"textgrid.poem.33380": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "An Fr\u00e4ulein M. von B*", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Autor, Theure, der mit Ehren,", "tokens": ["Ein", "Au\u00b7tor", ",", "Theu\u00b7re", ",", "der", "mit", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und als ein \u00e4chtkathol'scher Christ", "tokens": ["Und", "als", "ein", "\u00e4cht\u00b7ka\u00b7thol'\u00b7scher", "Christ"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Kindlein will zur Welt geb\u00e4hren,", "tokens": ["Sein", "Kin\u00b7dlein", "will", "zur", "Welt", "ge\u00b7b\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat vordersamst, wie es gebr\u00e4uchlich ist,", "tokens": ["Hat", "vor\u00b7der\u00b7samst", ",", "wie", "es", "ge\u00b7br\u00e4uch\u00b7lich", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nach einem guten Freund sich umzusehen,", "tokens": ["Nach", "ei\u00b7nem", "gu\u00b7ten", "Freund", "sich", "um\u00b7zu\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der bei der Taufe zu Gevatter ihm zu stehen", "tokens": ["Der", "bei", "der", "Tau\u00b7fe", "zu", "Ge\u00b7vat\u00b7ter", "ihm", "zu", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "NN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und f\u00fcr das Kindlein gut zu sagen sich entschlie\u00dft.", "tokens": ["Und", "f\u00fcr", "das", "Kin\u00b7dlein", "gut", "zu", "sa\u00b7gen", "sich", "ent\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "PTKZU", "VVINF", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich h\u00e4tte gern in meinen Kindesn\u00f6then", "tokens": ["Ich", "h\u00e4t\u00b7te", "gern", "in", "mei\u00b7nen", "Kin\u00b7des\u00b7n\u00f6\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Um dieses gute Werk, o Theure, dich gebeten,", "tokens": ["Um", "die\u00b7ses", "gu\u00b7te", "Werk", ",", "o", "Theu\u00b7re", ",", "dich", "ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "ADJA", "NN", "$,", "FM", "FM", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "W\u00fc\u00dft' ich nicht, da\u00df mein Kind so voller Schelmerei", "tokens": ["W\u00fc\u00dft'", "ich", "nicht", ",", "da\u00df", "mein", "Kind", "so", "vol\u00b7ler", "Schel\u00b7me\u00b7rei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und ein so ungezog'ner Junge sei,", "tokens": ["Und", "ein", "so", "un\u00b7ge\u00b7zo\u00b7g'\u00b7ner", "Jun\u00b7ge", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.12": {"text": "F\u00fcr den, nach christlichen Gebr\u00e4uchen,", "tokens": ["F\u00fcr", "den", ",", "nach", "christ\u00b7li\u00b7chen", "Ge\u00b7br\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.13": {"text": "Kein Mann, geschweige denn ein M\u00e4dchen deinesgleichen,", "tokens": ["Kein", "Mann", ",", "ge\u00b7schwei\u00b7ge", "denn", "ein", "M\u00e4d\u00b7chen", "dei\u00b7nes\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADJA", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mit Ehren sich verb\u00fcrgen kann.", "tokens": ["Mit", "Eh\u00b7ren", "sich", "ver\u00b7b\u00fcr\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Denn ach, gesetzt ich spreche dich d'rum an,", "tokens": ["Denn", "ach", ",", "ge\u00b7setzt", "ich", "spre\u00b7che", "dich", "d'\u00b7rum", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "VVPP", "PPER", "VVFIN", "PRF", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Wie k\u00f6nntest du zu all' den Schelmereien,", "tokens": ["Wie", "k\u00f6nn\u00b7test", "du", "zu", "all'", "den", "Schel\u00b7me\u00b7rei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Mit denen ich, sein Herr Papa,", "tokens": ["Mit", "de\u00b7nen", "ich", ",", "sein", "Herr", "Pa\u00b7pa", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Mir selbst zum Scandalum, so reichlich ihn versah,", "tokens": ["Mir", "selbst", "zum", "Scan\u00b7da\u00b7lum", ",", "so", "reich\u00b7lich", "ihn", "ver\u00b7sah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NE", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Vor aller Welt ihm deinen Namen leihen?", "tokens": ["Vor", "al\u00b7ler", "Welt", "ihm", "dei\u00b7nen", "Na\u00b7men", "lei\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Wie f\u00fcr den ausgelassenen", "tokens": ["Wie", "f\u00fcr", "den", "aus\u00b7ge\u00b7las\u00b7se\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.21": {"text": "Leichtfert'gen Buben B\u00fcrge stehen,", "tokens": ["Leicht\u00b7fert'\u00b7gen", "Bu\u00b7ben", "B\u00fcr\u00b7ge", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Der bis an's Ohr in lauter Schalkheit stecket,", "tokens": ["Der", "bis", "an's", "Ohr", "in", "lau\u00b7ter", "Schalk\u00b7heit", "ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "APPRART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Und schon so fr\u00fch die armen M\u00f6nche necket:", "tokens": ["Und", "schon", "so", "fr\u00fch", "die", "ar\u00b7men", "M\u00f6n\u00b7che", "ne\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Der \u00fcber Bilderchen und Amulette lacht,", "tokens": ["Der", "\u00fc\u00b7ber", "Bil\u00b7der\u00b7chen", "und", "A\u00b7mu\u00b7let\u00b7te", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und selbst dem Papst ein F\u00e4ustchen macht:", "tokens": ["Und", "selbst", "dem", "Papst", "ein", "F\u00e4ust\u00b7chen", "macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Dem, wenn er b\u00f6se wird, zu einem Nasenstieber", "tokens": ["Dem", ",", "wenn", "er", "b\u00f6\u00b7se", "wird", ",", "zu", "ei\u00b7nem", "Na\u00b7sen\u00b7stie\u00b7ber"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sankt Christoph selbst zu gro\u00df nicht ist,", "tokens": ["Sankt", "Chris\u00b7toph", "selbst", "zu", "gro\u00df", "nicht", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "PTKA", "ADJD", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Und der sich manchmal so vergi\u00dft,", "tokens": ["Und", "der", "sich", "manch\u00b7mal", "so", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Da\u00df er, obschon ein Kind, doch seine Amme lieber", "tokens": ["Da\u00df", "er", ",", "ob\u00b7schon", "ein", "Kind", ",", "doch", "sei\u00b7ne", "Am\u00b7me", "lie\u00b7ber"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUS", "ART", "NN", "$,", "ADV", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Als ein Marienbildchen k\u00fc\u00dft:", "tokens": ["Als", "ein", "Ma\u00b7ri\u00b7en\u00b7bild\u00b7chen", "k\u00fc\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.31": {"text": "Der, wenn er auf dem Steckenpferdchen reitet,", "tokens": ["Der", ",", "wenn", "er", "auf", "dem", "Ste\u00b7cken\u00b7pferd\u00b7chen", "rei\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Mit seiner Peitsche links und rechts", "tokens": ["Mit", "sei\u00b7ner", "Peit\u00b7sche", "links", "und", "rechts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Zuhaut, und ohne Schonung des Geschlechts", "tokens": ["Zu\u00b7haut", ",", "und", "oh\u00b7ne", "Scho\u00b7nung", "des", "Ge\u00b7schlechts"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Den Narr'n und N\u00e4rrinen auf ihre Schellen deutet;", "tokens": ["Den", "Narr'n", "und", "N\u00e4r\u00b7ri\u00b7nen", "auf", "ih\u00b7re", "Schel\u00b7len", "deu\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.35": {"text": "Der selbst mit deinen Schwesterchen", "tokens": ["Der", "selbst", "mit", "dei\u00b7nen", "Schwes\u00b7ter\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.36": {"text": "In Eva, sind sie auch so sch\u00f6n,", "tokens": ["In", "E\u00b7va", ",", "sind", "sie", "auch", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Und nicht so gut wie du, als wie mit B\u00e4uerinen,", "tokens": ["Und", "nicht", "so", "gut", "wie", "du", ",", "als", "wie", "mit", "B\u00e4u\u00b7e\u00b7ri\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADJD", "KOKOM", "PPER", "$,", "KOUS", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ja gar mit \u00fcberird'schen G\u00f6ttinen", "tokens": ["Ja", "gar", "mit", "\u00fc\u00b7ber\u00b7ird'\u00b7schen", "G\u00f6t\u00b7ti\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.39": {"text": "Als wie mit H\u00f6ckerweibern spricht?", "tokens": ["Als", "wie", "mit", "H\u00f6\u00b7cker\u00b7wei\u00b7bern", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Nein \u2013 sicher m\u00fc\u00dftest du dich sch\u00e4men,", "tokens": ["Nein", "\u2013", "si\u00b7cher", "m\u00fc\u00df\u00b7test", "du", "dich", "sch\u00e4\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "ADJD", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Den kleinen Wechselbalg im Angesicht", "tokens": ["Den", "klei\u00b7nen", "Wech\u00b7sel\u00b7balg", "im", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Der Welt in deinen Schutz zu nehmen.", "tokens": ["Der", "Welt", "in", "dei\u00b7nen", "Schutz", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Allein bei all' dem pr\u00e4tendirt", "tokens": ["Al\u00b7lein", "bei", "all'", "dem", "pr\u00e4\u00b7ten\u00b7dirt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Knabe doch, da\u00df er dir angeh\u00f6re,", "tokens": ["Der", "Kna\u00b7be", "doch", ",", "da\u00df", "er", "dir", "an\u00b7ge\u00b7h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ja, da\u00df er dir zum Theil sein Daseyn schuldig w\u00e4re,", "tokens": ["Ja", ",", "da\u00df", "er", "dir", "zum", "Theil", "sein", "Da\u00b7seyn", "schul\u00b7dig", "w\u00e4\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "PPER", "APPRART", "NN", "PPOSAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil du, als man ihn accouchirt,", "tokens": ["Weil", "du", ",", "als", "man", "ihn", "ac\u00b7cou\u00b7chirt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So g\u00fctig warst, mit eig'nen H\u00e4nden", "tokens": ["So", "g\u00fc\u00b7tig", "warst", ",", "mit", "eig'\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "(in allen Z\u00fcchten zwar) f\u00fcr ihn dich zu verwenden.", "tokens": ["(", "in", "al\u00b7len", "Z\u00fcch\u00b7ten", "zwar", ")", "f\u00fcr", "ihn", "dich", "zu", "ver\u00b7wen\u