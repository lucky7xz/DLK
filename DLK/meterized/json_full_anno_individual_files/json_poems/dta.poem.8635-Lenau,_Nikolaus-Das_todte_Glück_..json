{"dta.poem.8635": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Das todte Gl\u00fcck .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Leis' umrauscht von Himmelsquellen,", "tokens": ["Leis'", "um\u00b7rauscht", "von", "Him\u00b7mels\u00b7quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00dfe Sehnsucht in der Brust", "tokens": ["S\u00fc\u00b7\u00dfe", "Sehn\u00b7sucht", "in", "der", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sa\u00df ich einst die mondeshellen", "tokens": ["Sa\u00df", "ich", "einst", "die", "mon\u00b7des\u00b7hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "N\u00e4chte da in stiller Lust.", "tokens": ["N\u00e4ch\u00b7te", "da", "in", "stil\u00b7ler", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jene Zeit wird nimmer kommen,", "tokens": ["Je\u00b7ne", "Zeit", "wird", "nim\u00b7mer", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Himmelsquellen sind versiegt,", "tokens": ["Him\u00b7mels\u00b7quel\u00b7len", "sind", "ver\u00b7siegt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "All mein Sehnen ist verglommen,", "tokens": ["All", "mein", "Seh\u00b7nen", "ist", "ver\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mein Gl\u00fcck im Grabe liegt.", "tokens": ["Und", "mein", "Gl\u00fcck", "im", "Gra\u00b7be", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Weib, du riefst in b\u00f6ser Stunde", "tokens": ["Weib", ",", "du", "riefst", "in", "b\u00f6\u00b7ser", "Stun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem zauberischen Blick,", "tokens": ["Mit", "dem", "zau\u00b7be\u00b7ri\u00b7schen", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem wonnevollen Munde", "tokens": ["Mit", "dem", "won\u00b7ne\u00b7vol\u00b7len", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmeichelnd hin zu dir mein Gl\u00fcck.", "tokens": ["Schmei\u00b7chelnd", "hin", "zu", "dir", "mein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und es kam ein Kind und schmiegte,", "tokens": ["Und", "es", "kam", "ein", "Kind", "und", "schmieg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flehend sich in deinen Arm,", "tokens": ["Fle\u00b7hend", "sich", "in", "dei\u00b7nen", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der es mild umschlang und wiegte,", "tokens": ["Der", "es", "mild", "um\u00b7schlang", "und", "wieg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ein weicher Mutterarm.", "tokens": ["Als", "ein", "wei\u00b7cher", "Mut\u00b7ter\u00b7arm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun das Kind in Traumeswonnen", "tokens": ["Nun", "das", "Kind", "in", "Trau\u00b7mes\u00b7won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hingeschlummert, sich verlor;", "tokens": ["Hin\u00b7ge\u00b7schlum\u00b7mert", ",", "sich", "ver\u00b7lor", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nahmst du still und kaltbesonnen", "tokens": ["Nahmst", "du", "still", "und", "kalt\u00b7be\u00b7son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinen Todesdolch hervor.", "tokens": ["Dei\u00b7nen", "To\u00b7des\u00b7dolch", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Scharf geschliffen am Gesteine", "tokens": ["Scharf", "ge\u00b7schlif\u00b7fen", "am", "Ge\u00b7stei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines Herzens war der Stahl,", "tokens": ["Dei\u00b7nes", "Her\u00b7zens", "war", "der", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Kind, um das ich weine,", "tokens": ["Und", "das", "Kind", ",", "um", "das", "ich", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Athmete zum leztenmal.", "tokens": ["Ath\u00b7me\u00b7te", "zum", "lez\u00b7ten\u00b7mal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Und du stie\u00dfest leicht und munter", "tokens": ["Und", "du", "stie\u00b7\u00dfest", "leicht", "und", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein Steinchen in den Bach,", "tokens": ["Wie", "ein", "Stein\u00b7chen", "in", "den", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In das Grab mein Gl\u00fcck hinunter,", "tokens": ["In", "das", "Grab", "mein", "Gl\u00fcck", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sahst ihm ruhig, l\u00e4chelnd nach.", "tokens": ["Sahst", "ihm", "ru\u00b7hig", ",", "l\u00e4\u00b7chelnd", "nach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}