{"textgrid.poem.40770": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Spazierg\u00e4nge", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus der dumpfen Siechenstube nach den frischen gr\u00fcnen Hainen", "tokens": ["Aus", "der", "dum\u00b7pfen", "Sie\u00b7chen\u00b7stu\u00b7be", "nach", "den", "fri\u00b7schen", "gr\u00fc\u00b7nen", "Hai\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "L\u00e4\u00dft der Kranke gern sich leiten von den liebevollen Seinen,", "tokens": ["L\u00e4\u00dft", "der", "Kran\u00b7ke", "gern", "sich", "lei\u00b7ten", "von", "den", "lie\u00b7be\u00b7vol\u00b7len", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PRF", "VVINF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Da\u00df er dort ins Gras sich lagre, Kraft und neuen Glanz sein Auge", "tokens": ["Da\u00df", "er", "dort", "ins", "Gras", "sich", "la\u00b7gre", ",", "Kraft", "und", "neu\u00b7en", "Glanz", "sein", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "PRF", "VVFIN", "$,", "NN", "KON", "ADJA", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Seine Seele Muth und Hoffnung aus dem Gr\u00fcn der Wiesen sauge.", "tokens": ["Sei\u00b7ne", "See\u00b7le", "Muth", "und", "Hoff\u00b7nung", "aus", "dem", "Gr\u00fcn", "der", "Wie\u00b7sen", "sau\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Aus dem Finstern an die Sonne wird gef\u00fchrt der arme Blinde,", "tokens": ["Aus", "dem", "Fins\u00b7tern", "an", "die", "Son\u00b7ne", "wird", "ge\u00b7f\u00fchrt", "der", "ar\u00b7me", "Blin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ach, da\u00df nur ein Funke Lichtes Zugang in sein Dunkel finde!", "tokens": ["Ach", ",", "da\u00df", "nur", "ein", "Fun\u00b7ke", "Lich\u00b7tes", "Zu\u00b7gang", "in", "sein", "Dun\u00b7kel", "fin\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ADV", "ART", "NN", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Die versiegten Augenh\u00f6hlen gl\u00fchen dann gleich Flammenbronnen,", "tokens": ["Die", "ver\u00b7sieg\u00b7ten", "Au\u00b7gen\u00b7h\u00f6h\u00b7len", "gl\u00fc\u00b7hen", "dann", "gleich", "Flam\u00b7men\u00b7bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wie zwei runde Purpurrosen, wie zwei gro\u00dfe rothe Sonnen.", "tokens": ["Wie", "zwei", "run\u00b7de", "Pur\u00b7pur\u00b7ro\u00b7sen", ",", "wie", "zwei", "gro\u00b7\u00dfe", "ro\u00b7the", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "ADJA", "NN", "$,", "KOKOM", "CARD", "ADJA", "ADJA", "NN", "$."], "meter": "--+-+-+-+-+-+-+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Wenn der W\u00e4chter dem Gefangnen einen Festtag will bereiten,", "tokens": ["Wenn", "der", "W\u00e4ch\u00b7ter", "dem", "Ge\u00b7fang\u00b7nen", "ei\u00b7nen", "Fest\u00b7tag", "will", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Aus dem Kerker auf ein St\u00fcndchen l\u00e4\u00dft er an die Luft ihn schreiten,", "tokens": ["Aus", "dem", "Ker\u00b7ker", "auf", "ein", "St\u00fcnd\u00b7chen", "l\u00e4\u00dft", "er", "an", "die", "Luft", "ihn", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Da\u00df er seh', wie sie der Freiheit auf der Welt viel Raum noch g\u00f6nnen,", "tokens": ["Da\u00df", "er", "seh'", ",", "wie", "sie", "der", "Frei\u00b7heit", "auf", "der", "Welt", "viel", "Raum", "noch", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "APPR", "ART", "NN", "PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Da die Wolken frei noch segeln, frei die V\u00f6gel singen k\u00f6nnen!", "tokens": ["Da", "die", "Wol\u00b7ken", "frei", "noch", "se\u00b7geln", ",", "frei", "die", "V\u00f6\u00b7gel", "sin\u00b7gen", "k\u00f6n\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "ADV", "VVINF", "$,", "ADJD", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Also bin auch ich gestiegen auf der H\u00fcgel sonn'ge R\u00fccken,", "tokens": ["Al\u00b7so", "bin", "auch", "ich", "ge\u00b7stie\u00b7gen", "auf", "der", "H\u00fc\u00b7gel", "sonn'\u00b7ge", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wenn's wie Nacht der Blindheit unten dunkelte vor meinen Blicken,", "tokens": ["Wenn's", "wie", "Nacht", "der", "Blind\u00b7heit", "un\u00b7ten", "dun\u00b7kel\u00b7te", "vor", "mei\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NN", "ART", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Also sucht ich freie Bergluft, wenn ich Kerkerluft gewittert,", "tokens": ["Al\u00b7so", "sucht", "ich", "frei\u00b7e", "Berg\u00b7luft", ",", "wenn", "ich", "Ker\u00b7ker\u00b7luft", "ge\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,", "KOUS", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und das Gr\u00fcn, der Hoffnung Farbe, wenn mein Herz krank und zersplittert.", "tokens": ["Und", "das", "Gr\u00fcn", ",", "der", "Hoff\u00b7nung", "Far\u00b7be", ",", "wenn", "mein", "Herz", "krank", "und", "zer\u00b7split\u00b7tert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "NN", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "KON", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "In der Stadt, darin ich wohne, gibt's viel Kl\u00f6ster und Kasernen,", "tokens": ["In", "der", "Stadt", ",", "da\u00b7rin", "ich", "woh\u00b7ne", ",", "gibt's", "viel", "Kl\u00f6s\u00b7ter", "und", "Ka\u00b7ser\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PAV", "PPER", "VVFIN", "$,", "VVFIN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ries'ge Akten-Arsenale, Dome ragend zu den Sternen,", "tokens": ["Ries'\u00b7ge", "Ak\u00b7ten\u00b7Ar\u00b7se\u00b7na\u00b7le", ",", "Do\u00b7me", "ra\u00b7gend", "zu", "den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und dazwischen kleine M\u00e4nnlein, rufend im Triumphestone:", "tokens": ["Und", "da\u00b7zwi\u00b7schen", "klei\u00b7ne", "M\u00e4nn\u00b7lein", ",", "ru\u00b7fend", "im", "Tri\u00b7um\u00b7phes\u00b7to\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADJA", "NN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "----+-+-+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Seht, ", "tokens": ["Seht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "So geschieht's denn, da\u00df die Glocken br\u00fcllen allzugrell bisweilen,", "tokens": ["So", "ge\u00b7schieht's", "denn", ",", "da\u00df", "die", "Glo\u00b7cken", "br\u00fcl\u00b7len", "all\u00b7zu\u00b7grell", "bis\u00b7wei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df zu stark die Einen trommeln, und zu laut die Andern heulen,", "tokens": ["Da\u00df", "zu", "stark", "die", "Ei\u00b7nen", "trom\u00b7meln", ",", "und", "zu", "laut", "die", "An\u00b7dern", "heu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKA", "ADJD", "ART", "ART", "NN", "$,", "KON", "APPR", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Da\u00df der Dampf der Weihrauchf\u00e4sser allzudick die Luft verh\u00fclle;", "tokens": ["Da\u00df", "der", "Dampf", "der", "Weih\u00b7rauch\u00b7f\u00e4s\u00b7ser", "all\u00b7zu\u00b7dick", "die", "Luft", "ver\u00b7h\u00fcl\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "O dann such' ich auf den Bergen Licht und frische Luft und Stille.", "tokens": ["O", "dann", "such'", "ich", "auf", "den", "Ber\u00b7gen", "Licht", "und", "fri\u00b7sche", "Luft", "und", "Stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "So l\u00e4\u00dft Vieles leicht sich tragen, was zu Boden k\u00f6nnte pressen,", "tokens": ["So", "l\u00e4\u00dft", "Vie\u00b7les", "leicht", "sich", "tra\u00b7gen", ",", "was", "zu", "Bo\u00b7den", "k\u00f6nn\u00b7te", "pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "PRF", "VVINF", "$,", "PRELS", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "--+-+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wenn man nur f\u00fcr gute Sohlen nicht zu sorgen hat vergessen,", "tokens": ["Wenn", "man", "nur", "f\u00fcr", "gu\u00b7te", "Soh\u00b7len", "nicht", "zu", "sor\u00b7gen", "hat", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "ADJA", "NN", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wenn der Lenker der Gestirne mir des Herzens schlicht Begehren,", "tokens": ["Wenn", "der", "Len\u00b7ker", "der", "Ge\u00b7stir\u00b7ne", "mir", "des", "Her\u00b7zens", "schlicht", "Be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Nur das Wen'ge, d'rum ich flehe, wie bisher, noch will gew\u00e4hren:", "tokens": ["Nur", "das", "Wen'\u00b7ge", ",", "d'\u00b7rum", "ich", "fle\u00b7he", ",", "wie", "bis\u00b7her", ",", "noch", "will", "ge\u00b7w\u00e4h\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "PWAV", "ADV", "$,", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+----+-+-+-+-+-", "measure": "unknown.measure.septa"}}, "stanza.8": {"line.1": {"text": "Da\u00df er fest und aufrecht wandeln, nicht am Kr\u00fcckenstab mich humpeln,", "tokens": ["Da\u00df", "er", "fest", "und", "auf\u00b7recht", "wan\u00b7deln", ",", "nicht", "am", "Kr\u00fc\u00b7cken\u00b7stab", "mich", "hum\u00b7peln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "KON", "ADJD", "VVFIN", "$,", "PTKNEG", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.2": {"text": "Da\u00df er nicht die sch\u00f6nen Berge \u00fcbern Haufen lasse rumpeln,", "tokens": ["Da\u00df", "er", "nicht", "die", "sch\u00f6\u00b7nen", "Ber\u00b7ge", "\u00fc\u00b7bern", "Hau\u00b7fen", "las\u00b7se", "rum\u00b7peln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.3": {"text": "Da\u00df er seines Sch\u00f6pferodems einen Hauch fortan mir borge,", "tokens": ["Da\u00df", "er", "sei\u00b7nes", "Sch\u00f6p\u00b7fe\u00b7ro\u00b7dems", "ei\u00b7nen", "Hauch", "for\u00b7tan", "mir", "bor\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ART", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und ein bischen frische Bergluft, Sonnenschein und Gr\u00fcn besorge.", "tokens": ["Und", "ein", "bi\u00b7schen", "fri\u00b7sche", "Berg\u00b7luft", ",", "Son\u00b7nen\u00b7schein", "und", "Gr\u00fcn", "be\u00b7sor\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJA", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.9": {"line.1": {"text": "Aus der dumpfen Siechenstube nach den frischen gr\u00fcnen Hainen", "tokens": ["Aus", "der", "dum\u00b7pfen", "Sie\u00b7chen\u00b7stu\u00b7be", "nach", "den", "fri\u00b7schen", "gr\u00fc\u00b7nen", "Hai\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "L\u00e4\u00dft der Kranke gern sich leiten von den liebevollen Seinen,", "tokens": ["L\u00e4\u00dft", "der", "Kran\u00b7ke", "gern", "sich", "lei\u00b7ten", "von", "den", "lie\u00b7be\u00b7vol\u00b7len", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PRF", "VVINF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Da\u00df er dort ins Gras sich lagre, Kraft und neuen Glanz sein Auge", "tokens": ["Da\u00df", "er", "dort", "ins", "Gras", "sich", "la\u00b7gre", ",", "Kraft", "und", "neu\u00b7en", "Glanz", "sein", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "PRF", "VVFIN", "$,", "NN", "KON", "ADJA", "NN", 