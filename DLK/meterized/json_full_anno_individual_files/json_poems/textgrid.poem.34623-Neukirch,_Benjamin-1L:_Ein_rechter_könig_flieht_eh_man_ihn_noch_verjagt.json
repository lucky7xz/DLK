{"textgrid.poem.34623": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein rechter k\u00f6nig flieht/ eh man ihn noch verjagt/", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein rechter k\u00f6nig flieht/ eh man ihn noch verjagt/", "tokens": ["Ein", "rech\u00b7ter", "k\u00f6\u00b7nig", "flieht", "/", "eh", "man", "ihn", "noch", "ver\u00b7jagt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "KOUS", "PIS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein eidam hilfft ihm fort/ den er doch stets geplagt/", "tokens": ["Sein", "ei\u00b7dam", "hilfft", "ihm", "fort", "/", "den", "er", "doch", "stets", "ge\u00b7plagt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$(", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Armee und Flotte f\u00e4llt/ bevor sie wird geschlagen;", "tokens": ["Ar\u00b7mee", "und", "Flot\u00b7te", "f\u00e4llt", "/", "be\u00b7vor", "sie", "wird", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$(", "KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der nie das reich gesucht/ dem wird es angetragen.", "tokens": ["Der", "nie", "das", "reich", "ge\u00b7sucht", "/", "dem", "wird", "es", "an\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJD", "VVPP", "$(", "PDS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und dennoch fehlet es die rechten erben nicht.", "tokens": ["Und", "den\u00b7noch", "feh\u00b7let", "es", "die", "rech\u00b7ten", "er\u00b7ben", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die kirche steht erl\u00f6st/ die freiheit auffgericht:", "tokens": ["Die", "kir\u00b7che", "steht", "er\u00b7l\u00f6st", "/", "die", "frei\u00b7heit", "auff\u00b7ge\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$(", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und was man in Paris durch so viel hundert wochen", "tokens": ["Und", "was", "man", "in", "Pa\u00b7ris", "durch", "so", "viel", "hun\u00b7dert", "wo\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PIS", "APPR", "NE", "APPR", "ADV", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit klugheit \u00fcberlegt/ ist nun wie gla\u00df zerbrochen.", "tokens": ["Mit", "klug\u00b7heit", "\u00fc\u00b7ber\u00b7legt", "/", "ist", "nun", "wie", "gla\u00df", "zer\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$(", "VAFIN", "ADV", "KOKOM", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein leser dencke nicht/ aus furcht der prahlerey/", "tokens": ["Me