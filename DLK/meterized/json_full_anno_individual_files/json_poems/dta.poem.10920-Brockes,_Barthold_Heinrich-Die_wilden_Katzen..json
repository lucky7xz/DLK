{"dta.poem.10920": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die wilden Katzen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seht! wie in des wilden Waldes dunkel - gr\u00fcner Schatten-\nNacht,", "tokens": ["Seht", "!", "wie", "in", "des", "wil\u00b7den", "Wal\u00b7des", "dun\u00b7kel", "gr\u00fc\u00b7ner", "Schat\u00b7ten", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PWAV", "APPR", "ART", "ADJA", "NN", "ADJD", "$(", "ADJA", "TRUNC", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Auf dem d\u00fcrr - und morschen Baum halb ergrimmt, halb l\u00e4-", "tokens": ["Auf", "dem", "d\u00fcrr", "und", "mor\u00b7schen", "Baum", "halb", "er\u00b7grimmt", ",", "halb", "l\u00e4"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NE", "$(", "KON", "ADJA", "NN", "ADJD", "VVPP", "$,", "ADJD", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dort ein bunter wilder Kater einen Katzenbuckel macht!", "tokens": ["Dort", "ein", "bun\u00b7ter", "wil\u00b7der", "Ka\u00b7ter", "ei\u00b7nen", "Kat\u00b7zen\u00b7bu\u00b7ckel", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Seht, wie mit entflammten Augen hier ein andrer \u00fcber sich,", "tokens": ["Seht", ",", "wie", "mit", "ent\u00b7flamm\u00b7ten", "Au\u00b7gen", "hier", "ein", "an\u00b7drer", "\u00fc\u00b7ber", "sich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "APPR", "ADJA", "NN", "ADV", "ART", "ADJA", "APPR", "PRF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Schon im Sprung begriffen, sieht! seht, wie m\u00fctterlich die Katze", "tokens": ["Schon", "im", "Sprung", "be\u00b7grif\u00b7fen", ",", "sieht", "!", "seht", ",", "wie", "m\u00fct\u00b7ter\u00b7lich", "die", "Kat\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$,", "VVFIN", "$.", "VVFIN", "$,", "PWAV", "ADJD", "ART", "NN"], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.6": {"text": "An den n\u00e4rrischen Figuren ihrer Jungen sich vergn\u00fcgt,", "tokens": ["An", "den", "n\u00e4r\u00b7ri\u00b7schen", "Fi\u00b7gu\u00b7ren", "ih\u00b7rer", "Jun\u00b7gen", "sich", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Und, indem sich eines welzt, eines mit gestreckter Tatze,", "tokens": ["Und", ",", "in\u00b7dem", "sich", "ei\u00b7nes", "welzt", ",", "ei\u00b7nes", "mit", "ge\u00b7streck\u00b7ter", "Tat\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PRF", "ART", "ADJD", "$,", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Springt, und sich im Sprunge dreht, eine sich po\u00dfirlich biegt,", "tokens": ["Springt", ",", "und", "sich", "im", "Sprun\u00b7ge", "dreht", ",", "ei\u00b7ne", "sich", "po\u00df\u00b7ir\u00b7lich", "biegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "PRF", "APPRART", "NN", "VVFIN", "$,", "ART", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.9": {"text": "Und den regen R\u00fccken kr\u00fcmmt, innig sich zu freuen scheint!", "tokens": ["Und", "den", "re\u00b7gen", "R\u00fc\u00b7cken", "kr\u00fcmmt", ",", "in\u00b7nig", "sich", "zu", "freu\u00b7en", "scheint", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,", "ADJD", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Ist die\u00df Thier nun gleich sehr sch\u00e4dlich, n\u00fctzt es mehr doch,", "tokens": ["Ist", "die\u00df", "Thier", "nun", "gleich", "sehr", "sch\u00e4d\u00b7lich", ",", "n\u00fctzt", "es", "mehr", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "NN", "ADV", "ADV", "ADV", "ADJD", "$,", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.11": {"text": "(welches wir erkennen sollten) denn, so bald sie aufgerieben,", "tokens": ["(", "wel\u00b7ches", "wir", "er\u00b7ken\u00b7nen", "soll\u00b7ten", ")", "denn", ",", "so", "bald", "sie", "auf\u00b7ge\u00b7rie\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "VVINF", "VMFIN", "$(", "KON", "$,", "ADV", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Wird, mit ihren bunten B\u00e4lgen, gro\u00dfe Handelschaft getrieben.", "tokens": ["Wird", ",", "mit", "ih\u00b7ren", "bun\u00b7ten", "B\u00e4l\u00b7gen", ",", "gro\u00b7\u00dfe", "Han\u00b7del\u00b7schaft", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Da man sie, nicht nur im Winter gegen strenge K\u00e4lte tr\u00e4gt;", "tokens": ["Da", "man", "sie", ",", "nicht", "nur", "im", "Win\u00b7ter", "ge\u00b7gen", "stren\u00b7ge", "K\u00e4l\u00b7te", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "$,", "PTKNEG", "ADV", "APPRART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.14": {"text": "Sondern er auch gegen Fl\u00fcsse wunderbare Kr\u00e4fte hegt.", "tokens": ["Son\u00b7dern", "er", "auch", "ge\u00b7gen", "Fl\u00fcs\u00b7se", "wun\u00b7der\u00b7ba\u00b7re", "Kr\u00e4f\u00b7te", "hegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}