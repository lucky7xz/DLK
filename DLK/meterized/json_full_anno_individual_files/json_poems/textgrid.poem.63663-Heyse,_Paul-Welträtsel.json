{"textgrid.poem.63663": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Weltr\u00e4tsel", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manchmal, wenn j\u00e4h dein eigen Angesicht", "tokens": ["Manch\u00b7mal", ",", "wenn", "j\u00e4h", "dein", "ei\u00b7gen", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.2": {"text": "Aus klarer Spiegelfl\u00e4che zu dir spricht,", "tokens": ["Aus", "kla\u00b7rer", "Spie\u00b7gel\u00b7fl\u00e4\u00b7che", "zu", "dir", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "D\u00fcnkt dir's, du s\u00e4hst, was dir so wohlbekannt,", "tokens": ["D\u00fcnkt", "dir's", ",", "du", "s\u00e4hst", ",", "was", "dir", "so", "wohl\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PPER", "VVFIN", "$,", "PWS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In dunkle Hieroglyphen umgewandt.", "tokens": ["In", "dunk\u00b7le", "Hie\u00b7ro\u00b7gly\u00b7phen", "um\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du fragst dich, wem dies fremde Bildnis gleicht,", "tokens": ["Du", "fragst", "dich", ",", "wem", "dies", "frem\u00b7de", "Bild\u00b7nis", "gleicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWS", "PDS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bis vor dir selbst ein Graun dich \u00fcberschleicht", "tokens": ["Bis", "vor", "dir", "selbst", "ein", "Graun", "dich", "\u00fc\u00b7bersc\u00b7hleicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "PPER", "ADV", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und das Geheimnis deiner Einzigkeit", "tokens": ["Und", "das", "Ge\u00b7heim\u00b7nis", "dei\u00b7ner", "Ein\u00b7zig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit deinem dumpfen Frieden dich entzweit.", "tokens": ["Mit", "dei\u00b7nem", "dum\u00b7pfen", "Frie\u00b7den", "dich", "ent\u00b7zweit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wieder: siehst du einen Baum, ein Laub,", "tokens": ["Und", "wie\u00b7der", ":", "siehst", "du", "ei\u00b7nen", "Baum", ",", "ein", "Laub", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Sandkorn, einen bunten Sonnenstaub,", "tokens": ["Ein", "Sand\u00b7korn", ",", "ei\u00b7nen", "bun\u00b7ten", "Son\u00b7nen\u00b7staub", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ergreift dich's p