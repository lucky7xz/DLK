{"textgrid.poem.47042": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will den Sonnstrahl mit der Hand zerbrechen,", "tokens": ["Ich", "will", "den", "Sonn\u00b7strahl", "mit", "der", "Hand", "zer\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich will den Lufthauch bei dem Fittich fangen,", "tokens": ["Ich", "will", "den", "Luft\u00b7hauch", "bei", "dem", "Fit\u00b7tich", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Eh' dieser kalt dir r\u00fchren soll die Wangen,", "tokens": ["Eh'", "die\u00b7ser", "kalt", "dir", "r\u00fch\u00b7ren", "soll", "die", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "PPER", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Eh' jener hei\u00df die Stirne dir soll stechen.", "tokens": ["Eh'", "je\u00b7ner", "hei\u00df", "die", "Stir\u00b7ne", "dir", "soll", "ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJD", "ART", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die V\u00f6gel will ich zauberisch besprechen,", "tokens": ["Die", "V\u00f6\u00b7gel", "will", "ich",