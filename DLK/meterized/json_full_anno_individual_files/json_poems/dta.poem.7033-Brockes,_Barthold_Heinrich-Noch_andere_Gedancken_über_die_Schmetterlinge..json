{"dta.poem.7033": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Noch andere Gedancken \u00fcber  \n die Schmetterlinge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dar\u00fcber hab ich offt gefunden,", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "hab", "ich", "offt", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df es nicht anders lie\u00df,", "tokens": ["Da\u00df", "es", "nicht", "an\u00b7ders", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als wenn auf Bluhmen Bluhmen stunden;", "tokens": ["Als", "wenn", "auf", "Bluh\u00b7men", "Bluh\u00b7men", "stun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wodurch sich dann der Schmuck der Farben doppelt wie\u00df.", "tokens": ["Wo\u00b7durch", "sich", "dann", "der", "Schmuck", "der", "Far\u00b7ben", "dop\u00b7pelt", "wie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADV", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Offt \u00fcberwand die Bluhm\u2019, offt ward sie \u00fcberwunden.", "tokens": ["Offt", "\u00fc\u00b7ber\u00b7wand", "die", "Bluhm'", ",", "offt", "ward", "sie", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Indem ich dieses Farben Spiel,", "tokens": ["In\u00b7dem", "ich", "die\u00b7ses", "Far\u00b7ben", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das mir so wohl gefiel,", "tokens": ["Das", "mir", "so", "wohl", "ge\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Nebst meinem dritten Sohn, dem muntern ", "tokens": ["Nebst", "mei\u00b7nem", "drit\u00b7ten", "Sohn", ",", "dem", "mun\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und er dem einen ziemlich nah,", "tokens": ["Und", "er", "dem", "ei\u00b7nen", "ziem\u00b7lich", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ART", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Um selben zu erhaschen, ging,", "tokens": ["Um", "sel\u00b7ben", "zu", "er\u00b7ha\u00b7schen", ",", "ging", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "ADV", "PTKZU", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "H\u00f6rt ich, wie er schnell an zu ruffen fing:", "tokens": ["H\u00f6rt", "ich", ",", "wie", "er", "schnell", "an", "zu", "ruf\u00b7fen", "fing", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "PPER", "ADJD", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Papa! Papa!", "tokens": ["Pa\u00b7pa", "!", "Pa\u00b7pa", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Was hat doch dieser hier f\u00fcr eine lange Nase!", "tokens": ["Was", "hat", "doch", "die\u00b7ser", "hier", "f\u00fcr", "ei\u00b7ne", "lan\u00b7ge", "Na\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PDAT", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.14": {"text": "Ich lachte, f\u00fcgte mich zu ihm, und sahe klar,", "tokens": ["Ich", "lach\u00b7te", ",", "f\u00fcg\u00b7te", "mich", "zu", "ihm", ",", "und", "sa\u00b7he", "klar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$,", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da\u00df es des Thierchens Zunge war,", "tokens": ["Da\u00df", "es", "des", "Thier\u00b7chens", "Zun\u00b7ge", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ich fing es, setzte mich im Grase,", "tokens": ["Ich", "fing", "es", ",", "setz\u00b7te", "mich", "im", "Gra\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Um dieses Werckzeug zu betrachten.", "tokens": ["Um", "die\u00b7ses", "Wer\u00b7ck\u00b7zeug", "zu", "be\u00b7trach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Allein, indem ich sie kaum wieder funde,", "tokens": ["Al\u00b7lein", ",", "in\u00b7dem", "ich", "sie", "kaum", "wie\u00b7der", "fun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Weil sie in einem Kreis wie aufgewickelt stunde,", "tokens": ["Weil", "sie", "in", "ei\u00b7nem", "Kreis", "wie", "auf\u00b7ge\u00b7wi\u00b7ckelt", "stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "KOKOM", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Stund ich von neuen auf, zu sehn, wie sie es machten,", "tokens": ["Stund", "ich", "von", "neu\u00b7en", "auf", ",", "zu", "sehn", ",", "wie", "sie", "es", "mach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "PTKVZ", "$,", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wenn sie in Freyheit sich desselbigen bedienen.", "tokens": ["Wenn", "sie", "in", "Frey\u00b7heit", "sich", "des\u00b7sel\u00b7bi\u00b7gen", "be\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PRF", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ich lie\u00df den meinen wieder fliegen,", "tokens": ["Ich", "lie\u00df", "den", "mei\u00b7nen", "wie\u00b7der", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PPOSAT", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und sah\u2019 an andern mit Vergn\u00fcgen,", "tokens": ["Und", "sah'", "an", "an\u00b7dern", "mit", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Wie wunderbar von ihnen", "tokens": ["Wie", "wun\u00b7der\u00b7bar", "von", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Die\u00df kleine Werckzeug wird gebrauchet und bewegt.", "tokens": ["Die\u00df", "klei\u00b7ne", "Wer\u00b7ck\u00b7zeug", "wird", "ge\u00b7brau\u00b7chet", "und", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VAFIN", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Verwunderlich wird es geregt;", "tokens": ["Ver\u00b7wun\u00b7der\u00b7lich", "wird", "es", "ge\u00b7regt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.27": {"text": "Sie k\u00f6nnen sie tief in die Bluhmen sencken,", "tokens": ["Sie", "k\u00f6n\u00b7nen", "sie", "tief", "in", "die", "Bluh\u00b7men", "sen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.28": {"text": "Sie wissen sie auf tausend Art zu lencken,", "tokens": ["Sie", "wis\u00b7sen", "sie", "auf", "tau\u00b7send", "Art", "zu", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "CARD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Sie scheinen nicht allein damit zu spielen,", "tokens": ["Sie", "schei\u00b7nen", "nicht", "al\u00b7lein", "da\u00b7mit", "zu", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Sie scheinen recht, als wie mit einer Hand,", "tokens": ["Sie", "schei\u00b7nen", "recht", ",", "als", "wie", "mit", "ei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KOUS", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Damit zu tasten und zu f\u00fchlen.", "tokens": ["Da\u00b7mit", "zu", "tas\u00b7ten", "und", "zu", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}