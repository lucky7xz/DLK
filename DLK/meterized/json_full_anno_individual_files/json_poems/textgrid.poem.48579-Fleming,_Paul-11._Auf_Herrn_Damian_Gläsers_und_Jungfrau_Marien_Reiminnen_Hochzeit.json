{"textgrid.poem.48579": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "11. Auf Herrn Damian Gl\u00e4sers und Jungfrau Marien Reiminnen Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Will denn itzo nicht verg\u00fcnnen", "tokens": ["Will", "denn", "it\u00b7zo", "nicht", "ver\u00b7g\u00fcn\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "meine zarte Poesie", "tokens": ["mei\u00b7ne", "zar\u00b7te", "Poe\u00b7sie"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "eine Freude meinen Sinnen,", "tokens": ["ei\u00b7ne", "Freu\u00b7de", "mei\u00b7nen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die sie vor versaget nie?", "tokens": ["die", "sie", "vor", "ver\u00b7sa\u00b7get", "nie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "APPR", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schl\u00e4gt denn mir mein Ph\u00f6bus abe", "tokens": ["Schl\u00e4gt", "denn", "mir", "mein", "Ph\u00f6\u00b7bus", "a\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "eines schlechten Liedes Gabe?", "tokens": ["ei\u00b7nes", "schlech\u00b7ten", "Lie\u00b7des", "Ga\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der ich itzt vor andern Allen", "tokens": ["Der", "ich", "itzt", "vor", "an\u00b7dern", "Al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "solte mitte fr\u00f6lich sein", "tokens": ["sol\u00b7te", "mit\u00b7te", "fr\u00f6\u00b7lich", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADJD", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und den G\u00e4sten zu Gefallen", "tokens": ["und", "den", "G\u00e4s\u00b7ten", "zu", "Ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "etwas Lustigs stimmen ein,", "tokens": ["et\u00b7was", "Lus\u00b7tigs", "stim\u00b7men", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "bin, als w\u00e4r' ich selbst nicht meine,", "tokens": ["bin", ",", "als", "w\u00e4r'", "ich", "selbst", "nicht", "mei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "mu\u00df hier sitzen so alleine.", "tokens": ["mu\u00df", "hier", "sit\u00b7zen", "so", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Klio, Klio, la\u00df erbitten,", "tokens": ["Klio", ",", "Klio", ",", "la\u00df", "er\u00b7bit\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "VVIMP", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "la\u00df erbitten dich doch itzt,", "tokens": ["la\u00df", "er\u00b7bit\u00b7ten", "dich", "doch", "itzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "zeige dich einmal in G\u00fctten,", "tokens": ["zei\u00b7ge", "dich", "ein\u00b7mal", "in", "G\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "mache mir den Kopf erhitzt,", "tokens": ["ma\u00b7che", "mir", "den", "Kopf", "er\u00b7hitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df ich nur auf Lustgedichte", "tokens": ["da\u00df", "ich", "nur", "auf", "Lust\u00b7ge\u00b7dich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "meine Faust und Feder richte!", "tokens": ["mei\u00b7ne", "Faust", "und", "Fe\u00b7der", "rich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nein, es will mir nicht von Statten,", "tokens": ["Nein", ",", "es", "will", "mir", "nicht", "von", "Stat\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "nicht nach meinem Wundsche gehn.", "tokens": ["nicht", "nach", "mei\u00b7nem", "Wund\u00b7sche", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich mu\u00df hier im tiefen Schatten,", "tokens": ["Ich", "mu\u00df", "hier", "im", "tie\u00b7fen", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "weit von Pindus Glanze stehn.", "tokens": ["weit", "von", "Pin\u00b7dus", "Glan\u00b7ze", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nichts kan ich itzt lassen h\u00f6ren,", "tokens": ["Nichts", "kan", "ich", "itzt", "las\u00b7sen", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "was die bunten Jungfern ehren.", "tokens": ["was", "die", "bun\u00b7ten", "Jung\u00b7fern", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mir zwar hat das Gl\u00fcck' erwehret,", "tokens": ["Mir", "zwar", "hat", "das", "Gl\u00fcck", "er\u00b7weh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "werter Br\u00e4utgam, werte Braut,", "tokens": ["wer\u00b7ter", "Br\u00e4ut\u00b7gam", ",", "wer\u00b7te", "Braut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df ich nicht zu euch gekehret", "tokens": ["da\u00df", "ich", "nicht", "zu", "euch", "ge\u00b7keh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und die Freuden angeschaut;", "tokens": ["und", "die", "Freu\u00b7den", "an\u00b7ge\u00b7schaut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "eure s\u00fc\u00dfe Hochzeitfreuden", "tokens": ["eu\u00b7re", "s\u00fc\u00b7\u00dfe", "Hoch\u00b7zeit\u00b7freu\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "mu\u00df ich durch das Absein meiden,", "tokens": ["mu\u00df", "ich", "durch", "das", "Ab\u00b7sein", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Gl\u00fccke, wehre! Doch dein Wehren", "tokens": ["Gl\u00fc\u00b7cke", ",", "weh\u00b7re", "!", "Doch", "dein", "Weh\u00b7ren"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$.", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "kan mir Alles wehren nicht.", "tokens": ["kan", "mir", "Al\u00b7les", "weh\u00b7ren", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Di\u00df mein Hochzeitwundschverehren", "tokens": ["Di\u00df", "mein", "Hoch\u00b7zeit\u00b7wund\u00b7schver\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "alle Gl\u00fcckes Mi\u00dfgunst bricht.", "tokens": ["al\u00b7le", "Gl\u00fc\u00b7ckes", "Mi\u00df\u00b7gunst", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses Lied, das soll den Willen", "tokens": ["Die\u00b7ses", "Lied", ",", "das", "soll", "den", "Wil\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PDS", "VMFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "meiner Gegenwart erf\u00fcllen.", "tokens": ["mei\u00b7ner", "Ge\u00b7gen\u00b7wart", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nehmt es an, gepaarte Herzen!", "tokens": ["Nehmt", "es", "an", ",", "ge\u00b7paar\u00b7te", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nehmt es als mich selbsten an,", "tokens": ["Nehmt", "es", "als", "mich", "selbs\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "der ich eure Liebeskerzen", "tokens": ["der", "ich", "eu\u00b7re", "Lie\u00b7bes\u00b7ker\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "besser nicht beschenken kan!", "tokens": ["bes\u00b7ser", "nicht", "be\u00b7schen\u00b7ken", "kan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der Gesang soll von mir zeugen,", "tokens": ["Der", "Ge\u00b7sang", "soll", "von", "mir", "zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df ich euer bin zu eigen.", "tokens": ["da\u00df", "ich", "eu\u00b7er", "bin", "zu", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "VAFIN", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Braut, gedenket unterdessen,", "tokens": ["Braut", ",", "ge\u00b7den\u00b7ket", "un\u00b7ter\u00b7des\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df an euch was ", "tokens": ["da\u00df", "an", "euch", "was"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "PWS"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Br\u00e4utgam, tut auch nicht vergessen,", "tokens": ["Br\u00e4ut\u00b7gam", ",", "tut", "auch", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "was ihr nun fort reimen m\u00fc\u00dft!", "tokens": ["was", "ihr", "nun", "fort", "rei\u00b7men", "m\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PTKVZ", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ihr m\u00f6gt nach kurzen Tagen", "tokens": ["Da\u00df", "ihr", "m\u00f6gt", "nach", "kur\u00b7zen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "neue ", "tokens": ["neu\u00b7e"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.9": {"line.1": {"text": "Will denn itzo nicht verg\u00fcnnen", "tokens": ["Will", "denn", "it\u00b7zo", "nicht", "ver\u00b7g\u00fcn\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "meine zarte Poesie", "tokens": ["mei\u00b7ne", "zar\u00b7te", "Poe\u00b7sie"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "eine Freude meinen Sinnen,", "tokens": ["ei\u00b7ne", "Freu\u00b7de", "mei\u00b7nen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die sie vor versaget nie?", "tokens": ["die", "sie", "vor", "ver\u00b7sa\u00b7get", "nie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "APPR", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schl\u00e4gt denn mir mein Ph\u00f6bus abe", "tokens": ["Schl\u00e4gt", "denn", "mir", "mein", "Ph\u00f6\u00b7bus", "a\u00b7be"], "tok