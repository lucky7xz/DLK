{"dta.poem.7021": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein ausb\u00fcndig-sch\u00f6ner, in der  \n Bl\u00fchte stehender, Castanien-Baum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein neues zierliches Geb\u00e4ude,", "tokens": ["Ein", "neu\u00b7es", "zier\u00b7li\u00b7ches", "Ge\u00b7b\u00e4u\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So unweit hinter diesen Baum,", "tokens": ["So", "un\u00b7weit", "hin\u00b7ter", "die\u00b7sen", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vermehrt, im Unterscheid der Farben, unsre Freude.", "tokens": ["Ver\u00b7mehrt", ",", "im", "Un\u00b7ter\u00b7scheid", "der", "Far\u00b7ben", ",", "uns\u00b7re", "Freu\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPRART", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indem man es nie sonder Anmuth sieht,", "tokens": ["In\u00b7dem", "man", "es", "nie", "son\u00b7der", "An\u00b7muth", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn das bestrahlte Dach in heller R\u00f6the gl\u00fcht,", "tokens": ["Wenn", "das", "be\u00b7strahl\u00b7te", "Dach", "in", "hel\u00b7ler", "R\u00f6\u00b7the", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und hier von ungefehr ein kleiner Zweig sich beuget,", "tokens": ["Und", "hier", "von", "un\u00b7ge\u00b7fehr", "ein", "klei\u00b7ner", "Zweig", "sich", "beu\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJD", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da in den Bl\u00e4ttern sich sodann ein\u2019 Oeffnung zeiget;", "tokens": ["Da", "in", "den", "Bl\u00e4t\u00b7tern", "sich", "so\u00b7dann", "ein'", "Oeff\u00b7nung", "zei\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PRF", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So l\u00e4sst es fast, als wenn Aurorens Rosen-Licht", "tokens": ["So", "l\u00e4sst", "es", "fast", ",", "als", "wenn", "Au\u00b7ro\u00b7rens", "Ro\u00b7sen\u00b7Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOKOM", "KOUS", "NE", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Durch eine gr\u00fcne D\u00e4mmrung bricht.", "tokens": ["Durch", "ei\u00b7ne", "gr\u00fc\u00b7ne", "D\u00e4mm\u00b7rung", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Es scheint, als wenn es sich mit ihrem Schatten mische;", "tokens": ["Es", "scheint", ",", "als", "wenn", "es", "sich", "mit", "ih\u00b7rem", "Schat\u00b7ten", "mi\u00b7sche", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie, oder auch, als wenn durch dunckle B\u00fcsche", "tokens": ["Wie", ",", "o\u00b7der", "auch", ",", "als", "wenn", "durch", "dunck\u00b7le", "B\u00fc\u00b7sche"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "KON", "ADV", "$,", "KOKOM", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ein feurig Abend-Roth vom heitern Himmel strahlte,", "tokens": ["Ein", "feu\u00b7rig", "A\u00b7ben\u00b7dRoth", "vom", "hei\u00b7tern", "Him\u00b7mel", "strahl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und ihre Bl\u00e4tter, mehr verg\u00fcldet\u2019, als bemahlte.", "tokens": ["Und", "ih\u00b7re", "Bl\u00e4t\u00b7ter", ",", "mehr", "ver\u00b7g\u00fcl\u00b7det'", ",", "als", "be\u00b7mahl\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "$,", "KOUS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}