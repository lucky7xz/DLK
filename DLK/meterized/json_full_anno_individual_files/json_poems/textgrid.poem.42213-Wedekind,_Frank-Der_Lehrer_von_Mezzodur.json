", "sie", "ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sich zu legen auf das Bett,", "tokens": ["Sich", "zu", "le\u00b7gen", "auf", "das", "Bett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat sie dann auch umgebrungen,", "tokens": ["Hat", "sie", "dann", "auch", "um\u00b7ge\u00b7brun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sie ihn auch angefleht.", "tokens": ["Wie", "sie", "ihn", "auch", "an\u00b7ge\u00b7fleht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Er legt' nun selber Hand an sich", "tokens": ["Er", "legt'", "nun", "sel\u00b7ber", "Hand", "an", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "NN", "APPR", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und endete dann f\u00fcrchterlich.", "tokens": ["Und", "en\u00b7de\u00b7te", "dann", "f\u00fcrch\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Dienstm\u00e4dchen, das zugegen war,", "tokens": ["Das", "Dienst\u00b7m\u00e4d\u00b7chen", ",", "das", "zu\u00b7ge\u00b7gen", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PREL