{"dta.poem.7702": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu lesen lieb' ich nicht, was aneinander h\u00e4ngt", "tokens": ["Zu", "le\u00b7sen", "lieb'", "ich", "nicht", ",", "was", "an\u00b7ein\u00b7an\u00b7der", "h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So da\u00df ein jeder Schritt zum andern vorwerts dr\u00e4ngt;", "tokens": ["So", "da\u00df", "ein", "je\u00b7der", "Schritt", "zum", "an\u00b7dern", "vor\u00b7werts", "dr\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "PIAT", "NN", "APPRART", "ADJA", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo, wenn ich aus der Bahn hab' einen Schritt gethan,", "tokens": ["Wo", ",", "wenn", "ich", "aus", "der", "Bahn", "hab'", "ei\u00b7nen", "Schritt", "ge\u00b7than", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich sie verlor, und mu\u00df von vorne fangen an.", "tokens": ["Ich", "sie", "ver\u00b7lor", ",", "und", "mu\u00df", "von", "vor\u00b7ne", "fan\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "$,", "KON", "VMFIN", "APPR", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zu lesen lieb' ich das, wo ich auf jedem Schritte", "tokens": ["Zu", "le\u00b7sen", "lieb'", "ich", "das", ",", "wo", "ich", "auf", "je\u00b7dem", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VVFIN", "PPER", "PDS", "$,", "PWAV", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zugleich am Anfang bin, am End' und in der Mitte;", "tokens": ["Zu\u00b7gleich", "am", "An\u00b7fang", "bin", ",", "am", "End'", "und", "in", "der", "Mit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "$,", "APPRART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wo stillzustehen, fortzufahren, abzubrechen", "tokens": ["Wo", "still\u00b7zu\u00b7ste\u00b7hen", ",", "fort\u00b7zu\u00b7fah\u00b7ren", ",", "ab\u00b7zu\u00b7bre\u00b7chen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PWAV", "VVFIN", "$,", "VVIZU", "$,", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In meiner Willk\u00fcr steht, und mit darein zu sprechen.", "tokens": ["In", "mei\u00b7ner", "Will\u00b7k\u00fcr", "steht", ",", "und", "mit", "da\u00b7rein", "zu", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "KON", "APPR", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Den Dichter lieb' ich, der f\u00fcr mich versteht zu pflanzen", "tokens": ["Den", "Dich\u00b7ter", "lieb'", "ich", ",", "der", "f\u00fcr", "mich", "ver\u00b7steht", "zu", "pflan\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "APPR", "PPER", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Ganzes, das besteht aus tausend kleinen Ganzen.", "tokens": ["Ein", "Gan\u00b7zes", ",", "das", "be\u00b7steht", "aus", "tau\u00b7send", "klei\u00b7nen", "Gan\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "APPR", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}