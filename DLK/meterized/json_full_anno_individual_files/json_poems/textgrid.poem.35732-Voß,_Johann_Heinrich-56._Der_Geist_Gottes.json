{"textgrid.poem.35732": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "56. Der Geist Gottes", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was lauschest du, o Volk der Alemannen,", "tokens": ["Was", "lau\u00b7schest", "du", ",", "o", "Volk", "der", "A\u00b7le\u00b7man\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "FM", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Rufern: \u00bbHier, hier webet Gottes Geist,", "tokens": ["Den", "Ru\u00b7fern", ":", "\u00bb", "Hier", ",", "hier", "we\u00b7bet", "Got\u00b7tes", "Geist", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "ADV", "$,", "ADV", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Ulm' und Eich' entwurzelt, und die Tannen", "tokens": ["Der", "Ulm'", "und", "Eich'", "ent\u00b7wur\u00b7zelt", ",", "und", "die", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Donnerhall vom Felsabhange rei\u00dft!\u00ab", "tokens": ["Mit", "Don\u00b7ner\u00b7hall", "vom", "Fels\u00b7ab\u00b7han\u00b7ge", "rei\u00dft", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du h\u00f6rst sein Wehn; doch wei\u00dft du nicht, von wannen,", "tokens": ["Du", "h\u00f6rst", "sein", "Wehn", ";", "doch", "wei\u00dft", "du", "nicht", ",", "von", "wan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und nicht, wohin der Strom des Windes fleu\u00dft.", "tokens": ["Und", "nicht", ",", "wo\u00b7hin", "der", "Strom", "des", "Win\u00b7des", "fleu\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit linder Macht der Menschheit Knosp' entfaltend,", "tokens": ["Mit", "lin\u00b7der", "Macht", "der", "Menschheit", "Knosp'", "ent\u00b7fal\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NE", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "F\u00e4hrt Gottes Geist, umbildend und gestaltend.", "tokens": ["F\u00e4hrt", "Got\u00b7tes", "Geist", ",", "um\u00b7bil\u00b7dend", "und", "ge\u00b7stal\u00b7tend", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Oft leis anschwellend, oft unangek\u00fcndet,", "tokens": ["Oft", "leis", "an\u00b7schwel\u00b7lend", ",", "oft", "un\u00b7an\u00b7ge\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchwallt sein Segenshauch die \u00f6de Flur:", "tokens": ["Durch\u00b7wallt", "sein", "Se\u00b7gens\u00b7hauch", "die", "\u00f6\u00b7de", "Flur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gesang und Red' entspringt dem Schlaf, und windet", "tokens": ["Ge\u00b7sang", "und", "Red'", "ent\u00b7springt", "dem", "Schlaf", ",", "und", "win\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den sch\u00f6nen Kranz der Menschheit und Natur.", "tokens": ["Den", "sch\u00f6\u00b7nen", "Kranz", "der", "Menschheit", "und", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Urkraft, Verhalt und Zweck, tief ausgegr\u00fcndet,", "tokens": ["Ur\u00b7kraft", ",", "Ver\u00b7halt", "und", "Zweck", ",", "tief", "aus\u00b7ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Umschlingt der Anmut leichtgekn\u00fcpfte Schnur.", "tokens": ["Um\u00b7schlingt", "der", "An\u00b7mut", "leicht\u00b7ge\u00b7kn\u00fcpf\u00b7te", "Schnur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Viel angestaunt, von wenigen bewundert,", "tokens": ["Viel", "an\u00b7ge\u00b7staunt", ",", "von", "we\u00b7ni\u00b7gen", "be\u00b7wun\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Erscheint dem Volk sein goldenes Jahrhundert.", "tokens": ["Er\u00b7scheint", "dem", "Volk", "sein", "gol\u00b7de\u00b7nes", "Jahr\u00b7hun\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es starrt die Meng' in dumpfiger Erkaltung,", "tokens": ["Es", "starrt", "die", "Meng'", "in", "dump\u00b7fi\u00b7ger", "Er\u00b7kal\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie wenn der Geist auch atmet, und wie mild.", "tokens": ["Wie", "wenn", "der", "Geist", "auch", "at\u00b7met", ",", "und", "wie", "mild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,", "KON", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erschlaffung d\u00e4ucht des Menschensinns Entfaltung;", "tokens": ["Er\u00b7schlaf\u00b7fung", "d\u00e4ucht", "des", "Men\u00b7schen\u00b7sinns", "Ent\u00b7fal\u00b7tung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Man w\u00e4hnt Natur, was roh sich hebt und wild.", "tokens": ["Man", "w\u00e4hnt", "Na\u00b7tur", ",", "was", "roh", "sich", "hebt", "und", "wild", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "$,", "PRELS", "ADJD", "PRF", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem eitler Tand, dem schn\u00f6de Mi\u00dfgestaltung,", "tokens": ["Dem", "eit\u00b7ler", "Tand", ",", "dem", "schn\u00f6\u00b7de", "Mi\u00df\u00b7ge\u00b7stal\u00b7tung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Erscheint der Anmut reizendes Gebild.", "tokens": ["Er\u00b7scheint", "der", "An\u00b7mut", "rei\u00b7zen\u00b7des", "Ge\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die heiligen Begeisterungen funkeln", "tokens": ["Die", "hei\u00b7li\u00b7gen", "Be\u00b7geis\u00b7te\u00b7run\u00b7gen", "fun\u00b7keln"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Nachwelt erst, wie helle Stern' im Dunkeln.", "tokens": ["Der", "Nach\u00b7welt", "erst", ",", "wie", "hel\u00b7le", "Stern'", "im", "Dun\u00b7keln", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PWAV", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mir nachschaun wirst du dort im Felsenspalte:", "tokens": ["Mir", "nach\u00b7schaun", "wirst", "du", "dort", "im", "Fel\u00b7sen\u00b7spal\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So sprach der Geist: verh\u00fclle dein Gesicht!", "tokens": ["So", "sprach", "der", "Geist", ":", "ver\u00b7h\u00fcl\u00b7le", "dein", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schnell braust' ein Sturm, Erdbeben kracht', es hallte", "tokens": ["Schnell", "braust'", "ein", "Sturm", ",", "Erd\u00b7be\u00b7ben", "kracht'", ",", "es", "hall\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "NN", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Donnerstrahl, doch Gottes Geist war's nicht.", "tokens": ["Der", "Don\u00b7ner\u00b7strahl", ",", "doch", "Got\u00b7tes", "Geist", "wa\u00b7r's", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "NN", "NN", "VAFIN", "PTKNEG", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "In sanft durchschauerndem Ges\u00e4usel wallte", "tokens": ["In", "sanft", "durch\u00b7schau\u00b7ern\u00b7dem", "Ge\u00b7s\u00e4u\u00b7sel", "wall\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Geist einher: der Seher trat ans Licht,", "tokens": ["Der", "Geist", "ein\u00b7her", ":", "der", "Se\u00b7her", "trat", "ans", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und sah, wie fern die Herrlichkeit entschwebend", "tokens": ["Und", "sah", ",", "wie", "fern", "die", "Herr\u00b7lich\u00b7keit", "ent\u00b7schwe\u00b7bend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADJD", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verschimmerte; stumm sah er nach, und bebend.", "tokens": ["Ver\u00b7schim\u00b7mer\u00b7te", ";", "stumm", "sah", "er", "nach", ",", "und", "be\u00b7bend", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVPP", "$."], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Was lauschest du, o Volk der Alemannen,", "tokens": ["Was", "lau\u00b7schest", "du", ",", "o", "Volk", "der", "A\u00b7le\u00b7man\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "FM", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Rufern: \u00bbHier, hier webet Gottes Geist,", "tokens": ["Den", "Ru\u00b7fern", ":", "\u00bb", "Hier", ",", "hier", "we\u00b7bet", "Got\u00b7tes", "Geist", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "ADV", "$,", "ADV", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Ulm' und Eich' entwurzelt, und die Tannen", "tokens": ["Der", "Ulm'", "und", "Eich'", "ent\u00b7wur\u00b7zelt", ",", "und", "die", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Donnerhall vom Felsabhange rei\u00dft!\u00ab", "tokens": ["Mit