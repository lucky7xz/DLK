{"textgrid.poem.33330": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Lehren an ein M\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O M\u00e4dchen, schaue nicht zu viel", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "schau\u00b7e", "nicht", "zu", "viel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "PTKNEG", "PTKA", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf jeder Mode Fratzenspiel!", "tokens": ["Auf", "je\u00b7der", "Mo\u00b7de", "Frat\u00b7zen\u00b7spiel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein M\u00e4dchen, das nur Blonden mi\u00dft,", "tokens": ["Ein", "M\u00e4d\u00b7chen", ",", "das", "nur", "Blon\u00b7den", "mi\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dessen Buch der Spiegel ist,", "tokens": ["Und", "des\u00b7sen", "Buch", "der", "Spie\u00b7gel", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem ob dem Putz der Nachbarin", "tokens": ["Dem", "ob", "dem", "Putz", "der", "Nach\u00b7ba\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor Aerger beide Backen gl\u00fch'n,", "tokens": ["Vor", "A\u00b7er\u00b7ger", "bei\u00b7de", "Ba\u00b7cken", "gl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Das wird ein M\u00fchlstein f\u00fcr den Mann,", "tokens": ["Das", "wird", "ein", "M\u00fchl\u00b7stein", "f\u00fcr", "den", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit dem er lang nicht schwimmen kann.", "tokens": ["Mit", "dem", "er", "lang", "nicht", "schwim\u00b7men", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und sch\u00fcttelt er den M\u00fchlstein dann", "tokens": ["Und", "sch\u00fct\u00b7telt", "er", "den", "M\u00fchl\u00b7stein", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Vom Hals, so tr\u00e4gt der arme Tropf", "tokens": ["Vom", "Hals", ",", "so", "tr\u00e4gt", "der", "ar\u00b7me", "Tropf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Erst noch viel schwerer auf dem Kopf;", "tokens": ["Erst", "noch", "viel", "schwe\u00b7rer", "auf", "dem", "Kopf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Denn so ein Weiblein putzt sich dann", "tokens": ["Denn", "so", "ein", "Weib\u00b7lein", "putzt", "sich", "dann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Aus fremden S\u00e4ckel, und dem Mann", "tokens": ["Aus", "frem\u00b7den", "S\u00e4\u00b7ckel", ",", "und", "dem", "Mann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "W\u00e4chst ob des M\u00e4dchens Ziererei", "tokens": ["W\u00e4chst", "ob", "des", "M\u00e4d\u00b7chens", "Zie\u00b7re\u00b7rei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Manch' zentnerschweres Hirschgeweih,", "tokens": ["Man\u00b7ch'", "zent\u00b7ner\u00b7schwe\u00b7res", "Hirschge\u00b7weih", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Wovon man hier in uns'rer Stadt", "tokens": ["Wo\u00b7von", "man", "hier", "in", "un\u00b7s'\u00b7rer", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "Gar manches sch\u00f6ne Beispiel hat.", "tokens": ["Gar", "man\u00b7ches", "sch\u00f6\u00b7ne", "Bei\u00b7spiel", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "F\u00fcr's Zweite, M\u00e4dchen, liebe den,", "tokens": ["F\u00fcr's", "Zwei\u00b7te", ",", "M\u00e4d\u00b7chen", ",", "lie\u00b7be", "den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "NN", "$,", "VVFIN", "ART", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Den du zum Mann dir auserseh'n;", "tokens": ["Den", "du", "zum", "Mann", "dir", "aus\u00b7er\u00b7seh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Denn wer nur freit um's liebe Brod;", "tokens": ["Denn", "wer", "nur", "freit", "um's", "lie\u00b7be", "Brod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "NN", "NE", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Stirbt an der Liebe Hungersnoth.", "tokens": ["Stirbt", "an", "der", "Lie\u00b7be", "Hun\u00b7gers\u00b7noth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Die Frausucht auch zu dieser Frist", "tokens": ["Die", "Frau\u00b7sucht", "auch", "zu", "die\u00b7ser", "Frist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Bei M\u00e4dchen eine Seuche ist:", "tokens": ["Bei", "M\u00e4d\u00b7chen", "ei\u00b7ne", "Seu\u00b7che", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Darob sieht manche jeden Mann", "tokens": ["Da\u00b7rob", "sieht", "man\u00b7che", "je\u00b7den", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIS", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Als einen M\u00e4dchenheiland an,", "tokens": ["Als", "ei\u00b7nen", "M\u00e4d\u00b7chen\u00b7hei\u00b7land", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Der sie vom Jungfernfluch erl\u00f6st,", "tokens": ["Der", "sie", "vom", "Jung\u00b7fern\u00b7fluch", "er\u00b7l\u00f6st", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und sich an's Ehkreuz nageln l\u00e4\u00dft.", "tokens": ["Und", "sich", "an's", "Eh\u00b7kreuz", "na\u00b7geln", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPRART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "D'rum h\u00fcte dich vor dieser Pest,", "tokens": ["D'\u00b7rum", "h\u00fc\u00b7te", "dich", "vor", "die\u00b7ser", "Pest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.29": {"text": "Und so ein Mann sich finden l\u00e4\u00dft,", "tokens": ["Und", "so", "ein", "Mann", "sich", "fin\u00b7den", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PRF", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Der dein begehrt, so sehe nicht", "tokens": ["Der", "dein", "be\u00b7gehrt", ",", "so", "se\u00b7he", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPOSAT", "VVPP", "$,", "ADV", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Dem Freier blo\u00df nur in's Gesicht;", "tokens": ["Dem", "Frei\u00b7er", "blo\u00df", "nur", "in's", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Denn wi\u00df', da\u00df oft ein b\u00f6ser Mann", "tokens": ["Denn", "wi\u00df'", ",", "da\u00df", "oft", "ein", "b\u00f6\u00b7ser", "Mann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "In Engelslarve stecken kann.", "tokens": ["In", "En\u00b7gels\u00b7lar\u00b7ve", "ste\u00b7cken", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Auch weile nicht dein Auge nur", "tokens": ["Auch", "wei\u00b7le", "nicht", "dein", "Au\u00b7ge", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Auf Rock und Weste und Frisur,", "tokens": ["Auf", "Rock", "und", "Wes\u00b7te", "und", "Fri\u00b7sur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Sieh lieber zu, ob dir der Mann", "tokens": ["Sieh", "lie\u00b7ber", "zu", ",", "ob", "dir", "der", "Mann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Im Schlafrock auch gefallen kann:", "tokens": ["Im", "Schla\u00b7frock", "auch", "ge\u00b7fal\u00b7len", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Auch schiele nicht nach seinem Sack,", "tokens": ["Auch", "schie\u00b7le", "nicht", "nach", "sei\u00b7nem", "Sack", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Wie voll er ihn wohl haben mag,", "tokens": ["Wie", "voll", "er", "ihn", "wohl", "ha\u00b7ben", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPER", "ADV", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Noch ob auf seinem Amtsdekret", "tokens": ["Noch", "ob", "auf", "sei\u00b7nem", "Amts\u00b7de\u00b7kret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Ein Vierteldutzend Nullen steht;", "tokens": ["Ein", "Vier\u00b7tel\u00b7dut\u00b7zend", "Nul\u00b7len", "steht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Denn ach, kein Kr\u00e4mer in der Welt", "tokens": ["Denn", "ach", ",", "kein", "Kr\u00e4\u00b7mer", "in", "der", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Verkauft dir Gl\u00fcck um all dein Geld.", "tokens": ["Ver\u00b7kauft", "dir", "Gl\u00fcck", "um", "all", "dein", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Doch nimm ihn scharf in's Aug', ob nicht", "tokens": ["Doch", "nimm", "ihn", "scharf", "in's", "Aug'", ",", "ob", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "VVFIN", "APPRART", "NN", "$,", "KOUS", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Dein Reiz ihm so in's Auge sticht,", "tokens": ["Dein", "Reiz", "ihm", "so", "in's", "Au\u00b7ge", "sticht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Als wie, wenn ihn der Hunger dr\u00fcckt,", "tokens": ["Als", "wie", ",", "wenn", "ihn", "der", "Hun\u00b7ger", "dr\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Er hin auf einen Rostbeef blickt.", "tokens": ["Er", "hin", "auf", "ei\u00b7nen", "Rost\u00b7beef", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Denn wisse, so ein Vielfra\u00df hat", "tokens": ["Denn", "wis\u00b7se", ",", "so", "ein", "Viel\u00b7fra\u00df", "hat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADV", "ART", "NN", "VAFIN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.49": {"text": "In kurzer Zeit dich \u00fcbersatt,", "tokens": ["In", "kur\u00b7zer", "Zeit", "dich", "\u00fc\u00b7ber\u00b7satt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Und bald wird deiner Magd Gesicht", "tokens": ["Und", "bald", "wird", "dei\u00b7ner", "Magd", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "F\u00fcr ihn ein niedlicher Gericht.", "tokens": ["F\u00fcr", "ihn", "ein", "nied\u00b7li\u00b7cher", "Ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Die Liebe nur f\u00fcr ihren Mann", "tokens": ["Die", "Lie\u00b7be", "nur", "f\u00fcr", "ih\u00b7ren", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Des Weib's Genu\u00df so w\u00fcrzen kann,", "tokens": ["Des", "Weib's", "Ge\u00b7nu\u00df", "so", "w\u00fcr\u00b7zen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Da\u00df dieser ihm, wie's liebe Brod,", "tokens": ["Da\u00df", "die\u00b7ser", "ihm", ",", "wie's", "lie\u00b7be", "Brod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "PPER", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Nie eckel wird bis an den Tod.", "tokens": ["Nie", "ec\u00b7kel", "wird", "bis", "an", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "mea