{"textgrid.poem.53971": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Chanson f\u00fcr eine Frankfurterin", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die alte Herrn noch e mal Triebe ansetze \u2013", "tokens": ["Wenn", "die", "al\u00b7te", "Herrn", "noch", "e", "mal", "Trie\u00b7be", "an\u00b7set\u00b7ze", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "NE", "ADV", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "des find ich goldisch!", "tokens": ["des", "find", "ich", "gol\u00b7disch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wenn se dann nix wie Dummheite schw\u00e4tze \u2013", "tokens": ["Wenn", "se", "dann", "nix", "wie", "Dumm\u00b7hei\u00b7te", "schw\u00e4t\u00b7ze", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "FM", "ADV", "PIS", "KOKOM", "NN", "VVFIN", "$("], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "des find ich goldisch!", "tokens": ["des", "find", "ich", "gol\u00b7disch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Des hab ich von meim alte Herrn:", "tokens": ["Des", "hab", "ich", "von", "meim", "al\u00b7te", "Herrn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "PPER", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "ich hab halt die Alt-Metalle