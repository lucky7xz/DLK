{"dta.poem.6163": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den absonderlichen Bewe-  \n gungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So haben die Bewegungen denn immer", "tokens": ["So", "ha\u00b7ben", "die", "Be\u00b7we\u00b7gun\u00b7gen", "denn", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "ADV"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Blos eine Ursach nur. Durch eine Eintzelnheit", "tokens": ["Blos", "ei\u00b7ne", "Ur\u00b7sach", "nur", ".", "Durch", "ei\u00b7ne", "Eint\u00b7zeln\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geschehen sie allein; es bringet nimmer", "tokens": ["Ge\u00b7sche\u00b7hen", "sie", "al\u00b7lein", ";", "es", "brin\u00b7get", "nim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Regel voller Deutlichkeit,", "tokens": ["Die", "Re\u00b7gel", "vol\u00b7ler", "Deut\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verwirrung oder Dunckelheit.", "tokens": ["Ver\u00b7wir\u00b7rung", "o\u00b7der", "Dun\u00b7ckel\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es l\u00e4sset allezeit dasselbige sich sehen,", "tokens": ["Es", "l\u00e4s\u00b7set", "al\u00b7le\u00b7zeit", "das\u00b7sel\u00b7bi\u00b7ge", "sich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dieselbe W\u00fcrckung wird erkennet", "tokens": ["Die\u00b7sel\u00b7be", "W\u00fcr\u00b7ckung", "wird", "er\u00b7ken\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In allen Dingen, die man nennet", "tokens": ["In", "al\u00b7len", "Din\u00b7gen", ",", "die", "man", "nen\u00b7net"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PIS", "VVFIN"], "meter": "-+-+-++--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Erzengen, Wachsen, und Vergehn.", "tokens": ["Er\u00b7zen\u00b7gen", ",", "Wach\u00b7sen", ",", "und", "Ver\u00b7gehn", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}