{"dta.poem.13788": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Trost .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ees treibt und rauscht der Eisenquell", "tokens": ["\u201e", "es", "treibt", "und", "rauscht", "der", "Ei\u00b7sen\u00b7quell"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch fort mir durch die Glieder;", "tokens": ["Noch", "fort", "mir", "durch", "die", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Felsenlust, so k\u00fchl und hell,", "tokens": ["Die", "Fel\u00b7sen\u00b7lust", ",", "so", "k\u00fchl", "und", "hell", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lockt zu mir alle Br\u00fcder.\u201c", "tokens": ["Lockt", "zu", "mir", "al\u00b7le", "Br\u00fc\u00b7der", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PIAT", "NN", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}