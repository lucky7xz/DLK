{"dta.poem.12863": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie ihn mit wasser bespr\u00fctzte.  \n Aus dem welschen des   Michiele.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein erbarmen k\u00f6mmt zu langsam, h\u00e4tte deine mar-", "tokens": ["Dein", "er\u00b7bar\u00b7men", "k\u00f6mmt", "zu", "lang\u00b7sam", ",", "h\u00e4t\u00b7te", "dei\u00b7ne", "ma\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PTKA", "ADJD", "$,", "VAFIN", "PPOSAT", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "mel-hand", "tokens": ["mel\u00b7hand"], "token_info": ["word"], "pos": ["NE"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Auch schon itzt um meinetwegen alle brunnen ausgeleert.", "tokens": ["Auch", "schon", "itzt", "um", "mei\u00b7net\u00b7we\u00b7gen", "al\u00b7le", "brun\u00b7nen", "aus\u00b7ge\u00b7leert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Es ist hier nichts mehr zu l\u00f6schen; denn der hei\u00dfen liebe brand", "tokens": ["Es", "ist", "hier", "nichts", "mehr", "zu", "l\u00f6\u00b7schen", ";", "denn", "der", "hei\u00b7\u00dfen", "lie\u00b7be", "brand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "PIS", "PTKZU", "VVINF", "$.", "KON", "ART", "ADJA", "ADJA", "NN"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Hat mein allzuzartes hertze schon in asch\u2019 und staub verkehrt.", "tokens": ["Hat", "mein", "all\u00b7zu\u00b7zar\u00b7tes", "hert\u00b7ze", "schon", "in", "asch'", "und", "staub", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "NE", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}