{"textgrid.poem.41759": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df dieses Herz, das unruhvolle,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df dieses Herz, das unruhvolle,", "tokens": ["Da\u00df", "die\u00b7ses", "Herz", ",", "das", "un\u00b7ruh\u00b7vol\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht ganz in sich verzagen darf,", "tokens": ["Nicht", "ganz", "in", "sich", "ver\u00b7za\u00b7gen", "darf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf welche \u00f6de, kalte Scholle", "tokens": ["Auf", "wel\u00b7che", "\u00f6\u00b7de", ",", "kal\u00b7te", "Schol\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PRELS", "ADJD", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es auch ein hartes Schicksal warf!", "tokens": ["Es", "auch", "ein", "har\u00b7tes", "Schick\u00b7sal", "warf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df meine Augen leuchtend gl\u00e4nzen,", "tokens": ["Da\u00df", "mei\u00b7ne", "Au\u00b7gen", "leuch\u00b7tend", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als schauten sie gelobtes Land,", "tokens": ["Als", "schau\u00b7ten", "sie", "ge\u00b7lob\u00b7tes", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als weilten sie auf Siegeskr\u00e4nzen,", "tokens": ["Als", "weil\u00b7ten", "sie", "auf", "Sie\u00b7ges\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Anstatt auf Kett und Sklavenband!", "tokens": ["An\u00b7statt", "auf", "Kett", "und", "Skla\u00b7ven\u00b7band", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Das dank ich einem Talismane,", "tokens": ["Das", "dank", "ich", "ei\u00b7nem", "Ta\u00b7lis\u00b7ma\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den mir ein Bote Gottes gab,", "tokens": ["Den", "mir", "ein", "Bo\u00b7te", "Got\u00b7tes", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Engel mit der Friedensfahne,", "tokens": ["Ein", "En\u00b7gel", "mit", "der", "Frie\u00b7dens\u00b7fah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erhaben \u00fcber Tod und Grab.", "tokens": ["Er\u00b7ha\u00b7ben", "\u00fc\u00b7ber", "Tod", "und", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und soll ich noch das Kleinod nennen?", "tokens": ["Und", "soll", "ich", "noch", "das", "Klei\u00b7nod", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O liebe nur \u2013 dann ist es Dein!", "tokens": ["O", "lie\u00b7be", "nur", "\u2013", "dann", "ist", "es", "Dein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$(", "ADV", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann magst Du's einer Welt bekennen:", "tokens": ["Dann", "magst", "Du's", "ei\u00b7ner", "Welt", "be\u00b7ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Lieben nur ist Trost allein!", "tokens": ["Im", "Lie\u00b7ben", "nur", "ist", "Trost", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "VAFIN", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da\u00df dieses Herz, das unruhvolle,", "tokens": ["Da\u00df", "die\u00b7ses", "Herz", ",", "das", "un\u00b7ruh\u00b7vol\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht ganz in sich verzagen darf,", "tokens": ["Nicht", "ganz", "in", "sich", "ver\u00b7za\u00b7gen", "darf", ","], "token_info": ["w