{"textgrid.poem.54376": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hochwohlgebohrne Frau, ich bitte dich, vergieb.", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hochwohlgebohrne Frau, ich bitte dich, vergieb.", "tokens": ["Hoch\u00b7wohl\u00b7ge\u00b7bohr\u00b7ne", "Frau", ",", "ich", "bit\u00b7te", "dich", ",", "ver\u00b7gieb", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich nicht alsobald auf dein Geehrtes schrieb.", "tokens": ["Da\u00df", "ich", "nicht", "al\u00b7so\u00b7bald", "auf", "dein", "Ge\u00b7ehr\u00b7tes", "schrieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich konnte wahrlich nicht die schwache Feder f\u00fchren.", "tokens": ["Ich", "konn\u00b7te", "wahr\u00b7lich", "nicht", "die", "schwa\u00b7che", "Fe\u00b7der", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich weis, mein Schmerz kann dir zugleich das Herze r\u00fchren.", "tokens": ["Ich", "weis", ",", "mein", "Schmerz", "kann", "dir", "zu\u00b7gleich", "das", "Her\u00b7ze", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "PPOSAT", "NN", "VMFIN", "PPER", "ADV", "PDS", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "H\u00e4ttst du die Hand gesehn, der Schwulst war gar zu gro\u00df;", "tokens": ["H\u00e4ttst", "du", "die", "Hand", "ge\u00b7sehn", ",", "der", "Schwulst", "war", "gar", "zu", "gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVPP", "$,", "ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mir fiel bey dem Geschick ein allzuh\u00e4\u00dflich Los.", "tokens": ["Mir", "fiel", "bey", "dem", "Ge\u00b7schick", "ein", "all\u00b7zu\u00b7h\u00e4\u00df\u00b7lich", "Los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJD", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Erhalt ich sonst ein Blat von deinen liebsten H\u00e4nden,", "tokens": ["Er\u00b7halt", "ich", "sonst", "ein", "Blat", "von", "dei\u00b7nen", "liebs\u00b7ten", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So eil ich unges\u00e4umt, die Antwort drauf zu senden.", "tokens": ["So", "eil", "ich", "un\u00b7ge\u00b7s\u00e4umt", ",", "die", "Ant\u00b7wort", "drauf", "zu", "sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "$,", "ART", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn jedes druckt so gleich mit neuverst\u00e4rkter Lust,", "tokens": ["Denn", "je\u00b7des", "druckt", "so", "gleich", "mit", "neu\u00b7ver\u00b7st\u00e4rk\u00b7ter", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dein Bild, Geehrteste, in die ergebne Brust.", "tokens": ["Dein", "Bild", ",", "Ge\u00b7ehr\u00b7tes\u00b7te", ",", "in", "die", "er\u00b7geb\u00b7ne", "Brust", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie gl\u00fccklich kann ich mich bey deiner Freundschaft sch\u00e4tzen!", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "kann", "ich", "mich", "bey", "dei\u00b7ner", "Freund\u00b7schaft", "sch\u00e4t\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich darf mich ganz vertraut an deine Seite setzen:", "tokens": ["Ich", "darf", "mich", "ganz", "ver\u00b7traut", "an", "dei\u00b7ne", "Sei\u00b7te", "set\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich rede unverstellt, in ungest\u00f6rter Ruh;", "tokens": ["Ich", "re\u00b7de", "un\u00b7ver\u00b7stellt", ",", "in", "un\u00b7ge\u00b7st\u00f6r\u00b7ter", "Ruh", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dein aufmerksames Ohr h\u00f6rt mir gelassen zu.", "tokens": ["Dein", "auf\u00b7merk\u00b7sa\u00b7mes", "Ohr", "h\u00f6rt", "mir", "ge\u00b7las\u00b7sen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich kann dir Wohl und Weh und alles anvertrauen;", "tokens": ["Ich", "kann", "dir", "Wohl", "und", "Weh", "und", "al\u00b7les", "an\u00b7ver\u00b7trau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "KON", "NN", "KON", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich kann auf deine Huld die st\u00e4rksten Schl\u00f6sser bauen.", "tokens": ["Ich", "kann", "auf", "dei\u00b7ne", "Huld", "die", "st\u00e4rks\u00b7ten", "Schl\u00f6s\u00b7ser", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dies alles treibet mich zu einer Ehrfurcht an,", "tokens": ["Dies", "al\u00b7les", "trei\u00b7bet", "mich", "zu", "ei\u00b7ner", "Ehr\u00b7furcht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mit der man in der Welt gar nichts vergleichen kann.", "tokens": ["Mit", "der", "man", "in", "der", "Welt", "gar", "nichts", "ver\u00b7glei\u00b7chen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "APPR", "ART", "NN", "ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mich reizt kein sch\u00f6ner Baum, kein Garten, keine Fr\u00fcchte,", "tokens": ["Mich", "reizt", "kein", "sch\u00f6\u00b7ner", "Baum", ",", "kein", "Gar\u00b7ten", ",", "kei\u00b7ne", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sie bleiben gegen dich ein blosses Schaugerichte.", "tokens": ["Sie", "blei\u00b7ben", "ge\u00b7gen", "dich", "ein", "blos\u00b7ses", "Schau\u00b7ge\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wenn mich was laben soll, so ists ein werthes Blat", "tokens": ["Wenn", "mich", "was", "la\u00b7ben", "soll", ",", "so", "ists", "ein", "wert\u00b7hes", "Blat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Das deine Hand gesetzt, des werd ich niemals satt.", "tokens": ["Das", "dei\u00b7ne", "Hand", "ge\u00b7setzt", ",", "des", "werd", "ich", "nie\u00b7mals", "satt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVPP", "$,", "PDS", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Da la\u00df ich Obst und Wein, und alle Speisen stehen;", "tokens": ["Da", "la\u00df", "ich", "Obst", "und", "Wein", ",", "und", "al\u00b7le", "Spei\u00b7sen", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mein Geist weis nicht vor Lust, wie ihm dabey geschehen.", "tokens": ["Mein", "Geist", "weis", "nicht", "vor", "Lust", ",", "wie", "ihm", "da\u00b7bey", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "PTKNEG", "APPR", "NN", "$,", "PWAV", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Pflicht und Erkenntlichkeit erfordert dies von mir.", "tokens": ["Pflicht", "und", "Er\u00b7kennt\u00b7lich\u00b7keit", "er\u00b7for\u00b7dert", "dies", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PDS", "APPR", "PPER", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.26": {"text": "Mein Herz, Geehrteste, ist immerfort bey dir;", "tokens": ["Mein", "Herz", ",", "Ge\u00b7ehr\u00b7tes\u00b7te", ",", "ist", "im\u00b7mer\u00b7fort", "bey", "dir", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VAFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Das suchst du Tag vor Tag von neuem zu erg\u00f6tzen;", "tokens": ["Das", "suchst", "du", "Tag", "vor", "Tag", "von", "neu\u00b7em", "zu", "er\u00b7g\u00f6t\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "APPR", "NN", "APPR", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Mit deiner Freundschaft ist nichts in Vergleich zusetzen.", "tokens": ["Mit", "dei\u00b7ner", "Freund\u00b7schaft", "ist", "nichts", "in", "Ver\u00b7gleich", "zu\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Du hast mich, Wertheste, vor andern Freunden lieb,", "tokens": ["Du", "hast", "mich", ",", "Wert\u00b7hes\u00b7te", ",", "vor", "an\u00b7dern", "Freun\u00b7den", "lieb", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "NN", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und worauf gr\u00fcndet sich der ungemeine Trieb?", "tokens": ["Und", "wo\u00b7rauf", "gr\u00fcn\u00b7det", "sich", "der", "un\u00b7ge\u00b7mei\u00b7ne", "Trieb", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und was bewegt dich denn mich vielen vorzuziehen?", "tokens": ["Und", "was", "be\u00b7wegt", "dich", "denn", "mich", "vie\u00b7len", "vor\u00b7zu\u00b7zie\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "ADV", "PPER", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Du kennst mein Redlich seyn, mein Sorgen, mein Bem\u00fchen", "tokens": ["Du", "kennst", "mein", "Red\u00b7lich", "seyn", ",", "mein", "Sor\u00b7gen", ",", "mein", "Be\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VAINF", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Vor das was dich vergn\u00fcgt, und was dir wohlgef\u00e4llt;", "tokens": ["Vor", "das", "was", "dich", "ver\u00b7gn\u00fcgt", ",", "und", "was", "dir", "wohl\u00b7ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PWS", "PPER", "VVPP", "$,", "KON", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Mein Wille bleibet stets dem deinen heimgestellt;", "tokens": ["Mein", "Wil\u00b7le", "blei\u00b7bet", "stets", "dem", "dei\u00b7nen", "heim\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ja der Gehorsam daurt in unverr\u00fcckter Treue.", "tokens": ["Ja", "der", "Ge\u00b7hor\u00b7sam", "daurt", "in", "un\u00b7ver\u00b7r\u00fcck\u00b7ter", "Treu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.36": {"text": "Du glaubst nicht, wie ich mich schon auf die Messe freue.", "tokens": ["Du", "glaubst", "nicht", ",", "wie", "ich", "mich", "schon", "auf", "die", "Mes\u00b7se", "freu\u00b7e", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich zehle Stund und Tag bis ich dich sprechen kann", "tokens": ["Ich", "zeh\u00b7le", "Stund", "und", "Tag", "bis", "ich", "dich", "spre\u00b7chen", "kann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "PPER", "PRF", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Bey deiner Gegenwart treff ich mein alles an.", "tokens": ["Bey", "dei\u00b7ner", "Ge\u00b7gen\u00b7wart", "treff", "ich", "mein", "al\u00b7les", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hochwohlgebohrne Frau, ich bitte dich, vergieb.", "tokens": ["Hoch\u00b7wohl\u00b7ge\u00b7bohr\u00b7ne", "Frau", ",", "ich", "bit\u00b7te", "dich", ",", "ver\u00b7gieb", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich nicht alsobald auf dein Geehrtes schrieb.", "tokens": ["Da\u00df", "ich", "nicht", "al\u00b7so\u00b7bald", "auf", "dein", "Ge\u00b7ehr\u00b7tes", "schrieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich konnte wahrlich nicht die schwache Feder f\u00fchren.", "tokens": ["Ich", "konn\u00b7te", "wahr\u00b7lich", "nicht", "die", "schwa\u00b7che", "Fe\u00b7der", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich weis, mein Schmerz kann dir zugleich das Herze r\u00fchren.", "tokens": ["Ich", "weis", ",", "mein", "Schmerz", "kann", "dir", "zu\u00b7gleich", "das", "Her\u00b7ze", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "PPOSAT", "NN", "VMFIN", "PPER", "ADV", "PDS", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "H\u00e4ttst du die Hand gesehn, der Schwulst war gar zu gro\u00df;", "tokens": ["H\u00e4ttst", "du", "die", "Hand", "ge\u00b7sehn", ",", "der", "Schwulst", "war", "gar", "zu", "gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVPP", "$,", "ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mir fiel bey dem Geschick ein allzuh\u00e4\u00dflich Los.", "tokens": ["Mir", "fiel", "bey", "dem", "Ge\u00b7schick", "ein", "all\u00b7zu\u00b7h\u00e4\u00df\u00b7lich", "Los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJD", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Erhalt ich sonst ein Blat von deinen liebsten H\u00e4nden,", "tokens": ["Er\u00b7halt", "ich", "sonst", "ein", "Blat", "von", "dei\u00b7nen", "liebs\u00b7ten", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So eil ich unges\u00e4umt, die Antwort drauf zu senden.", "tokens": ["So", "eil", "ich", "un\u00b7ge\u00b7s\u00e4umt", ",", "die", "Ant\u00b7wort", "drauf", "zu", "sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "$,", "ART", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn jedes druckt so gleich mit neuverst\u00e4rkter Lust,", "tokens": ["Denn", "je\u00b7des", "druckt", "so", "gleich", "mit", "neu\u00b7ver\u00b7st\u00e4rk\u00b7ter", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dein Bild, Geehrteste, in die ergebne Brust.", "tokens": ["Dein", "Bild", ",", "Ge\u00b7ehr\u00b7tes\u00b7te", ",", "in", "die", "er\u00b7geb\u00b7ne", "Brust", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie gl\u00fccklich kann ich mich bey deiner Freundschaft sch\u00e4tzen!", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "kann", "ich", "mich", "bey", "dei\u00b7ner", "Freund\u00b7schaft", "sch\u00e4t\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich darf mich ganz vertraut an deine Seite setzen:", "tokens": ["Ich", "darf", "mich", "ganz", "ver\u00b7traut", "an", "dei\u00b7ne", "Sei\u00b7te", "set\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich rede unverstellt, in ungest\u00f6rter Ruh;", "tokens": ["Ich", "re\u00b7de", "un\u00b7ver\u00b7stellt", ",", "in", "un\u00b7ge\u00b7st\u00f6r\u00b7ter", "Ruh", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dein aufmerksames Ohr h\u00f6rt mir gelassen zu.", "tokens": ["Dein", "auf\u00b7merk\u00b7sa\u00b7mes", "Ohr", "h\u00f6rt", "mir", "ge\u00b7las\u00b7sen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich kann dir Wohl und Weh und alles anvertrauen;", "tokens": ["Ich", "kann", "dir", "Wohl", "und", "Weh", "und", "al\u00b7les", "an\u00b7ver\u00b7trau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "KON", "NN", "KON", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich kann auf deine Huld die st\u00e4rksten Schl\u00f6sser bauen.", "tokens": ["Ich", "kann", "auf", "dei\u00b7ne", "Huld", "die", "st\u00e4rks\u00b7ten", "Schl\u00f6s\u00b7ser", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dies alles treibet mich zu einer Ehrfurcht an,", "tokens": ["Dies", "al\u00b7les", "trei\u00b7bet", "mich", "zu", "ei\u00b7ner", "Ehr\u00b7furcht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mit der man in der Welt gar nichts vergleichen kann.", "tokens": ["Mit", "der", "man", "in", "der", "Welt", "gar", "nichts", "ver\u00b7glei\u00b7chen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "APPR", "ART", "NN", "ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mich reizt kein sch\u00f6ner Baum, kein Garten, keine Fr\u00fcchte,", "tokens": ["Mich", "reizt", "kein", "sch\u00f6\u00b7ner", "Baum", ",", "kein", "Gar\u00b7ten", ",", "kei\u00b7ne", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sie bleiben gegen dich ein blosses Schaugerichte.", "tokens": ["Sie", "blei\u00b7ben", "ge\u00b7gen", "dich", "ein", "blos\u00b7ses", "Schau\u00b7ge\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wenn mich was laben soll, so ists ein werthes Blat", "tokens": ["Wenn", "mich", "was", "la\u00b7ben", "soll", ",", "so", "ists", "ein", "wert\u00b7hes", "Blat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Das deine Hand gesetzt, des werd ich niemals satt.", "tokens": ["Das", "dei\u00b7ne", "Hand", "ge\u00b7setzt", ",", "des", "werd", "ich", "nie\u00b7mals", "satt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVPP", "$,", "PDS", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Da la\u00df ich Obst und Wein, und alle Speisen stehen;", "tokens": ["Da", "la\u00df", "ich", "Obst", "und", "Wein", ",", "und", "al\u00b7le", "Spei\u00b7sen", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mein Geist weis nicht vor Lust, wie ihm dabey geschehen.", "tokens": ["Mein", "Geist", "weis", "nicht", "vor", "Lust", ",", "wie", "ihm", "da\u00b7bey", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "PTKNEG", "APPR", "NN", "$,", "PWAV", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Pflicht und Erkenntlichkeit erfordert dies von mir.", "tokens": ["Pflicht", "und", "Er\u00b7kennt\u00b7lich\u00b7keit", "er\u00b7for\u00b7dert", "dies", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PDS", "APPR", "PPER", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.26": {"text": "Mein Herz, Geehrteste, ist immerfort bey dir;", "tokens": ["Mein", "Herz", ",", "Ge\u00b7ehr\u00b7tes\u00b7te", ",", "ist", "im\u00b7mer\u00b7fort", "bey", "dir", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VAFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Das suchst du Tag vor Tag von neuem zu erg\u00f6tzen;", "tokens": ["Das", "suchst", "du", "Tag", "vor", "Tag", "von", "neu\u00b7em", "zu", "er\u00b7g\u00f6t\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "APPR", "NN", "APPR", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Mit deiner Freundschaft ist nichts in Vergleich zusetzen.", "tokens": ["Mit", "dei\u00b7ner", "Freund\u00b7schaft", "ist", "nichts", "in", "Ver\u00b7gleich", "zu\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Du hast mich, Wertheste, vor andern Freunden lieb,", "tokens": ["Du", "hast", "mich", ",", "Wert\u00b7hes\u00b7te", ",", "vor", "an\u00b7dern", "Freun\u00b7den", "lieb", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "NN", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und worauf gr\u00fcndet sich der ungemeine Trieb?", "tokens": ["Und", "wo\u00b7rauf", "gr\u00fcn\u00b7det", "sich", "der", "un\u00b7ge\u00b7mei\u00b7ne", "Trieb", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und was bewegt dich denn mich vielen vorzuziehen?", "tokens": ["Und", "was", "be\u00b7wegt", "dich", "denn", "mich", "vie\u00b7len", "vor\u00b7zu\u00b7zie\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "ADV", "PPER", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Du kennst mein Redlich seyn, mein Sorgen, mein Bem\u00fchen", "tokens": ["Du", "kennst", "mein", "Red\u00b7lich", "seyn", ",", "mein", "Sor\u00b7gen", ",", "mein", "Be\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VAINF", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Vor das was dich vergn\u00fcgt, und was dir wohlgef\u00e4llt;", "tokens": ["Vor", "das", "was", "dich", "ver\u00b7gn\u00fcgt", ",", "und", "was", "dir", "wohl\u00b7ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PWS", "PPER", "VVPP", "$,", "KON", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Mein Wille bleibet stets dem deinen heimgestellt;", "tokens": ["Mein", "Wil\u00b7le", "blei\u00b7bet", "stets", "dem", "dei\u00b7nen", "heim\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ja der Gehorsam daurt in unverr\u00fcckter Treue.", "tokens": ["Ja", "der", "Ge\u00b7hor\u00b7sam", "daurt", "in", "un\u00b7ver\u00b7r\u00fcck\u00b7ter", "Treu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.36": {"text": "Du glaubst nicht, wie ich mich schon auf die Messe freue.", "tokens": ["Du", "glaubst", "nicht", ",", "wie", "ich", "mich", "schon", "auf", "die", "Mes\u00b7se", "freu\u00b7e", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich zehle Stund und Tag bis ich dich sprechen kann", "tokens": ["Ich", "zeh\u00b7le", "Stund", "und", "Tag", "bis", "ich", "dich", "spre\u00b7chen", "kann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "PPER", "PRF", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Bey deiner Gegenwart treff ich mein alles an.", "tokens": ["Bey", "dei\u00b7ner", "Ge\u00b7gen\u00b7wart", "treff", "ich", "mein", "al\u00b7les", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "