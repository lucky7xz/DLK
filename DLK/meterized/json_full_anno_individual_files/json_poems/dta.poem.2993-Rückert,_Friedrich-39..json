{"dta.poem.2993": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun dieses fehlte dir allein, um froh zu werden;", "tokens": ["Nun", "die\u00b7ses", "fehl\u00b7te", "dir", "al\u00b7lein", ",", "um", "froh", "zu", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "PPER", "ADV", "$,", "KOUI", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun hast du es, und bist nicht froher von Geberden.", "tokens": ["Nun", "hast", "du", "es", ",", "und", "bist", "nicht", "fro\u00b7her", "von", "Ge\u00b7ber\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "KON", "VAFIN", "PTKNEG", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du siehst, da\u00df dieses nicht das, was dir fehlte, war,", "tokens": ["Du", "siehst", ",", "da\u00df", "die\u00b7ses", "nicht", "das", ",", "was", "dir", "fehl\u00b7te", ",", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PDS", "PTKNEG", "PDS", "$,", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das aber, was dir fehlt, dir nie wird ganz und gar.", "tokens": ["Das", "a\u00b7ber", ",", "was", "dir", "fehlt", ",", "dir", "nie", "wird", "ganz", "und", "gar", "."], "token_info": ["word", "word", "punct", "word", "wo