{"textgrid.poem.26589": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Nebel sind zergangen,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nebel sind zergangen,", "tokens": ["Die", "Ne\u00b7bel", "sind", "zer\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verflogen \u00fcber Nacht \u2013", "tokens": ["Ver\u00b7flo\u00b7gen", "\u00fc\u00b7ber", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fast will die Erde prangen", "tokens": ["Fast", "will", "die", "Er\u00b7de", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In leiser Fr\u00fchlingspracht.", "tokens": ["In", "lei\u00b7ser", "Fr\u00fch\u00b7lings\u00b7pracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es strebt, vom Licht umflimmert,", "tokens": ["Es", "strebt", ",", "vom", "Licht", "um\u00b7flim\u00b7mert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der F\u00f6hrenwald empor,", "tokens": ["Der", "F\u00f6h\u00b7ren\u00b7wald", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf braunen \u00c4ckern schimmert", "t