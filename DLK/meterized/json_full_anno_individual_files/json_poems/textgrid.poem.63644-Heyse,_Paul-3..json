{"textgrid.poem.63644": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich, mein holdes Kind, wie oft geschah,", "tokens": ["Wenn", "ich", ",", "mein", "hol\u00b7des", "Kind", ",", "wie", "oft", "ge\u00b7schah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "PWAV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dir vorgefabelt wundersame Sachen,", "tokens": ["Dir", "vor\u00b7ge\u00b7fa\u00b7belt", "wun\u00b7der\u00b7sa\u00b7me", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sahst du mich an mit deinem klugen Lachen", "tokens": ["Sahst", "du", "mich", "an", "mit", "dei\u00b7nem", "klu\u00b7gen", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sagtest: Ich versteh' schon Spa\u00df, Papa.", "tokens": ["Und", "sag\u00b7test", ":", "Ich", "ver\u00b7steh'", "schon", "Spa\u00df", ",", "Pa\u00b7pa", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Glanz umflo\u00df dir Mund und Augen da,", "tokens": ["Ein", "Glanz", "um\u00b7flo\u00df", "dir", "Mund", "und", "Au\u00b7gen", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um auch die tiefste Schwermut froh zu machen.", "tokens": ["Um", "auch", "die", "tiefs\u00b7te", "Schwer\u00b7mut", "froh", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schon k\u00fcndete sich an des Geists Erwachen,", "tokens": ["Schon", "k\u00fcn\u00b7de\u00b7te", "sich", "an", "des", "Geists", "Er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der im Humor des Lebens Bl\u00fcte sah.", "tokens": ["Der", "im", "Hu\u00b7mor", "des", "Le\u00b7bens", "Bl\u00fc\u00b7te", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Das Schicksal aber hat nicht Spa\u00df verstanden.", "tokens": ["Das", "Schick\u00b7sal", "a\u00b7ber", "hat", "nicht", "Spa\u00df", "ver\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PTKNEG", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So unerbittlich war sein eh'rner Wille,", "tokens": ["So", "un\u00b7er\u00b7bitt\u00b7lich", "war", "sein", "eh'r\u00b7ner", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df aller Munterkeit ich l\u00e4ngst verga\u00df.", "tokens": ["Da\u00df", "al\u00b7ler", "Mun\u00b7ter\u00b7keit", "ich", "l\u00e4ngst", "ver\u00b7ga\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nichts, was des Lachens wert, scheint noch vorhanden", "tokens": ["Nichts", ",", "was", "des", "La\u00b7chens", "wert", ",", "scheint", "noch", "vor\u00b7han\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich horche Tag und Nacht \u2013 die Welt bleibt stille,", "tokens": ["Ich", "hor\u00b7che", "Tag", "und", "Nacht", "\u2013", "die", "Welt", "bleibt", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$(", "ART", "NN", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dieses Dasein ward ein schaler Spa\u00df.", "tokens": ["Und", "die\u00b7ses", "Da\u00b7sein", "ward", "ein", "scha\u00b7ler", "Spa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wenn ich, mein holdes Kind, wie oft geschah,", "tokens": ["Wenn", "ich", ",", "mein", "hol\u00b7des", "Kind", ",", "wie", "oft", "ge\u00b7schah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "