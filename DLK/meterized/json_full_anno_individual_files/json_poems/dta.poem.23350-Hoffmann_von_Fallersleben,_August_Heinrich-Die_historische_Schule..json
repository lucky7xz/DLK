{"dta.poem.23350": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die historische Schule.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr st\u00fctzt euch auf Geschichte,", "tokens": ["Ihr", "st\u00fctzt", "euch", "auf", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sucht nicht was ihr suchen sollt,", "tokens": ["Und", "sucht", "nicht", "was", "ihr", "su\u00b7chen", "sollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und findet was ihr finden wollt \u2014", "tokens": ["Und", "fin\u00b7det", "was", "ihr", "fin\u00b7den", "wollt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das nennet ihr Geschichte!", "tokens": ["Das", "nen\u00b7net", "ihr", "Ge\u00b7schich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und das Alte gehet doch zu Nichte.", "tokens": ["Und", "das", "Al\u00b7te", "ge\u00b7het", "doch", "zu", "Nich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "O leset die Geschichte!", "tokens": ["O", "le\u00b7set", "die", "Ge\u00b7schich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Und sehet wie der ew'ge Geist", "tokens": ["Und", "se\u00b7het", "wie", "der", "ew'\u00b7ge", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Neubau Altes niederrei\u00dft \u2014", "tokens": ["Zum", "Neu\u00b7bau", "Al\u00b7tes", "nie\u00b7der\u00b7rei\u00dft"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O lest \u2014 nie die Geschichte!", "tokens": ["O", "lest", "nie", "die", "Ge\u00b7schich\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$(", "ADV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und das Alte gehet doch zu Nichte.", "tokens": ["Und", "das", "Al\u00b7te", "ge\u00b7het", "doch", "zu", "Nich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}