{"textgrid.poem.66752": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was schreckt mich aus wachem Traume?", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was schreckt mich aus wachem Traume?", "tokens": ["Was", "schreckt", "mich", "aus", "wa\u00b7chem", "Trau\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Mond w\u00e4chst glutend herauf.", "tokens": ["Der", "Mond", "w\u00e4chst", "glu\u00b7tend", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Wellen mit salzigem Schaume", "tokens": ["Die", "Wel\u00b7len", "mit", "sal\u00b7zi\u00b7gem", "Schau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Bespritzen des Schwertes Knauf.", "tokens": ["Be\u00b7sprit\u00b7zen", "des", "Schwer\u00b7tes", "Knauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wo ist das H\u00e4uflein verschwunden,", "tokens": ["Wo", "ist", "das", "H\u00e4uf\u00b7lein", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das ich zu Siegen gef\u00fchrt?", "tokens": ["Das", "ich", "zu", "Sie\u00b7gen", "ge\u00b7f\u00fchrt", "?"], "token_