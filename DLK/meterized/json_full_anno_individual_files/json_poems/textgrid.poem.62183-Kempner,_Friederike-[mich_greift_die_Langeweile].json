{"textgrid.poem.62183": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[mich greift die Langeweile]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich greift die Langeweile,", "tokens": ["Mich", "greift", "die", "Lan\u00b7ge\u00b7wei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich schreibe keine Zeile,", "tokens": ["Ich", "schrei\u00b7be", "kei\u00b7ne", "Zei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein Vogel gedeiht in solcher Luft,", "tokens": ["Kein", "Vo\u00b7gel", "ge\u00b7deiht", "in", "sol\u00b7cher", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo alles nur nach Gelde ruft;", "tokens": ["Wo", "al\u00b7les", "nur", "nach", "Gel\u00b7de", "ruft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo alles raset nach Gewinn,", "tokens": ["Wo", "al\u00b7les", "ra\u00b7set", "nach", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kommt einem gar kein Lied in Sinn;", "tokens": ["Kommt", "ei\u00b7nem", "gar", "kein", "Lied", "in", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die B\u00e4ume stehen \u00f6d' und leer,", "tokens": ["Die", "B\u00e4u\u00b7me", "ste\u00b7hen", "\u00f6d'", "und", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Man h\u00f6rt kein einzig Zwitschern mehr!", "tokens": ["Man", "h\u00f6rt", "kein", "ein\u00b7zig", "Zwit\u00b7schern", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIAT", "ADJD", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mich greift die Langeweile,", "tokens": ["Mich", "greift", "die", "Lan\u00b7ge\u00b7wei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich schreibe keine Zeile,", "tokens": ["Ich", "schrei\u00b7be", "kei\u00b7ne", "Zei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein Vogel gedeiht in solcher Luft,", "tokens": ["Kein", "Vo\u00b7gel", "ge\u00b7deiht", "in", "sol\u00b7cher", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo alles nur nach Gelde ruft;", "tokens": ["Wo", "al\u00b7les", "nur", "nach", "Gel\u00b7de", "ruft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5":