{"dta.poem.5909": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So, da\u00df sie zur Bewegung sich zu lencken,", "tokens": ["So", ",", "da\u00df", "sie", "zur", "Be\u00b7we\u00b7gung", "sich", "zu", "len\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPRART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bereit und fertig sey. Sie fang auch w\u00fcrcklich an,", "tokens": ["Be\u00b7reit", "und", "fer\u00b7tig", "sey", ".", "Sie", "fang", "auch", "w\u00fcrck\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "$.", "PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So, da\u00df auch selbst das Aug es sehen kan.", "tokens": ["So", ",", "da\u00df", "auch", "selbst", "das", "Aug", "es", "se\u00b7hen", "kan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "ADV", "ART", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf! lasst uns dann, so weit ein menschlich Auge sieht,", "tokens": ["Auf", "!", "lasst", "uns", "dann", ",", "so", "weit", "ein", "menschlich", "Au\u00b7ge", "sieht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADJD", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}}}}