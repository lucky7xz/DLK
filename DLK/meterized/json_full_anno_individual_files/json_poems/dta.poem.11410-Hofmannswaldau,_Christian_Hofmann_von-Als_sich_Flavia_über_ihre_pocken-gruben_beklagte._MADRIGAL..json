{"dta.poem.11410": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sich Flavia \u00fcber ihre pocken-gruben  \n beklagte.  \n  MADRIGAL.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was z\u00f6rnest du doch auf die pocken?", "tokens": ["Was", "z\u00f6r\u00b7nest", "du", "doch", "auf", "die", "po\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verachte nur die gr\u00fcbgen nicht,", "tokens": ["Ver\u00b7ach\u00b7te", "nur", "die", "gr\u00fcb\u00b7gen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die selbst die liebe zugericht,", "tokens": ["Die", "selbst", "die", "lie\u00b7be", "zu\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um desto sicherer die hertzen anzulocken!", "tokens": ["Um", "des\u00b7to", "si\u00b7che\u00b7rer", "die", "hert\u00b7zen", "an\u00b7zu\u00b7lo\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zwar anfangs grub sie sich blos in ein gr\u00fcbgen ein;", "tokens": ["Zwar", "an\u00b7fangs", "grub", "sie", "sich", "blos", "in", "ein", "gr\u00fcb\u00b7gen", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Doch meinte sie allda nicht recht versteckt zu seyn:", "tokens": ["Doch", "mein\u00b7te", "sie", "all\u00b7da", "nicht", "recht", "ver\u00b7steckt", "zu", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "De\u00dfwegen machte sie noch mehr dergleichen h\u00f6len,", "tokens": ["De\u00df\u00b7we\u00b7gen", "mach\u00b7te", "sie", "noch", "mehr", "derg\u00b7lei\u00b7chen", "h\u00f6\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo sie mit schlauer list auf die verliebten seelen", "tokens": ["Wo", "sie", "mit", "schlau\u00b7er", "list", "auf", "die", "ver\u00b7lieb\u00b7ten", "see\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zu deiner luft mit netz und bogen zielt.", "tokens": ["Zu", "dei\u00b7ner", "luft", "mit", "netz", "und", "bo\u00b7gen", "zielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NE", "KON", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Glaubst du es nicht?", "tokens": ["Glaubst", "du", "es", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Mein allerliebstes licht!", "tokens": ["Mein", "al\u00b7ler\u00b7liebs\u00b7tes", "licht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "So schau nur auf mein hertz, das noch die wunde f\u00fchlt,", "tokens": ["So", "schau", "nur", "auf", "mein", "hertz", ",", "das", "noch", "die", "wun\u00b7de", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und unter s\u00fcssem schertz und lallen", "tokens": ["Und", "un\u00b7ter", "s\u00fcs\u00b7sem", "schertz", "und", "lal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Hier in die grub und in dein netz gefallen.", "tokens": ["Hier", "in", "die", "grub", "und", "in", "dein", "netz", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}