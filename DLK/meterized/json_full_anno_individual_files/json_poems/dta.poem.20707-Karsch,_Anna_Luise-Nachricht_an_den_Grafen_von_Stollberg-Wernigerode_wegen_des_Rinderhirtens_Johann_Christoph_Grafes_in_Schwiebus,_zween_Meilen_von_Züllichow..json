{"dta.poem.20707": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Nachricht  \n an den  \n  Grafen von Stollberg-Wernigerode  \n wegen des Rinderhirtens  \n  Johann Christoph Grafes  \n in Schwiebus, zween Meilen von Z\u00fcllichow.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Rinderhirte lebt noch dort,                 ", "tokens": ["Der", "Rin\u00b7der\u00b7hir\u00b7te", "lebt", "noch", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo er mir Fr\u00fcchte gab und Kr\u00e4nze;", "tokens": ["Wo", "er", "mir", "Fr\u00fcch\u00b7te", "gab", "und", "Kr\u00e4n\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "NN", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht Gram, nicht Mangel trieb ihn fort,", "tokens": ["Nicht", "Gram", ",", "nicht", "Man\u00b7gel", "trieb", "ihn", "fort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "$,", "PTKNEG", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er z\u00e4hlte sechs und vierzig Lenze,", "tokens": ["Er", "z\u00e4hl\u00b7te", "sechs", "und", "vier\u00b7zig", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "KON", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vielleicht seit seinem Kinderlauf,", "tokens": ["Viel\u00b7leicht", "seit", "sei\u00b7nem", "Kin\u00b7der\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vielleicht auch dr\u00fcber oder drunter.", "tokens": ["Viel\u00b7leicht", "auch", "dr\u00fc\u00b7ber", "o\u00b7der", "drun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PAV", "KON", "PAV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sein Auge blickt nicht reizend auf,", "tokens": ["Sein", "Au\u00b7ge", "blickt", "nicht", "rei\u00b7zend", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist nicht beflammt, nicht gro\u00df, nicht munter,", "tokens": ["Ist", "nicht", "be\u00b7flammt", ",", "nicht", "gro\u00df", ",", "nicht", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVFIN", "$,", "PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sein Lippenpaar verlocket nicht", "tokens": ["Sein", "Lip\u00b7pen\u00b7paar", "ver\u00b7lo\u00b7cket", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Zur L\u00fcsternheit nach einem Kusse \u2014", "tokens": ["Zur", "L\u00fcs\u00b7tern\u00b7heit", "nach", "ei\u00b7nem", "Kus\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "O Graf! Sein ganzes Angesicht", "tokens": ["O", "Graf", "!", "Sein", "gan\u00b7zes", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Empfing nichts von dem Honiggusse", "tokens": ["Emp\u00b7fing", "nichts", "von", "dem", "Ho\u00b7nig\u00b7gus\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der Grazien, die an der Braut", "tokens": ["Der", "Gra\u00b7zi\u00b7en", ",", "die", "an", "der", "Braut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Des F\u00fcrsten, der Dich Vater nennet,", "tokens": ["Des", "F\u00fcrs\u00b7ten", ",", "der", "Dich", "Va\u00b7ter", "nen\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Mund, Auge, Stirn und Brust gebaut.", "tokens": ["Mund", ",", "Au\u00b7ge", ",", "Stirn", "und", "Brust", "ge\u00b7baut", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "In dieses Menschen Miene kennet", "tokens": ["In", "die\u00b7ses", "Men\u00b7schen", "Mie\u00b7ne", "ken\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Man nichts von dem, was die Natur", "tokens": ["Man", "nichts", "von", "dem", ",", "was", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "PIS", "APPR", "ART", "$,", "PRELS", "ART", "NN"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.18": {"text": "Ihm mitgegeben zum Geschenke,", "tokens": ["Ihm", "mit\u00b7ge\u00b7ge\u00b7ben", "zum", "Ge\u00b7schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Als er der Dunkelheit entfuhr.", "tokens": ["Als", "er", "der", "Dun\u00b7kel\u00b7heit", "ent\u00b7fuhr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Man sieht nicht, da\u00df er besser denke", "tokens": ["Man", "sieht", "nicht", ",", "da\u00df", "er", "bes\u00b7ser", "den\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Wie mancher, den sie sch\u00f6n gemacht.", "tokens": ["Wie", "man\u00b7cher", ",", "den", "sie", "sch\u00f6n", "ge\u00b7macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Er scheinet uns so gar zuwider,", "tokens": ["Er", "schei\u00b7net", "uns", "so", "gar", "zu\u00b7wi\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "So bald er freundlich thut und lacht.", "tokens": ["So", "bald", "er", "freund\u00b7lich", "thut", "und", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und doch verdient er funfzig Lieder", "tokens": ["Und", "doch", "ver\u00b7dient", "er", "funf\u00b7zig", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Von wegen seiner Fr\u00f6mmigkeit.", "tokens": ["Von", "we\u00b7gen", "sei\u00b7ner", "Fr\u00f6m\u00b7mig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Die Tugend hat von seinen Thaten", "tokens": ["Die", "Tu\u00b7gend", "hat", "von", "sei\u00b7nen", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Gewi\u00df noch keine nicht gescheut.", "tokens": ["Ge\u00b7wi\u00df", "noch", "kei\u00b7ne", "nicht", "ge\u00b7scheut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Den Weg, auf den die Sp\u00f6tter traten,", "tokens": ["Den", "Weg", ",", "auf", "den", "die", "Sp\u00f6t\u00b7ter", "tra\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Vermied er immer, und die Bahn", "tokens": ["Ver\u00b7mied", "er", "im\u00b7mer", ",", "und", "die", "Bahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Der Gottsverge\u00dfnen und die Freuden", "tokens": ["Der", "Gotts\u00b7ver\u00b7ge\u00df\u00b7nen", "und", "die", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Der J\u00fcnglinge, die Lust auf Lust", "tokens": ["Der", "J\u00fcng\u00b7lin\u00b7ge", ",", "die", "Lust", "auf", "Lust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.32": {"text": "Verschlucken und ihr Auge weiden", "tokens": ["Ver\u00b7schlu\u00b7cken", "und", "ihr", "Au\u00b7ge", "wei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "An Dingen die verg\u00e4nglich sind.", "tokens": ["An", "Din\u00b7gen", "die", "ver\u00b7g\u00e4ng\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er spannte sein ererbtes Rind", "tokens": ["Er", "spann\u00b7te", "sein", "er\u00b7erb\u00b7tes", "Rind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Pflug, den er sich selber machte,", "tokens": ["Am", "Pflug", ",", "den", "er", "sich", "sel\u00b7ber", "mach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Geno\u00df, was Feld und Garten brachte,", "tokens": ["Ge\u00b7no\u00df", ",", "was", "Feld", "und", "Gar\u00b7ten", "brach\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "War stets zufrieden, war vergn\u00fcgt", "tokens": ["War", "stets", "zu\u00b7frie\u00b7den", ",", "war", "ver\u00b7gn\u00fcgt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit seinen selbstgezeugten R\u00fcben", "tokens": ["Mit", "sei\u00b7nen", "selbst\u00b7ge\u00b7zeug\u00b7ten", "R\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis Ru\u00dflands V\u00f6lker uns bekriegt", "tokens": ["Bis", "Ru\u00df\u00b7lands", "V\u00f6l\u00b7ker", "uns", "be\u00b7kriegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und manchen Landmann fortgetrieben.", "tokens": ["Und", "man\u00b7chen", "Land\u00b7mann", "fort\u00b7ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da ward sein kleines Gl\u00fcck zerst\u00f6rt.", "tokens": ["Da", "ward", "sein", "klei\u00b7nes", "Gl\u00fcck", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "O Menschenfreund! Du hast sein Klagen", "tokens": ["O", "Men\u00b7schen\u00b7freund", "!", "Du", "hast", "sein", "Kla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mit unverschlo\u00dfner Brust geh\u00f6rt,", "tokens": ["Mit", "un\u00b7ver\u00b7schlo\u00df\u00b7ner", "Brust", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und ihn zu retten beygetragen.", "tokens": ["Und", "ihn", "zu", "ret\u00b7ten", "bey\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Jezt dr\u00e4ngt ein nachbarlicher Feind", "tokens": ["Jezt", "dr\u00e4ngt", "ein", "nach\u00b7bar\u00b7li\u00b7cher", "Feind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.13": {"text": "Sein frommes Herz zu neuer Klage;", "tokens": ["Sein", "from\u00b7mes", "Herz", "zu", "neu\u00b7er", "Kla\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Rei\u00df ihn heraus, Du Tugendfreund!", "tokens": ["Rei\u00df", "ihn", "he\u00b7raus", ",", "Du", "Tu\u00b7gend\u00b7freund", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PPER", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Er wendet seine Lebenstage", "tokens": ["Er", "wen\u00b7det", "sei\u00b7ne", "Le\u00b7bens\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Zu mancher Schnizwerkarbeit an,", "tokens": ["Zu", "man\u00b7cher", "Sch\u00b7niz\u00b7wer\u00b7kar\u00b7beit", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Wenn er im Winter nicht die Erde", "tokens": ["Wenn", "er", "im", "Win\u00b7ter", "nicht", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Mit scharfem Pflug durchw\u00fchlen kann.", "tokens": ["Mit", "schar\u00b7fem", "Pflug", "durch\u00b7w\u00fch\u00b7len", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Hilf, G\u00f6nner! da\u00df er gl\u00fccklich werde!", "tokens": ["Hilf", ",", "G\u00f6n\u00b7ner", "!", "da\u00df", "er", "gl\u00fcck\u00b7lich", "wer\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Sein Feind, sein Widersacher nimmt", "tokens": ["Sein", "Feind", ",", "sein", "Wi\u00b7der\u00b7sa\u00b7cher", "nimmt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Ihm einen Theil von seinem Erbe.", "tokens": ["Ihm", "ei\u00b7nen", "Theil", "von", "sei\u00b7nem", "Er\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Er ist auf seine Stadt ergrimmt,", "tokens": ["Er", "ist", "auf", "sei\u00b7ne", "Stadt", "er\u00b7grimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "La\u00df zu, da\u00df er in Deiner sterbe,", "tokens": ["La\u00df", "zu", ",", "da\u00df", "er", "in", "Dei\u00b7ner", "ster\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Und glaube, da\u00df der Hirte frei,", "tokens": ["Und", "glau\u00b7be", ",", "da\u00df", "der", "Hir\u00b7te", "frei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Nicht unterth\u00e4nig, nicht gebunden", "tokens": ["Nicht", "un\u00b7ter\u00b7th\u00e4\u00b7nig", ",", "nicht", "ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "An irgend einem Herren sey.", "tokens": ["An", "ir\u00b7gend", "ei\u00b7nem", "Her\u00b7ren", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Empfinde, was Du oft empfunden,", "tokens": ["Emp\u00b7fin\u00b7de", ",", "was", "Du", "oft", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Wenn Du denjenigen erquickt,", "tokens": ["W