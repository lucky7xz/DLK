tokens": ["Und", "ihr", "Herz", "in", "Blu\u00b7men", ",", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Bergen und der heil'gen Stille.", "tokens": ["Ber\u00b7gen", "und", "der", "heil'\u00b7gen", "Stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Einst, als sie nach langen K\u00fcssen", "tokens": ["Einst", ",", "als", "sie", "nach", "lan\u00b7gen", "K\u00fcs\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Sich begl\u00fcckt in Armen hielten,", "tokens": ["Sich", "be\u00b7gl\u00fcckt", "in", "Ar\u00b7men", "hiel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und die Blicke zu einander", "tokens": ["Und", "die", "Bli\u00b7cke", "zu", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PRF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Sehns\u00fcchtig, befriedigt spielten,", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", ",", "be\u00b7frie\u00b7digt", "spiel\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Blickte er in ihre Augen,", "tokens": ["Blick\u00b7te", "er", "in", "ih\u00b7re", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Sie in seines Herzens Tiefe,", "tokens": ["Sie", "in", "sei\u00b7nes", "Her\u00b7zens", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Und so wie aus Geisterbrunnen", "tokens": ["Und", "so", "wie", "aus", "Geis\u00b7ter\u00b7brun\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Stiegen beiden in die lichten", "tokens": ["Stie\u00b7gen", "bei\u00b7den", "in", "die", "lich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Augen auf zwei gro\u00dfe Thr\u00e4nen,", "tokens": ["Au\u00b7gen", "auf", "zwei", "gro\u00b7\u00dfe", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Die sie fest im Zittern hielten.", "tokens": ["Die", "sie", "fest", "im", "Zit\u00b7tern", "hiel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Was bedeuten, sprach er seufzend,", "tokens": ["Was", "be\u00b7deu\u00b7ten", ",", "sprach", "er", "seuf\u00b7zend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "$,", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Die Gef\u00fchle, Liebe, diese", "tokens": ["Die", "Ge\u00b7f\u00fch\u00b7le", ",", "Lie\u00b7be", ",", "die\u00b7se"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Wehmuthsvollen s\u00fc\u00dfen Thr\u00e4nen,", "tokens": ["Weh\u00b7muths\u00b7vol\u00b7len", "s\u00fc\u00b7\u00dfen", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Die in Andacht du erwiederst?", "tokens": ["Die", "in", "An\u00b7dacht", "du", "er\u00b7wie\u00b7derst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "--+-++-+", "measure": "anapaest.init"}, "line.27": {"text": "Nein, ich mag sie nicht verbergen,", "tokens": ["Nein", ",", "ich", "mag", "sie", "nicht", "ver\u00b7ber\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Gern hab' ich sie dir gewiesen,", "tokens": ["Gern", "hab'", "ich", "sie", "dir", "ge\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Und die Thr\u00e4ne soll nicht rinnend", "tokens": ["Und", "die", "Thr\u00e4\u00b7ne", "soll", "nicht", "rin\u00b7nend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VMFIN", "PTKNEG", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Aus dem Blicke niederfliessen. \u2013", "tokens": ["Aus", "dem", "Bli\u00b7cke", "nie\u00b7der\u00b7flies\u00b7sen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Ein Geheimni\u00df ist es, sprach sie,", "tokens": ["Ein", "Ge\u00b7heim\u00b7ni\u00df", "ist", "es", ",", "sprach", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Wonach diese Wasser zielen,", "tokens": ["Wo\u00b7nach", "die\u00b7se", "Was\u00b7ser", "zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Das sie gerne mit der Andacht", "tokens": ["Das", "sie", "ger\u00b7ne", "mit", "der", "An\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Wollen aus dem Herzen ziehen,", "tokens": ["Wol\u00b7len", "aus", "dem", "Her\u00b7zen", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Aber schwach sind ihre Arme,", "tokens": ["A\u00b7ber", "schwach", "sind", "ih\u00b7re", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Und es f\u00e4llt in's Dunkle wieder,", "tokens": ["Und", "es", "f\u00e4llt", "in's", "Dunk\u00b7le", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Und erm\u00fcdet sinkt die Thr\u00e4ne", "tokens": ["Und", "er\u00b7m\u00fc\u00b7det", "sinkt", "die", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Ueber unsre Wange nieder. \u2013", "tokens": ["Ue\u00b7ber", "uns\u00b7re", "Wan\u00b7ge", "nie\u00b7der", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Also nur ist Erd' und Wasser,", "tokens": ["Al\u00b7so", "nur", "ist", "Erd'", "und", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Sang er, Luft, Licht und Gestirne", "tokens": ["Sang", "er", ",", "Luft", ",", "Licht", "und", "Ge\u00b7stir\u00b7ne"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.41": {"text": "Aus der Sehnsucht hergequollen,", "tokens": ["Aus", "der", "Sehn\u00b7sucht", "her\u00b7ge\u00b7quol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Ein Geheimni\u00df aufzufinden:", "tokens": ["Ein", "Ge\u00b7heim\u00b7ni\u00df", "auf\u00b7zu\u00b7fin\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Klar im Golde funkelt Sehnsucht,", "tokens": ["Klar", "im", "Gol\u00b7de", "fun\u00b7kelt", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "S\u00fc\u00df Ermatten gl\u00e4nzt im Silber;", "tokens": ["S\u00fc\u00df", "Er\u00b7mat\u00b7ten", "gl\u00e4nzt", "im", "Sil\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Wollte sich doch deine Thr\u00e4ne", "tokens": ["Woll\u00b7te", "sich", "doch", "dei\u00b7ne", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Auch gestalten als Erinnrung!", "tokens": ["Auch", "ge\u00b7stal\u00b7ten", "als", "E\u00b7rinn\u00b7rung", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KOKOM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Ward ja aus der Fluth Geheimni\u00df", "tokens": ["Ward", "ja", "aus", "der", "Fluth", "Ge\u00b7heim\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.48": {"text": "Doch der Bau der Welt gebildet.", "tokens": ["Doch", "der", "Bau", "der", "Welt", "ge\u00b7bil\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "S\u00fc\u00dfe Geister, regt euch alle,", "tokens": ["S\u00fc\u00b7\u00dfe", "Geis\u00b7ter", ",", "regt", "euch", "al\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Da\u00df ein Sein der Thr\u00e4n' entquille,", "tokens": ["Da\u00df", "ein", "Sein", "der", "Thr\u00e4n'", "ent\u00b7quil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Und ein neues Gold wird leuchten", "tokens": ["Und", "ein", "neu\u00b7es", "Gold", "wird", "leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "S\u00fc\u00dfer, sanfter, gl\u00e4nzen milder. \u2013", "tokens": ["S\u00fc\u00b7\u00dfer", ",", "sanf\u00b7ter", ",", "gl\u00e4n\u00b7zen", "mil\u00b7der", ".", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "VVFIN", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Und es waren Geister nahe,", "tokens": ["Und", "es", "wa\u00b7ren", "Geis\u00b7ter", "na\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Die im Quell mit Blumen spielten,", "tokens": ["Die", "im", "Quell", "mit", "Blu\u00b7men", "spiel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Sie erh\u00f6rten das Gebet, die", "tokens": ["Sie", "er\u00b7h\u00f6r\u00b7ten", "das", "Ge\u00b7bet", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.56": {"text": "Thr\u00e4nen sanken, Blumen fielen,", "tokens": ["Thr\u00e4\u00b7nen", "san\u00b7ken", ",", "Blu\u00b7men", "fie\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Griffen, hielten fest die Erde,", "tokens": ["Grif\u00b7fen", ",", "hiel\u00b7ten", "fest", "die", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Und geheimni\u00dfvoll zwei Lilien", "tokens": ["Und", "ge\u00b7heim\u00b7ni\u00df\u00b7voll", "zwei", "Li\u00b7li\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "CARD", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.59": {"text": "Sahen hin auf die Entz\u00fcckten,", "tokens": ["Sa\u00b7hen", "hin", "auf", "die", "Ent\u00b7z\u00fcck\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.60": {"text": "Inn'ger f\u00fchlten sie die Liebe.", "tokens": ["Inn'\u00b7ger", "f\u00fchl\u00b7ten", "sie", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.61": {"text": "Sanfte, goldne, silberwei\u00dfe,", "tokens": ["Sanf\u00b7te", ",", "gold\u00b7ne", ",", "sil\u00b7ber\u00b7wei\u00b7\u00dfe", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.62": {"text": "Also wardst du, Liebeslilge.", "tokens": ["Al\u00b7so", "wardst", "du", ",", "Lie\u00b7