{"dta.poem.10724": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "ArISTARCHUS,  \n SIVE DE CONTEMPTU LIN-  \n GU\u00c6 TEUTONIC \u00c6 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Was in der Welt die Sonn\u2019/ in der Sonn\u2019 ist das Liecht/", "tokens": ["Was", "in", "der", "Welt", "die", "Sonn'", "/", "in", "der", "Sonn'", "ist", "das", "Liecht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ART", "NN", "$(", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In dem Liecht\u2019 ist der glautz/ in dem glantz\u2019 ist die hitze:", "tokens": ["In", "dem", "Liecht'", "ist", "der", "glautz", "/", "in", "dem", "glant\u00b7z'", "ist", "die", "hit\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN", "$(", "APPR", "ART", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Das ist vns Menschen auch die wahre liebes pflicht/", "tokens": ["Das", "ist", "vns", "Men\u00b7schen", "auch", "die", "wah\u00b7re", "lie\u00b7bes", "pflicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "NN", "ADV", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd ein getrewes Hertz\u2019: es ist nichts nicht so n\u00fctze.", "tokens": ["Vnd", "ein", "ge\u00b7tre\u00b7wes", "Hertz'", ":", "es", "ist", "nichts", "nicht", "so", "n\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "PIS", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O wie gl\u00fcckselig ist/ auch in dem h\u00f6chsten schmertzen/", "tokens": ["O", "wie", "gl\u00fcck\u00b7se\u00b7lig", "ist", "/", "auch", "in", "dem", "h\u00f6chs\u00b7ten", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "$(", "ADV", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Der/ dem ein trewer Freund mitliebes brunst von Hertzen", "tokens": ["Der", "/", "dem", "ein", "tre\u00b7wer", "Freund", "mit\u00b7lie\u00b7bes", "brunst", "von", "Hert\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$(", "ART", "ART", "ADJA", "NN", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ohn falsch ist zugethan! ein solchen in der noth", "tokens": ["Ohn", "falsch", "ist", "zu\u00b7ge\u00b7than", "!", "ein", "sol\u00b7chen", "in", "der", "noth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADJD", "VAFIN", "ADJD", "$.", "ART", "PIAT", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vnd widerwertigkeit halt\u2019 ich f\u00fcr einen Gott.", "tokens": ["Vnd", "wi\u00b7der\u00b7wer\u00b7tig\u00b7keit", "halt'", "ich", "f\u00fcr", "ei\u00b7nen", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}