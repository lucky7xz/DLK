{"textgrid.poem.67511": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An Cornelia", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oftmals finden wir nicht, was wir uns sehnlich erflehten;", "tokens": ["Oft\u00b7mals", "fin\u00b7den", "wir", "nicht", ",", "was", "wir", "uns", "sehn\u00b7lich", "er\u00b7fleh\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Oft gew\u00e4hret das Gl\u00fcck, was wir im Traume kaum sehn.", "tokens": ["Oft", "ge\u00b7w\u00e4h\u00b7ret", "das", "Gl\u00fcck", ",", "was", "wir", "im", "Trau\u00b7me", "kaum", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "So, Cornelia, fanden wir Dich, den Engel an G\u00fcte,", "tokens": ["So", ",", "Cor\u00b7ne\u00b7lia", ",", "fan\u00b7den", "wir", "Dich", ",", "den", "En\u00b7gel", "an", "G\u00fc\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "$,", "VVFIN", "PPER", "PPER", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Freundin vom zartesten Sinn, und in der Unschuld ein Kind.", "tokens": ["Freun\u00b7din", "vom", "zar\u00b7tes\u00b7ten", "Sinn", ",", "und", "in", "der", "Un\u00b7schuld", "ein", "Kind", "."], "token_info": ["word", "word