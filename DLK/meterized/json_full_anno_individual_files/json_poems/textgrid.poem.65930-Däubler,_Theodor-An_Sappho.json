{"textgrid.poem.65930": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "An Sappho", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ber\u00fchmte Sappho, K\u00fcnderin des Feuers", "tokens": ["Be\u00b7r\u00fchm\u00b7te", "Sap\u00b7pho", ",", "K\u00fcn\u00b7de\u00b7rin", "des", "Feu\u00b7ers"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verz\u00fcckter Liebe um ein schlankes Wesen,", "tokens": ["Ver\u00b7z\u00fcck\u00b7ter", "Lie\u00b7be", "um", "ein", "schlan\u00b7kes", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du hast so zarte Silben sacht erlesen,", "tokens": ["Du", "hast", "so", "zar\u00b7te", "Sil\u00b7ben", "sacht", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verkl\u00e4rter Sang zu sein des Abenteuers,", "tokens": ["Ver\u00b7kl\u00e4r\u00b7ter", "Sang", "zu", "sein", "des", "A\u00b7bent\u00b7eu\u00b7ers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VAINF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das hin, vor des Umarmungsungeheuers", "tokens": ["Das", "hin", ",", "vor", "des", "Um\u00b7ar\u00b7mun\u00b7gsun\u00b7ge\u00b7heu\u00b7ers"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PTKVZ", "$,", "APPR", "ART", "NN"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gewalt, dich streckte: ich bin Weib gewesen!", "tokens": ["Ge\u00b7walt", ",", "dich", "streck\u00b7te", ":", "ich", "bin", "Weib", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du mu\u00dftest aus dem Rausch zu Mut genesen:", "tokens": ["Du", "mu\u00df\u00b7test", "aus", "dem", "Rausch", "zu", "Mut", "ge\u00b7ne\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun segle ewig, Brecherin des Steuers!", "tokens": ["Nun", "seg\u00b7le", "e\u00b7wig", ",", "Bre\u00b7che\u00b7rin", "des", "Steu\u00b7ers", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Des Weibes Leidenschaft durchschw\u00e4rmt der Meere", "tokens": ["Des", "Wei\u00b7bes", "Lei\u00b7den\u00b7schaft", "durch\u00b7schw\u00e4rmt", "der", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Felshalsendes Best\u00fcrmen, dann Zerst\u00e4uben,", "tokens": ["Fels\u00b7hal\u00b7sen\u00b7des", "Be\u00b7st\u00fcr\u00b7men", ",", "dann", "Zer\u00b7st\u00e4u\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"tex