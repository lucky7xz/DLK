{"dta.poem.19211": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du siehst ein Andres als du h\u00f6rest, und du schmeckest", "tokens": ["Du", "siehst", "ein", "And\u00b7res", "als", "du", "h\u00f6\u00b7rest", ",", "und", "du", "schme\u00b7ckest"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "PIS", "KOKOM", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und riechst ein Andres als du durchs Gef\u00fchl entdeckest,", "tokens": ["Und", "riechst", "ein", "And\u00b7res", "als", "du", "durchs", "Ge\u00b7f\u00fchl", "ent\u00b7de\u00b7ckest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PIS", "KOKOM", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}}, "stanza.2": {"line.1": {"text": "Am Ding, von welchem du verschiedne Kund' einziehst,", "tokens": ["Am", "Ding", ",", "von", "wel\u00b7chem", "du", "ver\u00b7schied\u00b7ne", "Kund'", "ein\u00b7ziehst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "PRELS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie du es f\u00fchlest, riechst, schmeckst, h\u00f6rest oder siehst.", "tokens": ["Wie", "du", "es", "f\u00fch\u00b7lest", ",", "riechst", ",", "schmeckst", ",", "h\u00f6\u00b7rest", "o\u00b7der", "siehst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auch ist kein Zweifel, da\u00df, sobald ein Sinn dir fehlt,", "tokens": ["Auch", "ist", "kein", "Zwei\u00b7fel", ",", "da\u00df", ",", "so\u00b7bald", "ein", "Sinn", "dir", "fehlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "KOUS", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleich eine Seite sich vom Dinge dir verhehlt;", "tokens": ["Gleich", "ei\u00b7ne", "Sei\u00b7te", "sich", "vom", "Din\u00b7ge", "dir", "ver\u00b7hehlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die wichtigste villeicht, wenn grade dir entweicht", "tokens": ["Die", "wich\u00b7tigs\u00b7te", "vil\u00b7leicht", ",", "wenn", "gra\u00b7de", "dir", "ent\u00b7weicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sinn, durch den das Ding vorz\u00fcglich dich erreicht;", "tokens": ["Der", "Sinn", ",", "durch", "den", "das", "Ding", "vor\u00b7z\u00fcg\u00b7lich", "dich", "er\u00b7reicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ART", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie ja ein Blinder mit all seinen andern Sinnen", "tokens": ["Wie", "ja", "ein", "Blin\u00b7der", "mit", "all", "sei\u00b7nen", "an\u00b7dern", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "PIAT", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Farben eines Bilds kann wenig abgewinnen.", "tokens": ["Den", "Far\u00b7ben", "ei\u00b7nes", "Bilds", "kann", "we\u00b7nig", "ab\u00b7ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Drum, wenn dir zu Gebot mehr als f\u00fcnf Sinne st\u00fcnden,", "tokens": ["Drum", ",", "wenn", "dir", "zu", "Ge\u00b7bot", "mehr", "als", "f\u00fcnf", "Sin\u00b7ne", "st\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "APPR", "NN", "PIAT", "KOKOM", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So w\u00fcrdest du auch mehr als jetzt vom Ding ergr\u00fcnden;", "tokens": ["So", "w\u00fcr\u00b7dest", "du", "auch", "mehr", "als", "jetzt", "vom", "Ding", "er\u00b7gr\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIAT", "KOKOM", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wie schon der edelste, den jetzt du hast, verst\u00e4rkt", "tokens": ["Wie", "schon", "der", "e\u00b7dels\u00b7te", ",", "den", "jetzt", "du", "hast", ",", "ver\u00b7st\u00e4rkt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "$,", "PRELS", "ADV", "PPER", "VAFIN", "$,", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch Kunst, dein Auge, mehr als von Natur bemerkt.", "tokens": ["Durch", "Kunst", ",", "dein", "Au\u00b7ge", ",", "mehr", "als", "von", "Na\u00b7tur", "be\u00b7merkt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPOSAT", "NN", "$,", "PIAT", "KOKOM", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und gieng dir nicht villeicht ein sechster Sinn verloren,", "tokens": ["Und", "gieng", "dir", "nicht", "vil\u00b7leicht", "ein", "sechs\u00b7ter", "Sinn", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein siebenter, villeicht auch wird er einst geboren?", "tokens": ["Ein", "sie\u00b7ben\u00b7ter", ",", "vil\u00b7leicht", "auch", "wird", "er", "einst", "ge\u00b7bo\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Weil mit den f\u00fcnfen doch, die dir inzwischen dienen,", "tokens": ["Weil", "mit", "den", "f\u00fcn\u00b7fen", "doch", ",", "die", "dir", "in\u00b7zwi\u00b7schen", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "VVFIN", "ADV", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du unzufrieden bist und kommst nicht aus mit ihnen,", "tokens": ["Du", "un\u00b7zu\u00b7frie\u00b7den", "bist", "und", "kommst", "nicht", "aus", "mit", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "KON", "VVFIN", "PTKNEG", "APPR", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Weil mit den f\u00fcnfen du so wenig kanst bezwingen", "tokens": ["Weil", "mit", "den", "f\u00fcn\u00b7fen", "du", "so", "we\u00b7nig", "kanst", "be\u00b7zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "VVFIN", "PPER", "ADV", "PIS", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Ding, das du sosehr begehrest zu durchdringen.", "tokens": ["Das", "Ding", ",", "das", "du", "so\u00b7sehr", "be\u00b7geh\u00b7rest", "zu", "durch\u00b7drin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Unn\u00fctze Tr\u00e4umerei! Gebrauche fein mit Fug", "tokens": ["Un\u00b7n\u00fct\u00b7ze", "Tr\u00e4u\u00b7me\u00b7rei", "!", "Ge\u00b7brau\u00b7che", "fein", "mit", "Fug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die f\u00fcnfe, die du hast, du hast daran genug.", "tokens": ["Die", "f\u00fcn\u00b7fe", ",", "die", "du", "hast", ",", "du", "hast", "da\u00b7ran", "ge\u00b7nug", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "PPER", "VAFIN", "$,", "PPER", "VAFIN", "PAV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Wo sollt' ein sechster Sinn herkommen oder hin?", "tokens": ["Wo", "sollt'", "ein", "sechs\u00b7ter", "Sinn", "her\u00b7kom\u00b7men", "o\u00b7der", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "W\u00e4r' es ein niedrerer, so w\u00e4r' es kein Gewinn;", "tokens": ["W\u00e4r'", "es", "ein", "nie\u00b7dre\u00b7rer", ",", "so", "w\u00e4r'", "es", "kein", "Ge\u00b7winn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "$,", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Dir k\u00f6nnt' ein h\u00f6herer nur als dein h\u00f6chster frommen,", "tokens": ["Dir", "k\u00f6nnt'", "ein", "h\u00f6\u00b7he\u00b7rer", "nur", "als", "dein", "h\u00f6chs\u00b7ter", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "ADV", "KOUS", "PPOSAT", "ADJA", "ADJA", "$,"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Doch \u00fcber'm Auge hat den Platz der Geist genommen.", "tokens": ["Doch", "\u00fc\u00b7ber'm", "Au\u00b7ge", "hat", "den", "Platz", "der", "Geist", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Wenn du es recht bedenkst, la\u00df ihm nur seinen Platz!", "tokens": ["Wenn", "du", "es", "recht", "be\u00b7denkst", ",", "la\u00df", "ihm", "nur", "sei\u00b7nen", "Platz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,", "VVIMP", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In ihm gefunden hast du den vermissten Schatz.", "tokens": ["In", "ihm", "ge\u00b7fun\u00b7den", "hast", "du", "den", "ver\u00b7miss\u00b7ten", "Schatz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}