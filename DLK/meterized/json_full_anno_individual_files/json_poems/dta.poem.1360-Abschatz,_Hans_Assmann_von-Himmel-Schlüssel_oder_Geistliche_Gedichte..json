{"dta.poem.1360": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kan nicht lustig seyn/ mein traurig Hertze weinet/", "tokens": ["Ich", "kan", "nicht", "lus\u00b7tig", "seyn", "/", "mein", "trau\u00b7rig", "Hert\u00b7ze", "wei\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$(", "PPOSAT", "ADJD", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn mein verstellter Mund erfreut zu lachen scheinet/", "tokens": ["Wenn", "mein", "ver\u00b7stell\u00b7ter", "Mund", "er\u00b7freut", "zu", "la\u00b7chen", "schei\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Sprache zwinget sich/ das Hertze saget nein/", "tokens": ["Die", "Spra\u00b7che", "zwin\u00b7get", "sich", "/", "das", "Hert\u00b7ze", "sa\u00b7get", "nein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$(", "PDS", "VVFIN", "VVFIN", "PTKANT", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es komme wie es will/ ich kan nicht lustig seyn.", "tokens": ["Es", "kom\u00b7me", "wie", "es", "will", "/", "ich", "kan", "nicht", "lus\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "VMFIN", "$(", "PPER", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df nicht was mir fehlt; ich darff nicht Mangel leyden/", "tokens": ["Ich", "wei\u00df", "nicht", "was", "mir", "fehlt", ";", "ich", "darff", "nicht", "Man\u00b7gel", "ley\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "$.", "PPER", "VMFIN", "PTKNEG", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des milden Himmels Gunst hat mir mein Theil bescheiden/", "tokens": ["Des", "mil\u00b7den", "Him\u00b7mels", "Gunst", "hat", "mir", "mein", "Theil", "be\u00b7schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Obgleich kein Uberflu\u00df im Kasten wird gez\u00e4hlt/", "tokens": ["Ob\u00b7gleich", "kein", "U\u00b7ber\u00b7flu\u00df", "im", "Kas\u00b7ten", "wird", "ge\u00b7z\u00e4hlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So reicht es immer zu/ ich wei\u00df nicht was mir fehlt.", "tokens": ["So", "reicht", "es", "im\u00b7mer", "zu", "/", "ich", "wei\u00df", "nicht", "was", "mir", "fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKZU", "$(", "PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mir mangelt freyer Mutt/ ich darff nicht Liebe klagen/", "tokens": ["Mir", "man\u00b7gelt", "frey\u00b7er", "Mutt", "/", "ich", "darff", "nicht", "Lie\u00b7be", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$(", "PPER", "VMFIN", "PTKNEG", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kan keuscher Ehe Frucht auff beyden Armen tragen/", "tokens": ["Kan", "keu\u00b7scher", "E\u00b7he", "Frucht", "auff", "bey\u00b7den", "Ar\u00b7men", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Ehrsucht plagt mich nicht/ die manchem wehe thut/", "tokens": ["Die", "Ehr\u00b7sucht", "plagt", "mich", "nicht", "/", "die", "man\u00b7chem", "we\u00b7he", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$(", "ART", "PIAT", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur das ist mein Beschwer/ mir mangelt freyer Mutt.", "tokens": ["Nur", "das", "ist", "mein", "Be\u00b7schwer", "/", "mir", "man\u00b7gelt", "frey\u00b7er", "Mutt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der schwache Leib empfind wohl offt Beschwerligkeiten/", "tokens": ["Der", "schwa\u00b7che", "Leib", "emp\u00b7find", "wohl", "offt", "Be\u00b7schwer\u00b7lig\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sieht ihm allgemach sein fr\u00fches Grab bereiten/", "tokens": ["Und", "sieht", "ihm", "all\u00b7ge\u00b7mach", "sein", "fr\u00fc\u00b7hes", "Grab", "be\u00b7rei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Tr\u00e4gt seinen Tod mit sich/ doch iedes Adams Kind", "tokens": ["Tr\u00e4gt", "sei\u00b7nen", "Tod", "mit", "sich", "/", "doch", "ie\u00b7des", "A\u00b7dams", "Kind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PRF", "$(", "ADV", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00fchlt unterweilen/ was der schwache Leib empfind.", "tokens": ["F\u00fchlt", "un\u00b7ter\u00b7wei\u00b7len", "/", "was", "der", "schwa\u00b7che", "Leib", "emp\u00b7find", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "PWS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die krancke Seele dr\u00fcckt wohl auch ein schwer Gebl\u00fctte/", "tokens": ["Die", "kran\u00b7cke", "See\u00b7le", "dr\u00fcckt", "wohl", "auch", "ein", "schwer", "Ge\u00b7bl\u00fct\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jedoch am meisten steckt der Mangel im Gem\u00fctte/", "tokens": ["Je\u00b7doch", "am", "meis\u00b7ten", "steckt", "der", "Man\u00b7gel", "im", "Ge\u00b7m\u00fct\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "PIS", "VVFIN", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch Hoffnung naher Grufft wird offt der Geist erquickt/", "tokens": ["Durch", "Hoff\u00b7nung", "na\u00b7her", "Grufft", "wird", "offt", "der", "Geist", "er\u00b7quickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn matter Glieder Last die krancke Seele dr\u00fcckt.", "tokens": ["Wenn", "mat\u00b7ter", "Glie\u00b7der", "Last", "die", "kran\u00b7cke", "See\u00b7le", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich leide steten Zwang/ bin bey vergn\u00fcgten Stunden", "tokens": ["Ich", "lei\u00b7de", "ste\u00b7ten", "Zwang", "/", "bin", "bey", "ver\u00b7gn\u00fcg\u00b7ten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$(", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An stille Traurigkeit/ Verdru\u00df und Angst gebunden/", "tokens": ["An", "stil\u00b7le", "Trau\u00b7rig\u00b7keit", "/", "Ver\u00b7dru\u00df", "und", "Angst", "ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bin wachend voller Schlaff/ und bey Gesundheit kranck/", "tokens": ["Bin", "wa\u00b7chend", "vol\u00b7ler", "Schlaff", "/", "und", "bey", "Ge\u00b7sund\u00b7heit", "kranck", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJA", "NN", "$(", "KON", "APPR", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bey Leben halber Tod/ ich leide steten Zwang.", "tokens": ["Bey", "Le\u00b7ben", "hal\u00b7ber", "Tod", "/", "ich", "lei\u00b7de", "ste\u00b7ten", "Zwang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPO", "NN", "$(", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ich bin mir selber gram/ da\u00df ich mich nicht kan zwingen", "tokens": ["Ich", "bin", "mir", "sel\u00b7ber", "gram", "/", "da\u00df", "ich", "mich", "nicht", "kan", "zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "$(", "KOUS", "PPER", "PRF", "PTKNEG", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In ungezwungner Lust ein St\u00fcndchen hinzubringen/", "tokens": ["In", "un\u00b7ge\u00b7zwung\u00b7ner", "Lust", "ein", "St\u00fcnd\u00b7chen", "hin\u00b7zu\u00b7brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und mehre den Verdru\u00df durch Ungedult und Scham/", "tokens": ["Und", "meh\u00b7re", "den", "Ver\u00b7dru\u00df", "durch", "Un\u00b7ge\u00b7dult", "und", "Scham", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch thu ich was ich will/ ich bin mir selber gram.", "tokens": ["Doch", "thu", "ich", "was", "ich", "will", "/", "ich", "bin", "mir", "sel\u00b7ber", "gram", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PWS", "PPER", "VMFIN", "$(", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wer lindert meine Qual? Last Gold und Silber fliessen/", "tokens": ["Wer", "lin\u00b7dert", "mei\u00b7ne", "Qual", "?", "Last", "Gold", "und", "Sil\u00b7ber", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$.", "NE", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Last mich der Perlen Staub/ Corallen-Blutt geniessen/", "tokens": ["Last", "mich", "der", "Per\u00b7len", "Staub", "/", "Coral\u00b7len\u00b7Blutt", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "$(", "NE", "VVPP", "$("], "meter": "+--+-++-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Bringt saure Quellen her/ und zieht den Geist aus Stahl/", "tokens": ["Bringt", "sau\u00b7re", "Quel\u00b7len", "her", "/", "und", "zieht", "den", "Geist", "aus", "Stahl", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$(", "KON", "VVFIN", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Di\u00df alles hilfft mich nicht: Wer lindert meine Qual?", "tokens": ["Di\u00df", "al\u00b7les", "hilfft", "mich", "nicht", ":", "Wer", "lin\u00b7dert", "mei\u00b7ne", "Qual", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "PTKNEG", "$.", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Kein Arzt verschafft mir Rath/ gl\u00fcckselig ist im Leben", "tokens": ["Kein", "Arzt", "ver\u00b7schafft", "mir", "Rath", "/", "gl\u00fcck\u00b7se\u00b7lig", "ist", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "NN", "$(", "ADJD", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wem GOtt ein fr\u00f6lich Hertz und freyen Sinn gegeben;", "tokens": ["Wem", "Gott", "ein", "fr\u00f6\u00b7lich", "Hertz", "und", "frey\u00b7en", "Sinn", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ART", "ADJD", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wofern der H\u00f6chste di\u00df nicht mitgetheilet hat/", "tokens": ["Wo\u00b7fern", "der", "H\u00f6chs\u00b7te", "di\u00df", "nicht", "mit\u00b7ge\u00b7thei\u00b7let", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PDS", "PTKNEG", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist alle M\u00fch umsonst/ kein Arzt verschafft mir Rath.", "tokens": ["Ist", "al\u00b7le", "M\u00fch", "um\u00b7sonst", "/", "kein", "Arzt", "ver\u00b7schafft", "mir", "Rath", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "$(", "PIAT", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Ach komm gew\u00fcnschter Tod/ du Artzney vieler Klagen/", "tokens": ["Ach", "komm", "ge\u00b7w\u00fcnschter", "Tod", "/", "du", "Artz\u00b7ney", "vie\u00b7ler", "Kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADJA", "NN", "$(", "PPER", "NN", "PIAT", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du Anfang s\u00fcsser Ruh/ du Ende schwerer Plagen/", "tokens": ["Du", "An\u00b7fang", "s\u00fcs\u00b7ser", "Ruh", "/", "du", "En\u00b7de", "schwe\u00b7rer", "Pla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$(", "PPER", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lust-Pforte/ Freuden-Schlaff/ Besieger aller Noth/", "tokens": ["Lust\u00b7P\u00b7for\u00b7te", "/", "Freu\u00b7den\u00b7Schlaff", "/", "Be\u00b7sie\u00b7ger", "al\u00b7ler", "Noth", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NE", "$(", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich sehne mich nach dir/ ach komm