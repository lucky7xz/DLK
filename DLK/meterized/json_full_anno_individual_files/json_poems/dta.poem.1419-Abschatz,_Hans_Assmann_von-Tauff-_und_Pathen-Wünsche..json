{"dta.poem.1419": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mit dir tritt in das Hau\u00df ein doppelt Segen ein/", "tokens": ["Mit", "dir", "tritt", "in", "das", "Hau\u00df", "ein", "dop\u00b7pelt", "Se\u00b7gen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJD", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein doppelt Bad macht dich an Leib und Seele rein/", "tokens": ["Ein", "dop\u00b7pelt", "Bad", "macht", "dich", "an", "Leib", "und", "See\u00b7le", "rein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NE", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dich w\u00e4scht der Tauffe Flutt mit Christi Blutt verbunden/", "tokens": ["Dich", "w\u00e4scht", "der", "Tauf\u00b7fe", "Flutt", "mit", "Chris\u00b7ti", "Blutt", "ver\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "NE", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer diese Quelle braucht/ hat Heyl und Leben funden.", "tokens": ["Wer", "die\u00b7se", "Quel\u00b7le", "braucht", "/", "hat", "Heyl", "und", "Le\u00b7ben", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "$(", "VAFIN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich w\u00fcnsche/ da\u00df dein Gl\u00fcck allzeit verdoppelt sey/", "tokens": ["Ich", "w\u00fcn\u00b7sche", "/", "da\u00df", "dein", "Gl\u00fcck", "all\u00b7zeit", "ver\u00b7dop\u00b7pelt", "sey", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was hier und ewig gutt/ dir zehnfach falle bey!", "tokens": ["Was", "hier", "und", "e\u00b7wig", "gutt", "/", "dir", "zehn\u00b7fach", "fal\u00b7le", "bey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KON", "ADJD", "ADJD", "$(", "PPER", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}