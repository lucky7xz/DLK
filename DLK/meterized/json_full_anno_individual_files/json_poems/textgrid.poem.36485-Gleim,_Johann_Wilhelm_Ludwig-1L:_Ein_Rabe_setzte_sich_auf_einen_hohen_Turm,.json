{"textgrid.poem.36485": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Rabe setzte sich auf einen hohen Turm,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Rabe setzte sich auf einen hohen Turm,", "tokens": ["Ein", "Ra\u00b7be", "setz\u00b7te", "sich", "auf", "ei\u00b7nen", "ho\u00b7hen", "Turm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Boreas die Luft aus ihrem Stillstand brachte;", "tokens": ["Als", "Bo\u00b7re\u00b7as", "die", "Luft", "aus", "ih\u00b7rem", "Still\u00b7stand", "brach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sa\u00df, f\u00fcrchtete zu fliegen, dachte:", "tokens": ["Sa\u00df", ",", "f\u00fcrch\u00b7te\u00b7te", "zu", "flie\u00b7gen", ",", "dach\u00b7te", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer diese Stadt und diesen Sturm", "tokens": ["Wer", "die\u00b7se", "Stadt", "und", "die\u00b7sen", "Sturm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PDAT", "NN", "KON", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Entstehen machte,", "tokens": ["Ent\u00b7ste\u00b7hen", "mach\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "me