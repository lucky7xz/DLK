{"dta.poem.8840": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XiX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Aber glaubet mir, das Leben l\u00e4\u00dft sich dann und wann", "tokens": ["A\u00b7ber", "glau\u00b7bet", "mir", ",", "das", "Le\u00b7ben", "l\u00e4\u00dft", "sich", "dann", "und", "wann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "ART", "NN", "VVFIN", "PRF", "ADV", "KON", "PWAV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Zwar des Leidens ganze B\u00fcrde ri\u00df mich oft schon halb", "tokens": ["Zwar", "des", "Lei\u00b7dens", "gan\u00b7ze", "B\u00fcr\u00b7de", "ri\u00df", "mich", "oft", "schon", "halb"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Doch ich hab' es immer wieder, wenn ich mich besann,", "tokens": ["Doch", "ich", "hab'", "es", "im\u00b7mer", "wie\u00b7der", ",", "wenn", "ich", "mich", "be\u00b7sann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Mir geziemt der volle Becher, mir der volle Klang der", "tokens": ["Mir", "ge\u00b7ziemt", "der", "vol\u00b7le", "Be\u00b7cher", ",", "mir", "der", "vol\u00b7le", "Klang", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PPER", "ART", "ADJA", "NN", "ART"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Denn den vollen Schmerz des Lebens hab' ich als ein", "tokens": ["Denn", "den", "vol\u00b7len", "Schmerz", "des", "Le\u00b7bens", "hab'", "ich", "als", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "PPER", "KOUS", "ART"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.6": {"text": "Doch nun f\u00fchl' ich, wie auf Fitt'gen, bis zum Himmel", "tokens": ["Doch", "nun", "f\u00fchl'", "ich", ",", "wie", "auf", "Fitt'\u00b7gen", ",", "bis", "zum", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PWAV", "APPR", "NN", "$,", "KOUS", "APPRART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Denn es lehrte mich das Leben, da\u00df man Alles kann", "tokens": ["Denn", "es", "lehr\u00b7te", "mich", "das", "Le\u00b7ben", ",", "da\u00df", "man", "Al\u00b7les", "kann"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN", "$,", "KOUS", "PIS", "PIS", "VMFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "Und es \u00f6ffnet gegen Alle sich das Herz in reiner Liebe,", "tokens": ["Und", "es", "\u00f6ff\u00b7net", "ge\u00b7gen", "Al\u00b7le", "sich", "das", "Herz", "in", "rei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PIAT", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Und ich will so gern mit Allen dieses Lebens Bann er-", "tokens": ["Und", "ich", "will", "so", "gern", "mit", "Al\u00b7len", "die\u00b7ses", "Le\u00b7bens", "Bann", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADV", "APPR", "NE", "PDAT", "NN", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "Schlie\u00dft den Kreis und leert die Flaschen, diese Som-", "tokens": ["Schlie\u00dft", "den", "Kreis", "und", "leert", "die", "Fla\u00b7schen", ",", "die\u00b7se", "Som"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,", "PDAT", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Schlimmre Zeiten werden kommen, die wir auch sodann", "tokens": ["Schlimm\u00b7re", "Zei\u00b7ten", "wer\u00b7den", "kom\u00b7men", ",", "die", "wir", "auch", "so\u00b7dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "VVINF", "$,", "PRELS", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}