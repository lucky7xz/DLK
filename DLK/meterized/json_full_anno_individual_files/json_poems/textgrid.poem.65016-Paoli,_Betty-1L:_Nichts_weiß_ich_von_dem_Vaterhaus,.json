{"textgrid.poem.65016": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nichts wei\u00df ich von dem Vaterhaus,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nichts wei\u00df ich von dem Vaterhaus,", "tokens": ["Nichts", "wei\u00df", "ich", "von", "dem", "Va\u00b7ter\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nichts von der Kindheit Paradiesen;", "tokens": ["Nichts", "von", "der", "Kind\u00b7heit", "Pa\u00b7ra\u00b7die\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Fr\u00fch trat ich in die Welt hinaus,", "tokens": ["Fr\u00fch", "trat", "ich", "in", "die", "Welt", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "APZR", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An meine eig'ne Kraft gewiesen.", "tokens": ["An", "mei\u00b7ne", "eig'\u00b7ne", "Kraft", "ge\u00b7wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hinschwanden meines Fr\u00fchlings Tage", "tokens": ["Hin\u00b7schwan\u00b7den", "mei\u00b7nes", "Fr\u00fch\u00b7lings", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Sorg' und Arbeit, M\u00fch' und Plage,", "tokens": ["In", "Sor\u00b7g'", "und", "Ar\u00b7beit", ",", "M\u00fch'", "und", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Das drohende Gespenst der Noth", "tokens": ["Das", "dro\u00b7hen\u00b7de", "Ge\u00b7spenst", "der", "Noth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fchlt ich mich grauenhaft umschlingen,", "tokens": ["F\u00fchlt", "ich", "mich", "grau\u00b7en\u00b7haft", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mit allen Kr\u00e4ften mu\u00dft ich ringen,", "tokens": ["Mit", "al\u00b7len", "Kr\u00e4f\u00b7ten", "mu\u00dft", "ich", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wie oft mein Innerstes bezwingen,", "tokens": ["Wie", "oft", "mein", "In\u00b7ners\u00b7tes", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Mich f\u00fcgen fremdem Machtgebot!", "tokens": ["Mich", "f\u00fc\u00b7gen", "frem\u00b7dem", "Macht\u00b7ge\u00b7bot", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Gepriesen seist du, Weltengeist!", "tokens": ["Ge\u00b7prie\u00b7sen", "seist", "du", ",", "Wel\u00b7ten\u00b7geist", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der mich gelehrt, was Leben hei\u00dft!", "tokens": ["Der", "mich", "ge\u00b7lehrt", ",", "was", "Le\u00b7ben", "hei\u00dft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "PWS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die sch\u00f6nen G\u00f6tterbilder, die", "tokens": ["Die", "sch\u00f6\u00b7nen", "G\u00f6t\u00b7ter\u00b7bil\u00b7der", ",", "die"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein tiefes Dunkel sanft durchlichtet,", "tokens": ["Mein", "tie\u00b7fes", "Dun\u00b7kel", "sanft", "durch\u00b7lich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In Schutt und Tr\u00fcmmer sanken sie,", "tokens": ["In", "Schutt", "und", "Tr\u00fcm\u00b7mer", "san\u00b7ken", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Leben schonungslos gerichtet.", "tokens": ["Vom", "Le\u00b7ben", "scho\u00b7nungs\u00b7los", "ge\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gest\u00fcrzt die schimmernden Idole!", "tokens": ["Ge\u00b7st\u00fcrzt", "die", "schim\u00b7mern\u00b7den", "I\u00b7do\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die lohe Flamme todte Kohle!", "tokens": ["Die", "lo\u00b7he", "Flam\u00b7me", "tod\u00b7te", "Koh\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Im Herzen tiefer stets der Sporn", "tokens": ["Im", "Her\u00b7zen", "tie\u00b7fer", "stets", "der", "Sporn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Zweifels, kaum mehr zu ertragen,", "tokens": ["Des", "Zwei\u00b7fels", ",", "kaum", "mehr", "zu", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der Drang, durch all' die dunkeln Fragen", "tokens": ["Der", "Drang", ",", "durch", "all'", "die", "dun\u00b7keln", "Fra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mich k\u00fchn und siegreich durchzuschlagen", "tokens": ["Mich", "k\u00fchn", "und", "sieg\u00b7reich", "durch\u00b7zu\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Zu der Erkenntni\u00df Weiheborn!", "tokens": ["Zu", "der", "Er\u00b7kennt\u00b7ni\u00df", "Wei\u00b7he\u00b7born", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Gepriesen seist du, Weltengeist,", "tokens": ["Ge\u00b7prie\u00b7sen", "seist", "du", ",", "Wel\u00b7ten\u00b7geist", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$,", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der mich gelehrt, was K\u00e4mpfen hei\u00dft!", "tokens": ["Der", "mich", "ge\u00b7lehrt", ",", "was", "K\u00e4mp\u00b7fen", "hei\u00dft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "PWS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Liebe, dran ich bis zum Sarg", "tokens": ["Die", "Lie\u00b7be", ",", "dran", "ich", "bis", "zum", "Sarg"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PAV", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begeistert hoffte festzuhalten,", "tokens": ["Be\u00b7geis\u00b7tert", "hoff\u00b7te", "fest\u00b7zu\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Meuchlers scharfe Waffe barg", "tokens": ["Des", "Meuch\u00b7lers", "schar\u00b7fe", "Waf\u00b7fe", "barg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie still in ihres Mantels Falten.", "tokens": ["Sie", "still", "in", "ih\u00b7res", "Man\u00b7tels", "Fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Getroffen von der Todeswunde", "tokens": ["Ge\u00b7trof\u00