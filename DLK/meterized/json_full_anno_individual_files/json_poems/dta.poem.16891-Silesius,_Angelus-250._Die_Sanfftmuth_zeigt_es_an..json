{"dta.poem.16891": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "250.  Die Sanfftmuth zeigt es an.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Kan ich an deiner Th\u00fcr vergoldet Oelholtz kennen:", "tokens": ["Kan", "ich", "an", "dei\u00b7ner", "Th\u00fcr", "ver\u00b7gol\u00b7det", "O\u00b7el\u00b7holtz", "ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So wil ich dich de\u00df Bliks den Tempel Gottes nennen.", "tokens": ["So", "wil", "ich", "dich", "de\u00df", "Bliks", "den", "Tem\u00b7pel", "Got\u00b7tes", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ART", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}