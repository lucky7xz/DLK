{"dta.poem.3885": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "10) Dancksagungs-Schreiben an einen  \n Freund.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Mein G\u00f6nner, deine Huld ist gro\u00df und un-", "tokens": ["Mein", "G\u00f6n\u00b7ner", ",", "dei\u00b7ne", "Huld", "ist", "gro\u00df", "und", "un"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "gemein,", "tokens": ["ge\u00b7mein", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Ich selbsten mu\u00df hiervon der treuste Zeuge seyn.", "tokens": ["Ich", "selbs\u00b7ten", "mu\u00df", "hier\u00b7von", "der", "treus\u00b7te", "Zeu\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PAV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat seinen Stuben Zins gleich ", "tokens": ["Hat", "sei\u00b7nen", "Stu\u00b7ben", "Zins", "gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "NE", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So hast du seine Schuld in Schorstein hin gemahlet,", "tokens": ["So", "hast", "du", "sei\u00b7ne", "Schuld", "in", "Schor\u00b7stein", "hin", "ge\u00b7mah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dieses mir zu gut; ", "tokens": ["Und", "die\u00b7ses", "mir", "zu", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O! schreibe doch, womit es zu vergelten sey,", "tokens": ["O", "!", "schrei\u00b7be", "doch", ",", "wo\u00b7mit", "es", "zu", "ver\u00b7gel\u00b7ten", "sey", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ADV", "$,", "PWAV", "PPER", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Drum fordre was du wilt, ich will dir Hertz und", "tokens": ["Drum", "ford\u00b7re", "was", "du", "wilt", ",", "ich", "will", "dir", "Hertz", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PWS", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "PPER", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Leben,", "tokens": ["Le\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Aus treu ergebnen Danck zum Wiedergelde geben;", "tokens": ["Aus", "treu", "er\u00b7geb\u00b7nen", "Danck", "zum", "Wie\u00b7der\u00b7gel\u00b7de", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dich gr\u00fcsset tausendmahl des frommen Vaters", "tokens": ["Dich", "gr\u00fcs\u00b7set", "tau\u00b7send\u00b7mahl", "des", "from\u00b7men", "Va\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Haus,", "tokens": ["Haus", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Uns l\u00f6scht nichts als der Tod dein Angedencken aus.", "tokens": ["Uns", "l\u00f6scht", "nichts", "als", "der", "Tod", "dein", "An\u00b7ge\u00b7den\u00b7cken", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "KOKOM", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn k\u00fcnfftig unser Berg viel Preu\u00dfel-Beeren", "tokens": ["Wenn", "k\u00fcnff\u00b7tig", "un\u00b7ser", "Berg", "viel", "Preu\u00b7\u00dfel\u00b7Bee\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "tr\u00e4get,", "tokens": ["tr\u00e4\u00b7get", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "So wird ein kleiner Stortz vor dich mit eingeleget,", "tokens": ["So", "wird", "ein", "klei\u00b7ner", "Stortz", "vor", "dich", "mit", "ein\u00b7ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "PRF", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und das in kurtzer Zeit, denn nimm ihn g\u00fctigst an,", "tokens": ["Und", "das", "in", "kurt\u00b7zer", "Zeit", ",", "denn", "nimm", "ihn", "g\u00fc\u00b7tigst", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ADJA", "NN", "$,", "KON", "VVIMP", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Weil meine Armuth sonst nichts bessers geben kan.", "tokens": ["Weil", "mei\u00b7ne", "Ar\u00b7muth", "sonst", "nichts", "bes\u00b7sers", "ge\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.19": {"text": "Denn w\u00fcnsch ich, da\u00df dein Mund die wohlgeschma-", "tokens": ["Denn", "w\u00fcnsch", "ich", ",", "da\u00df", "dein", "Mund", "die", "wohl\u00b7ge\u00b7schma"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "ckten Beeren,", "tokens": ["ck\u00b7ten", "Bee\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.21": {"text": "Nebst denen Deinigen in Freude kan verzehren.", "tokens": ["Nebst", "de\u00b7nen", "Dei\u00b7ni\u00b7gen", "in", "Freu\u00b7de", "kan", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPOSAT", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ist di\u00df Geschencke gleich vor dich, ", "tokens": ["Ist", "di\u00df", "Ge\u00b7schen\u00b7cke", "gleich", "vor", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "schlecht,", "tokens": ["schlecht", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.24": {"text": "So nimm es g\u00fctigst an von deinem treusten Knecht.", "tokens": ["So", "nimm", "es", "g\u00fc\u00b7tigst", "an", "von", "dei\u00b7nem", "treus\u00b7ten", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "APPR", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}