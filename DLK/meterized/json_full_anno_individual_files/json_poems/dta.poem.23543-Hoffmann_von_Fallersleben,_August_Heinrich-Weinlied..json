{"dta.poem.23543": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Weinlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer fragte je nach deinem Glauben,", "tokens": ["Wer", "frag\u00b7te", "je", "nach", "dei\u00b7nem", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn er vor dir mit Andacht sa\u00df,", "tokens": ["Wenn", "er", "vor", "dir", "mit", "An\u00b7dacht", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei dir, du edler Sohn der Trauben,", "tokens": ["Bei", "dir", ",", "du", "ed\u00b7ler", "Sohn", "der", "Trau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zeit und alle Welt verga\u00df?", "tokens": ["Die", "Zeit", "und", "al\u00b7le", "Welt", "ver\u00b7ga\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Willkommen, reiner Gottessegen,", "tokens": ["Will\u00b7kom\u00b7men", ",", "rei\u00b7ner", "Got\u00b7tes\u00b7se\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sei uns willkommen tausendmal!", "tokens": ["Sei", "uns", "will\u00b7kom\u00b7men", "tau\u00b7send\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gen\u00e4hrt vom Himmelsthau und Regen,", "tokens": ["Ge\u00b7n\u00e4hrt", "vom", "Him\u00b7mel\u00b7sthau", "und", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Getr\u00e4nkt vom Licht und Sonnenstrahl!", "tokens": ["Ge\u00b7tr\u00e4nkt", "vom", "Licht", "und", "Son\u00b7nen\u00b7strahl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Aus welcher Ehe du entsprungen \u2014", "tokens": ["Aus", "wel\u00b7cher", "E\u00b7he", "du", "ent\u00b7sprun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gesegnet sei ", "tokens": ["Ge\u00b7seg\u00b7net", "sei"], "token_info": ["word", "word"], "pos": ["VVPP", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und sprichst du auch in fremden Zungen,", "tokens": ["Und", "sprichst", "du", "auch", "in", "frem\u00b7den", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gesegnet sei dein Vaterland!", "tokens": ["Ge\u00b7seg\u00b7net", "sei", "dein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und w\u00e4rst ein Ketzer du, ein Heide,", "tokens": ["Und", "w\u00e4rst", "ein", "Ket\u00b7zer", "du", ",", "ein", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir Gl\u00e4ubigen verehren dich,", "tokens": ["Wir", "Gl\u00e4u\u00b7bi\u00b7gen", "ver\u00b7eh\u00b7ren", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir fliehn zu dir in unserm Leide,", "tokens": ["Wir", "fliehn", "zu", "dir", "in", "un\u00b7serm", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir freun mit dir uns inniglich.", "tokens": ["Wir", "freun", "mit", "dir", "uns", "in\u00b7nig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dich hat der Herr der Welt begnadet,", "tokens": ["Dich", "hat", "der", "Herr", "der", "Welt", "be\u00b7gna\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Der gro\u00dfe Wirth der Gl\u00e4ub'gen ladet", "tokens": ["Der", "gro\u00b7\u00dfe", "Wirth", "der", "Gl\u00e4ub'\u00b7gen", "la\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Uns alle, alle zu dir ein.", "tokens": ["Uns", "al\u00b7le", ",", "al\u00b7le", "zu", "dir", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PIS", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}