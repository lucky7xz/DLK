{"textgrid.poem.43103": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sei nicht so hart, wenn aus der Brust", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei nicht so hart, wenn aus der Brust", "tokens": ["Sei", "nicht", "so", "hart", ",", "wenn", "aus", "der", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Mi\u00dfton sich mir ringt,", "tokens": ["Ein", "Mi\u00df\u00b7ton", "sich", "mir", "ringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn oft ein trotzig-wildes Wort", "tokens": ["Wenn", "oft", "ein", "trot\u00b7zig\u00b7wil\u00b7des", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar zu unweiblich klingt.", "tokens": ["Gar", "zu", "un\u00b7weib\u00b7lich", "klingt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Hab' sonst nicht viel danach gefragt,", "tokens": ["Hab'", "sonst", "nicht", "viel", "da\u00b7nach", "ge\u00b7fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was zahme Weiberart,", "tokens": ["Was", "zah\u00b7me", "Wei\u00b7be\u00b7rart", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "War niemals sanft und selten still \u2013", "tokens": ["War", "nie\u00b7mals", "sanft", "und", "sel\u00b7ten", "still", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O sei darum nicht hart!", "tokens": ["O", "sei", "da\u00b7rum", "nicht", "hart", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PAV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich m\u00fch' mich jetzt, so recht zu sein", "tokens": ["Ich", "m\u00fch'", "mich", "jetzt", ",", "so", "recht", "zu", "sein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "$,", "ADV", "ADJD", "PTKZU", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie andre Weiber sind,", "tokens": ["Wie", "and\u00b7re", "Wei\u00b7ber", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und der Beweis, wie sehr mir's Ernst,", "tokens": ["Und", "der", "Be\u00b7weis", ",", "wie", "sehr", "mir's", "Ernst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "ADV", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist wohl mein kleines Kind.", "tokens": ["Ist", "wohl", "mein", "klei\u00b7nes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sei nic