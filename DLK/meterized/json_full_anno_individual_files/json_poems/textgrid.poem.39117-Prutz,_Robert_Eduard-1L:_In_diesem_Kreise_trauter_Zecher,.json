{"textgrid.poem.39117": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "1L: In diesem Kreise trauter Zecher,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In diesem Kreise trauter Zecher,", "tokens": ["In", "die\u00b7sem", "Krei\u00b7se", "trau\u00b7ter", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bei diesem bangen Abschiedsfest,", "tokens": ["Bei", "die\u00b7sem", "ban\u00b7gen", "Ab\u00b7schieds\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein letzter Gru\u00df, ein letzter Becher", "tokens": ["Ein", "letz\u00b7ter", "Gru\u00df", ",", "ein", "letz\u00b7ter", "Be\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Manne, der uns heut verl\u00e4\u00dft:", "tokens": ["Dem", "Man\u00b7ne", ",", "der", "uns", "heut", "ver\u00b7l\u00e4\u00dft", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem heut nach mancher stummen Klage,", "tokens": ["Dem", "heut", "nach", "man\u00b7cher", "stum\u00b7men", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nach mancher kummervollen Nacht,", "tokens": ["Nach", "man\u00b7cher", "kum\u00b7mer\u00b7vol\u00b7len", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem heut nach manchem tr\u00fcben Tage", "tokens": ["Dem", "heut", "nach", "man\u00b7chem", "tr\u00fc\u00b7ben", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Sonne der Erf\u00fcllung lacht!", "tokens": ["Die", "Son\u00b7ne", "der", "Er\u00b7f\u00fcl\u00b7lung", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Abschiedsfest \u2013 und dennoch kr\u00e4nze", "tokens": ["Ein", "Ab\u00b7schieds\u00b7fest", "\u2013", "und", "den\u00b7noch", "kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Hoffnung dieses letzte Mahl!", "tokens": ["Die", "Hoff\u00b7nung", "die\u00b7ses", "letz\u00b7te", "Mahl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Abschiedsfest \u2013 und doch kredenze", "tokens": ["Ein", "Ab\u00b7schieds\u00b7fest", "\u2013", "und", "doch", "kre\u00b7den\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "KON", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Freude l\u00e4chelnd den Pokal!", "tokens": ["Die", "Freu\u00b7de", "l\u00e4\u00b7chelnd", "den", "Po\u00b7kal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn den mit Schmerzen wir verlieren,", "tokens": ["Denn", "den", "mit", "Schmer\u00b7zen", "wir", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Den heut die G\u00f6tter uns entziehn,", "tokens": ["Den", "heut", "die", "G\u00f6t\u00b7ter", "uns", "ent\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihn nennt die Welt aufs neu den ihren,", "tokens": ["Ihn", "nennt", "die", "Welt", "aufs", "neu", "den", "ih\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJD", "ART", "PPOSS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und die Geschichte fordert ihn.", "tokens": ["Und", "die", "Ge\u00b7schich\u00b7te", "for\u00b7dert", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du gehst, o Freund, aus unsrer Mitte,", "tokens": ["Du", "gehst", ",", "o", "Freund", ",", "aus", "uns\u00b7rer", "Mit\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du, von dem Schicksal selbst geweiht,", "tokens": ["Du", ",", "von", "dem", "Schick\u00b7sal", "selbst", "ge\u00b7weiht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit m\u00e4nnlichem, mit tapferm Schritte,", "tokens": ["Mit", "m\u00e4nn\u00b7li\u00b7chem", ",", "mit", "tap\u00b7ferm", "Schrit\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du gehst aufs Schlachtfeld unsrer Zeit.", "tokens": ["Du", "gehst", "aufs", "Schlacht\u00b7feld", "uns\u00b7rer", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du wei\u00dft es selbst: die Welt will Fehde,", "tokens": ["Du", "wei\u00dft", "es", "selbst", ":", "die", "Welt", "will", "Feh\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "VMFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Friedens \u00d6lblatt ist verdorrt \u2013", "tokens": ["Des", "Frie\u00b7dens", "\u00d6l\u00b7blatt", "ist", "ver\u00b7dorrt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wohlan, auch du mit freier Rede,", "tokens": ["Wo\u00b7hlan", ",", "auch", "du", "mit", "frei\u00b7er", "Re\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O k\u00e4mpf auch du mit freiem Wort!", "tokens": ["O", "k\u00e4mpf", "auch", "du", "mit", "frei\u00b7em", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Geh hin, o Freund \u2013 bei deinem Namen,", "tokens": ["Geh", "hin", ",", "o", "Freund", "\u2013", "bei", "dei\u00b7nem", "Na\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "FM", "NN", "$(", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie werden alle Herzen weit!", "tokens": ["Wie", "wer\u00b7den", "al\u00b7le", "Her\u00b7zen", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geh hin, o Freund, und streu den Samen,", "tokens": ["Geh", "hin", ",", "o", "Freund", ",", "und", "streu", "den", "Sa\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "FM", "NN", "$,", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den k\u00f6stlichen, der k\u00fcnft'gen Zeit!", "tokens": ["Den", "k\u00f6st\u00b7li\u00b7chen", ",", "der", "k\u00fcnft'\u00b7gen", "Zeit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sei ein Pilot im Sturm der Wogen,", "tokens": ["Sei", "ein", "Pi\u00b7lot", "im", "Sturm", "der", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Blitz, der durch die Wolken bricht,", "tokens": ["Ein", "Blitz", ",", "der", "durch", "die", "Wol\u00b7ken", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sei du ein Stern am Himmelsbogen,", "tokens": ["Sei", "du", "ein", "Stern", "am", "Him\u00b7mels\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ja sei du selbst! mehr braucht es nicht. \u2013", "tokens": ["Ja", "sei", "du", "selbst", "!", "mehr", "braucht", "es", "nicht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "VAFIN", "PPER", "ADV", "$.", "ADV", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dem alle Herzen \u00e4ngstlich schlagen,", "tokens": ["Dem", "al\u00b7le", "Her\u00b7zen", "\u00e4ngst\u00b7lich", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den die Orakel prophezein,", "tokens": ["Den", "die", "O\u00b7ra\u00b7kel", "pro\u00b7phe\u00b7zein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er mu\u00df ja doch, er mu\u00df ja tagen,", "tokens": ["Er", "mu\u00df", "ja", "doch", ",", "er", "mu\u00df", "ja", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "$,", "PPER", "VMFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tag der Zukunft bricht herein!", "tokens": ["Der", "Tag", "der", "Zu\u00b7kunft", "bricht", "her\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Hort der Freiheit wird gehoben,", "tokens": ["Der", "Hort", "der", "Frei\u00b7heit", "wird", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Turm des Rechtes soll bestehn,", "tokens": ["Der", "Turm", "des", "Rech\u00b7tes", "soll", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und \u00fcber alle, hoch von oben,", "tokens": ["Und", "\u00fc\u00b7ber", "al\u00b7le", ",", "hoch", "von", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "$,", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das Banner des Gesetzes wehn!", "tokens": ["Das", "Ban\u00b7ner", "des", "Ge\u00b7set\u00b7zes", "wehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So la\u00dft uns froh die Gl\u00e4ser leeren,", "tokens": ["So", "la\u00dft", "uns", "froh", "die", "Gl\u00e4\u00b7ser", "lee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dr\u00fcckt noch einmal ihm die Hand:", "tokens": ["Und", "dr\u00fcckt", "noch", "ein\u00b7mal", "ihm", "die", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es gilt dem Manne, den wir ehren,", "tokens": ["Es", "gilt", "dem", "Man\u00b7ne", ",", "den", "wir", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es gilt dem deutschen Vaterland!", "tokens": ["Es", "gilt", "dem", "deut\u00b7schen", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es gilt dem kommenden Geschlechte,", "tokens": ["Es", "gilt", "dem", "kom\u00b7men\u00b7den", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es gilt dem k\u00fcnft'gen Morgenrot,", "tokens": ["Es", "gilt", "dem", "k\u00fcnft'\u00b7gen", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Freiheit gilt es und dem Rechte,", "tokens": ["Der", "Frei\u00b7heit", "gilt", "es", "und", "dem", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es gilt dem Leben und dem Tod!", "tokens": ["Es", "gilt", "dem", "Le\u00b7ben", "und", "dem", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "In diesem Kreise trauter Zecher,", "tokens": ["In", "die\u00b7sem", "Krei\u00b7se", "trau\u00b7ter", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bei diesem bangen Abschiedsfest,", "tokens": ["Bei", "die\u00b7sem", "ban\u00b7gen", "Ab\u00b7schieds\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein letzter Gru\u00df, ein letzter Becher", "tokens": ["Ein", "letz\u00b7ter", "Gru\u00df", ",", "ein", "letz\u00b7ter", "Be\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Manne, der uns heut verl\u00e4\u00dft:", "tokens": ["Dem", "Man\u00b7ne", ",", "der", "uns", "heut", "ver\u00b7l\u00e4\u00dft", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem heut nach mancher stummen Klage,", "tokens": ["Dem", "heut", "nach", "man\u00b7cher", "stum\u00b7men", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct