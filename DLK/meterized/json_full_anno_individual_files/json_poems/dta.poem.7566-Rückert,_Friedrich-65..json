{"dta.poem.7566": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "65.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verein' mit Selbstvertraun Mistraun in deine Kraft;", "tokens": ["Ver\u00b7ein'", "mit", "Selbst\u00b7ver\u00b7traun", "Mis\u00b7traun", "in", "dei\u00b7ne", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "NE", "NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch stetes Ringen wird der Sch\u00fcler meisterhaft.", "tokens": ["Durch", "ste\u00b7tes", "Rin\u00b7gen", "wird", "der", "Sch\u00fc\u00b7ler", "meis\u00b7ter\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df du's noch nicht vollbracht, da\u00df du es kannst vollbringen,", "tokens": ["Da\u00df", "du's", "noch", "nicht", "voll\u00b7bracht", ",", "da\u00df", "du", "es", "kannst", "voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PTKNEG", "VVPP", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du's vollbringen mu\u00dft, das macht es dich erringen.", "tokens": ["Da\u00df", "du's", "voll\u00b7brin\u00b7gen", "mu\u00dft", ",", "das", "macht", "es", "dich", "er\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "$,", "PDS", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Aufl\u00f6sen mu\u00dft du erst, doch Alles ist das nicht,", "tokens": ["Auf\u00b7l\u00f6\u00b7sen", "mu\u00dft", "du", "erst", ",", "doch", "Al\u00b7les", "ist", "das", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "$,", "ADV", "PIS", "VAFIN", "PDS", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Glanz der Au\u00dfenwelt in innerliches Licht.", "tokens": ["Den", "Glanz", "der", "Au\u00b7\u00dfen\u00b7welt", "in", "in\u00b7ner\u00b7li\u00b7ches", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Entfalten mu\u00dft du dann, und dieses ist der Kranz,", "tokens": ["Ent\u00b7fal\u00b7ten", "mu\u00dft", "du", "dann", ",", "und", "die\u00b7ses", "ist", "der", "Kranz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "$,", "KON", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das innerliche Licht in \u00e4u\u00dferlichen Glanz.", "tokens": ["Das", "in\u00b7ner\u00b7li\u00b7che", "Licht", "in", "\u00e4u\u00b7\u00dfer\u00b7li\u00b7chen", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du mu\u00dft die fremde Welt in deinen Busen fassen,", "tokens": ["Du", "mu\u00dft", "die", "frem\u00b7de", "Welt", "in", "dei\u00b7nen", "Bu\u00b7sen", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um als die eigne dann sie sch\u00f6ner zu entlassen.", "tokens": ["Um", "als", "die", "eig\u00b7ne", "dann", "sie", "sch\u00f6\u00b7ner", "zu", "ent\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "KOKOM", "ART", "ADJA", "ADV", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Das sagt dir der Poet, auch wenn du keiner bist,", "tokens": ["Das", "sagt", "dir", "der", "Po\u00b7et", ",", "auch", "wenn", "du", "kei\u00b7ner", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "KOUS", "PPER", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil doch die Poesie ein Bild des Lebens ist.", "tokens": ["Weil", "doch", "die", "Poe\u00b7sie", "ein", "Bild", "des", "Le\u00b7bens", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Die Dichtkunst m\u00f6gest du als Kunst des Lebens brauchen,", "tokens": ["Die", "Dicht\u00b7kunst", "m\u00f6\u00b7gest", "du", "als", "Kunst", "des", "Le\u00b7bens", "brau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "KOUS", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um recht dich in die Welt, die Welt in dich zu tauchen.", "tokens": ["Um", "recht", "dich", "in", "die", "Welt", ",", "die", "Welt", "in", "dich", "zu", "tau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PRF", "APPR", "ART", "NN", "$,", "ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Auf! wenn dein Bau dir selbst und andern sch\u00f6n soll deuchten,", "tokens": ["Auf", "!", "wenn", "dein", "Bau", "dir", "selbst", "und", "an\u00b7dern", "sch\u00f6n", "soll", "deuch\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "KOUS", "PPOSAT", "NN", "PPER", "ADV", "KON", "ADJA", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mische recht den Stoff des Trocknen und des Feuchten.", "tokens": ["So", "mi\u00b7sche", "recht", "den", "Stoff", "des", "Trock\u00b7nen", "und", "des", "Feuch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "So mischet Bl\u00fctenstaub die Bien' und Honigseim,", "tokens": ["So", "mi\u00b7schet", "Bl\u00fc\u00b7ten\u00b7staub", "die", "Bi\u00b7en'", "und", "Ho\u00b7ni\u00b7gseim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und baut die Zelle, wie der Dichter seinen Reim.", "tokens": ["Und", "baut", "die", "Zel\u00b7le", ",", "wie", "der", "Dich\u00b7ter", "sei\u00b7nen", "Reim", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Sieh, was das Trockne sei, und was das Feuchte, schau!", "tokens": ["Sieh", ",", "was", "das", "Trock\u00b7ne", "sei", ",", "und", "was", "das", "Feuch\u00b7te", ",", "schau", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VAFIN", "$,", "KON", "PWS", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Wissen ist der Staub, und das Gem\u00fcth der Thau.", "tokens": ["Das", "Wis\u00b7sen", "ist", "der", "Staub", ",", "und", "das", "Ge\u00b7m\u00fcth", "der", "Thau", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}