{"dta.poem.3104": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "149.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Geist kann nicht umhin, aus allem was gelungen", "tokens": ["Dein", "Geist", "kann", "nicht", "um\u00b7hin", ",", "aus", "al\u00b7lem", "was", "ge\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PTKNEG", "PTKVZ", "$,", "APPR", "PIS", "PWS", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu sehn ihm ist, sofort zu ziehn Schlu\u00dffolgerungen,", "tokens": ["Zu", "sehn", "ihm", "ist", ",", "so\u00b7fort", "zu", "ziehn", "Schlu\u00df\u00b7fol\u00b7ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "VAFIN", "$,", "ADV", "PTKZU", "VVINF", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und sie auf alles Ungesehne zu erstrecken,", "tokens": ["Und", "sie", "auf", "al\u00b7les", "Un\u00b7ge\u00b7seh\u00b7ne", "zu", "er\u00b7stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um, wenn er dis dann sieht, den Fehlschlu\u00df zu entdecken.", "tokens": ["Um", ",", "wenn", "er", "dis", "dann", "sieht", ",", "den", "Fehl\u00b7schlu\u00df", "zu", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "$,", "KOUS", "PPER", "PDS", "ADV", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "La\u00df dich den Schlu\u00df zur\u00fcck zu nehmen nicht verdrie\u00dfen,", "tokens": ["La\u00df", "dich", "den", "Schlu\u00df", "zu\u00b7r\u00fcck", "zu", "neh\u00b7men", "nicht", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um, was du neu gesehn, nun auch mit einzuschlie\u00dfen!", "tokens": ["Um", ",", "was", "du", "neu", "ge\u00b7sehn", ",", "nun", "auch", "mit", "ein\u00b7zu\u00b7schlie\u00b7\u00dfen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "$,", "PWS", "PPER", "ADJD", "VVPP", "$,", "ADV", "ADV", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nie falsch ist, was dein Geist sich bei den Dingen denkt;", "tokens": ["Nie", "falsch", "ist", ",", "was", "dein", "Geist", "sich", "bei", "den", "Din\u00b7gen", "denkt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es gilt nur nicht, wie du wol meinst, uneingeschrenkt.", "tokens": ["Es", "gilt", "nur", "nicht", ",", "wie", "du", "wol", "meinst", ",", "un\u00b7ein\u00b7ge\u00b7schrenkt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "PPER", "ADV", "ADV", "$,", "ADJD", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}