{"textgrid.poem.36268": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Kind an die Mutter", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich komme dich zu bitten,", "tokens": ["Ich", "kom\u00b7me", "dich", "zu", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du liebe Liebe,", "tokens": ["Du", "lie\u00b7be", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "O la\u00df dich gr\u00fcn umkr\u00e4nzen", "tokens": ["O", "la\u00df", "dich", "gr\u00fcn", "um\u00b7kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von deinem Kinde.", "tokens": ["Von", "dei\u00b7nem", "Kin\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Weg mit dem dunklen Schleier,", "tokens": ["Weg", "mit", "dem", "dunk\u00b7len", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Dann bist du sch\u00f6ner;", "tokens": ["Dann", "bist", "du", "sch\u00f6\u00b7ner", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und schau die s\u00fc\u00dfen Bl\u00fcten,", "tokens": ["Und", "schau", "die", "s\u00fc\