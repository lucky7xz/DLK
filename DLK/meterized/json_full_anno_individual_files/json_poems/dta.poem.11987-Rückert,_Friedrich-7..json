{"dta.poem.11987": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es war ein reicher Mann, der in der W\u00fcste Mitten", "tokens": ["Es", "war", "ein", "rei\u00b7cher", "Mann", ",", "der", "in", "der", "W\u00fcs\u00b7te", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Labehaus gebaut f\u00fcr die des Weges schritten.", "tokens": ["Ein", "La\u00b7be\u00b7haus", "ge\u00b7baut", "f\u00fcr", "die", "des", "We\u00b7ges", "schrit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und ein Verwalter war von ihm darein gesetzt,", "tokens": ["Und", "ein", "Ver\u00b7wal\u00b7ter", "war", "von", "ihm", "da\u00b7rein", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "PPER", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der jeden Durstigen mit frischem Trunke letzt.", "tokens": ["Der", "je\u00b7den", "Durs\u00b7ti\u00b7gen", "mit", "fri\u00b7schem", "Trun\u00b7ke", "letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und von den Wanderern die meisten dankbar priesen", "tokens": ["Und", "von", "den", "Wan\u00b7de\u00b7rern", "die", "meis\u00b7ten", "dank\u00b7bar", "prie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "VVFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Schenker, nicht den Herrn, ders eingeschenkt durch diesen.", "tokens": ["Den", "Schen\u00b7ker", ",", "nicht", "den", "Herrn", ",", "ders", "ein\u00b7ge\u00b7schenkt", "durch", "die\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "ART", "NN", "$,", "ADV", "VVFIN", "APPR", "PDAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Kaum einer dachte, was er jenem schuldig sei,", "tokens": ["Kaum", "ei\u00b7ner", "dach\u00b7te", ",", "was", "er", "je\u00b7nem", "schul\u00b7dig", "sei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$,", "PWS", "PPER", "PDAT", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dacht' ers, so verga\u00df er diesen dann dabei.", "tokens": ["Und", "dacht'", "ers", ",", "so", "ver\u00b7ga\u00df", "er", "die\u00b7sen", "dann", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "ADV", "VVFIN", "PPER", "PDS", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die schlimmsten waren doch, die ihren Trunk empfiengen,", "tokens": ["Die", "schlimms\u00b7ten", "wa\u00b7ren", "doch", ",", "die", "ih\u00b7ren", "Trunk", "emp\u00b7fi\u00b7en\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und ohne Dank f\u00fcr den und jenen weiter giengen.", "tokens": ["Und", "oh\u00b7ne", "Dank", "f\u00fcr", "den", "und", "je\u00b7nen", "wei\u00b7ter", "gien\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "ART", "KON", "PDS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch nicht der Reiche noch sein Gutsverwalter lie\u00dfen", "tokens": ["Doch", "nicht", "der", "Rei\u00b7che", "noch", "sein", "Guts\u00b7ver\u00b7wal\u00b7ter", "lie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "NE", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Danklose Durstige zu laben sich verdrie\u00dfen.", "tokens": ["Dank\u00b7lo\u00b7se", "Durs\u00b7ti\u00b7ge", "zu", "la\u00b7ben", "sich", "ver\u00b7drie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "PRF", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.7": {"line.1": {"text": "Wer ist der reiche Mann? dort Gott der ewige,", "tokens": ["Wer", "ist", "der", "rei\u00b7che", "Mann", "?", "dort", "Gott", "der", "e\u00b7wi\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$.", "ADV", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sein Verwalter hier jeder Freigebige.", "tokens": ["Und", "sein", "Ver\u00b7wal\u00b7ter", "hier", "je\u00b7der", "Frei\u00b7ge\u00b7bi\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}