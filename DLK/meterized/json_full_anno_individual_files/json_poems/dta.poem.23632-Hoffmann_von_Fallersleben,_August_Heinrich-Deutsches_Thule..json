{"dta.poem.23632": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Deutsches Thule.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ruht des Landes Ruhm und Kraft", "tokens": ["Es", "ruht", "des", "Lan\u00b7des", "Ruhm", "und", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In goldnen Vlie\u00dfen und Aehren,", "tokens": ["In", "gold\u00b7nen", "Vlie\u00b7\u00dfen", "und", "A\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und unsre Kunst und Wissenschaft", "tokens": ["Und", "uns\u00b7re", "Kunst", "und", "Wis\u00b7sen\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gilt nur den Huben und St\u00e4hren.", "tokens": ["Gilt", "nur", "den", "Hu\u00b7ben", "und", "St\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Was Opitz sang, war Phantasie,", "tokens": ["Was", "O\u00b7pitz", "sang", ",", "war", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "$,", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt geht in unsere Gauen,", "tokens": ["Jetzt", "geht", "in", "un\u00b7se\u00b7re", "Gau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die wahre Sch\u00e4ferpoesie", "tokens": ["Die", "wah\u00b7re", "Sch\u00e4\u00b7fer\u00b7po\u00b7e\u00b7sie"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist da lebendig zu schauen.", "tokens": ["Ist", "da", "le\u00b7ben\u00b7dig", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "O komm zu uns, du deutscher Christ,", "tokens": ["O", "komm", "zu", "uns", ",", "du", "deut\u00b7scher", "Christ", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und geh bei uns in die Schule,", "tokens": ["Und", "geh", "bei", "uns", "in", "die", "Schu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und lern', ob es so \u00fcbel ist", "tokens": ["Und", "lern'", ",", "ob", "es", "so", "\u00fc\u00b7bel", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier in der ", "tokens": ["Hier", "in", "der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ART"], "meter": "++-", "measure": "unknown.measure.di"}}}}}