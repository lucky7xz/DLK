{"textgrid.poem.53183": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Anbindt an Hn. Johann Heinrich Kolhan\u00df, Juc.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm her, du ehrlicher Herr Kohlhans, la\u00df dich binden,", "tokens": ["Komm", "her", ",", "du", "ehr\u00b7li\u00b7cher", "Herr", "Kohl\u00b7hans", ",", "la\u00df", "dich", "bin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "ADJD", "NN", "NE", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du kennst der Freundschafft Art, nim dieses schlechte Band", "tokens": ["Du", "kennst", "der", "Freund\u00b7schafft", "Art", ",", "nim", "die\u00b7ses", "schlech\u00b7te", "Band"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "VVIMP", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vorlieb vnd kn\u00fcpff es fest vmb deine werthe Hand", "tokens": ["Vor\u00b7lieb", "vnd", "kn\u00fcpff", "es", "fest", "vmb", "dei\u00b7ne", "wert\u00b7he", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd wei\u00df, da\u00df Lieb vnd Trew es dir zu ehren winden.", "tokens": ["Vnd", "wei\u00df", ",", "da\u00df", "Lieb", "vnd", "Trew", "es", "dir", "zu", "eh\u00b7ren", "win\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "NN", "KON", "NN", "PPER", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wirst du dich aber auch zu l\u00f6sen Mittel finden?", "tokens": ["Wirst", "du", "dich", "a\u00b7ber", "auch", "zu", "l\u00f6\u00b7sen", "Mit\u00b7tel", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "ADV", "PTKZU", "VVINF", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit nichten. Diese Schnur geht \u00fcber Diamant,", "tokens": ["Mit", "nich\u00b7ten", ".", "Die\u00b7se", "Schnur", "geht", "\u00fc\u00b7ber", "Di\u00b7a\u00b7mant", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$.", "PDAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie ist der Redligkeit vnzweiffelhafftes Pfand,", "tokens": ["Sie", "ist", "der", "Red\u00b7lig\u00b7keit", "vn\u00b7zweif\u00b7fel\u00b7haff\u00b7tes", "Pfand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die gegen dich bey vn\u00df, ob Gott wil, nicht sol schwinden.", "tokens": ["Die", "ge\u00b7gen", "dich", "bey", "vn\u00df", ",", "ob", "Gott", "wil", ",", "nicht", "sol", "schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "APPR", "NE", "$,", "KOUS", "NN", "VMFIN", "$,", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Inmittelst ist vn\u00df gnug, wenn du, du fromme Haut,", "tokens": ["In\u00b7mit\u00b7telst", "ist", "vn\u00df", "gnug", ",", "wenn", "du", ",", "du", "from\u00b7me", "Haut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ADV", "$,", "KOUS", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich bey vn\u00df fr\u00f6lich machst vnd schl\u00e4gest deine Laut,", "tokens": ["Dich", "bey", "vn\u00df", "fr\u00f6\u00b7lich", "machst", "vnd", "schl\u00e4\u00b7gest", "dei\u00b7ne", "Laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "ADJD", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im fall dieselbe nur noch endlich ist bezogen.", "tokens": ["Im", "fall", "die\u00b7sel\u00b7be", "nur", "noch", "end\u00b7lich", "ist", "be\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "ADV", "ADV", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dann soll dein sch\u00f6nes Fest geehrt seyn durch den Wein,", "tokens": ["Dann", "soll", "dein", "sch\u00f6\u00b7nes", "Fest", "ge\u00b7ehrt", "seyn", "durch", "den", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "NN", "VVPP", "VAINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich, leb ich, werd allhie wol nicht der letzte seyn,", "tokens": ["Ich", ",", "leb", "ich", ",", "werd", "all\u00b7hie", "wol", "nicht", "der", "letz\u00