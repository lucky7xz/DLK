{"dta.poem.2914": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "113.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer sich als Menschen f\u00fchlt, und tief in sich empfindet,", "tokens": ["Wer", "sich", "als", "Men\u00b7schen", "f\u00fchlt", ",", "und", "tief", "in", "sich", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "KOUS", "NN", "VVFIN", "$,", "KON", "ADJD", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df mit der Menschheit ihn die Menschlichkeit verbindet,", "tokens": ["Da\u00df", "mit", "der", "Menschheit", "ihn", "die", "Menschlich\u00b7keit", "ver\u00b7bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Der wird nicht wollen, wird nicht k\u00f6nnen auch, die Leiden", "tokens": ["Der", "wird", "nicht", "wol\u00b7len", ",", "wird", "nicht", "k\u00f6n\u00b7nen", "auch", ",", "die", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "PTKNEG", "VMFIN", "$,", "VAFIN", "PTKNEG", "VMFIN", "ADV", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Freuden des Geschlechts von seinen eignen scheiden.", "tokens": ["Und", "Freu\u00b7den", "des", "Ge\u00b7schlechts", "von", "sei\u00b7nen", "eig\u00b7nen", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wes irgend einer vom Geschlecht sich freut' und litt,", "tokens": ["Wes", "ir\u00b7gend", "ei\u00b7ner", "vom", "Ge\u00b7schlecht", "sich", "freut'", "und", "litt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "APPRART", "NN", "PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Mitfreuen wird es ihn, und leiden wird ers mit.", "tokens": ["Mit\u00b7freu\u00b7en", "wird", "es", "ihn", ",", "und", "lei\u00b7den", "wird", "ers", "mit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "$,", "KON", "VVINF", "VAFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch Freud' ist Geistesthat, zur Freud' ist er berufen;", "tokens": ["Doch", "Freud'", "ist", "Geis\u00b7test\u00b7hat", ",", "zur", "Freud'", "ist", "er", "be\u00b7ru\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NN", "$,", "APPRART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Thor nur glaubt, da\u00df ihn zum Leiden G\u00f6tter schufen.", "tokens": ["Ein", "Thor", "nur", "glaubt", ",", "da\u00df", "ihn", "zum", "Lei\u00b7den", "G\u00f6t\u00b7ter", "schu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Vernunft will freie That; wer ihre Stimme h\u00f6rt,", "tokens": ["Ver\u00b7nunft", "will", "frei\u00b7e", "That", ";", "wer", "ih\u00b7re", "Stim\u00b7me", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADJA", "NN", "$.", "PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "R\u00e4umt freudig weg, was ihm Freiheit und Freude st\u00f6rt,", "tokens": ["R\u00e4umt", "freu\u00b7dig", "weg", ",", "was", "ihm", "Frei\u00b7heit", "und", "Freu\u00b7de", "st\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$,", "PWS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.6": {"line.1": {"text": "R\u00e4umt weg die Leidenschaft, und mit ihr seine Leiden;", "tokens": ["R\u00e4umt", "weg", "die", "Lei\u00b7den\u00b7schaft", ",", "und", "mit", "ihr", "sei\u00b7ne", "Lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,", "KON", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird er nun auch darum den Anblick fremder meiden?", "tokens": ["Wird", "er", "nun", "auch", "da\u00b7rum", "den", "An\u00b7blick", "frem\u00b7der", "mei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PAV", "ART", "NN", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ja, wenn er, dumpf genug, nicht f\u00fchlt, was er nicht sieht,", "tokens": ["Ja", ",", "wenn", "er", ",", "dumpf", "ge\u00b7nug", ",", "nicht", "f\u00fchlt", ",", "was", "er", "nicht", "sieht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "$,", "ADJD", "ADV", "$,", "PTKNEG", "VVFIN", "$,", "PWS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch der Vorstellung mit dem Anblick sich entzieht.", "tokens": ["Auch", "der", "Vor\u00b7stel\u00b7lung", "mit", "dem", "An\u00b7blick", "sich", "ent\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-++-+-+-+-+", "measure": "unknown.measure.septa"}}, "stanza.8": {"line.1": {"text": "Viel lieber k\u00e4mpfen wird er mit des Geistes Waffen,", "tokens": ["Viel", "lie\u00b7ber", "k\u00e4mp\u00b7fen", "wird", "er", "mit", "des", "Geis\u00b7tes", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VAFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Leiden frei wie sich auch andere zu schaffen.", "tokens": ["Vom", "Lei\u00b7den", "frei", "wie", "sich", "auch", "an\u00b7de\u00b7re", "zu", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KOKOM", "PRF", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Hat er in sich bekriegt das Leid und es besiegt,", "tokens": ["Hat", "er", "in", "sich", "be\u00b7kriegt", "das", "Leid", "und", "es", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PRF", "VVFIN", "ART", "NN", "KON", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df \u00fcberwunden es zum Fu\u00df der Freude liegt;", "tokens": ["Da\u00df", "\u00fc\u00b7berw\u00b7un\u00b7den", "es", "zum", "Fu\u00df", "der", "Freu\u00b7de", "liegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "So wird er ihren Krieg auch andern helfen kriegen,", "tokens": ["So", "wird", "er", "ih\u00b7ren", "Krieg", "auch", "an\u00b7dern", "hel\u00b7fen", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "PIS", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie, von seinem Sieg gest\u00e4rkt, sich selbst besiegen.", "tokens": ["Da\u00df", "sie", ",", "von", "sei\u00b7nem", "Sieg", "ge\u00b7st\u00e4rkt", ",", "sich", "selbst", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Nicht weil er f\u00fchlt, da\u00df ers in sich allein vollbracht,", "tokens": ["Nicht", "weil", "er", "f\u00fchlt", ",", "da\u00df", "ers", "in", "sich", "al\u00b7lein", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PIS", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird er die schw\u00e4cheren verlassen in der Schlacht.", "tokens": ["Wird", "er", "die", "schw\u00e4\u00b7che\u00b7ren", "ver\u00b7las\u00b7sen", "in", "der", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Wes er sich selb sch\u00e4mt, wird er sich f\u00fcr sie nicht sch\u00e4men,", "tokens": ["Wes", "er", "sich", "selb", "sch\u00e4mt", ",", "wird", "er", "sich", "f\u00fcr", "sie", "nicht", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "ADJD", "VVFIN", "$,", "VAFIN", "PPER", "PRF", "APPR", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Mit Freuden wird er Theil an fremden Leiden nehmen:", "tokens": ["Mit", "Freu\u00b7den", "wird", "er", "Theil", "an", "frem\u00b7den", "Lei\u00b7den", "neh\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Ob er den Gipfel auch der G\u00f6ttlichkeit erstiegen,", "tokens": ["Ob", "er", "den", "Gip\u00b7fel", "auch", "der", "G\u00f6tt\u00b7lich\u00b7keit", "er\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Erdendunstgew\u00f6lk' in Aetherduft verfliegen;", "tokens": ["Wo", "Er\u00b7den\u00b7dunst\u00b7ge\u00b7w\u00f6l\u00b7k'", "in", "A\u00b7e\u00b7ther\u00b7duft", "ver\u00b7flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+++-+-+-", "measure": "unknown.measure.octa.plus"}}, "stanza.14": {"line.1": {"text": "Um wie vielmehr wenn er sich sagen mu\u00df, er sei", "tokens": ["Um", "wie", "viel\u00b7mehr", "wenn", "er", "sich", "sa\u00b7gen", "mu\u00df", ",", "er", "sei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUI", "KOKOM", "ADV", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch selbst von Leiden nicht und Leidenschaften frei.", "tokens": ["Noch", "selbst", "von", "Lei\u00b7den", "nicht", "und", "Lei\u00b7den\u00b7schaf\u00b7ten", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "PTKNEG", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}