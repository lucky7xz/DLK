{"textgrid.poem.40586": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbin Shannon's Fluth, am Feenpalaste,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbin Shannon's Fluth, am Feenpalaste,", "tokens": ["\u00bb", "in", "Shan\u00b7non's", "Fluth", ",", "am", "Fe\u00b7en\u00b7pa\u00b7las\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "NE", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist Gold das Dach und Kristall die Wand,", "tokens": ["Ist", "Gold", "das", "Dach", "und", "Kris\u00b7tall", "die", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die schlanken S\u00e4ulen sind silberne Maste,", "tokens": ["Die", "schlan\u00b7ken", "S\u00e4u\u00b7len", "sind", "sil\u00b7ber\u00b7ne", "Mas\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und jede Scheib' ein geschliffner Demant.", "tokens": ["Und", "je\u00b7de", "Scheib'", "ein", "ge\u00b7schliff\u00b7ner", "De\u00b7mant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nun horcht fein auf, ihr Jungen!\u00ab", "tokens": ["Nun", "horcht", "fein", "auf", ",", "ihr", "Jun\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "An Shannon's Bord steht, Einsturz drohend,", "tokens": ["An", "Shan\u00b7non's", "Bord", "steht", ",", "Ein\u00b7sturz", "dro\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Bau von Erde, wie f\u00fcr den Dachs", "tokens": ["Ein", "Bau", "von", "Er\u00b7de", ",", "wie", "f\u00fcr", "den", "Dachs"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Am Boden ein B\u00fcndel Reisig lohend,", "tokens": ["Am", "Bo\u00b7den", "ein", "B\u00fcn\u00b7del", "Rei\u00b7sig", "lo\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NE", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da wohnt der arme Pfeifer des Sacks.", "tokens": ["Da", "wohnt", "der", "ar\u00b7me", "Pfei\u00b7fer", "des", "Sacks", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und weiter erz\u00e4hlt er den Kindern:", "tokens": ["Und", "wei\u00b7ter", "er\u00b7z\u00e4hlt", "er", "den", "Kin\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbholdselige Fee aus K\u00f6nigsgeschlechten!", "tokens": ["\u00bb", "hold\u00b7se\u00b7li\u00b7ge", "Fee", "aus", "K\u00f6\u00b7nigs\u00b7ge\u00b7schlech\u00b7ten", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+---+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "O Sch\u00f6nheit von Erins Blut und Schlag!", "tokens": ["O", "Sch\u00f6n\u00b7heit", "von", "E\u00b7rins", "Blut", "und", "Schlag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NE", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schwarz ist ihr Haar, wie sein Himmel in N\u00e4chten,", "tokens": ["Schwarz", "ist", "ihr", "Haar", ",", "wie", "sein", "Him\u00b7mel", "in", "N\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,", "PWAV", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Blau ist ihr Aug', wie sein Himmel am Tag.", "tokens": ["Blau", "ist", "ihr", "Aug'", ",", "wie", "sein", "Him\u00b7mel", "am", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "PWAV", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "So seid doch still, ihr Jungen!\u00ab", "tokens": ["So", "seid", "doch", "still", ",", "ihr", "Jun\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sein krankes Weib, in Lumpen zerrissen,", "tokens": ["Sein", "kran\u00b7kes", "Weib", ",", "in", "Lum\u00b7pen", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bes\u00e4nftigt schwer den S\u00e4ugling, der schreit;", "tokens": ["Be\u00b7s\u00e4nf\u00b7tigt", "schwer", "den", "S\u00e4ug\u00b7ling", ",", "der", "schreit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "An Mutterbr\u00fcsten schon darben m\u00fcssen!", "tokens": ["An", "Mut\u00b7ter\u00b7br\u00fcs\u00b7ten", "schon", "dar\u00b7ben", "m\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Entbehrung f\u00fcrs Leben lernt er bei Zeit!", "tokens": ["Ent\u00b7beh\u00b7rung", "f\u00fcrs", "Le\u00b7ben", "lernt", "er", "bei", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und weiter f\u00e4hrt der Spielmann:", "tokens": ["Und", "wei\u00b7ter", "f\u00e4hrt", "der", "Spiel\u00b7mann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbund Elfenkinder, rothwangige Kleine,", "tokens": ["\u00bb", "und", "El\u00b7fen\u00b7kin\u00b7der", ",", "roth\u00b7wan\u00b7gi\u00b7ge", "Klei\u00b7ne", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "KON", "NN", "$,", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gar liebliche Pagen, dienen der Fee,", "tokens": ["Gar", "lieb\u00b7li\u00b7che", "Pa\u00b7gen", ",", "die\u00b7nen", "der", "Fee", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr Wort ist Gesang, wie des Vogels im Haine,", "tokens": ["Ihr", "Wort", "ist", "Ge\u00b7sang", ",", "wie", "des", "Vo\u00b7gels", "im", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ihr Leib ist Glanz, wie der Weihnacht Schnee.", "tokens": ["Ihr", "Leib", "ist", "Glanz", ",", "wie", "der", "Weih\u00b7nacht", "Schnee", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "PWAV", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So haltet Fried', ihr Jungen!\u00ab", "tokens": ["So", "hal\u00b7tet", "Fried'", ",", "ihr", "Jun\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Am Schopfe zerrt der rothk\u00f6pfige Harry", "tokens": ["Am", "Schop\u00b7fe", "zerrt", "der", "roth\u00b7k\u00f6p\u00b7fi\u00b7ge", "Har\u00b7ry"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den pockennarbigen Jack, wie im Krampf,", "tokens": ["Den", "po\u00b7cken\u00b7nar\u00b7bi\u00b7gen", "Jack", ",", "wie", "im", "Krampf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dazwischen heult die schielende Mary;", "tokens": ["Da\u00b7zwi\u00b7schen", "heult", "die", "schie\u00b7len\u00b7de", "Ma\u00b7ry", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um eine Kartoffel ein Zwergenkampf!", "tokens": ["Um", "ei\u00b7ne", "Kar\u00b7tof\u00b7fel", "ein", "Zwer\u00b7gen\u00b7kampf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und weiter f\u00e4hrt der Alte:", "tokens": ["Und", "wei\u00b7ter", "f\u00e4hrt", "der", "Al\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbin ewiger Jugend der sch\u00f6nste Ritter", "tokens": ["\u00bb", "in", "e\u00b7wi\u00b7ger", "Ju\u00b7gend", "der", "sch\u00f6ns\u00b7te", "Rit\u00b7ter"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der holden Fee zu F\u00fc\u00dfen sitzt,", "tokens": ["Der", "hol\u00b7den", "Fee", "zu", "F\u00fc\u00b7\u00dfen", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von selbst ert\u00f6nt ihm zur Seite die Zither,", "tokens": ["Von", "selbst", "er\u00b7t\u00f6nt", "ihm", "zur", "Sei\u00b7te", "die", "Zi\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er schlummert, auf ihren Schoo\u00df gest\u00fctzt.", "tokens": ["Er", "schlum\u00b7mert", ",", "auf", "ih\u00b7ren", "Schoo\u00df", "ge\u00b7st\u00fctzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Was st\u00f6hnt ihr nun, ihr Jungen?\u00ab", "tokens": ["Was", "st\u00f6hnt", "ihr", "nun", ",", "ihr", "Jun\u00b7gen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ein Schnarchen der Kinder um die Wette!", "tokens": ["Ein", "Schnar\u00b7chen", "der", "Kin\u00b7der", "um", "die", "Wet\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nach hitzigen Schlachten Waffenruh!", "tokens": ["Nach", "hit\u00b7zi\u00b7gen", "Schlach\u00b7ten", "Waf\u00b7fen\u00b7ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Pfeifer selbst auf die harte St\u00e4tte", "tokens": ["Der", "Pfei\u00b7fer", "selbst", "auf", "die", "har\u00b7te", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sinkt todesmatt, als s\u00e4nk' er zur Truh',", "tokens": ["Sinkt", "to\u00b7des\u00b7matt", ",", "als", "s\u00e4nk'", "er", "zur", "Truh'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "KOUS", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und f\u00e4llt in Schlaf und Tr\u00e4umen:", "tokens": ["Und", "f\u00e4llt", "in", "Schlaf", "und", "Tr\u00e4u\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Er ist verwandelt! Er selbst der Ritter,", "tokens": ["Er", "ist", "ver\u00b7wan\u00b7delt", "!", "Er", "selbst", "der", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der zu den F\u00fc\u00dfen der Feie sitzt!", "tokens": ["Der", "zu", "den", "F\u00fc\u00b7\u00dfen", "der", "Fei\u00b7e", "sitzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Von selbst ert\u00f6nt ihm zur Seite die Zither,", "tokens": ["Von", "selbst", "er\u00b7t\u00f6nt", "ihm", "zur", "Sei\u00b7te", "die", "Zi\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er schlummert, auf ihren Schoo\u00df gest\u00fctzt,", "tokens": ["Er", "schlum\u00b7mert", ",", "auf", "ih\u00b7ren", "Schoo\u00df", "ge\u00b7st\u00fctzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Schl\u00e4gt auf zu ihr die Augen:", "tokens": ["Schl\u00e4gt", "auf", "zu", "ihr", "die", "Au\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbholdselige Fee, das war ein Bangen!", "tokens": ["\u00bb", "hold\u00b7se\u00b7li\u00b7ge", "Fee", ",", "das", "war", "ein", "Ban\u00b7gen", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Welch b\u00f6ser Traum! Noch bebt mein Leib!", "tokens": ["Welch", "b\u00f6\u00b7ser", "Traum", "!", "Noch", "bebt", "mein", "Leib", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die sanften Elfen unb\u00e4ndige Rangen!", "tokens": ["Die", "sanf\u00b7ten", "El\u00b7fen", "un\u00b7b\u00e4n\u00b7di\u00b7ge", "Ran\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein Bettler ich, du ein h\u00e4\u00dflich Weib,", "tokens": ["E