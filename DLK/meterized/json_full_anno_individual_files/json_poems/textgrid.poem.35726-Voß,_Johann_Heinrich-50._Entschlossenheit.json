{"textgrid.poem.35726": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "50. Entschlossenheit", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vorw\u00e4rts, mein Geist, den schroffen Pfad", "tokens": ["Vor\u00b7w\u00e4rts", ",", "mein", "Geist", ",", "den", "schrof\u00b7fen", "Pfad"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nicht tr\u00e4g umhergeschauet!", "tokens": ["Nicht", "tr\u00e4g", "um\u00b7her\u00b7ge\u00b7schau\u00b7et", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dort oben winkt die Ruhestatt!", "tokens": ["Dort", "o\u00b7ben", "winkt", "die", "Ru\u00b7he\u00b7statt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohlauf, dir selbst vertrauet!", "tokens": ["Wohl\u00b7auf", ",", "dir", "selbst", "ver\u00b7trau\u00b7et", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dich, Gottes Odem, du Verstand,", "tokens": ["Dich", ",", "Got\u00b7tes", "O\u00b7dem", ",", "du", "Ver\u00b7stand", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Staub geh\u00fcllt, hat Gottes Hand", "tokens": ["In", "Staub", "ge\u00b7h\u00fcllt", ",", "hat", "G