ch", "so", "schlimm", "Ge\u00b7fahr", "und", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verachtung selbst, ja schn\u00f6der Tod:", "tokens": ["Ver\u00b7ach\u00b7tung", "selbst", ",", "ja", "schn\u00f6\u00b7der", "Tod", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Unredlich sein ist schlimmer!", "tokens": ["Un\u00b7red\u00b7lich", "sein", "ist", "schlim\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wir m\u00fcssen, m\u00fcssen vorw\u00e4rts gehn,", "tokens": ["Wir", "m\u00fcs\u00b7sen", ",", "m\u00fcs\u00b7sen", "vor\u00b7w\u00e4rts", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Wahn und Trug auch toben!", "tokens": ["Wie", "Wahn", "und", "Trug", "auch", "to\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Uns hat, zum Himmel aufzusehn,", "tokens": ["Uns", "hat", ",", "zum", "Him\u00b7mel", "auf\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott selbst das Haupt erhoben!", "tokens": ["Gott", "selbst", "das", "Haupt", "er\u00b7ho\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum wank' und fall' es links und rechts:", "tokens": ["Drum", "wank'", "und", "fall'", "es", "links", "und", "rechts", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir sind unsterbliches Geschlechts;", "tokens": ["Wir", "sind", "uns\u00b7terb\u00b7li\u00b7ches", "Ge\u00b7schlechts", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Vaterland ist oben!", "tokens": ["Das", "Va\u00b7ter\u00b7land", "ist", "o\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ach, unsrer Heimat eingedenk,", "tokens": ["Ach", ",", "uns\u00b7rer", "Hei\u00b7mat", "ein\u00b7ge\u00b7denk", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00dft uns doch gehn wie Br\u00fcder,", "tokens": ["La\u00dft", "uns", "doch", "gehn", "wie", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVINF", "KOKOM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Lieb', ohn' Eifer und Gez\u00e4nk,", "tokens": ["In", "Lieb'", ",", "ohn'", "Ei\u00b7fer", "und", "Ge\u00b7z\u00e4nk", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "NN