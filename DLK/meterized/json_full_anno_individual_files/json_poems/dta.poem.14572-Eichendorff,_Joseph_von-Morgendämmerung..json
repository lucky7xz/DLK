{"dta.poem.14572": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Morgend\u00e4mmerung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein still Erwarten in den B\u00e4umen,", "tokens": ["Es", "ist", "ein", "still", "Er\u00b7war\u00b7ten", "in", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Nachtigallen in den B\u00fcschen schlagen", "tokens": ["Die", "Nach\u00b7ti\u00b7gal\u00b7len", "in", "den", "B\u00fc\u00b7schen", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In irren Klagen, k\u00f6nnen's doch nicht sagen,", "tokens": ["In", "ir\u00b7ren", "Kla\u00b7gen", ",", "k\u00f6n\u00b7nen's", "doch", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Schmerzen all' und Wonne, halb in Tr\u00e4umen.", "tokens": ["Die", "Schmer\u00b7zen", "all'", "und", "Won\u00b7ne", ",", "halb", "in", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "KON", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}