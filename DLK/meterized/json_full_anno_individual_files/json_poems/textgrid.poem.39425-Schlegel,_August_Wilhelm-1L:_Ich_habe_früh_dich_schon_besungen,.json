, ",", "eilt", "hin\u00b7zu", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erweckt sie aus der starren Ruh!", "tokens": ["Er\u00b7weckt", "sie", "aus", "der", "star\u00b7ren", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Nichts mehr von wilden Schlachtget\u00f6sen!", "tokens": ["Nichts", "mehr", "von", "wil\u00b7den", "Schlacht\u00b7ge\u00b7t\u00f6\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df dir den strengen Panzer l\u00f6sen.", "tokens": ["La\u00df", "dir", "den", "stren\u00b7gen", "Pan\u00b7zer", "l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O hielt' ihn nur ein seidnes Band,", "tokens": ["O", "hielt'", "ihn", "nur", "ein", "seid\u00b7nes", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ART", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie gern zerri\u00df' es meine Hand!", "tokens": ["Wie", "gern", "zer\u00b7ri\u00df'", "es", "mei\u00b7ne", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}