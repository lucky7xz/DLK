{"dta.poem.20125": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Einem Todten.   Wilhelm Krause starb zu Malaga 1842.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei Jahre kaum, als heitre Tr\u00e4ume scheuchten", "tokens": ["Zwei", "Jah\u00b7re", "kaum", ",", "als", "heit\u00b7re", "Tr\u00e4u\u00b7me", "scheuch\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADV", "$,", "KOUS", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Sorgen dunklen Schwarm aus Deiner Brust;", "tokens": ["Der", "Sor\u00b7gen", "dunk\u00b7len", "Schwarm", "aus", "Dei\u00b7ner", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du riefst \u201eAde!\u201c ich sah Dein Auge leuchten,", "tokens": ["Du", "riefst", "\u201e", "A\u00b7de", "!", "\u201c", "ich", "sah", "Dein", "Au\u00b7ge", "leuch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "NN", "$.", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und f\u00fchlte Thr\u00e4nen doch das meine feuchten,", "tokens": ["Und", "f\u00fchl\u00b7te", "Thr\u00e4\u00b7nen", "doch", "das", "mei\u00b7ne", "feuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ADV", "ART", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich war der ew\u2019gen Trennung mir bewu\u00dft.", "tokens": ["Ich", "war", "der", "ew'\u00b7gen", "Tren\u00b7nung", "mir", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mein armer Wilm, das Roth auf Deinen Wangen,", "tokens": ["Mein", "ar\u00b7mer", "Wilm", ",", "das", "Roth", "auf", "Dei\u00b7nen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es war das Kleid des frischen Lebens nicht,", "tokens": ["Es", "war", "das", "Kleid", "des", "fri\u00b7schen", "Le\u00b7bens", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Tod nur, sichrer Dich in\u2019s Netz zu fangen,", "tokens": ["Der", "Tod", "nur", ",", "sich\u00b7rer", "Dich", "in's", "Netz", "zu", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Lie\u00df Rosen bl\u00fchn auf Deinem Angesicht.", "tokens": ["Lie\u00df", "Ro\u00b7sen", "bl\u00fchn", "auf", "Dei\u00b7nem", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du sahst das Ro\u00df des Matadors sich b\u00e4umen,", "tokens": ["Du", "sahst", "das", "Ro\u00df", "des", "Ma\u00b7ta\u00b7dors", "sich", "b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eh\u2019 Deine Barke noch vom Ufer stie\u00df, \u2014", "tokens": ["Eh'", "Dei\u00b7ne", "Bar\u00b7ke", "noch", "vom", "U\u00b7fer", "stie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gen Spanien ging\u2019s, \u2014 Du durftest heiter tr\u00e4umen", "tokens": ["Gen", "Spa\u00b7ni\u00b7en", "ging's", ",", "Du", "durf\u00b7test", "hei\u00b7ter", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "$,", "$(", "PPER", "VVFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von duft\u2019gen Mandel- und Kastanienb\u00e4umen,", "tokens": ["Von", "duft'\u00b7gen", "Man\u00b7del", "und", "Kas\u00b7ta\u00b7ni\u00b7en\u00b7b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Denn Deine Zukunft barg ein Paradies.", "tokens": ["Denn", "Dei\u00b7ne", "Zu\u00b7kunft", "barg", "ein", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch statt vom Duft der Bl\u00fcthen zu gesunden,", "tokens": ["Doch", "statt", "vom", "Duft", "der", "Bl\u00fc\u00b7then", "zu", "ge\u00b7sun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Hat Dich der Hauch des Todes angeweht,", "tokens": ["Hat", "Dich", "der", "Hauch", "des", "To\u00b7des", "an\u00b7ge\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Als Sensenmann vor meiner Seele steht.", "tokens": ["Als", "Sen\u00b7sen\u00b7mann", "vor", "mei\u00b7ner", "See\u00b7le", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich sah ihn l\u00e4ngst Dich Schritt vor Schritt", "tokens": ["Ich", "sah", "ihn", "l\u00e4ngst", "Dich", "Schritt", "vor", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPER", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "bewachen,", "tokens": ["be\u00b7wa\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Gleich einem Schatten Dir zur Seite gehn,", "tokens": ["Gleich", "ei\u00b7nem", "Schat\u00b7ten", "Dir", "zur", "Sei\u00b7te", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Behende sprang er mit Dir in den Nachen,", "tokens": ["Be\u00b7hen\u00b7de", "sprang", "er", "mit", "Dir", "in", "den", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Und immer schien er h\u00f6hnisch mir zu lachen,", "tokens": ["Und", "im\u00b7mer", "schien", "er", "h\u00f6h\u00b7nisch", "mir", "zu", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So oft Du riefst: \u201eauf fr\u00f6hlich Wiedersehn!\u201c", "tokens": ["So", "oft", "Du", "riefst", ":", "\u201e", "auf", "fr\u00f6h\u00b7lich", "Wie\u00b7der\u00b7sehn", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$.", "$(", "APPR", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Auf Wiedersehn! wann, Freund? statt Herzens-", "tokens": ["Auf", "Wie\u00b7der\u00b7sehn", "!", "wann", ",", "Freund", "?", "statt", "Her\u00b7zens"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$.", "PWAV", "$,", "NN", "$.", "APPR", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "frieden", "tokens": ["frie\u00b7den"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Hat ew\u2019ge Ruh die Ferne Dir geschenkt,", "tokens": ["Hat", "ew'\u00b7ge", "Ruh", "die", "Fer\u00b7ne", "Dir", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und in die Gruft, die ", "tokens": ["Und", "in", "die", "Gruft", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "schieden,", "tokens": ["schie\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Hat man ", "tokens": ["Hat", "man"], "token_info": ["word", "word"], "pos": ["VAFIN", "PIS"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6n ist das Leben! ach, man lernt es lieben", "tokens": ["Sch\u00f6n", "ist", "das", "Le\u00b7ben", "!", "ach", ",", "man", "lernt", "es", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$.", "XY", "$,", "PIS", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Recht innig erst, wenn man es meiden soll,", "tokens": ["Recht", "in\u00b7nig", "erst", ",", "wenn", "man", "es", "mei\u00b7den", "soll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "$,", "KOUS", "PIS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch in die weite Welt hinaus getrieben,", "tokens": ["Doch", "in", "die", "wei\u00b7te", "Welt", "hin\u00b7aus", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APZR", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo fremd wie wir auch unser Herz geblieben,", "tokens": ["Wo", "fremd", "wie", "wir", "auch", "un\u00b7ser", "Herz", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KOKOM", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da wird der Tod uns doppelt qualenvoll.", "tokens": ["Da", "wird", "der", "Tod", "uns", "dop\u00b7pelt", "qua\u00b7len\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Auf welcher Wange sahst Du Thr\u00e4nen gl\u00e4nzen?", "tokens": ["Auf", "wel\u00b7cher", "Wan\u00b7ge", "sahst", "Du", "Thr\u00e4\u00b7nen", "gl\u00e4n\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wer hat Dein brechend Auge zugedr\u00fcckt?", "tokens": ["Wer", "hat", "Dein", "bre\u00b7chend", "Au\u00b7ge", "zu\u00b7ge\u00b7dr\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mein armer Wilm, mit Immortellenkr\u00e4nzen", "tokens": ["Mein", "ar\u00b7mer", "Wilm", ",", "mit", "Im\u00b7mor\u00b7tel\u00b7len\u00b7kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Hat fl\u00fccht\u2019ges Mitleid nur Dein Grab geschm\u00fcckt.", "tokens": ["Hat", "fl\u00fccht'\u00b7ges", "Mit\u00b7leid", "nur", "Dein", "Grab", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was half es Dir, da\u00df sch\u00f6ner dort die Rosen,", "tokens": ["Was", "half", "es", "Dir", ",", "da\u00df", "sch\u00f6\u00b7ner", "dort", "die", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$,", "KOUS", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und goldner selbst des Himmels Sterne gl\u00fchn?", "tokens": ["Und", "gold\u00b7ner", "selbst", "des", "Him\u00b7mels", "Ster\u00b7ne", "gl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nun gilt es gleich \u2014 ob rauhe St\u00fcrme tosen,", "tokens": ["Nun", "gilt", "es", "gleich", "ob", "rau\u00b7he", "St\u00fcr\u00b7me", "to\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ob linde Weste mit den Blumen kosen,", "tokens": ["Ob", "lin\u00b7de", "Wes\u00b7te", "mit", "den", "Blu\u00b7men", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit Blumen, Freund, die Deinem Grab entbl\u00fchn.", "tokens": ["Mit", "Blu\u00b7men", ",", "Freund", ",", "die", "Dei\u00b7nem", "Grab", "ent\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du ruhtest besser wohl am heim\u2019schen Strande,", "tokens": ["Du", "ruh\u00b7test", "bes\u00b7ser", "wohl", "am", "heim'\u00b7schen", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Im D\u00fcnensand, wo Du zu ruhn geglaubt:", "tokens": ["Im", "D\u00fc\u00b7nen\u00b7sand", ",", "wo", "Du", "zu", "ruhn", "ge\u00b7glaubt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein Ku\u00df der Liebe h\u00e4tt\u2019 im Vaterlande", "tokens": ["Ein", "Ku\u00df", "der", "Lie\u00b7be", "h\u00e4tt'", "im", "Va\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Dem Tode seinen Stachel noch geraubt.", "tokens": ["Dem", "To\u00b7de", "sei\u00b7nen", "Sta\u00b7chel", "noch", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch jetzt, wo Du den bittren Kampf bestanden,", "tokens": ["Doch", "jetzt", ",", "wo", "Du", "den", "bit\u00b7tren", "Kampf", "be\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", 