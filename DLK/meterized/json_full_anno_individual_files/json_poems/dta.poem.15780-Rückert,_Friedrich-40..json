{"dta.poem.15780": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Unvollkommenheit der Sprach' hab' ich verachtet,", "tokens": ["Die", "Un\u00b7voll\u00b7kom\u00b7men\u00b7heit", "der", "Sprach'", "hab'", "ich", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nach vollkommener, die ehmals war, geschmachtet.", "tokens": ["Und", "nach", "voll\u00b7kom\u00b7me\u00b7ner", ",", "die", "eh\u00b7mals", "war", ",", "ge\u00b7schmach\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "$,", "PRELS", "ADV", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Das g\u00f6ttliche Sanskrit ist im Prakrit gebrochen;", "tokens": ["Das", "g\u00f6tt\u00b7li\u00b7che", "Sans\u00b7krit", "ist", "im", "Pra\u00b7krit", "ge\u00b7bro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem\u00fcthig stammelt dis, wo jenes k\u00fchn gesprochen.", "tokens": ["De\u00b7m\u00fct\u00b7hig", "stam\u00b7melt", "dis", ",", "wo", "je\u00b7nes", "k\u00fchn", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PDS", "$,", "PWAV", "PDS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch dem Geschick der Sprach' und Herzen dacht' ich nach,", "tokens": ["Doch", "dem", "Ge\u00b7schick", "der", "Sprach'", "und", "Her\u00b7zen", "dacht'", "ich", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "KON", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bin zufrieden nun, da\u00df jener Stolz zerbrach.", "tokens": ["Und", "bin", "zu\u00b7frie\u00b7den", "nun", ",", "da\u00df", "je\u00b7ner", "Stolz", "zer\u00b7brach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ADV", "$,", "KOUS", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur Unvollkommenes kann den Vollkommnern preisen;", "tokens": ["Nur", "Un\u00b7voll\u00b7kom\u00b7me\u00b7nes", "kann", "den", "Voll\u00b7komm\u00b7nern", "prei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VMFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem\u00fcthig lasset uns ihm stammeln unsre Weisen.", "tokens": ["De\u00b7m\u00fct\u00b7hig", "las\u00b7set", "uns", "ihm", "stam\u00b7meln", "uns\u00b7re", "Wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}