{"dta.poem.11845": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Schmelz der Wiesen tr\u00e4nkt das Wasserrad nicht nur,", "tokens": ["Den", "Schmelz", "der", "Wie\u00b7sen", "tr\u00e4nkt", "das", "Was\u00b7ser\u00b7rad", "nicht", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist auch, fern gesehn, ein reger Schmuck der Flur.", "tokens": ["Es", "ist", "auch", ",", "fern", "ge\u00b7sehn", ",", "ein", "re\u00b7ger", "Schmuck", "der", "Flur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ADJD", "VVPP", "$,", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wenn du n\u00e4her k\u00f6mmst, h\u00f6rst du es traurig st\u00f6hnen:", "tokens": ["Doch", "wenn", "du", "n\u00e4\u00b7her", "k\u00f6mmst", ",", "h\u00f6rst", "du", "es", "trau\u00b7rig", "st\u00f6h\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schwer ist N\u00fctzliches vereinigen dem Sch\u00f6nen.", "tokens": ["So", "schwer", "ist", "N\u00fctz\u00b7li\u00b7ches", "ver\u00b7ei\u00b7ni\u00b7gen", "dem", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "