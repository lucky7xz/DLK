r die Reben", "tokens": ["Nun", "wi\u00dft", "ihr", ",", "wer", "die", "Re\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am Rhein uns hat gegeben.", "tokens": ["Am", "Rhein", "uns", "hat", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und will noch Einer wagen,", "tokens": ["Und", "will", "noch", "Ei\u00b7ner", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den heil'gen Wein zu schm\u00e4hn,", "tokens": ["Den", "heil'\u00b7gen", "Wein", "zu", "schm\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der soll in Wasserfluthen", "tokens": ["Der", "soll", "in", "Was\u00b7ser\u00b7flut\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erb\u00e4rmlich untergehn!", "tokens": ["Er\u00b7b\u00e4rm\u00b7lich", "un\u00b7ter\u00b7gehn", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sto\u00dft an und singt, ihr Br\u00fcder:", "tokens": ["Sto\u00dft", "an", "und", "singt", ",", "ihr", "Br\u00fc\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "