{"dta.poem.8718": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "ViII .  \n  Die Heimkehr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Zu Paris am K\u00f6nigsschlosse,", "tokens": ["Zu", "Pa\u00b7ris", "am", "K\u00f6\u00b7nigs\u00b7schlos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das der Prinz nunmehr bezogen,", "tokens": ["Das", "der", "Prinz", "nun\u00b7mehr", "be\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Harrt der Wagen lange Reihe,", "tokens": ["Harrt", "der", "Wa\u00b7gen", "lan\u00b7ge", "Rei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dr\u00e4ngen sich des Volkes Wogen.", "tokens": ["Dr\u00e4n\u00b7gen", "sich", "des", "Vol\u00b7kes", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf der kunstgeschm\u00fcckten Treppe", "tokens": ["Auf", "der", "kunst\u00b7ge\u00b7schm\u00fcck\u00b7ten", "Trep\u00b7pe"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehn die k\u00f6niglichen Garden,", "tokens": ["Stehn", "die", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Gar\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem Andrang des Volks zu wehren", "tokens": ["Dem", "An\u00b7drang", "des", "Volks", "zu", "weh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit dem Sto\u00df der Hellebarden.", "tokens": ["Mit", "dem", "Sto\u00df", "der", "Hel\u00b7le\u00b7bar\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Johann Kasimir, gebleichet", "tokens": ["Jo\u00b7hann", "Ka\u00b7si\u00b7mir", ",", "ge\u00b7blei\u00b7chet"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "NE", "$,", "VVFIN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Von des Kummers langem Drucke,", "tokens": ["Von", "des", "Kum\u00b7mers", "lan\u00b7gem", "Dru\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stieg herab, seit lange wieder", "tokens": ["Stieg", "her\u00b7ab", ",", "seit", "lan\u00b7ge", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "KOUS", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem vollen F\u00fcrstenschmucke.", "tokens": ["In", "dem", "vol\u00b7len", "F\u00fcrs\u00b7ten\u00b7schmu\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf dem Haupt die sammtne M\u00fctze,", "tokens": ["Auf", "dem", "Haupt", "die", "sammt\u00b7ne", "M\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den Busch des Reihers brannten,", "tokens": ["Um", "den", "Busch", "des", "Rei\u00b7hers", "brann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In vielfache Schnur gewunden,", "tokens": ["In", "viel\u00b7fa\u00b7che", "Schnur", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gro\u00dfe helle Diamanten.", "tokens": ["Gro\u00b7\u00dfe", "hel\u00b7le", "Di\u00b7a\u00b7man\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "An dem sammtnen Oberkleide", "tokens": ["An", "dem", "sammt\u00b7nen", "O\u00b7berk\u00b7lei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weite Aermel niederhangen,", "tokens": ["Wei\u00b7te", "A\u00b7er\u00b7mel", "nie\u00b7der\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Drauf das goldne Fell des Widders,", "tokens": ["Drauf", "das", "gold\u00b7ne", "Fell", "des", "Wid\u00b7ders", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Demantkette prangen.", "tokens": ["Und", "die", "De\u00b7mant\u00b7ket\u00b7te", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "---+--+-", "measure": "iambic.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Der kostbare Perserg\u00fcrtel", "tokens": ["Der", "kost\u00b7ba\u00b7re", "Per\u00b7ser\u00b7g\u00fcr\u00b7tel"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Tr\u00e4gt des S\u00e4bels Eisenbogen", "tokens": ["Tr\u00e4gt", "des", "S\u00e4\u00b7bels", "Ei\u00b7sen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit rubinbeseztem Griffe,", "tokens": ["Mit", "ru\u00b7bin\u00b7be\u00b7sez\u00b7tem", "Grif\u00b7fe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den der J\u00fcngling oft gezogen. \u2014", "tokens": ["Den", "der", "J\u00fcng\u00b7ling", "oft", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ihn umrauschen die Begleiter:", "tokens": ["Ihn", "um\u00b7rau\u00b7schen", "die", "Be\u00b7glei\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sully, Angouleme, nebst andern,", "tokens": ["Sul\u00b7ly", ",", "An\u00b7gou\u00b7le\u00b7me", ",", "nebst", "an\u00b7dern", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "APPR", "ADJA", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sagen ihm viel s\u00fc\u00dfe Worte,", "tokens": ["Sa\u00b7gen", "ihm", "viel", "s\u00fc\u00b7\u00dfe", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "W\u00fcnschen ihm ein gl\u00fccklich Wandern.", "tokens": ["W\u00fcn\u00b7schen", "ihm", "ein", "gl\u00fcck\u00b7lich", "Wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch der Zug, die Treppe nieder,", "tokens": ["Doch", "der", "Zug", ",", "die", "Trep\u00b7pe", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df auf jeder Stufe stocken,", "tokens": ["Mu\u00df", "auf", "je\u00b7der", "Stu\u00b7fe", "sto\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unaufhaltsam str\u00f6mt das Volk zu,", "tokens": ["Un\u00b7auf\u00b7halt\u00b7sam", "str\u00f6mt", "das", "Volk", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit gutm\u00fcthigem Frohlocken.", "tokens": ["Mit", "gut\u00b7m\u00fct\u00b7hi\u00b7gem", "Froh\u00b7lo\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "In der Treppe tiefster Ecke,", "tokens": ["In", "der", "Trep\u00b7pe", "tiefs\u00b7ter", "E\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hinter des Hartschirers R\u00fccken,", "tokens": ["Hin\u00b7ter", "des", "Hart\u00b7schi\u00b7rers", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.3": {"text": "Hat ein M\u00e4dchen sich geschmieget,", "tokens": ["Hat", "ein", "M\u00e4d\u00b7chen", "sich", "ge\u00b7schmie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den Zug hervorzublicken.", "tokens": ["Auf", "den", "Zug", "her\u00b7vor\u00b7zu\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Eingebettelt in die Stelle", "tokens": ["Ein\u00b7ge\u00b7bet\u00b7telt", "in", "die", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat sie sich mit bangem Flehen,", "tokens": ["Hat", "sie", "sich", "mit", "ban\u00b7gem", "Fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie d\u00fcrfe nur noch einmal", "tokens": ["Da\u00df", "sie", "d\u00fcr\u00b7fe", "nur", "noch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "ADV", "ADV", "ADV"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Unbemerkt den Prinzen sehen.", "tokens": ["Un\u00b7be\u00b7merkt", "den", "Prin\u00b7zen", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Also hat in scheuer Demuth", "tokens": ["Al\u00b7so", "hat", "in", "scheu\u00b7er", "De\u00b7muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Klara Hebert sich verborgen,", "tokens": ["Kla\u00b7ra", "He\u00b7bert", "sich", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmer braucht ja ihre Liebe", "tokens": ["Nim\u00b7mer", "braucht", "ja", "ih\u00b7re", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr den Theuren mehr zu sorgen.", "tokens": ["F\u00fcr", "den", "Theu\u00b7ren", "mehr", "zu", "sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Nicht gewahrt der rauhe Wachmann", "tokens": ["Nicht", "ge\u00b7wahrt", "der", "rau\u00b7he", "Wach\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVPP", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihres Herzens lautes Pochen,", "tokens": ["Ih\u00b7res", "Her\u00b7zens", "lau\u00b7tes", "Po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie manche hei\u00dfe Thr\u00e4ne", "tokens": ["Und", "wie", "man\u00b7che", "hei\u00b7\u00dfe", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIAT", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aus den Augen ihr gebrochen.", "tokens": ["Aus", "den", "Au\u00b7gen", "ihr", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Pl\u00f6tzlich h\u00e4lt Johannes inne,", "tokens": ["Pl\u00f6tz\u00b7lich", "h\u00e4lt", "Jo\u00b7han\u00b7nes", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Forschend blickt er ins Gedr\u00e4nge;", "tokens": ["For\u00b7schend", "blickt", "er", "ins", "Ge\u00b7dr\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch nicht sieht er, die er suchet", "tokens": ["Doch", "nicht", "sieht", "er", ",", "die", "er", "su\u00b7chet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Volkes bunter Menge.", "tokens": ["In", "des", "Vol\u00b7kes", "bun\u00b7ter", "Men\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Und der Liebe bange Zweifel", "tokens": ["Und", "der", "Lie\u00b7be", "ban\u00b7ge", "Zwei\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihm die Seele jezt erfassen:", "tokens": ["Ihm", "die", "See\u00b7le", "jezt", "er\u00b7fas\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201eklara!\u201c ruft er laut und schmerzlich,", "tokens": ["\u201e", "kla\u00b7ra", "!", "\u201c", "ruft", "er", "laut", "und", "schmerz\u00b7lich", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "$(", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201eklara! willst du mich verlassen?\u201c \u2014", "tokens": ["\u201e", "kla\u00b7ra", "!", "willst", "du", "mich", "ver\u00b7las\u00b7sen", "?", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ITJ", "$.", "VMFIN", "PPER", "PRF", "VVINF", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Wie sie so ihn h\u00f6ret rufen,", "tokens": ["Wie", "sie", "so", "ihn", "h\u00f6\u00b7ret", "ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPER", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcrzt sie hin mit lautem Weinen,", "tokens": ["St\u00fcrzt", "sie", "hin", "mit", "lau\u00b7tem", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "wo