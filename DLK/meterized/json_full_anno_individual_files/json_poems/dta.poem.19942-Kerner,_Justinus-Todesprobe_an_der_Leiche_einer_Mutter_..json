{"dta.poem.19942": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Todesprobe an der Leiche einer Mutter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl ihr Aug' erloschen steht,", "tokens": ["Wohl", "ihr", "Aug'", "er\u00b7lo\u00b7schen", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohl die Pulse nicht mehr schlagen", "tokens": ["Wohl", "die", "Pul\u00b7se", "nicht", "mehr", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PTKNEG", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit Klagen", "tokens": ["Und", "mit", "Kla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Jedes von der Todten geht.", "tokens": ["Je\u00b7des", "von", "der", "Tod\u00b7ten", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch sie kann noch lebend seyn!", "tokens": ["Doch", "sie", "kann", "noch", "le\u00b7bend", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Todesk\u00e4lte, Blick der Leichen,", "tokens": ["To\u00b7des\u00b7k\u00e4l\u00b7te", ",", "Blick", "der", "Lei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlechte Zeichen!", "tokens": ["Schlech\u00b7te", "Zei\u00b7chen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Bringet schnell ihr Kind herein!", "tokens": ["Brin\u00b7get", "schnell", "ihr", "Kind", "her\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Legt ihr ", "tokens": ["Legt", "ihr"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "R\u00fchrt auch ", "tokens": ["R\u00fchrt", "auch"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Dann auf immer", "tokens": ["Dann", "auf", "im\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ADV"], "meter": "-+--", "measure": "dactylic.init"}, "line.4": {"text": "Ist sie todt, \u2014 und aus ihr Schmerz.", "tokens": ["Ist", "sie", "todt", ",", "und", "aus", "ihr", "Schmerz", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "$(", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}