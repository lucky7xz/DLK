{"textgrid.poem.33754": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Athene, der du gleichst, sie hat gewaltet", "tokens": ["A\u00b7the\u00b7ne", ",", "der", "du", "gleichst", ",", "sie", "hat", "ge\u00b7wal\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVPP"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Im Bildungsgang der Menschheit zu der Sitte,", "tokens": ["Im", "Bil\u00b7dungs\u00b7gang", "der", "Menschheit", "zu", "der", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So war sie auf dem Parthenon in Mitte", "tokens": ["So", "war", "sie", "auf", "dem", "Par\u00b7the\u00b7non", "in", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der G\u00f6tter abgebildet, sch\u00f6n gestaltet.", "tokens": ["Der", "G\u00f6t\u00b7ter", "ab\u00b7ge\u00b7bil\u00b7det", ",", "sch\u00f6n", "ge\u00b7stal\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von ihr kam, was die Macht der Kunst entfaltet,", "tokens": ["Von", "ihr", "kam", ",", "was", "die", "Macht", "der", "Kunst", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Zu ihr allj\u00e4hrlich kam im Chortanzschritte", "tokens": ["Zu", "ihr", "all\u00b7j\u00e4hr\u00b7lich", "kam", "im", "Chor\u00b7tanz\u00b7schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Festzug, ihr vor allen galt die Bitte:", "tokens": ["Der", "Fest\u00b7zug", ",", "ihr", "vor", "al\u00b7len", "galt", "die", "Bit\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "APPR", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbihr hohen G\u00f6tter, sch\u00fctzet und erhaltet!\u00ab", "tokens": ["\u00bb", "ihr", "ho\u00b7hen", "G\u00f6t\u00b7ter", ",", "sch\u00fct\u00b7zet", "und", "er\u00b7hal\u00b7tet", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich sah dich einstmals mir entgegenkommen \u2013", "tokens": ["Ich", "sah", "dich", "einst\u00b7mals", "mir", "ent\u00b7ge\u00b7gen\u00b7kom\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bedeutungsvoll genug, \u2013 beim S\u00e4ulengange", "tokens": ["Be\u00b7deu\u00b7tungs\u00b7voll", "ge\u00b7nug", ",", "\u2013", "beim", "S\u00e4u\u00b7len\