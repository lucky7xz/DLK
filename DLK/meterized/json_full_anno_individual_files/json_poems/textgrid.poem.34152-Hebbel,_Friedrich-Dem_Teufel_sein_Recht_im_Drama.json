{"textgrid.poem.34152": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Dem Teufel sein Recht im Drama", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Brecht ihr dem Teufel die Z\u00e4hne erst aus, was will's noch beweisen,", "tokens": ["Brecht", "ihr", "dem", "Teu\u00b7fel", "die", "Z\u00e4h\u00b7ne", "erst", "aus", ",", "was", "will's", "noch", "be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "ADV", "PTKVZ", "$,", "PWS", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Da\u00df der Herr ihn besiegt, welchem zu Ehren ihr's thut?", "tokens": ["Da\u00df", "der", "Herr", "ihn", "be\u00b7siegt", ",", "wel\u00b7chem", "zu", "Eh\u00b7ren", "ih\u00b7r's", "thut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$,", "PRELS", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "--++-+---+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Wenn ihr dem Einzelcharacter sein Nein im Drama verbietet:", "tokens": ["Wenn", "ihr", "dem", "Ein\u00b7zelc\u00b7ha\u00b7rac\u00b7ter", "sein", "Nein", "im", "Dra\u00b7ma", "ver\u00b7bie\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Was beweis't noch das Ja eures entmarkten Gedichts?", "tokens": ["Was", "be\u00b7weis't", "noch", "das", "Ja", "eu\u00b7res", "ent\u00b7mark\u00b7ten", "Ge\u00b7d