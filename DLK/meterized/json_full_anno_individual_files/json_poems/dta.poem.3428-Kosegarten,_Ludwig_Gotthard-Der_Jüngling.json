{"dta.poem.3428": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Der J\u00fcngling", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Meine Jugend flieht. Wie soll ich, schw&#252;ler", "tokens": ["Mei\u00b7ne", "Ju\u00b7gend", "flieht", ".", "Wie", "soll", "ich", ",", "schw", "&#252;", "ler"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "XML_entity", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "PWAV", "VMFIN", "PPER", "$,", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gedanke,", "tokens": ["Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Wie ertragen die zuckenden Blitze, die grollenden", "tokens": ["Wie", "er\u00b7tra\u00b7gen", "die", "zu\u00b7cken\u00b7den", "Blit\u00b7ze", ",", "die", "grol\u00b7len\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA"], "meter": "--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Donner", "tokens": ["Don\u00b7ner"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Deines Gewitters? \u2014 Sie flieht mit ihren strahlen-", "tokens": ["Dei\u00b7nes", "Ge\u00b7wit\u00b7ters", "?", "Sie", "flieht", "mit", "ih\u00b7ren", "strah\u00b7len"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "$(", "PPER", "VVFIN", "APPR", "PPOSAT", "TRUNC"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "den Rosen,", "tokens": ["den", "Ro\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Ihren duftenden Bl\u00fcthen, und ihren knospenden", "tokens": ["Ih\u00b7ren", "duf\u00b7ten\u00b7den", "Bl\u00fc\u00b7then", ",", "und", "ih\u00b7ren", "knos\u00b7pen\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Kr\u00e4ften,", "tokens": ["Kr\u00e4f\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Ihren Freuden und Schmerzen, und heissausbrechen-", "tokens": ["Ih\u00b7ren", "Freu\u00b7den", "und", "Schmer\u00b7zen", ",", "und", "heiss\u00b7aus\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "KON", "TRUNC"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "den Thr\u00e4nen,", "tokens": ["den", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Ihrem heroischen Muth und ihrer ergreifenden", "tokens": ["Ih\u00b7rem", "he\u00b7ro\u00b7i\u00b7schen", "Muth", "und", "ih\u00b7rer", "er\u00b7grei\u00b7fen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "ADJA"], "meter": "+--+--+-+--+--", "measure": "dactylic.di.plus"}, "line.12": {"text": "Liebe.", "tokens": ["Lie\u00b7be", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Ewig flieht sie. Sie kehret nicht wieder, die Frische", "tokens": ["E\u00b7wig", "flieht", "sie", ".", "Sie", "keh\u00b7ret", "nicht", "wie\u00b7der", ",", "die", "Fri\u00b7sche"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PTKNEG", "ADV", "$,", "ART", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "des Lebens,", "tokens": ["des", "Le\u00b7bens", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Meiner Tage begeisternde Glorie \u2014 Ewig ent-", "tokens": ["Mei\u00b7ner", "Ta\u00b7ge", "be\u00b7geis\u00b7tern\u00b7de", "Glo\u00b7rie", "E\u00b7wig", "ent"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$(", "ADJD", "TRUNC"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "flieht sie,", "tokens": ["flieht", "sie", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Und mich ersch\u00fcttert nicht mehr der Tumult der", "tokens": ["Und", "mich", "er\u00b7sch\u00fct\u00b7tert", "nicht", "mehr", "der", "Tu\u00b7mult", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "ART"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "k\u00e4mpfenden Kr\u00e4fte,", "tokens": ["k\u00e4mp\u00b7fen\u00b7den", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Noch des Ahnens seliger Schauer \u2014 Ich st\u00fcrze so", "tokens": ["Noch", "des", "Ah\u00b7nens", "se\u00b7li\u00b7ger", "Schau\u00b7er", "Ich", "st\u00fcr\u00b7ze", "so"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$(", "PPER", "VVFIN", "ADV"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "trunken", "tokens": ["trun\u00b7ken"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Der Natur nicht mehr um den Hals mit k\u00f6stlichen", "tokens": ["Der", "Na\u00b7tur", "nicht", "mehr", "um", "den", "Hals", "mit", "k\u00f6st\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "ADV", "APPR", "ART", "NN", "APPR", "ADJA"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Z\u00e4hren,", "tokens": ["Z\u00e4h\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Liebe schon k\u00fchler, und dichte schon k\u00e4lter \u2014 Und", "tokens": ["Lie\u00b7be", "schon", "k\u00fch\u00b7ler", ",", "und", "dich\u00b7te", "schon", "k\u00e4l\u00b7ter", "Und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ADV", "ADJD", "$,", "KON", "VVFIN", "ADV", "ADJD", "$(", "KON"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.24": {"text": "kannst du nicht weilen,", "tokens": ["kannst", "du", "nicht", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "S\u00fcsse Geliebte? \u2014 Und kannst nicht verschieben die", "tokens": ["S\u00fcs\u00b7se", "Ge\u00b7lieb\u00b7te", "?", "Und", "kannst", "nicht", "ver\u00b7schie\u00b7ben", "die"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$.", "$(", "KON", "VMFIN", "PTKNEG", "VVFIN", "ART"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.26": {"text": "Wehen des Abschieds", "tokens": ["We\u00b7hen", "des", "Ab\u00b7schieds"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "Wenige Monden lang? \u2014 Ich liebe dich innig. Ich", "tokens": ["We\u00b7ni\u00b7ge", "Mon\u00b7den", "lang", "?", "Ich", "lie\u00b7be", "dich", "in\u00b7nig", ".", "Ich"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "ADJD", "$.", "$(", "PPER", "VVFIN", "PPER", "ADJD", "$.", "PPER"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.28": {"text": "ringe,", "tokens": ["rin\u00b7ge", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.29": {"text": "Dich zu halten. Ich fasse den Saum des entschl\u00fcp-", "tokens": ["Dich", "zu", "hal\u00b7ten", ".", "Ich", "fas\u00b7se", "den", "Saum", "des", "ent\u00b7schl\u00fcp"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "TRUNC"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.30": {"text": "fenden Kleides!", "tokens": ["fen\u00b7den", "Klei\u00b7des", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Warum qu\u00e4lst du mich so, mein Liebling, und", "tokens": ["Wa\u00b7rum", "qu\u00e4lst", "du", "mich", "so", ",", "mein", "Lieb\u00b7ling", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "$,", "PPOSAT", "NN", "$,", "KON"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "warum zerreisst mir", "tokens": ["wa\u00b7rum", "zer\u00b7reisst", "mir"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Deiner Verzweifelung Schrey die Seel'? Ich liebe", "tokens": ["Dei\u00b7ner", "Ver\u00b7zwei\u00b7fe\u00b7lung", "Schrey", "die", "Seel'", "?", "Ich", "lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "dich z\u00e4rtlich.", "tokens": ["dich", "z\u00e4rt\u00b7lich", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Wie die Verm\u00e4hlte den Tag nach der heissen berau-", "tokens": ["Wie", "die", "Ver\u00b7m\u00e4hl\u00b7te", "den", "Tag", "nach", "der", "heis\u00b7sen", "be\u00b7rau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "APPR", "ART", "ADJA", "TRUNC"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.6": {"text": "schenden Brautnacht", "tokens": ["schen\u00b7den", "Braut\u00b7nacht"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ihren nun ganz Umfangenen leibt, so lieb' ich dich,", "tokens": ["Ih\u00b7ren", "nun", "ganz", "Um\u00b7fan\u00b7ge\u00b7nen", "leibt", ",", "so", "lieb'", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADV", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "+-+-+---+-+-+", "measure": "unknown.measure.hexa"}, "line.8": {"text": "Trauter.", "tokens": ["Trau\u00b7ter", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Denn du hast mir, berauscht von Gen\u00fcssen am Busen gelegen", "tokens": ["Denn", "du", "hast", "mir", ",", "be\u00b7rauscht", "von", "Ge\u00b7n\u00fcs\u00b7sen", "am", "Bu\u00b7sen", "ge\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "$,", "VVFIN", "APPR", "NN", "APPRART", "NN", "VVPP"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Monden und Jahre lang. Nun ruft das herrische", "tokens": ["Mon\u00b7den", "und", "Jah\u00b7re", "lang", ".", "Nun", "ruft", "das", "her\u00b7ri\u00b7sche"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADJD", "$.", "ADV", "VVFIN", "ART", "ADJA"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Schicksal.", "tokens": ["Schick\u00b7sal", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.12": {"text": "Ach, was haschest, was fassest du flehend den", "tokens": ["Ach", ",", "was", "ha\u00b7schest", ",", "was", "fas\u00b7sest", "du", "fle\u00b7hend", "den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "VVPP", "ART"], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.13": {"text": "fliegenden Zipfel", "tokens": ["flie\u00b7gen\u00b7den", "Zip\u00b7fel"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Meines Gewandes? Mich ruft das unwiderrufliche", "tokens": ["Mei\u00b7nes", "Ge\u00b7wan\u00b7des", "?", "Mich", "ruft", "das", "un\u00b7wi\u00b7der\u00b7ruf\u00b7li\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA"], "meter": "+--+--+--+-+--", "measure": "dactylic.tri.plus"}, "line.15": {"text": "Schicksal.", "tokens": ["Schick\u00b7sal", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.16": {"text": "Wehe! schon f\u00fchl' ich die m\u00e4chtigen Arme mich", "tokens": ["We\u00b7he", "!", "schon", "f\u00fchl'", "ich", "die", "m\u00e4ch\u00b7ti\u00b7gen", "Ar\u00b7me", "mich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "PPER"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.17": {"text": "rings umschlingend.", "tokens": ["rings", "um\u00b7schlin\u00b7gend", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.18": {"text": "Wehe! Weh! es reisst mich hinweg. Fahr wohl,", "tokens": ["We\u00b7he", "!", "Weh", "!", "es", "reisst", "mich", "hin\u00b7weg", ".", "Fahr", "wohl", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "NN", "ADV", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "mein Geliebter!", "tokens": ["mein", "Ge\u00b7lieb\u00b7ter", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Warum raufst du dein Haar, und ringest die", "tokens": ["Wa\u00b7rum", "raufst", "du", "dein", "Haar", ",", "und", "rin\u00b7gest", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "H\u00e4nde, Verzagter?", "tokens": ["H\u00e4n\u00b7de", ",", "Ver\u00b7zag\u00b7ter", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Kannst du tilgen die Schrift, geschrieben mit gol-", "tokens": ["Kannst", "du", "til\u00b7gen", "die", "Schrift", ",", "ge\u00b7schrie\u00b7ben", "mit", "gol"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "ART", "NN", "$,", "VVPP", "APPR", "TRUNC"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "denem Griffel", "tokens": ["de\u00b7nem", "Grif\u00b7fel"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.5": {"text": "In die demantene Schicksalstafel? Kannst du", "tokens": ["In", "die", "de\u00b7man\u00b7te\u00b7ne", "Schick\u00b7sal\u00b7sta\u00b7fel", "?", "Kannst", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "VMFIN", "PPER"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Orion", "tokens": ["O\u00b7rion"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Hemmen, dass er nicht wasche die Locken im Bade", "tokens": ["Hem\u00b7men", ",", "dass", "er", "nicht", "wa\u00b7sche", "die", "Lo\u00b7cken", "im", "Ba\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "des Meeres?", "tokens": ["des", "Mee\u00b7res", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Kannst du dem Gestern gebieten: Sey heute? \u2014", "tokens": ["Kannst", "du", "dem", "Ge\u00b7stern", "ge\u00b7bie\u00b7ten", ":", "Sey", "heu\u00b7te", "?"], "token_inf