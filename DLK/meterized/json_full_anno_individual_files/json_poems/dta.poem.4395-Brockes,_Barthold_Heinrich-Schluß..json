{"dta.poem.4395": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schlu\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des grossen Sch\u00f6pfers Wunder-Macht", "tokens": ["Des", "gros\u00b7sen", "Sch\u00f6p\u00b7fers", "Wun\u00b7der\u00b7Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab ich so manches mahl besungen.", "tokens": ["Hab", "ich", "so", "man\u00b7ches", "mahl", "be\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von Seiner Creaturen Pracht", "tokens": ["Von", "Sei\u00b7ner", "Crea\u00b7tu\u00b7ren", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hat oft mein Saiten-Spiel geklungen.", "tokens": ["Hat", "oft", "mein", "Sai\u00b7ten\u00b7Spiel", "ge\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich habe von der sch\u00f6nen Welt", "tokens": ["Ich", "ha\u00b7be", "von", "der", "sch\u00f6\u00b7nen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verschiednes andern vorgestellt.", "tokens": ["Ver\u00b7schied\u00b7nes", "an\u00b7dern", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich hab\u2019 auch dann und wann gesp\u00fchret,", "tokens": ["Ich", "hab'", "auch", "dann", "und", "wann", "ge\u00b7sp\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "KON", "PWAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df manchen mein Gesang ger\u00fchret,", "tokens": ["Da\u00df", "man\u00b7chen", "mein", "Ge\u00b7sang", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und da\u00df, was man von mir gelesen,", "tokens": ["Und", "da\u00df", ",", "was", "man", "von", "mir", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "PRELS", "PIS", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Nicht immer ohne Nutz gewesen.", "tokens": ["Nicht", "im\u00b7mer", "oh\u00b7ne", "Nutz", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Wenn ich nun alles \u00fcberlege,", "tokens": ["Wenn", "ich", "nun", "al\u00b7les", "\u00fc\u00b7berl\u00b7e\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und alles, was ich schrieb, erwege;", "tokens": ["Und", "al\u00b7les", ",", "was", "ich", "schrieb", ",", "er\u00b7we\u00b7ge", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was ist doch wohl, von allem, mein?", "tokens": ["Was", "ist", "doch", "wohl", ",", "von", "al\u00b7lem", ",", "mein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "$,", "APPR", "PIS", "$,", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht die von mir besungne Pracht,", "tokens": ["Nicht", "die", "von", "mir", "be\u00b7sung\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht das, was ich davon gedacht,", "tokens": ["Nicht", "das", ",", "was", "ich", "da\u00b7von", "ge\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDS", "$,", "PWS", "PPER", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht das, was ich davon geschrieben,", "tokens": ["Nicht", "das", ",", "was", "ich", "da\u00b7von", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDS", "$,", "PWS", "PPER", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht, was mich dazu angetrieben,", "tokens": ["Nicht", ",", "was", "mich", "da\u00b7zu", "an\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PRELS", "PRF", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nicht der Verstand, nicht das Verm\u00f6gen,", "tokens": ["Nicht", "der", "Ver\u00b7stand", ",", "nicht", "das", "Ver\u00b7m\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "PTKNEG", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Die Sinnen nicht, nicht das Erwegen,", "tokens": ["Die", "Sin\u00b7nen", "nicht", ",", "nicht", "das", "Er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$,", "PTKNEG", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Nicht Zeit, nicht Umst\u00e4nd\u2019, Ort und Ruh,", "tokens": ["Nicht", "Zeit", ",", "nicht", "Um\u00b7st\u00e4nd'", ",", "Ort", "und", "Ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ja kaum der Wille h\u00f6rt mir zu.", "tokens": ["Ja", "kaum", "der", "Wil\u00b7le", "h\u00f6rt", "mir", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Indem wir alle gute Gaben", "tokens": ["In\u00b7dem", "wir", "al\u00b7le", "gu\u00b7te", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Vom Sch\u00f6pfer blo\u00df empfangen haben.", "tokens": ["Vom", "Sch\u00f6p\u00b7fer", "blo\u00df", "emp\u00b7fan\u00b7gen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "So opfre ich denn meine Lieder", "tokens": ["So", "opf\u00b7re", "ich", "denn", "mei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Dem, Der sie mir geschenket, wieder,", "tokens": ["Dem", ",", "Der", "sie", "mir", "ge\u00b7schen\u00b7ket", ",", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "$,", "ART", "PPER", "PPER", "VVPP", "$,", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und schreibe denen, die sie lesen,", "tokens": ["Und", "schrei\u00b7be", "de\u00b7nen", ",", "die", "sie", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Da die Natur, in meinem Klang,", "tokens": ["Da", "die", "Na\u00b7tur", ",", "in", "mei\u00b7nem", "Klang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Sich selber, und nicht ich, besang,", "tokens": ["Sich", "sel\u00b7ber", ",", "und", "nicht", "ich", ",", "be\u00b7sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ADV", "$,", "KON", "PTKNEG", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Da\u00df ich das Werkzeug blo\u00df gewesen.", "tokens": ["Da\u00df", "ich", "das", "Werk\u00b7zeug", "blo\u00df", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}