{"dta.poem.21533": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Der Spielmann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.85", "da:0.14"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Lustig, lustig, alte Fiedel!", "tokens": ["Lus\u00b7tig", ",", "lus\u00b7tig", ",", "al\u00b7te", "Fie\u00b7del", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sing dein neckisch Zauberliedel,", "tokens": ["Sing", "dein", "ne\u00b7ckisch", "Zau\u00b7berl\u00b7ie\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "La\u00df erklingen deine Saiten!", "tokens": ["La\u00df", "er\u00b7klin\u00b7gen", "dei\u00b7ne", "Sai\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, mit jedem Strich vom Bogen", "tokens": ["Ach", ",", "mit", "je\u00b7dem", "Strich", "vom", "Bo\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PIAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kommen T\u00f6ne angezogen,", "tokens": ["Kom\u00b7men", "T\u00f6\u00b7ne", "an\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die uns All\u2019n die Seele weiten.", "tokens": ["Die", "uns", "All'n", "die", "See\u00b7le", "wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Taumel griff beim Zauberklange,", "tokens": ["Tau\u00b7mel", "griff", "beim", "Zau\u00b7ber\u00b7klan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn des Spielers d\u00fcrre, lange", "tokens": ["Wenn", "des", "Spie\u00b7lers", "d\u00fcr\u00b7re", ",", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Finger an das Griffbrett packten,", "tokens": ["Fin\u00b7ger", "an", "das", "Griff\u00b7brett", "pack\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "All\u2019 die Dirnen, bald sie liegen", "tokens": ["All'", "die", "Dir\u00b7nen", ",", "bald", "sie", "lie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "$,", "ADV", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In den nerv\u2019gen Armen, fliegen", "tokens": ["In", "den", "ner\u00b7v'\u00b7gen", "Ar\u00b7men", ",", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hin nach feur\u2019ger Weise Takten.", "tokens": ["Hin", "nach", "feur'\u00b7ger", "Wei\u00b7se", "Tak\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Scherzen, Lachen, Kichern, Singen", "tokens": ["Scher\u00b7zen", ",", "La\u00b7chen", ",", "Ki\u00b7chern", ",", "Sin\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der alten Fiedel dringen", "tokens": ["Aus", "der", "al\u00b7ten", "Fie\u00b7del", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den Schwarm den wild bewegten,", "tokens": ["In", "den", "Schwarm", "den", "wild", "be\u00b7weg\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie dreh\u2019n sich fest umschlungen,", "tokens": ["Und", "sie", "dreh'n", "sich", "fest", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "All\u2019 die M\u00e4del mit den Jungen", "tokens": ["All'", "die", "M\u00e4\u00b7del", "mit", "den", "Jun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hin im Tanz, dem toll bewegten.", "tokens": ["Hin", "im", "Tanz", ",", "dem", "toll", "be\u00b7weg\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "PRELS", "ADJD", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Steht der Spielmann da im grauen", "tokens": ["Steht", "der", "Spiel\u00b7mann", "da", "im", "grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bart, dem unter busch\u2019gen Brauen", "tokens": ["Bart", ",", "dem", "un\u00b7ter", "busch'\u00b7gen", "Brau\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dunkle Augen schmerzlich flammen,", "tokens": ["Dunk\u00b7le", "Au\u00b7gen", "schmerz\u00b7lich", "flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An der S\u00e4ule, und es gleiten", "tokens": ["An", "der", "S\u00e4u\u00b7le", ",", "und", "es", "glei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mark\u2019ge Striche ob den Saiten,", "tokens": ["Mark'\u00b7ge", "Stri\u00b7che", "ob", "den", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lockt er\u2019s junge Dorf zusammen.", "tokens": ["Lockt", "er's", "jun\u00b7ge", "Dorf", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Laughin f\u00e4llt sein Haar hernieder,", "tokens": ["Laug\u00b7hin", "f\u00e4llt", "sein", "Haar", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kahler Mantel h\u00fcllt die Glieder,", "tokens": ["Kah\u00b7ler", "Man\u00b7tel", "h\u00fcllt", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sein Hut, der ist nicht gl\u00e4tter;", "tokens": ["Und", "sein", "Hut", ",", "der", "ist", "nicht", "gl\u00e4t\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tiefgezog\u2019ne Furchen haben", "tokens": ["Tief\u00b7ge\u00b7zo\u00b7g'\u00b7ne", "Fur\u00b7chen", "ha\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "In die Wangen sich gegraben,", "tokens": ["In", "die", "Wan\u00b7gen", "sich", "ge\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die vergilbt von Sturm und Wetter.", "tokens": ["Die", "ver\u00b7gilbt", "von", "Sturm", "und", "Wet\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach, vermodert sind die Jungen,", "tokens": ["Ach", ",", "ver\u00b7mo\u00b7dert", "sind", "die", "Jun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wo ist zuletzt erklungen", "tokens": ["Und", "wo", "ist", "zu\u00b7letzt", "er\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VAFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seiner Dirne munt\u2019res Liedel?", "tokens": ["Sei\u00b7ner", "Dir\u00b7ne", "munt'\u00b7res", "Lie\u00b7del", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wanderdrang hat sie getrieben,", "tokens": ["Wan\u00b7der\u00b7drang", "hat", "sie", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einsam ist er dann geblieben,", "tokens": ["Ein\u00b7sam", "ist", "er", "dann", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sein Liebstes ward die Fiedel!", "tokens": ["Und", "sein", "Liebs\u00b7tes", "ward", "die", "Fie\u00b7del", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}