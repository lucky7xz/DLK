{"textgrid.poem.49616": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die hilfreiche Regierung", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbihr gabt mir Steine, ich bat um Brot\u00ab,", "tokens": ["\u00bb", "ihr", "gabt", "mir", "Stei\u00b7ne", ",", "ich", "bat", "um", "Brot", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$(", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist irgendwo in der Schrift zu lesen.", "tokens": ["Ist", "ir\u00b7gend\u00b7wo", "in", "der", "Schrift", "zu", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die christlichen Sachsen sind in der Not", "tokens": ["Die", "christ\u00b7li\u00b7chen", "Sach\u00b7sen", "sind", "in", "der", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Barmherziger noch und milder gewesen.", "tokens": ["Barm\u00b7her\u00b7zi\u00b7ger", "noch", "und", "mil\u00b7der", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADJD", "VAPP", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Weber schrieben ins Deutsche Reich:", "tokens": ["Die", "We\u00b7ber", "schrie\u00b7ben", "ins", "Deut\u00b7sche", "Reich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir haben Hunger, nun zeigt Erbarmen,", "tokens": ["Wir", "ha\u00b7ben", "Hun\u00b7ger", ",", "nun", "zeigt", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "ADV", "VVFIN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da wurden die Herren in Dresden weich", "tokens": ["Da", "wur\u00b7den", "die", "Her\u00b7ren", "in", "Dres\u00b7den", "weich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NE", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und schickten eine Menge \u2013 Gendarmen.", "tokens": ["Und", "schick\u00b7ten", "ei\u00b7ne", "Men\u00b7ge", "\u2013", "Gen\u00b7dar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$(", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Gendarmen sind gut f\u00fcr die bittere Not,", "tokens": ["Gen\u00b7dar\u00b7men", "sind", "gut", "f\u00fcr", "die", "bit\u00b7te\u00b7re", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Denn jeder, den sie ins Zuchthaus stecken,", "tokens": ["Denn", "je\u00b7der", ",", "den", "sie", "ins", "Zucht\u00b7haus", "ste\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der findet drinnen den sanften Tod", "tokens": ["Der", "fin\u00b7det", "drin\u00b7nen", "den", "sanf\u00b7ten", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und mu\u00df nicht auf der Stra\u00dfe verrecken.", "tokens": ["Und", "mu\u00df", "nicht", "auf", "der", "Stra\u00b7\u00dfe", "ver\u00b7re\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbihr gabt mir Steine, ich bat um Brot\u00ab,", "tokens": ["\u00bb", "ihr", "gabt", "mir", "Stei\u00b7ne", ",", "ich", "bat", "um", "Brot", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$(", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist irgendwo in der Schrift zu lesen.", "tokens": ["Ist", "ir\u00b7gend\u00b7wo", "in", "der", "Schrift", "zu", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die christlichen Sachsen sind in der Not", "tokens": ["Die", "christ\u00b7li\u00b7chen", "Sach\u00b7sen", "sind", "in", "der", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Barmherziger noch und milder gewesen.", "tokens": ["Barm\u00b7her\u00b7zi\u00b7ger", "noch", "und