: {"line.1": {"text": "O kehre doch wieder zur\u00fcck in die ver\u00f6dete Seele,", "tokens": ["O", "keh\u00b7re", "doch", "wie\u00b7der", "zu\u00b7r\u00fcck", "in", "die", "ver\u00b7\u00f6\u00b7de\u00b7te", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+--+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die deine schmeichelnde Macht nur erh\u00e4lt!", "tokens": ["Die", "dei\u00b7ne", "schmei\u00b7cheln\u00b7de", "Macht", "nur", "er\u00b7h\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Entdecke mir, Hofnung, den Trost, auch in der ferne-", "tokens": ["Ent\u00b7de\u00b7cke", "mir", ",", "Hof\u00b7nung", ",", "den", "Trost", ",", "auch", "in", "der", "fer\u00b7ne"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "$,", "ADV", "APPR", "ART", "TRUNC"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Selinen einmal nur wieder zu sehn.", "tokens": ["Se\u00b7li\u00b7nen", "ein\u00b7mal", "nur", "wie\u00b7der", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}