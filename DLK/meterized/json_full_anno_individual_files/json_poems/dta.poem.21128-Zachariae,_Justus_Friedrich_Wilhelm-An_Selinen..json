it Freude gekr\u00e4nzt,", "tokens": ["Die", "Lieb", "und", "Freund\u00b7schaft", "mit", "Freu\u00b7de", "ge\u00b7kr\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ach wenige Stunden sinds nur! Der melancholischen", "tokens": ["Ach", "we\u00b7ni\u00b7ge", "Stun\u00b7den", "sinds", "nur", "!", "Der", "me\u00b7lan\u00b7cho\u00b7li\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "PIAT", "NN", "VAFIN", "ADV", "$.", "ART", "ADJA"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und der durchweinten N\u00e4chte so viel!", "tokens": ["Und", "der", "durch\u00b7wein\u00b7ten", "N\u00e4ch\u00b7te", "so", "viel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.16": {"line.1": {"text": "Doch wollt\u2019 ich mit ruhigem Blick den halbverbl\u00fcheten", "tokens": ["Doch", "wollt'", "ich", "mit", "ru\u00b7hi\u00b7gem", "Blick", "den", "halb\u00b7ver\u00b7bl\u00fc\u00b7he\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Gleich schwarzen Wintern dahinst\u00fcrmen sehn;", "tokens": ["Gleich", "schwar\u00b7zen", "Win\u00b7tern", "da\u00b7hin\u00b7st\u00fcr\u00b7men", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn nicht in dem m\u00e4chtigsten Leid der letzte Trost", "tokens": ["Wenn", "nicht", "in", "d