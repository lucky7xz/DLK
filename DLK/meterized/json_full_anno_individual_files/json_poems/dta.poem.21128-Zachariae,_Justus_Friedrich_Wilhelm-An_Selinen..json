{"dta.poem.21128": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An Selinen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vortreflichste deines Geschlechts, in deren g\u00f6ttliche\nSeele", "tokens": ["Vor\u00b7tre\u00b7flichs\u00b7te", "dei\u00b7nes", "Ge\u00b7schlechts", ",", "in", "de\u00b7ren", "g\u00f6tt\u00b7li\u00b7che", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "APPR", "PRELAT", "ADJA", "NN"], "meter": "+-+-+--+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Der Sch\u00f6pfer alle die Tugend gehaucht,", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "al\u00b7le", "die", "Tu\u00b7gend", "ge\u00b7haucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Durch die oft ein irdischer Geist, zum Thron der Gott-", "tokens": ["Durch", "die", "oft", "ein", "ir\u00b7di\u00b7scher", "Geist", ",", "zum", "Thron", "der", "Got\u00b7t"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ADV", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "TRUNC"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Sich unter heilige Seraphim dr\u00e4ngt.", "tokens": ["Sich", "un\u00b7ter", "hei\u00b7li\u00b7ge", "Se\u00b7ra\u00b7phim", "dr\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Seraphim lieben ihn schon, und die Unsterblichen", "tokens": ["Die", "Se\u00b7ra\u00b7phim", "lie\u00b7ben", "ihn", "schon", ",", "und", "die", "U\u00b7nsterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "KON", "ART", "NN"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "Erziehn ihn um sich zur Ewigkeit auf;", "tokens": ["Er\u00b7ziehn", "ihn", "um", "sich", "zur", "E\u00b7wig\u00b7keit", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und lehren auf Erden ihn schon ein Lied zum Lobe der", "tokens": ["Und", "leh\u00b7ren", "auf", "Er\u00b7den", "ihn", "schon", "ein", "Lied", "zum", "Lo\u00b7be", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "ART"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und in die g\u00fcldenen Harfen ein Lied;", "tokens": ["Und", "in", "die", "g\u00fcl\u00b7de\u00b7nen", "Har\u00b7fen", "ein", "Lied", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ach da\u00df noch, Seline, mich nicht die hohe S\u00e4ngerin", "tokens": ["Ach", "da\u00df", "noch", ",", "Se\u00b7li\u00b7ne", ",", "mich", "nicht", "die", "ho\u00b7he", "S\u00e4n\u00b7ge\u00b7rin"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "KOUS", "ADV", "$,", "NE", "$,", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Die G \u2012 \u2012 C \u2012 \u2012 und K \u2012 \u2012 gelehrt!", "tokens": ["Die", "G", "\u2012", "\u2012", "C", "\u2012", "\u2012", "und", "K", "\u2012", "\u2012", "ge\u00b7lehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "$(", "XY", "$(", "$(", "KON", "NE", "$(", "$(", "VVPP", "$."], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Sie, welche hoch \u00fcber mir stehn, sie w\u00fcrden dich edler", "tokens": ["Sie", ",", "wel\u00b7che", "hoch", "\u00fc\u00b7ber", "mir", "stehn", ",", "sie", "w\u00fcr\u00b7den", "dich", "ed\u00b7ler"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVINF", "$,", "PPER", "VAFIN", "PPER", "ADJA"], "meter": "-+-+---+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und deine w\u00fcrdigern Herolde seyn.", "tokens": ["Und", "dei\u00b7ne", "w\u00fcr\u00b7di\u00b7gern", "He\u00b7rol\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch wie? Soll noch l\u00e4nger mein Herz die stillen", "tokens": ["Doch", "wie", "?", "Soll", "noch", "l\u00e4n\u00b7ger", "mein", "Herz", "die", "stil\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "$.", "VMFIN", "ADV", "ADJD", "PPOSAT", "NN", "ART", "ADJA"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die deine Tugenden in ihm erzeugt?", "tokens": ["Die", "dei\u00b7ne", "Tu\u00b7gen\u00b7den", "in", "ihm", "er\u00b7zeugt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So schallte mein k\u00fchner Gesang, von deinem Werthe", "tokens": ["So", "schall\u00b7te", "mein", "k\u00fch\u00b7ner", "Ge\u00b7sang", ",", "von", "dei\u00b7nem", "Wert\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Nicht in die hellere k\u00fcnftige Welt;", "tokens": ["Nicht", "in", "die", "hel\u00b7le\u00b7re", "k\u00fcnf\u00b7ti\u00b7ge", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "So h\u00e4tte dein Auge noch nicht, wenn es erheiternder", "tokens": ["So", "h\u00e4t\u00b7te", "dein", "Au\u00b7ge", "noch", "nicht", ",", "wenn", "es", "er\u00b7hei\u00b7tern\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "ADJA"], "meter": "-+--+--++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Als von dem Himmel ein lichtes Gew\u00f6lk", "tokens": ["Als", "von", "dem", "Him\u00b7mel", "ein", "lich\u00b7tes", "Ge\u00b7w\u00f6lk"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In mein gleichg\u00fcltiges Herz die heilige Flamme gegossen,", "tokens": ["In", "mein", "gleich\u00b7g\u00fcl\u00b7ti\u00b7ges", "Herz", "die", "hei\u00b7li\u00b7ge", "Flam\u00b7me", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die zu unsterblichen Liedern mich zwingt;", "tokens": ["Die", "zu", "uns\u00b7terb\u00b7li\u00b7chen", "Lie\u00b7dern", "mich", "zwingt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKZU", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "So h\u00e4tte mir deine Hand nie den Gram vom Auge ge-", "tokens": ["So", "h\u00e4t\u00b7te", "mir", "dei\u00b7ne", "Hand", "nie", "den", "Gram", "vom", "Au\u00b7ge", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ART", "NN", "APPRART", "NN", "TRUNC"], "meter": "-+--+-++-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Der \u00fcber die traurige Wange gethaut;", "tokens": ["Der", "\u00fc\u00b7ber", "die", "trau\u00b7ri\u00b7ge", "Wan\u00b7ge", "ge\u00b7thaut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Stirne die Jugend entzog, und den gewaltigsten", "tokens": ["Der", "Stir\u00b7ne", "die", "Ju\u00b7gend", "ent\u00b7zog", ",", "und", "den", "ge\u00b7wal\u00b7tigs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,", "KON", "ART", "ADJA"], "meter": "-+--+--+-+-+--", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und dunkler Verzweiflung zum Opfer mich gab;", "tokens": ["Und", "dunk\u00b7ler", "Ver\u00b7zwei\u00b7flung", "zum", "Op\u00b7fer", "mich", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "So h\u00e4tt\u2019 ich nicht Thr\u00e4nen gesehn, durch die die m\u00e4ch-", "tokens": ["So", "h\u00e4tt'", "ich", "nicht", "Thr\u00e4\u00b7nen", "ge\u00b7sehn", ",", "durch", "die", "die", "m\u00e4ch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "NN", "VVPP", "$,", "APPR", "PRELS", "ART", "TRUNC"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Dein blaues siegendes Auge getr\u00fcbt;", "tokens": ["Dein", "blau\u00b7es", "sie\u00b7gen\u00b7des", "Au\u00b7ge", "ge\u00b7tr\u00fcbt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So h\u00e4tt\u2019 ich nicht Seufzer geh\u00f6rt, und unnachsprechli-", "tokens": ["So", "h\u00e4tt'", "ich", "nicht", "Seuf\u00b7zer", "ge\u00b7h\u00f6rt", ",", "und", "un\u00b7nach\u00b7sprech\u00b7li"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "NN", "VVFIN", "$,", "KON", "TRUNC"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die eine Seele der andern nur sagt.", "tokens": ["Die", "ei\u00b7ne", "See\u00b7le", "der", "an\u00b7dern", "nur", "sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "ADJA", "ADV", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Du Tag, da ihr sanftes Gesicht, wie die Fr\u00fchlingsson-", "tokens": ["Du", "Tag", ",", "da", "ihr", "sanf\u00b7tes", "Ge\u00b7sicht", ",", "wie", "die", "Fr\u00fch\u00b7lings\u00b7son"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$,", "PWAV", "ART", "TRUNC"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "Sey du mir ewig ein festlicher Tag!", "tokens": ["Sey", "du", "mir", "e\u00b7wig", "ein", "fest\u00b7li\u00b7cher", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Da sagte mein klopfendes Herz, und sagt\u2019 es voller Be-", "tokens": ["Da", "sag\u00b7te", "mein", "klop\u00b7fen\u00b7des", "Herz", ",", "und", "sagt'", "es", "vol\u00b7ler", "Be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER", "ADJA", "TRUNC"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das ist Sie! Und ich empfand es, Sie wars.", "tokens": ["Das", "ist", "Sie", "!", "Und", "ich", "emp\u00b7fand", "es", ",", "Sie", "wars", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$.", "KON", "PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "$."], "meter": "----+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "So l\u00e4chelt an Even vordem ein heitres Auge voll Un-", "tokens": ["So", "l\u00e4\u00b7chelt", "an", "E\u00b7ven", "vor\u00b7dem", "ein", "heit\u00b7res", "Au\u00b7ge", "voll", "Un"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NE", "ADV", "ART", "ADJA", "NN", "ADJD", "TRUNC"], "meter": "-+--+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und fr\u00f6lich h\u00fcpfte die junge Natur:", "tokens": ["Und", "fr\u00f6\u00b7lich", "h\u00fcpf\u00b7te", "die", "jun\u00b7ge", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie ihr triumphirender Blick, der aus unschuldigen", "tokens": ["Wie", "ihr", "tri\u00b7um\u00b7phi\u00b7ren\u00b7der", "Blick", ",", "der", "aus", "un\u00b7schul\u00b7di\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Tief in die weichere Seele mir drang.", "tokens": ["Tief", "in", "die", "wei\u00b7che\u00b7re", "See\u00b7le", "mir", "drang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Seele verlohr sich in sie, und ward erhabner ge-", "tokens": ["Die", "See\u00b7le", "ver\u00b7lohr", "sich", "in", "sie", ",", "und", "ward", "er\u00b7hab\u00b7ner", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPER", "$,", "KON", "VAFIN", "ADJA", "TRUNC"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und schlo\u00df sich s\u00fc\u00dfen Entz\u00fcckungen auf;", "tokens": ["Und", "schlo\u00df", "sich", "s\u00fc\u00b7\u00dfen", "Ent\u00b7z\u00fc\u00b7ckun\u00b7gen", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.3": {"text": "So wie dem m\u00e4chtigen Stral die junge Rose sich \u00f6fnet,", "tokens": ["So", "wie", "dem", "m\u00e4ch\u00b7ti\u00b7gen", "Stral", "die", "jun\u00b7ge", "Ro\u00b7se", "sich", "\u00f6f\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und froh des Morgenthaus Segen empf\u00e4ngt.", "tokens": ["Und", "froh", "des", "Mor\u00b7gent\u00b7haus", "Se\u00b7gen", "emp\u00b7f\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Mein weichergebildetes Herz empfand nun h\u00f6here Freu-", "tokens": ["Mein", "wei\u00b7cher\u00b7ge\u00b7bil\u00b7de\u00b7tes", "Herz", "emp\u00b7fand", "nun", "h\u00f6\u00b7he\u00b7re", "Freu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+--+--+-+-+--+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Als die, so flatterud die Jugend durchflog.", "tokens": ["Als", "die", ",", "so", "flat\u00b7te\u00b7rud", "die", "Ju\u00b7gend", "durch\u00b7flog", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$,", "ADV", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Wie paradiesisch ward mir das Thal ehrw\u00fcrdiger Eichen,", "tokens": ["Wie", "pa\u00b7ra\u00b7die\u00b7sisch", "ward", "mir", "das", "Thal", "ehr\u00b7w\u00fcr\u00b7di\u00b7ger", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das dich zu mir, o Seline, gef\u00fchrt!", "tokens": ["Das", "dich", "zu", "mir", ",", "o", "Se\u00b7li\u00b7ne", ",", "ge\u00b7f\u00fchrt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PPER", "$,", "FM", "NE", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Da sah ich den Himmel zuerst vom Lenz und Freude", "tokens": ["Da", "sah", "ich", "den", "Him\u00b7mel", "zu\u00b7erst", "vom", "Lenz", "und", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "KON", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da erst verstand ich der B\u00fcsche Ger\u00e4usch;", "tokens": ["Da", "erst", "ver\u00b7stand", "ich", "der", "B\u00fc\u00b7sche", "Ge\u00b7r\u00e4usch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da gieng der holdselige West zuerst gef\u00fchlt mir vor\u00fcber,", "tokens": ["Da", "gieng", "der", "hold\u00b7se\u00b7li\u00b7ge", "West", "zu\u00b7erst", "ge\u00b7f\u00fchlt", "mir", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "VVPP", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Und f\u00fchlend h\u00f6rt ich der Nachtigall Lied.", "tokens": ["Und", "f\u00fch\u00b7lend", "h\u00f6rt", "ich", "der", "Nach\u00b7ti\u00b7g