{"textgrid.poem.34973": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "10", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sitzen am Kreuzweg drei Frauen,", "tokens": ["Es", "sit\u00b7zen", "am", "Kreuz\u00b7weg", "drei", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "CARD", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sie grinsen und spinnen,", "tokens": ["Sie", "grin\u00b7sen", "und", "spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sie seufzen und sinnen;", "tokens": ["Sie", "seuf\u00b7zen", "und", "sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sie sind gar h\u00e4\u00dflich anzuschauen.", "tokens": ["Sie", "sind", "gar", "h\u00e4\u00df\u00b7lich", "an\u00b7zu\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die erste tr\u00e4gt den Rocken,", "tokens": ["Die", "ers\u00b7te", "tr\u00e4gt", "den", "Ro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie dreht die F\u00e4den,", "tokens": ["Sie", "dreht", "die", "F\u00e4\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Befeuchtet jeden;", "tokens": ["Be\u00b7feuch\u00b7tet", "je\u00b7den", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PIAT", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Deshalb ist die H\u00e4ngelippe so trocken.", "tokens": ["Des\u00b7halb", "ist", "die", "H\u00e4n\u00b7ge\u00b7lip\u00b7pe", "so", "tro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Die zweite l\u00e4\u00dft tanzen die Spindel;", "tokens": ["Die", "zwei\u00b7te", "l\u00e4\u00dft", "tan\u00b7zen", "die", "Spin\u00b7del", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "VVFIN", "ART", "NN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das wirbelt im Kreise,", "tokens": ["Das", "wir\u00b7belt", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "In drolliger Weise;", "tokens": ["In", "drol\u00b7li\u00b7ger", "Wei\u00b7se", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Augen der Alten sind rot wie Zindel.", "tokens": ["Die", "Au\u00b7gen", "der", "Al\u00b7ten", "sind", "rot", "wie", "Zin\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADJD", "KOKOM", "NE", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Es h\u00e4lt die dritte Parze", "tokens": ["Es", "h\u00e4lt", "die", "drit\u00b7te", "Par\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In H\u00e4nden die Schere,", "tokens": ["In", "H\u00e4n\u00b7den", "die", "Sche\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sie summt Miserere;", "tokens": ["Sie", "summt", "Mi\u00b7se\u00b7re\u00b7re", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Die Nase ist spitz, drauf sitzt eine Warze.", "tokens": ["Die", "Na\u00b7se", "ist", "spitz", ",", "drauf", "sitzt", "ei\u00b7ne", "War\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PAV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "O spute dich und zerschneide", "tokens": ["O", "spu\u00b7te", "dich", "und", "zer\u00b7schnei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Den Faden, den b\u00f6sen,", "tokens": ["Den", "Fa\u00b7den", ",", "den", "b\u00f6\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und la\u00df mich genesen", "tokens": ["Und", "la\u00df", "mich", "ge\u00b7ne\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "VVPP"], "meter": "-+----", "measure": "dactylic.init"}, "line.4": {"text": "Von diesem schrecklichen Lebe