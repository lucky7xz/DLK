{"dta.poem.9366": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .  \n  Sonnet/  \n von 8- und 7-sylbigen versen.  \n  Nach der Erfindung der 20. Ode unsers  \n Poeten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Tugend will ich allzeit lieben/", "tokens": ["Tu\u00b7gend", "will", "ich", "all\u00b7zeit", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tugend ist der beste Freund/", "tokens": ["Tu\u00b7gend", "ist", "der", "bes\u00b7te", "Freund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wenn die g\u00fcldne Sonne scheint/", "tokens": ["wenn", "die", "g\u00fcld\u00b7ne", "Son\u00b7ne", "scheint", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wenn die Wolcken uns betr\u00fcben/", "tokens": ["wenn", "die", "Wol\u00b7cken", "uns", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "wenn wier uns im Reisen \u00fcben;", "tokens": ["wenn", "wier", "uns", "im", "Rei\u00b7sen", "\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Tugend wird uns nimmer feind/", "tokens": ["Tu\u00b7gend", "wird", "uns", "nim\u00b7mer", "feind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "alles ist sehr wohl gemeint;", "tokens": ["al\u00b7les", "ist", "sehr", "wohl", "ge\u00b7meint", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Freunde wie der rauch verstieben/", "tokens": ["Freun\u00b7de", "wie", "der", "rauch", "ver\u00b7stie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Tugend aber jmmer bleibt/", "tokens": ["Tu\u00b7gend", "a\u00b7ber", "jm\u00b7mer", "bleibt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "die kein Armuth nicht vertreibt-", "tokens": ["die", "kein", "Ar\u00b7muth", "nicht", "ver\u00b7treib\u00b7t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "PTKNEG", "TRUNC"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Freunde/ wenn sie gleich verbunden", "tokens": ["Freun\u00b7de", "/", "wenn", "sie", "gleich", "ver\u00b7bun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "KOUS", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "sich zu geben in den Todt/", "tokens": ["sich", "zu", "ge\u00b7ben", "in", "den", "Todt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "wenn wier aber seyn in noth/", "tokens": ["wenn", "wier", "a\u00b7ber", "seyn", "in", "noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAINF", "APPR", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "wird nichts fl\u00fcchtiger gefunden.", "tokens": ["wird", "nichts", "fl\u00fcch\u00b7ti\u00b7ger", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJA", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}