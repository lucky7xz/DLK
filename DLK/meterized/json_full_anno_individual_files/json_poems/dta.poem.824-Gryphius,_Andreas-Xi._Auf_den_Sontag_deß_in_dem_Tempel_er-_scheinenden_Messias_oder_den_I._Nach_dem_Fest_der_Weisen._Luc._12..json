{"dta.poem.824": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Xi.  Auf den Sontag de\u00df in dem Tempel er-  \n scheinenden Messias/ oder den  I.  Nach dem  \n Fest der Weisen. Luc. 12.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.85", "nl:0.14"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Der ists mein Hertz den Gott vor allen hat verehret ", "tokens": ["Der", "ists", "mein", "Hertz", "den", "Gott", "vor", "al\u00b7len", "hat", "ver\u00b7eh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ART", "NN", "APPR", "PIS", "VAFIN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit de\u00df Geistes Frewden Oel/ dessen wort die See-", "tokens": ["Mit", "de\u00df", "Geis\u00b7tes", "Frew\u00b7den", "O\u00b7el", "/", "des\u00b7sen", "wort", "die", "See"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$(", "PRELAT", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "len r\u00fchrt/ ", "tokens": ["len", "r\u00fchrt", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Gleich als stralen lichter glutt/ der Zung/ Aug vnd Sinne", "tokens": ["Gleich", "als", "stra\u00b7len", "lich\u00b7ter", "glutt", "/", "der", "Zung", "/", "Aug", "vnd", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOKOM", "VVFIN", "ADJD", "ADJD", "$(", "ART", "NN", "$(", "NN", "KON", "NN"], "meter": "+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.5": {"text": "regirt/ ", "tokens": ["re\u00b7girt", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Vnd was noch k\u00fcnfftig wei\u00df\u2019t/ der hier die Lehrer lehret.", "tokens": ["Vnd", "was", "noch", "k\u00fcnff\u00b7tig", "wei\u00df't", "/", "der", "hier", "die", "Leh\u00b7rer", "leh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJD", "VVFIN", "$(", "ART", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O wol dem der den Rath de\u00df weisen Vaters h\u00f6ret!", "tokens": ["O", "wol", "dem", "der", "den", "Rath", "de\u00df", "wei\u00b7sen", "Va\u00b7ters", "h\u00f6\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ART", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Welcher in de\u00df HErren krafft nicht gemeine reden f\u00fchrt/", "tokens": ["Wel\u00b7cher", "in", "de\u00df", "Her\u00b7ren", "krafft", "nicht", "ge\u00b7mei\u00b7ne", "re\u00b7den", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "ART", "NN", "VVFIN", "PTKNEG", "ADJA", "VVINF", "VVFIN", "$("], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Vnd als aller Heyden trost seinen newen Tempel zihrt/", "tokens": ["Vnd", "als", "al\u00b7ler", "Hey\u00b7den", "trost", "sei\u00b7nen", "ne\u00b7wen", "Tem\u00b7pel", "zihrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+-+----+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Den Tempel/ dessen Ruhm Er gegenwertig mehret.", "tokens": ["Den", "Tem\u00b7pel", "/", "des\u00b7sen", "Ruhm", "Er", "ge\u00b7gen\u00b7wer\u00b7tig", "meh\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELAT", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "La\u00df Seele/ la\u00df wie Er das schlechte Vaterland/", "tokens": ["La\u00df", "See\u00b7le", "/", "la\u00df", "wie", "Er", "das", "schlech\u00b7te", "Va\u00b7ter\u00b7land", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$(", "VVIMP", "PWAV", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df Freunde/ Statt vnd weg/ la\u00df deiner Mutter Hand/", "tokens": ["La\u00df", "Freun\u00b7de", "/", "Statt", "vnd", "weg", "/", "la\u00df", "dei\u00b7ner", "Mut\u00b7ter", "Hand", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$(", "NN", "KON", "PTKVZ", "$(", "VVIMP", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd bleib wo Jesus jhm sein Vatter theil erkohren.", "tokens": ["Vnd", "bleib", "wo", "Je\u00b7sus", "jhm", "sein", "Vat\u00b7ter", "theil", "er\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWAV", "NE", "PPER", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Hier such jhn/ we\u00f1 du wilt/ hier schleu\u00dft Er niemand aus/", "tokens": ["Hier", "such", "jhn", "/", "we\u00f1", "du", "wilt", "/", "hier", "schleu\u00dft", "Er", "nie\u00b7mand", "aus", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "VMFIN", "$(", "ADV", "VVFIN", "PPER", "PIS", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier h\u00e4lt Er Mittags Ruh/ hier ist sein eigen Hau\u00df", "tokens": ["Hier", "h\u00e4lt", "Er", "Mit\u00b7tags", "Ruh", "/", "hier", "ist", "sein", "ei\u00b7gen", "Hau\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$(", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier findt jhn/ wer bey nacht vnd Jrrthumb jhn verlohren.", "tokens": ["Hier", "findt", "jhn", "/", "wer", "bey", "nacht", "vnd", "Jrr\u00b7thumb", "jhn", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "PWS", "APPR", "NN", "KON", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}