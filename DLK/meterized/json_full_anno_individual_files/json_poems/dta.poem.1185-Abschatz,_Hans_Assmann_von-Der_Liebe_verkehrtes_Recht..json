{"dta.poem.1185": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der Liebe verkehrtes Recht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wie grausam sind/ o Liebe/ deine Rechte!", "tokens": ["Wie", "grau\u00b7sam", "sind", "/", "o", "Lie\u00b7be", "/", "dei\u00b7ne", "Rech\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$(", "FM", "NN", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein leichter Sinn schmeckt tausendfache Lust/", "tokens": ["Ein", "leich\u00b7ter", "Sinn", "schmeckt", "tau\u00b7send\u00b7fa\u00b7che", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Thr\u00e4nen Tranck/ der Seufftzer schwere Kost", "tokens": ["Der", "Thr\u00e4\u00b7nen", "Tranck", "/", "der", "Seufft\u00b7zer", "schwe\u00b7re", "Kost"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "N\u00e4hrt und verzehrt die Hertzen treuer Knechte;", "tokens": ["N\u00e4hrt", "und", "ver\u00b7zehrt", "die", "Hert\u00b7zen", "treu\u00b7er", "Knech\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie grausam seyn/ o Liebe/ deine Rechte!", "tokens": ["Wie", "gr