{"dta.poem.12426": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Gesellschaftslied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dieterlein. Wohlauf ihr Narren, zieht all mit mir,               ", "tokens": ["Die\u00b7ter\u00b7lein", ".", "Wohl\u00b7auf", "ihr", "Nar\u00b7ren", ",", "zieht", "all", "mit", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "PPOSAT", "NN", "$,", "VVFIN", "PIAT", "APPR", "PPER", "$,"], "meter": "+---+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Zieht all mit mir,", "tokens": ["Zieht", "all", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wohl heuer in diesem Jahre,", "tokens": ["Wohl", "heu\u00b7er", "in", "die\u00b7sem", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In diesem Jahre.", "tokens": ["In", "die\u00b7sem", "Jah\u00b7re", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Alle. Habens gern gethan,               ", "tokens": ["Al\u00b7le", ".", "Ha\u00b7bens", "gern", "ge\u00b7than", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "$.", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Thuns noch einmals,", "tokens": ["Thuns", "noch", "ein\u00b7mals", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Was gehts dich denn an?", "tokens": ["Was", "gehts", "dich", "denn", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Dich gehts gar nichts an!", "tokens": ["Dich", "gehts", "gar", "nichts", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Was fragst denn du darnach?", "tokens": ["Was", "fragst", "denn", "du", "dar\u00b7nach", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "PPER", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Was hast denn du davon?", "tokens": ["Was", "hast", "denn", "du", "da\u00b7von", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "KON", "PPER", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dieterlein. Bin ich ein Narr, bins nicht allein,               ", "tokens": ["Die\u00b7ter\u00b7lein", ".", "Bin", "ich", "ein", "Narr", ",", "bins", "nicht", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "ART", "NN", "$,", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Achts sicher klein,", "tokens": ["Achts", "si\u00b7cher", "klein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wollt Gott, ich w\u00e4r nur ein Narre,", "tokens": ["Wollt", "Gott", ",", "ich", "w\u00e4r", "nur", "ein", "Nar\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nach meinem Sinne.", "tokens": ["Nach", "mei\u00b7nem", "Sin\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Alle. H\u00e4ttst gern so gethan,               ", "tokens": ["Al\u00b7le", ".", "H\u00e4ttst", "gern", "so", "ge\u00b7than", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "$.", "VVFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Th\u00e4tst noch einmal, u. s. w.", "tokens": ["Th\u00e4tst", "noch", "ein\u00b7mal", ",", "u.", "s.", "w."], "token_info": ["word", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["VVFIN", "ADV", "ADV", "$,", "KON", "VVIMP", "NN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Dieterlein. Wollt Gott, ich w\u00e4r ein kleins V\u00f6gelein,               ", "tokens": ["Die\u00b7ter\u00b7lein", ".", "Wollt", "Gott", ",", "ich", "w\u00e4r", "ein", "kleins", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Waldv\u00f6glein klein,", "tokens": ["Wald\u00b7v\u00f6\u00b7glein", "klein", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Zur Lieben wollt ich mich schwingen,", "tokens": ["Zur", "Lie\u00b7ben", "wollt", "ich", "mich", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ins Fenster springen.", "tokens": ["Ins", "Fens\u00b7ter", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Alle. H\u00e4ttst gern gethan, u. s. w.             ", "tokens": ["Al\u00b7le", ".", "H\u00e4ttst", "gern", "ge\u00b7than", ",", "u.", "s.", "w."], "token_info": ["word", "punct", "word", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["PIAT", "$.", "VVFIN", "ADV", "VVPP", "$,", "KON", "VVIMP", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Dieterlein. Wollt Gott, ich w\u00e4r ein klein K\u00e4tzelein,               ", "tokens": ["Die\u00b7ter\u00b7lein", ".", "Wollt", "Gott", ",", "ich", "w\u00e4r", "ein", "klein", "K\u00e4t\u00b7zel\u00b7ein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "NN", "$,", "PPER", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Klein K\u00e4tzelein,", "tokens": ["Klein", "K\u00e4t\u00b7zel\u00b7ein", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Gar lieblich wollt ich ihr mausen", "tokens": ["Gar", "lieb\u00b7lich", "wollt", "ich", "ihr", "mau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In ihrem Hause.", "tokens": ["In", "ih\u00b7rem", "Hau\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Alle. H\u00e4ttst gern gethan, u. s. w.             ", "tokens": ["Al\u00b7le", ".", "H\u00e4ttst", "gern", "ge\u00b7than", ",", "u.", "s.", "w."], "token_info": ["word", "punct", "word", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["PIAT", "$.", "VVFIN", "ADV", "VVPP", "$,", "KON", "VVIMP", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.9": {"line.1": {"text": "Dieterlein. Wollt Gott, ich w\u00e4r ein klein H\u00fcndelein,               ", "tokens": ["Die\u00b7ter\u00b7lein", ".", "Wollt", "Gott", ",", "ich", "w\u00e4r", "ein", "klein", "H\u00fcn\u00b7del\u00b7ein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "NN", "$,", "PPER", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "H\u00fcndelein klein,", "tokens": ["H\u00fcn\u00b7del\u00b7ein", "klein", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Gar treulich wollt ich ihr jagen,", "tokens": ["Gar", "treu\u00b7lich", "wollt", "ich", "ihr", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Hirsch und Hasen.", "tokens": ["Die", "Hirsch", "und", "Ha\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Alle. H\u00e4ttst gern gethan, u. s. w.             ", "tokens": ["Al\u00b7le", ".", "H\u00e4ttst", "gern", "ge\u00b7than", ",", "u.", "s.", "w."], "token_info": ["word", "punct", "word", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["PIAT", "$.", "VVFIN", "ADV", "VVPP", "$,", "KON", "VVIMP", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "Dieterlein. Wollt Gott, ich w\u00e4r ein klein Pferdelein,               ", "tokens": ["Die\u00b7ter\u00b7lein", ".", "Wollt", "Gott", ",", "ich", "w\u00e4r", "ein", "klein", "Pfer\u00b7de\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "NN", "$,", "PPER", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Artig Zeltelein,", "tokens": ["Ar\u00b7tig", "Zel\u00b7tel\u00b7ein", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Gar sanfte wollt ich ihr traben,", "tokens": ["Gar", "sanf\u00b7te", "wollt", "ich", "ihr", "tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zu ihrem Knaben.", "tokens": ["Zu", "ih\u00b7rem", "Kna\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Alle. H\u00e4ttst gern gethan, u. s. w.             ", "tokens": ["Al\u00b7le", ".", "H\u00e4ttst", "gern", "ge\u00b7than", ",", "u.", "s.", "w."], "token_info": ["word", "punct", "word", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["PIAT", "$.", "VVFIN", "ADV", "VVPP", "$,", "KON", "VVIMP", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.13": {"line.1": {"text": "Dieterlein. Zu ihrem Knaben ins K\u00e4mmerlein,               ", "tokens": ["Die\u00b7ter\u00b7lein", ".", "Zu", "ih\u00b7rem", "Kna\u00b7ben", "ins", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ins K\u00e4mmerlein,", "tokens": ["Ins", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Gern w\u00fcrd ich dann sehen,", "tokens": ["Gern", "w\u00fcrd", "ich", "dann", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Euch Herren gehen.", "tokens": ["Euch", "Her\u00b7ren", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Alle. Drauf trinken wir alle               ", "tokens": ["Al\u00b7le", ".", "Drauf", "trin\u00b7ken", "wir", "al\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "$.", "PAV", "VVFIN", "PPER", "PIAT"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Diesen Wein mit Schalle,", "tokens": ["Die\u00b7sen", "Wein", "mit", "Schal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dieser Wein vor anderm Wein,", "tokens": ["Die\u00b7ser", "Wein", "vor", "an\u00b7derm", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "PIS", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist aller Welt ein F\u00fcrste,", "tokens": ["Ist", "al\u00b7ler", "Welt", "ein", "F\u00fcrs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Trink mein lieber Dieterlein,", "tokens": ["Trink", "mein", "lie\u00b7ber", "Die\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und da\u00df dich nimmer d\u00fcrste,", "tokens": ["Und", "da\u00df", "dich", "nim\u00b7mer", "d\u00fcrs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Trinks gar aus,", "tokens": ["Trinks", "gar", "aus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Trinks gar aus.", "tokens": ["Trinks", "gar", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.15": {"line.1": {"text": "Dieterlein. Der Wein schmeckt wohl,               ", "tokens": ["Die\u00b7ter\u00b7lein", ".", "Der", "Wein", "schmeckt", "wohl", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht mich oft trunken,", "tokens": ["Macht", "mich", "oft", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Darum soll man ihn loben,", "tokens": ["Da\u00b7rum", "soll", "man", "ihn", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PIS", "PPER", "VVINF", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mir ist verk\u00fcndt,", "tokens": ["Mir", "ist", "ver\u00b7k\u00fcndt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,"], "meter": "++-+", "measure": "iambic.di"}, "line.5": {"text": "Ein seltsam Spiel,", "tokens": ["Ein", "selt\u00b7sam", "Spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ein Vogel auf dem Brunnen,", "tokens": ["Ein", "Vo\u00b7gel", "auf", "dem", "Brun\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein seltsam Fang,", "tokens": ["Ein", "selt\u00b7sam", "Fang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Macht mich oft siech,", "tokens": ["Macht", "mich", "oft", "siech", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Vor Lachen mu\u00df ich schweigen,", "tokens": ["Vor", "La\u00b7chen", "mu\u00df", "ich", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Kurz Griff sind auf der Lauten.", "tokens": ["Kurz", "Griff", "sind", "auf", "der", "Lau\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Alle. So trinken wir d