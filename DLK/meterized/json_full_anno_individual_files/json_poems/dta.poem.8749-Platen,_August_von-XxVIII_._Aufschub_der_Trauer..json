{"dta.poem.8749": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxVIII .  \n  Aufschub der Trauer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Das schatt'ge Gr\u00fcn, o wie dich's k\u00fchlt!", "tokens": ["Das", "schatt'\u00b7ge", "Gr\u00fcn", ",", "o", "wie", "dich's", "k\u00fchlt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "FM", "KOKOM", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie leicht ist all das Weh verschmerzt,", "tokens": ["Wie", "leicht", "ist", "all", "das", "Weh", "ver\u00b7schmerzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PIAT", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das in der Seele w\u00fchlt!", "tokens": ["Das", "in", "der", "See\u00b7le", "w\u00fchlt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des Liebchens Bildni\u00df zeige sich", "tokens": ["Des", "Lieb\u00b7chens", "Bild\u00b7ni\u00df", "zei\u00b7ge", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In jedem Quell, an dem du stehst,", "tokens": ["In", "je\u00b7dem", "Quell", ",", "an", "dem", "du", "stehst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein sanftes Lied beruh'ge dich,", "tokens": ["Ein", "sanf\u00b7tes", "Lied", "be\u00b7ruh'\u00b7ge", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn durch den Wald du gehst.", "tokens": ["Wenn", "durch", "den", "Wald", "du", "gehst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "D'rum warte, bis der Winter naht,", "tokens": ["D'\u00b7rum", "war\u00b7te", ",", "bis", "der", "Win\u00b7ter", "naht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Bis alles starr und \u00f6de liegt,", "tokens": ["Bis", "al\u00b7les", "starr", "und", "\u00f6\u00b7de", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Reif und Schnee auf Flur und Saat", "tokens": ["Und", "Reif", "und", "Schnee", "auf", "Flur", "und", "Saat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich melancholisch wiegt!", "tokens": ["Dich", "me\u00b7lan\u00b7cho\u00b7lisch", "wiegt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}