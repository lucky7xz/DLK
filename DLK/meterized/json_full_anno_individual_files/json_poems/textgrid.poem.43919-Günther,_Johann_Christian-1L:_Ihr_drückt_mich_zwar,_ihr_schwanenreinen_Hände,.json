-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Meldet ihr dabey", "tokens": ["Mel\u00b7det", "ihr", "da\u00b7bey"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Den Ursprung solcher Qual", "tokens": ["Den", "Ur\u00b7sprung", "sol\u00b7cher", "Qual"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und sagt, es sey", "tokens": ["Und", "sagt", ",", "es", "sey"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Nichts anders als ein Strahl.", "tokens": ["Nichts", "an\u00b7ders", "als", "ein", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein holder Strahl der feuerreichen Blicke", "tokens": ["Ein", "hol\u00b7der", "Strahl", "der", "feu\u00b7er\u00b7rei\u00b7chen", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Steckt unverhoft den Siz der Freyheit an;", "tokens": ["Steckt", "un\u00b7ver\u00b7hoft", "den", "Siz", "der", "Frey\u00b7heit", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da diese flieht, so bleibt kein Trost zur\u00fccke,", "tokens": ["Da", "die\u00b7se", "flieht", ",", "so", "bleibt", "kein", "Trost", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS