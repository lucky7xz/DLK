{"dta.poem.18490": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines Wucherers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der mit dem Juden-spies bey tag und nacht gerannt/", "tokens": ["Der", "mit", "dem", "Ju\u00b7den\u00b7spies", "bey", "tag", "und", "nacht", "ge\u00b7rannt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Um wucher unerm\u00fcdt durchstrichen see und landt/", "tokens": ["Um", "wu\u00b7cher", "un\u00b7er\u00b7m\u00fcdt", "durch\u00b7stri\u00b7chen", "see", "und", "landt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "ADJD", "ADJA", "NN", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und auf gewinn gedacht mit viel gefahr und sorgen/", "tokens": ["Und", "auf", "ge\u00b7winn", "ge\u00b7dacht", "mit", "viel", "ge\u00b7fahr", "und", "sor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "APPR", "PIAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der ist gantz nackt und blos in dieser grufft verborgen.", "tokens": ["Der", "ist", "gantz", "nackt", "und", "blos", "in", "die\u00b7ser", "grufft", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "KON", "ADV", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-