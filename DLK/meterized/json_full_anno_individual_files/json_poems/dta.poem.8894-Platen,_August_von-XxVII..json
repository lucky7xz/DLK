{"dta.poem.8894": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Mit ihrer Farbenpracht dem Meer entstiegen,", "tokens": ["Mit", "ih\u00b7rer", "Far\u00b7ben\u00b7pracht", "dem", "Meer", "ent\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier scheint auf bunten Wolken sie zu fliegen,", "tokens": ["Hier", "scheint", "auf", "bun\u00b7ten", "Wol\u00b7ken", "sie", "zu", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gleich einer zauberischen Fee Morgane.", "tokens": ["Gleich", "ei\u00b7ner", "zau\u00b7be\u00b7ri\u00b7schen", "Fee", "Mor\u00b7ga\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie seyd ihr gro\u00df, ihr hohen Tiziane,", "tokens": ["Wie", "seyd", "ihr", "gro\u00df", ",", "ihr", "ho\u00b7hen", "Ti\u00b7zi\u00b7a\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie zart Bellin, dal Piombo wie gediegen,", "tokens": ["Wie", "zart", "Bel\u00b7lin", ",", "dal", "Piom\u00b7bo", "wie", "ge\u00b7die\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NE", "$,", "KOUS", "NE", "PWAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und o wie lernt sich ird'scher Schmerz besiegen", "tokens": ["Und", "o", "wie", "lernt", "sich", "ird'\u00b7scher", "Schmerz", "be\u00b7sie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "FM", "KOKOM", "VVFIN", "PRF", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor Paolo's heiligem Sebastiane!", "tokens": ["Vor", "Pao\u00b7lo's", "hei\u00b7li\u00b7gem", "Se\u00b7bas\u00b7ti\u00b7a\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch was auch Farb' und Pinsel hier vollbrachte,", "tokens": ["Doch", "was", "auch", "Fa\u00b7rb'", "und", "Pin\u00b7sel", "hier", "voll\u00b7brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "NN", "KON", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Der Meissel ist nicht ungebraucht geblieben,", "tokens": ["Der", "Meis\u00b7sel", "ist", "nicht", "un\u00b7ge\u00b7braucht", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und manchen Stein durchdringt das Sch\u00f6ngedachte:", "tokens": ["Und", "man\u00b7chen", "Stein", "durch\u00b7dringt", "das", "Sch\u00f6n\u00b7ge\u00b7dach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ja, wen es je nach San Giulian getrieben,", "tokens": ["Ja", ",", "wen", "es", "je", "nach", "San", "Giu\u00b7li\u00b7an", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "PPER", "ADV", "APPR", "NE", "NE", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Damit er dort des Heilands Schlaf betrachte,", "tokens": ["Da\u00b7mit", "er", "dort", "des", "Hei\u00b7lands", "Schlaf", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der mu\u00df den g\u00f6ttlichen Campagna lieben!", "tokens": ["Der", "mu\u00df", "den", "g\u00f6tt\u00b7li\u00b7chen", "Cam\u00b7pag\u00b7na", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}