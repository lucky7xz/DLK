"tokens": ["Das", "So\u00b7lo", "ei\u00b7ner", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Bald rudern auf bekr\u00e4nztem Kahn,", "tokens": ["Bald", "ru\u00b7dern", "auf", "be\u00b7kr\u00e4nz\u00b7tem", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den See hinab, den See hinan;", "tokens": ["Den", "See", "hin\u00b7ab", ",", "den", "See", "hi\u00b7nan", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald Fischchen angeln aus der Flut,", "tokens": ["Bald", "Fischchen", "an\u00b7geln", "aus", "der", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald locken junge V\u00f6gelbrut;", "tokens": ["Bald", "lo\u00b7cken", "jun\u00b7ge", "V\u00f6\u00b7gel\u00b7brut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Bald atmen auf der Maienflur", "tokens": ["Bald", "at\u00b7men", "auf", "der", "Mai\u00b7en\u00b7flur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Balsam bl\u00fchender Natur;", "tokens": ["Den", "Bal\u00b7sam", "bl\u00fc\u00b7hen\u00b7der", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald, um die d\u00fcnnbebuschten H\u00f6hn,", "tokens": ["Bald", ",", "um", "die", "d\u00fcnn\u00b7be\u00b7buschten", "H\u00f6hn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUI", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Nach Erd- und Heidelbeeren gehn.", "tokens": ["Nach", "Erd", "und", "Hei\u00b7del\u00b7bee\u00b7ren", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein Blumengurt, ein Myrtenhut", "tokens": ["Ein", "Blu\u00b7men\u00b7gurt", ",", "ein", "Myr\u00b7ten\u00b7hut"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "K\u00fchlt Liebchen vor des Sommers Glut.", "tokens": ["K\u00fchlt", "Lieb\u00b7chen", "vor", "des", "Som\u00b7mers", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist Liebchen m\u00fcde, bett' ich's gleich", "tokens": ["Ist", "Lieb\u00b7chen", "m\u00fc\u00b7de", ",", "bett'", "ich's", "gleich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NN", "ADJD", "$,", "VVFIN", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Moos und Thymi\u00e4nchen weich.", "tokens": ["Auf", "Moos", "und", "Thy\u00b7mi\u00b7\u00e4n\u00b7chen", "weich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ein Wams, verbr\u00e4mt mit Schwanenfell,", "tokens": ["Ein", "Wams", ",", "ver\u00b7br\u00e4mt", "mit", "Schwa\u00b7nen\u00b7fell", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Kn\u00f6pfen von Krystallen hell,", "tokens": ["Mit", "Kn\u00f6p\u00b7fen", "von", "Krys\u00b7tal\u00b7len", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein R\u00f6ckchen wei\u00df, aus zarter Woll',", "tokens": ["Ein", "R\u00f6ck\u00b7chen", "wei\u00df", ",", "aus", "zar\u00b7ter", "Woll'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus L\u00e4mmchenwoll' es tragen soll.", "tokens": ["Aus", "L\u00e4mm\u00b7chen\u00b7woll'", "es", "tra\u00b7gen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und h\u00fcpfen soll's in Saffian,", "tokens": ["Und", "h\u00fcp\u00b7fen", "soll's", "in", "Saf\u00b7fi\u00b7an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit goldnen Sp\u00e4nglein auf dem Spann,", "tokens": ["Mit", "gold\u00b7nen", "Sp\u00e4ng\u00b7lein", "auf", "dem", "Spann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3":