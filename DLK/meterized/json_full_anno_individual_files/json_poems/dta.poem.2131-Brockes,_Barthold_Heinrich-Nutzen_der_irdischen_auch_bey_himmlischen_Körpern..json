{"dta.poem.2131": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nutzen der irdischen auch bey  \n himmlischen K\u00f6rpern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah den dickbelaubten Wipfel von einem hohen", "tokens": ["Ich", "sah", "den", "dick\u00b7be\u00b7laub\u00b7ten", "Wip\u00b7fel", "von", "ei\u00b7nem", "ho\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Eichbaum an,", "tokens": ["Eich\u00b7baum", "an", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Durch dessen dichtverschr\u00e4nkte Bl\u00e4tter, und gr\u00fcner", "tokens": ["Durch", "des\u00b7sen", "dicht\u00b7ver\u00b7schr\u00e4nk\u00b7te", "Bl\u00e4t\u00b7ter", ",", "und", "gr\u00fc\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "$,", "KON", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Schatten Dunkelheit,", "tokens": ["Schat\u00b7ten", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wie tief er sich darinn versenkt, der schnelle Blick nicht", "tokens": ["Wie", "tief", "er", "sich", "da\u00b7rinn", "ver\u00b7senkt", ",", "der", "schnel\u00b7le", "Blick", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "PRF", "PAV", "VVFIN", "$,", "ART", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "dringen kann,", "tokens": ["drin\u00b7gen", "kann", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Bis ich zuletzt, an ein Paar Stellen, des hellen Himmels", "tokens": ["Bis", "ich", "zu\u00b7letzt", ",", "an", "ein", "Paar", "Stel\u00b7len", ",", "des", "hel\u00b7len", "Him\u00b7mels"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+--++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Heiterkeit", "tokens": ["Hei\u00b7ter\u00b7keit"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Wie helle Sterne schimmern sah, die ich um desto mehr", "tokens": ["Wie", "hel\u00b7le", "Ster\u00b7ne", "schim\u00b7mern", "sah", ",", "die", "ich", "um", "des\u00b7to", "mehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "VVFIN", "$,", "PRELS", "PPER", "APPR", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "bemerkte,", "tokens": ["be\u00b7merk\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Als mir der dunkle Gegensatz der gr\u00fcnen Nacht die Au-", "tokens": ["Als", "mir", "der", "dunk\u00b7le", "Ge\u00b7gen\u00b7satz", "der", "gr\u00fc\u00b7nen", "Nacht", "die", "Au"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "gen st\u00e4rkte.", "tokens": ["gen", "st\u00e4rk\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Es zeigt mir, dacht ich, die\u00df Gesicht die Wahrheit mit", "tokens": ["Es", "zeigt", "mir", ",", "dacht", "ich", ",", "die\u00df", "Ge\u00b7sicht", "die", "Wahr\u00b7heit", "mit"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "PDS", "NN", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vergn\u00fcgen an:", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "an", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Wie irdsche Dunkelheit den Glanz des Himmels noch", "tokens": ["Wie", "ird\u00b7sche", "Dun\u00b7kel\u00b7heit", "den", "Glanz", "des", "Him\u00b7mels", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "ART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "versch\u00f6nern kann.", "tokens": ["ver\u00b7sch\u00f6\u00b7nern", "kann", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}