{"dta.poem.13898": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "V .  \n  Die Heymonskinder .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "id:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ei sieh da, so einsam Herr Komponist!", "tokens": ["Ei", "sieh", "da", ",", "so", "ein\u00b7sam", "Herr", "Kom\u00b7po\u00b7nist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,", "ADV", "ADJD", "NN", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Steig' auf mit, 's ist Schad' um die Schuh,", "tokens": ["Steig'", "auf", "mit", ",", "'", "s", "ist", "Schad", "'", "um", "die", "Schuh", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PTKVZ", "$,", "$(", "PPER", "VAFIN", "NE", "$(", "APPR", "ART", "NN", "$,"], "meter": "-----+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Du l\u00f6st erst die Schwinge \u2014 und wo keine ist,", "tokens": ["Du", "l\u00f6st", "erst", "die", "Schwin\u00b7ge", "und", "wo", "kei\u00b7ne", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$(", "KON", "PWAV", "PIAT", "VAFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da mach' uns die Fl\u00fcgel dazu!", "tokens": ["Da", "mach'", "uns", "die", "Fl\u00fc\u00b7gel", "da\u00b7zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}