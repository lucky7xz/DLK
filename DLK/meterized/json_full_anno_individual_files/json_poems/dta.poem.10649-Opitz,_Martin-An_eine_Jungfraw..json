{"dta.poem.10649": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "An eine Jungfraw.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Vmb alles Gut vnd Gelt in diesem gantzen Lande", "tokens": ["Vmb", "al\u00b7les", "Gut", "vnd", "Gelt", "in", "die\u00b7sem", "gant\u00b7zen", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ADJD", "KON", "NN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sag ich noch euch/ Jungfraw/ noch andern was zu schande/", "tokens": ["Sag", "ich", "noch", "euch", "/", "Jung\u00b7fraw", "/", "noch", "an\u00b7dern", "was", "zu", "schan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPER", "$(", "NN", "$(", "ADV", "ADJA", "PRELS", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd wolte Warlich nicht/ da\u00df jemandt sprechen solt/", "tokens": ["Vnd", "wol\u00b7te", "War\u00b7lich", "nicht", "/", "da\u00df", "je\u00b7mandt", "spre\u00b7chen", "solt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "$(", "KOUS", "PIS", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich wolt jhn nur au\u00df Ha\u00df verkleinern vnverschuldt/", "tokens": ["Ich", "wolt", "jhn", "nur", "au\u00df", "Ha\u00df", "ver\u00b7klei\u00b7nern", "vn\u00b7ver\u00b7schuldt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr m\u00f6get aber doch darneben k\u00fcnlich gleuben/", "tokens": ["Ihr", "m\u00f6\u00b7get", "a\u00b7ber", "doch", "dar\u00b7ne\u00b7ben", "k\u00fcn\u00b7lich", "gleu\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PAV", "ADJD", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Da\u00df ich ohn euch/ Gott lob/ wol werde leben bleiben/", "tokens": ["Da\u00df", "ich", "ohn", "euch", "/", "Gott", "lob", "/", "wol", "wer\u00b7de", "le\u00b7ben", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "$(", "NN", "NN", "$(", "ADV", "VAFIN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich will derhalben auch mich nimmer vnderstehn/", "tokens": ["Ich", "will", "der\u00b7hal\u00b7ben", "auch", "mich", "nim\u00b7mer", "vn\u00b7der\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "ADV", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von wegen ewrer Huldt mit Lugen vmbzugehn/", "tokens": ["Von", "we\u00b7gen", "ew\u00b7rer", "Huldt", "mit", "Lu\u00b7gen", "vmb\u00b7zu\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Di\u00df alles la\u00df ich euch die Hoffeleuth erzeigen/", "tokens": ["Di\u00df", "al\u00b7les", "la\u00df", "ich", "euch", "die", "Hof\u00b7fe\u00b7leuth", "er\u00b7zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVIMP", "PPER", "PRF", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die sonsten zimlich hoch mit reden k\u00f6nnen steigen/", "tokens": ["Die", "sons\u00b7ten", "zim\u00b7lich", "hoch", "mit", "re\u00b7den", "k\u00f6n\u00b7nen", "stei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ADJD", "APPR", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die jedes Wort auffziehen mit sonderlichem schein/", "tokens": ["Die", "je\u00b7des", "Wort", "auff\u00b7zie\u00b7hen", "mit", "son\u00b7der\u00b7li\u00b7chem", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Auff da\u00df sie desto mehr in ewrer Gnade sein.", "tokens": ["Auff", "da\u00df", "sie", "des\u00b7to", "mehr", "in", "ew\u00b7rer", "Gna\u00b7de", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Sie thun wol einen Eydt/ doch nicht ohn heimlich lachen/", "tokens": ["Sie", "thun", "wol", "ei\u00b7nen", "Eydt", "/", "doch", "nicht", "ohn", "heim\u00b7lich", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$(", "ADV", "PTKNEG", "APPR", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df ewer \u00e4ugelein die Sonne finster machen/", "tokens": ["Da\u00df", "e\u00b7wer", "\u00e4u\u00b7ge\u00b7lein", "die", "Son\u00b7ne", "fins\u00b7ter", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Vnd da\u00df sie Sternen sein am hellen Firmament/", "tokens": ["Vnd", "da\u00df", "sie", "Ster\u00b7nen", "sein", "am", "hel\u00b7len", "Fir\u00b7ma\u00b7ment", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "PPOSAT", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Von denen Frewd vnd Leyd zu jhnen wirdt gewendt/", "tokens": ["Von", "de\u00b7nen", "Frewd", "vnd", "Leyd", "zu", "jh\u00b7nen", "wirdt", "ge\u00b7wendt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "KON", "NN", "APPR", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sie schw\u00f6ren hoch vnd thewr/ da\u00df Gott euch au\u00dferlesen/", "tokens": ["Sie", "schw\u00f6\u00b7ren", "hoch", "vnd", "thewr", "/", "da\u00df", "Gott", "euch", "au\u00b7\u00dfer\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$(", "KOUS", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Vor aller Zierligkeit/ vor allem sch\u00f6nen Wesen/", "tokens": ["Vor", "al\u00b7ler", "Zier\u00b7lig\u00b7keit", "/", "vor", "al\u00b7lem", "sch\u00f6\u00b7nen", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$(", "APPR", "PIS", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Vnd sagen/ seelig sey das Jahr vnd denn die Zeit/", "tokens": ["Vnd", "sa\u00b7gen", "/", "see\u00b7lig", "sey", "das", "Jahr", "vnd", "denn", "die", "Zeit", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$(", "ADJD", "VAFIN", "ART", "NN", "KON", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "In der jhr grosse Zier der Welt geboren seit/", "tokens": ["In", "der", "jhr", "gros\u00b7se", "Zier", "der", "Welt", "ge\u00b7bo\u00b7ren", "seit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVPP", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sie sprechen wol darbey/ da\u00df jhr durch blo\u00df anblicken/", "tokens": ["Sie", "spre\u00b7chen", "wol", "dar\u00b7bey", "/", "da\u00df", "jhr", "durch", "blo\u00df", "an\u00b7bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$(", "KOUS", "PPER", "APPR", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Auch ein Gem\u00fct von Stein verm\u00f6get zu entz\u00fccken/", "tokens": ["Auch", "ein", "Ge\u00b7m\u00fct", "von", "Stein", "ver\u00b7m\u00f6\u00b7get", "zu", "ent\u00b7z\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "VVPP", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Da\u00df au\u00df America die beste Specerey", "tokens": ["Da\u00df", "au\u00df", "A\u00b7me\u00b7ri\u00b7ca", "die", "bes\u00b7te", "Spe\u00b7ce\u00b7rey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Mit ewrem Athem weit nicht zuvergleichen sey.", "tokens": ["Mit", "ew\u00b7rem", "A\u00b7them", "weit", "nicht", "zu\u00b7ver\u00b7glei\u00b7chen", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PTKNEG", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da\u00df sch\u00f6ner H\u00e4nde nicht gemahlet werden k\u00fcnten/", "tokens": ["Da\u00df", "sch\u00f6\u00b7ner", "H\u00e4n\u00b7de", "nicht", "ge\u00b7mah\u00b7let", "wer\u00b7den", "k\u00fcn\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PTKNEG", "VVPP", "VAINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da\u00df gegen jhnen Schnee zugleichen sey der Dinten/", "tokens": ["Da\u00df", "ge\u00b7gen", "jh\u00b7nen", "Schnee", "zu\u00b7glei\u00b7chen", "sey", "der", "Din\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "NN", "VVPP", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df iedes Z\u00e4nlin sey ein k\u00f6stlicher Demant/", "tokens": ["Da\u00df", "ie\u00b7des", "Z\u00e4n\u00b7lin", "sey", "ein", "k\u00f6st\u00b7li\u00b7cher", "De\u00b7mant", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.28": {"text": "Das die Natur daran all jhre Kunst gewant.", "tokens": ["Das", "die", "Na\u00b7tur", "da\u00b7ran", "all", "jhre", "Kunst", "ge\u00b7want", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PAV", "PIAT", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Vnd da\u00df die Lippen auch/ so als die Rosen bl\u00fchen/", "tokens": ["Vnd", "da\u00df", "die", "Lip\u00b7pen", "auch", "/", "so", "als", "die", "Ro\u00b7sen", "bl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "$(", "ADV", "KOUS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Weit sein den Edelsten Corallen vorzuziehen/", "tokens": ["Weit", "sein", "den", "E\u00b7dels\u00b7ten", "Co\u00b7ral\u00b7len", "vor\u00b7zu\u00b7zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "ART", "ADJA", "NN", "VVIZU", "$("], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.31": {"text": "Das Haar (ich glaube nicht da\u00df es von Hertzen k\u00fcmpt)", "tokens": ["Das", "Haar", "(", "ich", "glau\u00b7be", "nicht", "da\u00df", "es", "von", "Hert\u00b7zen", "k\u00fcmpt", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPER", "VVFIN", "PTKNEG", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ein jeglicher vor Golt vnd beste Perlen nimpt.", "tokens": ["Ein", "jeg\u00b7li\u00b7cher", "vor", "Golt", "vnd", "bes\u00b7te", "Per\u00b7len", "nimpt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "APPR", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Sie setzen wol hinzu/ wann sie euch reden h\u00f6ren/", "tokens": ["Sie", "set\u00b7zen", "wol", "hin\u00b7zu", "/", "wann", "sie", "euch", "re\u00b7den", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$(", "PWAV", "PPER", "PPER", "VVINF", "VVINF", "$("], "meter": "-+-+----+--+-", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Da\u00df auch ein jedes Wort jhr Hertze thu versehren/", "tokens": ["Da\u00df", "auch", "ein", "je\u00b7des", "Wort", "jhr", "Hert\u00b7ze", "thu", "ver\u00b7seh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "PIAT", "NN", "PPER", "VVFIN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Vnd da\u00df der starcke Mars durch ewre Liebligkeit", "tokens": ["Vnd", "da\u00df", "der", "star\u00b7cke", "Mars", "durch", "ew\u00b7re", "Lieb\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die Waffen abzuthun euch w\u00fcrde sein bereit.", "tokens": ["Die", "Waf\u00b7fen", "ab\u00b7zu\u00b7thun", "euch", "w\u00fcr\u00b7de", "sein", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "PPER", "VAFIN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Geliebet euch hernach von Venus was zusingen/", "tokens": ["Ge\u00b7lie\u00b7bet", "euch", "her\u00b7nach", "von", "Ve\u00b7nus", "was", "zu\u00b7sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die Winde k\u00f6nnet jhr durch ewre Stimme zwingen/", "tokens": ["Die", "Win\u00b7de", "k\u00f6n\u00b7net", "jhr", "durch", "ew\u00b7re", "Stim\u00b7me", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Vnd wann jhr weiter auch euch zu der Lauten findt/", "tokens": ["Vnd", "wann", "jhr", "wei\u00b7ter", "auch", "euch", "zu", "der", "Lau\u00b7ten", "findt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ist Orpheus vngelehrt/ vnd gegen euch ein Kindt.", "tokens": ["Ist", "Or\u00b7pheus", "vn\u00b7ge\u00b7lehrt", "/", "vnd", "ge\u00b7gen", "euch", "ein", "Kindt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADJD", "$(", "KON", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wann jhr zu Felde kompt wohin man euch sicht gehen/", "tokens": ["Wann", "jhr", "zu", "Fel\u00b7de", "kompt", "wo\u00b7hin", "man", "euch", "sicht", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word