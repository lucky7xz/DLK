{"dta.poem.5848": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch selbst in Dingen, die so klein,", "tokens": ["Auch", "selbst", "in", "Din\u00b7gen", ",", "die", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df, wegen ihrer Niedrigkeit", "tokens": ["Da\u00df", ",", "we\u00b7gen", "ih\u00b7rer", "Nied\u00b7rig\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie, wie es scheinet, kaum des Ansehns w\u00fcrdig seyn,", "tokens": ["Sie", ",", "wie", "es", "schei\u00b7net", ",", "kaum", "des", "An\u00b7sehns", "w\u00fcr\u00b7dig", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da werden wir, wenn wir es wol ergr\u00fcnden,", "tokens": ["Da", "wer\u00b7den", "wir", ",", "wenn", "wir", "es", "wol", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dieselben Ordnungen dieselbe Weisheit finden.", "tokens": ["Die\u00b7sel\u00b7ben", "Ord\u00b7nun\u00b7gen", "die\u00b7sel\u00b7be", "Weis\u00b7heit", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PDAT", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Die in den herrlichsten und auserlesnen man", "tokens": ["Die", "in", "den", "herr\u00b7lichs\u00b7ten", "und", "au\u00b7ser\u00b7les\u00b7nen", "man"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "KON", "VVFIN", "PIS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nur immer finden kan.", "tokens": ["Nur", "im\u00b7mer", "fin\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}