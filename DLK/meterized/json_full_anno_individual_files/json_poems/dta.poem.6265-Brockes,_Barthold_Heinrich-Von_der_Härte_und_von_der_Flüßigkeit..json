{"dta.poem.6265": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Marmor und das Ertzt, die hart, die lange bleiben,", "tokens": ["Der", "Mar\u00b7mor", "und", "das", "Ertzt", ",", "die", "hart", ",", "die", "lan\u00b7ge", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "PRELS", "ADJD", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "(worinn, der Zeit zu Trotz, wir Helden-Nahmen schreiben)", "tokens": ["(", "wo\u00b7rinn", ",", "der", "Zeit", "zu", "Trotz", ",", "wir", "Hel\u00b7den\u00b7Nah\u00b7men", "schrei\u00b7ben", ")"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$,", "ART", "NN", "APPR", "NN", "$,", "PPER", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die m\u00e4chtig einen Sturm von aussen abzutreiben;", "tokens": ["Die", "m\u00e4ch\u00b7tig", "ei\u00b7nen", "Sturm", "von", "aus\u00b7sen", "ab\u00b7zu\u00b7trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "APPR", "NE", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sind undurchdringlich f\u00fcr die Lufft.", "tokens": ["Sind", "un\u00b7durch\u00b7dring\u00b7lich", "f\u00fcr", "die", "Lufft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn ihre L\u00f6cherchen, die enge,", "tokens": ["Denn", "ih\u00b7re", "L\u00f6\u00b7cher\u00b7chen", ",", "die", "en\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Verstatten durch so kleine G\u00e4nge", "tokens": ["Ver\u00b7stat\u00b7ten", "durch", "so", "klei\u00b7ne", "G\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gantz keinen Eingang einem Dufft,", "tokens": ["Gantz", "kei\u00b7nen", "Ein\u00b7gang", "ei\u00b7nem", "Dufft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Statt da\u00df er durch den Stahl gantz ungehindert gehet;", "tokens": ["Statt", "da\u00df", "er", "durch", "den", "Stahl", "gantz", "un\u00b7ge\u00b7hin\u00b7dert", "ge\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "APPR", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.9": {"text": "Wodurch sein Wesen denn so lange nicht bestehet.", "tokens": ["Wo\u00b7durch", "sein", "We\u00b7sen", "denn", "so", "lan\u00b7ge", "nicht", "be\u00b7ste\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "KON", "ADV", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Auch C\u00f6rperchen von Saltz, so sich in Eisen finden,", "tokens": ["Auch", "C\u00f6r\u00b7per\u00b7chen", "von", "Saltz", ",", "so", "sich", "in", "Ei\u00b7sen", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "APPR", "NE", "$,", "ADV", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sind schnell geschmoltzen, schnell zerst\u00f6hret.", "tokens": ["Sind", "schnell", "ge\u00b7schmolt\u00b7zen", ",", "schnell", "zer\u00b7st\u00f6h\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Das Eisen rostet dann, und wird gar leicht,", "tokens": ["Das", "Ei\u00b7sen", "ros\u00b7tet", "dann", ",", "und", "wird", "gar", "leicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "KON", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "So bald die L\u00fcffte feucht,", "tokens": ["So", "bald", "die", "L\u00fcff\u00b7te", "feucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Durch Lufft und Feuchtigkeit versehret.", "tokens": ["Durch", "Lufft", "und", "Feuch\u00b7tig\u00b7keit", "ver\u00b7seh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}