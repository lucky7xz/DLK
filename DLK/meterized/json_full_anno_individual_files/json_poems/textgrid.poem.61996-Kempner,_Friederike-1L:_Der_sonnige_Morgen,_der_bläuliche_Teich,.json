{"textgrid.poem.61996": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der sonnige Morgen, der bl\u00e4uliche Teich,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der sonnige Morgen, der bl\u00e4uliche Teich,", "tokens": ["Der", "son\u00b7ni\u00b7ge", "Mor\u00b7gen", ",", "der", "bl\u00e4u\u00b7li\u00b7che", "Teich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die lockigen betaueten Reben,", "tokens": ["Die", "lo\u00b7cki\u00b7gen", "be\u00b7tau\u00b7e\u00b7ten", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-++--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie spiegeln mir wieder, an Reizen so reich,", "tokens": ["Sie", "spie\u00b7geln", "mir", "wie\u00b7der", ",", "an", "Rei\u00b7zen", "so", "reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "An Wundern, das herrliche Leben!", "tokens": ["An", "Wun\u00b7dern", ",", "das", "herr\u00b7li\u00b7che", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Den Gott im Herzen, die eherne Macht, \u2013", "tokens": ["Den", "Gott", "im", "Her\u00b7zen", ",", "die", "e\u00b7her\u00b7ne", "Macht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct",