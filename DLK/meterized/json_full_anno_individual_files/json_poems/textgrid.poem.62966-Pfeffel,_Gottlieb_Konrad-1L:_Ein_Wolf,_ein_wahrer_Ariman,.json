{"textgrid.poem.62966": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Wolf, ein wahrer Ariman,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Wolf, ein wahrer Ariman,", "tokens": ["Ein", "Wolf", ",", "ein", "wah\u00b7rer", "A\u00b7ri\u00b7man", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der so viel Schaafe niedermachte,", "tokens": ["Der", "so", "viel", "Schaa\u00b7fe", "nie\u00b7der\u00b7mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als kaum der Fleischer Tamerlan", "tokens": ["Als", "kaum", "der", "Flei\u00b7scher", "Ta\u00b7mer\u00b7lan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Kriegsgott Menschenopfer brachte,", "tokens": ["Dem", "Kriegs\u00b7gott", "Men\u00b7schen\u00b7op\u00b7fer", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Lag auf den Tod am Magenkrampf", "tokens": ["Lag", "auf", "den", "Tod", "am", "Ma\u00b7gen\u00b7krampf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In seiner Kluft. Sein treuer Vetter", "tokens": ["In", "sei\u00b7ner", "Kluft", ".", "Sein", "treu\u00b7er", "Vet\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Spie\u00dfgesell, ein frecher Sp\u00f6tter,", "tokens": ["Und", "Spie\u00df\u00b7ge\u00b7sell", ",", "ein", "fre\u00b7cher", "Sp\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Besucht ihn, um im letzten Kampf", "tokens": ["Be\u00b7sucht", "ihn", ",", "um", "im", "letz\u00b7ten", "Kampf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUI", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ihm beyzustehen: Alle Wetter!", "tokens": ["Ihm", "bey\u00b7zu\u00b7ste\u00b7hen", ":", "Al\u00b7le", "Wet\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Rief er, was machst du armer Gauch,", "tokens": ["Rief", "er", ",", "was", "machst", "du", "ar\u00b7mer", "Gauch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Zwickt dich vielleicht ein Lamm im Bauch?", "tokens": ["Zwickt", "dich", "viel\u00b7leicht", "ein", "Lamm", "im", "Bauch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Steh auf; la\u00df uns ein Schmalthier jagen;", "tokens": ["Steh", "auf", ";", "la\u00df", "uns", "ein", "Schmal\u00b7thier", "ja\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "VVIMP", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ein Teufel treibt den andern aus. \u2013", "tokens": ["Ein", "Teu\u00b7fel", "treibt", "den", "an\u00b7dern", "aus", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Was sagst du? Zittre vor dem R\u00e4cher", "tokens": ["Was", "sagst", "du", "?", "Zitt\u00b7re", "vor", "dem", "R\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Der Unschuld, sprach der kranke Sch\u00e4cher", "tokens": ["Der", "Un\u00b7schuld", ",", "sprach", "der", "kran\u00b7ke", "Sch\u00e4\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Mit schwacher Stimme: keine Maus", "tokens": ["Mit", "schwa\u00b7cher", "Stim\u00b7me", ":", "kei\u00b7ne", "Maus"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Will ich mehr t\u00f6dten: gleich den Bi\u00dfen", "tokens": ["Will", "ich", "mehr", "t\u00f6d\u00b7ten", ":", "gleich", "den", "Bi\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$.", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Der Viper nagt mich mein Gewi\u00dfen;", "tokens": ["Der", "Vi\u00b7per", "nagt", "mich", "mein", "Ge\u00b7wi\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Aleckto, mit dem H\u00f6llenpfuhl", "tokens": ["A\u00b7leck\u00b7to", ",", "mit", "dem", "H\u00f6l\u00b7len\u00b7pfuhl"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.20": {"text": "Im Blicke, st\u00fcrmet meine H\u00f6hle,", "tokens": ["Im", "Bli\u00b7cke", ",", "st\u00fcr\u00b7met", "mei\u00b7ne", "H\u00f6h\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und rei\u00dfet meine schwarze Seele", "tokens": ["Und", "rei\u00b7\u00dfet", "mei\u00b7ne", "schwar\u00b7ze", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Vor Minos ernsten Richterstuhl.", "tokens": ["Vor", "Mi\u00b7nos", "erns\u00b7ten", "Rich\u00b7ter\u00b7stuhl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Ha, Freund! Itzt flo\u00dfen seine Z\u00e4hren:", "tokens": ["Ha", ",", "Freund", "!", "Itzt", "flo\u00b7\u00dfen", "sei\u00b7ne", "Z\u00e4h\u00b7ren", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Wird Jupiter mein Flehn erh\u00f6ren,", "tokens": ["Wird", "Ju\u00b7pi\u00b7ter", "mein", "Flehn", "er\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Macht seine Gnade mich gesund;", "tokens": ["Macht", "sei\u00b7ne", "Gna\u00b7de", "mich", "ge\u00b7sund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "So will ich meine S\u00fcnden b\u00fc\u00dfen,", "tokens": ["So", "will", "ich", "mei\u00b7ne", "S\u00fcn\u00b7den", "b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Nur Wurzeln und nur Gras genie\u00dfen,", "tokens": ["Nur", "Wur\u00b7zeln", "und", "nur", "Gras", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Und mit dem frommen Sch\u00e4ferhund", "tokens": ["Und", "mit", "dem", "from\u00b7men", "Sch\u00e4\u00b7fer\u00b7hund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Die Heerde vor den W\u00f6lfen sch\u00fctzen,", "tokens": ["Die", "Heer\u00b7de", "vor", "den", "W\u00f6l\u00b7fen", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Ja, selbst mein Blut f\u00fcr sie verspritzen.", "tokens": ["Ja", ",", "selbst", "mein", "Blut", "f\u00fcr", "sie", "ver\u00b7sprit\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Der Vetter sch\u00fcttelte den Kopf,", "tokens": ["Der", "Vet\u00b7ter", "sch\u00fct\u00b7tel\u00b7te", "den", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Und sprach bey sich, der arme Tropf!", "tokens": ["Und", "sprach", "bey", "sich", ",", "der", "ar\u00b7me", "Tropf", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Das Fieber macht ihn phantasieren:", "tokens": ["Das", "Fie\u00b7ber", "macht", "ihn", "phan\u00b7ta\u00b7sie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Hier w\u00fcrden Luftklystier, Magnet,", "tokens": ["Hier", "w\u00fcr\u00b7den", "Luft\u00b7klys\u00b7tier", ",", "Mag\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.35": {"text": "Und selbst Apoll den Ruhm verlieren.", "tokens": ["Und", "selbst", "A\u00b7poll", "den", "Ruhm", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.36": {"text": "Er k\u00fc\u00dft den Freund und seufzt und geht.", "tokens": ["Er", "k\u00fc\u00dft", "den", "Freund", "und", "seufzt", "und", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Kaum bleicht der zackigte Planet", "tokens": ["Kaum", "bleicht", "der", "za\u00b7ckig\u00b7te", "Pla\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.38": {"text": "Zum andernmal die braunen Schatten,", "tokens": ["Zum", "an\u00b7dern\u00b7mal", "die", "brau\u00b7nen", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "So kehrt er in den Hain zur\u00fcck,", "tokens": ["So", "kehrt", "er", "in", "den", "Hain", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Um ihn zur Erde zu bestatten,", "tokens": ["Um", "ihn", "zur", "Er\u00b7de", "zu", "be\u00b7stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Und sieht ihn mit erstauntem Blick", "tokens": ["Und", "sieht", "ihn", "mit", "er\u00b7staun\u00b7tem", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Vor einem fetten Widder sitzen,", "tokens": ["Vor", "ei\u00b7nem", "fet\u00b7ten", "Wid\u00b7der", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Aus dem er Herz und Nieren fra\u00df.", "tokens": ["Aus", "dem", "er", "Herz", "und", "Nie\u00b7ren", "fra\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Ey, ey, Herr Bruder, was ist das?", "tokens": ["Ey", ",", "ey", ",", "Herr", "Bru\u00b7der", ",", "was", "ist", "das", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NN", "NN", "$,", "PWS", "VAFIN", "PDS", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.45": {"text": "Rief er, hei\u00dft das die Heerde sch\u00fctzen,", "tokens": ["Rief", "er", ",", "hei\u00dft", "das", "die", "Heer\u00b7de", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PDS", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.46": {"text": "Und selbst sein Blut f\u00fcr sie verspritzen?", "tokens": ["Und", "selbst", "sein", "Blut", "f\u00fcr", "sie", "ver\u00b7sprit\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Hier zog der graue B\u00f6sewicht", "tokens": ["Hier", "zog", "der", "grau\u00b7e", "B\u00f6\u00b7se\u00b7wicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Sein finster blutiges Gesicht", "tokens": ["Sein", "fins\u00b7ter", "blu\u00b7ti\u00b7ges", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Ins L\u00e4cheln, wie beym Sturm und Blitzen", "tokens": ["Ins", "L\u00e4\u00b7cheln", ",", "wie", "beym", "Sturm", "und", "Blit\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Das Seegespenst im Tafelgolf:", "tokens": ["Das", "See\u00b7ge\u00b7spenst", "im", "Ta\u00b7fel\u00b7golf", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Je nun, sprach er, und strich den Magen,", "tokens": ["Je", "nun", ",", "sprach", "er", ",", "und", "strich", "den", "Ma\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "PPER", "$,", "KON", "ADJD", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.52": {"text": "Ich war ein Lamm in kranken Tagen;", "tokens": ["Ich", "war", "ein", "Lamm", "in", "kran\u00b7ken", "Ta\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Gesund bin ich nun wieder Wolf.", "tokens": ["Ge\u00b7sund", "bin", "ich", "nun", "wie\u00b7der", "Wolf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADV", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Wolf, ein wahrer Ariman,", "tokens": ["Ein", "Wolf", ",", "ein", "wah\u00b7rer", "A\u00b7ri\u00b7man", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der so viel Schaafe niedermachte,", "tokens": ["Der", "so", "viel", "Schaa\u00b7fe", "nie\u00b7der\u00b7mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als kaum der Fleischer Tamerlan", "tokens": ["Als", "kaum", "der", "Flei\u00b7scher", "Ta\u00b7mer\u00b7lan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Kriegsgott Menschenopfer brachte,", "tokens": ["Dem", "Kriegs\u00b7gott", "Men\u00b7schen\u00b7op\u00b7fer", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Lag auf den Tod am Magenkrampf", "tokens": ["Lag", "auf", "den", "Tod", "am", "Ma\u00b7gen\u00b7krampf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In seiner Kluft. Sein treuer Vetter", "tokens": ["In", "sei\u00b7ner", "Kluft", ".", "Sein", "treu\u00b7er", "Vet\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Spie\u00dfgesell, ein frecher Sp\u00f6tter,", "tokens": ["Und", "Spie\u00df\u00b7ge\u00b7sell", ",", "ein", "fre\u00b7cher", "Sp\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "punc