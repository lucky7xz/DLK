, "und", "Blit\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Das Seegespenst im Tafelgolf:", "tokens": ["Das", "See\u00b7ge\u00b7spenst", "im", "Ta\u00b7fel\u00b7golf", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Je nun, sprach er, und strich den Magen,", "tokens": ["Je", "nun", ",", "sprach", "er", ",", "und", "strich", "den", "Ma\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "PPER", "$,", "KON", "ADJD", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.52": {"text": "Ich war ein Lamm in kranken Tagen;", "tokens": ["Ich", "war", "ein", "Lamm", "in", "kran\u00b7ken", "Ta\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Gesund bin ich nun wieder Wolf.", "tokens": ["Ge\u00b7sund", "bin", "ich", "nun", "wie\u00b7der", "Wolf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADV", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}