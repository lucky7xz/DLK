 "sch\u00fct\u00b7tel\u00b7te", "den", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Und sprach bey sich, der arme Tropf!", "tokens": ["Und", "sprach", "bey", "sich", ",", "der", "ar\u00b7me", "Tropf", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Das Fieber macht ihn phantasieren:", "tokens": ["Das", "Fie\u00b7ber", "macht", "ihn", "phan\u00b7ta\u00b7sie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Hier w\u00fcrden Luftklystier, Magnet,", "tokens": ["Hier", "w\u00fcr\u00b7den", "Luft\u00b7klys\u00b7tier", ",", "Mag\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.35": {"text": "Und selbst Apoll den Ruhm verlieren.", "tokens": ["Und", "selbst", "A\u00b7poll", "den", "Ruhm", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.36": {"text": "Er k\u00fc\u00dft den Freund und seufzt und geht.", "tokens": ["Er", "k\u00fc\u00dft", "den", "Freund", "und", "seufzt", "und", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Kaum bleicht der zackigte Planet", "tokens": ["Kaum", "bl