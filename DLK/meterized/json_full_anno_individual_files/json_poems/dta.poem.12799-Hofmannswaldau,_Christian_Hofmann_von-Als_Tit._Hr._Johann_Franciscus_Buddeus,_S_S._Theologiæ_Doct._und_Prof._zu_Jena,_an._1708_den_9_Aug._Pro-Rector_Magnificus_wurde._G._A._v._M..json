{"dta.poem.12799": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als  Tit.  \n Hr. Johann Franciscus Buddeus,  \n  S S.  Theologi\u00e6 Doct.  und  Prof.  zu Jena,  \n  an.  1708 den 9  Aug. Pro-Rector  \n Magnificus  wurde.  \n G. A. v. M.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es scheinet zwar sehr schwer, das lehrer-amt verwalten,", "tokens": ["Es", "schei\u00b7net", "zwar", "sehr", "schwer", ",", "das", "leh\u00b7rer\u00b7amt", "ver\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der regierungs-last zugleich gewachsen seyn.", "tokens": ["Und", "der", "re\u00b7gie\u00b7rungs\u00b7last", "zu\u00b7gleich", "ge\u00b7wach\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn mu\u00df der liebe glut nicht bey der furcht erkalten?", "tokens": ["Denn", "mu\u00df", "der", "lie\u00b7be", "glut", "nicht", "bey", "der", "furcht", "er\u00b7kal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die sanfftmuth stimmet schlecht mit zucht und strafen ein.", "tokens": ["Die", "sanfft\u00b7muth", "stim\u00b7met", "schlecht", "mit", "zucht", "und", "stra\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "NN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein lehrer herrscht ja nicht; er zeigt durch weise liebe", "tokens": ["Ein", "leh\u00b7rer", "herrscht", "ja", "nicht", ";", "er", "zeigt", "durch", "wei\u00b7se", "lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKNEG", "$.", "PPER", "VVFIN", "APPR", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sonder allen zwanck den lernenden die bahn;", "tokens": ["Und", "son\u00b7der", "al\u00b7len", "zwanck", "den", "ler\u00b7nen\u00b7den", "die", "bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "ART", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer aber herrscht, der zwingt durch des gesetzes triebe,", "tokens": ["Wer", "a\u00b7ber", "herrscht", ",", "der", "zwingt", "durch", "des", "ge\u00b7set\u00b7zes", "trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "PRELS", "VVFIN", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und strengt mit straf und zucht uns zum gehorsam an.", "tokens": ["Und", "strengt", "mit", "straf", "und", "zucht", "uns", "zum", "ge\u00b7hor\u00b7sam", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "VVFIN", "KON", "VVFIN", "PPER", "APPRART", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die strafe zeuget furcht; Mit kluger sanfftmuth lehren", "tokens": ["Die", "stra\u00b7fe", "zeu\u00b7get", "furcht", ";", "Mit", "klu\u00b7ger", "sanfft\u00b7muth", "leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "$.", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zieht lauter liebe nach, die keine furcht vertr\u00e4gt.", "tokens": ["Zieht", "lau\u00b7ter", "lie\u00b7be", "nach", ",", "die", "kei\u00b7ne", "furcht", "ver\u00b7tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVFIN", "PTKVZ", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer herrscht, der lehret nicht; man mu\u00df sein urtheil h\u00f6ren,", "tokens": ["Wer", "herrscht", ",", "der", "leh\u00b7ret", "nicht", ";", "man", "mu\u00df", "sein", "ur\u00b7theil", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "VVFIN", "PTKNEG", "$.", "PIS", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das allen widerspruch mit macht zu boden legt.", "tokens": ["Das", "al\u00b7len", "wi\u00b7der\u00b7spruch", "mit", "macht", "zu", "bo\u00b7den", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "VVFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So scheint es denn sehr schwer, das lehrer-amt verwalten,", "tokens": ["So", "scheint", "es", "denn", "sehr", "schwer", ",", "das", "leh\u00b7rer\u00b7amt", "ver\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und der regierungs-last zugleich gewachsen seyn.", "tokens": ["Und", "der", "re\u00b7gie\u00b7rungs\u00b7last", "zu\u00b7gleich", "ge\u00b7wach\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Solt\u2019 aber maucher es auch vor unm\u00f6glich halten,", "tokens": ["Solt'", "a\u00b7ber", "mau\u00b7cher", "es", "auch", "vor", "un\u00b7m\u00f6g\u00b7lich", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "PPER", "ADV", "APPR", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So trifft die m\u00f6gligkeit doch mit der wahrheit ein.", "tokens": ["So", "trifft", "die", "m\u00f6g\u00b7lig\u00b7keit", "doch", "mit", "der", "wahr\u00b7heit", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie viele V\u00e4ter sieht man lehren und regieren?", "tokens": ["Wie", "vie\u00b7le", "V\u00e4\u00b7ter", "sieht", "man", "leh\u00b7ren", "und", "re\u00b7gie\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "PIS", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Es k\u00f6nnen lieb\u2019 und ernst gar wol beysammen stehn.", "tokens": ["Es", "k\u00f6n\u00b7nen", "lieb'", "und", "ernst", "gar", "wol", "bey\u00b7sam\u00b7men", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVFIN", "KON", "ADJD", "ADV", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wir d\u00f6rffen nur den witz auf hohe schulen f\u00fchren,", "tokens": ["Wir", "d\u00f6rf\u00b7fen", "nur", "den", "witz", "auf", "ho\u00b7he", "schu\u00b7len", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So wird der zweiffel bald als wie ein dampff vergehn.", "tokens": ["So", "wird", "der", "zweif\u00b7fel", "bald", "als", "wie", "ein", "dampff", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "KOUS", "KOKOM", "ART", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hier ist der weisheit quell; hier ist es, wo wir lernen,", "tokens": ["Hier", "ist", "der", "weis\u00b7heit", "quell", ";", "hier", "ist", "es", ",", "wo", "wir", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$.", "ADV", "VAFIN", "PPER", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df, was unm\u00f6glich scheint, wol m\u00f6glich werden kan.", "tokens": ["Da\u00df", ",", "was", "un\u00b7m\u00f6g\u00b7lich", "scheint", ",", "wol", "m\u00f6g\u00b7lich", "wer\u00b7den", "kan", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "ADJD", "VVFIN", "$,", "ADV", "ADJD", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wir wollen uns nicht erst bis in die fremd\u2019 entfernen,", "tokens": ["Wir", "wol\u00b7len", "uns", "nicht", "erst", "bis", "in", "die", "fremd'", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Saline zeiget uns das sch\u00f6nste beyspiel an;", "tokens": ["Sa\u00b7li\u00b7ne", "zei\u00b7get", "uns", "das", "sch\u00f6ns\u00b7te", "bey\u00b7spiel", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.25": {"text": "Saline, wo so viel ber\u00fchmte M\u00e4nner lehren,", "tokens": ["Sa\u00b7li\u00b7ne", ",", "wo", "so", "viel", "be\u00b7r\u00fchm\u00b7te", "M\u00e4n\u00b7ner", "leh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADV", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.26": {"text": "Die dennoch den Parna\u00df mit gro\u00dfem ruhm regiert.", "tokens": ["Die", "den\u00b7noch", "den", "Par\u00b7na\u00df", "mit", "gro\u00b7\u00dfem", "ruhm", "re\u00b7giert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.27": {"text": "Saline, wo wir itzt Buddeens namen ehren,", "tokens": ["Sa\u00b7li\u00b7ne", ",", "wo", "wir", "itzt", "Bud\u00b7deens", "na\u00b7men", "eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "ADV", "NE", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.28": {"text": "Den sein verdienst zwar mehr, als alle titul ziert.", "tokens": ["Den", "sein", "ver\u00b7dienst", "zwar", "mehr", ",", "als", "al\u00b7le", "ti\u00b7tul", "ziert", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAINF", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Ehrw\u00fcrdigster Patron! du hast es schon gewiesen:", "tokens": ["Ehr\u00b7w\u00fcr\u00b7digs\u00b7ter", "Pat\u00b7ron", "!", "du", "hast", "es", "schon", "ge\u00b7wie\u00b7sen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.30": {"text": "Da\u00df kluge lehrer auch klug im regieren sind.", "tokens": ["Da\u00df", "klu\u00b7ge", "leh\u00b7rer", "auch", "klug", "im", "re\u00b7gie\u00b7ren", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "ADJD", "APPRART", "ADJA", "VAFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Fridricianens mund hat schon an dir gepriesen,", "tokens": ["Frid\u00b7ri\u00b7ci\u00b7a\u00b7nens", "mund", "hat", "schon", "an", "dir", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "---+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Was unser Saal-Athen in hoffnung itzt beginnt.", "tokens": ["Was", "un\u00b7ser", "Saa\u00b7lA\u00b7then", "in", "hoff\u00b7nung", "itzt", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "In hoffnung, da es dir den scepter anvertrauet,", "tokens": ["In", "hoff\u00b7nung", ",", "da", "es", "dir", "den", "scep\u00b7ter", "an\u00b7ver\u00b7trau\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PPER", "PPER", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und seinen purpur itzt um deine schultern legt.", "tokens": ["Und", "sei\u00b7nen", "pur\u00b7pur", "itzt", "um", "dei\u00b7ne", "schul\u00b7tern", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "ADV", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "In hoffnung, aber die auf feste gr\u00fcnde bauet,", "tokens": ["In", "hoff\u00b7nung", ",", "a\u00b7ber", "die", "auf", "fes\u00b7te", "gr\u00fcn\u00b7de", "bau\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "ART", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So keiner wellen macht, kein harter sturm bewegt.", "tokens": ["So", "kei\u00b7ner", "wel\u00b7len", "macht", ",", "kein", "har\u00b7ter", "sturm", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVINF", "VVFIN", "$,", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die b\u00fcrd\u2019 ist freylich schwehr; doch wie du stets mit freuden", "tokens": ["Die", "b\u00fcrd'", "ist", "frey\u00b7lich", "schwehr", ";", "doch", "wie", "du", "stets", "mit", "freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VAFIN", "ADV", "ADJD", "$.", "KON", "PWAV", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Dein lehr-amt pflegst zu thun, auch ehmals diese last,", "tokens": ["Dein", "lehr\u00b7amt", "pflegst", "zu", "thun", ",", "auch", "eh\u00b7mals", "die\u00b7se", "last", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKZU", "VVINF", "$,", "ADV", "ADV", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die manche lieber gar, als allzuwichtig, meiden,", "tokens": ["Die", "man\u00b7che", "lie\u00b7ber", "gar", ",", "als", "all\u00b7zu\u00b7wich\u00b7tig", ",", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ADV", "$,", "KOUS", "ADJD", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Mit unersch\u00f6pfftem muth zugleich getragen hast;", "tokens": ["Mit", "un\u00b7er\u00b7sch\u00f6pff\u00b7tem", "muth", "zu\u00b7gleich", "ge\u00b7tra\u00b7gen", "hast", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So wird sie dich itzund auch nicht zu boden dr\u00fccken.", "tokens": ["So", "wird", "sie", "dich", "it\u00b7zund", "auch", "nicht", "zu", "bo\u00b7den", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "ADV", "PTKNEG", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wen GOtt mit solcher krafft des Geistes ausger\u00fcst,", "tokens": ["Wen", "Gott", "mit", "sol\u00b7cher", "krafft", "des", "Geis\u00b7tes", "aus\u00b7ge\u00b7r\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Der wei\u00df durch m\u00fch und witz in alles sich zu schicken,", "tokens": ["Der", "wei\u00df", "durch", "m\u00fch", "und", "witz", "in", "al\u00b7les", "sich", "zu", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJD", "KON", "NN", "APPR", "PIS", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Dabey ein schwacher geist offt muth und rath vermist.", "tokens": ["Da\u00b7bey", "ein", "schwa\u00b7cher", "geist", "offt", "muth", "und", "rath", "ver\u00b7mist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "ADV", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Du wirst den unterscheid der \u00e4mter nicht vermengen;", "tokens": ["Du", "wirst", "den", "un\u00b7ter\u00b7scheid", "der", "\u00e4m\u00b7ter", "nicht", "ver\u00b7men\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Du wirst den lernenden mit lieb entgegen gehn,", "tokens": ["Du", "wirst", "den", "ler\u00b7nen\u00b7den", "mit", "lieb", "ent\u00b7ge\u00b7gen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADJD", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Und doch dem eigensinn der bosheit nichts verhengen,", "tokens": ["Und", "doch", "dem", "ei\u00b7gen\u00b7sinn", "der", "bos\u00b7heit", "nichts", "ver\u00b7hen\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Denn diesem wird dein ernst mit strafen widerstehn.", "tokens": ["Denn", "die\u00b7sem", "wird", "dein", "ernst", "mit", "stra\u00b7fen", "wi\u00b7der\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPOSAT", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Du wirst die irrenden durch GOttes wort bestreiten,", "tokens": ["Du", "wirst", "die", "ir\u00b7ren\u00b7den", "durch", "Got\u00b7tes", "wort", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Und durch der wahrheit licht den unverstand zerstreun:", "tokens": ["Und", "durch", "der", "wahr\u00b7heit", "licht", "den", "un\u00b7ver\u00b7stand", "zer\u00b7streun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Du wirst die unschuld stets durch treuen schutz begleiten,", "tokens": ["Du", "wirst", "die", "un\u00b7schuld", "stets", "durch", "treu\u00b7en", "schutz", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Und durch gerechtigkeit dein Saal-Athen erfreun.", "tokens": ["Und", "durch", "ge\u00b7rech\u00b7tig\u00b7keit", "dein", "Saa\u00b7lA\u00b7then", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Die V\u00e4ter, welche dir mit ruhm zur seite sitzen,", "tokens": ["Die", "V\u00e4\u00b7ter", ",", "wel\u00b7che", "dir", "mit", "ruhm", "zur", "sei\u00b7te", "sit\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Gehn dir mit gutem rath und beystand an die hand.", "tokens": ["Gehn", "dir", "mit", "gu\u00b7tem", "rath", "und", "beys\u00b7tand", "an", "die", "hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "KON", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Sie helffen dir das recht und die gesetze sch\u00fctzen;", "tokens": ["Sie", "helf\u00b7fen", "dir", "das", "recht", "und", "die", "ge\u00b7set\u00b7ze", "sch\u00fct\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJD", "KON", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "So bl\u00fcht durch dich und sie des Pindus ruhestand.", "tokens": ["So", "bl\u00fcht", "durch", "dich", "und", "sie", "des", "Pin\u00b7dus", "ru\u00b7he\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "KON", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Die jugend wird dich selbst als einen Vater ehren,", "tokens": ["Die", "ju\u00b7gend", "wird", "dich", "selbst", "als", "ei\u00b7nen", "Va\u00b7ter", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Sie wei\u00df, da\u00df du mit ernst vor ihre wohlfahrt wachst:", "tokens": ["Sie", "wei\u00df", ",", "da\u00df", "du", "mit", "ernst", "vor", "ih\u00b7re", "wohl\u00b7fahrt", "wachst", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "ADJD", "APPR", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Die jugend, welche du durch deine weise lehren", "tokens": ["Die", "ju\u00b7gend", ",", "wel\u00b7che", "du", "durch", "dei\u00b7ne", "wei\u00b7se", "leh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Und dein exempel klug und auch gehorsam machst.", "tokens": ["Und", "dein", "ex\u00b7em\u00b7pel", "klug", "und", "auch", "ge\u00b7hor\u00b7sam", "machst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "KON", "ADV", "ADJD", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.61": {"text": "Sie l\u00e4st sich nicht den wahn der falschen zungen blenden,", "tokens": ["Sie", "l\u00e4st", "sich", "nicht", "den", "wahn", "der", "fal\u00b7schen", "zun\u00b7gen", "blen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Sie bleibet, wie bisher, dem reinen Lehrer hold.", "tokens": ["Sie", "blei\u00b7bet", ",", "wie", "bis\u00b7her", ",", "dem", "rei\u00b7nen", "Leh\u00b7rer", "hold", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "$,", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Die wolcken k\u00f6nnen uns die sonne nicht entwenden,", "tokens": ["Die", "wol\u00b7cken", "k\u00f6n\u00b7nen", "uns", "die", "son\u00b7ne", "nicht", "ent\u00b7wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "So hefftig als sie ihr schon offtmals beygewolt.", "tokens": ["So", "heff\u00b7tig", "als", "sie", "ihr", "schon", "offt\u00b7mals", "bey\u00b7ge\u00b7wolt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Du lehrest treu und klug, du wirst auch klug regieren:", "tokens": ["Du", "leh\u00b7rest", "treu", "und", "klug", ",", "du", "wirst", "auch", "klug", "re\u00b7gie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Der H\u00f6chste, der dich liebt, steht dir mit segen bey.", "tokens": ["Der", "H\u00f6chs\u00b7te", ",", "der", "dich", "liebt", ",", "steht", "dir", "mit", "se\u00b7gen", "bey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.67": {"text": "Er wird mit ehr und ruhm den schwehren scepter zieren,", "tokens": ["Er", "wird", "mit", "ehr", "und", "ruhm", "den", "schweh\u00b7ren", "scep\u00b7ter", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Damit dein regiment nichts als vergn\u00fcgen sey.", "tokens": ["Da\u00b7mit", "dein", "re\u00b7gi\u00b7ment", "nichts", "als", "ver\u00b7gn\u00fc\u00b7gen", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "VVFIN", "PIS", "KOKOM", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Saline w\u00fcnschet es. GOtt lasse dich nur leben,", "tokens": ["Sa\u00b7li\u00b7ne", "w\u00fcn\u00b7schet", "es", ".", "Gott", "las\u00b7se", "dich", "nur", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "NN", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.70": {"text": "So steigt der w\u00fcnsche ziel wie cedern in die h\u00f6h:", "tokens": ["So", "steigt", "der", "w\u00fcn\u00b7sche", "ziel", "wie", "ce\u00b7dern", "in", "die", "h\u00f6h", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "KOKOM", "ADJD", "APPR", "ART", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "So wird Buddeus uns ein sch\u00f6n exempel geben:", "tokens": ["So", "wird", "Bud\u00b7deus", "uns", "ein", "sch\u00f6n", "ex\u00b7em\u00b7pel", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "PPER", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.72": {"text": "Da\u00df lehr und regiment wohl bey einander steh.", "tokens": ["Da\u00df", "lehr", "und", "re\u00b7gi\u00b7ment", "wohl", "bey", "ein\u00b7an\u00b7der", "steh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "VVFIN", "ADV", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}