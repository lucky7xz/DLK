{"dta.poem.2680": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gedancken au\u00df dem 126. Psalm/  \n  Bey Beerdigung Fr. J. B. g. L. den 19.  \n April 1676.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Sto\u00df/ der's Auge trifft/ bringt freylich nasse Z\u00e4hren/", "tokens": ["Ein", "Sto\u00df", "/", "der's", "Au\u00b7ge", "trifft", "/", "bringt", "frey\u00b7lich", "nas\u00b7se", "Z\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "VVFIN", "$(", "VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch der ins Hertze geht/ richt Seel und Leben hin.", "tokens": ["Doch", "der", "ins", "Hert\u00b7ze", "geht", "/", "richt", "Seel", "und", "Le\u00b7ben", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "VVFIN", "VVFIN", "$(", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was soll/ ", "tokens": ["Was", "soll", "/"], "token_info": ["word", "word", "punct"], "pos": ["PWS", "VMFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Sein Zufall kr\u00e4nckt mich selbst/ da\u00df ich voll Thr\u00e4nen bin.", "tokens": ["Sein", "Zu\u00b7fall", "kr\u00e4nckt", "mich", "selbst", "/", "da\u00df", "ich", "voll", "Thr\u00e4\u00b7nen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PPER", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es scheint zwar ungeschickt zu Thr\u00e4nen/ Thr\u00e4nen setzen", "tokens": ["Es", "scheint", "zwar", "un\u00b7ge\u00b7schickt", "zu", "Thr\u00e4\u00b7nen", "/", "Thr\u00e4\u00b7nen", "set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "NN", "$(", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und bey Bek\u00fcmmerten mehrt weinen nur das Leid:", "tokens": ["Und", "bey", "Be\u00b7k\u00fcm\u00b7mer\u00b7ten", "mehrt", "wei\u00b7nen", "nur", "das", "Leid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "VVINF", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch weil die Thr\u00e4nen sind der Engel ihr Ergetzen/", "tokens": ["Doch", "weil", "die", "Thr\u00e4\u00b7nen", "sind", "der", "En\u00b7gel", "ihr", "Er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein Balsam/ der mit Macht bek\u00e4mpfft die Traurigkeit;", "tokens": ["Ein", "Bal\u00b7sam", "/", "der", "mit", "Macht", "be\u00b7k\u00e4mpfft", "die", "Trau\u00b7rig\u00b7keit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und was man satt beklagt/ doch niemals satt beweinet/", "tokens": ["Und", "was", "man", "satt", "be\u00b7klagt", "/", "doch", "nie\u00b7mals", "satt", "be\u00b7wei\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "ADJD", "VVPP", "$(", "ADV", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So la\u00df er Thr\u00e4nen jetzt der Liebsten Opffer seyn.", "tokens": ["So", "la\u00df", "er", "Thr\u00e4\u00b7nen", "jetzt", "der", "Liebs\u00b7ten", "Opf\u00b7fer", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "NN", "ADV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich wei\u00df da\u00df auch sein Geist gantz willig hier erscheinet/", "tokens": ["Ich", "wei\u00df", "da\u00df", "auch", "sein", "Geist", "gantz", "wil\u00b7lig", "hier", "er\u00b7schei\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil selbst der Himmel ihm fl\u00f6st diese Tropffen ein.", "tokens": ["Weil", "selbst", "der", "Him\u00b7mel", "ihm", "fl\u00f6st", "die\u00b7se", "Tropf\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Di\u00df was sein Mund gelehrt in feurigen ", "tokens": ["Di\u00df", "was", "sein", "Mund", "ge\u00b7lehrt", "in", "feu\u00b7ri\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRELS", "PPOSAT", "NN", "VVPP", "APPR", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wenn wahrer Andacht Blitz die Zunge hat entflammt;", "tokens": ["Wenn", "wah\u00b7rer", "An\u00b7dacht", "Blitz", "die", "Zun\u00b7ge", "hat", "ent\u00b7flammt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie unser Sterbligkeit besteh in engen Schrancken/", "tokens": ["Wie", "un\u00b7ser", "Ster\u00b7blig\u00b7keit", "be\u00b7steh", "in", "en\u00b7gen", "Schran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und von der ersten Schuld der Tod noch auf uns stammt/", "tokens": ["Und", "von", "der", "ers\u00b7ten", "Schuld", "der", "Tod", "noch", "auf", "uns", "stammt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ja ein verwe\u00dflich Korn wir s\u00e4en in die Erden:", "tokens": ["Ja", "ein", "ver\u00b7we\u00df\u00b7lich", "Korn", "wir", "s\u00e4\u00b7en", "in", "die", "Er\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "ADJD", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Macht jetzt die Folgerung an seiner Liebsten klar.", "tokens": ["Macht", "jetzt", "die", "Fol\u00b7ge\u00b7rung", "an", "sei\u00b7ner", "Liebs\u00b7ten", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.19": {"text": "Er sieht sein halbes Hertz zu Stanb und Asche werden/", "tokens": ["Er", "sieht", "sein", "hal\u00b7bes", "Hertz", "zu", "Stanb", "und", "A\u00b7sche", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und seiner Jahre Trost liegt auf der schwartzen Baar.", "tokens": ["Und", "sei\u00b7ner", "Jah\u00b7re", "Trost", "liegt", "auf", "der", "schwart\u00b7zen", "Baar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Heist dieses nicht in Angst und bittren Thr\u00e4nen s\u00e4en?", "tokens": ["Heist", "die\u00b7ses", "nicht", "in", "Angst", "und", "bit\u00b7tren", "Thr\u00e4\u00b7nen", "s\u00e4\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "APPR", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Gleich wie der Ackersmann mit seufftzen seine Frucht/", "tokens": ["Gleich", "wie", "der", "A\u00b7ckers\u00b7mann", "mit", "seufft\u00b7zen", "sei\u00b7ne", "Frucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wenn starcker Hagel f\u00e4llt/ wenn rauhe Winde wehen/", "tokens": ["Wenn", "star\u00b7cker", "Ha\u00b7gel", "f\u00e4llt", "/", "wenn", "rau\u00b7he", "Win\u00b7de", "we\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "$(", "KOUS", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der Erden zuvertraun auf k\u00fcnfftig Wachsthum sucht:", "tokens": ["Der", "Er\u00b7den", "zu\u00b7ver\u00b7traun", "auf", "k\u00fcnff\u00b7tig", "Wach\u00b7sthum", "sucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Es schleust der B\u00e4the Scho\u00df die Hoffnung von dem Jahre", "tokens": ["Es", "schleust", "der", "B\u00e4\u00b7the", "Scho\u00df", "die", "Hoff\u00b7nung", "von", "dem", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die er nur mit Gebet und Wunsch bef\u00f6dern kan:", "tokens": ["Die", "er", "nur", "mit", "Ge\u00b7bet", "und", "Wunsch", "be\u00b7f\u00f6\u00b7dern", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Hier liegt ", "tokens": ["Hier", "liegt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.28": {"text": "Und nimmt die Thr\u00e4nen noch/ die er jetzt liefert/ an.", "tokens": ["Und", "nimmt", "die", "Thr\u00e4\u00b7nen", "noch", "/", "die", "er", "jetzt", "lie\u00b7fert", "/", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$(", "PRELS", "PPER", "ADV", "VVFIN", "$(", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Vors erste nenn ich es erhitzte Liebes-Thr\u00e4nen", "tokens": ["Vors", "ers\u00b7te", "nenn", "ich", "es", "er\u00b7hitz\u00b7te", "Lie\u00b7bes\u00b7Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VVFIN", "PPER", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die von den Tugenden der Liebsten au\u00dfgeprest.", "tokens": ["Die", "von", "den", "Tu\u00b7gen\u00b7den", "der", "Liebs\u00b7ten", "au\u00df\u00b7ge\u00b7prest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Gewi\u00df da\u00df sie nach sich ein unaufh\u00f6rlich sehnen", "tokens": ["Ge\u00b7wi\u00df", "da\u00df", "sie", "nach", "sich", "ein", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "APPR", "PRF", "ART", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Zum Denckmal steter Treu in seinem Hertzen l\u00e4st.", "tokens": ["Zum", "Denck\u00b7mal", "ste\u00b7ter", "Treu", "in", "sei\u00b7nem", "Hert\u00b7zen", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Es mag das Morgenland des Balsams theure Gaben/", "tokens": ["Es", "mag", "das", "Mor\u00b7gen\u00b7land", "des", "Bal\u00b7sams", "theu\u00b7re", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die Myrrh\u2019 und Aloe ber\u00fchmten Gr\u00e4bern weyhn;", "tokens": ["Die", "Myr\u00b7rh'", "und", "A\u00b7loe", "be\u00b7r\u00fchm\u00b7ten", "Gr\u00e4\u00b7bern", "weyhn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Hier kan die ", "tokens": ["Hier", "kan", "die"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.36": {"text": "Wenn es ihr Ehschatz wird mit Thr\u00e4nen \u00fcberstreun.", "tokens": ["Wenn", "es", "ihr", "Eh\u00b7schatz", "wird", "mit", "Thr\u00e4\u00b7nen", "\u00fc\u00b7bers\u00b7treun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Denn diese rinnen auch au\u00df recht getreuem Hertzen/", "tokens": ["Denn", "die\u00b7se", "rin\u00b7nen", "auch", "au\u00df", "recht", "ge\u00b7treu\u00b7em", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "APPR", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Bevor wenn er erwegt den klaglichen Verlust/", "tokens": ["Be\u00b7vor", "wenn", "er", "er\u00b7wegt", "den", "klag\u00b7li\u00b7chen", "Ver\u00b7lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und wie sie jederzeit als Aertztin seiner Schmertzen", "tokens": ["Und", "wie", "sie", "je\u00b7der\u00b7zeit", "als", "A\u00b7ertz\u00b7tin", "sei\u00b7ner", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "KOUS", "NN", "PPOSAT", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Und Kummerwenderin zu rathen hat gewust.", "tokens": ["Und", "Kum\u00b7mer\u00b7wen\u00b7de\u00b7rin", "zu", "ra\u00b7then", "hat", "ge\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKZU", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Der Kern der ", "tokens": ["Der", "Kern", "der"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.42": {"text": "De\u00df Vaters Priesterthum hie\u00df sie nicht weltlich seyn/", "tokens": ["De\u00df", "Va\u00b7ters", "Pries\u00b7ter\u00b7thum", "hie\u00df", "sie", "nicht", "welt\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Nach dem sie nun/ mein Herr/ als Priester ihm geschworen/", "tokens": ["Nach", "dem", "sie", "nun", "/", "mein", "Herr", "/", "als", "Pries\u00b7ter", "ihm", "ge\u00b7schwo\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word