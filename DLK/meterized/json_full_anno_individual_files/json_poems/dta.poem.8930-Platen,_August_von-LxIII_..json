{"dta.poem.8930": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "LxIII .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Entsagens werd' ich ohne Seufzer missen,", "tokens": ["Ent\u00b7sa\u00b7gens", "werd'", "ich", "oh\u00b7ne", "Seuf\u00b7zer", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo man bedr\u00e4ngt von tausend Hindernissen", "tokens": ["Wo", "man", "be\u00b7dr\u00e4ngt", "von", "tau\u00b7send", "Hin\u00b7der\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich m\u00fcde qu\u00e4lt und dennoch mu\u00df verderben.", "tokens": ["Sich", "m\u00fc\u00b7de", "qu\u00e4lt", "und", "den\u00b7noch", "mu\u00df", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "KON", "ADV", "VMFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zwar mancher Vortheil l\u00e4\u00dft sich hier erwerben,", "tokens": ["Zwar", "man\u00b7cher", "Vor\u00b7theil", "l\u00e4\u00dft", "sich", "hier", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Staatsw\u00fcrden, Wohlstand, eine Last von Wissen,", "tokens": ["Staats\u00b7w\u00fcr\u00b7den", ",", "Wohl\u00b7stand", ",", "ei\u00b7ne", "Last", "von", "Wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und unsre Deutschen waren stets beflissen,", "tokens": ["Und", "uns\u00b7re", "Deut\u00b7schen", "wa\u00b7ren", "stets", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich abzuplagen und geplagt zu sterben.", "tokens": ["Sich", "ab\u00b7zu\u00b7pla\u00b7gen", "und", "ge\u00b7plagt", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVIZU", "KON", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Solcher darf zu keiner Zeit ermatten,", "tokens": ["Ein", "Sol\u00b7cher", "darf", "zu", "kei\u00b7ner", "Zeit", "er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er f\u00f6rdre sich, er schmeichle jeder Mode,", "tokens": ["Er", "f\u00f6rd\u00b7re", "sich", ",", "er", "schmeich\u00b7le", "je\u00b7der", "Mo\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sey dabey, wo Gl\u00fcck und Macht sich gatten.", "tokens": ["Und", "sey", "da\u00b7bey", ",", "wo", "Gl\u00fcck", "und", "Macht", "sich", "gat\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "$,", "PWAV", "NN", "KON", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mir, der ich blos ein wandernder Rhapsode,", "tokens": ["Mir", ",", "der", "ich", "blos", "ein", "wan\u00b7dern\u00b7der", "Rhap\u00b7so\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+++-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Gen\u00fcgt ein Freund, ein Becher Wein im Schatten,", "tokens": ["Ge\u00b7n\u00fcgt", "ein", "Freund", ",", "ein", "Be\u00b7cher", "Wein", "im", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ein ber\u00fchmter Name nach dem Tode.", "tokens": ["Und", "ein", "be\u00b7r\u00fchm\u00b7ter", "Na\u00b7me", "nach", "dem", "To\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}