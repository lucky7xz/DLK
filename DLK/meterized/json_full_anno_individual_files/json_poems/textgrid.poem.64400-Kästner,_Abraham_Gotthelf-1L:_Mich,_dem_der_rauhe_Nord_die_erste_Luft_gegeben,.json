{"textgrid.poem.64400": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mich, dem der rauhe Nord die erste Luft gegeben,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich, dem der rauhe Nord die erste Luft gegeben,", "tokens": ["Mich", ",", "dem", "der", "rau\u00b7he", "Nord", "die", "ers\u00b7te", "Luft", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hie\u00df meiner F\u00fcrstinn Wink in Leipzigs G\u00e4rten leben.", "tokens": ["Hie\u00df", "mei\u00b7ner", "F\u00fcrs\u00b7tinn", "Wink", "in", "Leip\u00b7zigs", "G\u00e4r\u00b7ten", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Kette ward mir schwer, ich brach sie voller Wuth,", "tokens": ["Die", "Ket\u00b7te", "ward", "mir", "schwer", ",", "ich", "brach", "sie", "vol\u00b7ler", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch ich erkaufte mir die Freyheit durch mein Blut.", "tokens": ["Doch", "ich", "er\u00b7kauf\u00b7te", "mir", "die", "Frey\u00b7heit", "durch", "mein", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Noch klaget Doris mich; die fast zerst\u00e4ubten Knochen", "tokens": ["Noch", "kla\u00b7get", "Do\u00b7ris", "mich", ";", "die", "fast", "zer\u00b7st\u00e4ub\u00b7ten", "Kno\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "PPER", "$.", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bewegten sich, als sie das holde Wort gesprochen:", "tokens": ["Be\u00b7weg\u00b7ten", "sich", ",", "als", "sie", "das", "hol\u00b7de", "Wort", "ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u00bbmich dau'rt das arme Thier! O sollt er lebend seyn,", "tokens": ["\u00bb", "mich", "dau'rt", "das", "ar\u00b7me", "Thier", "!", "O", "sollt", "er", "le\u00b7bend", "seyn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "NE", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie w\u00fcrd' er uns nicht jetzt voll Artigkeit erfreun!\u00ab", "tokens": ["Wie", "w\u00fcrd'", "er", "uns", "nicht", "jetzt", "voll", "Ar\u00b7tig\u00b7keit", "er\u00b7freun", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "PTKNEG", "ADV", "ADJD", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "lin