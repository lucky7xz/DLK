{"textgrid.poem.64894": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieh dort durch des Tempelbaues Hallen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh dort durch des Tempelbaues Hallen", "tokens": ["Sieh", "dort", "durch", "des", "Tem\u00b7pel\u00b7bau\u00b7es", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ernst die gottgeweihte Jungfrau wallen!", "tokens": ["Ernst", "die", "gott\u00b7ge\u00b7weih\u00b7te", "Jung\u00b7frau", "wal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eine Lampe, deren Flamme zittert,", "tokens": ["Ei\u00b7ne", "Lam\u00b7pe", ",", "de\u00b7ren", "Flam\u00b7me", "zit\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Tr\u00e4gt sie, und mit reiner Hand umgittert", "tokens": ["Tr\u00e4gt", "sie", ",", "und", "mit", "rei\u00b7ner", "Hand", "um\u00b7git\u00b7tert"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "APPR", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sorglich sie die\u00df Licht, da\u00df es im Grimme", "tokens": ["Sorg\u00b7lich", "sie", "die\u00df", "Licht", ",", "da\u00df", "es", "im", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PPER", "PDS", "NN", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Rauher St\u00fcrme sterbend nicht verglimme.", "tokens": ["Rau\u00b7her", "St\u00fcr\u00b7me", "ster\u00b7bend", "nicht", "ver\u00b7glim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Nicht vermag es alle dunklen Stellen", "tokens": ["Nicht", "ver\u00b7mag", "es", "al\u00b7le", "dunk\u00b7len", "Stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Dieses weiten Domes zu erhellen;", "tokens": ["Die\u00b7ses", "wei\u00b7ten", "Do\u00b7mes", "zu", "er\u00b7hel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Doch gen\u00fcgt's, da\u00df Helle es verbreitet", "tokens": ["Doch", "ge\u00b7n\u00fcgt's", ",", "da\u00df", "Hel\u00b7le", "es", "ver\u00b7brei\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NE", "$,", "KOUS", "NE", "PPER", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Auf dem Pfad, auf dem die Jungfrau schreitet,", "tokens": ["Auf", "dem", "Pfad", ",", "auf", "dem", "die", "Jung\u00b7frau", "schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Da\u00df ihr Schein, der milde, ernste, klare,", "tokens": ["Da\u00df", "ihr", "Schein", ",", "der", "mil\u00b7de", ",", "erns\u00b7te", ",", "kla\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "$,", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Ihr die Richtung zeige zum Altare,", "tokens": ["Ihr", "die", "Rich\u00b7tung", "zei\u00b7ge", "zum", "Al\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Wo die Perlen ihres sel'gen Sehnens", "tokens": ["Wo", "die", "Per\u00b7len", "ih\u00b7res", "sel'\u00b7gen", "Seh\u00b7nens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Thauen, wie der Balsam Magdalenens.", "tokens": ["Thau\u00b7en", ",", "wie", "der", "Bal\u00b7sam", "Mag\u00b7da\u00b7le\u00b7nens", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Jener Jungfrau gleichet meine Seele,", "tokens": ["Je\u00b7ner", "Jung\u00b7frau", "glei\u00b7chet", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Wie sie durch des Lebens Dunkel schreitet,", "tokens": ["Wie", "sie", "durch", "des", "Le\u00b7bens", "Dun\u00b7kel", "schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Nur vom ew'gen Liebesstrahl geleitet,", "tokens": ["Nur", "vom", "ew'\u00b7gen", "Lie\u00b7bes\u00b7strahl", "ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Da\u00df sie nimmer ihres Weges fehle.", "tokens": ["Da\u00df", "sie", "nim\u00b7mer", "ih\u00b7res", "We\u00b7ges", "feh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "All ihr Denken, F\u00fchlen, Thun und Treiben", "tokens": ["All", "ihr", "Den\u00b7ken", ",", "F\u00fch\u00b7len", ",", "Thun", "und", "Trei\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Dienet nur die Flamme zu beschirmen,", "tokens": ["Die\u00b7net", "nur", "die", "Flam\u00b7me", "zu", "be\u00b7schir\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Da\u00df sie von des Lebens rauhen St\u00fcrmen", "tokens": ["Da\u00df", "sie", "von", "des", "Le\u00b7bens", "rau\u00b7hen", "St\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Unerreicht und unber\u00fchrt mag bleiben.", "tokens": ["Un\u00b7er\u00b7reicht", "und", "un\u00b7be\u00b7r\u00fchrt", "mag", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Meines Daseins dunkle Schattenmassen", "tokens": ["Mei\u00b7nes", "Da\u00b7seins", "dunk\u00b7le", "Schat\u00b7ten\u00b7mas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Kann sie nicht zerstreu'n mit m\u00e4cht'ger Lichtung,", "tokens": ["Kann", "sie", "nicht", "zer\u00b7streu'n", "mit", "m\u00e4cht'\u00b7ger", "Lich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Aber leuchtend weis't sie mir die Richtung", "tokens": ["A\u00b7ber", "leuch\u00b7tend", "weis't", "sie", "mir", "die", "Rich\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Zu dem Trost nur mehr allein zu fassen;", "tokens": ["Zu", "dem", "Trost", "nur", "mehr", "al\u00b7lein", "zu", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Sichert mir den Weg, den ich betreten,", "tokens": ["Si\u00b7chert", "mir", "den", "Weg", ",", "den", "ich", "be\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Leitet mich mit ihrer frommen Helle", "tokens": ["Lei\u00b7tet", "mich", "mit", "ih\u00b7rer", "from\u00b7men", "Hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "Treulich hin zur wundervollen Stelle,", "tokens": ["Treu\u00b7lich", "hin", "zur", "wun\u00b7der\u00b7vol\u00b7len", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Wo sich Klagen wandeln zu Gebeten. \u2013", "tokens": ["Wo", "sich", "Kla\u00b7gen", "wan\u00b7deln", "zu", "Ge\u00b7be\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PRF", "NN", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.31": {"text": "O es gleicht mein unverl\u00f6schlich Lieben", "tokens": ["O", "es", "gleicht", "mein", "un\u00b7ver\u00b7l\u00f6\u00b7schlich", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VVFIN", "PPOSAT", "ADJD", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "Jenem Stern, der einstmals fortgetrieben", "tokens": ["Je\u00b7nem", "Stern", ",", "der", "einst\u00b7mals", "fort\u00b7ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.33": {"text": "Die drei K\u00f6nige aus ihren Landen,", "tokens": ["Die", "drei", "K\u00f6\u00b7ni\u00b7ge", "aus", "ih\u00b7ren", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "Der sie \u00fcber Berg und Meer gef\u00fchret,", "tokens": ["Der", "sie", "\u00fc\u00b7ber", "Berg", "und", "Meer", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.35": {"text": "Bis sie endlich, wonnevoll ger\u00fchret,", "tokens": ["Bis", "sie", "end\u00b7lich", ",", "won\u00b7ne\u00b7voll", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.36": {"text": "Betend vor dem Gotteskinde standen.", "tokens": ["Be\u00b7tend", "vor", "dem", "Got\u00b7tes\u00b7kin\u00b7de", "stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sieh dort durch des Tempelbaues Hallen", "tokens": ["Sieh", "dort", "durch", "des", "Tem\u00b7pel\u00b7bau\u00b7es", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ernst die gottgeweihte Jungfrau wallen!", "tokens": ["Ernst", "die", "gott\u00b7ge\u00b7weih\u00b7te", "Jung\u00b7frau", "wal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eine Lampe, deren Flamme zittert,", "tokens": ["Ei\u00b7ne", "Lam\u00b7pe", ",", "de\u00b7ren", "Flam\u00b7me", "zit\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"tex