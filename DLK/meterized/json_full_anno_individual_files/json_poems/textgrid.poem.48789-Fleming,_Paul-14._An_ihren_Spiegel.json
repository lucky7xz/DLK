{"textgrid.poem.48789": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "14. An ihren Spiegel", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du drei viermal mehr Gl\u00fcckseliger als ich,", "tokens": ["O", "du", "drei", "vier\u00b7mal", "mehr", "Gl\u00fcck\u00b7se\u00b7li\u00b7ger", "als", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "CARD", "ADV", "PIAT", "NN", "KOUS", "PPER", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "der du der Liebsten Glanz in deinem Auge tr\u00e4gest", "tokens": ["der", "du", "der", "Liebs\u00b7ten", "Glanz", "in", "dei\u00b7nem", "Au\u00b7ge", "tr\u00e4\u00b7gest"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und selbst zu lieben sich das sch\u00f6ne Kind bewegest,", "tokens": ["und", "selbst", "zu", "lie\u00b7ben", "sich", "das", "sch\u00f6\u00b7ne", "Kind", "be\u00b7we\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKZU", "VVFIN", "PRF", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "daher sie nur wird stolz, sieht weit hin \u00fcber mich,", "tokens": ["da\u00b7her", "sie", "nur", "wird", "stolz", ",", "sieht", "weit", "hin", "\u00fc\u00b7ber", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "VAFIN", "ADJD", "$,", "VVFIN", "ADJD", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "giebt ihre Gunst ihr selbst und achtet mehr auf dich,", "tokens": ["giebt", "ih\u00b7re", "Gunst", "ihr", "selbst", "und", "ach\u00b7tet", "mehr", "auf", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "ADV", "KON", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "in dem du bist bem\u00fcht und h\u00f6chsten Flei\u00df anlegest,", "tokens": ["in", "dem", "du", "bist", "be\u00b7m\u00fcht", "und", "h\u00f6chs\u00b7ten", "Flei\u00df", "an\u00b7le\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "VVPP", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df du dich, wie sie sich, an allen Gliedern regest,", "tokens": ["da\u00df", "du", "dich", ",", "wie", "sie", "sich", ",", "an", "al\u00b7len", "Glie\u00b7dern", "re\u00b7gest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "$,", "PWAV", "PPER", "PRF", "$,", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "durch dich schaut sie sich an und redet selbst mit sich.", "tokens": ["durch", "dich", "schaut", "sie", "sich", "an", "und", "re\u00b7det", "selbst", "mit", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PRF", "PTKVZ", "KON", "VVFIN", "ADV", "APPR", "PRF", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Du rechtes Freudenwerk von fr\u00fch an bis zu Nachte,", "tokens": ["Du", "rech\u00b7tes", "Freu\u00b7den\u00b7werk", "von", "fr\u00fch", "an", "bis", "zu", "Nach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "ADJD", "APPR", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "wie mach' ichs, da\u00df ich sie doch einmal so betrachte,", "tokens": ["wie", "mach'", "ichs", ",", "da\u00df", "ich", "sie", "doch", "ein\u00b7mal", "so", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "$,", "KOUS", "PPER", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "als wie du allzeit tust? So mein' ich, kan es gehn,", "tokens": ["als", "wie", "du", "all\u00b7zeit", "tust", "?", "So", "mein'", "ich", ",", "kan", "es", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPER", "ADV", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "versuch es einen Tag und g\u00f6nne mir dein Gl\u00fccke,", "tokens": ["ver\u00b7such", "es", "ei\u00b7nen", "Tag", "und", "g\u00f6n\u00b7ne", "mir", "dein", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und da\u00df ich wieder gleich in ihre Blicke blicke,", "tokens": ["und", "da\u00df", "ich", "wie\u00b7der", "gleich", "in", "ih\u00b7re", "Bli\u00b7cke", "bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so la\u00df di\u00df Auge hier an deine Stelle stehn,", "tokens": ["so", "la\u00df", "di\u00df", "Au\u00b7ge", "hier", "an", "dei\u00b7ne", "Stel\u00b7le", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PDS", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "O du drei viermal mehr Gl\u00fcckseliger als ich,", "tokens": ["O", "du", "drei", "vier\u00b7mal", "mehr", "Gl\u00fcck\u00b7se\u00b7li\u00b7ger", "als", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "CARD", "ADV", "PIAT", "NN", "KOUS", "PPER", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "der du der Liebsten Glanz in deinem Auge tr\u00e4gest", "tokens": ["der", "du", "der", "Liebs\u00b7ten", "Glanz", "in", "dei\u00b7nem", "Au\u00b7ge", "tr\u00e4\u00b7gest"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und selbst zu lieben sich das sch\u00f6ne Kind bewegest,", "tokens": ["und", "selbst", "zu", "lie\u00b7ben", "sich", "das", "sch\u00f6\u00b7ne", "Kind", "be\u00b7we\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKZU", "VVFIN", "PRF", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "m