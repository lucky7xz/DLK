{"textgrid.poem.33726": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wind, Wolke, Lichtstrahl ziehn die alte Reise", "tokens": ["Wind", ",", "Wol\u00b7ke", ",", "Licht\u00b7strahl", "ziehn", "die", "al\u00b7te", "Rei\u00b7se"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um unsern Erdball, t\u00fcrmen Nacht und Wogen,", "tokens": ["Um", "un\u00b7sern", "Erd\u00b7ball", ",", "t\u00fcr\u00b7men", "Nacht", "und", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Versenken Schiffe, w\u00f6lben Regenbogen \u2013", "tokens": ["Ver\u00b7sen\u00b7ken", "Schif\u00b7fe", ",", "w\u00f6l\u00b7ben", "Re\u00b7gen\u00b7bo\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das alte Schauspiel, stets in neuer Weise.", "tokens": ["Das", "al\u00b7te", "Schau\u00b7spiel", ",", "stets", "in", "neu\u00b7er", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Monde wiederholen ihre Kreise,", "tokens": ["Die", "Mon\u00b7de", "wie\u00b7der\u00b7ho\u00b7len", "ih\u00b7re", "Krei\u00b7se", ","], "token_info": [