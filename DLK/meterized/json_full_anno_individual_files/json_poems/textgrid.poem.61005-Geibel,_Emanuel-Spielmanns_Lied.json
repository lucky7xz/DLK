{"textgrid.poem.61005": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Spielmanns Lied", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und legt ihr zwischen mich und sie", "tokens": ["Und", "legt", "ihr", "zwi\u00b7schen", "mich", "und", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch Strom und Tal und H\u00fcgel,", "tokens": ["Auch", "Strom", "und", "Tal", "und", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gestrenge Herrn, ihr trennt uns nie,", "tokens": ["Ge\u00b7stren\u00b7ge", "Herrn", ",", "ihr", "trennt", "uns", "nie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Lied, das Lied hat Fl\u00fcgel.", "tokens": ["Das", "Lied", ",", "das", "Lied", "hat", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich bin ein Spielmann wohlbekannt,", "tokens": ["Ich", "bin", "ein", "Spiel\u00b7mann", "wohl\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich mache mich auf die Reise,", "tokens": ["Ich", "ma\u00b7che", "mich", "auf", "die", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und sing' hinfort durchs ganze Land", "tokens": ["Und", "sing'", "hin\u00b7fort", "durchs", "gan\u00b7ze", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nur noch die eine Weise:", "tokens": ["Nur", "noch", "die", "ei\u00b7ne", "Wei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "\u00bbich habe dich lieb, du S\u00fc\u00dfe,", "tokens": ["\u00bb", "ich", "ha\u00b7be", "dich", "lieb", ",", "du", "S\u00fc\u00b7\u00dfe", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADJD", "$,", "PPER", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Du meine Lust und Qual,", "tokens": ["Du", "mei\u00b7ne", "Lust", "und", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ich habe dich lieb und gr\u00fc\u00dfe", "tokens": ["Ich", "ha\u00b7be", "dich", "lieb", "und", "gr\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "KON", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Dich tausend, tausendmal!\u00ab", "tokens": ["Dich", "tau\u00b7send", ",", "tau\u00b7send\u00b7mal", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVPP", "$,", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wandr' ich durch den laub'gen Wald,", "tokens": ["Und", "wandr'", "ich", "durch", "den", "laub'\u00b7gen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Fink und Amsel schweifen:", "tokens": ["Wo", "Fink", "und", "Am\u00b7sel", "schwei\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Lied erlauscht das V\u00f6lkchen bald", "tokens": ["Mein", "Lied", "er\u00b7lauscht", "das", "V\u00f6lk\u00b7chen", "bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hebt es an zu pfeifen.", "tokens": ["Und", "hebt", "es", "an", "zu", "pfei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und auf der Heide h\u00f6rt's der Wind,", "tokens": ["Und", "auf", "der", "Hei\u00b7de", "h\u00f6rt's", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der spannt die Fl\u00fcgel heiter", "tokens": ["Der", "spannt", "die", "Fl\u00fc\u00b7gel", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und tr\u00e4gt es \u00fcber den Strom geschwind", "tokens": ["Und", "tr\u00e4gt", "es", "\u00fc\u00b7ber", "den", "Strom", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und \u00fcber den Berg und weiter:", "tokens": ["Und", "\u00fc\u00b7ber", "den", "Berg", "und", "wei\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "KON", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "\u00bbich habe dich lieb, du S\u00fc\u00dfe,", "tokens": ["\u00bb", "ich", "ha\u00b7be", "dich", "lieb", ",", "du", "S\u00fc\u00b7\u00dfe", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADJD", "$,", "PPER", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Du meine Lust und Qual,", "tokens": ["Du", "mei\u00b7ne", "Lust", "und", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ich habe dich lieb und gr\u00fc\u00dfe", "tokens": ["Ich", "ha\u00b7be", "dich", "lieb", "und", "gr\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "KON", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Dich tausend, tausendmal!\u00ab", "tokens": ["Dich", "tau\u00b7send", ",", "tau\u00b7send\u00b7mal", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVPP", "$,", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Durch Stadt und Dorf, durch Wies' und Korn", "tokens": ["Durch", "Stadt", "und", "Dorf", ",", "durch", "Wies'", "und", "Korn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Spiel' ich's auf meinen Z\u00fcgen,", "tokens": ["Spiel'", "ich's", "auf", "mei\u00b7nen", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da singen's bald zu Nacht am Born", "tokens": ["Da", "sin\u00b7gen's", "bald", "zu", "Nacht", "am", "Born"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die M\u00e4gde mit den Kr\u00fcgen,", "tokens": ["Die", "M\u00e4g\u00b7de", "mit", "den", "Kr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der J\u00e4ger summt es vor sich her,", "tokens": ["Der", "J\u00e4\u00b7ger", "summt", "es", "vor", "sich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sp\u00fcrt er im Buchenhage,", "tokens": ["Sp\u00fcrt", "er", "im", "Bu\u00b7chen\u00b7ha\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Fischer wirft sein Netz ins Meer", "tokens": ["Der", "Fi\u00b7scher", "wirft", "sein", "Netz", "ins", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und singt's zum Ruderschlage:", "tokens": ["Und", "singt's", "zum", "Ru\u00b7der\u00b7schla\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "\u00bbich habe dich lieb, du S\u00fc\u00dfe,", "tokens": ["\u00bb", "ich", "ha\u00b7be", "dich", "lieb", ",", "du", "S\u00fc\u00b7\u00dfe", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADJD", "$,", "PPER", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Du meine Lust und Qual,", "tokens": ["Du", "mei\u00b7ne", "Lust", "und", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ich habe dich lieb und gr\u00fc\u00dfe", "tokens": ["Ich", "ha\u00b7be", "dich", "lieb", "und", "gr\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "KON", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Dich tausend, tausendmal!\u00ab", "tokens": ["Dich", "tau\u00b7send", ",", "tau\u00b7send\u00b7mal", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVPP", "$,", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und frischer Wind und Waldv\u00f6glein", "tokens": ["Und", "fri\u00b7scher", "Wind", "und", "Wald\u00b7v\u00f6\u00b7glein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Fischer, M\u00e4gd' und J\u00e4ger,", "tokens": ["Und", "Fi\u00b7scher", ",", "M\u00e4gd'", "und", "J\u00e4\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die m\u00fcssen alle Boten sein", "tokens": ["Die", "m\u00fcs\u00b7sen", "al\u00b7le", "Bo\u00b7ten", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meiner Liebe Tr\u00e4ger.", "tokens": ["Und", "mei\u00b7ner", "Lie\u00b7be", "Tr\u00e4\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So kommt's im Ernst, so kommt's im Scherz", "tokens": ["So", "kommt's", "im", "Ernst", ",", "so", "kommt's", "im", "Scherz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu deinem Ohr am Ende;", "tokens": ["Zu", "dei\u00b7nem", "Ohr", "am", "En\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wenn du's h\u00f6rst, da pocht dein Herz,", "tokens": ["Und", "wenn", "du's", "h\u00f6rst", ",", "da", "pocht", "dein", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVFIN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du sp\u00fcrst es, wer es sende:", "tokens": ["Du", "sp\u00fcrst", "es", ",", "wer", "es", "