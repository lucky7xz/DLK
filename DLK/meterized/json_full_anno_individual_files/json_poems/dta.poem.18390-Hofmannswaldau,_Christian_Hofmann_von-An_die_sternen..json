{"dta.poem.18390": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An die sternen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr b\u00fcrger stiller nacht/ ihr kinder voller flammen/", "tokens": ["Ihr", "b\u00fcr\u00b7ger", "stil\u00b7ler", "nacht", "/", "ihr", "kin\u00b7der", "vol\u00b7ler", "flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$(", "PPOSAT", "NN", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr br\u00fcder reiner glut/ ihr leichtes himmels-heer/", "tokens": ["Ihr", "br\u00fc\u00b7der", "rei\u00b7ner", "glut", "/", "ihr", "leich\u00b7tes", "him\u00b7mels\u00b7heer", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ihr voll licht und glantz/ von finsternis seyd leer/", "tokens": ["Die", "ihr", "voll", "licht", "und", "glantz", "/", "von", "fins\u00b7ter\u00b7nis", "seyd", "leer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "NN", "KON", "NN", "$(", "APPR", "NE", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jhr sterne/ die ihr hier als freunde kommt zusammen/", "tokens": ["Ihr", "ster\u00b7ne", "/", "die", "ihr", "hier", "als", "freun\u00b7de", "kommt", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PRELS", "PPER", "ADV", "KOUS", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jetzt da die mattigkeit die glieder will durchkriechen/", "tokens": ["Jetzt", "da", "die", "mat\u00b7tig\u00b7keit", "die", "glie\u00b7der", "will", "durch\u00b7krie\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und da ein sanffter schlaff die sinnen \u00fcberschlichen/", "tokens": ["Und", "da", "ein", "sanff\u00b7ter", "schlaff", "die", "sin\u00b7nen", "\u00fc\u00b7berschli\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJD", "VVFIN", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Was weist ihr doch umbsonst jetzt euer reines licht", "tokens": ["Was", "weist", "ihr", "doch", "um\u00b7bsonst", "jetzt", "eu\u00b7er", "rei\u00b7nes", "licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und lasset euer aug an goldnen fenstern sehen/", "tokens": ["Und", "las\u00b7set", "eu\u00b7er", "aug", "an", "gold\u00b7nen", "fens\u00b7tern", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ambrette wird gewi\u00df euch eine nase drehen/", "tokens": ["Am\u00b7bret\u00b7te", "wird", "ge\u00b7wi\u00df", "euch", "ei\u00b7ne", "na\u00b7se", "dre\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "PPER", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Verkricht euch wieder nur; ihr schaut sie heute nicht.", "tokens": ["Ver\u00b7kricht", "euch", "wie\u00b7der", "nur", ";", "ihr", "schaut", "sie", "heu\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}