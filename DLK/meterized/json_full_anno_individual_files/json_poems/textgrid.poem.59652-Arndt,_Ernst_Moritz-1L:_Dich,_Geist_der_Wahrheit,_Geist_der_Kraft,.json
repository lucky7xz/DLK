kens": ["Und", "sprich", "mir", "zu", "und", "leg'", "mir's", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was ich nicht mehr begreifen mag,", "tokens": ["Was", "ich", "nicht", "mehr", "be\u00b7grei\u00b7fen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was Christus zu den S\u00fcndern sprach.", "tokens": ["Was", "Chris\u00b7tus", "zu", "den", "S\u00fcn\u00b7dern", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Sprich mir das Wort der Liebe zu,", "tokens": ["Sprich", "mir", "das", "Wort", "der", "Lie\u00b7be", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den rechten Klang verstehst nur du,", "tokens": ["Den", "rech\u00b7ten", "Klang", "ver\u00b7stehst", "nur", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das rechte Wort, den rechten Klang,", "tokens": ["Das", "rech\u00b7te", "Wort", ",", "den", "rech\u00b7ten", "Klang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Glaubens Hoffnung und Empfang.", "tokens": ["Des", "Glau\u00b7bens", "Hoff\u00b7nung", "und", "Emp\u00b7fang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.16": {"line.1": {"text": "O Geist der Liebe, Geist des Herrn!", "tokens": ["O", "Geist", "der", "Lie\u00b7be", ",", "Geist", "des", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Himmelsfreude Gnadenstern!", "tokens": ["Der", "Him\u00b7mels\u00b7freu\u00b7de", "Gna\u00b7dens\u00b7tern", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Geh auf in mir mit deinem Schein!", "tokens": ["Geh", "auf", "in", "mir", "mit", "dei\u00b7nem", "Schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So kann ich wieder fr\u00f6hlich sein.", "tokens": ["So", "kann", "ich", "wie\u00b7der", "fr\u00f6h\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}