{"textgrid.poem.65703": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: So wie ein Gockelhan mit aufgereckter Krone", "genre": "verse", "period": "N.A.", "pub_year": 1726, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wie ein Gockelhan mit aufgereckter Krone", "tokens": ["So", "wie", "ein", "Go\u00b7ckel\u00b7han", "mit", "auf\u00b7ge\u00b7reck\u00b7ter", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein Schauger\u00fcst besteigt, und auf dem waichen Trohne", "tokens": ["Sein", "Schau\u00b7ge\u00b7r\u00fcst", "be\u00b7steigt", ",", "und", "auf", "dem", "wai\u00b7chen", "Troh\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Fl\u00fcgel munter stra\u00fc\u00dft, den stolzen Hals erhebt,", "tokens": ["Die", "Fl\u00fc\u00b7gel", "mun\u00b7ter", "stra\u00fc\u00dft", ",", "den", "stol\u00b7zen", "Hals", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und bald ein Feldgeschrey, vor dem der Hof erbebt,", "tokens": ["Und", "bald", "ein", "Feld\u00b7ge\u00b7schrey", ",", "vor", "dem", "der", "Hof", "er\u00b7bebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Getrost erschallen l\u00e4\u00dft; bald sehnlich um sich blicket,", "tokens": ["Ge\u00b7trost", "er\u00b7schal\u00b7len", "l\u00e4\u00dft", ";", "bald", "sehn\u00b7lich", "um", "sich", "bli\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVFIN", "$.", "ADV", "ADV", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ob auch sein holder Tohn das Federvolk entz\u00fccket:", "tokens": ["Ob", "auch", "sein", "hol\u00b7der", "Tohn", "das", "Fe\u00b7der\u00b7volk", "ent\u00b7z\u00fc\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So greifft zur Fr\u00fchlingszeit ein junger B\u00fccherheld", "tokens": ["So", "greifft", "zur", "Fr\u00fch\u00b7lings\u00b7zeit", "ein", "jun\u00b7ger", "B\u00fc\u00b7cher\u00b7held"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von ne\u00fcem Trieb belebt, zu Trost der klugen Welt,", "tokens": ["Von", "ne\u00b7\u00fcem", "Trieb", "be\u00b7lebt", ",", "zu", "Trost", "der", "klu\u00b7gen", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.9": {"text": "Nach dem gesch\u00e4rften Kiel, und will mit Tausend Z\u00fcgen", "tokens": ["Nach", "dem", "ge\u00b7sch\u00e4rf\u00b7ten", "Kiel", ",", "und", "will", "mit", "Tau\u00b7send", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "VMFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Dichter stolzes Heer besch\u00e4men und besiegen.", "tokens": ["Der", "Dich\u00b7ter", "stol\u00b7zes", "Heer", "be\u00b7sch\u00e4\u00b7men", "und", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bald reist sein sch\u00f6nes Blatt dem Patrioten zu;", "tokens": ["Bald", "reist", "sein", "sch\u00f6\u00b7nes", "Blatt", "dem", "Pat\u00b7ri\u00b7o\u00b7ten", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Held ist auser sich; er kennet keine Ruh,", "tokens": ["Der", "Held", "ist", "au\u00b7ser", "sich", ";", "er", "ken\u00b7net", "kei\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PRF", "$.", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bis eine schnelle Post ihn sein Verh\u00e4ngni\u00df lehret.", "tokens": ["Bis", "ei\u00b7ne", "schnel\u00b7le", "Post", "ihn", "sein", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "leh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch, wenn sein zitternd Ohr die Trauerbottschaft h\u00f6ret,", "tokens": ["Doch", ",", "wenn", "sein", "zit\u00b7ternd", "Ohr", "die", "Trau\u00b7er\u00b7bott\u00b7schaft", "h\u00f6\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPOSAT", "ADJD", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ihn ein strenger Spruch ver\u00e4chtlich unterdr\u00fcckt,", "tokens": ["Da\u00df", "ihn", "ein", "stren\u00b7ger", "Spruch", "ver\u00b7\u00e4cht\u00b7lich", "un\u00b7ter\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und Hamburgs Bl\u00e4tter nicht mit seiner Frucht geschm\u00fcckt.", "tokens": ["Und", "Ham\u00b7burgs", "Bl\u00e4t\u00b7ter", "nicht", "mit", "sei\u00b7ner", "Frucht", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O welch ein Donnerstrahl f\u00fcr den best\u00fcrzten Dichter!", "tokens": ["O", "welch", "ein", "Don\u00b7ner\u00b7strahl", "f\u00fcr", "den", "be\u00b7st\u00fcrz\u00b7ten", "Dich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie? rufft er halb entseelt, verdammt der eckle Richter", "tokens": ["Wie", "?", "rufft", "er", "halb", "ent\u00b7seelt", ",", "ver\u00b7dammt", "der", "ec\u00b7kle", "Rich\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "ADJD", "VVPP", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Unschuldvolles Blatt? Ist dies der Weysheit Lohn?", "tokens": ["Mein", "Un\u00b7schuld\u00b7vol\u00b7les", "Blatt", "?", "Ist", "dies", "der", "Weys\u00b7heit", "Lohn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "VAFIN", "PDS", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Jedoch die Rache folgt auf unverdienten Hohn.", "tokens": ["Je\u00b7doch", "die", "Ra\u00b7che", "folgt", "auf", "un\u00b7ver\u00b7dien\u00b7ten", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich schwere bey dem Schimpf, der meine Schrift beflecket,", "tokens": ["Ich", "schwe\u00b7re", "bey", "dem", "Schimpf", ",", "der", "mei\u00b7ne", "Schrift", "be\u00b7fle\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ich zum letsten mal der Dorheit Quell entdecket,", "tokens": ["Da\u00df", "ich", "zum", "lets\u00b7ten", "mal", "der", "Dor\u00b7heit", "Quell", "ent\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Laster Brut bek\u00e4mpft. Komm, tumme Barbarey!", "tokens": ["Der", "Las\u00b7ter", "Brut", "be\u00b7k\u00e4mpft", ".", "Komm", ",", "tum\u00b7me", "Bar\u00b7ba\u00b7rey", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$.", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es steht Germanien dir f\u00f6rter blo\u00df und frey.", "tokens": ["Es", "steht", "Ger\u00b7ma\u00b7ni\u00b7en", "dir", "f\u00f6r\u00b7ter", "blo\u00df", "und", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PPER", "ADJA", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sollte Kunst und Witz aus aller Welt verfliegen,", "tokens": ["Und", "soll\u00b7te", "Kunst", "und", "Witz", "aus", "al\u00b7ler", "Welt", "ver\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "KON", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Soll mein beschimpfter Kiel auf ewig m\u00fcssig ligen.", "tokens": ["Soll", "mein", "be\u00b7schimpf\u00b7ter", "Kiel", "auf", "e\u00b7wig", "m\u00fcs\u00b7sig", "li\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So wie ein Gockelhan mit aufgereckter Krone", "tokens": ["So", "wie", "ein", "Go\u00b7ckel\u00b7han", "mit", "auf\u00b7ge\u00b7reck\u00b7ter", "Kro\u00b7ne"], "tok