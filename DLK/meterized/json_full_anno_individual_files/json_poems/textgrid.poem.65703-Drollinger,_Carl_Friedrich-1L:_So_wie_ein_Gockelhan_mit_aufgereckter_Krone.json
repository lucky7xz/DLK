": "Es steht Germanien dir f\u00f6rter blo\u00df und frey.", "tokens": ["Es", "steht", "Ger\u00b7ma\u00b7ni\u00b7en", "dir", "f\u00f6r\u00b7ter", "blo\u00df", "und", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PPER", "ADJA", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sollte Kunst und Witz aus aller Welt verfliegen,", "tokens": ["Und", "soll\u00b7te", "Kunst", "und", "Witz", "aus", "al\u00b7ler", "Welt", "ver\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "KON", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Soll mein beschimpfter Kiel auf ewig m\u00fcssig ligen.", "tokens": ["Soll", "mein", "be\u00b7schimpf\u00b7ter", "Kiel", "auf", "e\u00b7wig", "m\u00fcs\u00b7sig", "li\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}