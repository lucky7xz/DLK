{"dta.poem.3296": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Morpheus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Bey Venus ward von Sch\u00e4ferinnen", "tokens": ["Bey", "Ve\u00b7nus", "ward", "von", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der holde Morpheus hart verklagt:", "tokens": ["Der", "hol\u00b7de", "Mor\u00b7pheus", "hart", "ver\u00b7klagt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird sein abscheuliches Beginnen", "tokens": ["Wird", "sein", "ab\u00b7scheu\u00b7li\u00b7ches", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Jhm, sprachen sie, nicht untersagt.", "tokens": ["Jhm", ",", "spra\u00b7chen", "sie", ",", "nicht", "un\u00b7ter\u00b7sagt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "PPER", "$,", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bey Tage sind wir Sch\u00e4fern spr\u00f6de:", "tokens": ["Bey", "Ta\u00b7ge", "sind", "wir", "Sch\u00e4\u00b7fern", "spr\u00f6\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch sieh, wie schalkhaft Morpheus ist!", "tokens": ["Doch", "sieh", ",", "wie", "schalk\u00b7haft", "Mor\u00b7pheus", "ist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADJD", "NE", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jm Traum ist keine Hirtinn bl\u00f6de;", "tokens": ["Jm", "Traum", "ist", "kei\u00b7ne", "Hir\u00b7tinn", "bl\u00f6\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ja, leider! auch die Unschuld k\u00fcsst.", "tokens": ["Ja", ",", "lei\u00b7der", "!", "auch", "die", "Un\u00b7schuld", "k\u00fcsst", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "$.", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Sch\u00e4fer weihen ihm Ges\u00e4nge:", "tokens": ["Die", "Sch\u00e4\u00b7fer", "wei\u00b7hen", "ihm", "Ge\u00b7s\u00e4n\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er heuchelt ihrer Z\u00e4rtlichkeit,", "tokens": ["Er", "heu\u00b7chelt", "ih\u00b7rer", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und spottet unsrer keuschen Strenge,", "tokens": ["Und", "spot\u00b7tet", "uns\u00b7rer", "keu\u00b7schen", "Stren\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ach! uns manche Lust verbeut.", "tokens": ["Die", "ach", "!", "uns", "man\u00b7che", "Lust", "ver\u00b7beut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "XY", "$.", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Thyrsis, der zu Doris F\u00fcssen", "tokens": ["Ein", "Thyr\u00b7sis", ",", "der", "zu", "Do\u00b7ris", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vor wenig Stunden trostlos lag,", "tokens": ["Vor", "we\u00b7nig", "Stun\u00b7den", "trost\u00b7los", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kann tr\u00e4umend seine Spr\u00f6de k\u00fcssen,", "tokens": ["Kann", "tr\u00e4u\u00b7mend", "sei\u00b7ne", "Spr\u00f6\u00b7de", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die alles will, was Morpheus mag.", "tokens": ["Die", "al\u00b7les", "will", ",", "was", "Mor\u00b7pheus", "mag."], "token_info": ["word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["ART", "PIS", "VMFIN", "$,", "PRELS", "NE", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hier unterbrach die langen Klagen", "tokens": ["Hier", "un\u00b7ter\u00b7brach", "die", "lan\u00b7gen", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Traumgott voller Ungeduld,", "tokens": ["Der", "Traum\u00b7gott", "vol\u00b7ler", "Un\u00b7ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sprach: o G\u00f6ttinn! darf ichs wagen;", "tokens": ["Und", "sprach", ":", "o", "G\u00f6t\u00b7tinn", "!", "darf", "ichs", "wa\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "FM", "NN", "$.", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So h\u00f6re mich mit gleicher Huld.", "tokens": ["So", "h\u00f6\u00b7re", "mich", "mit", "glei\u00b7cher", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So m\u00fcsse dir der Weltkreis fr\u00f6hnen,", "tokens": ["So", "m\u00fcs\u00b7se", "dir", "der", "Welt\u00b7kreis", "fr\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Amors Bogen sey begl\u00fcckt,", "tokens": ["Und", "A\u00b7mors", "Bo\u00b7gen", "sey", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Solang auf Wangen junger Sch\u00f6nen", "tokens": ["So\u00b7lang", "auf", "Wan\u00b7gen", "jun\u00b7ger", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein bl\u00fchend Morgenroth entz\u00fcckt!", "tokens": ["Ein", "bl\u00fc\u00b7hend", "Mor\u00b7gen\u00b7roth", "ent\u00b7z\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich mu\u00df der frommen M\u00e4dchen lachen:", "tokens": ["Ich", "mu\u00df", "der", "from\u00b7men", "M\u00e4d\u00b7chen", "la\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie tr\u00e4umen von verliebter Lust!", "tokens": ["Sie", "tr\u00e4u\u00b7men", "von", "ver\u00b7lieb\u00b7ter", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Welch Wunder? herrscht, wann M\u00e4dchen wachen,", "tokens": ["Welch", "Wun\u00b7der", "?", "herrscht", ",", "wann", "M\u00e4d\u00b7chen", "wa\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "VVFIN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Liebe nicht in ihrer Brust?", "tokens": ["Die", "Lie\u00b7be", "nicht", "in", "ih\u00b7rer", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich weis, was ieder Sch\u00f6nen fehlet,", "tokens": ["Ich", "weis", ",", "was", "ie\u00b7der", "Sch\u00f6\u00b7nen", "feh\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Um die mein stiller Fittig spielt;", "tokens": ["Um", "die", "mein", "stil\u00b7ler", "Fit\u00b7tig", "spielt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sehe was ihr Herz verhehlet,", "tokens": ["Und", "se\u00b7he", "was", "ihr", "Herz", "ver\u00b7heh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und oft sie selbst nur dunkel f\u00fchlt.", "tokens": ["Und", "oft", "sie", "selbst", "nur", "dun\u00b7kel", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Manch M\u00e4dchen prangt mit scheuer Tugend,", "tokens": ["Manch", "M\u00e4d\u00b7chen", "prangt", "mit", "scheu\u00b7er", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das ingeheim zu Amorn fleht,", "tokens": ["Das", "in\u00b7ge\u00b7heim", "zu", "A\u00b7morn", "fleht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann itzt im Fr\u00fchling muntrer Jugend", "tokens": ["Wann", "itzt", "im", "Fr\u00fch\u00b7ling", "mun\u00b7trer", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jhr Busen in der F\u00fclle steht.", "tokens": ["Ihr", "Bu\u00b7sen", "in", "der", "F\u00fcl\u00b7le", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie seufzt, und, o gerechter Kummer!", "tokens": ["Sie", "seufzt", ",", "und", ",", "o", "ge\u00b7rech\u00b7ter", "Kum\u00b7mer", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "$,", "FM", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es jammert mich der Sch\u00e4ferinn:", "tokens": ["Es", "jam\u00b7mert", "mich", "der", "Sch\u00e4\u00b7fe\u00b7rinn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich f\u00fchre sie bey fr\u00fchem Schlummer", "tokens": ["Ich", "f\u00fch\u00b7re", "sie", "bey", "fr\u00fc\u00b7hem", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In ihres Hirten Arme hin.", "tokens": ["In", "ih\u00b7res", "Hir\u00b7ten", "Ar\u00b7me", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Liebt Chloe nichts, als ihre Heerde?", "tokens": ["Liebt", "Chloe", "nichts", ",", "als", "ih\u00b7re", "Heer\u00b7de", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PIS", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie glaubts! ihr Auge saget mir,", "tokens": ["Sie", "glaubts", "!", "ihr", "Au\u00b7ge", "sa\u00b7get", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Chloen Damon k\u00fcssen werde;", "tokens": ["Da\u00df", "Chloen", "Da\u00b7mon", "k\u00fcs\u00b7sen", "wer\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich verrath es ihm und ihr.", "tokens": ["Und", "ich", "ver\u00b7rath", "es", "ihm", "und", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Spr\u00f6de schleicht mit mir in Gr\u00fcnde", "tokens": ["Die", "Spr\u00f6\u00b7de", "schleicht", "mit", "mir", "in", "Gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu B\u00fcschen, wo kein Fremder lauscht,", "tokens": ["Zu", "B\u00fc\u00b7schen", ",", "wo", "kein", "Frem\u00b7der", "lauscht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wann beym Geschw\u00e4tze sanfter Winde", "tokens": ["Wann", "beym", "Ge\u00b7schw\u00e4t\u00b7ze", "sanf\u00b7ter", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Scherz geheimer Schm\u00e4tzchen rauscht.", "tokens": ["Der", "Scherz", "ge\u00b7hei\u00b7mer", "Schm\u00e4tz\u00b7chen", "rauscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ein ieder gleichet seinen Tr\u00e4umen:", "tokens": ["Ein", "ie\u00b7der", "glei\u00b7chet", "sei\u00b7nen", "Tr\u00e4u\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jm Traume zecht Anakreon:", "tokens": ["Jm", "Trau\u00b7me", "zecht", "A\u00b7nak\u00b7re\u00b7on", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Dichter jauchzt bey seinen Reimen,", "tokens": ["Ein", "Dich\u00b7ter", "jauchzt", "bey", "sei\u00b7nen", "Rei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und flattert um den Helikon.", "tokens": ["Und", "flat\u00b7tert", "um", "den", "He\u00b7li\u00b7kon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr euch, Monaden! ficht mit Schl\u00fcssen", "tokens": ["F\u00fcr", "euch", ",", "Mo\u00b7na\u00b7den", "!", "ficht", "mit", "Schl\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "NN", "$.", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Liebling der Ontologie;", "tokens": ["Ein", "Lieb\u00b7ling", "der", "On\u00b7to\u00b7lo\u00b7gie", ";"], "token_inf