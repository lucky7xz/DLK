{"textgrid.poem.39910": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In jener Zeiten Schwere,", "tokens": ["In", "je\u00b7ner", "Zei\u00b7ten", "Schwe\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo Luthers fromme Lehre", "tokens": ["Wo", "Lu\u00b7thers", "from\u00b7me", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die uns zu Gottes Ehre", "tokens": ["Die", "uns", "zu", "Got\u00b7tes", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der theure ", "tokens": ["Der", "theu\u00b7re"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Noch h\u00fclflos in der Wiege", "tokens": ["Noch", "h\u00fcl\u00b7flos", "in", "der", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bedr\u00e4ngt vom schweren Kriege,", "tokens": ["Be\u00b7dr\u00e4ngt", "vom", "schwe\u00b7ren", "Krie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ge\u00e4ngstet ward vom Siege", "tokens": ["Ge\u00b7\u00e4ngs\u00b7tet", "ward", "vom", "Sie\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der span'schen Heeresmacht:", "tokens": ["Der", "span'\u00b7schen", "Hee\u00b7res\u00b7macht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da hat es sich begeben,", "tokens": ["Da", "hat", "es", "sich", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df noch ein rechtes Leben,", "tokens": ["Da\u00df", "noch", "ein", "rech\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein mutiges Bestreben", "tokens": ["Ein", "mu\u00b7ti\u00b7ges", "Be\u00b7stre\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im D\u00f6rflein ", "tokens": ["Im", "D\u00f6r\u00b7flein"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Dort lehrte noch zur Stunde", "tokens": ["Dort", "lehr\u00b7te", "noch", "zur", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit seinem freien Munde,", "tokens": ["Mit", "sei\u00b7nem", "frei\u00b7en", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Aus seines Herzens Grunde,", "tokens": ["Aus", "sei\u00b7nes", "Her\u00b7zens", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Pfarrer fromm und klar.", "tokens": ["Ein", "Pfar\u00b7rer", "fromm", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So stand er ohne Sorgen,", "tokens": ["So", "stand", "er", "oh\u00b7ne", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An einem Sonntagsmorgen", "tokens": ["An", "ei\u00b7nem", "Sonn\u00b7tags\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Kanzelstuhl geborgen,", "tokens": ["Im", "Kan\u00b7zel\u00b7stuhl", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um den die Menge wogt:", "tokens": ["Um", "den", "die", "Men\u00b7ge", "wogt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da kommt mit raschen Schritten", "tokens": ["Da", "kommt", "mit", "ra\u00b7schen", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In andrer H\u00f6rer Mitten", "tokens": ["In", "an\u00b7drer", "H\u00f6\u00b7rer", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Durch's Kirchenthor geschritten", "tokens": ["Durch's", "Kir\u00b7chen\u00b7thor", "ge\u00b7schrit\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "\u00bb'S ist keiner von den B\u00f6sen!", "tokens": ["\u00bb", "'s", "ist", "kei\u00b7ner", "von", "den", "B\u00f6\u00b7sen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "VAFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jedoch den Text ihm lesen,", "tokens": ["Je\u00b7doch", "den", "Text", "ihm", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mag doch nicht \u00fcbel wesen!\u00ab", "tokens": ["Mag", "doch", "nicht", "\u00fc\u00b7bel", "we\u00b7sen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Pfarrer bei sich spricht:", "tokens": ["Der", "Pfar\u00b7rer", "bei", "sich", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er fordert die verdammten,", "tokens": ["Er", "for\u00b7dert", "die", "ver\u00b7damm\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Unglaubigen Beamten,", "tokens": ["Un\u00b7glau\u00b7bi\u00b7gen", "Be\u00b7am\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Die all' der H\u00f6ll' entstammten,", "tokens": ["Die", "all'", "der", "H\u00f6ll'", "ent\u00b7stamm\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vor Gottes Strafgericht.", "tokens": ["Vor", "Got\u00b7tes", "Straf\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Als er der langen Predigt", "tokens": ["Als", "er", "der", "lan\u00b7gen", "Pre\u00b7digt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Poltern sich entledigt,", "tokens": ["Mit", "Pol\u00b7tern", "sich", "ent\u00b7le\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr Schwei\u00df und M\u00fch' entsch\u00e4digt,", "tokens": ["F\u00fcr", "Schwei\u00df", "und", "M\u00fch'", "ent\u00b7sch\u00e4\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Steigt er vergn\u00fcgt herab;", "tokens": ["Steigt", "er", "ver\u00b7gn\u00fcgt", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ihm naht der Vogt mit Nicken,", "tokens": ["Ihm", "naht", "der", "Vogt", "mit", "Ni\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Gru\u00df und H\u00e4ndedr\u00fccken:", "tokens": ["Mit", "Gru\u00df", "und", "H\u00e4n\u00b7de\u00b7dr\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbla\u00dft euch zu Mittag blicken,", "tokens": ["\u00bb", "la\u00dft", "euch", "zu", "Mit\u00b7tag", "bli\u00b7cken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ehrw\u00fcrd'ger, schlagt's nicht ab!\u00ab", "tokens": ["Ehr\u00b7w\u00fcrd'\u00b7ger", ",", "schlagt's", "nicht", "ab", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Was ist's? er mu\u00df wohl kommen!", "tokens": ["Was", "ist's", "?", "er", "mu\u00df", "wohl", "kom\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$.", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er sa\u00df und a\u00df beklommen,", "tokens": ["Er", "sa\u00df", "und", "a\u00df", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch als er eingenommen", "tokens": ["Doch", "als", "er", "ein\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den guten Neckarwein,", "tokens": ["Den", "gu\u00b7ten", "Ne\u00b7ckar\u00b7wein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und ihn der Vogt von Herzen", "tokens": ["Und", "ihn", "der", "Vogt", "von", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erg\u00f6tzt mit ehrbar'n Scherzen,", "tokens": ["Er\u00b7g\u00f6tzt", "mit", "ehr\u00b7ba\u00b7r'n", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wollt' ihn schon reun und schmerzen", "tokens": ["Wollt'", "ihn", "schon", "reun", "und", "schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das \u00fcberm\u00e4\u00df'ge Schrei'n.", "tokens": ["Das", "\u00fc\u00b7ber\u00b7m\u00e4\u00df'\u00b7ge", "Schrei'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Behaglich war es Beiden;", "tokens": ["Be\u00b7hag\u00b7lich", "war", "es", "Bei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da sprach der Vogt bei'm Scheiden", "tokens": ["Da", "sprach", "der", "Vogt", "bei'm", "Schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zuletzt: \u00bbHerr! k\u00f6nnt Ihr's meiden,", "tokens": ["Zu\u00b7letzt", ":", "\u00bb", "Herr", "!", "k\u00f6nnt", "Ih\u00b7r's", "mei\u00b7den", ","], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "$(", "NN", "$.", "VVFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So predigt nicht so streng!", "tokens": ["So", "pre\u00b7digt", "nicht", "so", "streng", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Schimpfen und das Schelten,", "tokens": ["Das", "Schimp\u00b7fen", "und", "das", "Schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Glaubt mir, es frommet selten,", "tokens": ["Glaubt", "mir", ",", "es", "from\u00b7met", "sel\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wem es just soll gelten,", "tokens": ["Und", "wem", "es", "just", "soll", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem macht's um's Herz nicht eng.\u00ab", "tokens": ["Dem", "macht's", "um's", "Herz", "nicht", "eng", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Das zieht dem guten Alten", "tokens": ["Das", "zieht", "dem", "gu\u00b7ten", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Stirn' auf's neu' in Falten;", "tokens": ["Die", "Stirn'", "auf's", "neu'", "in", "Fal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er spricht halb ungehalten,", "tokens": ["Er", "spricht", "halb", "un\u00b7ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Halb aber noch im Scherz:", "tokens": ["Halb", "a\u00b7ber", "noch", "im", "Scherz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbihr werdet mich nicht fragen,", "tokens": ["\u00bb", "ihr", "wer\u00b7det", "mich", "nicht", "fra\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch wenn Ihr's k\u00f6nnt ertragen,", "tokens": ["Doch", "wenn", "Ih\u00b7r's", "k\u00f6nnt", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVFIN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Was hier die Leute sagen,", "tokens": ["Was", "hier", "die", "Leu\u00b7te", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So leg' ich's Euch an's Herz:", "tokens": ["So", "leg'", "ich's", "Euch", "an's", "Herz", ":"], "token_in