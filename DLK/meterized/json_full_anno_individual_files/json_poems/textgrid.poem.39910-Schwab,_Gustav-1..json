i\u00b7schet", "nicht", "im", "Tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mag Trunk und Spiel nicht \u00fcben,", "tokens": ["Mag", "Trunk", "und", "Spiel", "nicht", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Treibt kein verbotnes Lieben,", "tokens": ["Treibt", "kein", "ver\u00b7bot\u00b7nes", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O, la\u00dft die Leute schm\u00e4hen,", "tokens": ["O", ",", "la\u00dft", "die", "Leu\u00b7te", "schm\u00e4\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie werden's nicht verstehen: \u2013", "tokens": ["Sie", "wer\u00b7den's", "nicht", "ver\u00b7ste\u00b7hen", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr scheut das Kirchengehen!", "tokens": ["Ihr", "scheut", "das", "Kir\u00b7chen\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}