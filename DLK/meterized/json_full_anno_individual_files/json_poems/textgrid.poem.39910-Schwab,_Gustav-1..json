, "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "So stand er ohne Sorgen,", "tokens": ["So", "stand", "er", "oh\u00b7ne", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An einem Sonntagsmorgen", "tokens": ["An", "ei\u00b7nem", "Sonn\u00b7tags\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Kanzelstuhl geborgen,", "tokens": ["Im", "Kan\u00b7zel\u00b7stuhl", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um den die Menge wogt:", "tokens": ["Um", "den", "die", "Men\u00b7ge", "wogt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da kommt mit raschen Schritten", "tokens": ["Da", "kommt", "mit", "ra\u00b7schen", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In andrer H\u00f6rer Mitten", "tokens": ["In", "an\u00b7drer", "H\u00f6\u00b7rer", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Durch's Kirchenthor geschritten", "tokens": ["Durch's", "Kir\u00b7chen\u00b7thor", "ge\u00b7schrit\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.13": {"line.1": {"text": "\u00bb'S ist keiner von den B\u00f6sen!", "tokens": ["\u00bb", "'s", "ist", "kei\u00b7ner", "von", "den", "B\u00f6\u00b7sen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "VAFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jedoch den Text ihm lesen,", "tokens": ["Je\u00b7doch", "den", "Text", "ihm", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mag doch nicht \u00fcbel wesen!\u00ab", "tokens": ["Mag", "doch", "nicht", "\u00fc\u00b7bel", "we\u00b7sen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Pfarrer bei sich spricht:", "tokens": ["Der", "Pfar\u00b7rer", "bei", "sich", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er fordert die verdammten,", "tokens": ["Er", "for\u00b7dert", "die", "ver\u00b7damm\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Unglaubigen Beamten,", "tokens": ["Un\u00b7glau\u00b7bi\u00b7gen", "Be\u00b7am\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Die all' der H\u00f6ll' entstammten,", "tokens": ["Die", "all'", "der", "H\u00f6ll'", "ent\u00b7stamm\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vor Gottes Strafgericht.", "tokens": ["Vor", "Got\u00b7tes", "Straf\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Als er der langen Predigt", "tokens": ["Als", "er", "der", "lan\u00b7gen", "Pre\u00b7digt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Poltern sich entledigt,", "tokens": ["Mit", "Pol\u00b7tern", "sich", "ent\u00b7le\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr Schwei\u00df und M\u00fch' entsch\u00e4digt,", "tokens": ["F\u00fcr", "Schwei\u00df", "und", "M\u00fch'", "ent\u00b7sch\u00e4\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Steigt er vergn\u00fcgt herab;", "tokens": ["Steigt", "er", "ver\u00b7gn\u00fcgt", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ihm naht der Vogt mit Nicken,", "tokens": ["Ihm", "naht", "der", "Vogt", "mit", "Ni\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Gru\u00df und H\u00e4ndedr\u00fccken:", "tokens": ["Mit", "Gru\u00df", "und", "H\u00e4n\u00b7de\u00b7dr\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "