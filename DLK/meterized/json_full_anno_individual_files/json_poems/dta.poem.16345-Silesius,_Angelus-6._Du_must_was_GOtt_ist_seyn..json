{"dta.poem.16345": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "6.  Du must was GOtt ist seyn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Sol ich mein letztes End/ und ersten Anfang finden/", "tokens": ["Sol", "ich", "mein", "letz\u00b7tes", "End", "/", "und", "ers\u00b7ten", "An\u00b7fang", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mu\u00df ich mich in GOtt/ un\u0304 GOtt in mir ergr\u00fcnden.", "tokens": ["So", "mu\u00df", "ich", "mich", "in", "Gott", "/", "un\u0304", "Gott", "in", "mir", "er\u00b7gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "NN", "$(", "KON", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und werden das was Er: Ich mu\u00df ein Schein im", "tokens": ["Und", "wer\u00b7den", "das", "was", "Er", ":", "Ich", "mu\u00df", "ein", "Schein", "im"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PDS", "PRELS", "PPER", "$.", "PPER", "VMFIN", "ART", "NN", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schein/", "tokens": ["Schein", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Ich mu\u