{"textgrid.poem.64306": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "25. Lied eines Deutschen in fremden Kriegsdiensten", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An fernem Ufer hingebannt,", "tokens": ["An", "fer\u00b7nem", "U\u00b7fer", "hin\u00b7ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Thut mir's von Herzen weh,", "tokens": ["Thut", "mir's", "von", "Her\u00b7zen", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich mein liebes Vaterland", "tokens": ["Da\u00df", "ich", "mein", "lie\u00b7bes", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht mehr mit Augen seh.", "tokens": ["Nicht", "mehr", "mit", "Au\u00b7gen", "seh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sehne t\u00e4glich mich zur\u00fcck;", "tokens": ["Ich", "seh\u00b7ne", "t\u00e4g\u00b7lich", "mich", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das l\u00e4\u00dft mir keine Ruh!", "tokens": ["Das", "l\u00e4\u00dft", "mir", "kei\u00b7ne", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich werfe manchen nassen Blick", "tokens": ["Ich", "wer\u00b7fe", "man\u00b7chen", "nas\u00b7sen", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem wilden Meere zu.", "tokens": ["Dem", "wil\u00b7den", "Mee\u00b7re", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das war zuvor nicht meine Art!", "tokens": ["Das", "war", "zu\u00b7vor", "nicht", "mei\u00b7ne", "Art", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun wein' ich, wie ein Kind;", "tokens": ["Nun", "wein'", "ich", ",", "wie", "ein", "Kind", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df oft am schwarzen Knebelbart", "tokens": ["Da\u00df", "oft", "am", "schwar\u00b7zen", "Kne\u00b7bel\u00b7bart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die helle Thr\u00e4ne rinnt.", "tokens": ["Die", "hel\u00b7le", "Thr\u00e4\u00b7ne", "rinnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O wehe dem, der mich mit Trug", "tokens": ["O", "we\u00b7he", "dem", ",", "der", "mich", "mit", "Trug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "$,", "PRELS", "PRF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dieses Land gebracht!", "tokens": ["In", "die\u00b7ses", "Land", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Leid verwandle sich in Fluch,", "tokens": ["Mein", "Leid", "ver\u00b7wand\u00b7le", "sich", "in", "Fluch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und qu\u00e4l' ihn Tag und Nacht!", "tokens": ["Und", "qu\u00e4l'", "ihn", "Tag", "und", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er trank mir zu auf Josephs Wohl", "tokens": ["Er", "trank", "mir", "zu", "auf", "Jo\u00b7se\u00b7phs", "Wohl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "APPR", "NE", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In altem Rheinschen Wein,", "tokens": ["In", "al\u00b7tem", "Rhein\u00b7schen", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Go\u00df bis zum Rand die Gl\u00e4ser voll", "tokens": ["Go\u00df", "bis", "zum", "Rand", "die", "Gl\u00e4\u00b7ser", "voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "APPRART", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schenkte weidlich ein:", "tokens": ["Und", "schenk\u00b7te", "weid\u00b7lich", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Bis da\u00df ich taumelte; da las", "tokens": ["Bis", "da\u00df", "ich", "tau\u00b7mel\u00b7te", ";", "da", "las"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Bube Formeln her;", "tokens": ["Der", "Bu\u00b7be", "For\u00b7meln", "her", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sang den Schwur beim vollen Glas,", "tokens": ["Ich", "sang", "den", "Schwur", "beim", "vol\u00b7len", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und trank, und bat um mehr.", "tokens": ["Und", "trank", ",", "und", "bat", "um", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da gab er mir sein schn\u00f6des Gold,", "tokens": ["Da", "gab", "er", "mir", "sein", "schn\u00f6\u00b7des", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zahlte meine Zech;", "tokens": ["Und", "zahl\u00b7te", "mei\u00b7ne", "Zech", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun war ich in des K\u00f6nigs Sold,", "tokens": ["Nun", "war", "ich", "in", "des", "K\u00f6\u00b7nigs", "Sold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mu\u00dfte mit ihm weg.", "tokens": ["Und", "mu\u00df\u00b7te", "mit", "ihm", "weg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die lieben Eltern k\u00fcmmern mich!", "tokens": ["Die", "lie\u00b7ben", "El\u00b7tern", "k\u00fcm\u00b7mern", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Vater h\u00e4rmt sich ab;", "tokens": ["Der", "Va\u00b7ter", "h\u00e4rmt", "sich", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Mutter weinet bitterlich,", "tokens": ["Die", "Mut\u00b7ter", "wei\u00b7net", "bit\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00fcnschet sich ins Grab.", "tokens": ["Und", "w\u00fcn\u00b7schet", "sich", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und du, mein s\u00fc\u00dfes Hannchen, weinst", "tokens": ["Und", "du", ",", "mein", "s\u00fc\u00b7\u00dfes", "Hann\u00b7chen", ",", "weinst"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die blauen Augen rot!", "tokens": ["Die", "blau\u00b7en", "Au\u00b7gen", "rot", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie tr\u00f6sten dich; du aber meinst,", "tokens": ["Sie", "tr\u00f6s\u00b7ten", "dich", ";", "du", "a\u00b7ber", "meinst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Nikolas sei tot.", "tokens": ["Dein", "Ni\u00b7ko\u00b7las", "sei", "tot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "All was du siehst, das mahnet dich", "tokens": ["All", "was", "du", "siehst", ",", "das", "mah\u00b7net", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "PWS", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An deinen Nikolas:", "tokens": ["An", "dei\u00b7nen", "Ni\u00b7ko\u00b7las", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Linde, unter welcher ich", "tokens": ["Die", "Lin\u00b7de", ",", "un\u00b7ter", "wel\u00b7cher", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "$,", "APPR", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit dir im Schatten sa\u00df;", "tokens": ["Mit", "dir", "im", "Schat\u00b7ten", "sa\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der Weinstock, welchen meine Hand", "tokens": ["Der", "Wein\u00b7stock", ",", "wel\u00b7chen", "mei\u00b7ne", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAT", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr Hannchen auferzog,", "tokens": ["F\u00fcr", "Hann\u00b7chen", "auf\u00b7er\u00b7zog", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und fr\u00fch die zarten Reben band,", "tokens": ["Und", "fr\u00fch", "die", "zar\u00b7ten", "Re\u00b7ben", "band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dir zur Laube bog.", "tokens": ["Und", "dir", "zur", "Lau\u00b7be", "bog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Dort warfst du mir mit loser Hand", "tokens": ["Dort", "warfst", "du", "mir", "mit", "lo\u00b7ser", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Beeren in den Mund;", "tokens": ["Die", "Bee\u00b7ren", "in", "den", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort war es, wo wir Hand in Hand", "tokens": ["Dort", "war", "es", ",", "wo", "wir", "Hand", "in", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWAV", "PPER", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beschworen unsern Bund.", "tokens": ["Be\u00b7schwo\u00b7ren", "un\u00b7sern", "Bund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Wie war's den Abend uns so wohl!", "tokens": ["Wie", "wa\u00b7r's", "den", "A\u00b7bend", "uns", "so", "wohl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich f\u00fchrte dich nach Haus;", "tokens": ["Ich", "f\u00fchr\u00b7te", "dich", "nach", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So manche stille Thr\u00e4ne quoll", "tokens": ["So", "man\u00b7che", "stil\u00b7le", "Thr\u00e4\u00b7ne", "quoll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf deinen Blumenstrau\u00df.", "tokens": ["Auf", "dei\u00b7nen", "Blu\u00b7men\u00b7strau\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "So freundlich lachten Wald und Thal", "tokens": ["So", "freund\u00b7lich", "lach\u00b7ten", "Wald", "und", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meinem Leben nicht!", "tokens": ["In", "mei\u00b7nem", "Le\u00b7ben", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Abendr\u00f6te sanfter Strahl", "tokens": ["Der", "A\u00b7bend\u00b7r\u00f6\u00b7te", "sanf\u00b7ter", "Strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erhellte dein Gesicht.", "tokens": ["Er\u00b7hell\u00b7te", "dein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Wie Turtelt\u00e4ubchen liebten wir,", "tokens": ["Wie", "Tur\u00b7tel\u00b7t\u00e4ub\u00b7chen", "lieb\u00b7ten", "wir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und teilten Freud' und Not;", "tokens": ["Und", "teil\u00b7ten", "Freud'", "und", "Not", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir sagten oft, uns w\u00fcrde hier", "tokens": ["Wir", "sag\u00b7ten", "oft", ",", "uns", "w\u00fcr\u00b7de", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts trennen, als der Tod.", "tokens": ["Nichts", "tren\u00b7nen", ",", "als", "der", "Tod", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Nun seufz' ich spat, und seufze fr\u00fch:", "tokens": ["Nun", "seuf\u00b7z'", "ich", "spat", ",", "und", "seuf\u00b7ze", "fr\u00fch", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erbarm dich, lieber Gott!", "tokens": ["Er\u00b7barm", "dich", ",", "lie\u00b7ber", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und rette mich, und rette sie", "tokens": ["Und", "ret\u00b7te", "mich", ",", "und", "ret\u00b7te", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch einen sanften Tod!", "tokens": ["Durch", "ei\u00b7nen", "sanf\u00b7ten", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "An fernem Ufer hingebannt,", "tokens": ["An", "fer\u00b7nem", "U\u00b7fer", "hin\u00b7ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Thut mir's von Herzen weh,", "tokens": ["Thut", "mir's", "von", "Her\u00b7zen", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich mein liebes 