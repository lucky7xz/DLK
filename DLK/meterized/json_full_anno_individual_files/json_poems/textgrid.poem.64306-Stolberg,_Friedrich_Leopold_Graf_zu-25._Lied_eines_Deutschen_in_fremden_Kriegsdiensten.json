ambic.tetra"}, "line.2": {"text": "Nun wein' ich, wie ein Kind;", "tokens": ["Nun", "wein'", "ich", ",", "wie", "ein", "Kind", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df oft am schwarzen Knebelbart", "tokens": ["Da\u00df", "oft", "am", "schwar\u00b7zen", "Kne\u00b7bel\u00b7bart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die helle Thr\u00e4ne rinnt.", "tokens": ["Die", "hel\u00b7le", "Thr\u00e4\u00b7ne", "rinnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "O wehe dem, der mich mit Trug", "tokens": ["O", "we\u00b7he", "dem", ",", "der", "mich", "mit", "Trug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "$,", "PRELS", "PRF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dieses Land gebracht!", "tokens": ["In", "die\u00b7ses", "Land", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Leid verwandle sich in Fluch,", "tokens": ["Mein", "Leid", "ver\u00b7wand\u00b7le", "sich", "in", "Fluch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und qu\u00e4l' ihn Tag und Nacht!", "tokens": ["Und", "qu\u00e4l'", "ihn", "Tag", "und", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Er trank mir zu auf Josephs Wohl", "tokens": ["Er", "trank", "mir", "zu", "auf", "Jo\u00b7se\u00b7phs", "Wohl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "APPR", "NE", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In altem Rheinschen Wein,", "tokens": ["In", "al\u00b7tem", "Rhein\u00b7schen", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Go\u00df bis zum Rand die Gl\u00e4ser voll", "tokens": ["Go\u00df", "bis", "zum", "Rand", "die", "Gl\u00e4\u00b7ser", "voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "APPRART", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schenkte weidlich ein:", "tokens": ["Und", "schenk\u00b7te", "weid\u00b7lich", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Bis da\u00df ich taumelte; da las", "tokens": ["Bis", "da\u00df", "ich", "tau\u00b7mel\u00b7te", ";", "da", "las"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Bube Formeln her;", "tokens": ["Der", "Bu\u00b7be", "For\u00b7meln", "her", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sang den Schwur beim vollen Glas,", "tokens": ["Ich", "sang", "den", "Schwur", "beim", "vol\u00b7len", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und trank, und bat um mehr.", "tokens": ["Und", "trank", ",", "und", "bat", "um", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Da gab er mir sein schn\u00f6des Gold,", "tokens": ["Da", "gab", "er", "mir", "sein", "schn\u00f6\u00b7des", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zahlte meine Zech;", "tokens": ["Und", "zahl\u00b7te", "mei\u00b7ne", "Zech", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun war ich in des K\u00f6nigs Sold,", "tokens": ["Nun", "war", "ich", "in", "des", "K\u00f6\u00b7nigs", "Sold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mu\u00dfte mit ihm weg.", "tokens": ["Und", "mu\u00df\u00b7te", "mit", "ihm", "weg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "Die lieben Eltern k\u00fcmmern mich!", "tokens": ["Die", "lie\u00b7ben", "El\u00b7tern", "k\u00fcm\u00b7mern", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "po