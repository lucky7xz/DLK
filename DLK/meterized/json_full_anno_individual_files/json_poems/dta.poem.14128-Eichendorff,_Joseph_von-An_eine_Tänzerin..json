{"dta.poem.14128": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An eine T\u00e4nzerin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Castagnetten lustig schwingen", "tokens": ["Cas\u00b7tag\u00b7net\u00b7ten", "lus\u00b7tig", "schwin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seh' ich Dich, Du zierlich Kind!", "tokens": ["Seh'", "ich", "Dich", ",", "Du", "zier\u00b7lich", "Kind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PPER", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der Locken schwarzen Ringen", "tokens": ["Mit", "der", "Lo\u00b7cken", "schwar\u00b7zen", "Rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spielt der sommerlaue Wind.", "tokens": ["Spielt", "der", "som\u00b7mer\u00b7lau\u00b7e", "Wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00fcnstlich regst Du sch\u00f6ne Glieder,", "tokens": ["K\u00fcnst\u00b7lich", "regst", "Du", "sch\u00f6\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gl\u00fchendwild", "tokens": ["Gl\u00fc\u00b7hend\u00b7wild"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Z\u00e4rtlichmild", "tokens": ["Z\u00e4rt\u00b7lich\u00b7mild"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Tauchest in Musik Du nieder,", "tokens": ["Tau\u00b7chest", "in", "Mu\u00b7sik", "Du", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und die Woge hebt Dich wieder.", "tokens": ["Und", "die", "Wo\u00b7ge", "hebt", "Dich", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}