{"textgrid.poem.35276": {"metadata": {"author": {"name": "Rubiner, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich kann nicht immer vor der Scheibe stehn.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann nicht immer vor der Scheibe stehn.", "tokens": ["Ich", "kann", "nicht", "im\u00b7mer", "vor", "der", "Schei\u00b7be", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Meine Haare wachsen auf einmal weich aus dem Kinn.", "tokens": ["Mei\u00b7ne", "Haa\u00b7re", "wach\u00b7sen", "auf", "ein\u00b7mal", "weich", "aus", "dem", "Kinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ich habe Menschengesichter nie angesehn.", "tokens": ["Ich", "ha\u00b7be", "Men\u00b7schen\u00b7ge\u00b7sich\u00b7ter", "nie", "an\u00b7ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mein Anzug wird so weit. Ich versinke drin.", "tokens": ["Mein", "An\u00b7zug", "wird", "so", "weit", ".", "Ich", "ver\u00b7sin\u00b7ke", "drin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Pflasterkugeln sind hei\u00df, der Stein rutscht unter mir weg.", "tokens": ["Die", "Pflas\u00b7ter\u00b7ku\u00b7geln", "sind", "hei\u00df", ",", "der", "Stein", "rutscht", "un\u00b7ter", "mir", "weg", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich bin ja ganz klein. Wer sieht mich? Wer wei\u00df von mir?", "tokens": ["Ich", "bin", "ja", "ganz", "klein", ".", "Wer", "sieht", "mich", "?", "Wer", "wei\u00df", "von", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "$.", "PWS", "VVFIN", "PPER", "$.", "PWS", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mauern w\u00f6lben sich auf die Augen wie hohle H\u00e4nde voll Dreck.", "tokens": ["Mau\u00b7ern", "w\u00f6l\u00b7ben", "sich", "auf", "die", "Au\u00b7gen", "wie", "hoh\u00b7le", "H\u00e4n\u00b7de", "voll", "Dreck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ART", "NN", "KOKOM", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "+-+--+-+--+-+--+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Gelb kreisen Lichters\u00e4ulen. Fernher droht Stank von Bier.", "tokens": ["Gelb", "krei\u00b7sen", "Lich\u00b7ter\u00b7s\u00e4u\u00b7len", ".", "Fern\u00b7her", "droht", "Stank", "von", "Bier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$.", "NN", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "B\u00e4rte zittern spitz. Ein Kneifer f\u00e4llt, sicher klirrt's auf dem Tische.", "tokens": ["B\u00e4r\u00b7te", "zit\u00b7tern", "spitz", ".", "Ein", "Knei\u00b7fer", "f\u00e4llt", ",", "si\u00b7cher", "klirrt's", "auf", "dem", "Ti\u00b7sche", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "$.", "ART", "NN", "VVFIN", "$,", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Hab ich mich neulich mit dieser gr\u00fcnen Hure gerollt?", "tokens": ["Hab", "ich", "mich", "neu\u00b7lich", "mit", "die\u00b7ser", "gr\u00fc\u00b7nen", "Hu\u00b7re", "ge\u00b7rollt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "APPR", "PDAT", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "\u00dcber schwarze B\u00e4uche streichen Daumen dick und still wie wei\u00dfe Fische.", "tokens": ["\u00dc\u00b7ber", "schwar\u00b7ze", "B\u00e4u\u00b7che", "strei\u00b7chen", "Dau\u00b7men", "dick", "und", "still", "wie", "wei\u00b7\u00dfe", "Fi\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "ADJD", "KON", "ADJD", "KOKOM", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Tr\u00e4ge Z\u00e4hne schnappen im Rauch. Da schweben unerschwingliche Plomben von Gold.", "tokens": ["Tr\u00e4\u00b7ge", "Z\u00e4h\u00b7ne", "schnap\u00b7pen", "im", "Rauch", ".", "Da", "schwe\u00b7ben", "un\u00b7er\u00b7schwing\u00b7li\u00b7che", "Plom\u00b7ben", "von", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "APPRART", "NN", "$.", "ADV", "VVFIN", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+--+-+-+-+--+--+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Es riecht nach Zwiebeln. Ich habe in allen Weibern gesteckt.", "tokens": ["Es", "riecht", "nach", "Zwie\u00b7beln", ".", "Ich", "ha\u00b7be", "in", "al\u00b7len", "Wei\u00b7bern", "ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "PPER", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wo ist die Stra\u00dfe hin? Ich stehe im dunklen Gang.", "tokens": ["Wo", "ist", "die", "Stra\u00b7\u00dfe", "hin", "?", "Ich", "ste\u00b7he", "im", "dunk\u00b7len", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein schwarzes Loch weicht auf. Das Licht dr\u00fcben ist d\u00fcnn und verfleckt.", "tokens": ["Ein", "schwar\u00b7zes", "Loch", "weicht", "auf", ".", "Das", "Licht", "dr\u00fc\u00b7ben", "ist", "d\u00fcnn", "und", "ver\u00b7fleckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$.", "ART", "NN", "ADV", "VAFIN", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+---+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Mir ist kalt. Die Scheibe ist hoch und dick. Alles dauert so lang.", "tokens": ["Mir", "ist", "kalt", ".", "Die", "Schei\u00b7be", "ist", "hoch", "und", "dick", ".", "Al\u00b7les", "dau\u00b7ert", "so", "lang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$.", "PIS", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+--+-+--+--+", "measure": "trochaic.septa.relaxed"}}, "stanza.5": {"line.1": {"text": "Ein heller Glaskreis spritzt Licht, rund um mich sind gl\u00e4nzende Scheiben.", "tokens": ["Ein", "hel\u00b7ler", "Glas\u00b7kreis", "spritzt", "Licht", ",", "rund", "um", "mich", "sind", "gl\u00e4n\u00b7zen\u00b7de", "Schei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$,", "ADJD", "APPR", "PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Unter den Tischen dr\u00e4ngen Schuhe und R\u00f6cke und verflie\u00dfen am Rand.", "tokens": ["Un\u00b7ter", "den", "Ti\u00b7schen", "dr\u00e4n\u00b7gen", "Schu\u00b7he", "und", "R\u00f6\u00b7cke", "und", "ver\u00b7flie\u00b7\u00dfen", "am", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "KON", "NN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+-+-+--+-+-+--+", "measure": "iambic.octa.plus.invert"}, "line.3": {"text": "Alle Leute sind klein wie ich. Ich strecke mich hoch. Wo soll ich bleiben?", "tokens": ["Al\u00b7le", "Leu\u00b7te", "sind", "klein", "wie", "ich", ".", "Ich", "stre\u00b7cke", "mich", "hoch", ".", "Wo", "soll", "ich", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "KOKOM", "PPER", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$.", "PWAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "--+--+-+-+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wo sind meine F\u00fc\u00dfe? Die Menschen sind ruhig. Finde ich meine Hand?", "tokens": ["Wo", "sind", "mei\u00b7ne", "F\u00fc\u00b7\u00dfe", "?", "Die", "Men\u00b7schen", "sind", "ru\u00b7hig", ".", "Fin\u00b7de", "ich", "mei\u00b7ne", "Hand", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "$.", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "----+--+--+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Goldene N\u00e4gel flitzen hin und her durch die Nacht.", "tokens": ["Gol\u00b7de\u00b7ne", "N\u00e4\u00b7gel", "flit\u00b7zen", "hin", "und", "her", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die Menschen im Caf\u00e9 sitzen faul und dick.", "tokens": ["Die", "Men\u00b7schen", "im", "Ca\u00b7f\u00e9", "sit\u00b7zen", "faul", "und", "dick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es ist hei\u00df. Mein Herz platzt. Mein Schuh kracht.", "tokens": ["Es", "ist", "hei\u00df", ".", "Mein", "Herz", "platzt", ".", "Mein", "Schuh", "kracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "PPOSAT", "NN", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Ich stehe fest. Hinter dieser Scheibe kann ich sie wie im Regen sehn.", "tokens": ["Ich", "ste\u00b7he", "fest", ".", "Hin\u00b7ter", "die\u00b7ser", "Schei\u00b7be", "kann", "ich", "sie", "wie", "im", "Re\u00b7gen", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "APPR", "PDAT", "NN", "VMFIN", "PPER", "PPER", "KOKOM", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Ich atme aufs Glas. Meine H\u00e4nde sind fort. R\u00e4der fahren vorbei.", "tokens": ["Ich", "at\u00b7me", "aufs", "Glas", ".", "Mei\u00b7ne", "H\u00e4n\u00b7de", "sind", "fort", ".", "R\u00e4\u00b7der", "fah\u00b7ren", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "PPOSAT", "NN", "VAFIN", "PTKVZ", "$.", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+-+-+-+--+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Aus der Eisenbahn sah ich am Wege Windm\u00fchlenfl\u00fcgel drehn.", "tokens": ["Aus", "der", "Ei\u00b7sen\u00b7bahn", "sah", "ich", "am", "We\u00b7ge", "Wind\u00b7m\u00fch\u00b7len\u00b7fl\u00fc\u00b7gel", "drehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Jetzt fliegt die Bombe. Schnell. Es ist noch still. Kein Schrei.", "tokens": ["Jetzt", "fliegt", "die", "Bom\u00b7be", ".", "Schnell", ".", "Es", "ist", "noch", "still", ".", "Kein", "Schrei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "AR