rd", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "APPR", "NN", "PTKVZ", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bald wird ein sanfter Geist den stillen Grund durchwehen,", "tokens": ["Bald", "wird", "ein", "sanf\u00b7ter", "Geist", "den", "stil\u00b7len", "Grund", "durch\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und seine Friedenskraft beleben Herz und Mut!", "tokens": ["Und", "sei\u00b7ne", "Frie\u00b7dens\u00b7kraft", "be\u00b7le\u00b7ben", "Herz", "und", "Mut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gott ", "tokens": ["Gott"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Da sinkt der Geist oft gar in Tod und Ohnmacht hin;", "tokens": ["Da", "sinkt", "der", "Geist", "oft", "gar", "in", "Tod", "und", "Ohn\u00b7macht", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gott ", "tokens": ["Gott"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Sein Geist, der richtet auf den bl\u00f6den Glaubenssinn.", "tokens": ["Sein", "Geist", ",", "der", "rich\u00b7tet", "auf", "den", "bl\u00f6\u00b7den", "Glau\u00b7bens\u00b7sinn", "."], "token_info