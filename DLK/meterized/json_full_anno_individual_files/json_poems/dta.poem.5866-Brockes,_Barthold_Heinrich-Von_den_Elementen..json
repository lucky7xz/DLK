{"dta.poem.5866": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich glaube gleichwol doch von Geistern, die so klug,", "tokens": ["Ich", "glau\u00b7be", "gleich\u00b7wol", "doch", "von", "Geis\u00b7tern", ",", "die", "so", "klug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df jeder seinen Satz, mit gutem Grund und Fug,", "tokens": ["Da\u00df", "je\u00b7der", "sei\u00b7nen", "Satz", ",", "mit", "gu\u00b7tem", "Grund", "und", "Fug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vertheidiget und unterst\u00fctzet;", "tokens": ["Ver\u00b7thei\u00b7di\u00b7get", "und", "un\u00b7ter\u00b7st\u00fct\u00b7zet", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und da\u00df die so gelehrten Lehren,", "tokens": ["Und", "da\u00df", "die", "so", "ge\u00b7lehr\u00b7ten", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wol etwan zu vereinen w\u00e4ren;", "tokens": ["Wol", "et\u00b7wan", "zu", "ver\u00b7ei\u00b7nen", "w\u00e4\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Wenn man nur wie man billig sollte,", "tokens": ["Wenn", "man", "nur", "wie", "man", "bil\u00b7lig", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "KOKOM", "PIS", "ADJD", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Doppel-Deutigkeit der W\u00f6rter \u00e4ndern wollte.", "tokens": ["Die", "Dop\u00b7pel\u00b7Deu\u00b7tig\u00b7keit", "der", "W\u00f6r\u00b7ter", "\u00e4n\u00b7dern", "woll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es sey die Lufft, es sey die Gluht,", "tokens": ["Es", "sey", "die", "Lufft", ",", "es", "sey", "die", "Gluht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Es sey die Erd\u2019, es sey die Fluht,", "tokens": ["Es", "sey", "die", "Erd'", ",", "es", "sey", "die", "Fluht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So man zum Grunde setzt der Welt, und aller Sachen;", "tokens": ["So", "man", "zum", "Grun\u00b7de", "setzt", "der", "Welt", ",", "und", "al\u00b7ler", "Sa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPRART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie sind Materie, aus welcher sie zu machen.", "tokens": ["Sie", "sind", "Ma\u00b7te\u00b7rie", ",", "aus", "wel\u00b7cher", "sie", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "APPR", "PRELS", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "Es kommt darauf nur an, mit Sorgfalt nachzusehn,", "tokens": ["Es", "kommt", "da\u00b7rauf", "nur", "an", ",", "mit", "Sorg\u00b7falt", "nach\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ADV", "PTKVZ", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie eigentlich das, was sie Elementen", "tokens": ["Wie", "ei\u00b7gent\u00b7lich", "das", ",", "was", "sie", "E\u00b7le\u00b7men\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "PDS", "$,", "PRELS", "PPER", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.14": {"text": "In ihren Lehren nennten,", "tokens": ["In", "ih\u00b7ren", "Leh\u00b7ren", "nenn\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Und so verschiedentlich vermischten; zu verstehn.", "tokens": ["Und", "so", "ver\u00b7schie\u00b7dent\u00b7lich", "ver\u00b7mischten", ";", "zu", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "$.", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "Man nehm in seiner Lehr", "tokens": ["Man", "nehm", "in", "sei\u00b7ner", "Lehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Von Elementen, eins, man nehme mehr;", "tokens": ["Von", "E\u00b7le\u00b7men\u00b7ten", ",", "eins", ",", "man", "neh\u00b7me", "mehr", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PIS", "$,", "PIS", "VVFIN", "ADV", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "So kan man, wie vorhin, durch k\u00fcnsteln, drehen, dichten,", "tokens": ["So", "kan", "man", ",", "wie", "vor\u00b7hin", ",", "durch", "k\u00fcns\u00b7teln", ",", "dre\u00b7hen", ",", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "$,", "PWAV", "ADV", "$,", "APPR", "VVINF", "$,", "VVINF", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein neues Lehr-Geb\u00e4u daraus errichten.", "tokens": ["Ein", "neu\u00b7es", "Lehr\u00b7Ge\u00b7b\u00e4u", "da\u00b7raus", "er\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}