{"textgrid.poem.65693": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der schw\u00fcle Sommer ist verschwunden.", "genre": "verse", "period": "N.A.", "pub_year": 1715, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der schw\u00fcle Sommer ist verschwunden.", "tokens": ["Der", "schw\u00fc\u00b7le", "Som\u00b7mer", "ist", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonne la\u00fcfft der k\u00fchlen Wage zu.", "tokens": ["Die", "Son\u00b7ne", "la\u00fcfft", "der", "k\u00fch\u00b7len", "Wa\u00b7ge", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Erde neiget sich zur Ruh", "tokens": ["Die", "Er\u00b7de", "nei\u00b7get", "sich", "zur", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach ihren arbeitsvollen Stunden.", "tokens": ["Nach", "ih\u00b7ren", "ar\u00b7beits\u00b7vol\u00b7len", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr bunter Schmuck wird bl\u00f6d und alt,", "tokens": ["Ihr", "bun\u00b7ter", "Schmuck", "wird", "bl\u00f6d", "und", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und, was sich n\u00e4chst im Flor befunden;", "tokens": ["Und", ",", "was", "sich", "n\u00e4chst", "im", "Flor", "be\u00b7fun\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "PRF", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ver\u00e4ndert Farben und Gestalt.", "tokens": ["Ver\u00b7\u00e4n\u00b7dert", "Far\u00b7ben", "und", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Himmel tr\u00fcbet sich. Es haucht ein frischer Duft", "tokens": ["Der", "Him\u00b7mel", "tr\u00fc\u00b7bet", "sich", ".", "Es", "haucht", "ein", "fri\u00b7scher", "Duft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gleich einer k\u00fchlen Abendluft,", "tokens": ["Gleich", "ei\u00b7ner", "k\u00fch\u00b7len", "A\u00b7ben\u00b7dluft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und will des Jahres Abend k\u00fchlen.", "tokens": ["Und", "will", "des", "Jah\u00b7res", "A\u00b7bend", "k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der Baume Zierat weicht; die leichten Winde spielen", "tokens": ["Der", "Bau\u00b7me", "Zie\u00b7rat", "weicht", ";", "die", "leich\u00b7ten", "Win\u00b7de", "spie\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$.", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit dem entlaubten Schmuck. O welch ein Unbestand!", "tokens": ["Mit", "dem", "ent\u00b7laub\u00b7ten", "Schmuck", ".", "O", "welch", "ein", "Un\u00b7be\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "NE", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch nein! ich kenne deine Hand,", "tokens": ["Doch", "nein", "!", "ich", "ken\u00b7ne", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du grosser Sch\u00f6pfer und Erhalter!", "tokens": ["Du", "gros\u00b7ser", "Sch\u00f6p\u00b7fer", "und", "Er\u00b7hal\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Laubes Schirm, die schattenvolle Wand,", "tokens": ["Des", "Lau\u00b7bes", "Schirm", ",", "die", "schat\u00b7ten\u00b7vol\u00b7le", "Wand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die ihrer Fr\u00fcchte zartes Alter", "tokens": ["Die", "ih\u00b7rer", "Fr\u00fcch\u00b7te", "zar\u00b7tes", "Al\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vor Hitz und Sturm in Sicherheit beschlo\u00df,", "tokens": ["Vor", "Hitz", "und", "Sturm", "in", "Si\u00b7cher\u00b7heit", "be\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Hat nun die tre\u00fce Hut vollendet,", "tokens": ["Hat", "nun", "die", "tre\u00b7\u00fce", "Hut", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da der verwahrte Schatz gezeitigt und geendet;", "tokens": ["Da", "der", "ver\u00b7wahr\u00b7te", "Schatz", "ge\u00b7zei\u00b7tigt", "und", "ge\u00b7en\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Drum f\u00e4llt sie weg, und stellt ihn frey und blo\u00df.", "tokens": ["Drum", "f\u00e4llt", "sie", "weg", ",", "und", "stellt", "ihn", "frey", "und", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "O reicher Schatz, den wir bewundern m\u00fcssen!", "tokens": ["O", "rei\u00b7cher", "Schatz", ",", "den", "wir", "be\u00b7wun\u00b7dern", "m\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$,", "PRELS", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Schau, wie die s\u00fcsse Last die schwanken Aeste be\u00fcgt!", "tokens": ["Schau", ",", "wie", "die", "s\u00fcs\u00b7se", "Last", "die", "schwan\u00b7ken", "A\u00b7es\u00b7te", "be\u00fcgt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Es scheint, als wollen sie die werte Muter k\u00fcssen,", "tokens": ["Es", "scheint", ",", "als", "wol\u00b7len", "sie", "die", "wer\u00b7te", "Mu\u00b7ter", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Muter, welche sie geze\u00fcgt.", "tokens": ["Die", "Mu\u00b7ter", ",", "wel\u00b7che", "sie", "ge\u00b7ze\u00fcgt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der Bl\u00e4tter Schmuck, der allgemach verfle\u00fcgt", "tokens": ["Der", "Bl\u00e4t\u00b7ter", "Schmuck", ",", "der", "all\u00b7ge\u00b7mach", "ver\u00b7fle\u00b7\u00fcgt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Erscheinet nun noch eins so pr\u00e4chtig.", "tokens": ["Er\u00b7schei\u00b7net", "nun", "noch", "eins", "so", "pr\u00e4ch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Die schlanke Rede steht an Frucht und Zierat tr\u00e4chtig.", "tokens": ["Die", "schlan\u00b7ke", "Re\u00b7de", "steht", "an", "Frucht", "und", "Zie\u00b7rat", "tr\u00e4ch\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Schau, wie sie ihre gr\u00fcne Pracht", "tokens": ["Schau", ",", "wie", "sie", "ih\u00b7re", "gr\u00fc\u00b7ne", "Pracht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Mit Gold und Purpur ausgesticket;", "tokens": ["Mit", "Gold", "und", "Pur\u00b7pur", "aus\u00b7ge\u00b7sti\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Wie sich ihr sterbend Laub zu guter Letzte schm\u00fccket,", "tokens": ["Wie", "sich", "ihr", "ster\u00b7bend", "Laub", "zu", "gu\u00b7ter", "Letz\u00b7te", "schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "ADJD", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und seinen Abschied herrlich macht.", "tokens": ["Und", "sei\u00b7nen", "Ab\u00b7schied", "herr\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie aber? welch betr\u00fcbtes Bild", "tokens": ["Wie", "a\u00b7ber", "?", "welch", "be\u00b7tr\u00fcb\u00b7tes", "Bild"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "$.", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erblick ich voller Scham und Schanden!", "tokens": ["Er\u00b7blick", "ich", "vol\u00b7ler", "Scham", "und", "Schan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich Armer ach! Mein Herbst ist auch vorhanden;", "tokens": ["Ich", "Ar\u00b7mer", "ach", "!", "Mein", "Herbst", "ist", "auch", "vor\u00b7han\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ITJ", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Sommer ist bereits erf\u00fcllt.", "tokens": ["Mein", "Som\u00b7mer", "ist", "be\u00b7reits", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie darf ich, H\u00f6chster! vor dir stehn,", "tokens": ["Wie", "darf", "ich", ",", "H\u00f6chs\u00b7ter", "!", "vor", "dir", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "$,", "NN", "$.", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und mein besch\u00e4mtes Haupt zu deinen Wolken strecken?", "tokens": ["Und", "mein", "be\u00b7sch\u00e4m\u00b7tes", "Haupt", "zu", "dei\u00b7nen", "Wol\u00b7ken", "stre\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich bin ein kahler Baum, gleich einer d\u00fcrren Hecken,", "tokens": ["Ich", "bin", "ein", "kah\u00b7ler", "Baum", ",", "gleich", "ei\u00b7ner", "d\u00fcr\u00b7ren", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von keinen Fr\u00fcchten reich, von keiner Zierat sch\u00f6n.", "tokens": ["Von", "kei\u00b7nen", "Fr\u00fcch\u00b7ten", "reich", ",", "von", "kei\u00b7ner", "Zie\u00b7rat", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$,", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O wehe mir! die Art der Rache blicket schon,", "tokens": ["O", "we\u00b7he", "mir", "!", "die", "Art", "der", "Ra\u00b7che", "bli\u00b7cket", "schon", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "ART", "NN", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und dra\u00fct mir schn\u00f6dem Holtz mit dem verdienten Lohn!", "tokens": ["Und", "dra\u00fct", "mir", "schn\u00f6\u00b7dem", "Holtz", "mit", "dem", "ver\u00b7dien\u00b7ten", "Lohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Erbarme dich! Erwecke meine Kraft,", "tokens": ["Er\u00b7bar\u00b7me", "dich", "!", "Er\u00b7we\u00b7cke", "mei\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Du Wesen voller Huld und Liebe;", "tokens": ["Du", "We\u00b7sen", "vol\u00b7ler", "Huld", "und", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und f\u00fclle mich mit ne\u00fcem Saft,", "tokens": ["Und", "f\u00fcl\u00b7le", "mich", "mit", "ne\u00b7\u00fcem", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.14": {"text": "Mit einem gnadenvollen Triebe,", "tokens": ["Mit", "ei\u00b7nem", "gna\u00b7den\u00b7vol\u00b7len", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Eh mich dein Grimm zur Straf und Flamme rafft!", "tokens": ["Eh", "mich", "dein", "Grimm", "zur", "Straf", "und", "Flam\u00b7me", "rafft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NE", "APPRART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Herr, la\u00df mich noch in dieser Zeit,", "tokens": ["Herr", ",", "la\u00df", "mich", "noch", "in", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Obgleich mit sp\u00e4ter Frucht, zu deinem Ruhme dienen!", "tokens": ["Ob\u00b7gleich", "mit", "sp\u00e4\u00b7ter", "Frucht", ",", "zu", "dei\u00b7nem", "Ruh\u00b7me", "