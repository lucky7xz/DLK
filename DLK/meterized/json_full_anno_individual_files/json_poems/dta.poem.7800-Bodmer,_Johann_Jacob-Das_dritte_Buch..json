{"dta.poem.7800": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Das dritte Buch.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gelobt sey unser Gott, sein Nahme sey gepriesen,", "tokens": ["Ge\u00b7lobt", "sey", "un\u00b7ser", "Gott", ",", "sein", "Nah\u00b7me", "sey", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der solche Wunderg\u00fct an seinem Volck erwiesen.", "tokens": ["Der", "sol\u00b7che", "Wun\u00b7der\u00b7g\u00fct", "an", "sei\u00b7nem", "Volck", "er\u00b7wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gelobet sey der Herr, der Sch\u00fctzer Jsrael", "tokens": ["Ge\u00b7lo\u00b7bet", "sey", "der", "Herr", ",", "der", "Sch\u00fct\u00b7zer", "Js\u00b7rael"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der uns zieht aus dem Staub, der unsrer Freude Quell.", "tokens": ["Der", "uns", "zieht", "aus", "dem", "Staub", ",", "der", "uns\u00b7rer", "Freu\u00b7de", "Quell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Gelob", "tokens": ["Ge\u00b7lob"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Der unsern K\u00f6nig l\u00e4st sieghafft zu Hause kehren.", "tokens": ["Der", "un\u00b7sern", "K\u00f6\u00b7nig", "l\u00e4st", "sieg\u00b7hafft", "zu", "Hau\u00b7se", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gelobet sey die G\u00fct des Sch\u00f6pfers dieser Welt,", "tokens": ["Ge\u00b7lo\u00b7bet", "sey", "die", "G\u00fct", "des", "Sch\u00f6p\u00b7fers", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der unsre Feinde st\u00fcrtzt und unser Land erh\u00e4lt.", "tokens": ["Der", "uns\u00b7re", "Fein\u00b7de", "st\u00fcrtzt", "und", "un\u00b7ser", "Land", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Willkommen grosser Saul! was soll man von dir sagen:", "tokens": ["Will\u00b7kom\u00b7men", "gros\u00b7ser", "Saul", "!", "was", "soll", "man", "von", "dir", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$.", "PWS", "VMFIN", "PIS", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du hast dem frechen Feind selbst tausend Mann erschlagen.", "tokens": ["Du", "hast", "dem", "fre\u00b7chen", "Feind", "selbst", "tau\u00b7send", "Mann", "er\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "CARD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}