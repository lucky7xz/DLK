{"dta.poem.19550": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Ampel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "An des Jahres Wende sprach ich: Muse,", "tokens": ["An", "des", "Jah\u00b7res", "Wen\u00b7de", "sprach", "ich", ":", "Mu\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "PPER", "$.", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Keiner Mutter Hand bescheert mich! Gieb mir", "tokens": ["Kei\u00b7ner", "Mut\u00b7ter", "Hand", "be\u00b7scheert", "mich", "!", "Gieb", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "NN", "VVFIN", "PPER", "$.", "VVIMP", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Du mein Angebinde, Muse! fleht' ich.", "tokens": ["Du", "mein", "An\u00b7ge\u00b7bin\u00b7de", ",", "Mu\u00b7se", "!", "fleht'", "ich", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In die Kammer lauschend von dem Lager,", "tokens": ["In", "die", "Kam\u00b7mer", "lau\u00b7schend", "von", "dem", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sah ich bald der Schwestern eine schreiten.", "tokens": ["Sah", "ich", "bald", "der", "Schwes\u00b7tern", "ei\u00b7ne", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Auf mein Tischchen setzt' sie einer Ampel", "tokens": ["Auf", "mein", "Tischchen", "setzt'", "sie", "ei\u00b7ner", "Am\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zarte Form mit schlankgeschweiften Henkeln,", "tokens": ["Zar\u00b7te", "Form", "mit", "schlank\u00b7ge\u00b7schweif\u00b7ten", "Hen\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Aber die mir keineswegs antik schien.", "tokens": ["A\u00b7ber", "die", "mir", "kei\u00b7nes\u00b7wegs", "an\u00b7tik", "schien", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Ich erschrack. Was meinst Du, Muse? R\u00e4thst Du", "tokens": ["Ich", "er\u00b7schrack", ".", "Was", "meinst", "Du", ",", "Mu\u00b7se", "?", "R\u00e4thst", "Du"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PWS", "VVFIN", "PPER", "$,", "NN", "$.", "VVFIN", "PPER"], "meter": "+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.10": {"text": "N\u00e4chtlich auszufeilen meine Verse?", "tokens": ["N\u00e4cht\u00b7lich", "aus\u00b7zu\u00b7fei\u00b7len", "mei\u00b7ne", "Ver\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Schon entschwebend wandte sie das Antlitz", "tokens": ["Schon", "ent\u00b7schwe\u00b7bend", "wand\u00b7te", "sie", "das", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Halb. Ich sah des Musenhauptes edeln", "tokens": ["Halb", ".", "Ich", "sah", "des", "Mu\u00b7sen\u00b7haup\u00b7tes", "e\u00b7deln"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$.", "PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Umri\u00df mit den spottend feinen Lippen ...", "tokens": ["Um\u00b7ri\u00df", "mit", "den", "spot\u00b7tend", "fei\u00b7nen", "Lip\u00b7pen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJD", "ADJA", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Als ich dann in neuem Jahr erwachte,", "tokens": ["Als", "ich", "dann", "in", "neu\u00b7em", "Jahr", "er\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Keine Ampel! Doch ich fand sie wieder \u2014", "tokens": ["Kei\u00b7ne", "Am\u00b7pel", "!", "Doch", "ich", "fand", "sie", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "KON", "PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Und erkannte gleich sie an der zarten", "tokens": ["Und", "er\u00b7kann\u00b7te", "gleich", "sie", "an", "der", "zar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPER", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Form und an den schlankgeschweiften Henkeln \u2014", "tokens": ["Form", "und", "an", "den", "schlank\u00b7ge\u00b7schweif\u00b7ten", "Hen\u00b7keln"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "In des Liebchens Hand, das mir die Treppe", "tokens": ["In", "des", "Lieb\u00b7chens", "Hand", ",", "das", "mir", "die", "Trep\u00b7pe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "N\u00e4chtlich hellt mit stillen Ampelstrahlen.", "tokens": ["N\u00e4cht\u00b7lich", "hellt", "mit", "stil\u00b7len", "Am\u00b7pel\u00b7strah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Scheidend auf die letzte Stufe setzt' sie", "tokens": ["Schei\u00b7dend", "auf", "die", "letz\u00b7te", "Stu\u00b7fe", "setzt'", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Das Geschenk der Muse sacht und k\u00fc\u00dft' mich.", "tokens": ["Das", "Ge\u00b7schenk", "der", "Mu\u00b7se", "sacht", "und", "k\u00fc\u00dft'", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}