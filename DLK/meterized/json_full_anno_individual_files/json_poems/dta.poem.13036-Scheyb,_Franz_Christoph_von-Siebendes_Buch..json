{"dta.poem.13036": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Etwelche stunden dort, liebkosten ihm so sehr,", "tokens": ["Et\u00b7wel\u00b7che", "stun\u00b7den", "dort", ",", "lieb\u00b7kos\u00b7ten", "ihm", "so", "sehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "ADV", "$,", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wann er nicht nur Rath, auch selbst ihr Vater w\u00e4r:", "tokens": ["Als", "wann", "er", "nicht", "nur", "Rath", ",", "auch", "selbst", "ihr", "Va\u00b7ter", "w\u00e4r", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPER", "PTKNEG", "ADV", "NN", "$,", "ADV", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Fast jede drang sich hin und zeigte Rangs-Begierde,", "tokens": ["Fast", "je\u00b7de", "drang", "sich", "hin", "und", "zeig\u00b7te", "Rangs\u00b7Be\u00b7gier\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gleich als ob jene mehr, als die gesch\u00e4zet w\u00fcrde.", "tokens": ["Gleich", "als", "ob", "je\u00b7ne", "mehr", ",", "als", "die", "ge\u00b7sch\u00e4\u00b7zet", "w\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "KOUS", "PDS", "ADV", "$,", "KOUS", "ART", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Indem nun die\u00df Gespr\u00e4ch sich allgemach verlohr,", "tokens": ["In\u00b7dem", "nun", "die\u00df", "Ge\u00b7spr\u00e4ch", "sich", "all\u00b7ge\u00b7mach", "ver\u00b7lohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDS", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "250So trat er zu dem Krei\u00df, um laut zu reden, vor.", "tokens": ["trat", "er", "zu", "dem", "Krei\u00df", ",", "um", "laut", "zu", "re\u00b7den", ",", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KOUI", "ADJD", "PTKZU", "VVINF", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Er spielte mit der Hand an seiner goldnen Kette,", "tokens": ["Er", "spiel\u00b7te", "mit", "der", "Hand", "an", "sei\u00b7ner", "gold\u00b7nen", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als er noch um sich sah, und endlich also redte:", "tokens": ["Als", "er", "noch", "um", "sich", "sah", ",", "und", "end\u00b7lich", "al\u00b7so", "red\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PRF", "VVFIN", "$,", "KON", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}