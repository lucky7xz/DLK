{"dta.poem.21282": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Iv .  \n  Die Sch\u00f6nheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1752", "urn": "urn:nbn:de:kobv:b4-200905199117", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie lieblich ist des heitern Himmels Wonne,", "tokens": ["Wie", "lieb\u00b7lich", "ist", "des", "hei\u00b7tern", "Him\u00b7mels", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der reine Mond, der hellen Sterne Heer,", "tokens": ["Der", "rei\u00b7ne", "Mond", ",", "der", "hel\u00b7len", "Ster\u00b7ne", "Heer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aurorens Licht, der Glanz der g\u00fcldnen Sonne!", "tokens": ["Au\u00b7ro\u00b7rens", "Licht", ",", "der", "Glanz", "der", "g\u00fcld\u00b7nen", "Son\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und doch ergetzt ein sch\u00f6n Gesicht weit mehr.", "tokens": ["Und", "doch", "er\u00b7getzt", "ein", "sch\u00f6n", "Ge\u00b7sicht", "weit", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJD", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Tropfen Kraft, die Wald und Feld verj\u00fcngen,", "tokens": ["Der", "Trop\u00b7fen", "Kraft", ",", "die", "Wald", "und", "Feld", "ver\u00b7j\u00fcn\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Belebt sie kaum, wie uns ein froher Ku\u00df,", "tokens": ["Be\u00b7lebt", "sie", "kaum", ",", "wie", "uns", "ein", "fro\u00b7her", "Ku\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und nimmer kann ein Vogel s\u00fcsser singen,", "tokens": ["Und", "nim\u00b7mer", "kann", "ein", "Vo\u00b7gel", "s\u00fcs\u00b7ser", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als uns ein Mund, den man verehren mu\u00df.", "tokens": ["Als", "uns", "ein", "Mund", ",", "den", "man", "ver\u00b7eh\u00b7ren", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Eleonor! auf Deren zarten Wangen", "tokens": ["E\u00b7leo\u00b7nor", "!", "auf", "De\u00b7ren", "zar\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Jugend Bl\u00fcht in frischen Rosen lacht,", "tokens": ["Der", "Ju\u00b7gend", "Bl\u00fcht", "in", "fri\u00b7schen", "Ro\u00b7sen", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Z\u00e4rtlichkeit, Bewundrung und Verlangen", "tokens": ["Und", "Z\u00e4rt\u00b7lich\u00b7keit", ",", "Be\u00b7wund\u00b7rung", "und", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dir, und nur Dir so zeitig eigen macht;", "tokens": ["Dir", ",", "und", "nur", "Dir", "so", "zei\u00b7tig", "ei\u00b7gen", "macht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KON", "ADV", "PPER", "ADV", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ob Psyche gleich die Liebe selbst regierte,", "tokens": ["Ob", "Psy\u00b7che", "gleich", "die", "Lie\u00b7be", "selbst", "re\u00b7gier\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als sie, mit Recht, des Gottes G\u00f6ttinn hie\u00df;", "tokens": ["Als", "sie", ",", "mit", "Recht", ",", "des", "Got\u00b7tes", "G\u00f6t\u00b7tinn", "hie\u00df", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So glaub ich doch, da\u00df ihn nichts sch\u00f6ners r\u00fchrte,", "tokens": ["So", "glaub", "ich", "doch", ",", "da\u00df", "ihn", "nichts", "sch\u00f6\u00b7ners", "r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als die Natur in Deiner Bildung wies.", "tokens": ["Als", "die", "Na\u00b7tur", "in", "Dei\u00b7ner", "Bil\u00b7dung", "wies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dein Auge spielt und Deine Locken fliegen", "tokens": ["Dein", "Au\u00b7ge", "spielt", "und", "Dei\u00b7ne", "Lo\u00b7cken", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sanft, wie die Luft im Strahl der Sonne wallt;", "tokens": ["Sanft", ",", "wie", "die", "Luft", "im", "Strahl", "der", "Son\u00b7ne", "wallt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Gef\u00e4lligkeit und Anmuth und Vergn\u00fcgen", "tokens": ["Ge\u00b7f\u00e4l\u00b7lig\u00b7keit", "und", "An\u00b7muth", "und", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind ungetrennt von Deinem Aufenthalt.", "tokens": ["Sind", "un\u00b7ge\u00b7trennt", "von", "Dei\u00b7nem", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dir huldigen die Herzen muntrer Jugend,", "tokens": ["Dir", "hul\u00b7di\u00b7gen", "die", "Her\u00b7zen", "mun\u00b7trer", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das Alter selbst beneidet deinen Witz.", "tokens": ["Das", "Al\u00b7ter", "selbst", "be\u00b7nei\u00b7det", "dei\u00b7nen", "Witz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es wird, in Dir, der angenehmsten Tugend,", "tokens": ["Es", "wird", ",", "in", "Dir", ",", "der", "an\u00b7ge\u00b7nehms\u00b7ten", "Tu\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und nirgend sonst der angenehmste Sitz.", "tokens": ["Und", "nir\u00b7gend", "sonst", "der", "an\u00b7ge\u00b7nehms\u00b7te", "Sitz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Man schmeichelt mir, da\u00df, in zufriednen Stunden,", "tokens": ["Man", "schmei\u00b7chelt", "mir", ",", "da\u00df", ",", "in", "zu\u00b7fried\u00b7nen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "KOUS", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eleonor auch meine Lieder singt,", "tokens": ["E\u00b7leo\u00b7nor", "auch", "mei\u00b7ne", "Lie\u00b7der", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und manches Wort, das viele nicht empfunden,", "tokens": ["Und", "man\u00b7ches", "Wort", ",", "das", "vie\u00b7le", "nicht", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PIS", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch Jhre Stimm\u2019 in aller Herzen dringt.", "tokens": ["Durch", "Ih\u00b7re", "Stimm'", "in", "al\u00b7ler", "Her\u00b7zen", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gew\u00e4hre mir, den Dichter zu begl\u00fccken,", "tokens": ["Ge\u00b7w\u00e4h\u00b7re", "mir", ",", "den", "Dich\u00b7ter", "zu", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der edler nichts als Deinen Beyfall fand,", "tokens": ["Der", "ed\u00b7ler", "nichts", "als", "Dei\u00b7nen", "Bey\u00b7fall", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIS", "KOKOM", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nur einen Blick von Deinen sch\u00f6nen Blicken,", "tokens": ["Nur", "ei\u00b7nen", "Blick", "von", "Dei\u00b7nen", "sch\u00f6\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nur einen Ku\u00df auf Deine weisse Hand.", "tokens": ["Nur", "ei\u00b7nen", "Ku\u00df", "auf", "Dei\u00b7ne", "weis\u00b7se", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}