Und", "doch", ",", "wie", "hier", "vor", "mei\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das eigne Werk sich neu enth\u00fcllt,", "tokens": ["Das", "eig\u00b7ne", "Werk", "sich", "neu", "ent\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich selber will es nicht erquicken,", "tokens": ["Mich", "sel\u00b7ber", "will", "es", "nicht", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und fast wie Scham ist, was mich f\u00fcllt.", "tokens": ["Und", "fast", "wie", "Scham", "ist", ",", "was", "mich", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "NN", "VAFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob nichts am hohen Gleichma\u00df fehle,", "tokens": ["Ob", "nichts", "am", "ho\u00b7hen", "Gleich\u00b7ma\u00df", "feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ob jedem Sinn genug getan:", "tokens": ["Ob", "je\u00b7dem", "Sinn", "ge\u00b7nug", "ge\u00b7tan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein Schauer quillt in meine Seele,", "tokens": ["Kein", "Schau\u00b7er", "quillt", "in", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Kein Unnennbares r\u00fchrt mich an.", "tokens": ["Kein", "Un\u00b7nenn\u00b7ba\u00b7res", "r\u00fchrt", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "O Fluch, dem diese Zeit verfallen,", "tokens": ["O", "Fluch", ",", "dem", "die\u00b7se", "Zeit", "ver\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sie kein gro\u00dfer Puls durchbebt,", "tokens": ["Da\u00df", "sie", "kein", "gro\u00b7\u00dfer", "Puls", "durch\u00b7bebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Sehnen, das, geteilt von allen,", "tokens": ["Kein", "Seh\u00b7nen", ",", "das", ",", "ge\u00b7teilt", "von", "al\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PDS", "$,", "VVPP", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im K\u00fcnstler nach Gestaltung strebt,", "tokens": ["Im", "K\u00fcnst\u00b7ler", "nach", "Ge\u00b7stal\u00b7tung", "strebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das ihm nicht Rast g\u00f6nnt, bis er's endlich", "tokens": ["Das", "ihm", "nicht", "Rast", "g\u00f6nnt", ",", "bis", "er's", "end\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "PTKNEG", "NN", "VVFIN", "$,", "KOUS", "PIS", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bew\u00e4ltigt in den Marmor fl\u00f6\u00dft", "tokens": ["Be\u00b7w\u00e4l\u00b7tigt", "in", "den", "Mar\u00b7mor", "fl\u00f6\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und so in Sch\u00f6nheit allverst\u00e4ndlich", "tokens": ["Und", "so", "in", "Sch\u00f6n\u00b7heit", "all\u00b7ver\u00b7st\u00e4nd\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das R\u00e4tsel seiner Tage l\u00f6st!", "tokens": ["Das", "R\u00e4t\u00b7sel", "sei\u00b7ner", "Ta\u00b7ge", "l\u00f6st", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Wohl b\u00e4nd'gen wir den Stein und k\u00fcren,", "tokens": ["Wohl", "b\u00e4n\u00b7d'\u00b7gen", "wir", "den", "Stein", "und", "k\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KON", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bewu\u00dft berechnend, jede Zier,", "tokens": ["Be\u00b7wu\u00dft", "be\u00b7rech\u00b7nend", ",", "je\u00b7de", "Zier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch, wie wir glatt den Mei\u00dfel f\u00fchren,", "tokens": ["Doch", ",", "wie", "wir", "glatt", "den", "Mei\u00b7\u00dfel", "f\u00fch\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur vom Vergangnen zehren wir.", "tokens": ["Nur", "vom", "Ver\u00b7gang\u00b7nen", "zeh\u00b7ren", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O trostlos kluges Auserlesen,", "tokens": ["O", "trost\u00b7los", "klu\u00b7ges", "Au\u00b7ser\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dabei kein Blitz die Brust durchz\u00fcckt!", "tokens": ["Da\u00b7bei", "kein", "Blitz", "die", "Brust", "durch\u00b7z\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was sch\u00f6n wird, ist schon dagewesen,", "tokens": ["Was", "sch\u00f6n", "wird", ",", "ist", "schon", "da\u00b7ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und nachgeahmt ist, was uns gl\u00fcckt.", "tokens": ["Und", "nach\u00b7ge\u00b7ahmt", "ist", ",", "was", "uns", "gl\u00fcckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Der Kreis der Formen liegt beschlossen,", "tokens": ["Der", "Kreis", "der", "For\u00b7men", "liegt", "be\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die einst der Griechen Geist beseelt;", "tokens": ["Die", "einst", "der", "Grie\u00b7chen", "Geist", "be\u00b7seelt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umsonst durchtasten wir verdrossen", "tokens": ["Um\u00b7sonst", "durch\u00b7tas\u00b7ten", "wir", "ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Leben, dem der Inhalt fehlt.", "tokens": ["Ein", "Le\u00b7ben", ",", "dem", "der", "In\u00b7halt", "fehlt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo lodert noch ein Opferfunken?", "tokens": ["Wo", "lo\u00b7dert", "noch", "ein", "Op\u00b7fer\u00b7fun\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo bl\u00fcht ein Fest noch, das nicht hohl?", "tokens": ["Wo", "bl\u00fcht", "ein", "Fest", "noch", ",", "das", "nicht", "hohl", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "$,", "PRELS", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Glaub' ist, ach, dahingesunken,", "tokens": ["Der", "Glaub'", "ist", ",", "ach", ",", "da\u00b7hin\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ITJ", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und toter Schmuck ward sein Symbol.", "tokens": ["Und", "to\u00b7ter", "Schmuck", "ward", "sein", "Sym\u00b7bol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Sieh her, noch braun sind diese Haare,", "tokens": ["Sieh", "her", ",", "noch", "braun", "sind", "die\u00b7se", "Haa\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ADV", "ADJD", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und nicht das Alter schuf mich bla\u00df;", "tokens": ["Und", "nicht", "das", "Al\u00b7ter", "schuf", "mich", "bla\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch g\u00e4b' ich alle meine Jahre", "tokens": ["Doch", "g\u00e4b'", "ich", "al\u00b7le", "mei\u00b7ne", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr ", "tokens": ["F\u00fcr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Nicht weil des Volks verstummend Gaffen,", "tokens": ["Nicht", "weil", "des", "Volks", "ver\u00b7stum\u00b7mend", "Gaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "ART", "NN", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Welt Bewundrung ihm gelohnt;", "tokens": ["Der", "Welt", "Be\u00b7wund\u00b7rung", "ihm", "ge\u00b7lohnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nein, weil der Zeus, den er geschaffen,", "tokens": ["Nein", ",", "weil", "der", "Zeus", ",", "den", "er", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "ART", "NE", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihm selbst, ein Gott, im Sinn gethront.", "tokens": ["Ihm", "selbst", ",", "ein", "Gott", ",", "im", "Sinn", "ge\u00b7thront", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ART", "NN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Das war sein Stern, das war sein Segen,", "tokens": ["Das", "war", "sein", "Stern", ",", "das", "war", "sein", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ihn mit ungebrochnem Flug", "tokens": ["Da\u00df", "ihn", "mit", "un\u00b7ge\u00b7broch\u00b7nem", "Flug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der h\u00f6chsten Urgestalt entgegen", "tokens": ["Der", "h\u00f6chs\u00b7ten", "Ur\u00b7ge\u00b7stalt", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Andacht heil'ger Fittich trug.", "tokens": ["Der", "An\u00b7dacht", "heil'\u00b7ger", "Fit\u00b7tich", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er durft' im Reigen der Erkornen", "tokens": ["Er", "durft'", "im", "Rei\u00b7gen", "der", "Er\u00b7kor\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Voll Glanz noch den Olympos sehn,", "tokens": ["Voll", "Glanz", "noch", "den", "O\u00b7lym\u00b7pos", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Indes wir armen Nachgebornen", "tokens": ["In\u00b7des", "wir", "ar\u00b7men", "Nach\u00b7ge\u00b7bor\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In g\u00f6tterloser W\u00fcste stehn.", "tokens": ["In", "g\u00f6t\u00b7ter\u00b7lo\u00b7ser", "W\u00fcs\u00b7te", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Da uns der Himmel ward entrissen,", "tokens": ["Da", "uns", "der", "Him\u00b7mel", "ward", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schwand auch des Schaffens himmlisch Gl\u00fcck;", "tokens": ["Schwand", "auch", "des", "Schaf\u00b7fens", "himm\u00b7lisch", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADJD", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wohl wissen wir's, doch alles Wissen", "tokens": ["Wohl", "wis\u00b7sen", "wir's", ",", "doch", "al\u00b7les", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVINF", "VAFIN", "$,", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bringt das Verlorne nie zur\u00fcck.", "tokens": ["Bringt", "das", "Ver\u00b7lor\u00b7ne", "nie", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und keine neue Kunst mag werden,", "tokens": ["Und", "kei\u00b7ne", "neu\u00b7e", "Kunst", "mag", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis \u00fcber dieser Zeiten Gruft", "tokens": ["Bis", "\u00fc\u00b7ber", "die\u00b7ser", "Zei\u00b7ten", "Gruft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein neuer Gott erscheint auf Erden", "tokens": ["Ein", "neu\u00b7er", "Gott", "er\u00b7scheint", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und seine Priesterin beruft.", "tokens": ["Und", "sei\u00b7ne", "Pries\u00b7te\u00b7rin", "be\u00b7ruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}