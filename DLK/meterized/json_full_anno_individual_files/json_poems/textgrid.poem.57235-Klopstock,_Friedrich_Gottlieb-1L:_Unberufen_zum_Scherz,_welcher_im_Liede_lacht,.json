{"textgrid.poem.57235": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unberufen zum Scherz, welcher im Liede lacht,", "genre": "verse", "period": "N.A.", "pub_year": 1749, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unberufen zum Scherz, welcher im Liede lacht,", "tokens": ["Un\u00b7be\u00b7ru\u00b7fen", "zum", "Scherz", ",", "wel\u00b7cher", "im", "Lie\u00b7de", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Nicht gew\u00f6hnet zu sehn Knidia's G\u00f6tterchen,", "tokens": ["Nicht", "ge\u00b7w\u00f6h\u00b7net", "zu", "sehn", "Kni\u00b7dia's", "G\u00f6t\u00b7ter\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "PTKZU", "VVINF", "NE", "NE", "$,"], "meter": "+-+--++-+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wollt' ich Lieder, wie Schmidt singt,", "tokens": ["Wollt'", "ich", "Lie\u00b7der", ",", "wie", "Schmidt", "singt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "$,", "PWAV", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieder singen, wie Hagedorn.", "tokens": ["Lie\u00b7der", "sin\u00b7gen", ",", "wie", "Ha\u00b7ge\u00b7dorn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Schon glitt, z\u00e4rtliche Braut, meine verlorne Hand", "tokens": ["Schon", "glitt", ",", "z\u00e4rt\u00b7li\u00b7che", "Braut", ",", "mei\u00b7ne", "ver\u00b7lor\u00b7ne", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Nach Anakreons Spiel, rann es, wie Silberton", "tokens": ["Nach", "A\u00b7nak\u00b7re\u00b7ons", "Spiel", ",", "rann", "es", ",", "wie", "Sil\u00b7ber\u00b7ton"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "NN", "$,", "VVFIN", "PPER", "$,", "PWAV", "NE"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Durch die Saiten herunter,", "tokens": ["Durch", "die", "Sai\u00b7ten", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Vom hinfliegenden blonden Haar;", "tokens": ["Vom", "hin\u00b7flie\u00b7gen\u00b7den", "blon\u00b7den", "Haar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Von dem Kuss, der gerauht, halb nur empfunden wird,", "tokens": ["Von", "dem", "Kuss", ",", "der", "ge\u00b7rauht", ",", "halb", "nur", "emp\u00b7fun\u00b7den", "wird", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VVPP", "$,", "ADJD", "ADV", "VVPP", "VAFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Von der s\u00fcsseren Lust eines gegebenen;", "tokens": ["Von", "der", "s\u00fcs\u00b7se\u00b7ren", "Lust", "ei\u00b7nes", "ge\u00b7ge\u00b7be\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Von dem frohen Gelispel", "tokens": ["Von", "dem", "fro\u00b7hen", "Ge\u00b7lis\u00b7pel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Unter Freunden und Freundinnen,", "tokens": ["Un\u00b7ter", "Freun\u00b7den", "und", "Freun\u00b7din\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn die schnellre Musik in die Versamlung sich", "tokens": ["Wenn", "die", "schnell\u00b7re", "Mu\u00b7sik", "in", "die", "Ver\u00b7sam\u00b7lung", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ART", "NN", "PRF"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ungest\u00fcmer ergiesst, Fl\u00fcgel der T\u00e4nzer hat,", "tokens": ["Un\u00b7ge\u00b7st\u00fc\u00b7mer", "er\u00b7giesst", ",", "Fl\u00fc\u00b7gel", "der", "T\u00e4n\u00b7zer", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und das wildere M\u00e4dchen", "tokens": ["Und", "das", "wil\u00b7de\u00b7re", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Feuervoller vor\u00fcberrauscht;", "tokens": ["Feu\u00b7er\u00b7vol\u00b7ler", "vor\u00b7\u00fc\u00b7berr\u00b7auscht", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Von der bebenden Brust, welche sich sanft erhebt,", "tokens": ["Von", "der", "be\u00b7ben\u00b7den", "Brust", ",", "wel\u00b7che", "sich", "sanft", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Nicht gesehen, will seyn, aber gesehen wird:", "tokens": ["Nicht", "ge\u00b7se\u00b7hen", ",", "will", "seyn", ",", "a\u00b7ber", "ge\u00b7se\u00b7hen", "wird", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$,", "VMFIN", "VAINF", "$,", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und von allem, was sonst noch", "tokens": ["Und", "von", "al\u00b7lem", ",", "was", "sonst", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "$,", "PRELS", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die Lieder zur Freude lockt.", "tokens": ["Durch", "die", "Lie\u00b7der", "zur", "Freu\u00b7de", "lockt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Doch mit Blicken voll Ernst winket Urania,", "tokens": ["Doch", "mit", "Bli\u00b7cken", "voll", "Ernst", "win\u00b7ket", "U\u00b7ra\u00b7nia", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "NN", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Meine Muse, mir zu, gleich der unsterblichen,", "tokens": ["Mei\u00b7ne", "Mu\u00b7se", ",", "mir", "zu", ",", "gleich", "der", "uns\u00b7terb\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "PTKVZ", "$,", "ADV", "ART", "ADJA", "$,"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Tiefer denkenden Singer,", "tokens": ["Tie\u00b7fer", "den\u00b7ken\u00b7den", "Sin\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Oder, g\u00f6ttliche Fanny, dir!", "tokens": ["O\u00b7der", ",", "g\u00f6tt\u00b7li\u00b7che", "Fan\u00b7ny", ",", "dir", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Singe, sprach sie zu mir, was die Natur dich lehrt!", "tokens": ["Sin\u00b7ge", ",", "sprach", "sie", "zu", "mir", ",", "was", "die", "Na\u00b7tur", "dich", "lehrt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Jene Lieder hat dich nicht die Natur gelehrt;", "tokens": ["Je\u00b7ne", "Lie\u00b7der", "hat", "dich", "nicht", "die", "Na\u00b7tur", "ge\u00b7lehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Aber Freundschaft, und Tugend", "tokens": ["A\u00b7ber", "Freund\u00b7schaft", ",", "und", "Tu\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$,", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sollten deine Ges\u00e4nge seyn!", "tokens": ["Soll\u00b7ten", "dei\u00b7ne", "Ge\u00b7s\u00e4n\u00b7ge", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Also sprach sie, und stieg zu dem Olymp empor.", "tokens": ["Al\u00b7so", "sprach", "sie", ",", "und", "stieg", "zu", "dem", "O\u00b7lymp", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Aber darf auch ihr Ernst, bey dem Ger\u00e4usch der Lust,", "tokens": ["A\u00b7ber", "darf", "auch", "ihr", "Ernst", ",", "bey", "dem", "Ge\u00b7r\u00e4usch", "der", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Bey den bl\u00fchenden Minen,", "tokens": ["Bey", "den", "bl\u00fc\u00b7hen\u00b7den", "Mi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Leises Trittes vor\u00fcbergehn?", "tokens": ["Lei\u00b7ses", "Trit\u00b7tes", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Ja! du h\u00f6rest mich, Braut, und dein gebildet Herz", "tokens": ["Ja", "!", "du", "h\u00f6\u00b7rest", "mich", ",", "Braut", ",", "und", "dein", "ge\u00b7bil\u00b7det", "Herz"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "PPER", "$,", "NN", "$,", "KON", "PPOSAT", "VVPP", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Mischt zur Freude den Ernst, f\u00fchlt so die Freude mehr!", "tokens": ["Mischt", "zur", "Freu\u00b7de", "den", "Ernst", ",", "f\u00fchlt", "so", "die", "Freu\u00b7de", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Du verkennest das L\u00e4cheln", "tokens": ["Du", "ver\u00b7ken\u00b7nest", "das", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "In dem Auge der Tugend nicht!", "tokens": ["In", "dem", "Au\u00b7ge", "der", "Tu\u00b7gend", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Wenn die Lippe nicht mehr bl\u00fchet, die Wange nicht,", "tokens": ["Wenn", "die", "Lip\u00b7pe", "nicht", "mehr", "bl\u00fc\u00b7het", ",", "die", "Wan\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "$,", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wenn der sterbende Blick sich in die Nacht verliert,", "tokens": ["Wenn", "der", "ster\u00b7ben\u00b7de", "Blick", "sich", "in", "die", "Nacht", "ver\u00b7liert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wenn wir unsrer Verlangen", "tokens": ["Wenn", "wir", "uns\u00b7rer", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Thorheit weis' und verachtend sehn;", "tokens": ["Thor\u00b7heit", "weis'", "und", "ver\u00b7ach\u00b7tend", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADJD", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Wenn, wo sonst uns der Lenz auch zu der Blume rief,", "tokens": ["Wenn", ",", "wo", "sonst", "uns", "der", "Lenz", "auch", "zu", "der", "Blu\u00b7me", "rief", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "ADV", "PPER", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da, bey unserem Grab' Enkel und Enkelin,", "tokens": ["Da", ",", "bey", "un\u00b7se\u00b7rem", "Grab'", "En\u00b7kel", "und", "En\u00b7ke\u00b7lin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+----+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Uns vergessend, sich lieben:", "tokens": ["Uns", "ver\u00b7ges\u00b7send", ",", "sich", "lie\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "PRF", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dann ist, Freundin, die Tugend noch!", "tokens": ["Dann", "ist", ",", "Freun\u00b7din", ",", "die", "Tu\u00b7gend", "noch", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "NN", "$,", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Jene Tugend, die du kennst, und bescheiden thust,", "tokens": ["Je\u00b7ne", "Tu\u00b7gend", ",", "die", "du", "kennst", ",", "und", "be\u00b7schei\u00b7den", "thust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die den, welchen du liebst, neben dir gl\u00fccklich macht,", "tokens": ["Die", "den", ",", "wel\u00b7chen", "du", "liebst", ",", "ne\u00b7ben", "dir", "gl\u00fcck\u00b7lich", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "$,", "PWAT", "PPER", "VVFIN", "$,", "APPR", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Die dem Auge der Mutter", "tokens": ["Die", "dem", "Au\u00b7ge", "der", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Heimlich Thr\u00e4nen der Freud' entlockt.", "tokens": ["Heim\u00b7lich", "Thr\u00e4\u00b7nen", "der", "Freud'", "ent\u00b7lockt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.13": {"line.1": {"text": "Unberufen zum Scherz, welcher im Liede lacht,", "tokens": ["Un\u00b7be\u00b7ru\u00b7fen", "zum", "Scherz", ",", "wel\u00b7cher", "im