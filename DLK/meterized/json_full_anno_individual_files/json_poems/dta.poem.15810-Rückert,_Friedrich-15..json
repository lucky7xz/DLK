{"dta.poem.15810": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wem schenk' ich dieses Buch? Dir? Deinem Schwesterlein?", "tokens": ["Wem", "schenk'", "ich", "die\u00b7ses", "Buch", "?", "Dir", "?", "Dei\u00b7nem", "Schwes\u00b7ter\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PDAT", "NN", "$.", "PPER", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du bist daf\u00fcr zu gro\u00df, es ist daf\u00fcr zu klein.", "tokens": ["Du", "bist", "da\u00b7f\u00fcr", "zu", "gro\u00df", ",", "es", "ist", "da\u00b7f\u00fcr", "zu", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "PTKA", "ADJD", "$,", "PPER", "VAFIN", "PAV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Euch beiden schenk' ich es, da\u00df draus die kleine lerne,", "tokens": ["Euch", "bei\u00b7den", "schenk'", "ich", "es", ",", "da\u00df", "draus", "die", "klei\u00b7ne", "ler\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PAV", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was du, die gr\u00f6\u00dfere, sie lehrest leicht und gerne.", "tokens": ["Was", "du", ",", "die", "gr\u00f6\u00b7\u00dfe\u00b7re", ",", "sie", "leh\u00b7rest", "leicht", "und", "ger\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "ART", "ADJA", "$,", "PPER", "VVFIN", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Kinderschuhe zogst du selbst aus noch nicht lange,", "tokens": ["Die", "Kin\u00b7der\u00b7schu\u00b7he", "zogst", "du", "selbst", "aus", "noch", "nicht", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und kannst dich ohne M\u00fch bequemen ihrem Gange.", "tokens": ["Und", "kannst", "dich", "oh\u00b7ne", "M\u00fch", "be\u00b7que\u00b7men", "ih\u00b7rem", "Gan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "NN", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und eurer Mutter ist kein sch\u00f6nres Gl\u00fcck verliehn,", "tokens": ["Und", "eu\u00b7rer", "Mut\u00b7ter", "ist", "kein", "sch\u00f6n\u00b7res", "Gl\u00fcck", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wenn die Tochter hilft das T\u00f6chterchen erziehn.", "tokens": ["Als", "wenn", "die", "Toch\u00b7ter", "hilft", "das", "T\u00f6ch\u00b7ter\u00b7chen", "er\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}