: "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus ungef\u00e4rbter Pflicht ohn Nachtheil zu begeben,", "tokens": ["Aus", "un\u00b7ge\u00b7f\u00e4rb\u00b7ter", "Pflicht", "ohn", "Nacht\u00b7heil", "zu", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wirstu t\u00e4glich seyn der Freyheit liebster Gast,", "tokens": ["So", "wirs\u00b7tu", "t\u00e4g\u00b7lich", "seyn", "der", "Frey\u00b7heit", "liebs\u00b7ter", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "VAINF", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und kanst viel freyer noch als selbst dein K\u00f6nig leben.", "tokens": ["Und", "kanst", "viel", "frey\u00b7er", "noch", "als", "selbst", "dein", "K\u00f6\u00b7nig", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADJD", "ADV", "KOKOM", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du suchest Netz und Strick und w\u00fcnschest frey zu seyn,", "tokens": ["Du", "su\u00b7chest", "Netz", "und", "Strick", "und", "w\u00fcn\u00b7schest", "frey", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "KON", "VVFIN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Fall es ja dein Ernst wil ich dir Mittel sagen:", "tokens": ["Im", "Fall", "es", "ja", "dein", "Ernst", "wil", "ich", "dir", "Mit\u00b7tel", "sa\u00b7gen