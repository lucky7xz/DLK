{"textgrid.poem.52018": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du suchest Netz und Strick und w\u00fcnschest frey zu seyn,", "tokens": ["Du", "su\u00b7chest", "Netz", "und", "Strick", "und", "w\u00fcn\u00b7schest", "frey", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "KON", "VVFIN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Fall es ja dein Ernst wil ich dir Mittel sagen:", "tokens": ["Im", "Fall", "es", "ja", "dein", "Ernst", "wil", "ich", "dir", "Mit\u00b7tel", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "PPOSAT", "NN", "VMFIN", "PPER", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gib den Begierden nicht zu viel im Hertzen ein,", "tokens": ["Gib", "den", "Be\u00b7gier\u00b7den", "nicht", "zu", "viel", "im", "Hert\u00b7zen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKNEG", "PTKA", "PIS", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Die sonsten immer zu nach h\u00f6hern Aemtern fragen.", "tokens": ["Die", "sons\u00b7ten", "im\u00b7mer", "zu", "nach", "h\u00f6\u00b7hern", "A\u00b7em\u00b7tern", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "PTKZU", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Verschlag dir nicht die Gunst, die dir dein Herr erweist,", "tokens": ["Ver\u00b7schlag", "dir", "nicht", "die", "Gunst", ",", "die", "dir", "dein", "Herr", "er\u00b7weist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ART", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gehe n\u00fcchtern umb mit seinem Schlu\u00df und Willen:", "tokens": ["Und", "ge\u00b7he", "n\u00fcch\u00b7tern", "umb", "mit", "sei\u00b7nem", "Schlu\u00df", "und", "Wil\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00df den erkaufften Geitz, wie recht er es auch heist,", "tokens": ["La\u00df", "den", "er\u00b7kauff\u00b7ten", "Geitz", ",", "wie", "recht", "er", "es", "auch", "heist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$,", "PWAV", "ADJD", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An armer Leute Gutt nicht seinen Hunger stillen.", "tokens": ["An", "ar\u00b7mer", "Leu\u00b7te", "Gutt", "nicht", "sei\u00b7nen", "Hun\u00b7ger", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mach' nicht aus deinem Schlo\u00df ein Kauffhaus derer Waar,", "tokens": ["Mach'", "nicht", "aus", "dei\u00b7nem", "Schlo\u00df", "ein", "Kauff\u00b7haus", "de\u00b7rer", "Waar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "ART", "NN", "PDS", "NN", "$,"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die abzuw\u00e4gen ist in des Gewissens Schaalen:", "tokens": ["Die", "ab\u00b7zu\u00b7w\u00e4\u00b7gen", "ist", "in", "des", "Ge\u00b7wis\u00b7sens", "Schaa\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVIZU", "VAFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bewahre deinen Eyd, was b\u00f6ser Leute Schaar", "tokens": ["Be\u00b7wah\u00b7re", "dei\u00b7nen", "Eyd", ",", "was", "b\u00f6\u00b7ser", "Leu\u00b7te", "Schaar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PWS", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dir vor verdammten Rauch wil vor die Augen mahlen.", "tokens": ["Dir", "vor", "ver\u00b7damm\u00b7ten", "Rauch", "wil", "vor", "die", "Au\u00b7gen", "mah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Steig auch nicht gar zu hoch in deines Herren Gunst,", "tokens": ["Steig", "auch", "nicht", "gar", "zu", "hoch", "in", "dei\u00b7nes", "Her\u00b7ren", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "ADV", "PTKA", "ADJD", "APP