{"dta.poem.1644": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Zeit-Verderber/ Ruhe-Stehler/ ", "tokens": ["Zeit\u00b7Ver\u00b7der\u00b7ber", "/", "Ru\u00b7he\u00b7Steh\u00b7ler", "/"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Thron und Hof/ dich geb ich lo\u00df:", "tokens": ["Thron", "und", "Hof", "/", "dich", "geb", "ich", "lo\u00df", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$(", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frische W\u00e4lder/ gr\u00fcne Th\u00e4ler", "tokens": ["Fri\u00b7sche", "W\u00e4l\u00b7der", "/", "gr\u00fc\u00b7ne", "Th\u00e4\u00b7ler"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fassen mich in ihre Scho\u00df/", "tokens": ["Fas\u00b7sen", "mich", "in", "ih\u00b7re", "Scho\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich herrsche da und bin gefangen/", "tokens": ["Ich", "herr\u00b7sche", "da", "und", "bin", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier kan ich erst die freye Ruh erlangen.", "tokens": ["Hier", "kan", "ich", "erst", "die", "frey\u00b7e", "Ruh", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Unter Gold-gew\u00fcrckten Decken", "tokens": ["Un\u00b7ter", "Gold\u00b7ge\u00b7w\u00fcrck\u00b7ten", "De\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt die bange Furcht verh\u00fcllt:", "tokens": ["Liegt", "die", "ban\u00b7ge", "Furcht", "ver\u00b7h\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die sich dort im Grase strecken", "tokens": ["Die", "sich", "dort", "im", "Gra\u00b7se", "stre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00fczt der sichern Armutt Schild:", "tokens": ["Sch\u00fczt", "der", "si\u00b7chern", "Ar\u00b7mutt", "Schild", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer andern herrscht/ ist selbst gefangen/", "tokens": ["Wer", "an\u00b7dern", "herrscht", "/", "ist", "selbst", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$(", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kan keine Ruh als in der Grufft erlangen.", "tokens": ["Kan", "kei\u00b7ne", "Ruh", "als", "in", "der", "Grufft", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "KOKOM", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}