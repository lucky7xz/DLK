{"textgrid.poem.32761": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Einladung", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, Liebchen, komm aufs Land!", "tokens": ["Komm", ",", "Lieb\u00b7chen", ",", "komm", "aufs", "Land", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Winter ist vergangen;", "tokens": ["Der", "Win\u00b7ter", "ist", "ver\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Thal und H\u00fcgel prangen", "tokens": ["Und", "Thal", "und", "H\u00fc\u00b7gel", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im farbigen Gewand.", "tokens": ["Im", "far\u00b7bi\u00b7gen", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sieh, wie am blauen See", "tokens": ["Sieh", ",", "wie", "am", "blau\u00b7en", "See"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Hagedorne bl\u00fchen,", "tokens": ["Die", "Ha\u00b7ge\u00b7dor\u00b7ne", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wei\u00dfe Sch\u00e4fchen ziehen", "tokens": ["Und", "wei\u00b7\u00dfe", "Sch\u00e4f\u00b7chen", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch blumenreichen Klee.", "tokens": ["Durch", "blu\u00b7men\u00b7rei\u00b7chen", "Klee", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und hier im Schatten girrt", "tokens": ["Und", "hier", "im", "Schat\u00b7ten", "girrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein frommes Turtelt\u00e4ubchen,", "tokens": ["Ein", "from\u00b7mes", "Tur\u00b7tel\u00b7t\u00e4ub\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und lockt das arme Weibchen,", "tokens": ["Und", "lockt", "das", "ar\u00b7me", "Weib\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das sich im Hain verirrt.", "tokens": ["Das", "sich", "im", "Hain", "ver\u00b7irrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O sieh! Es k\u00f6mmt zur\u00fcck!", "tokens": ["O", "sieh", "!", "Es", "k\u00f6mmt", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ach, wenn auch du mich h\u00f6rtest,", "tokens": ["Ach", ",", "wenn", "auch", "du", "mich", "