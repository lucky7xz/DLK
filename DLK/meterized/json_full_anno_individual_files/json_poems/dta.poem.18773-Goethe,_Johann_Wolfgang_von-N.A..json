{"dta.poem.18773": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Der Liebende wird nicht irre gehn,", "tokens": ["Der", "Lie\u00b7ben\u00b7de", "wird", "nicht", "ir\u00b7re", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "NN", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "W\u00e4r\u2019s um ihn her auch noch so tr\u00fcbe.", "tokens": ["W\u00e4r's", "um", "ihn", "her", "auch", "noch", "so", "tr\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "ADV", "ADV", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sollten Leila und Medschnun auferstehn,", "tokens": ["Soll\u00b7ten", "Lei\u00b7la", "und", "Medsc\u00b7hnun", "auf\u00b7er\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "KON", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Von mir erf\u00fchren sie den Weg der Liebe.", "tokens": ["Von", "mir", "er\u00b7f\u00fch\u00b7ren", "sie", "den", "Weg", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ist\u2019s m\u00f6glich dass ich Liebchen dich kose!", "tokens": ["Ist's", "m\u00f6g\u00b7lich", "dass", "ich", "Lieb\u00b7chen", "dich", "ko\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KOUS", "PPER", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vernehme der g\u00f6ttlichen Stimme Schall!", "tokens": ["Ver\u00b7neh\u00b7me", "der", "g\u00f6tt\u00b7li\u00b7chen", "Stim\u00b7me", "Schall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Unm\u00f6glich scheint immer die Rose,", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "scheint", "im\u00b7mer", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unbegreiflich die Nachtigall.", "tokens": ["Un\u00b7be\u00b7greif\u00b7lich", "die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}