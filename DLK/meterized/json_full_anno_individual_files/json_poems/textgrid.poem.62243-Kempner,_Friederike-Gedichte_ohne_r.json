{"textgrid.poem.62243": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Gedichte ohne r", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie viel Licht im Sonnenball,", "tokens": ["Wie", "viel", "Licht", "im", "Son\u00b7nen\u00b7ball", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie viel Staub im Weltenall,", "tokens": ["Wie", "viel", "Staub", "im", "Wel\u00b7ten\u00b7all", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie viel Staub und wie viel Sand", "tokens": ["Wie", "viel", "Staub", "und", "wie", "viel", "Sand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "KON", "PWAV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gibt's nicht schon im Heimatland!", "tokens": ["Gibt's", "nicht", "schon", "im", "Hei\u00b7mat\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie viel hohes, sch\u00f6nes Licht", "tokens": ["Wie", "viel", "ho\u00b7hes", ",", "sch\u00f6\u00b7nes", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat's im deutschen Lande nicht!", "tokens": ["Hat's", "im", "deut\u00b7schen", "Lan\u00b7de", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie viel Angst in Blitzes Schein,", "tokens": ["Wie", "viel", "Angst", "in", "Blit\u00b7zes", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie viel Lust im Glase Wein!", "tokens": ["Wie", "viel", "Lust", "im", "Gla\u00b7se", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPRART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Doch ganz komisch ging man um,", "tokens": ["Doch", "ganz", "ko\u00b7misch", "ging", "man", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Alles schaffend, meistens stumm; \u2013", "tokens": ["Al\u00b7les", "schaf\u00b7fend", ",", "meis\u00b7tens", "stumm", ";", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PIS", "ADJD", "$,", "ADV", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Blo\u00df den Menschen ausgenommen,", "tokens": ["Blo\u00df", "den", "Men\u00b7schen", "aus\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Lebt sonst alles still beklommen,", "tokens": ["Lebt", "sonst", "al\u00b7les", "still", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Dem Menschen allein die Kunst man gab", "tokens": ["Dem", "Men\u00b7schen", "al\u00b7lein", "die", "Kunst", "man", "gab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "NN", "PIS", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Zu z\u00e4hlen all' sein Gut und Hab',", "tokens": ["Zu", "z\u00e4h\u00b7len", "all'", "sein", "Gut", "und", "Hab'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PIS", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Zu sagen, wie's und was ihm tut,", "tokens": ["Zu", "sa\u00b7gen", ",", "wie's", "und", "was", "ihm", "tut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VVFIN", "KON", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und wie ihm jetzt und je zu Mut:", "tokens": ["Und", "wie", "ihm", "jetzt", "und", "je", "zu", "Mut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "KON", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wenn ihn die Habsucht voll gef\u00fcllt,", "tokens": ["Wenn", "ihn", "die", "Hab\u00b7sucht", "voll", "ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und wenn die hei\u00dfe Sucht gestillt!", "tokens": ["Und", "wenn", "die", "hei\u00b7\u00dfe", "Sucht", "ge\u00b7stillt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Wie wonneatmend das Gef\u00fchl,", "tokens": ["Wie", "won\u00b7ne\u00b7at\u00b7mend", "das", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Wenn nah' man einem hohen Ziel.", "tokens": ["Wenn", "nah'", "man", "ei\u00b7nem", "ho\u00b7hen", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "K\u00fchn manche dies in's Auge nehmen,", "tokens": ["K\u00fchn", "man\u00b7che", "dies", "in's", "Au\u00b7ge", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "PDS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ohne des Mi\u00dflingens sich zu sch\u00e4men,", "tokens": ["Oh\u00b7ne", "des", "Mi\u00df\u00b7lin\u00b7gens", "sich", "zu", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Weil edle Pflanzen oft eingehen,", "tokens": ["Weil", "ed\u00b7le", "Pflan\u00b7zen", "oft", "ein\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie auf sand'gem Boden stehen! \u2013", "tokens": ["Wenn", "sie", "auf", "san\u00b7d'\u00b7gem", "Bo\u00b7den", "ste\u00b7hen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Ja, all' dies ist jenen nicht geg\u00f6nnt,", "tokens": ["Ja", ",", "all'", "dies", "ist", "je\u00b7nen", "nicht", "ge\u00b7g\u00f6nnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIAT", "PDS", "VAFIN", "PDS", "PTKNEG", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Die man nach uns Gesch\u00f6pfe Gottes nennt:", "tokens": ["Die", "man", "nach", "uns", "Ge\u00b7sch\u00f6p\u00b7fe", "Got\u00b7tes", "nennt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "PPER", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das edle Wild kann es nicht sagen,", "tokens": ["Das", "ed\u00b7le", "Wild", "kann", "es", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Jagd und Hunde ihm behagen,", "tokens": ["Wie", "Jagd", "und", "Hun\u00b7de", "ihm", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn man den Hals des Fisches sticht,", "tokens": ["Und", "wenn", "man", "den", "Hals", "des", "Fi\u00b7sches", "sticht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Denkt man, die Fische empfinden's nicht!", "tokens": ["Denkt", "man", ",", "die", "Fi\u00b7sche", "emp\u00b7fin\u00b7den's", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Ei, sehet doch, wie doppelt unbillig,", "tokens": ["Ei", ",", "se\u00b7het", "doch", ",", "wie", "dop\u00b7pelt", "un\u00b7bil\u00b7lig", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "$,", "PWAV", "ADJD", "ADJD", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Die Fische zucken ja, doch b\u00f6swillig,", "tokens": ["Die", "Fi\u00b7sche", "zu\u00b7cken", "ja", ",", "doch", "b\u00f6s\u00b7wil\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Will man sie dennoch gef\u00fchllos nennen,", "tokens": ["Will", "man", "sie", "den\u00b7noch", "ge\u00b7f\u00fchl\u00b7los", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "Weil sie nicht seufzen und klagen k\u00f6nnen;", "tokens": ["Weil", "sie", "nicht", "seuf\u00b7zen", "und", "kla\u00b7gen", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "KON", "VVINF", "VMINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Und so geht's bis zum Elephanten hin,", "tokens": ["Und", "so", "geht's", "bis", "zum", "E\u00b7le\u00b7phan\u00b7ten", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "APPRART", "NN", "PTKVZ", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Still beh\u00e4lt es den unabh\u00e4ngigen Sinn,", "tokens": ["Still", "be\u00b7h\u00e4lt", "es", "den", "un\u00b7ab\u00b7h\u00e4n\u00b7gi\u00b7gen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--++-+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Das gute, weise, edle Vieh,", "tokens": ["Das", "gu\u00b7te", ",", "wei\u00b7se", ",", "ed\u00b7le", "Vieh", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Denn Sklavendienst beugt seine Weisheit nie,", "tokens": ["Denn", "Skla\u00b7ven\u00b7dienst", "beugt", "sei\u00b7ne", "Weis\u00b7heit", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Stolz denkt es an das heimatlich Gebiet,", "tokens": ["Stolz", "denkt", "es", "an", "das", "hei\u00b7mat\u00b7lich", "Ge\u00b7biet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Sanft duldend, was im Ausland ihm geschieht.", "tokens": ["Sanft", "dul\u00b7dend", ",", "was", "im", "Aus\u00b7land", "ihm", "ge\u00b7schieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Aus all' diesem ziehe ich den Schlu\u00df,", "tokens": ["Aus", "all'", "die\u00b7sem", "zie\u00b7he", "ich", "den", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PDS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df die Sagekunst man haben mu\u00df,", "tokens": ["Da\u00df", "die", "Sa\u00b7ge\u00b7kunst", "man", "ha\u00b7ben", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIS", "VAINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nicht um zu klagen stets Leid und Weh',", "tokens": ["Nicht", "um", "zu", "kla\u00b7gen", "stets", "Leid", "und", "Weh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUI", "PTKZU", "VVINF", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da ich Elephanten standhaft seh':", "tokens": ["Da", "ich", "E\u00b7le\u00b7phan\u00b7ten", "stand\u00b7haft", "seh'", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADJD", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "(und ach, wie langweilig ist man,", "tokens": ["(", "und", "ach", ",", "wie", "lang\u00b7wei\u00b7lig", "ist", "man", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "XY", "$,", "PWAV", "ADJD", "VAFIN", "PIS", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wenn man niemals von sich schweigen kann);", "tokens": ["Wenn", "man", "nie\u00b7mals", "von", "sich", "schwei\u00b7gen", "kann", ")", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "PRF", "VVINF", "VMFIN", "$(", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Deshalb denk' ich lediglich alsdann,", "tokens": ["Des\u00b7halb", "denk'", "ich", "le\u00b7di\u00b7glich", "als\u00b7dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Wenn man etwas sagt, was n\u00fctzen kann,", "tokens": ["Wenn", "man", "et\u00b7was", "sagt", ",", "was", "n\u00fct\u00b7zen", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "$,", "PWS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Was man weise nennt, und edles denkt,", "tokens": ["Was", "man", "wei\u00b7se", "nennt", ",", "und", "ed\u00b7les", "denkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "VVFIN", "$,", "KON", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Wenn man dies dem Geist des N\u00e4chsten schenkt.", "tokens": ["Wenn", "man", "dies", "dem", "Geist", "des", "N\u00e4chs\u00b7ten", "schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PDS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "B\u00f6ses so zum Besten wendet:", "tokens": ["B\u00f6\u00b7ses", "so", "zum", "Bes\u00b7ten", "wen\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wenn dann die Lippe niemals endet,", "tokens": ["Wenn", "dann", "die", "Lip\u00b7pe", "nie\u00b7mals", "en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sie hat sich damit nicht gesch\u00e4ndet,", "tokens": ["Sie", "hat", "sich", "da\u00b7mit", "nicht", "ge\u00b7sch\u00e4n\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PAV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Dazu hat sie ein Gott gespendet;", "tokens": ["Da\u00b7zu", "hat", "sie", "ein", "Gott", "ge\u00b7spen\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.15": {"text": "Da\u00df das Aug', das Wahn geblendet,", "tokens": ["Da\u00df", "das", "Aug'", ",", "das", "Wahn", "ge\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Sich dem hellen Tag zuwendet;", "tokens": ["Sich", "dem", "hel\u00b7len", "Tag", "zu\u00b7wen\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Seelen schwachen, schon im Sinken,", "tokens": ["See\u00b7len", "schwa\u00b7chen", ",", "schon", "im", "Sin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Heil und Hoffnung zuzuwinken!", "tokens": ["Heil", "und", "Hoff\u00b7nung", "zu\u00b7zu\u00b7win\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Und die Besten und Gescheiten,", "tokens": ["Und", "die", "Bes\u00b7ten", "und", "Ge\u00b7schei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.20": {"text": "Mit den edlen Geistesgaben,", "tokens": ["Mit", "den", "ed\u00b7len", "Geis\u00b7tes\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Die zu tun im Sinne haben,", "tokens": ["Die", "zu", "tun", "im", "Sin\u00b7ne", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKZU", "VVINF", "APPRART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "An die Taten zu gew\u00f6hnen,", "tokens": ["An", "die", "Ta\u00b7ten", "zu", "ge\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "An die guten, menschlich sch\u00f6nen;", "tokens": ["An", "die", "gu\u00b7ten", ",", "menschlich", "sch\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJD", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.24": {"text": "Ja, und m\u00e4chtig hohen Seelen,", "tokens": ["Ja", ",", "und", "m\u00e4ch\u00b7tig", "ho\u00b7hen", "See\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Die gottlob auch niemals fehlen,", "tokens": ["Die", "gott\u00b7lob", "auch", "nie\u0