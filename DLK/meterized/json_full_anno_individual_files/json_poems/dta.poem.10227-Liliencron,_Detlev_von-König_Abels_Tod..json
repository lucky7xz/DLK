{"dta.poem.10227": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig Abels Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig tr\u00e4umt im Pupurzelt,", "tokens": ["Der", "K\u00f6\u00b7nig", "tr\u00e4umt", "im", "Pu\u00b7pur\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der blauen Ampel D\u00e4mmer f\u00e4llt", "tokens": ["Der", "blau\u00b7en", "Am\u00b7pel", "D\u00e4m\u00b7mer", "f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "K\u00f6nig Erich steht vor ihm, na\u00df aus der Flut,", "tokens": ["K\u00f6\u00b7nig", "E\u00b7rich", "steht", "vor", "ihm", ",", "na\u00df", "aus", "der", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "PPER", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "\u201ehinweg, hinweg, bei Christi Blut,", "tokens": ["\u201e", "hin\u00b7weg", ",", "hin\u00b7weg", ",", "bei", "Chris\u00b7ti", "Blut", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "ADV", "$,", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Steilauf der K\u00f6nig: \u201eGratias.", "tokens": ["Stei\u00b7lauf", "der", "K\u00f6\u00b7nig", ":", "\u201e", "Gra\u00b7ti\u00b7as", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "$(", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mein Schuppenhemd, und rufe rasch", "tokens": ["Mein", "Schup\u00b7pen\u00b7hemd", ",", "und", "ru\u00b7fe", "rasch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wulff Bokwoldt, der Page, wie der Hund", "tokens": ["Wulff", "Bok\u00b7woldt", ",", "der", "Pa\u00b7ge", ",", "wie", "der", "Hund"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Im Traume l\u00e4chelt sein junger Mund,", "tokens": ["Im", "Trau\u00b7me", "l\u00e4\u00b7chelt", "sein", "jun\u00b7ger", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Im Walde, voll des s\u00fc\u00dfen Schalls,", "tokens": ["Im", "Wal\u00b7de", ",", "voll", "des", "s\u00fc\u00b7\u00dfen", "Schalls", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie knotet lustig um seinen Hals", "tokens": ["Sie", "kno\u00b7tet", "lus\u00b7tig", "um", "sei\u00b7nen", "Hals"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}