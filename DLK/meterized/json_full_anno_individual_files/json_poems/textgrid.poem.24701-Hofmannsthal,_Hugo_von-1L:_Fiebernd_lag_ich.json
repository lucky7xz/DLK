{"textgrid.poem.24701": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fiebernd lag ich", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fiebernd lag ich", "tokens": ["Fie\u00b7bernd", "lag", "ich"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Und es begehrten", "tokens": ["Und", "es", "be\u00b7gehr\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die lechzenden Lippen", "tokens": ["Die", "lech\u00b7zen\u00b7den", "Lip\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Nach dem verwehrten", "tokens": ["Nach", "dem", "ver\u00b7wehr\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "K\u00fchlenden Trunk,", "tokens": ["K\u00fch\u00b7len\u00b7den", "Trunk", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und es verzehrten", "tokens": ["Und", "es", "ver\u00b7zehr\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "++-+-", "measure": "iambic.di"}, "line.7": {"text": "Sich die kranken", "tokens": ["Sich", "die", "kran\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["PRF", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Hei\u00dfen Gedanken,", "tokens": ["Hei\u00b7\u00dfen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di