{"dta.poem.2675": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Nachbarschafft des Lebens und Todtes/  \n  Bey Beerdigung Fr. E. T. g. S. erwogen/  \n den 7. Aug. 1675.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was kan/ O Nachbarin/ ich krancker Nachbar schrei-\nben?", "tokens": ["Was", "kan", "/", "O", "Nach\u00b7ba\u00b7rin", "/", "ich", "kran\u00b7cker", "Nach\u00b7bar", "schrei", "ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$(", "NE", "NN", "$(", "PPER", "ADJA", "NN", "TRUNC", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Tod sieht weder Freund-noch Nachbarschafft", "tokens": ["Der", "Tod", "sieht", "we\u00b7der", "Freun\u00b7dnoch", "Nach\u00b7bar\u00b7schafft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch mein Versprechen soll auch nicht zur\u00fccke bleiben/", "tokens": ["Doch", "mein", "Ver\u00b7spre\u00b7chen", "soll", "auch", "nicht", "zu\u00b7r\u00fc\u00b7cke", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ADV", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich halte noch mein Wort so viel ich immer kan.", "tokens": ["Ich", "hal\u00b7te", "noch", "mein", "Wort", "so", "viel", "ich", "im\u00b7mer", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "ADV", "PPER", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zwar wer Ergetzligkeit und sch\u00f6ne ", "tokens": ["Zwar", "wer", "Er\u00b7getz\u00b7lig\u00b7keit", "und", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "NN", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der lese nicht das Blat/ das Wermuth nur bedeckt.", "tokens": ["Der", "le\u00b7se", "nicht", "das", "Blat", "/", "das", "Wer\u00b7muth", "nur", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ART", "NN", "$(", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Feder so vor l\u00e4ngst bey Leichen au\u00dfge\u00fcbet", "tokens": ["Die", "Fe\u00b7der", "so", "vor", "l\u00e4ngst", "bey", "Lei\u00b7chen", "au\u00df\u00b7ge\u00b7\u00fc\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sucht nicht der Redner Zier/ der Dichter ihr Confect.", "tokens": ["Sucht", "nicht", "der", "Red\u00b7ner", "Zier", "/", "der", "Dich\u00b7ter", "ihr", "Con\u00b7fect", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich w\u00fcrde/ ", "tokens": ["Ich", "w\u00fcr\u00b7de", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VAFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Wer g\u00f6nnt Verlebten nicht die h\u00f6chst verlangte Ruh?", "tokens": ["Wer", "g\u00f6nnt", "Ver\u00b7leb\u00b7ten", "nicht", "die", "h\u00f6chst", "ver\u00b7lang\u00b7te", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "PTKNEG", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es st\u00f6rte nur mein Reim dein eisriges Begehren/", "tokens": ["Es", "st\u00f6r\u00b7te", "nur", "mein", "Reim", "dein", "eis\u00b7ri\u00b7ges", "Be\u00b7geh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das dieses in sich h\u00e4lt: Man ruffe mir Gl\u00fcck zu!", "tokens": ["Das", "die\u00b7ses", "in", "sich", "h\u00e4lt", ":", "Man", "ruf\u00b7fe", "mir", "Gl\u00fcck", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PDAT", "APPR", "PRF", "VVFIN", "$.", "PIS", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Und solches schrey ich nach: Gl\u00fcck zu! zum \u00fcberwinden;", "tokens": ["Und", "sol\u00b7ches", "schrey", "ich", "nach", ":", "Gl\u00fcck", "zu", "!", "zum", "\u00fc\u00b7berw\u00b7in\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PTKVZ", "$.", "NN", "PTKVZ", "$.", "APPRART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wo noch de\u00df Nachbars Mund dein leichter Schatten h\u00f6rt.", "tokens": ["Wo", "noch", "de\u00df", "Nach\u00b7bars", "Mund", "dein", "leich\u00b7ter", "Schat\u00b7ten", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Gl\u00fcck zu/ da\u00df du den Port de\u00df Friedens k\u00f6nnen finden", "tokens": ["Gl\u00fcck", "zu", "/", "da\u00df", "du", "den", "Port", "de\u00df", "Frie\u00b7dens", "k\u00f6n\u00b7nen", "fin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKZU", "$(", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und dich kein Ungl\u00fccks-Sturm noch Jammer-Welle st\u00f6rt.", "tokens": ["Und", "dich", "kein", "Un\u00b7gl\u00fccks\u00b7Sturm", "noch", "Jam\u00b7mer\u00b7Wel\u00b7le", "st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIAT", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du hast genug gelebt/ du hast genug erfahren/", "tokens": ["Du", "hast", "ge\u00b7nug", "ge\u00b7lebt", "/", "du", "hast", "ge\u00b7nug", "er\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$(", "PPER", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wie Leben und der Tod verkn\u00fcpffte Nachbarn seyn!", "tokens": ["Wie", "Le\u00b7ben", "und", "der", "Tod", "ver\u00b7kn\u00fcpff\u00b7te", "Nach\u00b7barn", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "ART", "NN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Jtzt geh\u2019 stu Lehens-satt bey den beschneyten Haaren", "tokens": ["Jtzt", "geh'", "stu", "Le\u00b7hens\u00b7satt", "bey", "den", "be\u00b7schney\u00b7ten", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Als Himmels-B\u00fcrger in die Ehren-Pforten ein.", "tokens": ["Als", "Him\u00b7mels\u00b7B\u00fcr\u00b7ger", "in", "die", "Eh\u00b7ren\u00b7P\u00b7for\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hier gr\u00fcbelt die Vernunfft: Ob bey der ersten Wiegen", "tokens": ["Hier", "gr\u00fc\u00b7belt", "die", "Ver\u00b7nunfft", ":", "Ob", "bey", "der", "ers\u00b7ten", "Wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Den Tod ein Sterblicher auch Nachbar nennen mag?", "tokens": ["Den", "Tod", "ein", "Sterb\u00b7li\u00b7cher", "auch", "Nach\u00b7bar", "nen\u00b7nen", "mag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ob stehn/ und unter gehn sich so zusammen f\u00fcgen", "tokens": ["Ob", "stehn", "/", "und", "un\u00b7ter", "gehn", "sich", "so", "zu\u00b7sam\u00b7men", "f\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVINF", "$(", "KON", "APPR", "VVFIN", "PRF", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Das nichts verhindern kan Zeit/ Monat/ Jahr und Tag?", "tokens": ["Das", "nichts", "ver\u00b7hin\u00b7dern", "kan", "Zeit", "/", "Mo\u00b7nat", "/", "Jahr", "und", "Tag", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVINF", "VMFIN", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Nein/ weil der Mutter-Leib den Menschen h\u00e4lt umbfangen", "tokens": ["Nein", "/", "weil", "der", "Mut\u00b7ter\u00b7Leib", "den", "Men\u00b7schen", "h\u00e4lt", "umb\u00b7fan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$(", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Baut Furcht und Schrecken offt demselben eine Bahr:", "tokens": ["Baut", "Furcht", "und", "Schre\u00b7cken", "offt", "dem\u00b7sel\u00b7ben", "ei\u00b7ne", "Bahr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "PDAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und wie viel sind dahin unangeschaut gegangen", "tokens": ["Und", "wie", "viel", "sind", "da\u00b7hin", "un\u00b7an\u00b7ge\u00b7schaut", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "VAFIN", "PAV", "ADJD", "VVPP"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.28": {"text": "Wenn ein verleschtes Licht der Frucht Erstickung war?", "tokens": ["Wenn", "ein", "ver\u00b7leschtes", "Licht", "der", "Frucht", "Er\u00b7sti\u00b7ckung", "war", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-++-+-+-+", "measure": "unknown.measure.hexa"}, "line.29": {"text": "Ja treten wir heran: Die erste Stund ist Weinen", "tokens": ["Ja", "tre\u00b7ten", "wir", "he\u00b7ran", ":", "Die", "ers\u00b7te", "Stund", "ist", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "ADJA", "NN", "VAFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wenn viertzig Tag hernach ein wenig L\u00e4cheln k\u00f6mmt/", "tokens": ["Wenn", "viert\u00b7zig", "Tag", "her\u00b7nach", "ein", "we\u00b7nig", "L\u00e4\u00b7cheln", "k\u00f6mmt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "ADV", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wir sind Gefangnen gleich eh\u2019 als wirs k\u00f6nnen meinen/", "tokens": ["Wir", "sind", "Ge\u00b7fang\u00b7nen", "gleich", "eh'", "als", "wirs", "k\u00f6n\u00b7nen", "mei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ADV", "KOUS", "KOUS", "PIS", "VMFIN", "PPOSAT", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wenn uns ein harter Druck des Nachts den Athem nimmt.", "tokens": ["Wenn", "uns", "ein", "har\u00b7ter", "Druck", "des", "Nachts", "den", "A\u00b7them", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und wie viel sterben so? Ist nicht bey unserm Schreiten", "tokens": ["Und", "wie", "viel", "ster\u00b7ben", "so", "?", "Ist", "nicht", "bey", "un\u00b7serm", "Schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "VVFIN", "ADV", "$.", "VAFIN", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Nur die Gebrechligkeit so uns die F\u00fcsse f\u00fchrt?", "tokens": ["Nur", "die", "Ge\u00b7brech\u00b7lig\u00b7keit", "so", "uns", "die", "F\u00fcs\u00b7se", "f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Nenn ich der Jugend Ei\u00df? Wo tausend Seelen gleiten", "tokens": ["Nenn", "ich", "der", "Ju\u00b7gend", "Ei\u00df", "?", "Wo", "tau\u00b7send", "See\u00b7len", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$.", "PWAV", "CARD", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wo jede Stunde sich der Tod als Nachbar r\u00fchrt.", "tokens": ["Wo", "je\u00b7de", "Stun\u00b7de", "sich", "der", "Tod", "als", "Nach\u00b7bar", "r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PRF", "ART", "NN", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Viel st\u00fcrtzt Verwegenheit zu den verblasten Schaaren/", "tokens": ["Viel", "st\u00fcrtzt", "Ver\u00b7we\u00b7gen\u00b7heit", "zu", "den", "ver\u00b7blas\u00b7ten", "Schaa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Viel l\u00e4st der selbst-Betrug de\u00df Todes Garn nicht schaun.", "tokens": ["Viel", "l\u00e4st", "der", "selbst\u00b7Be\u00b7trug", "de\u00df", "To\u00b7des", "Garn", "nicht", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Theils sind in ihrer Lust/ theils in dem Zorn gefahren/", "tokens": ["Theils", "sind", "in", "ih\u00b7rer", "Lust", "/", "theils", "in", "dem", "Zorn", "ge\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Den Meisten mu\u00df oft selbst vor ihrem Leben graun.", "tokens": ["Den", "Meis\u00b7ten", "mu\u00df", "oft", "selbst", "vor", "ih\u00b7rem", "Le\u00b7ben", "graun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Verkl\u00e4rt sich der Verstand? so werden doch Begierden", "tokens": ["Ver\u00b7kl\u00e4rt", "sich", "der", "Ver\u00b7stand", "?", "so", "wer\u00b7den", "doch", "Be\u00b7gier\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "$.", "ADV", "VAFIN", "ADV", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine