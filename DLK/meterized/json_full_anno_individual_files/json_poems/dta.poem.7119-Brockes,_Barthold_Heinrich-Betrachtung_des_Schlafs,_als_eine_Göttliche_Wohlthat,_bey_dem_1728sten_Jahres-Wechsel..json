{"dta.poem.7119": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sp\u00fcret \u00fcberdem, wie eine sanffte Stille,", "tokens": ["Man", "sp\u00fc\u00b7ret", "\u00fc\u00b7ber\u00b7dem", ",", "wie", "ei\u00b7ne", "sanff\u00b7te", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu eben diesem Zweck sodann die Lufft erf\u00fclle,", "tokens": ["Zu", "e\u00b7ben", "die\u00b7sem", "Zweck", "so\u00b7dann", "die", "Lufft", "er\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PDAT", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die sonsten voll Ger\u00e4usch und \u00fcberall", "tokens": ["Die", "sons\u00b7ten", "voll", "Ge\u00b7r\u00e4usch", "und", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Voll T\u00f6ne, voller Lerm und Schall.", "tokens": ["Voll", "T\u00f6\u00b7ne", ",", "vol\u00b7ler", "Lerm", "und", "Schall", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie dann vermuthlich auch viel rege Geistigkeiten,", "tokens": ["Wie", "dann", "ver\u00b7muth\u00b7lich", "auch", "viel", "re\u00b7ge", "Geis\u00b7tig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die aus dem Licht in uns entstehn,", "tokens": ["Die", "aus", "dem", "Licht", "in", "uns", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auch wieder mit dem Licht vergehn.", "tokens": ["Auch", "wie\u00b7der", "mit", "dem", "Licht", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}