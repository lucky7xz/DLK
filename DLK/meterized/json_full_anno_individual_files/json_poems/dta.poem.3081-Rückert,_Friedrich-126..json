{"dta.poem.3081": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "126.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein alter Spruch: das beste Leichentuch", "tokens": ["Es", "ist", "ein", "al\u00b7ter", "Spruch", ":", "das", "bes\u00b7te", "Lei\u00b7chen\u00b7tuch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist Redlichkeit, sie w\u00fcrzt den Tod mit Wohlgeruch.", "tokens": ["Ist", "Red\u00b7lich\u00b7keit", ",", "sie", "w\u00fcrzt", "den", "Tod", "mit", "Wohl\u00b7ge\u00b7ruch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es ist ein alter Spruch: wenn sie mit dir nun schreiten", "tokens": ["Es", "ist", "ein", "al\u00b7ter", "Spruch", ":", "wenn", "sie", "mit", "dir", "nun", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "KOUS", "PPER", "APPR", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu Grabe, werden sie verschieden dich begleiten.", "tokens": ["Zu", "Gra\u00b7be", ",", "wer\u00b7den", "sie", "ver\u00b7schie\u00b7den", "dich", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VAFIN", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dein einer Freund, dein Gut, bleibt hinter dir im Haus;", "tokens": ["Dein", "ei\u00b7ner", "Freund", ",", "dein", "Gut", ",", "bleibt", "hin\u00b7ter", "dir", "im", "Haus", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein andrer Freund, dein Ruhm, fliegt in die Welt hinaus.", "tokens": ["Dein", "an\u00b7drer", "Freund", ",", "dein", "Ruhm", ",", "fliegt", "in", "die", "Welt", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dein dritter Freund, dein Freund, begleitet dich ans Grab,", "tokens": ["Dein", "drit\u00b7ter", "Freund", ",", "dein", "Freund", ",", "be\u00b7glei\u00b7tet", "dich", "ans", "Grab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und kehret um, sobald er warf die Scholl' hinab.", "tokens": ["Und", "keh\u00b7ret", "um", ",", "so\u00b7bald", "er", "warf", "die", "Scholl'", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Liebe schickt villeicht dir ein Paar Thr\u00e4nen nach,", "tokens": ["Die", "Lie\u00b7be", "schickt", "vil\u00b7leicht", "dir", "ein", "Paar", "Thr\u00e4\u00b7nen", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPER", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch auf der gro\u00dfen Reis' ist dis Geleite schwach.", "tokens": ["Doch", "auf", "der", "gro\u00b7\u00dfen", "Reis'", "ist", "dis", "Ge\u00b7lei\u00b7te", "schwach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VAFIN", "PDS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ein gut Gewissen nur wird bei der Hand dich fassen,", "tokens": ["Ein", "gut", "Ge\u00b7wis\u00b7sen", "nur", "wird", "bei", "der", "Hand", "dich", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "VAFIN", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Nur der Geleitsmann wird dich nimmermehr verlassen.", "tokens": ["Nur", "der", "Ge\u00b7leits\u00b7mann", "wird", "dich", "nim\u00b7mer\u00b7mehr", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und was du Gutes hast vorausgesandt mit Beten,", "tokens": ["Und", "was", "du", "Gu\u00b7tes", "hast", "vor\u00b7aus\u00b7ge\u00b7sandt", "mit", "Be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "NN", "VAFIN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tritt dir entgegen dort, und wird dich dort vertreten.", "tokens": ["Tritt", "dir", "ent\u00b7ge\u00b7gen", "dort", ",", "und", "wird", "dich", "dort", "ver\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "$,", "KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}}}}}