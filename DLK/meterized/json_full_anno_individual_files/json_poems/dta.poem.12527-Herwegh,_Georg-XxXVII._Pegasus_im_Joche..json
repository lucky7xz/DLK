{"dta.poem.12527": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XxXVII.  \n  Pegasus im Joche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201emu\u00df ich, sprach mein Pegasus,", "tokens": ["\u201e", "mu\u00df", "ich", ",", "sprach", "mein", "Pe\u00b7ga\u00b7sus", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "meiner Freiheit denn entsagen:", "tokens": ["mei\u00b7ner", "Frei\u00b7heit", "denn", "ent\u00b7sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zieh' ich lieber doch am Pflug,", "tokens": ["Zieh'", "ich", "lie\u00b7ber", "doch", "am", "Pflug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "als selbacht am gro\u00dfen Wagen,", "tokens": ["als", "sel\u00b7bacht", "am", "gro\u00b7\u00dfen", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Fre\u00df' ich lieber doch mein Heu", "tokens": ["Fre\u00df'", "ich", "lie\u00b7ber", "doch", "mein", "Heu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "aus des letzten Bauern Raufe,", "tokens": ["aus", "des", "letz\u00b7ten", "Bau\u00b7ern", "Rau\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Als ich aus der Marmorkrippe", "tokens": ["Als", "ich", "aus", "der", "Mar\u00b7mor\u00b7krip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "mit dem Vieh des Hofes saufe.\u201c", "tokens": ["mit", "dem", "Vieh", "des", "Ho\u00b7fes", "sau\u00b7fe", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}