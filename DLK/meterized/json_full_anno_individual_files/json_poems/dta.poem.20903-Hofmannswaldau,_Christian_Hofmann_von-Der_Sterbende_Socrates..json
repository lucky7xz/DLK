{"dta.poem.20903": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Wer etwan einen Zweiffel sp\u00fchret/", "tokens": ["Wer", "et\u00b7wan", "ei\u00b7nen", "Zweif\u00b7fel", "sp\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der melde sich bey Zeiten an.", "tokens": ["Der", "mel\u00b7de", "sich", "bey", "Zei\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es wei\u00df es doch wol iederman/", "tokens": ["Es", "wei\u00df", "es", "doch", "wol", "ie\u00b7der\u00b7man", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "PIS", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo man dergleichen Reden f\u00fchret/", "tokens": ["Wo", "man", "derg\u00b7lei\u00b7chen", "Re\u00b7den", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PIS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df leichte was entgehen kan.", "tokens": ["Da\u00df", "leich\u00b7te", "was", "ent\u00b7ge\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "PWS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}