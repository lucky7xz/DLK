{"textgrid.poem.24995": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Ewige Jugend", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n war's, als aus dem Morgenrot", "tokens": ["Sch\u00f6n", "wa\u00b7r's", ",", "als", "aus", "dem", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mein Leben anhub aufzustrahlen", "tokens": ["Mein", "Le\u00b7ben", "an\u00b7hub", "auf\u00b7zu\u00b7strah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und mir die Lust in vollen Schalen", "tokens": ["Und", "mir", "die", "Lust", "in", "vol\u00b7len", "Scha\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die reichsten ihrer Spenden bot;", "tokens": ["Die", "reichs\u00b7ten", "ih\u00b7rer", "Spen\u00b7den", "bot", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch nicht ", "tokens": ["Doch", "nicht"], "token_info": ["word", "word"], "pos": ["KON", "PTKNEG"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Und bleichend mit den braunen Haaren,", "tokens": ["Und", "blei\u00b7chend", "mit", "den", "brau\u00b7nen", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich preise die, die nie vergeht", "tokens": ["Ich", "prei\u00b7se", "die", ",", "die", "nie", "ver\u00b7geht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und sch\u00f6ner aufbl\u00fcht mit den Jahren.", "tokens": ["Und", "sch\u00f6\u00b7ner", "auf\u00b7bl\u00fcht", "mit", "den", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Das G\u00f6tterbild, das immerdar", "tokens": ["Das", "G\u00f6t\u00b7ter\u00b7bild", ",", "das", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich feierte mit Hymnensange,", "tokens": ["Ich", "fei\u00b7er\u00b7te", "mit", "Hym\u00b7nen\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sch\u00fctz es, da\u00df es ewig prange", "tokens": ["Sie", "sch\u00fctz", "es", ",", "da\u00df", "es", "e\u00b7wig", "pran\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf meines Herzens Weihaltar,", "tokens": ["Auf", "mei\u00b7nes", "Her\u00b7zens", "Wei\u00b7hal\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und meine Leier stimme sie,", "tokens": ["Und", "mei\u00b7ne", "Lei\u00b7er", "stim\u00b7me", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df alles Herrliche und Sch\u00f6ne", "tokens": ["Da\u00df", "al\u00b7les", "Herr\u00b7li\u00b7che", "und", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In voller sel'ger Harmonie,", "tokens": ["In", "vol\u00b7ler", "sel'\u00b7ger", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus ihren Saiten wiedert\u00f6ne!", "tokens": ["Aus", "ih\u00b7ren", "Sai\u00b7ten", "wie\u00b7der\u00b7t\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie trage aufw\u00e4rts meinen Geist,", "tokens": ["Sie", "tra\u00b7ge", "auf\u00b7w\u00e4rts", "mei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf da\u00df er hoch und h\u00f6her ringe,", "tokens": ["Auf", "da\u00df", "er", "hoch", "und", "h\u00f6\u00b7her", "rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So wie in Jugendkraft die Schwinge", "tokens": ["So", "wie", "in", "Ju\u00b7gend\u00b7kraft", "die", "Schwin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den alten Aar nach oben rei\u00dft;", "tokens": ["Den", "al\u00b7ten", "Aar", "nach", "o\u00b7ben", "rei\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er schwebe, himmelsluftgewiegt,", "tokens": ["Er", "schwe\u00b7be", ",", "him\u00b7mel\u00b7sluft\u00b7ge\u00b7wiegt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Indes, vom Lichtglanz ungeblendet,", "tokens": ["In\u00b7des", ",", "vom", "Licht\u00b7glanz", "un\u00b7ge\u00b7blen\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er auf die Welt, die unten liegt,", "tokens": ["Er", "auf", "die", "Welt", ",", "die", "un\u00b7ten", "liegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Sonnenblicke niedersendet.", "tokens": ["Die", "Son\u00b7nen\u00b7bli\u00b7cke", "nie\u00b7der\u00b7sen\u00b7det", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "H\u00e4uft dann des Alters Wintertag", "tokens": ["H\u00e4uft", "dann", "des", "Al\u00b7ters", "Win\u00b7ter\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den letzten Schnee auf meine Locken:", "tokens": ["Den", "letz\u00b7ten", "Schnee", "auf", "mei\u00b7ne", "Lo\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht schrecken mich die wei\u00dfen Flocken;", "tokens": ["Nicht", "schre\u00b7cken", "mich", "die", "wei\u00b7\u00dfen", "Flo\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wei\u00df, ein neuer Lenz folgt nach;", "tokens": ["Ich", "wei\u00df", ",", "ein", "neu\u00b7er", "Lenz", "folgt", "nach", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und heller noch, als da ich jung,", "tokens": ["Und", "hel\u00b7ler", "noch", ",", "als", "da", "ich", "jung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "KOKOM", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Abendrot der Alpen Firne,", "tokens": ["Wie", "A\u00b7ben\u00b7drot", "der", "Al\u00b7pen", "Fir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Umleuchte mir Begeisterung,", "tokens": ["Um\u00b7leuch\u00b7te", "mir", "Be\u00b7geis\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn sie zum Grab sich neigt, die Stirne.", "tokens": ["Wenn", "sie", "zum", "Grab", "sich", "neigt", ",", "die", "Stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PRF", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gedr\u00fcckt hat so der Genius", "tokens": ["Ge\u00b7dr\u00fcckt", "hat", "so", "der", "Ge\u00b7nius"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Dem einundachtzigj\u00e4hr'gen Greise,", "tokens": ["Dem", "ein\u00b7un\u00b7dacht\u00b7zig\u00b7j\u00e4hr'\u00b7gen", "Grei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem hehren ", "tokens": ["Dem", "heh\u00b7ren"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Auf Stirn und Mund den Weiheku\u00df;", "tokens": ["Auf", "Stirn", "und", "Mund", "den", "Wei\u00b7he\u00b7ku\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und, w\u00e4hrend er im Morgenlicht", "tokens": ["Und", ",", "w\u00e4h\u00b7rend", "er", "im", "Mor\u00b7gen\u00b7licht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Opfer bracht' am Musenherde,", "tokens": ["Sein", "Op\u00b7fer", "bracht'", "am", "Mu\u00b7sen\u00b7her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Noch auf den Lippen ein Gedicht,", "tokens": ["Noch", "auf", "den", "Lip\u00b7pen", "ein", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ward er entr\u00fcckt von dieser Erde.", "tokens": ["Ward", "er", "ent\u00b7r\u00fcckt", "von", "die\u00b7ser", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sch\u00f6n war's, als aus dem Morgenrot", "tokens": ["Sch\u00f6n", "wa\u00b7r's", ",", "als", "aus", "dem", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mein Leben anhub aufzustrahlen", "tokens": ["Mein", "Le\u00b7ben", "an\u00b7hub", "auf\u00b7zu\u00b7strah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und mir die Lust in vollen Schalen", "tokens": ["Und", "mir", "die", "Lust", "in", "vol\u00b7len", "Scha\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die reichsten ihrer Spenden bot;", "tokens": ["Die", "reichs\u00b7ten", "ih\u00b7rer", "Spen\u00b7den", "bot", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "