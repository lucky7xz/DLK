{"textgrid.poem.25129": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: O G\u00f6ttin! lausche diesem armen Lied,", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O G\u00f6ttin! lausche diesem armen Lied,", "tokens": ["O", "G\u00f6t\u00b7tin", "!", "lau\u00b7sche", "die\u00b7sem", "ar\u00b7men", "Lied", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das lieb Erinnern, s\u00fc\u00dfer Zwang geboren;", "tokens": ["Das", "lieb", "E\u00b7rin\u00b7nern", ",", "s\u00fc\u00b7\u00dfer", "Zwang", "ge\u00b7bo\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "NN", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verzeih, das dein Geheimnis es erriet", "tokens": ["Ver\u00b7zeih", ",", "das", "dein", "Ge\u00b7heim\u00b7nis", "es", "er\u00b7riet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wiederk\u00fcndet deinen eignen Ohren:", "tokens": ["Und", "wie\u00b7der\u00b7k\u00fcn\u00b7det", "dei\u00b7nen", "eig\u00b7nen", "Oh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich tr\u00e4umte heut \u2013 denn sollte wacher Sinn", "tokens": ["Ich", "tr\u00e4um\u00b7te", "heut", "\u2013", "denn", "soll\u00b7te", "wa\u00b7cher", "Sinn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KON", "VMFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wohl je die lichtbeschwingte Psyche schauen? \u2013", "tokens": ["Wohl", "je", "die", "licht\u00b7be\u00b7schwing\u00b7te", "Psy\u00b7che", "schau\u00b7en", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In lichtem Walde schritt ich f\u00fcr mich hin,", "tokens": ["In", "lich\u00b7tem", "Wal\u00b7de", "schritt", "ich", "f\u00fcr", "mich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da pl\u00f6tzlich fa\u00dfte mich ehrf\u00fcrchtig Grauen:", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "fa\u00df\u00b7te", "mich", "ehr\u00b7f\u00fcrch\u00b7tig", "Grau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Eng Seit an Seite lag ein sch\u00f6nes Paar", "tokens": ["Eng", "Seit", "an", "Sei\u00b7te", "lag", "ein", "sch\u00f6\u00b7nes", "Paar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ins Gras gebettet, \u00fcber ihnen spann", "tokens": ["Ins", "Gras", "ge\u00b7bet\u00b7tet", ",", "\u00fc\u00b7ber", "ih\u00b7nen", "spann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "$,", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Das Laub ein fl\u00fcsternd Dach, ein B\u00e4chlein rann", "tokens": ["Das", "Laub", "ein", "fl\u00fcs\u00b7ternd", "Dach", ",", "ein", "B\u00e4ch\u00b7lein", "rann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJD", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Durchs Gr\u00fcn, kaum wahrnehmbar.", "tokens": ["Durchs", "Gr\u00fcn", ",", "kaum", "wahr\u00b7nehm\u00b7bar", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auf blumiger Au, die bunt und silberklar", "tokens": ["Auf", "blu\u00b7mi\u00b7ger", "Au", ",", "die", "bunt", "und", "sil\u00b7ber\u00b7klar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADJD", "KON", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und k\u00fchl und duftend in die Stille sann,", "tokens": ["Und", "k\u00fchl", "und", "duf\u00b7tend", "in", "die", "Stil\u00b7le", "sann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sanftatmend lagen sie, die Fl\u00fcgel bogen", "tokens": ["Sanf\u00b7tat\u00b7mend", "la\u00b7gen", "sie", ",", "die", "Fl\u00fc\u00b7gel", "bo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich aneinander und die Arme auch,", "tokens": ["Sich", "an\u00b7ein\u00b7an\u00b7der", "und", "die", "Ar\u00b7me", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "KON", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Lippen trennte nur ein Atemhauch,", "tokens": ["Die", "Lip\u00b7pen", "trenn\u00b7te", "nur", "ein", "A\u00b7tem\u00b7hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als halbe Schlummer Mund von Mund gezogen,", "tokens": ["Als", "hal\u00b7be", "Schlum\u00b7mer", "Mund", "von", "Mund", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Als w\u00fcrden jungerwachte Liebeswogen", "tokens": ["Als", "w\u00fcr\u00b7den", "jun\u00b7ger\u00b7wach\u00b7te", "Lie\u00b7bes\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zu neuem seligen K\u00fcssen sie begl\u00fccken.", "tokens": ["Zu", "neu\u00b7em", "se\u00b7li\u00b7gen", "K\u00fcs\u00b7sen", "sie", "be\u00b7gl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Den Knaben kannte ich;", "tokens": ["Den", "Kna\u00b7ben", "kann\u00b7te", "ich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Du Taube doch, du lieblichstes Entz\u00fccken,", "tokens": ["Du", "Tau\u00b7be", "doch", ",", "du", "lieb\u00b7lichs\u00b7tes", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Warst Psyche sicherlich!", "tokens": ["Warst", "Psy\u00b7che", "si\u00b7cher\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O letztgebornes lieblichstes Gesicht", "tokens": ["O", "letzt\u00b7ge\u00b7bor\u00b7nes", "lieb\u00b7lichs\u00b7tes", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hoch \u00fcber des Olymps verbleichter Pracht!", "tokens": ["Hoch", "\u00fc\u00b7ber", "des", "O\u00b7lymps", "ver\u00b7bleich\u00b7ter", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "O sch\u00f6ner du als erstes Sternenlicht,", "tokens": ["O", "sch\u00f6\u00b7ner", "du", "als", "ers\u00b7tes", "Ster\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "PPER", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das wie ein Gl\u00fchwurm in den Abend wacht.", "tokens": ["Das", "wie", "ein", "Gl\u00fch\u00b7wurm", "in", "den", "A\u00b7bend", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ja sch\u00f6ner du! Obgleich nicht ", "tokens": ["Ja", "sch\u00f6\u00b7ner", "du", "!", "Ob\u00b7gleich", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "ADJD", "PPER", "$.", "KOUS", "PTKNEG"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Noch Opfer dir geschichtet", "tokens": ["Noch", "Op\u00b7fer", "dir", "ge\u00b7schich\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und n\u00e4chtens keine s\u00fc\u00dfe M\u00e4dchenschar", "tokens": ["Und", "n\u00e4ch\u00b7tens", "kei\u00b7ne", "s\u00fc\u00b7\u00dfe", "M\u00e4d\u00b7chen\u00b7schar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Zu dir Ges\u00e4nge richtet:", "tokens": ["Zu", "dir", "Ge\u00b7s\u00e4n\u00b7ge", "rich\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Kein Wort, kein Fl\u00f6tenspiel, kein frommer Rauch,", "tokens": ["Kein", "Wort", ",", "kein", "Fl\u00f6\u00b7ten\u00b7spiel", ",", "kein", "from\u00b7mer", "Rauch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der sanft aus schwingenden Gef\u00e4\u00dfen wellte,", "tokens": ["Der", "sanft", "aus", "schwin\u00b7gen\u00b7den", "Ge\u00b7f\u00e4\u00b7\u00dfen", "well\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Kein Schrein, kein Hain, nicht ein inbr\u00fcnstiger Hauch,", "tokens": ["Kein", "Schrein", ",", "kein", "Hain", ",", "nicht", "ein", "in\u00b7br\u00fcns\u00b7ti\u00b7ger", "Hauch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "Der eines bleichen Priesters Tr\u00e4umen schwellte.", "tokens": ["Der", "ei\u00b7nes", "blei\u00b7chen", "Pries\u00b7ters", "Tr\u00e4u\u00b7men", "schwell\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Strahlendste! Zu sp\u00e4t f\u00fcr jene Zeit,", "tokens": ["O", "Strah\u00b7lends\u00b7te", "!", "Zu", "sp\u00e4t", "f\u00fcr", "je\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PTKA", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu sp\u00e4t, zu sp\u00e4t auch f\u00fcr leichtgl\u00e4ubige Leier,", "tokens": ["Zu", "sp\u00e4t", ",", "zu", "sp\u00e4t", "auch", "f\u00fcr", "leicht\u00b7gl\u00e4u\u00b7bi\u00b7ge", "Lei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "PTKA", "ADJD", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die heilig sprach des Waldes Einsamkeit,", "tokens": ["Die", "hei\u00b7lig", "sprach", "des", "Wal\u00b7des", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Heilig die Luft, das Wasser und das Feuer.", "tokens": ["Hei\u00b7lig", "die", "Luft", ",", "das", "Was\u00b7ser", "und", "das", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Doch selbst in unsern Tagen, die so ferne", "tokens": ["Doch", "selbst", "in", "un\u00b7sern", "Ta\u00b7gen", ",", "die", "so", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von froher Fr\u00f6mmigkeit, ergl\u00e4nzt dein Flug,", "tokens": ["Von", "fro\u00b7her", "Fr\u00f6m\u00b7mig\u00b7keit", ",", "er\u00b7gl\u00e4nzt", "dein", "Flug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der \u00fcber st\u00fcrzenden Olymp dich trug,", "tokens": ["Der", "\u00fc\u00b7ber", "st\u00fcr\u00b7zen\u00b7den", "O\u00b7lymp", "dich", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nun meinen Augen, und ich bete gerne.", "tokens": ["Nun", "mei\u00b7nen", "Au\u00b7gen", ",", "und", "ich", "be\u00b7te", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So la\u00df ", "tokens": ["So", "la\u00df"], "token_info": ["word", "word"], "pos": ["ADV", "VVIMP"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Die betet am Altar,", "tokens": ["Die", "be\u00b7tet", "am", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Dein Wort, dein Fl\u00f6tenspiel, dein frommer Rauch,", "tokens": ["Dein", "Wort", ",", "dein", "Fl\u00f6\u00b7ten\u00b7spiel", ",", "dein", "from\u00b7mer", "Rauch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Den dir ein schwingend Weihgef\u00e4\u00df entsendet,", "tokens": ["Den", "dir", "ein", "schwin\u00b7gend", "Weih\u00b7ge\u00b7f\u00e4\u00df", "ent\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Dein Schrein, dein Hain und dein inbr\u00fcnstiger Hauch,", "tokens": ["Dein", "Schrein", ",", "dein", "Hain", "und", "dein", "in\u00b7br\u00fcns\u00b7ti\u00b7ger", "Hauch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "Den eines bleichen Priesters Traum dir spendet.", "tokens": ["Den", "ei\u00b7nes", "blei\u00b7chen", "Pries\u00b7ters", "Traum", "dir", "spen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich will, dein Priester, dir den Tempel richten", "tokens": ["Ich", "will", ",", "dein", "Pries\u00b7ter", ","