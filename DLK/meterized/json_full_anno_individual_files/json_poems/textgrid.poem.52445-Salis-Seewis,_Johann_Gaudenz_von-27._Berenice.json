], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Auge schwimmt im schmachtenden Verlangen,", "tokens": ["Ihr", "Au\u00b7ge", "schwimmt", "im", "schmach\u00b7ten\u00b7den", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von s\u00fc\u00dfer R\u00fchrung feucht.", "tokens": ["Von", "s\u00fc\u00b7\u00dfer", "R\u00fch\u00b7rung", "feucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Wie aus des Munds halb aufgehauchter Bl\u00fcte", "tokens": ["Wie", "aus", "des", "Munds", "halb", "auf\u00b7ge\u00b7hauch\u00b7ter", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr \u00c4therodem flieht!", "tokens": ["Ihr", "\u00c4\u00b7the\u00b7ro\u00b7dem", "flieht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Lippen nun ein L\u00e4cheln milder G\u00fcte", "tokens": ["Die", "Lip\u00b7pen", "nun", "ein", "L\u00e4\u00b7cheln", "mil\u00b7der", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sanft in die H\u00f6he zieht!", "tokens": ["Sanft", "in", "die", "H\u00f6\u00b7he", "zieht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"