{"textgrid.poem.52445": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "27. Berenice", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie tritt hervor; ihr Kirschenbl\u00fctenreiser", "tokens": ["Sie", "tritt", "her\u00b7vor", ";", "ihr", "Kir\u00b7schen\u00b7bl\u00fc\u00b7ten\u00b7rei\u00b7ser"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Enth\u00fcllt ihr Angesicht!", "tokens": ["Ent\u00b7h\u00fcllt", "ihr", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lauscht, Nymphen, lauscht! Dryaden, lispelt leiser!", "tokens": ["Lauscht", ",", "Nym\u00b7phen", ",", "lauscht", "!", "Dry\u00b7a\u00b7den", ",", "lis\u00b7pelt", "lei\u00b7ser", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$.", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr, Weste, atmet nicht!", "tokens": ["Ihr", ",", "Wes\u00b7te", ",", "at\u00b7met", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bl\u00fcht gl\u00e4nzender, ihr Wiesenanemonen,", "tokens": ["Bl\u00fcht", "gl\u00e4n\u00b7zen\u00b7der", ",", "ihr", "Wie\u00b7sen\u00b7a\u00b7ne\u00b7mo\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seit euch ihr Fu\u00df betrat;", "tokens": ["Seit", "euch", "ihr", "Fu\u00df", "be\u00b7trat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Cytisus, senk alle Bl\u00fctenkronen", "tokens": ["O", "Cy\u00b7ti\u00b7sus", ",", "senk", "al\u00b7le", "Bl\u00fc\u00b7ten\u00b7kro\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "VVFIN", "PIAT", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Auf meiner Holden Pfad!", "tokens": ["Auf", "mei\u00b7ner", "Hol\u00b7den", "Pfad", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Holdselige! auf silbernen Narzissen", "tokens": ["Hold\u00b7se\u00b7li\u00b7ge", "!", "auf", "sil\u00b7ber\u00b7nen", "Nar\u00b7zis\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Weht rauschend ihr Gewand;", "tokens": ["Weht", "rau\u00b7schend", "ihr", "Ge\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Angerklee strebt ihren Saum zu k\u00fcssen,", "tokens": ["Der", "An\u00b7ger\u00b7klee", "strebt", "ih\u00b7ren", "Saum", "zu", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Grases Halm die Hand.", "tokens": ["Des", "Gra\u00b7ses", "Halm", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Rosenlicht umflie\u00dft die zarten Wangen,", "tokens": ["Ein", "Ro\u00b7sen\u00b7licht", "um\u00b7flie\u00dft", "die", "zar\u00b7ten", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die stille Sehnsucht bleicht;", "tokens": ["Die", "stil\u00b7le", "Sehn\u00b7sucht", "bleicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Auge schwimmt im schmachtenden Verlangen,", "tokens": ["Ihr", "Au\u00b7ge", "schwimmt", "im", "schmach\u00b7ten\u00b7den", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von s\u00fc\u00dfer R\u00fchrung feucht.", "tokens": ["Von", "s\u00fc\u00b7\u00dfer", "R\u00fch\u00b7rung", "feucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie aus des Munds halb aufgehauchter Bl\u00fcte", "tokens": ["Wie", "aus", "des", "Munds", "halb", "auf\u00b7ge\u00b7hauch\u00b7ter", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr \u00c4therodem flieht!", "tokens": ["Ihr", "\u00c4\u00b7the\u00b7ro\u00b7dem", "flieht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Lippen nun ein L\u00e4cheln milder G\u00fcte", "tokens": ["Die", "Lip\u00b7pen", "nun", "ein", "L\u00e4\u00b7cheln", "mil\u00b7der", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sanft in die H\u00f6he zieht!", "tokens": ["Sanft", "in", "die", "H\u00f6\u00b7he", "zieht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Vom Jugenddrang, der ihren Busen f\u00fcllet,", "tokens": ["Vom", "Ju\u00b7gend\u00b7drang", ",", "der", "ih\u00b7ren", "Bu\u00b7sen", "f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erbebt der Schleife Band;", "tokens": ["Er\u00b7bebt", "der", "Schlei\u00b7fe", "Band", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erbebt der Flor, so sorgsam \u00fcberh\u00fcllet", "tokens": ["Er\u00b7bebt", "der", "Flor", ",", "so", "sorg\u00b7sam", "\u00fc\u00b7berh\u00b7\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von ihrer Mutter Hand.", "tokens": ["Von", "ih\u00b7rer", "Mut\u00b7ter", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wie sich ihr Haar, mit weichem Niederwallen,", "tokens": ["Wie", "sich", "ihr", "Haar", ",", "mit", "wei\u00b7chem", "Nie\u00b7der\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In lose Ringel schlingt,", "tokens": ["In", "lo\u00b7se", "Rin\u00b7gel", "schlingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, der Natur aus offner Hand entfallen,", "tokens": ["Und", ",", "der", "Na\u00b7tur", "aus", "off\u00b7ner", "Hand", "ent\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf ihren G\u00fcrtel sinkt!", "tokens": ["Auf", "ih\u00b7ren", "G\u00fcr\u00b7tel", "sinkt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Seht, wie der Hut, aus falbem Stroh gewoben,", "tokens": ["Seht", ",", "wie", "der", "Hut", ",", "aus", "fal\u00b7bem", "Stroh", "ge\u00b7wo\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich auf ihr Auge senkt!", "tokens": ["Sich", "auf", "ihr", "Au\u00b7ge", "senkt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch niedlich noch, wenn er, im Gehn verschoben,", "tokens": ["Auch", "nied\u00b7lich", "noch", ",", "wenn", "er", ",", "im", "Gehn", "ver\u00b7scho\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "KOUS", "PPER", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nachl\u00e4ssig seitw\u00e4rts h\u00e4ngt!", "tokens": ["Nach\u00b7l\u00e4s\u00b7sig", "seit\u00b7w\u00e4rts", "h\u00e4ngt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sie schwebt dahin, auf Lotus und Violen,", "tokens": ["Sie", "schwebt", "da\u00b7hin", ",", "auf", "Lo\u00b7tus", "und", "Vi\u00b7o\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit leisem Feenschritt,", "tokens": ["Mit", "lei\u00b7sem", "Feen\u00b7schritt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wie Iris leicht, mit purpurhellen Sohlen,", "tokens": ["Wie", "I\u00b7ris", "leicht", ",", "mit", "pur\u00b7pur\u00b7hel\u00b7len", "Soh\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf blaue Wolken tritt.", "tokens": ["Auf", "blau\u00b7e", "Wol\u00b7ken", "tritt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ach! sie entschwand ins Gr\u00fcn der Gartenh\u00fctte,", "tokens": ["Ach", "!", "sie", "ent\u00b7schwand", "ins", "Gr\u00fcn", "der", "Gar\u00b7ten\u00b7h\u00fct\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Gei\u00dfblatt dicht umlaubt.", "tokens": ["Die", "Gei\u00df\u00b7blatt", "dicht", "um\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun rausch, o Quell, durchwehter Kirschbaum, sch\u00fctte", "tokens": ["Nun", "rausch", ",", "o", "Quell", ",", "durch\u00b7weh\u00b7ter", "Kirschbaum", ",", "sch\u00fct\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "$,", "FM", "NN", "$,", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Noch Bl\u00fcten auf ihr Haupt!", "tokens": ["Noch", "Bl\u00fc\u00b7ten", "auf", "ihr", "Haupt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sie tritt hervor; ihr Kirschenbl\u00fctenreiser", "tokens": ["Sie", "tritt", "her\u00b7vor", ";", "ihr", "Kir\u00b7schen\u00b7bl\u00fc\u00b7ten\u00b7rei\u00b7ser"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Enth\u00fcllt ihr Angesicht!", "tokens": ["Ent\u00b7h\u00fcllt", "ihr", "An\u00b7ge\u00b7sicht", "!"], "token_in