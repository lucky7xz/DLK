{"dta.poem.13218": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eaus den Betrachtungen erfolget dieser Saz:", "tokens": ["\u201e", "aus", "den", "Be\u00b7trach\u00b7tun\u00b7gen", "er\u00b7fol\u00b7get", "die\u00b7ser", "Saz", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VVFIN", "PDAT", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "\u201ejhr wachet nicht f\u00fcr mich, nur f\u00fcr des Himmels Schaz.", "tokens": ["\u201e", "jhr", "wa\u00b7chet", "nicht", "f\u00fcr", "mich", ",", "nur", "f\u00fcr", "des", "Him\u00b7mels", "Schaz", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "$,", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eseynd nun die Kinder sein, so wird er sie begl\u00fccken,", "tokens": ["\u201e", "seynd", "nun", "die", "Kin\u00b7der", "sein", ",", "so", "wird", "er", "sie", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "ART", "NN", "VAINF", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "440\u201dUnd unsrer Feinde Rath durch diesen Schaz verr\u00fccken.", "tokens": ["\"", "Und", "uns\u00b7rer", "Fein\u00b7de", "Rath", "durch", "die\u00b7sen", "Schaz", "ver\u00b7r\u00fc\u00b7cken", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPOSAT", "NN", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201edie\u00df f\u00fchrt ich in dem Sinn. So werdet ihr das Amt,", "tokens": ["\u201e", "die\u00df", "f\u00fchrt", "ich", "in", "dem", "Sinn", ".", "So", "wer\u00b7det", "ihr", "das", "Amt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201evon dem so vieles Heil, so viele Wohlfart stammt,", "tokens": ["\u201e", "von", "dem", "so", "vie\u00b7les", "Heil", ",", "so", "vie\u00b7le", "Wohl\u00b7fart", "stammt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "ADV", "PIS", "NN", "$,", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201enach meiner Zuversicht so emsiger verrichten,", "tokens": ["\u201e", "nach", "mei\u00b7ner", "Zu\u00b7ver\u00b7sicht", "so", "em\u00b7si\u00b7ger", "ver\u00b7rich\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eals, Tugenden! ihr selbst euch wollt darzu verpflichten.", "tokens": ["\u201e", "als", ",", "Tu\u00b7gen\u00b7den", "!", "ihr", "selbst", "euch", "wollt", "dar\u00b7zu", "ver\u00b7pflich\u00b7ten", "."], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "$,", "NN", "$.", "PPER", "ADV", "PPER", "VMFIN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "445\u201dSonst, ich erkenn' es wohl, ist ausser diesem Saal", "tokens": ["\"", "Sonst", ",", "ich", "er\u00b7kenn'", "es", "wohl", ",", "ist", "aus\u00b7ser", "die\u00b7sem", "Saal"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eauch andrer Tugenden noch eine grosse Zahl,", "tokens": ["\u201e", "auch", "an\u00b7drer", "Tu\u00b7gen\u00b7den", "noch", "ei\u00b7ne", "gros\u00b7se", "Zahl", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201edie weder Danck noch Lohn von meiner Hand empfangen;", "tokens": ["\u201e", "die", "we\u00b7der", "Danck", "noch", "Lohn", "von", "mei\u00b7ner", "Hand", "emp\u00b7fan\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "KON", "NN", "ADV", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eauch diese sollen einst mit meinen Gaben prangen;", "tokens": ["\u201e", "auch", "die\u00b7se", "sol\u00b7len", "einst", "mit", "mei\u00b7nen", "Ga\u00b7ben", "pran\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PDS", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201ebesonders die gar oft in dieser Krieges-Zeit", "tokens": ["\u201e", "be\u00b7son\u00b7ders", "die", "gar", "oft", "in", "die\u00b7ser", "Krie\u00b7ge\u00b7sZeit"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ART", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "450\u201dDer Liebe meines Throns so Guth als Bluth geweiht.", "tokens": ["\"", "Der", "Lie\u00b7be", "mei\u00b7nes", "Throns", "so", "Guth", "als", "Bluth", "ge\u00b7weiht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PPOSAT", "NN", "ADV", "NN", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201evor, in, und nach dem Lauf der Furcht hab ich beschlossen:", "tokens": ["\u201e", "vor", ",", "in", ",", "und", "nach", "dem", "Lauf", "der", "Furcht", "hab", "ich", "be\u00b7schlos\u00b7sen", ":"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "APPR", "$,", "KON", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201eda\u00df, welche mir gen\u00fczt und nichts daf\u00fcr genossen,", "tokens": ["\u201e", "da\u00df", ",", "wel\u00b7che", "mir", "ge\u00b7n\u00fczt", "und", "nichts", "da\u00b7f\u00fcr", "ge\u00b7nos\u00b7sen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "$,", "PRELS", "PPER", "VVPP", "KON", "PIS", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201e(So lang Verfolgung, Noth, Gefahr und Schrecken rast,", "tokens": ["\u201e", "(", "So", "lang", "Ver\u00b7fol\u00b7gung", ",", "Noth", ",", "Ge\u00b7fahr", "und", "Schre\u00b7cken", "rast", ","], "token_info": ["punct", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "ADV", "ADJD", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201emacht sich ein Helden-Herz zu nichts als Muth gefa\u00dft)", "tokens": ["\u201e", "macht", "sich", "ein", "Hel\u00b7den\u00b7Herz", "zu", "nichts", "als", "Muth", "ge\u00b7fa\u00dft", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PRF", "ART", "NN", "APPR", "PIS", "KOKOM", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "455\u201dSo bald der L\u00e4nder Fried und Wohlseyn hergestellet,", "tokens": ["\"", "So", "bald", "der", "L\u00e4n\u00b7der", "Fried", "und", "Wohl\u00b7seyn", "her\u00b7ge\u00b7stel\u00b7let", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ART", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201eder Feinde Groll ged\u00e4mpft, und ihre Macht gef\u00e4llet,", "tokens": ["\u201e", "der", "Fein\u00b7de", "Groll", "ge\u00b7d\u00e4mpft", ",", "und", "ih\u00b7re", "Macht", "ge\u00b7f\u00e4l\u00b7let", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADJD", "VVPP", "$,", "KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "\u201eder Zeiten stiller Lauf gew\u00fcnschte Ruhe bringt,", "tokens": ["\u201e", "der", "Zei\u00b7ten", "stil\u00b7ler", "Lauf", "ge\u00b7w\u00fcnschte", "Ru\u00b7he", "bringt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "\u201eanstatt des Bluts ein Kranz das Krieger-Schwert umringt;", "tokens": ["\u201e", "an\u00b7statt", "des", "Bluts", "ein", "Kranz", "das", "Krie\u00b7ger\u00b7Schwert", "um\u00b7ringt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "\u201esie meines Zepters Huld und Gnad\u2019 erhalten sollen:", "tokens": ["\u201e", "sie", "mei\u00b7nes", "Zep\u00b7ters", "Huld", "und", "Gnad'", "er\u00b7hal\u00b7ten", "sol\u00b7len", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "PPOSAT", "NN", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "460\u201dIch mu\u00dfte bi\u00df daher, was ich nicht wollte, wollen.", "tokens": ["\"", "Ich", "mu\u00df\u00b7te", "bi\u00df", "da\u00b7her", ",", "was", "ich", "nicht", "woll\u00b7te", ",", "wol\u00b7len", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "PAV", "$,", "PWS", "PPER", "PTKNEG", "VMFIN", "$,", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "\u201ewo man um den Besiz ererbter Rechte sicht,", "tokens": ["\u201e", "wo", "man", "um", "den", "Be\u00b7siz", "er\u00b7erb\u00b7ter", "Rech\u00b7te", "sicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIS", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "\u201ewo es dem Feind an Recht und Billigkeit gebricht,", "tokens": ["\u201e", "wo", "es", "dem", "Feind", "an", "Recht", "und", "Bil\u00b7lig\u00b7keit", "ge\u00b7bricht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "\u201eda kann man leicht des Kriegs, Frucht, End und Folg ermessen.", "tokens": ["\u201e", "da", "kann", "man", "leicht", "des", "Kriegs", ",", "Frucht", ",", "End", "und", "Folg", "er\u00b7mes\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PIS", "ADJD", "ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "\u201egOtt schl\u00e4gt die, welche GOtt aus Herrsch-Begier vergessen.", "tokens": ["\u201e", "gOtt", "schl\u00e4gt", "die", ",", "wel\u00b7che", "Gott", "aus", "Herr\u00b7schBe\u00b7gier", "ver\u00b7ges\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "ART", "$,", "PWAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}