"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da wohnt Apollo nicht;", "tokens": ["Da", "wohnt", "A\u00b7pol\u00b7lo", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mund, Sinn und Hertze wancken", "tokens": ["Mund", ",", "Sinn", "und", "Hert\u00b7ze", "wan\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alle Lust gebricht.", "tokens": ["Und", "al\u00b7le", "Lust", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Wundsch doch bringt im Schreiben", "tokens": ["Mein", "Wund\u00b7sch", "doch", "bringt", "im", "Schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Den Mangel wider ein,", "tokens": ["Den", "Man\u00b7gel", "wi\u00b7der", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Wundsch, dem zu bekleiben", "tokens": ["Mein", "Wund\u00b7sch", ",", "dem", "zu", "be\u00b7klei\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der Himmel hold wird seyn.", "tokens": ["Der", "Him\u00b7mel", "hold", "wird", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Der Name S\u00e4nffteleben,", "tokens": ["Der", "Na\u00b7me", "S\u00e4nff\u00b7te\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den eure Liebste hat,", "tokens": ["Den", "eu\u00b7re", "Liebs\u00b7te", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wil mir die Hoffnung geben", "tokens": ["Wil", "mir", "die", "Hoff\u00b7nung", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der Sanfftmut in der That,", "tokens": ["Der", "Sanfft\u00b7mut", "in", "der", "That", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und solcher sch\u00f6nen Gaben,", "tokens": ["Und", "sol\u00b7cher", "sch\u00f6\u00b7nen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nechst Gottes Gunst, darzu,", "tokens": ["Nechst", "Got\u00b7tes", "Gunst", ",", "dar\u00b7zu", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "NN", "$,", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dadurch zwey Herzen haben", "tokens": ["Da\u00b7durch", "zwey", "Her\u00b7zen", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "CARD", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "De\u00df sanfften Lebens Ruh.", "tokens": ["De\u00df", "sanff\u00b7ten", "Le\u00b7bens", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sie wird sich fleissig m\u00fchen,", "tokens": ["Sie", "wird", "sich", "fleis\u00b7sig", "m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Euch rechter Trost zu seyn,", "tokens": ["Euch", "rech\u00b7ter", "Trost", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird mein' Geschwister ziehen", "tokens": ["Wird", "mein'", "Ge\u00b7schwis\u00b7ter", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur Tugend nur allein,", "tokens": ["Zur", "Tu\u00b7gend", "nur", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie heissen K\u00fcnste fassen", "tokens": ["Sie", "heis\u00b7sen", "K\u00fcns\u00b7te", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und gute Zucht zugleich;", "tokens": ["Und", "gu\u00b7te", "Zucht", "zu\u00b7gleich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wem Eltern di\u00df verlassen", "tokens": ["Wem", "El\u00b7tern", "di\u00df", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "NN", "PDS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der erbt ein K\u00f6nigreich.", "tokens": ["Der", "erbt", "ein", "K\u00f6\u00b7nig\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Mir wolle GOTT verleyhen", "tokens": ["Mir", "wol\u00b7le", "GoTT", "ver\u00b7ley\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NE", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein werthes Vatterland,", "tokens": ["Mein", "wert\u00b7hes", "Vat\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sch\u00f6nen W\u00fcsteneyen,", "tokens": ["Die", "sch\u00f6\u00b7nen", "W\u00fcs\u00b7te\u00b7ne\u00b7yen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Den klaren Boberstrand,", "tokens": ["Den", "kla\u00b7ren", "Bo\u00b7bers\u00b7trand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Euch und die lieben Meinen,", "tokens": ["Euch", "und", "die", "lie\u00b7ben", "Mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wann Rettung wird geschehn", "tokens": ["Wann", "Ret\u00b7tung", "wird", "ge\u00b7schehn"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und neue Zeit erscheinen,", "tokens": ["Und", "neu\u00b7e", "Zeit", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit Freuden anzusehn.", "tokens": ["Mit", "Freu\u00b7den", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}