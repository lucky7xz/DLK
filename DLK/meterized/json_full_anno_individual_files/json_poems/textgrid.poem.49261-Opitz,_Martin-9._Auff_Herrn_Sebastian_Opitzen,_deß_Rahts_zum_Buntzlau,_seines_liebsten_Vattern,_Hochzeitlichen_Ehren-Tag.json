{"textgrid.poem.49261": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "9. Auff Herrn Sebastian Opitzen, de\u00df Rahts zum Buntzlau, seines liebsten Vattern, Hochzeitlichen Ehren-Tag", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was wolt' ich lieber schreiben", "tokens": ["Was", "wolt'", "ich", "lie\u00b7ber", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als euch ein Ehren Lied,", "tokens": ["Als", "euch", "ein", "Eh\u00b7ren", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und solches einverleiben", "tokens": ["Und", "sol\u00b7ches", "ein\u00b7ver\u00b7lei\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Zeit, die immer bl\u00fcht?", "tokens": ["Der", "Zeit", ",", "die", "im\u00b7mer", "bl\u00fcht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr kennet mein Gem\u00fcte,", "tokens": ["Ihr", "ken\u00b7net", "mein", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O Vatter, gar zu wol!", "tokens": ["O", "Vat\u00b7ter", ",", "gar", "zu", "wol", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "PTKA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Kindliches Gebl\u00fcte", "tokens": ["Mein", "Kind\u00b7li\u00b7ches", "Ge\u00b7bl\u00fc\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist aller Treue voll.", "tokens": ["Ist", "al\u00b7ler", "Treu\u00b7e", "voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gleichwol mu\u00df ich bekennen,", "tokens": ["Gleich\u00b7wol", "mu\u00df", "ich", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich war nicht, der ich bin;", "tokens": ["Ich", "war", "nicht", ",", "der", "ich", "bin", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Geist wil nimmer brennen,", "tokens": ["Mein", "Geist", "wil", "nim\u00b7mer", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch steigen wie vorhin;", "tokens": ["Noch", "stei\u00b7gen", "wie", "vor\u00b7hin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Di\u00df thut f\u00fcr allen Sachen", "tokens": ["Di\u00df", "thut", "f\u00fcr", "al\u00b7len", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der