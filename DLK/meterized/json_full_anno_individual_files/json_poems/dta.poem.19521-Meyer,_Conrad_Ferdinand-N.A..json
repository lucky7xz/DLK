{"dta.poem.19521": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Tag, schein' herein! Die Kammer steht dir offen!", "tokens": ["Tag", ",", "schein'", "her\u00b7ein", "!", "Die", "Kam\u00b7mer", "steht", "dir", "of\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Holdsel'ger Lenzesmorgen, schein' herein!", "tokens": ["Hold\u00b7sel'\u00b7ger", "Len\u00b7zes\u00b7mor\u00b7gen", ",", "schein'", "her\u00b7ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Schon glitzert, von der Sonne Strahl getroffen,", "tokens": ["Schon", "glit\u00b7zert", ",", "von", "der", "Son\u00b7ne", "Strahl", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Tintenfa\u00df, der eichne B\u00fccherschrein.", "tokens": ["Das", "Tin\u00b7ten\u00b7fa\u00df", ",", "der", "eich\u00b7ne", "B\u00fc\u00b7cher\u00b7schrein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vogt Winter mu\u00df dem Lenze Rechnung geben,", "tokens": ["Vogt", "Win\u00b7ter", "mu\u00df", "dem", "Len\u00b7ze", "Rech\u00b7nung", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VMFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem sch\u00f6nen Erben \u00fcber Hof und Haus \u2014", "tokens": ["Dem", "sch\u00f6\u00b7nen", "Er\u00b7ben", "\u00fc\u00b7ber", "Hof", "und", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Auch mir zu gut geschrieben ist ein Leben \u2014", "tokens": ["Auch", "mir", "zu", "gut", "ge\u00b7schrie\u00b7ben", "ist", "ein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKA", "ADJD", "VVPP", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Tag, schein' herein und, Leben, flieh hinaus!", "tokens": ["Tag", ",", "schein'", "her\u00b7ein", "und", ",", "Le\u00b7ben", ",", "flieh", "hin\u00b7aus", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "KON", "$,", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich war von einem schweren Bann gebunden.", "tokens": ["Ich", "war", "von", "ei\u00b7nem", "schwe\u00b7ren", "Bann", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich lebte nicht. Ich lag im Traum erstarrt.", "tokens": ["Ich", "leb\u00b7te", "nicht", ".", "Ich", "lag", "im", "Traum", "er\u00b7starrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "PPER", "VVFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von vielen tausend unverbrauchten Stunden", "tokens": ["Von", "vie\u00b7len", "tau\u00b7send", "un\u00b7ver\u00b7brauch\u00b7ten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schwillt ungest\u00fcm mir nun die Gegenwart.", "tokens": ["Schwillt", "un\u00b7ge\u00b7st\u00fcm", "mir", "nun", "die", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aus dunkelm Grunde gr\u00fcne Saat zu wecken", "tokens": ["Aus", "dun\u00b7kelm", "Grun\u00b7de", "gr\u00fc\u00b7ne", "Saat", "zu", "we\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bedarf es Sonnenstrahles nur und Thaus,", "tokens": ["Be\u00b7darf", "es", "Son\u00b7nen\u00b7strah\u00b7les", "nur", "und", "Thaus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "ADV", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich f\u00fchle wie sich tausend Keime strecken.", "tokens": ["Ich", "f\u00fch\u00b7le", "wie", "sich", "tau\u00b7send", "Kei\u00b7me", "stre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PRF", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Tag, schein' herein und, Leben, flieh hinaus!", "tokens": ["Tag", ",", "schein'", "her\u00b7ein", "und", ",", "Le\u00b7ben", ",", "flieh", "hin\u00b7aus", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "KON", "$,", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Segel zieht auf wunderk\u00fchlen Pfaden,", "tokens": ["Ein", "Se\u00b7gel", "zieht", "auf", "wun\u00b7der\u00b7k\u00fch\u00b7len", "Pfa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Flutendunkel spiegelt sich der Tag.", "tokens": ["In", "Flu\u00b7ten\u00b7dun\u00b7kel", "spie\u00b7gelt", "sich", "der", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was hat die Barke dort f\u00fcr mich geladen?", "tokens": ["Was", "hat", "die", "Bar\u00b7ke", "dort", "f\u00fcr", "mich", "ge\u00b7la\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vielleicht ist's etwas das mich freuen mag!", "tokens": ["Viel\u00b7leicht", "ist's", "et\u00b7was", "das", "mich", "freu\u00b7en", "mag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Entgegen ihr! Was wird die Barke bringen", "tokens": ["Ent\u00b7ge\u00b7gen", "ihr", "!", "Was", "wird", "die", "Bar\u00b7ke", "brin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "$.", "PWS", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durch blauer Wellen freudiges Gebraus?", "tokens": ["Durch", "blau\u00b7er", "Wel\u00b7len", "freu\u00b7di\u00b7ges", "Ge\u00b7braus", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Entgegen ihr! Mit weitgestreckten Schwingen!", "tokens": ["Ent\u00b7ge\u00b7gen", "ihr", "!", "Mit", "weit\u00b7ge\u00b7streck\u00b7ten", "Schwin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "$.", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Tag, schein' herein und, Leben, flieh hinaus!", "tokens": ["Tag", ",", "schein'", "her\u00b7ein", "und", ",", "Le\u00b7ben", ",", "flieh", "hin\u00b7aus", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "KON", "$,", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}