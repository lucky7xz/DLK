{"textgrid.poem.26134": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Unter bl\u00fchenden B\u00e4umen kann alles geschehen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel bl\u00fchende B\u00e4ume aufgebaut stehen,", "tokens": ["Viel", "bl\u00fc\u00b7hen\u00b7de", "B\u00e4u\u00b7me", "auf\u00b7ge\u00b7baut", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Landstra\u00dfe ist wie ein Ballhaus zu sehen.", "tokens": ["Die", "Land\u00b7stra\u00b7\u00dfe", "ist", "wie", "ein", "Ball\u00b7haus", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-++-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo sich wei\u00dfgekleidete M\u00e4dchen drehen.", "tokens": ["Wo", "sich", "wei\u00df\u00b7ge\u00b7klei\u00b7de\u00b7te", "M\u00e4d\u00b7chen", "dre\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.2": {"line.1": {"text": "Zwischen Himmel und Erde jetzt Feste vorgehen,", "tokens": ["Zwi\u00b7schen", "Him\u00b7mel", "und", "Er\u00b7de", "jetzt", "Fes\u00b7te", "vor\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "NN", "VVINF", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wo wie Hochzeitsfahnen die B\u00e4ume wehen,", "tokens": ["Wo", "wie", "Hoch\u00b7zeits\u00b7fah\u00b