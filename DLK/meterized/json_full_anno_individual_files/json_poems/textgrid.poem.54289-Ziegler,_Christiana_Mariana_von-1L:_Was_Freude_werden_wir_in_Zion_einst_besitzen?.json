{"textgrid.poem.54289": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was Freude werden wir in Zion einst besitzen?", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Freude werden wir in Zion einst besitzen?", "tokens": ["Was", "Freu\u00b7de", "wer\u00b7den", "wir", "in", "Zi\u00b7on", "einst", "be\u00b7sit\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "PPER", "APPR", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Daselbst wird keine Noth auf unsre Scheitel blitzen,", "tokens": ["Da\u00b7selbst", "wird", "kei\u00b7ne", "Noth", "auf", "uns\u00b7re", "Schei\u00b7tel", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Kummer-Faden wird so dann gerissen seyn,", "tokens": ["Der", "Kum\u00b7mer\u00b7Fa\u00b7den", "wird", "so", "dann", "ge\u00b7ris\u00b7sen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Auserwehlten Schaar wei\u00df nichts von Schmertz und Pein.", "tokens": ["Der", "Au\u00b7ser\u00b7wehl\u00b7ten", "Schaar", "wei\u00df", "nichts", "von", "Schmertz", "und", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PIS", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bedenckt es, Sterblichen, was ist das Rund der Erden?", "tokens": ["Be\u00b7denckt", "es", ",", "Sterb\u00b7li\u00b7chen", ",", "was", "ist", "das", "Rund", "der", "Er\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "PWS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein grosses Hospital, ein Zeughau\u00df der Beschwerden,", "tokens": ["Ein", "gros\u00b7ses", "Hos\u00b7pi\u00b7tal", ",", "ein", "Zeug\u00b7hau\u00df", "der", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein Paradie\u00df, das zwar sch\u00f6n in die Augen f\u00e4llt,", "tokens": ["Ein", "Pa\u00b7ra\u00b7die\u00df", ",", "das", "zwar", "sch\u00f6n", "in", "die", "Au\u00b7gen", "f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Das aber auch zugleich viel Schlangen in sich h\u00e4lt.", "tokens": ["Das", "a\u00b7ber", "auch", "zu\u00b7gleich", "viel", "Schlan\u00b7gen", "in", "sich", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADV", "PIAT", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dort aber wohnet man in sichern Friedens-H\u00e4usern,", "tokens": ["Dort", "a\u00b7ber", "woh\u00b7net", "man", "in", "si\u00b7chern", "Frie\u00b7dens\u00b7H\u00e4u\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Statt D\u00f6rnern tragen wir uns nur mit Palmen-Reisern,", "tokens": ["Statt", "D\u00f6r\u00b7nern", "tra\u00b7gen", "wir", "uns", "nur", "mit", "Pal\u00b7men\u00b7Rei\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "PRF", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Kein Joch beschwert den Hal\u00df, die Steine fallen ab,", "tokens": ["Kein", "Joch", "be\u00b7schwert", "den", "Hal\u00df", ",", "die", "Stei\u00b7ne", "fal\u00b7len", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Aus diesen haut man uns im Sterben unser Grab.", "tokens": ["Aus", "die\u00b7sen", "haut", "man", "uns", "im", "Ster\u00b7ben", "un\u00b7ser", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PIS", "PPER", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da kan der morsche Leib still und geruhig schlafen,", "tokens": ["Da", "kan", "der", "mor\u00b7sche", "Leib", "still", "und", "ge\u00b7ru\u00b7hig", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "PTKVZ", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und unsre Seele kommt in den gew\u00fcnschten Hafen,", "tokens": ["Und", "uns\u00b7re", "See\u00b7le", "kommt", "in", "den", "ge\u00b7w\u00fcnschten", "Ha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Die auf dem wilden Meer der Erden, mit Verdru\u00df,", "tokens": ["Die", "auf", "dem", "wil\u00b7den", "Meer", "der", "Er\u00b7den", ",", "mit", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Bey St\u00fcrmen hin und her best\u00e4ndig kreutzen mu\u00df.", "tokens": ["Bey", "St\u00fcr\u00b7men", "hin", "und", "her", "be\u00b7st\u00e4n\u00b7dig", "kreut\u00b7zen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "KON", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wer wolte wohl daher der Welt den Vorzug g\u00f6nnen.", "tokens": ["Wer", "wol\u00b7te", "wohl", "da\u00b7her", "der", "Welt", "den", "Vor\u00b7zug", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PAV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da wir in stoltzer Ruh dereinst dort leben k\u00f6nnen.", "tokens": ["Da", "wir", "in", "stolt\u00b7zer", "Ruh", "de\u00b7reinst", "dort", "le\u00b7ben", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ADV", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Flieht, blinde Menschen, doch den Tand der Eitelkeit,", "tokens": ["Flieht", ",", "blin\u00b7de", "Men\u00b7schen", ",", "doch", "den", "Tand", "der", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Freude dauret nur auf eine kurtze Zeit.", "tokens": ["Die", "Freu\u00b7de", "dau\u00b7ret", "nur", "auf", "ei\u00b7ne", "kurt\u00b7ze", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wer sich im Zauber-Gla\u00df der Welt so sehr bespiegelt,", "tokens": ["Wer", "sich", "im", "Zau\u00b7ber\u00b7Gla\u00df", "der", "Welt", "so", "sehr", "be\u00b7spie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPRART", "NN", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dem wird in Salem einst so Th\u00fcr als Thor verriegelt.", "tokens": ["Dem", "wird", "in", "Sa\u00b7lem", "einst", "so", "Th\u00fcr", "als", "Thor", "ver\u00b7rie\u00b7gelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NE", "ADV", "ADV", "NN", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Vergafft euch nicht so sehr im Scharlach dieser Welt,", "tokens": ["Ver\u00b7gafft", "euch", "nicht", "so", "sehr", "im", "Schar\u00b7lach", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "APPRART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Weil selbiger doch nicht best\u00e4ndig Farbe h\u00e4lt.", "tokens": ["Weil", "sel\u00b7bi\u00b7ger", "doch", "nicht", "be\u00b7st\u00e4n\u00b7dig", "Far\u00b7be", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "PTKNEG", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wen heut das Gl\u00fccke k\u00fcst, dem wirfft es morgen nieder,", "tokens": ["Wen", "heut", "das", "Gl\u00fc\u00b7cke", "k\u00fcst", ",", "dem", "wirfft", "es", "mor\u00b7gen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der ihm im Schoose sa\u00df, singt jetzo Klage-Lieder;", "tokens": ["Der", "ihm", "im", "Schoo\u00b7se", "sa\u00df", ",", "singt", "jet\u00b7zo", "Kla\u00b7ge\u00b7Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wer fr\u00fch sich starck befind, der liegt offt Abends kranck,", "tokens": ["Wer", "fr\u00fch", "sich", "starck", "be\u00b7find", ",", "der", "liegt", "offt", "A\u00b7bends", "kranck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PRF", "ADJD", "VVPP", "$,", "PRELS", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ihr hofft auf Fried und Ruh, und findet Streit und Zanck.", "tokens": ["Ihr", "hofft", "auf", "Fried", "und", "Ruh", ",", "und", "fin\u00b7det", "Streit", "und", "Zanck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,", "KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der heute Freundschafft macht, will morgen wieder brechen,", "tokens": ["Der", "heu\u00b7te", "Freund\u00b7schafft", "macht", ",", "will", "mor\u00b7gen", "wie\u00b7der", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und sich durch Spott und Hohn an uns gewaltig r\u00e4chen.", "tokens": ["Und", "sich", "durch", "Spott", "und", "Hohn", "an", "uns", "ge\u00b7wal\u00b7tig", "r\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "NN", "KON", "NN", "APPR", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Heucheley der Welt und Falschheit ist zu gro\u00df,", "tokens": ["Die", "Heu\u00b7che\u00b7ley", "der", "Welt", "und", "Falschheit", "ist", "zu", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.32": {"text": "Auch in der Freundlichkeit f\u00fchlt man offt Joabs Sto\u00df.", "tokens": ["Auch", "in", "der", "Freund\u00b7lich\u00b7keit", "f\u00fchlt", "man", "offt", "Joabs", "Sto\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PIS", "ADV", "NE", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Wohl euch! die ihr der Welt den Scheide-Brief bald gebet,", "tokens": ["Wohl", "euch", "!", "die", "ihr", "der", "Welt", "den", "Schei\u00b7de\u00b7Brief", "bald", "ge\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$.", "PRELS", "PPER", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und nicht den Schnecken gleich an Kedars H\u00fctten klebet,", "tokens": ["Und", "nicht", "den", "Schne\u00b7cken", "gleich", "an", "Ke\u00b7dars", "H\u00fct\u00b7ten", "kle\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "ADV", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der Tausch gereut euch nicht, den ihr begl\u00fcckt gethan,", "tokens": ["Der", "Tausch", "ge\u00b7reut", "euch", "nicht", ",", "den", "ihr", "be\u00b7gl\u00fcckt", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "PPER", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ihr trefft in Sion dort was unverg\u00e4nglichs an.", "tokens": ["Ihr", "trefft", "in", "Si\u00b7on", "dort", "was", "un\u00b7ver\u00b7g\u00e4ng\u00b7lichs", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "ADV", "PWS", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was Freude werden wir in Zion einst besitzen?", "tokens": ["Was", "Freu\u00b7de", "wer\u00b7den", "wir", "in", "Zi\u00b7on", "einst", "be\u00b7sit\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "PPER", "APPR", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Daselbst wird keine Noth auf unsre Scheitel blitzen,", "tokens": ["Da\u00b7selbst", "wird", "kei\u00b7ne", "Noth", "auf", "uns\u00b7re", "Schei\u00b7tel", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Kummer-Faden wird so dann gerissen seyn,", "tokens": ["Der", "Kum\u00b7mer\u00b7Fa\u00b7den", "wird", "so", "dann", "ge\u00b7ris\u00b7sen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Auserwehlten Schaar wei\u00df nichts von Schmertz und Pein.", "tokens": ["Der", "Au\u00b7ser\u00b7wehl\u00b7ten", "Schaar", "wei\u00df", "nichts", "von", "Schmertz", "und", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PIS", "APPR",