{"textgrid.poem.52951": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du h\u00f6rtest wohl die m\u00e4rchenhafte Kunde", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du h\u00f6rtest wohl die m\u00e4rchenhafte Kunde", "tokens": ["Du", "h\u00f6r\u00b7test", "wohl", "die", "m\u00e4r\u00b7chen\u00b7haf\u00b7te", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von einer Stadt am Meere, die vor Jahren", "tokens": ["Von", "ei\u00b7ner", "Stadt", "am", "Mee\u00b7re", ",", "die", "vor", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch eine Sturmflut ward hinabgeschlungen?", "tokens": ["Durch", "ei\u00b7ne", "Sturm\u00b7flut", "ward", "hin\u00b7ab\u00b7ge\u00b7schlun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch blinkt es oft und wallt herauf vom Grunde,", "tokens": ["Noch", "blinkt", "es", "oft", "und", "wallt", "her\u00b7auf", "vom", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wenn die Schiffer sonntags dr\u00fcberfahren,", "tokens": ["Und", "wenn", "die", "Schif\u00b7fer", "sonn\u00b7tags", "dr\u00fc\u00b7berf\u00b7ah\u00b7ren", "