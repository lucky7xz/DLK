, "tokens": ["Als", "nur", "ein", "lei\u00b7ses", "Wal\u00b7len", "o\u00b7der", "Blin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Noch treib ich leicht und selig auf der Welle,", "tokens": ["Noch", "treib", "ich", "leicht", "und", "se\u00b7lig", "auf", "der", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Begl\u00e4nzt von Deiner Augen Sternenhelle.", "tokens": ["Be\u00b7gl\u00e4nzt", "von", "Dei\u00b7ner", "Au\u00b7gen", "Ster\u00b7nen\u00b7hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du h\u00f6rtest wohl die m\u00e4rchenhafte Kunde", "tokens": ["Du", "h\u00f6r\u00b7test", "wohl", "die", "m\u00e4r\u00b7chen\u00b7haf\u00b7te", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von einer Stadt am Meere, die vor Jahren", "tokens": ["Von", "ei\u00b7ner", "Stadt", "am", "Mee\u00b7re", ",", "die", "vor", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch eine Sturmflut ward hinabgeschlungen?", "tokens": ["Durch", "ei\u00b7ne", "Sturm\u00b7flut", "ward", "hin\u00b7ab\u00b7ge\u00b7schlun\u00b7gen", "?"], "token_info": ["word", "wo