{"textgrid.poem.43952": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erinnert euch mit mir, ihr Blumen, B\u00e4um und Schatten,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erinnert euch mit mir, ihr Blumen, B\u00e4um und Schatten,", "tokens": ["E\u00b7rin\u00b7nert", "euch", "mit", "mir", ",", "ihr", "Blu\u00b7men", ",", "B\u00e4um", "und", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der oft mit Flavien gehaltnen Abendlust!", "tokens": ["Der", "oft", "mit", "Fla\u00b7vi\u00b7en", "ge\u00b7halt\u00b7nen", "A\u00b7bend\u00b7lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die B\u00e4che glei\u00dfen noch von Flammen treuer Brust,", "tokens": ["Die", "B\u00e4\u00b7che", "glei\u00b7\u00dfen", "noch", "von", "Flam\u00b7men", "treu\u00b7er", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In der wir werthes Paar des Himmels Vorschmack hatten.", "tokens": ["In", "der", "wir", "wert\u00b7hes", "Paar", "des", "Him\u00b7mels", "Vor\u00b7schmack", "hat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJA", "NN", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O g\u00f6ldne Fr\u00fchlingszeit! Mein Herz, was kommt dir ein?", "tokens": ["O", "g\u00f6ld\u00b7ne", "Fr\u00fch\u00b7lings\u00b7zeit", "!", "Mein", "Herz", ",", "was", "kommt", "dir", "ein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "PPOSAT", "NN", "$,", "PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du liebest Flavien, sie ist ja nicht mehr dein.", "tokens": ["Du", "lie\u00b7best", "Fla\u00b7vi\u00b7en", ",", "sie", "ist", "ja", "nicht", "mehr", "dein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hier war es, wo ihr Haupt mir oft die Achsel dr\u00fcckte,", "tokens": ["Hier", "war", "es", ",", "wo", "ihr", "Haupt", "mir", "oft", "die", "Ach\u00b7sel", "dr\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verschweigt, ihr Linden, mehr, als ich nicht sagen darf;", "tokens": ["Ver\u00b7schweigt", ",", "ihr", "Lin\u00b7den", ",", "mehr", ",", "als", "ich", "nicht", "sa\u00b7gen", "darf", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPOSAT", "NN", "$,", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier war es, wo sie mich mit Klee und Quendel warf", "tokens": ["Hier", "war", "es", ",", "wo", "sie", "mich", "mit", "Klee", "und", "Quen\u00b7del", "warf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWAV", "PPER", "PRF", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wo ich ihr die Schoos voll junger Bl\u00fcthen pfl\u00fcckte.", "tokens": ["Und", "wo", "ich", "ihr", "die", "Schoos", "voll", "jun\u00b7ger", "Bl\u00fc\u00b7then", "pfl\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "ART", "NN", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da war noch gute Zeit. Mein Herz, was kommt dir ein?", "tokens": ["Da", "war", "noch", "gu\u00b7te", "Zeit", ".", "Mein", "Herz", ",", "was", "kommt", "dir", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJA", "NN", "$.", "PPOSAT", "NN", "$,", "PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Betr\u00fcbt dich Flavia? Sie ist ja nicht mehr dein.", "tokens": ["Be\u00b7tr\u00fcbt", "dich", "Fla\u00b7via", "?", "Sie", "ist", "ja", "nicht", "mehr", "dein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NE", "$.", "PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "PPOSAT", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Erinnert euch mit mir, ihr Blumen, B\u00e4um u