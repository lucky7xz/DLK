{"dta.poem.778": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XiV.  \n Beschlu\u00df de\u00df  XXIV.  Jahrs.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Sch\u00f6pffer/ der du mich so wunderlich gemacht. ", "tokens": ["Mein", "Sch\u00f6pf\u00b7fer", "/", "der", "du", "mich", "so", "wun\u00b7der\u00b7lich", "ge\u00b7macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erl\u00f6ser/ der du mich zu deinem Kind erkohren/", "tokens": ["Er\u00b7l\u00f6\u00b7ser", "/", "der", "du", "mich", "zu", "dei\u00b7nem", "Kind", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "GoTT reiner Geist/ der du mich durch dich new-gebohren", "tokens": ["GoTT", "rei\u00b7ner", "Geist", "/", "der", "du", "mich", "durch", "dich", "ne\u00b7wge\u00b7boh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$(", "PRELS", "PPER", "PRF", "APPR", "PPER", "ADJA"], "meter": "---+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dreyeinig Einigkeit/ vmb die manch tausend wacht:", "tokens": ["Drey\u00b7ei\u00b7nig", "Ei\u00b7nig\u00b7keit", "/", "vmb", "die", "manch", "tau\u00b7send", "wacht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$(", "APPR", "ART", "PIAT", "CARD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "GoTt/ den manch tausend r\u00fchmt mit heilighoher pracht/", "tokens": ["GoTt", "/", "den", "manch", "tau\u00b7send", "r\u00fchmt", "mit", "hei\u00b7lig\u00b7ho\u00b7her", "pracht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "PIAT", "CARD", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ich falle dir zu Fue\u00df/ der/ da\u00df ich nicht verlohren/", "tokens": ["Ich", "fal\u00b7le", "dir", "zu", "Fu\u00b7e\u00df", "/", "der", "/", "da\u00df", "ich", "nicht", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NE", "$(", "ART", "$(", "KOUS", "PPER", "PTKNEG", "VVPP", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nur dir zu dancken wei\u00df. Dem/ wie du dich verschworen/", "tokens": ["Nur", "dir", "zu", "dan\u00b7cken", "wei\u00df", ".", "Dem", "/", "wie", "du", "dich", "ver\u00b7schwo\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKZU", "VVINF", "VVFIN", "$.", "ART", "$(", "PWAV", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Anblick h\u00f6her lust in hoher angst gebracht:", "tokens": ["Dein", "An\u00b7blick", "h\u00f6\u00b7her", "lust", "in", "ho\u00b7her", "angst", "ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich ward durch dich/ durch dich hab ich di\u00df licht beschawet", "tokens": ["Ich", "ward", "durch", "dich", "/", "durch", "dich", "hab", "ich", "di\u00df", "licht", "be\u00b7scha\u00b7wet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$(", "APPR", "PPER", "VAFIN", "PPER", "PDS", "NN", "VVFIN"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du hast au\u00df so viel angst ob der mir jtzt noch grawet", "tokens": ["Du", "hast", "au\u00df", "so", "viel", "angst", "ob", "der", "mir", "jtzt", "noch", "gra\u00b7wet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "ADV", "VVPP", "KOUS", "ART", "PPER", "ADV", "ADV", "VVFIN"], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vnd dieses Jahr so offt/ mich von der grufft befrey\u2019t.", "tokens": ["Vnd", "die\u00b7ses", "Jahr", "so", "offt", "/", "mich", "von", "der", "grufft", "be\u00b7frey'", "t."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PDAT", "NN", "ADV", "ADV", "$(", "PPER", "APPR", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "La\u00df ferner \u00fcber mir die reiche G\u00fctte schweben/", "tokens": ["La\u00df", "fer\u00b7ner", "\u00fc\u00b7ber", "mir", "die", "rei\u00b7che", "G\u00fct\u00b7te", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df mich dir/ weil ich leb/ vnd wenn ich sterbe leben", "tokens": ["La\u00df", "mich", "dir", "/", "weil", "ich", "leb", "/", "vnd", "wenn", "ich", "ster\u00b7be", "le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPER", "$(", "KOUS", "PPER", "VVFIN", "$(", "KON", "KOUS", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd wenn mir zeit abgeht/ verley die Ewigkeit.", "tokens": ["Vnd", "wenn", "mir", "zeit", "ab\u00b7geht", "/", "ver\u00b7ley", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN", "$(", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}