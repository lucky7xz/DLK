{"dta.poem.14432": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Nachtigall'n aus gr\u00fcnen Hallen schallen,", "tokens": ["Wenn", "Nach\u00b7ti\u00b7gall'n", "aus", "gr\u00fc\u00b7nen", "Hal\u00b7len", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wen m\u00f6chten nicht die tiefen T\u00f6ne r\u00fchren;", "tokens": ["Wen", "m\u00f6ch\u00b7ten", "nicht", "die", "tie\u00b7fen", "T\u00f6\u00b7ne", "r\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wen nicht das s\u00fc\u00dfe Herzeleid verf\u00fchren,", "tokens": ["Wen", "nicht", "das", "s\u00fc\u00b7\u00dfe", "Her\u00b7ze\u00b7leid", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Liebesschlagen todt vom Baum zu fallen? \u2014", "tokens": ["Im", "Lie\u00b7bes\u00b7schla\u00b7gen", "todt", "vom", "Baum", "zu", "fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}