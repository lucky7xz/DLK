{"dta.poem.12421": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Doppelte Liebe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht lang es ist,               ", "tokens": ["Nicht", "lang", "es", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "In Fastnacht-Frist,", "tokens": ["In", "Fast\u00b7nacht\u00b7Frist", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Hab ich mir auserkoren,", "tokens": ["Hab", "ich", "mir", "au\u00b7ser\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zwey Jungfraun zart,", "tokens": ["Zwey", "Jung\u00b7fraun", "zart", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Von guter Art", "tokens": ["Von", "gu\u00b7ter", "Art"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und tugendlich geboren.", "tokens": ["Und", "tu\u00b7gend\u00b7lich", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Am Abend spat", "tokens": ["Am", "A\u00b7bend", "spat"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Schneewei\u00df ihr Waat,", "tokens": ["Schnee\u00b7wei\u00df", "ihr", "Waat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Durchaus ganz wohlgezieret,", "tokens": ["Durc\u00b7haus", "ganz", "wohl\u00b7ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich ihnen gern", "tokens": ["Ich", "ih\u00b7nen", "gern"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In Zucht und Ehrn", "tokens": ["In", "Zucht", "und", "Ehrn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Gef\u00e4llig h\u00e4tt' hofieret.", "tokens": ["Ge\u00b7f\u00e4l\u00b7lig", "h\u00e4tt'", "ho\u00b7fie\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch durft ich nicht,", "tokens": ["Doch", "durft", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Dieweil es Sitt", "tokens": ["Die\u00b7weil", "es", "Sitt"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein jeder Zeit zu halten;", "tokens": ["Ein", "je\u00b7der", "Zeit", "zu", "hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach Klagens Brauch", "tokens": ["Nach", "Kla\u00b7gens", "Brauch"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Darum ich auch", "tokens": ["Da\u00b7rum", "ich", "auch"], "token_info": ["word", "word", "word"], "pos": ["PAV", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Den lieben Gott lie\u00df walten.", "tokens": ["Den", "lie\u00b7ben", "Gott", "lie\u00df", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und schm\u00fcckt mich sehr,", "tokens": ["Und", "schm\u00fcckt", "mich", "sehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Als ob ich w\u00e4r,", "tokens": ["Als", "ob", "ich", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein Sohn der armen Frauen,", "tokens": ["Ein", "Sohn", "der", "ar\u00b7men", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit kleinem Ruhm,", "tokens": ["Mit", "klei\u00b7nem", "Ruhm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Recht wie die Blum", "tokens": ["Recht", "wie", "die", "Blum"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NE"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Den Winter in der Auen.", "tokens": ["Den", "Win\u00b7ter", "in", "der", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Vor beyder Th\u00fcr", "tokens": ["Vor", "bey\u00b7der", "Th\u00fcr"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ich stehe hier,", "tokens": ["Ich", "ste\u00b7he", "hier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So zwischen beyden Frauen,", "tokens": ["So", "zwi\u00b7schen", "bey\u00b7den", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ganz gr\u00e4mlich schier,", "tokens": ["Ganz", "gr\u00e4m\u00b7lich", "schier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wies M\u00fcllerthier", "tokens": ["Wies", "M\u00fcl\u00b7ler\u00b7thier"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Zwey B\u00fcndel Heu mag schauen.", "tokens": ["Zwey", "B\u00fcn\u00b7del", "Heu", "mag", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Schleich auf den Zehn", "tokens": ["Schleich", "auf", "den", "Zehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "CARD"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Zum Schlafen gehn,", "tokens": ["Zum", "Schla\u00b7fen", "gehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Vor gro\u00dfem Leid und Kummer;", "tokens": ["Vor", "gro\u00b7\u00dfem", "Leid", "und", "Kum\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In dem bedacht", "tokens": ["In", "dem", "be\u00b7dacht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In selbig Nacht", "tokens": ["In", "sel\u00b7big", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJD", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Den sch\u00f6n und edlen Sommer.", "tokens": ["Den", "sch\u00f6n", "und", "ed\u00b7len", "Som\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "In kurzer Zeit", "tokens": ["In", "kur\u00b7zer", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Er breitet weit", "tokens": ["Er", "brei\u00b7tet", "weit"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Blum auf gr\u00fcner Heiden,", "tokens": ["Die", "Blum", "auf", "gr\u00fc\u00b7ner", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Manch sch\u00f6nen Strauch,", "tokens": ["Manch", "sch\u00f6\u00b7nen", "Strauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Darin ich auch", "tokens": ["Da\u00b7rin", "ich", "auch"], "token_info": ["word", "word", "word"], "pos": ["PAV", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mich hoff mit Lust zu weiden.", "tokens": ["Mich", "hoff", "mit", "Lust", "zu", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}