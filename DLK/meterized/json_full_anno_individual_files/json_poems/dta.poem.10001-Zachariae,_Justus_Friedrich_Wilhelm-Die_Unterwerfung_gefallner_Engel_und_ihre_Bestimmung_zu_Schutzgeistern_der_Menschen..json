{"dta.poem.10001": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Unterwerfung gefallner Engel  \n und ihre Bestimmung  \n zu  \n Schutzgeistern der Menschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und sie zogen hinab. Mit welchem entz\u00fcckten", "tokens": ["Und", "sie", "zo\u00b7gen", "hin\u00b7ab", ".", "Mit", "wel\u00b7chem", "ent\u00b7z\u00fcck\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$.", "APPR", "PWAT", "ADJA"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sah Orion der Sch\u00f6pfung Gesicht, die stralenden", "tokens": ["Sah", "O\u00b7rion", "der", "Sch\u00f6p\u00b7fung", "Ge\u00b7sicht", ",", "die", "stra\u00b7len\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NN", "ART", "NN", "NN", "$,", "ART", "ADJA"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und die hellen Planeten! mit welcher Begeistrung ver-", "tokens": ["Und", "die", "hel\u00b7len", "Pla\u00b7ne\u00b7ten", "!", "mit", "wel\u00b7cher", "Be\u00b7geis\u00b7trung", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$.", "APPR", "PWAT", "NN", "TRUNC"], "meter": "--+-+---+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Ges\u00e4nge der Sph\u00e4ren! Sie flogen durch zahllose", "tokens": ["Die", "Ge\u00b7s\u00e4n\u00b7ge", "der", "Sph\u00e4\u00b7ren", "!", "Sie", "flo\u00b7gen", "durch", "zahl\u00b7lo\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "APPR", "ADJA"], "meter": "--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Bis zu unserm Sonnensystem. Der silberne Mond", "tokens": ["Bis", "zu", "un\u00b7serm", "Son\u00b7nen\u00b7sys\u00b7tem", ".", "Der", "sil\u00b7ber\u00b7ne", "Mond"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Leuchtend \u00fcber der Erde, Dies ist sie, die k\u00fcnftige", "tokens": ["Leuch\u00b7tend", "\u00fc\u00b7ber", "der", "Er\u00b7de", ",", "Dies", "ist", "sie", ",", "die", "k\u00fcnf\u00b7ti\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "PDS", "VAFIN", "PPER", "$,", "ART", "ADJA"], "meter": "+-+--+-+---+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Euch vom Sch\u00f6pfer bestimmt, (sprach Gabriel;) bald", "tokens": ["Euch", "vom", "Sch\u00f6p\u00b7fer", "be\u00b7stimmt", ",", "(", "sprach", "Ga\u00b7bri\u00b7el", ";)", "bald"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "emoticon", "word"], "pos": ["PPER", "APPRART", "NN", "VVPP", "$,", "$(", "VVFIN", "NE", "$(", "ADV"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Gott dich zur Erde herunter berufen, dem Ersten der", "tokens": ["Gott", "dich", "zur", "Er\u00b7de", "her\u00b7un\u00b7ter", "be\u00b7ru\u00b7fen", ",", "dem", "Ers\u00b7ten", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PRF", "APPRART", "NN", "APZR", "VVPP", "$,", "ART", "NN", "ART"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.9": {"text": "Dich zum Schutzgeist zu geben; ich eile hinab nach der", "tokens": ["Dich", "zum", "Schutz\u00b7geist", "zu", "ge\u00b7ben", ";", "ich", "ei\u00b7le", "hin\u00b7ab", "nach", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "PTKZU", "VVINF", "$.", "PPER", "VVFIN", "ADV", "APPR", "ART"], "meter": "+-+--+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Von des Allm\u00e4chtgen Sohn die fernern Befehle zu h\u00f6ren.", "tokens": ["Von", "des", "All\u00b7m\u00e4cht\u00b7gen", "Sohn", "die", "fer\u00b7nern", "Be\u00b7feh\u00b7le", "zu", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}}}}}