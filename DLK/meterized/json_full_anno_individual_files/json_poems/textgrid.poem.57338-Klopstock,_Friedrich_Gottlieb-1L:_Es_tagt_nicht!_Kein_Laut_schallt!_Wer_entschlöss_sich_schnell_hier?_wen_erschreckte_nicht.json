{"textgrid.poem.57338": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es tagt nicht! Kein Laut schallt! Wer entschl\u00f6ss sich schnell hier? wen erschreckte nicht", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es tagt nicht! Kein Laut schallt! Wer entschl\u00f6ss sich schnell hier? wen erschreckte nicht", "tokens": ["Es", "tagt", "nicht", "!", "Kein", "Laut", "schallt", "!", "Wer", "ent\u00b7schl\u00f6ss", "sich", "schnell", "hier", "?", "wen", "er\u00b7schreck\u00b7te", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "PIAT", "APPR", "VVFIN", "$.", "PWS", "VVFIN", "PRF", "ADJD", "ADV", "$.", "PWS", "VVFIN", "PTKNEG"], "meter": "-+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Das Graunvolle der Wahl?", "tokens": ["Das", "Graun\u00b7vol\u00b7le", "der", "Wahl", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Doch sie sey dein Schicksal; du erk\u00f6hrst doch Blindheit? Des Geh\u00f6rs Verlust", "tokens": ["Doch", "sie", "sey", "dein", "Schick\u00b7sal", ";", "du", "er\u00b7k\u00f6hrst", "doch", "Blind\u00b7heit", "?", "Des", "Ge\u00b7h\u00f6rs", "Ver\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "NN", "$.", "ART", "NN", "NN"], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Vereinsamt, und du lebst", "tokens": ["Ver\u00b7ein\u00b7samt", ",", "und", "du", "lebst"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "KON", "PPER", "VVFIN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Mit den Menschen nicht mehr. Wenn du also kein Gott bist: so w\u00e4hlst du recht,", "tokens": ["Mit", "den", "Men\u00b7schen", "nicht", "mehr", ".", "Wenn", "du", "al\u00b7so", "kein", "Gott", "bist", ":", "so", "w\u00e4hlst", "du", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "ADV", "$.", "KOUS", "PPER", "ADV", "PIAT", "NN", "VAFIN", "$.", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Willst blind seyn, und entfliehst", "tokens": ["Willst", "blind", "seyn", ",", "und", "ent\u00b7fliehst"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "ADJD", "VAINF", "$,", "KON", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Den nur Sterblichen nicht. \u00bbSehr ernst ist der Gedanke von dieser Wahl,", "tokens": ["Den", "nur", "Sterb\u00b7li\u00b7chen", "nicht", ".", "\u00bb", "Sehr", "ernst", "ist", "der", "Ge\u00b7dan\u00b7ke", "von", "die\u00b7ser", "Wahl", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "PTKNEG", "$.", "$(", "ADV", "ADJD", "VAFIN", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+--+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.8": {"text": "Versenkt tief mich in Schmerz,", "tokens": ["Ver\u00b7senkt", "tief", "mich", "in", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "In zu tr\u00fcbes Gef\u00fchl! Doch was Wahl? Es umringt schon den ahndenden,", "tokens": ["In", "zu", "tr\u00fc\u00b7bes", "Ge\u00b7f\u00fchl", "!", "Doch", "was", "Wahl", "?", "Es", "um\u00b7ringt", "schon", "den", "ahn\u00b7den\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "NN", "$.", "KON", "PWS", "NN", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+--", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Schon wehdroht mir die Nacht!\u00ab", "tokens": ["Schon", "weh\u00b7droht", "mir", "die", "Nacht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Das Licht schwand: doch entbehrst du das freundliche Wort des Geliebten nicht;", "tokens": ["Das", "Licht", "schwand", ":", "doch", "ent\u00b7behrst", "du", "das", "freund\u00b7li\u00b7che", "Wort", "des", "Ge\u00b7lieb\u00b7ten", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Nicht Stromfall, noch den Schlag", "tokens": ["Nicht", "Strom\u00b7fall", ",", "noch", "den", "Schlag"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Der gefl\u00fcchteten Wolke, die donnernd sich w\u00e4lzt, dass die H\u00fctte bebt,", "tokens": ["Der", "ge\u00b7fl\u00fcch\u00b7te\u00b7ten", "Wol\u00b7ke", ",", "die", "don\u00b7nernd", "sich", "w\u00e4lzt", ",", "dass", "die", "H\u00fct\u00b7te", "bebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "PRF", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.14": {"text": "(ein Graun Zagenden nur)", "tokens": ["(", "ein", "Graun", "Za\u00b7gen\u00b7den", "nur", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "ADV", "$("], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Und lautwirbelnd Sturmwind' an Felsenkl\u00fcften herbrausen! nicht Waldger\u00e4usch", "tokens": ["Und", "laut\u00b7wir\u00b7belnd", "Sturm\u00b7wind'", "an", "Fel\u00b7sen\u00b7kl\u00fcf\u00b7ten", "her\u00b7brau\u00b7sen", "!", "nicht", "Wald\u00b7ge\u00b7r\u00e4usch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "NN", "APPR", "NN", "VVPP", "$.", "PTKNEG", "NN"], "meter": "-+--+--+-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Von Mayluft, die dich labt;", "tokens": ["Von", "May\u00b7luft", ",", "die", "dich", "labt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Noch das frohe Gesing am verhohlnen Nestbau; nicht den s\u00fcssen Reiz", "tokens": ["Noch", "das", "fro\u00b7he", "Ge\u00b7sing", "am", "ver\u00b7hohl\u00b7nen", "Nest\u00b7bau", ";", "nicht", "den", "s\u00fcs\u00b7sen", "Reiz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$.", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+--+--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.18": {"text": "Der Tonkunst; und gewann", "tokens": ["Der", "Ton\u00b7kunst", ";", "und", "ge\u00b7wann"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Die Dichtkunst dein Herz auch, nicht den Reihen, in welchem sie schwebt, nachdem", "tokens": ["Die", "Dicht\u00b7kunst", "dein", "Herz", "auch", ",", "nicht", "den", "Rei\u00b7hen", ",", "in", "wel\u00b7chem", "sie", "schwebt", ",", "nach\u00b7dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADV", "$,", "PTKNEG", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "KOUS"], "meter": "-+--+-+-+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "Der Inhalt ihr gebeut:", "tokens": ["Der", "In\u00b7halt", "ihr", "ge\u00b7beut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Entbehrst nicht die Bezaubrung, wenn beydo, darreichend die Schwesterband,", "tokens": ["Ent\u00b7behrst", "nicht", "die", "Be\u00b7zaub\u00b7rung", ",", "wenn", "bey\u00b7do", ",", "dar\u00b7rei\u00b7chend", "die", "Schwes\u00b7ter\u00b7band", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "$,", "KOUS", "NE", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+---+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Durch Eintracht sich erh\u00f6hn,", "tokens": ["Durch", "Ein\u00b7tracht", "sich", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Und gelehriges Ohres, entz\u00fcckt, die Drommet' und das Horn vernimt", "tokens": ["Und", "ge\u00b7leh\u00b7ri\u00b7ges", "Oh\u00b7res", ",", "ent\u00b7z\u00fcckt", ",", "die", "Drom\u00b7met'", "und", "das", "Horn", "ver\u00b7nimt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "VVPP", "$,", "ART", "NN", "KON", "ART", "NN", "VVFIN"], "meter": "--+--+--+-+-+-+-+", "measure": "anapaest.tri.plus"}, "line.24": {"text": "Der Nachhall im Gebirg.", "tokens": ["Der", "Nach\u00b7hall", "im", "Ge\u00b7birg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Wer taub dann ihn gewahrt in der Freude, den Blinden, der tr\u00fcbt den Blick", "tokens": ["Wer", "taub", "dann", "ihn", "ge\u00b7wahrt", "in", "der", "Freu\u00b7de", ",", "den", "Blin\u00b7den", ",", "der", "tr\u00fcbt", "den", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "PPER", "VVPP", "APPR", "ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.26": {"text": "Vor Mitleid mit sich selbst.", "tokens": ["Vor", "Mit\u00b7leid", "mit", "sich", "selbst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Und du m\u00f6chtest das Wundergeb\u00e4ude, worin die geregte Luft", "tokens": ["Und", "du", "m\u00f6ch\u00b7test", "das", "Wun\u00b7der\u00b7ge\u00b7b\u00e4u\u00b7de", ",", "wo\u00b7rin", "die", "ge\u00b7reg\u00b7te", "Luft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "--+--+--+-+-+-+-+", "measure": "anapaest.tri.plus"}, "line.28": {"text": "Zum Laut wird, den du liebst,", "tokens": ["Zum", "Laut", "wird", ",", "den", "du", "liebst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "APPR", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Wie gesunken dir denken, zerst\u00f6ret, dass nun sich ihr Wallen dir", "tokens": ["Wie", "ge\u00b7sun\u00b7ken", "dir", "den\u00b7ken", ",", "zer\u00b7st\u00f6\u00b7ret", ",", "dass", "nun", "sich", "ihr", "Wal\u00b7len", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "PPER", "VVINF", "$,", "VVFIN", "$,", "KOUS", "ADV", "PRF", "PPOSAT", "NN", "PPER"], "meter": "--+--+--+-+-+-+-+", "measure": "anapaest.tri.plus"}, "line.30": {"text": "Umsonst naht, und wie stumm", "tokens": ["Um\u00b7sonst", "naht", ",", "und", "wie", "stumm"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KON", "PWAV", "ADJD"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.31": {"text": "Dir zerfliesst; ah zerst\u00f6rt Geh\u00f6rgang, die erklingende Grotte, drin", "tokens": ["Dir", "zer\u00b7fliesst", ";", "ah", "zer\u00b7st\u00f6rt", "Ge\u00b7h\u00f6r\u00b7gang", ",", "die", "er\u00b7klin\u00b7gen\u00b7de", "Grot\u00b7te", ",", "drin"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVPP", "NN", "$,", "ART", "ADJA", "NN", "$,", "ADV"], "meter": "+-+--+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.32": {"text": "Den Ambos, und von ihr", "tokens": ["Den", "Am\u00b7bos", ",", "und", "von", "ihr"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "APPR", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.33": {"text": "Zu dem Munde den Weg, und an ihrem Gew\u00f6lbe die F\u00e4serchen,", "tokens": ["Zu", "dem", "Mun\u00b7de", "den", "Weg", ",", "und", "an", "ih\u00b7rem", "Ge\u00b7w\u00f6l\u00b7be", "die", "F\u00e4\u00b7ser\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "KON", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "--+--+--+--+--+--", "measure": "anapaest.tetra.plus"}, "line.34": {"text": "Sie Aufhalt des Get\u00f6ns,", "tokens": ["Sie", "Auf\u00b7halt", "des", "Ge\u00b7t\u00f6ns", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.35": {"text": "Dass es sanft sich verliere; die feineren Saiten, sie sind gestimt", "tokens": ["Dass", "es", "sanft", "sich", "ver\u00b7lie\u00b7re", ";", "die", "fei\u00b7ne\u00b7ren", "Sai\u00b7ten", ",", "sie", "sind", "ge\u00b7stimt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PRF", "VVFIN", "$.", "ART", "ADJA", "NN", "$,", "PPER", "VAFIN", "VVPP"], "meter": "--+--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.36": {"text": "Dem Anwehn, das sie r\u00fchrt;", "tokens": ["Dem", "An\u00b7wehn", ",", "das", "sie", "r\u00fchrt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "(wie Windemen nicht allen gestimt) den Vorsaal, wo es netzend rint,", "tokens": ["(", "wie", "Win\u00b7de\u00b7men", "nicht", "al\u00b7len", "ge\u00b7stimt", ")", "den", "Vor\u00b7saal", ",", "wo", "es", "net\u00b7zend", "rint", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "NN", "PTKNEG", "PIS", "VVPP", "$(", "ART", "NN", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.38": {"text": "Emporwallt, wie der Quell;", "tokens": ["Em\u00b7por\u00b7wallt", ",", "wie", "der", "Quell", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Die gebogenen R\u00f6hren, der Schnecke Gewinde, die Scheidewand,", "tokens": ["Die", "ge\u00b7bo\u00b7ge\u00b7nen", "R\u00f6h\u00b7ren", ",", "der", "Schne\u00b7cke", "Ge\u00b7win\u00b7de", ",", "die", "Schei\u00b7de\u00b7wand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,", "ART", "NN", "$,"], "meter": "--+--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.40": {"text": "Das ganze Labirinth?", "tokens": ["Das", "gan\u00b7ze", "La\u00b7bi\u00b7rinth", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es tagt nicht! Kein Laut schallt! Wer entschl\u00f6ss sich schnell hier? wen erschreckte nicht", "tokens": ["Es", "tagt", "nicht", "!", "Kein", "Laut", "schallt", "!", "Wer", "ent\u00b7schl\u00f6ss", "sich", "schnell", "hier", "?", "wen", "er\u00b7schreck\u00b7te", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "PIAT", "APPR", "VVFIN", "$.", "PWS", "VVFIN", "PRF", "ADJD", "ADV", "$.", "PWS", "VVFIN", "PTKNEG"], "meter": "-+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Das Graunvolle der Wahl?", "tokens": ["Das", "Graun\u00b7vol\u00b7le", "der", "Wahl", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Doch sie sey dein Schicksal; du erk\u00f6hrst doch Blindheit? Des Geh\u00f6rs Verlust", "tokens": ["Doch", "sie", "sey", "dein", "Schick\u00b7sal", ";", "du", "er\u00b7k\u00f6hrst", "doch", "Blind\u00b7heit", "?", "Des", "Ge\u00b7h\u00f6rs", "Ver\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "NN", "$.", "ART", "NN", "NN"], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Vereinsamt, und du lebst", "tokens": ["Ver\u00b7ein\u00b7samt", ",", "und", "du", "lebst"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "KON", "PPER", "VVFIN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Mit den Menschen nicht mehr. Wenn du also kein Gott bist: so w\u00e4hlst du recht,", "tokens": ["Mit", "den", "Men\u00b7schen", "nicht", "mehr", ".", "Wenn", "du", "al\u00b7so", "kein", "Gott", "bist", ":", "so", "w\u00e4hlst", "du", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "ADV", "$.", "KOUS", "PPER", "ADV", "PIAT", "NN", "VAFIN", "$.", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Willst blind seyn, und entfliehst", "tokens": ["Willst", "blind", "seyn", ",", "und", "ent\u00b7fliehst"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "ADJD", "VAINF", "$,", "KON", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Den nur Sterblichen nicht. \u00bbSehr ernst ist der Gedanke von dieser Wahl,", "tokens": ["Den", "nur", "Sterb\u00b7li\u00b7chen", "nicht", ".", "\u00bb", "Sehr", "ernst", "ist", "der", "Ge\u00b7dan\u00b7ke", "von", "die\u00b7ser", "Wahl", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "PTKNEG", "$.", "$(", "ADV", "ADJD", "VAFIN", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+--+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.8": {"text": "Versenkt tief mich in Schmerz,", "tokens": ["Ver\u00b7senkt", "tief", "mich", "in", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "In zu tr\u00fcbes Gef\u00fchl! Doch was Wahl? Es umringt schon den ahndenden,", "tokens": ["In", "zu", "tr\u00fc\u00b7bes", "Ge\u00b7f\u00fchl", "!", "Doch", "was", "Wahl", "?", "Es", "um\u00b7ringt", "schon", "den", "ahn\u00b7den\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "NN", "$.", "KON", "PWS", "NN", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+--", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Schon wehdroht mir die Nacht!\u00ab", "tokens": ["Schon", "weh\u00b7droht", "mir", "die", "Nacht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Das Licht schwand: doch entbehrst du das freundliche Wort des Geliebten nicht;", "tokens": ["Das", "Licht", "schwand", ":", "doch", "ent\u00b7behrst", "du", "das", "freund\u00b7li\u00b7che", "Wort", "des", "Ge\u00b7lieb\u00b7ten", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Nicht Stromfall, noch den Schlag", "tokens": ["Nicht", "Strom\u00b7fall", ",", "noch", "den", "Schlag"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Der gefl\u00fcchteten Wolke, die donnernd sich w\u00e4lzt, dass die H\u00fctte bebt,", "tokens": ["Der", "ge\u00b7fl\u00fcch\u00b7te\u00b7ten", "Wol\u00b7ke", ",", "die", "don\u00b7nernd", "sich", "w\u00e4lzt", ",", "dass", "die", "H\u00fct\u00b7te", "bebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "PRF", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.14": {"text": "(ein Graun Zagenden nur)", "tokens": ["(", "ein", "Graun", "Za\u00b7gen\u00b7den", "nur", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "ADV", "$("], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Und lautwirbelnd Sturmwind' an Felsenkl\u00fcften herbrausen! nicht Waldger\u00e4usch", "tokens": ["Und", "laut\u00b7wir\u00b7belnd", "Sturm\u00b7wind'", "an", "Fel\u00b7sen\u00b7kl\u00fcf\u00b7ten", "her\u00b7brau\u00b7sen", "!", "nicht", "Wald\u00b7ge\u00b7r\u00e4usch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "NN", "APPR", "NN", "VVPP", "$.", "PTKNEG", "NN"], "meter": "-+--+--+-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Von Mayluft, die dich labt;", "tokens": ["Von", "May\u00b7luft", ",", "die", "dich", "labt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Noch das frohe Gesing am verhohlnen Nestbau; nicht den s\u00fcssen Reiz", "tokens": ["Noch", "das", "fro\u00b7he", "Ge\u00b7sing", "am", "ver\u00b7hohl\u00b7nen", "Nest\u00b7bau", ";", "nicht", "den", "s\u00fcs\u00b7sen", "Reiz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$.", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+--+--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.18": {"text": "Der Tonkunst; und gewann", "tokens": ["Der", "Ton\u00b7kunst", ";", "und", "ge\u00b7wann"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Die Dichtkunst dein Herz auch, nicht den Reihen, in welchem sie schwebt, nachdem", "tokens": ["Die", "Dicht\u00b7kunst", "dein", "Herz", "auch", ",", "nicht", "den", "Rei\u00b7hen", ",", "in", "wel\u00b7chem", "sie", "schwebt", ",", "nach\u00b7dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADV", "$,", "PTKNEG", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "KOUS"], "meter": "-+--+-+-+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "Der Inhalt ihr gebeut:", "tokens": ["Der", "In\u00b7halt", "ihr", "ge\u00b7beut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Entbehrst nicht die Bezaubrung, wenn beydo, darreichend die Schwesterband,", "tokens": ["Ent\u00b7behrst", "nicht", "die", "Be\u00b7zaub\u00b7rung", ",", "wenn", "bey\u00b7do", ",", "dar\u00b7rei\u00b7chend", "die", "Schwes\u00b7ter\u00b7band", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "$,", "KOUS", "NE", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+---+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Durch Eintracht sich erh\u00f6hn,", "tokens": ["Durch", "Ein\u00b7tracht", "sich", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Und gelehriges Ohres, entz\u00fcckt, die Drommet' und das Horn vernimt", "tokens": ["Und", "ge\u00b7leh\u00b7ri\u00b7ges", "Oh\u00b7res", ",", "ent\u00b7z\u00fcckt", ",", "die", "Drom\u00b7met'", "und", "das", "Horn", "ver\u00b7nimt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "VVPP", "$,", "ART", "NN", "KON", "ART", "NN", "VVFIN"], "meter": "--+--+--+-+-+-+-+", "measure": "anapaest.tri.plus"}, "line.24": {"text": "Der Nachhall im Gebirg.", "tokens": ["Der", "Nach\u00b7hall", "im", "Ge\u00b7birg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Wer taub dann ihn gewahrt in der Freude, den Blinden, der tr\u00fcbt den Blick", "tokens": ["Wer", "taub", "dann", "ihn", "ge\u00b7wahrt", "in", "der", "Freu\u00b7de", ",", "den", "Blin\u00b7den", ",", "der", "tr\u00fcbt", "den", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "PPER", "VVPP", "APPR", "ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.26": {"text": "Vor Mitleid mit sich selbst.", "tokens": ["Vor", "Mit\u00b7leid", "mit", "sich", "selbst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Und du m\u00f6chtest das Wundergeb\u00e4ude, worin die geregte Luft", "tokens": ["Und", "du", "m\u00f6ch\u00b7test", "das", "Wun\u00b7der\u00b7ge\u00b7b\u00e4u\u00b7de", ",", "wo\u00b7rin", "die", "ge\u00b7reg\u00b7te", "Luft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "--+--+--+-+-+-+-+", "measure": "anapaest.tri.plus"}, "line.28": {"text": "Zum Laut wird, den du liebst,", "tokens": ["Zum", "Laut", "wird", ",", "den", "du", "liebst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "APPR", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Wie gesunken dir denken, zerst\u00f6ret, dass nun sich ihr Wallen dir", "tokens": ["Wie", "ge\u00b7sun\u00b7ken", "dir", "den\u00b7ken", ",", "zer\u00b7st\u00f6\u00b7ret", ",", "dass", "nun", "sich", "ihr", "Wal\u00b7len", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "PPER", "VVINF", "$,", "VVFIN", "$,", "KOUS", "ADV", "PRF", "PPOSAT", "NN", "PPER"], "meter": "--+--+--+-+-+-+-+", "measure": "anapaest.tri.plus"}, "line.30": {"text": "Umsonst naht, und wie stumm", "tokens": ["Um\u00b7sonst", "naht", ",", "und", "wie", "stu