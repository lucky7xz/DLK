{"textgrid.poem.25130": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du sollst nicht Lethe suchen, sollst nicht Wein", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sollst nicht Lethe suchen, sollst nicht Wein", "tokens": ["Du", "sollst", "nicht", "Le\u00b7the", "su\u00b7chen", ",", "sollst", "nicht", "Wein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "VVINF", "$,", "VMFIN", "PTKNEG", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus harter giftiger Wolfsmilchwurzel klopfen,", "tokens": ["Aus", "har\u00b7ter", "gif\u00b7ti\u00b7ger", "Wolfs\u00b7milch\u00b7wur\u00b7zel", "klop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Noch soll Proserpinas blutrote Pein,", "tokens": ["Noch", "soll", "Pro\u00b7ser\u00b7pi\u00b7nas", "blut\u00b7ro\u00b7te", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Nachtschattentraube, deine Stirn umtropfen.", "tokens": ["Nacht\u00b7schat\u00b7ten\u00b7trau\u00b7be", ",", "dei\u00b7ne", "Stirn", "um\u00b7trop\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dein Rosenkranz sei nicht aus Taxusperlen,", "tokens": ["Dein", "Ro\u00b7sen\u00b7kranz", "sei", "nicht", "aus", "Ta\u00b7xu\u00b7sper\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dein Gram soll nicht zum flaumigen Kauz sich retten,", "tokens": ["Dein", "Gram", "soll", "nicht", "zum", "flau\u00b7mi\u00b7gen", "Kauz", "sich", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PTKNEG", "APPRART", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Im schwarzen Falter ein Symbol erblicken", "tokens": ["Im", "schwar\u00b7zen", "Fal\u00b7ter", "ein", "Sym\u00b7bol", "er\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und klagend wandeln unter Trauererlen,", "tokens": ["Und", "kla\u00b7gend", "wan\u00b7deln", "un\u00b7ter", "Trau\u00b7er\u00b7er\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sonst wird nur schl\u00e4fernd Dunkel dich umbetten", "tokens": ["Sonst", "wird", "nur", "schl\u00e4\u00b7fernd", "Dun\u00b7kel", "dich", "um\u00b7bet\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und deiner Seele wache Qual ersticken.", "tokens": ["Und", "dei\u00b7ner", "See\u00b7le", "wa\u00b7che", "Qual", "er\u00b7sti\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch wenn Melancholie herniederdr\u00e4ngt,", "tokens": ["Doch", "wenn", "Me\u00b7lan\u00b7cho\u00b7lie", "her\u00b7nie\u00b7der\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gleich wie vom Fr\u00fchlingshimmel Wolkenweinen", "tokens": ["Gleich", "wie", "vom", "Fr\u00fch\u00b7lings\u00b7him\u00b7mel", "Wol\u00b7ken\u00b7wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPRART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um gr\u00fcne H\u00f6hn sein graues Bahrtuch h\u00e4ngt", "tokens": ["Um", "gr\u00fc\u00b7ne", "H\u00f6hn", "sein", "grau\u00b7es", "Bahr\u00b7tuch", "h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und volle Nahrung gibt den m\u00fcden kleinen", "tokens": ["Und", "vol\u00b7le", "Nah\u00b7rung", "gibt", "den", "m\u00fc\u00b7den", "klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Kopfh\u00e4ngerischen Blumen, \u2013 oh, so dr\u00fccke", "tokens": ["Kopf\u00b7h\u00e4n\u00b7ge\u00b7ri\u00b7schen", "Blu\u00b7men", ",", "\u2013", "oh", ",", "so", "dr\u00fc\u00b7cke"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "$(", "FM", "$,", "ADV", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Dein Leid in fr\u00fchen Rosenkelch und schm\u00fccke", "tokens": ["Dein", "Leid", "in", "fr\u00fc\u00b7hen", "Ro\u00b7sen\u00b7kelch", "und", "schm\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "P\u00e4onienbl\u00fchn mit dunklen Gramgeschmeiden;", "tokens": ["P\u00e4o\u00b7ni\u00b7en\u00b7bl\u00fchn", "mit", "dunk\u00b7len", "Gram\u00b7ge\u00b7schmei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und ist die Herzensherrin tr\u00fcb, verdrossen \u2013", "tokens": ["Und", "ist", "die", "Her\u00b7zens\u00b7her\u00b7rin", "tr\u00fcb", ",", "ver\u00b7dros\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJD", "$,", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Halt ihre Hand in H\u00e4nden sanft verschlossen", "tokens": ["Halt", "ih\u00b7re", "Hand", "in", "H\u00e4n\u00b7den", "sanft", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und la\u00df den Blick in ihren Augen weiden.", "tokens": ["Und", "la\u00df", "den", "Blick", "in", "ih\u00b7ren", "Au\u00b7gen", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie lebt, wo Sch\u00f6nheit ist, die sterben mu\u00df,", "tokens": ["Sie", "lebt", ",", "wo", "Sch\u00f6n\u00b7heit", "ist", ",", "die", "ster\u00b7ben", "mu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "VAFIN", "$,", "PRELS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo Freude ist, die stets, die Hand am Munde,", "tokens": ["Wo", "Freu\u00b7de", "ist", ",", "die", "stets", ",", "die", "Hand", "am", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "$,", "PRELS", "ADV", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Gehn bereit \u2013 bei schmerzendem Genu\u00df,", "tokens": ["Zum", "Gehn", "be\u00b7reit", "\u2013", "bei", "schmer\u00b7zen\u00b7dem", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$(", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Gifte braut aus jeder seligen Stunde.", "tokens": ["Der", "Gif\u00b7te", "braut", "aus", "je\u00b7der", "se\u00b7li\u00b7gen", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ja selbst im Tempel aller hohen Wonnen", "tokens": ["Ja", "selbst", "im", "Tem\u00b7pel", "al\u00b7ler", "ho\u00b7hen", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "APPRART", "NN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Besitzt Melancholie Altar und St\u00e4tte \u2013", "tokens": ["Be\u00b7sitzt", "Me\u00b7lan\u00b7cho\u00b7lie", "Al\u00b7tar", "und", "St\u00e4t\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wenngleich nur der sie sieht, der Mut gewonnen,", "tokens": ["Wenn\u00b7gleich", "nur", "der", "sie", "sieht", ",", "der", "Mut", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "PPER", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Freudenbeere allzusanfte Gl\u00e4tte", "tokens": ["Der", "Freu\u00b7den\u00b7bee\u00b7re", "all\u00b7zu\u00b7sanf\u00b7te", "Gl\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Mit durstiger Zunge aufzutun: er findet", "tokens": ["Mit", "durs\u00b7ti\u00b7ger", "Zun\u00b7ge", "auf\u00b7zu\u00b7tun", ":", "er", "fin\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$.", "PPER", "VVFIN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Die Schwermut ihrer Macht, die ewig bindet.", "tokens": ["Die", "Schwer\u00b7mut", "ih\u00b7rer", "Macht", ",", "die", "e\u00b7wig", "bin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du sollst nicht Lethe suchen, sollst nicht Wein", "tokens": ["Du", "sollst", "nicht", "Le\u00b7the", "su\u00b7chen", ",", "sollst", "nicht", "Wein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "VVINF", "$,", "VMFIN", "PTKNEG", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus harter giftiger Wolfsmilchwurzel klopfen,", "tokens": ["Aus", "har\u