{"dta.poem.14895": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die wunderliche Prinzessin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df sie ihn nicht k\u00f6nnen zwingen,", "tokens": ["Da\u00df", "sie", "ihn", "nicht", "k\u00f6n\u00b7nen", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selbst zu Spielzeug sich verwandelt,", "tokens": ["Selbst", "zu", "Spiel\u00b7zeug", "sich", "ver\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Alte spielt mit ihnen.", "tokens": ["Und", "der", "Al\u00b7te", "spielt", "mit", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie m\u00fcssen th\u00f6richt tanzen,", "tokens": ["Und", "sie", "m\u00fcs\u00b7sen", "th\u00f6\u00b7richt", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Manche mit der Kron' geschm\u00fccket", "tokens": ["Man\u00b7che", "mit", "der", "Kron'", "ge\u00b7schm\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und im purpurnen Talare", "tokens": ["Und", "im", "pur\u00b7pur\u00b7nen", "Ta\u00b7la\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Feierlich den Reigen f\u00fchren.", "tokens": ["Fei\u00b7er\u00b7lich", "den", "Rei\u00b7gen", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Andre schweben lispelnd lose,", "tokens": ["And\u00b7re", "schwe\u00b7ben", "lis\u00b7pelnd", "lo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Andre m\u00fcssen m\u00e4nnlich l\u00e4rmen,", "tokens": ["And\u00b7re", "m\u00fcs\u00b7sen", "m\u00e4nn\u00b7lich", "l\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Rittern rei\u00dfen aus die Rosse", "tokens": ["Rit\u00b7tern", "rei\u00b7\u00dfen", "aus", "die", "Ros\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und die schreien gar erb\u00e4rmlich.", "tokens": ["Und", "die", "schrei\u00b7en", "gar", "er\u00b7b\u00e4rm\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Bis sie endlich alle m\u00fcde", "tokens": ["Bis", "sie", "end\u00b7lich", "al\u00b7le", "m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wieder kommen zu Verstande,", "tokens": ["Wie\u00b7der", "kom\u00b7men", "zu", "Ver\u00b7stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Mit der ganzen Welt im Frieden,", "tokens": ["Mit", "der", "gan\u00b7zen", "Welt", "im", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Legen ab die Maskerade.", "tokens": ["Le\u00b7gen", "ab", "die", "Mas\u00b7ke\u00b7ra\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "\u201ej\u00e4ger sind wir nicht, noch Ritter,\u201c", "tokens": ["\u201e", "j\u00e4\u00b7ger", "sind", "wir", "nicht", ",", "noch", "Rit\u00b7ter", ",", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "PTKNEG", "$,", "ADV", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "H\u00f6rt man sie von fern noch summen,", "tokens": ["H\u00f6rt", "man", "sie", "von", "fern", "noch", "sum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPR", "ADJD", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "\u201espiel nur war das \u2014 wir sind Dichter!\u201c \u2014", "tokens": ["\u201e", "spiel", "nur", "war", "das", "wir", "sind", "Dich\u00b7ter", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVFIN", "ADV", "VAFIN", "ART", "$(", "PPER", "VAFIN", "NN", "$.", "$(", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.19": {"text": "So vertost der ganze Plunder,", "tokens": ["So", "ver\u00b7tost", "der", "gan\u00b7ze", "Plun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "N\u00fcchtern liegt die Welt wie ehe,", "tokens": ["N\u00fcch\u00b7tern", "liegt", "die", "Welt", "wie", "e\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KOKOM", "KOUS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Und die Zaub'rin bei dem Alten", "tokens": ["Und", "die", "Zaub'\u00b7rin", "bei", "dem", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Spielt' die vor'gen Spiele wieder", "tokens": ["Spielt'", "die", "vor'\u00b7gen", "Spie\u00b7le", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Einsam wohl noch lange Jahre. \u2014", "tokens": ["Ein\u00b7sam", "wohl", "noch", "lan\u00b7ge", "Jah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADV", "ADV", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}