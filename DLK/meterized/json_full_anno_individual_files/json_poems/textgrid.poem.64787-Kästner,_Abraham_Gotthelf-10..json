{"textgrid.poem.64787": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie stark, wie treu mein Herze dich verehret,", "tokens": ["Wie", "stark", ",", "wie", "treu", "mein", "Her\u00b7ze", "dich", "ver\u00b7eh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "PPOSAT", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr welch ein Gl\u00fcck es deine Huld sich sch\u00e4tzt,", "tokens": ["F\u00fcr", "welch", "ein", "Gl\u00fcck", "es", "dei\u00b7ne", "Huld", "sich", "sch\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ART", "NN", "PPER", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hat noch kein Lied, o Freundinn, dich gelehret,", "tokens": ["Hat", "noch", "kein", "Lied", ",", "o", "Freun\u00b7dinn", ",", "dich", "ge\u00b7leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$,", "FM", "NN", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So sehr mich auch die Dichtkunst sonst erg\u00f6tzt.", "tokens": ["So", "sehr", "mich", "auch", "die", "Dicht\u00b7kunst", "sonst", "er\u00b7g\u00f6tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zu zeitig wird mit falschheitsvollen Schw\u00fcren", "tokens": ["Zu", "zei\u00b7tig", "wird", "mit", "falschheits\u00b7vol\u00b7len", "Schw\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+----+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Der Sch\u00f6nen Huld oft Dichtern zugeneigt:", "tokens": ["Der", "Sch\u00f6\u00b7nen", "Huld", "oft", "Dich\u00b7tern", "zu\u00b7gen\u00b7eigt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das w\u00fcnscht' ich nie; drum hab' ich, dich zu r\u00fchren,", "tokens": ["Das", "w\u00fcnscht'", "ich", "nie", ";", "drum", "hab'", "ich", ",", "dich", "zu", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$.", "PAV", "VAFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dir eh' mein Herz, als meinen Witz gezeigt.", "tokens": ["Dir", "eh'", "mein", "Herz", ",", "als", "mei\u00b7nen", "Witz", "ge\u00b7zeigt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "PPOSAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Beneide nicht ber\u00fchmter Sch\u00f6nen Menge,", "tokens": ["Be\u00b7nei\u00b7de", "nicht", "be\u00b7r\u00fchm\u00b7ter", "Sch\u00f6\u00b7nen", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von deren Preis so manches Lied erz\u00e4hlt,", "tokens": ["Von", "de\u00b7ren", "Preis", "so", "man\u00b7ches", "Lied", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erf\u00fcllt ihr Lob unz\u00e4hlige Ges\u00e4nge,", "tokens": ["Er\u00b7f\u00fcllt", "ihr", "Lob", "un\u00b7z\u00e4h\u00b7li\u00b7ge", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So zeigt es oft, wie merklich sie gefehlt.", "tokens": ["So", "zeigt", "es", "oft", ",", "wie", "merk\u00b7lich", "sie", "ge\u00b7fehlt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die matte Gluth nur unschuldsvoller Triebe,", "tokens": ["Die", "mat\u00b7te", "Gluth", "nur", "un\u00b7schulds\u00b7vol\u00b7ler", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Pflegt wenigmal der Dichter Gluth zu seyn.", "tokens": ["Pflegt", "we\u00b7nig\u00b7mal", "der", "Dich\u00b7ter", "Gluth", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein feurig Lied, voll Z\u00e4rtlichkeit und Liebe,", "tokens": ["Ein", "feu\u00b7rig", "Lied", ",", "voll", "Z\u00e4rt\u00b7lich\u00b7keit", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sieht man nicht oft der strengsten Tugend weyhn.", "tokens": ["Sieht", "man", "nicht", "oft", "der", "strengs\u00b7ten", "Tu\u00b7gend", "weyhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So r\u00fchm ich nicht, Geliebte, deine G\u00fcte", "tokens": ["So", "r\u00fchm", "ich", "nicht", ",", "Ge\u00b7lieb\u00b7te", ",", "dei\u00b7ne", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch einen Reim, der deine Tugend kr\u00e4nkt.", "tokens": ["Durch", "ei\u00b7nen", "Reim", ",", "der", "dei\u00b7ne", "Tu\u00b7gend", "kr\u00e4nkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich fand in dir das sch\u00e4tzbarste Gem\u00fcthe,", "tokens": ["Ich", "fand", "in", "dir", "das", "sch\u00e4tz\u00b7bars\u00b7te", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Das z\u00e4rtlich f\u00fchlt, wenn es erhaben denkt:", "tokens": ["Das", "z\u00e4rt\u00b7lich", "f\u00fchlt", ",", "wenn", "es", "er\u00b7ha\u00b7ben", "denkt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das bey der Lust der angenehmsten Triebe", "tokens": ["Das", "bey", "der", "Lust", "der", "an\u00b7ge\u00b7nehms\u00b7ten", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nie seinen Werth noch seine Pflicht vergi\u00dft,", "tokens": ["Nie", "sei\u00b7nen", "Werth", "noch", "sei\u00b7ne", "Pflicht", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und gleich so stark an Tugend, als an Liebe,", "tokens": ["Und", "gleich", "so", "stark", "an", "Tu\u00b7gend", ",", "als", "an", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "APPR", "NN", "$,", "KOUS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So voller Gluth; wie voller Unschuld k\u00fc\u00dft.", "tokens": ["So", "vol\u00b7ler", "Gluth", ";", "wie", "vol\u00b7ler", "Un\u00b7schuld", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$.", "PWAV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein kluger Geist, den Witz und Kenntni\u00df zieret,", "tokens": ["Ein", "klu\u00b7ger", "Geist", ",", "den", "Witz", "und", "Kennt\u00b7ni\u00df", "zie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein z\u00e4rtlich Herz, von edlen Trieben voll,", "tokens": ["Ein", "z\u00e4rt\u00b7lich", "Herz", ",", "von", "ed\u00b7len", "Trie\u00b7ben", "voll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sind, was bey dir, Vollkommne, mich ger\u00fchret,", "tokens": ["Sind", ",", "was", "bey", "dir", ",", "Voll\u00b7komm\u00b7ne", ",", "mich", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "APPR", "PPER", "$,", "NN", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und was mich dir auf ewig fesseln soll.", "tokens": ["Und", "was", "mich", "dir", "auf", "e\u00b7wig", "fes\u00b7seln", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "APPR", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verschm\u00e4h' mit mir, was Thoren soll begl\u00fccken,", "tokens": ["Ver\u00b7schm\u00e4h'", "mit", "mir", ",", "was", "Tho\u00b7ren", "soll", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "PWS", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nur eitle Pracht, um Gold nur feile Lust:", "tokens": ["Nur", "eit\u00b7le", "Pracht", ",", "um", "Gold", "nur", "fei\u00b7le", "Lust", ":"], "token_info": ["word", "word", "word", "punct", "word", "word"