{"dta.poem.10344": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n  Lied des Gefangenen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Als meine Gro\u00dfmutter die Lise behext,", "tokens": ["Als", "mei\u00b7ne", "Gro\u00df\u00b7mut\u00b7ter", "die", "Li\u00b7se", "be\u00b7hext", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da wollten die Leut sie verbrennen.", "tokens": ["Da", "woll\u00b7ten", "die", "Leut", "sie", "ver\u00b7bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Schon hatte der Amtmann viel Dinte verklext,", "tokens": ["Schon", "hat\u00b7te", "der", "Amt\u00b7mann", "viel", "Din\u00b7te", "ver\u00b7klext", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Doch wollte sie nicht bekennen.", "tokens": ["Doch", "woll\u00b7te", "sie", "nicht", "be\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und als man sie in den Kessel schob,", "tokens": ["Und", "als", "man", "sie", "in", "den", "Kes\u00b7sel", "schob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da schrie sie Mord und Wehe;", "tokens": ["Da", "schrie", "sie", "Mord", "und", "We\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und als sich der schwarze Qualm erhob,", "tokens": ["Und", "als", "sich", "der", "schwar\u00b7ze", "Qualm", "er\u00b7hob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Da flog sie als Rab' in die H\u00f6he.", "tokens": ["Da", "flog", "sie", "als", "Rab'", "in", "die", "H\u00f6\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Mein schwarzes, gefiedertes Gro\u00dfm\u00fctterlein!", "tokens": ["Mein", "schwar\u00b7zes", ",", "ge\u00b7fie\u00b7der\u00b7tes", "Gro\u00df\u00b7m\u00fct\u00b7ter\u00b7lein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O komm' mich im Thurme besuchen,", "tokens": ["O", "komm'", "mich", "im", "Thur\u00b7me", "be\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "++--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Komm'! fliege geschwinde durch's Gitter herein,", "tokens": ["Komm'", "!", "flie\u00b7ge", "ge\u00b7schwin\u00b7de", "durch's", "Git\u00b7ter", "her\u00b7ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "VVFIN", "ADJA", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und bringe mir K\u00e4se und Kuchen.", "tokens": ["Und", "brin\u00b7ge", "mir", "K\u00e4\u00b7se", "und", "Ku\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Mein schwarzes, gefiedertes Gro\u00dfm\u00fctterlein!", "tokens": ["Mein", "schwar\u00b7zes", ",", "ge\u00b7fie\u00b7der\u00b7tes", "Gro\u00df\u00b7m\u00fct\u00b7ter\u00b7lein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O m\u00f6chtest du nur sorgen,", "tokens": ["O", "m\u00f6ch\u00b7test", "du", "nur", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df die Muhme nicht auspickt die Augen mein,", "tokens": ["Da\u00df", "die", "Muh\u00b7me", "nicht", "aus\u00b7pickt", "die", "Au\u00b7gen", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VVFIN", "ART", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Wenn ich luftig schwebe morgen.", "tokens": ["Wenn", "ich", "luf\u00b7tig", "schwe\u00b7be", "mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}