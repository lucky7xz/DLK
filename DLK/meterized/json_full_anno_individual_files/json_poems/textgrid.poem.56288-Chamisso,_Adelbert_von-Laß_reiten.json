{"textgrid.poem.56288": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "La\u00df reiten", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ritt ein Reiter die Stra\u00dfe hinaus,", "tokens": ["Es", "ritt", "ein", "Rei\u00b7ter", "die", "Stra\u00b7\u00dfe", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Spur verwehte der Wind.", "tokens": ["Die", "Spur", "ver\u00b7weh\u00b7te", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ein M\u00e4dchen zerpfl\u00fcckt einen", "tokens": ["Ein", "M\u00e4d\u00b7chen", "zer\u00b7pfl\u00fcckt", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Rosenstrau\u00df,", "tokens": ["Ro\u00b7sen\u00b7strau\u00df", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Und weint die Augen sich blind.", "tokens": ["Und", "weint", "die", "Au\u00b7gen", "sich", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "\u00bbdu warst mir so rosig und wohlgemut,", "tokens": ["\u00bb", "du", "warst", "mir", "so", "ro\u00b7sig", "und", "wohl\u00b7ge\u00b7mut", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos"