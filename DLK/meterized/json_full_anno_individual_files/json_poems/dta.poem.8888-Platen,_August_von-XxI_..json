{"dta.poem.8888": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Hinaus zu sehn, wo Schiff und Gondel schweben,", "tokens": ["Hin\u00b7aus", "zu", "sehn", ",", "wo", "Schiff", "und", "Gon\u00b7del", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "PWAV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn die Lagune, ruhig, spiegeleben,", "tokens": ["Wenn", "die", "La\u00b7gu\u00b7ne", ",", "ru\u00b7hig", ",", "spie\u00b7ge\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In sich verflie\u00dft, Venedig sanft umsp\u00fchlet!", "tokens": ["In", "sich", "ver\u00b7flie\u00dft", ",", "Ve\u00b7ne\u00b7dig", "sanft", "um\u00b7sp\u00fch\u00b7let", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVFIN", "$,", "NE", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In's Innre wieder dann gezogen f\u00fchlet", "tokens": ["In's", "Inn\u00b7re", "wie\u00b7der", "dann", "ge\u00b7zo\u00b7gen", "f\u00fch\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ADV", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Auge sich, wo nach den Wolken streben", "tokens": ["Das", "Au\u00b7ge", "sich", ",", "wo", "nach", "den", "Wol\u00b7ken", "stre\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "$,", "PWAV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Pallast und Kirche, wo ein lautes Leben", "tokens": ["Pal\u00b7last", "und", "Kir\u00b7che", ",", "wo", "ein", "lau\u00b7tes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf allen Stufen des Rialto w\u00fchlet.", "tokens": ["Auf", "al\u00b7len", "Stu\u00b7fen", "des", "Ri\u00b7al\u00b7to", "w\u00fch\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NE", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein frohes V\u00f6lkchen lieber M\u00fcssigg\u00e4nger,", "tokens": ["Ein", "fro\u00b7hes", "V\u00f6lk\u00b7chen", "lie\u00b7ber", "M\u00fcs\u00b7sig\u00b7g\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es schw\u00e4rmt umher, es l\u00e4\u00dft durch nichts sich st\u00f6ren,", "tokens": ["Es", "schw\u00e4rmt", "um\u00b7her", ",", "es", "l\u00e4\u00dft", "durch", "nichts", "sich", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "PIS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und st\u00f6rt auch niemals einen Grillenf\u00e4nger.", "tokens": ["Und", "st\u00f6rt", "auch", "nie\u00b7mals", "ei\u00b7nen", "Gril\u00b7len\u00b7f\u00e4n\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Des Abends sammelt sich's zu ganzen Ch\u00f6ren,", "tokens": ["Des", "A\u00b7bends", "sam\u00b7melt", "sich's", "zu", "gan\u00b7zen", "Ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn auf dem Markusplatze will's den S\u00e4nger,", "tokens": ["Denn", "auf", "dem", "Mar\u00b7kus\u00b7plat\u00b7ze", "will's", "den", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und den Erz\u00e4hler auf der Riva h\u00f6ren.", "tokens": ["Und", "den", "Er\u00b7z\u00e4h\u00b7ler", "auf", "der", "Ri\u00b7va", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}