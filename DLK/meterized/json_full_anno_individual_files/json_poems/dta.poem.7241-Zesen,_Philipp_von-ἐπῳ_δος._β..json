{"dta.poem.7241": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "\u1f10\u03c0\u1ff3 \u03b4\u03bf\u03c2. \u03b2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1640", "urn": "urn:nbn:de:kobv:b4-20685-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier ligt in dieser Gruft ein zweig der sch\u00f6nen Ju-", "tokens": ["Hier", "ligt", "in", "die\u00b7ser", "Gruft", "ein", "zweig", "der", "sch\u00f6\u00b7nen", "Ju"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PDAT", "NN", "ART", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein sehr fromb gem\u00fcth/ ", "tokens": ["Ein", "sehr", "fromb", "ge\u00b7m\u00fcth", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJD", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Vnd des Fr\u00fchlings bl\u00fcth/", "tokens": ["Vnd", "des", "Fr\u00fch\u00b7lings", "bl\u00fcth", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Der Eltern Ruhm vnd Zier das Eigenthumb der", "tokens": ["Der", "El\u00b7tern", "Ruhm", "vnd", "Zier", "das", "Ei\u00b7gen\u00b7thumb", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Tugend;", "tokens": ["Tu\u00b7gend", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Die Seel ist aber nauf zur wahren Ruh gereist/", "tokens": ["Die", "Seel", "ist", "a\u00b7ber", "nauf", "zur", "wah\u00b7ren", "Ruh", "ge\u00b7reist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Tantzt vnd springet", "tokens": ["Tantzt", "vnd", "sprin\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "lieblich singet/", "tokens": ["lieb\u00b7lich", "sin\u00b7get", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "In dem Sie stets den Himmels-rau geneist.", "tokens": ["In", "dem", "Sie", "stets", "den", "Him\u00b7mels\u00b7rau", "ge\u00b7neist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}