{"textgrid.poem.56689": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Prolet", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was treibt dich, dieses Leben fortzufahren,", "tokens": ["Was", "treibt", "dich", ",", "die\u00b7ses", "Le\u00b7ben", "fort\u00b7zu\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PDAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Prolet in deinem schmierigen Gewand,", "tokens": ["Pro\u00b7let", "in", "dei\u00b7nem", "schmie\u00b7ri\u00b7gen", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "nachdem der Wollust jugendlicher Brand", "tokens": ["nach\u00b7dem", "der", "Wol\u00b7lust", "ju\u00b7gend\u00b7li\u00b7cher", "Brand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "erlosch nach allzu schnell verrauschten Jahren?", "tokens": ["er\u00b7losch", "nach", "all\u00b7zu", "schnell", "ver\u00b7rauschten", "Jah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PTKA", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Hohl\u00e4ugig, hager, mit ergrauten Haaren,", "tokens": ["Hoh\u00b7l\u00e4u\u00b7gig", ",", "ha\u00b7ger", ",", "mit", "er\u00b7grau\u00b7ten", "Haa\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "APPR", "ADJA", 