{"dta.poem.10159": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Herbst-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da ich mit frohen Blicken hier", "tokens": ["Da", "ich", "mit", "fro\u00b7hen", "Bli\u00b7cken", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des k\u00fchlen Herbstes bunte Zier,", "tokens": ["Des", "k\u00fch\u00b7len", "Herbs\u00b7tes", "bun\u00b7te", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In dem noch sch\u00f6nen Uberrest", "tokens": ["In", "dem", "noch", "sch\u00f6\u00b7nen", "Ub\u00b7er\u00b7rest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der halb entbl\u00e4tterten Allee,", "tokens": ["Der", "halb", "ent\u00b7bl\u00e4t\u00b7ter\u00b7ten", "Al\u00b7lee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die noch an hohen Zweigen fest,", "tokens": ["Die", "noch", "an", "ho\u00b7hen", "Zwei\u00b7gen", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bald \u00fcberhaupt, bald einzeln sehe;", "tokens": ["Bald", "\u00fc\u00b7ber\u00b7haupt", ",", "bald", "ein\u00b7zeln", "se\u00b7he", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bin ich durch diese Pracht der Bl\u00e4tter,", "tokens": ["Bin", "ich", "durch", "die\u00b7se", "Pracht", "der", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zumahl bey dem ametzt so sch\u00f6nen Wetter,", "tokens": ["Zu\u00b7mahl", "bey", "dem", "a\u00b7metzt", "so", "sch\u00f6\u00b7nen", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Nicht obenhin, recht inniglich ger\u00fchrt.", "tokens": ["Nicht", "o\u00b7ben\u00b7hin", ",", "recht", "in\u00b7nig\u00b7lich", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}}, "stanza.2": {"line.1": {"text": "Ich seh\u2019 anietzt die spielende Natur", "tokens": ["Ich", "seh'", "an\u00b7ietzt", "die", "spie\u00b7len\u00b7de", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit holder Farben Glantz nicht nur", "tokens": ["Mit", "hol\u00b7der", "Far\u00b7ben", "Glantz", "nicht", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erhabner B\u00e4ume Wipffel kr\u00e4ntzen;", "tokens": ["Er\u00b7hab\u00b7ner", "B\u00e4u\u00b7me", "Wipf\u00b7fel", "kr\u00e4nt\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Man sieht die Erde selbst voll bunter Bl\u00e4tter gl\u00e4ntzen.", "tokens": ["Man", "sieht", "die", "Er\u00b7de", "selbst", "voll", "bun\u00b7ter", "Bl\u00e4t\u00b7ter", "gl\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der dunckle Steig, den, mehr als die ietzt d\u00fcnne Schatten,", "tokens": ["Der", "dunck\u00b7le", "Steig", ",", "den", ",", "mehr", "als", "die", "ietzt", "d\u00fcn\u00b7ne", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "$,", "PIAT", "KOKOM", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der langen N\u00e4chte Thau geschw\u00e4rtzet hatten,", "tokens": ["Der", "lan\u00b7gen", "N\u00e4ch\u00b7te", "Thau", "ge\u00b7schw\u00e4rt\u00b7zet", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lag gantz von bunten Laub bedeckt, und sch\u00f6n geschm\u00fcckt.", "tokens": ["Lag", "gantz", "von", "bun\u00b7ten", "Laub", "be\u00b7deckt", ",", "und", "sch\u00f6n", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "ADJA", "NN", "VVPP", "$,", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es schien, als wenn der B\u00e4ume Schaar", "tokens": ["Es", "schien", ",", "als", "wenn", "der", "B\u00e4u\u00b7me", "Schaar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nunmehro recht besch\u00e4fftigt war,", "tokens": ["Nun\u00b7meh\u00b7ro", "recht", "be\u00b7sch\u00e4ff\u00b7tigt", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich selbst der Haare Schmuck zu rauben,", "tokens": ["Sich", "selbst", "der", "Haa\u00b7re", "Schmuck", "zu", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Aus Danckbarkeit sich gleichsam zu entlauben;", "tokens": ["Aus", "Dan\u00b7ck\u00b7bar\u00b7keit", "sich", "gleich\u00b7sam", "zu", "ent\u00b7lau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Um ihrer Mutter braunes Kleid", "tokens": ["Um", "ih\u00b7rer", "Mut\u00b7ter", "brau\u00b7nes", "Kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit bunten Farben auszuschm\u00fccken,", "tokens": ["Mit", "bun\u00b7ten", "Far\u00b7ben", "aus\u00b7zu\u00b7schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und sie mit Purpur hier, mit Golde dort zu sticken.", "tokens": ["Und", "sie", "mit", "Pur\u00b7pur", "hier", ",", "mit", "Gol\u00b7de", "dort", "zu", "sti\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "ADV", "$,", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Jm Garten hatte sie ihr bunt-gef\u00e4rbt Gewand", "tokens": ["Jm", "Gar\u00b7ten", "hat\u00b7te", "sie", "ihr", "bunt\u00b7ge\u00b7f\u00e4rbt", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schon ab-, doch auf das neu mit unsichtbarer Hand,", "tokens": ["Schon", "ab", ",", "doch", "auf", "das", "neu", "mit", "un\u00b7sicht\u00b7ba\u00b7rer", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "$,", "ADV", "APPR", "ART", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein g\u00fcldnes wieder angeleget.", "tokens": ["Ein", "g\u00fcld\u00b7nes", "wie\u00b7der", "an\u00b7ge\u00b7le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bluhm\u2019 aus Africa, die g\u00fcldne Bl\u00e4tter tr\u00e4get,", "tokens": ["Die", "Bluhm'", "aus", "A\u00b7fri\u00b7ca", ",", "die", "g\u00fcld\u00b7ne", "Bl\u00e4t\u00b7ter", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nasturtium, die gelbe Ritter-Sporen,", "tokens": ["Nas\u00b7tur\u00b7ti\u00b7um", ",", "die", "gel\u00b7be", "Rit\u00b7ter\u00b7S\u00b7po\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Sonnen-Bluhm\u2019, in gleichfalls g\u00fcldner Zier,", "tokens": ["Die", "Son\u00b7nen\u00b7Bluhm'", ",", "in", "gleich\u00b7falls", "g\u00fcld\u00b7ner", "Zier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die kamen mir,", "tokens": ["Die", "ka\u00b7men", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Als ich sie \u00fcbersah, fast gleichsam vor,", "tokens": ["Als", "ich", "sie", "\u00fc\u00b7ber\u00b7sah", ",", "fast", "gleich\u00b7sam", "vor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Als wie ein Schlaff-Rock von Drap d\u2019or,", "tokens": ["Als", "wie", "ein", "Schlaff\u00b7Rock", "von", "Drap", "d'\u00b7or", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "APPR", "NE", "NE", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.10": {"text": "In welchem die Natur, eh sie zur langen Ruh", "tokens": ["In", "wel\u00b7chem", "die", "Na\u00b7tur", ",", "eh", "sie", "zur", "lan\u00b7gen", "Ruh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "$,", "KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die m\u00fcden Glieder neigte,", "tokens": ["Die", "m\u00fc\u00b7den", "Glie\u00b7der", "neig\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Annoch zu guter letzt sich halb entkleidet zeigte.", "tokens": ["An\u00b7noch", "zu", "gu\u00b7ter", "letzt", "sich", "halb", "ent\u00b7klei\u00b7det", "zeig\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "ADV", "PRF", "ADJD", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Damit der Schlaff-Rock auch nicht gar einf\u00e4rbig schien,", "tokens": ["Da\u00b7mit", "der", "Schlaff\u00b7Rock", "auch", "nicht", "gar", "ein\u00b7f\u00e4r\u00b7big", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "PTKNEG", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.14": {"text": "War auch demselbigen nicht nur noch etwas gr\u00fcn,", "tokens": ["War", "auch", "dem\u00b7sel\u00b7bi\u00b7gen", "nicht", "nur", "noch", "et\u00b7was", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDS", "PTKNEG", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nein, auch von Farben Wunder-sch\u00f6n,", "tokens": ["Nein", ",", "auch", "von", "Far\u00b7ben", "Wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ein Winter-Rosen-Busch noch hier und dort zu sehn,", "tokens": ["Ein", "Win\u00b7ter\u00b7Ro\u00b7sen\u00b7Busch", "noch", "hier", "und", "dort", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "KON", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "An deren feurigem Rubinen-gleichen Prangen", "tokens": ["An", "de\u00b7ren", "feu\u00b7ri\u00b7gem", "Ru\u00b7bi\u00b7nen\u00b7glei\u00b7chen", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Das menschliche Gesicht, fast wider willen, hangen,", "tokens": ["Das", "menschli\u00b7che", "Ge\u00b7sicht", ",", "fast", "wi\u00b7der", "wil\u00b7len", ",", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Und, fast gezwungen, kleben bleibt.", "tokens": ["Und", ",", "fast", "ge\u00b7zwun\u00b7gen", ",", "kle\u00b7ben", "bleibt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "VVPP", "$,", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dergleichen angenehmer Schertz", "tokens": ["Derg\u00b7lei\u00b7chen", "an\u00b7ge\u00b7neh\u00b7mer", "Schertz"], "token_info": ["word", "word", "word"], "pos": ["PIS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der lieblich spielenden Gedancken", "tokens": ["Der", "lieb\u00b7lich", "spie\u00b7len\u00b7den", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erf\u00fcllte mein ger\u00fchrtes Hertz.", "tokens": ["Er\u00b7f\u00fcll\u00b7te", "mein", "ge\u00b7r\u00fchr\u00b7tes", "Hertz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Trauben-reiche Reben-Rancken", "tokens": ["Die", "Trau\u00b7ben\u00b7rei\u00b7che", "Re\u00b7ben\u00b7Ran\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vergn\u00fcgten mich zugleich im hellen Sonnen-Schein,", "tokens": ["Ver\u00b7gn\u00fcg\u00b7ten", "mich", "zu\u00b7gleich", "im", "hel\u00b7len", "Son\u00b7nen\u00b7Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und fiel mir, in der Lust und Anmuth, dieses ein:", "tokens": ["Und", "fiel", "mir", ",", "in", "der", "Lust", "und", "An\u00b7muth", ",", "die\u00b7ses", "ein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "KON", "NN", "$,", "PDS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "\u201eewigs, Selbst\u00e4ndigs, Allm\u00e4chtiges Wesen,", "tokens": ["\u201e", "e\u00b7wigs", ",", "Selb\u00b7st\u00e4n\u00b7digs", ",", "All\u00b7m\u00e4ch\u00b7ti\u00b7ges", "We\u00b7sen", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["