{"dta.poem.7493": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XlIX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00fcg' ich ein Schwert als Krieger um die Lenden,", "tokens": ["Tr\u00fcg'", "ich", "ein", "Schwert", "als", "Krie\u00b7ger", "um", "die", "Len\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KOUS", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ging' ich als Landmann hinter einem Pfluge,", "tokens": ["Ging'", "ich", "als", "Land\u00b7mann", "hin\u00b7ter", "ei\u00b7nem", "Pflu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KOUS", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann s\u00e4\u00df' ich Abends froh bei meinem Kruge,", "tokens": ["Dann", "s\u00e4\u00df'", "ich", "A\u00b7bends", "froh", "bei", "mei\u00b7nem", "Kru\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um mit dem Tag mein Tagewerk zu enden.", "tokens": ["Um", "mit", "dem", "Tag", "mein", "Ta\u00b7ge\u00b7werk", "zu", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So aber, wenn sie sich zur Ruhe wenden,", "tokens": ["So", "a\u00b7ber", ",", "wenn", "sie", "sich", "zur", "Ru\u00b7he", "wen\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schweift ", "tokens": ["Schweift"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Und meine Seele kreist in stetem Fluge,", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "kreist", "in", "ste\u00b7tem", "Flu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr will kein Abend seinen Frieden spenden.", "tokens": ["Ihr", "will", "kein", "A\u00b7bend", "sei\u00b7nen", "Frie\u00b7den", "spen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dem Himmlischen erbaun wir keine Schranken,", "tokens": ["Dem", "Himm\u00b7li\u00b7schen", "er\u00b7baun", "wir", "kei\u00b7ne", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es folgt uns nach ins laute Weltgetriebe", "tokens": ["Es", "folgt", "uns", "nach", "ins", "lau\u00b7te", "Welt\u00b7ge\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wird im Schlummer auch nicht von uns wanken.", "tokens": ["Und", "wird", "im", "Schlum\u00b7mer", "auch", "nicht", "von", "uns", "wan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ADV", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Kein Ort \u2014 da\u00df ich vor ihnen sicher bliebe!", "tokens": ["Kein", "Ort", "da\u00df", "ich", "vor", "ih\u00b7nen", "si\u00b7cher", "blie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "KOUS", "PPER", "APPR", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Gleich Blitzen z\u00fccken um mich die Gedanken", "tokens": ["Gleich", "Blit\u00b7zen", "z\u00fc\u00b7cken", "um", "mich", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "APPR", "PRF", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und treffen mich selbst in dem Arm der Liebe.", "tokens": ["Und", "tref\u00b7fen", "mich", "selbst", "in", "dem", "Arm", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}}}}}