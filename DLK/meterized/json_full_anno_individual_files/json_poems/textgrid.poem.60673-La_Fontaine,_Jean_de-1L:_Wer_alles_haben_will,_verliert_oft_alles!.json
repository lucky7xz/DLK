, ",", "das", "sich", "al\u00b7le", "mer\u00b7ken", "soll\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie viele gibt es doch von solchem Schlag,", "tokens": ["Wie", "vie\u00b7le", "gibt", "es", "doch", "von", "sol\u00b7chem", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die arm geworden sind an einem Tag,", "tokens": ["Die", "arm", "ge\u00b7wor\u00b7den", "sind", "an", "ei\u00b7nem", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAPP", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weil sie in einem Tag zu Reichtum kommen wollten.", "tokens": ["Weil", "sie", "in", "ei\u00b7nem", "Tag", "zu", "Reich\u00b7tum", "kom\u00b7men", "woll\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer alles haben will, verliert oft alles!", "tokens": ["Wer", "al\u00b7les", "ha\u00b7ben", "will", ",", "ver\u00b7liert", "oft", "al\u00b7les", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VAINF", "VMFIN", "$,", "VVFIN", "ADV", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dies zu erkl\u00e4ren, denk ich jenes Falles,", "tokens": ["Dies", "zu", "er\u00b7kl\u00e4\u00b7ren", ",", "denk", "ich", "je