{"textgrid.poem.60673": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer alles haben will, verliert oft alles!", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer alles haben will, verliert oft alles!", "tokens": ["Wer", "al\u00b7les", "ha\u00b7ben", "will", ",", "ver\u00b7liert", "oft", "al\u00b7les", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VAINF", "VMFIN", "$,", "VVFIN", "ADV", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dies zu erkl\u00e4ren, denk ich jenes Falles,", "tokens": ["Dies", "zu", "er\u00b7kl\u00e4\u00b7ren", ",", "denk", "ich", "je\u00b7nes", "Fal\u00b7les", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In dem ein Huhn, wie uns die Fabel lehrt,", "tokens": ["In", "dem", "ein", "Huhn", ",", "wie", "uns", "die", "Fa\u00b7bel", "lehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem Geizhals, dem es zugeh\u00f6rt,", "tokens": ["Dem", "Geiz\u00b7hals", ",", "dem", "es", "zu\u00b7ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von Tag zu Tag ein goldnes Ei beschert.", "tokens": ["Von", "Tag", "zu", "Tag", "ein", "gold\u00b7nes", "Ei", "be\u00b7schert", "."], "