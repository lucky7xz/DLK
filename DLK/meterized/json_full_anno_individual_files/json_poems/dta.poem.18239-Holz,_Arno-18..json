{"dta.poem.18239": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Den einst die Sehnsucht schmeichelnd dir kredenzt,", "tokens": ["Den", "einst", "die", "Sehn\u00b7sucht", "schmei\u00b7chelnd", "dir", "kre\u00b7denzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sieh, ich bin elend, und dies Herz ist krank,", "tokens": ["Sieh", ",", "ich", "bin", "e\u00b7lend", ",", "und", "dies", "Herz", "ist", "krank", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ADJD", "$,", "KON", "PDS", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drin Ha\u00df und Liebe aneinander grenzt!", "tokens": ["Drin", "Ha\u00df", "und", "Lie\u00b7be", "an\u00b7ein\u00b7an\u00b7der", "grenzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das aber macht, ich trank von jenem Saft,", "tokens": ["Das", "a\u00b7ber", "macht", ",", "ich", "trank", "von", "je\u00b7nem", "Saft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gemischt aus Himmels", "tokens": ["Ge\u00b7mischt", "aus", "Him\u00b7mels"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Zu viel f\u00fcr diese kurze Erdenhast", "tokens": ["Zu", "viel", "f\u00fcr", "die\u00b7se", "kur\u00b7ze", "Er\u00b7den\u00b7hast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ach, zu wenig f\u00fcr die Ewigkeit!", "tokens": ["Und", "ach", ",", "zu", "we\u00b7nig", "f\u00fcr", "die", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PTKA", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und doch; wie du auch lachst zu Spiel und Scherz,", "tokens": ["Und", "doch", ";", "wie", "du", "auch", "lachst", "zu", "Spiel", "und", "Scherz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PWAV", "PPER", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht g\u00e4b ich meinen gramverst\u00f6rten Sinn", "tokens": ["Nicht", "g\u00e4b", "ich", "mei\u00b7nen", "gram\u00b7ver\u00b7st\u00f6r\u00b7ten", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr deine Lust und f\u00fcr dein leichtes Herz,", "tokens": ["F\u00fcr", "dei\u00b7ne", "Lust", "und", "f\u00fcr", "dein", "leich\u00b7tes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um das ", "tokens": ["Um", "das"], "token_info": ["word", "word"], "pos": ["KOUI", "ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Denn Werke des Gedankens und der That,", "tokens": ["Denn", "Wer\u00b7ke", "des", "Ge\u00b7dan\u00b7kens", "und", "der", "That", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie sie dein Geist noch niemals in sich trug,", "tokens": ["Wie", "sie", "dein", "Geist", "noch", "nie\u00b7mals", "in", "sich", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ADV", "ADV", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sollst du vollbracht sehn, wenn die Zeit sich naht,", "tokens": ["Sollst", "du", "voll\u00b7bracht", "sehn", ",", "wenn", "die", "Zeit", "sich", "naht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$,", "KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch meiner Seele letzten Athemzug!", "tokens": ["Durch", "mei\u00b7ner", "See\u00b7le", "letz\u00b7ten", "A\u00b7them\u00b7zug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}