{"dta.poem.21221": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n  An die Clio.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Jtzt ist es Clio zeit/ du meine Sorg vnd M\u00fch/", "tokens": ["Jtzt", "ist", "es", "Clio", "zeit", "/", "du", "mei\u00b7ne", "Sorg", "vnd", "M\u00fch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "NN", "$(", "PPER", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bewaffne deinen Mund mit Gottheit-vollen flammen/", "tokens": ["Be\u00b7waff\u00b7ne", "dei\u00b7nen", "Mund", "mit", "Gott\u00b7heit\u00b7vol\u00b7len", "flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd lerne heilig sein/ vnd setze wol zusammen", "tokens": ["Vnd", "ler\u00b7ne", "hei\u00b7lig", "sein", "/", "vnd", "set\u00b7ze", "wol", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "VAINF", "$(", "KON", "VVFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die W\u00f6rter vnd den Sinn/ vnd fall auff beyde Knie.", "tokens": ["Die", "W\u00f6r\u00b7ter", "vnd", "den", "Sinn", "/", "vnd", "fall", "auff", "bey\u00b7de", "Knie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$(", "KON", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du solt den ", "tokens": ["Du", "solt", "den"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Wirst auff die Axen du/ die blo\u00df de\u00df Himmels Ecken/", "tokens": ["Wirst", "auff", "die", "A\u00b7xen", "du", "/", "die", "blo\u00df", "de\u00df", "Him\u00b7mels", "E\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PPER", "$(", "ART", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd nicht ", "tokens": ["Vnd", "nicht"], "token_info": ["word", "word"], "pos": ["KON", "PTKNEG"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Jtzt k\u00fcrtze dieses Werck/ das jeder lieber list.", "tokens": ["Jtzt", "k\u00fcrt\u00b7ze", "die\u00b7ses", "Werck", "/", "das", "je\u00b7der", "lie\u00b7ber", "list", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "$(", "PRELS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Genung/ wann nur ein Satz dem ", "tokens": ["Ge\u00b7nung", "/", "wann", "nur", "ein", "Satz", "dem"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "PWAV", "ADV", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd h\u00f6rt al\u00df ein ", "tokens": ["Vnd", "h\u00f6rt", "al\u00df", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Viel B\u00fccher thun es nicht; du bist wol dran gewesen/", "tokens": ["Viel", "B\u00fc\u00b7cher", "thun", "es", "nicht", ";", "du", "bist", "wol", "dran", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VAFIN", "ADV", "PAV", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}