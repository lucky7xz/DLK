{"dta.poem.11473": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Bey der beerdigung tit. Fr. M. Cath.  \n Stryckin, geb. Alexandrin,  an. 1706 d. 26  \n Nov.  im nahmen Herrn  \n Lic. B.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich geb es leichte zu, Hoch-edler! da\u00df die wunden,", "tokens": ["Ich", "geb", "es", "leich\u00b7te", "zu", ",", "Hoch\u00b7ed\u00b7ler", "!", "da\u00df", "die", "wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$,", "NN", "$.", "KOUS", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So deiner liebsten tod dir in das hertze schl\u00e4gt,", "tokens": ["So", "dei\u00b7ner", "liebs\u00b7ten", "tod", "dir", "in", "das", "hert\u00b7ze", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allzuempfindlich sind: Ich hab es auch empfunden,", "tokens": ["All\u00b7zu\u00b7em\u00b7pfind\u00b7lich", "sind", ":", "Ich", "hab", "es", "auch", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wei\u00df, was dieser schlag vor angst und pein erregt.", "tokens": ["Und", "wei\u00df", ",", "was", "die\u00b7ser", "schlag", "vor", "angst", "und", "pein", "er\u00b7regt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PDAT", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum untersteh ich mich, dein klagen nicht zu tadeln;", "tokens": ["Drum", "un\u00b7ter\u00b7steh", "ich", "mich", ",", "dein", "kla\u00b7gen", "nicht", "zu", "ta\u00b7deln", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "$,", "PPOSAT", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn welch gesetze lehrt uns unbeweglich seyn?", "tokens": ["Denn", "welch", "ge\u00b7set\u00b7ze", "lehrt", "uns", "un\u00b7be\u00b7weg\u00b7lich", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "VVFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Scharrt doch, was tugend, witz und nette sitten adeln,", "tokens": ["Scharrt", "doch", ",", "was", "tu\u00b7gend", ",", "witz", "und", "net\u00b7te", "sit\u00b7ten", "a\u00b7deln", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PRELS", "ADJD", "$,", "VVIMP", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auch selbst ein ", "tokens": ["Auch", "selbst", "ein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Ich kenne den verlust, und wei\u00df, was du verlohren;", "tokens": ["Ich", "ken\u00b7ne", "den", "ver\u00b7lust", ",", "und", "wei\u00df", ",", "was", "du", "ver\u00b7loh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "VVFIN", "$,", "KON", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dergleichen perlen sind itzt kein gemeiner schatz:", "tokens": ["Derg\u00b7lei\u00b7chen", "per\u00b7len", "sind", "itzt", "kein", "ge\u00b7mei\u00b7ner", "schatz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "VAFIN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein tugend-reiches weib ist nicht so bald gebohren:", "tokens": ["Ein", "tu\u00b7gen\u00b7drei\u00b7ches", "weib", "ist", "nicht", "so", "bald", "ge\u00b7boh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die tugend findet nicht in allen frauen platz.", "tokens": ["Die", "tu\u00b7gend", "fin\u00b7det", "nicht", "in", "al\u00b7len", "frau\u00b7en", "platz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Man findet Helenen, die schwanen im gesichte,", "tokens": ["Man", "fin\u00b7det", "He\u00b7le\u00b7nen", ",", "die", "schwa\u00b7nen", "im", "ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NE", "$,", "PRELS", "VVFIN", "APPRART", "ADJA", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Und in der seele doch so schwartz, als raben, seyn;", "tokens": ["Und", "in", "der", "see\u00b7le", "doch", "so", "schwartz", ",", "als", "ra\u00b7ben", ",", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "VVFIN", "ADV", "ADV", "ADJD", "$,", "KOUS", "NN", "$,", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Denn die erfahrung macht des Plato satz zu nichte:", "tokens": ["Denn", "die", "er\u00b7fah\u00b7rung", "macht", "des", "Pla\u00b7to", "satz", "zu", "nich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NE", "ADJD", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die tugend kehre stets in sch\u00f6nen leibern ein.", "tokens": ["Die", "tu\u00b7gend", "keh\u00b7re", "stets", "in", "sch\u00f6\u00b7nen", "lei\u00b7bern", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dein weinen ist gerecht. Weil deine Catharine,", "tokens": ["Dein", "wei\u00b7nen", "ist", "ge\u00b7recht", ".", "Weil", "dei\u00b7ne", "Cat\u00b7ha\u00b7ri\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "ADJD", "$.", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dem geist und leibe nach, vollkommen sch\u00f6ne war;", "tokens": ["Dem", "geist", "und", "lei\u00b7be", "nach", ",", "voll\u00b7kom\u00b7men", "sch\u00f6\u00b7ne", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "PTKVZ", "$,", "ADJD", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ach! w\u00e4re sie doch noch! so kehrte sich die b\u00fchne", "tokens": ["Ach", "!", "w\u00e4\u00b7re", "sie", "doch", "noch", "!", "so", "kehr\u00b7te", "sich", "die", "b\u00fch\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VAFIN", "PPER", "ADV", "ADV", "$.", "ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der s\u00fcssen liebe nicht in eine todten-bahr.", "tokens": ["Der", "s\u00fcs\u00b7sen", "lie\u00b7be", "nicht", "in", "ei\u00b7ne", "tod\u00b7ten\u00b7bahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Drum, edle thr\u00e4nen! rinnt, die liebe will es haben:", "tokens": ["Drum", ",", "ed\u00b7le", "thr\u00e4\u00b7nen", "!", "rinnt", ",", "die", "lie\u00b7be", "will", "es", "ha\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ADJA", "NN", "$.", "VVFIN", "$,", "PRELS", "VVFIN", "VMFIN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dein grosser vater f\u00e4llt selbst deiner wehmuth bey:", "tokens": ["Dein", "gros\u00b7ser", "va\u00b7ter", "f\u00e4llt", "selbst", "dei\u00b7ner", "weh\u00b7muth", "bey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ach! da\u00df wir doch, was fromm und seltsam ist, begraben!", "tokens": ["Ach", "!", "da\u00df", "wir", "doch", ",", "was", "fromm", "und", "selt\u00b7sam", "ist", ",", "be\u00b7gra\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "ADV", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ist in der gantzen welt nichts vor dem tode frey?", "tokens": ["Ist", "in", "der", "gant\u00b7zen", "welt", "nichts", "vor", "dem", "to\u00b7de", "frey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "PIS", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wei\u00df denn Papinian hier keinen rath zu geben?", "tokens": ["Wei\u00df", "denn", "Pa\u00b7pi\u00b7ni\u00b7an", "hier", "kei\u00b7nen", "rath", "zu", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Darff der gesetzte tag nicht abgeschrieben seyn?", "tokens": ["Darff", "der", "ge\u00b7setz\u00b7te", "tag", "nicht", "ab\u00b7ge\u00b7schrie\u00b7ben", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Hat denn die jugend nicht ein gr\u00f6sser recht zu leben?", "tokens": ["Hat", "denn", "die", "ju\u00b7gend", "nicht", "ein", "gr\u00f6s\u00b7ser", "recht", "zu", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKNEG", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und schl\u00e4gt der donner auch in gr\u00fcne myrthen ein?", "tokens": ["Und", "schl\u00e4gt", "der", "don\u00b7ner", "auch", "in", "gr\u00fc\u00b7ne", "myr\u00b7then", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ach leyder! ja er schl\u00e4gt, und hat bereits geschlagen,", "tokens": ["Ach", "ley\u00b7der", "!", "ja", "er", "schl\u00e4gt", ",", "und", "hat", "be\u00b7reits", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "ADV", "PPER", "VVFIN", "$,", "KON", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df dein best\u00fcrtzter geist kaum bey sich selber ist.", "tokens": ["Da\u00df", "dein", "be\u00b7st\u00fcrtz\u00b7ter", "geist", "kaum", "bey", "sich", "sel\u00b7ber", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "PRF", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dein schatz, dein holder schatz, wird in die grufft getragen,", "tokens": ["Dein", "schatz", ",", "dein", "hol\u00b7der", "schatz", ",", "wird", "in", "die", "grufft", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wo man nur einsamkeit und finsterni\u00df erkiest.", "tokens": ["Wo", "man", "nur", "ein\u00b7sam\u00b7keit", "und", "fins\u00b7ter\u00b7ni\u00df", "er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADJD", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die rosen wandeln sich in traurige eypressen;", "tokens": ["Die", "ro\u00b7sen", "wan\u00b7deln", "sich", "in", "trau\u00b7ri\u00b7ge", "ey\u00b7pres\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Allein, was rei\u00df ich hier die wunden weiter auf?", "tokens": ["Al\u00b7lein", ",", "was", "rei\u00df", "ich", "hier", "die", "wun\u00b7den", "wei\u00b7ter", "auf", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wo aug und traurigkeit gem\u00fcth und hertze pressen,", "tokens": ["Wo", "aug", "und", "trau\u00b7rig\u00b7keit", "ge\u00b7m\u00fcth", "und", "hert\u00b7ze", "pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "NN", "ADJD", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da l\u00e4st man ohnedem den thr\u00e4nen ihren lauff.", "tokens": ["Da", "l\u00e4st", "man", "oh\u00b7ne\u00b7dem", "den", "thr\u00e4\u00b7nen", "ih\u00b7ren", "lauff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PAV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und darum solt ich nur bew\u00e4hrte pflaster bringen;", "tokens": ["Und", "da\u00b7rum", "solt", "ich", "nur", "be\u00b7w\u00e4hr\u00b7te", "pflas\u00b7ter", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VMFIN", "PPER", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Doch wer die harffe selbst an bittre weyden h\u00e4ngt,", "tokens": ["Doch", "wer", "die", "harf\u00b7fe", "selbst", "an", "bitt\u00b7re", "wey\u00b7den", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Dem kan, Hoch-edler Stryck! kein rechter trost gelingen:", "tokens": ["Dem", "kan", ",", "Hoch\u00b7ed\u00b7ler", "Stryck", "!", "kein", "rech\u00b7ter", "trost", "ge\u00b7lin\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "ADJA", "NN", "$.", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wie sehr man immerhin auf \u00f6l und balsam denckt.", "tokens": ["Wie", "sehr", "man", "im\u00b7mer\u00b7hin", "auf", "\u00f6l", "und", "bal\u00b7sam", "denckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "ADV", "APPR", "NE", "KON", "ADJD", "VVFIN", "$."