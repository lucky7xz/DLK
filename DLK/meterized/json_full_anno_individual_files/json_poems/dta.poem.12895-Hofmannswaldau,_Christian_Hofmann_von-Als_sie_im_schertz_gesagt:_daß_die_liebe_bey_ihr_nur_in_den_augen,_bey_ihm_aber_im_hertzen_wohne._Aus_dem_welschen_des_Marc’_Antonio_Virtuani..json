{"dta.poem.12895": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie im schertz gesagt: da\u00df die  \n liebe bey ihr nur in den augen, bey ihm  \n aber im hertzen wohne.  \n Aus dem welschen des   Marc\u2019 Antonio  \n Virtuani.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein schertz stimmt mit der wahrheit ein,", "tokens": ["Dein", "schertz", "stimmt", "mit", "der", "wahr\u00b7heit", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil ich der liebe glut in meinem hertzen sp\u00fcre.", "tokens": ["Weil", "ich", "der", "lie\u00b7be", "glut", "in", "mei\u00b7nem", "hert\u00b7zen", "sp\u00fc\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja, Chloris! du hast recht, denn deiner augen schein", "tokens": ["Ja", ",", "Chlo\u00b7ris", "!", "du", "hast", "recht", ",", "denn", "dei\u00b7ner", "au\u00b7gen", "schein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NE", "$.", "PPER", "VAFIN", "ADJD", "$,", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Beweist, da\u00df Amor selbst derselben blicke f\u00fchre.", "tokens": ["Be\u00b7weist", ",", "da\u00df", "A\u00b7mor", "selbst", "der\u00b7sel\u00b7ben", "bli\u00b7cke", "f\u00fch\u00b7re", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NE", "ADV", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nur blo\u00df die w\u00fcrckung macht bey uns den unterscheid;", "tokens": ["Nur", "blo\u00df", "die", "w\u00fcr\u00b7ckung", "macht", "bey", "uns", "den", "un\u00b7ter\u00b7scheid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nachdem der lose dieb dein kaltes hertz ergetzet,", "tokens": ["Nach\u00b7dem", "der", "lo\u00b7se", "dieb", "dein", "kal\u00b7tes", "hertz", "er\u00b7get\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mich aber nur in angst und hei\u00dfe flammen setzet.", "tokens": ["Mich", "a\u00b7ber", "nur", "in", "angst", "und", "hei\u00b7\u00dfe", "flam\u00b7men", "set\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "NN", "KON", "VVFIN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie w\u00e4r ich demnach so erfreut,", "tokens": ["Wie", "w\u00e4r", "ich", "dem\u00b7nach", "so", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PAV", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Wenn es der himmel f\u00fcgen solte,", "tokens": ["Wenn", "es", "der", "him\u00b7mel", "f\u00fc\u00b7gen", "sol\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df Amsr seinen platz in uns verwechseln wolte!", "tokens": ["Da\u00df", "Amsr", "sei\u00b7nen", "platz", "in", "uns", "ver\u00b7wech\u00b7seln", "wol\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}