{"textgrid.poem.26625": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Also seh' ich dich wieder, du schimmernde Stadt an der Donau,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Also seh' ich dich wieder, du schimmernde Stadt an der Donau,", "tokens": ["Al\u00b7so", "seh'", "ich", "dich", "wie\u00b7der", ",", "du", "schim\u00b7mern\u00b7de", "Stadt", "an", "der", "Do\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$,", "PPER", "ADJA", "NN", "APPR", "ART", "NE", "$,"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Die ich seit Jahren bereits nur mehr im Fluge gestreift!", "tokens": ["Die", "ich", "seit", "Jah\u00b7ren", "be\u00b7reits", "nur", "mehr", "im", "Flu\u00b7ge", "ge\u00b7streift", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADV", "ADV", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Traut umfing mich ein l\u00e4ndliches Heim, es heischte die Muse", "tokens": ["Traut", "um\u00b7fing", "mich", "ein", "l\u00e4nd\u00b7li\u00b7ches", "Heim", ",", "es", "heischte", "die", "Mu\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Ernsteste Sammlung \u2013 und so hielt ich mich selber verbannt.", "tokens": ["Erns\u00b7tes\u00b7te", "Samm\u00b7lung", "\u2013", "und", "so", "hielt", "ich", "mich", "sel\u00b7ber", "ver\u00b7bannt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "KON", "ADV", "VVFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Jetzt, am Abend des Lebens, nach fast vollendetem Tagwerk,", "tokens": ["Jetzt", ",", "am", "A\u00b7bend", "des", "Le\u00b7bens", ",", "nach", "fast", "voll\u00b7en\u00b7de\u00b7tem", "Tag\u00b7werk", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "NN", "ART", "NN", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Treibt Erinn'rung mich, treibt mich die Sehnsucht zur\u00fcck.", "tokens": ["Treibt", "Er\u00b7inn'\u00b7rung", "mich", ",", "treibt", "mich", "die", "Sehn\u00b7sucht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Freilich bist du nicht mehr, die du warst! Es gingen die Zeiten", "tokens": ["Frei\u00b7lich", "bist", "du", "nicht", "mehr", ",", "die", "du", "warst", "!", "Es", "gin\u00b7gen", "die", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "$,", "PRELS", "PPER", "VAFIN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Mit ver\u00e4nderndem Lauf \u00fcber dein Weichbild dahin.", "tokens": ["Mit", "ver\u00b7\u00e4n\u00b7dern\u00b7dem", "Lauf", "\u00fc\u00b7ber", "dein", "Weich\u00b7bild", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PAV", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Altes, Gewohntes versank, daran mir die Seele gehangen,", "tokens": ["Al\u00b7tes", ",", "Ge\u00b7wohn\u00b7tes", "ver\u00b7sank", ",", "da\u00b7ran", "mir", "die", "See\u00b7le", "ge\u00b7han\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "$,", "PAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Und ein Fremdling l\u00e4ngst bin ich dem neuen Geschlecht.", "tokens": ["Und", "ein", "Fremd\u00b7ling", "l\u00e4ngst", "bin", "ich", "dem", "neu\u00b7en", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Aber es weht noch die Luft her\u00fcber vom Kahlengebirge,", "tokens": ["A\u00b7ber", "es", "weht", "noch", "die", "Luft", "her\u00b7\u00fc\u00b7ber", "vom", "Kah\u00b7len\u00b7ge\u00b7bir\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Die ich geathmet als Kind, die mich zum Manne gereift;", "tokens": ["Die", "ich", "ge\u00b7ath\u00b7met", "als", "Kind", ",", "die", "mich", "zum", "Man\u00b7ne", "ge\u00b7reift", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "KOKOM", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Noch zu gewahren dem Aug' sind Reste entschwundener Tage,", "tokens": ["Noch", "zu", "ge\u00b7wah\u00b7ren", "dem", "Aug'", "sind", "Res\u00b7te", "ent\u00b7schwun\u00b7de\u00b7ner", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "ART", "NN", "VAFIN", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Still wehm\u00fcthig erfreu'n sie des Elegikers Herz.", "tokens": ["Still", "weh\u00b7m\u00fct\u00b7hig", "er\u00b7freu'n", "sie", "des", "E\u00b7le\u00b7gi\u00b7kers", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Und so sei mir gegr\u00fc\u00dft! F\u00fcr immer nun bleib' ich der Deine,", "tokens": ["Und", "so", "sei", "mir", "ge\u00b7gr\u00fc\u00dft", "!", "F\u00fcr", "im\u00b7mer", "nun", "bleib'", "ich", "der", "Dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$.", "APPR", "ADV", "ADV", "VVFIN", "PPER", "ART", "PPOSAT", "$,"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Ob du auch nie mich vermi\u00dft, h\u00e4ltst du mich liebend doch fest.", "tokens": ["Ob", "du", "auch", "nie", "mich", "ver\u00b7mi\u00dft", ",", "h\u00e4ltst", "du", "mich", "lie\u00b7bend", "doch", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "ADJD", "ADV", "PTKVZ", "$."], "meter": "+---+-+-+-+--+", "measu