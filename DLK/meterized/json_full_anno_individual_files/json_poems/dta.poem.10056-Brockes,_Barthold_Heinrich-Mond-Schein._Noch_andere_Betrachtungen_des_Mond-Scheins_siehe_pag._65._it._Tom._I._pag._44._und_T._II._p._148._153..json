{"dta.poem.10056": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mond-Schein. Noch andere Betrachtungen des Mond-Scheins siehe  pag. 65.  \n it. Tom. I. pag.  44. und  T. II. p.  148. 153.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit einer still- und reinen Lust,", "tokens": ["Mit", "ei\u00b7ner", "still", "und", "rei\u00b7nen", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einer sanften Freud und Wonne,", "tokens": ["Mit", "ei\u00b7ner", "sanf\u00b7ten", "Freud", "und", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erf\u00fcllte neulich meine Brust", "tokens": ["Er\u00b7f\u00fcll\u00b7te", "neu\u00b7lich", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der helle Mond, die weisse Schatten-Sonne;", "tokens": ["Der", "hel\u00b7le", "Mond", ",", "die", "weis\u00b7se", "Schat\u00b7ten\u00b7Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Indem sich nemlich Licht und Schatten,", "tokens": ["In\u00b7dem", "sich", "nem\u00b7lich", "Licht", "und", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "NN", "KON", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Auf Erden, in der Lufft und Fluth,", "tokens": ["Auf", "Er\u00b7den", ",", "in", "der", "Lufft", "und", "Fluth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So angenehm gemischt, so sanft verbunden hatten,", "tokens": ["So", "an\u00b7ge\u00b7nehm", "ge\u00b7mischt", ",", "so", "sanft", "ver\u00b7bun\u00b7den", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "ADV", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df durch mein Auge, Blut und Muth", "tokens": ["Da\u00df", "durch", "mein", "Au\u00b7ge", ",", "Blut", "und", "Muth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zu einer Harmonie, zu einer s\u00fcssen Stille,", "tokens": ["Zu", "ei\u00b7ner", "Har\u00b7mo\u00b7nie", ",", "zu", "ei\u00b7ner", "s\u00fcs\u00b7sen", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und in der Creaturen Pracht und F\u00fclle,", "tokens": ["Und", "in", "der", "Crea\u00b7tu\u00b7ren", "Pracht", "und", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Zu dem, der sie erschuff, gebracht,", "tokens": ["Zu", "dem", ",", "der", "sie", "er\u00b7schuff", ",", "ge\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "PTKVZ", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und halb entz\u00fccket ward.", "tokens": ["Und", "halb", "ent\u00b7z\u00fc\u00b7cket", "ward", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es war die holde Nacht", "tokens": ["Es", "war", "die", "hol\u00b7de", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von denen R\u00e4chten, welche man", "tokens": ["Von", "de\u00b7nen", "R\u00e4ch\u00b7ten", ",", "wel\u00b7che", "man"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PRELS", "NN", "$,", "PRELS", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht leicht, als in der Tichter Schriften, finden,", "tokens": ["Nicht", "leicht", ",", "als", "in", "der", "Tich\u00b7ter", "Schrif\u00b7ten", ",", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "KOUS", "APPR", "ART", "NN", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und keiner gnug bewundern kann.", "tokens": ["Und", "kei\u00b7ner", "gnug", "be\u00b7wun\u00b7dern", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Himmel war entw\u00f6lckt, gantz heiter, klar und rein.", "tokens": ["Der", "Him\u00b7mel", "war", "ent\u00b7w\u00f6lckt", ",", "gantz", "hei\u00b7ter", ",", "klar", "und", "rein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Lufft war lau und still, es funckelten die Sterne", "tokens": ["Die", "Lufft", "war", "lau", "und", "still", ",", "es", "fun\u00b7ckel\u00b7ten", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In einem regen Glantz. Der Silber-farbne Schein", "tokens": ["In", "ei\u00b7nem", "re\u00b7gen", "Glantz", ".", "Der", "Sil\u00b7ber\u00b7fa\u00b7rb\u00b7ne", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "Des eben vollen Monds, erf\u00fcllt mit sanftem Strahl", "tokens": ["Des", "e\u00b7ben", "vol\u00b7len", "Monds", ",", "er\u00b7f\u00fcllt", "mit", "sanf\u00b7tem", "Strahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wald, Wiesen, G\u00e4rten, Berg und Thal.", "tokens": ["Wald", ",", "Wie\u00b7sen", ",", "G\u00e4r\u00b7ten", ",", "Berg", "und", "Thal", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein\u2019 ungemein\u2019 und angenehme Stille", "tokens": ["Ein'", "un\u00b7ge\u00b7mein'", "und", "an\u00b7ge\u00b7neh\u00b7me", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Regieret\u2019 \u00fcberall:", "tokens": ["Re\u00b7gie\u00b7ret'", "\u00fc\u00b7be\u00b7rall", ":"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Und ward durch nichts, als durch der Nachtigall", "tokens": ["Und", "ward", "durch", "nichts", ",", "als", "durch", "der", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PIS", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Hell klingend singen, unterbrochen.", "tokens": ["Hell", "klin\u00b7gend", "sin\u00b7gen", ",", "un\u00b7ter\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADJD", "VVINF", "$,", "VVPP", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie lieblich ist, sprach ich, des Mondes sanftes Licht,", "tokens": ["Wie", "lieb\u00b7lich", "ist", ",", "sprach", "ich", ",", "des", "Mon\u00b7des", "sanf\u00b7tes", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann es das feuchte Rund der duncklen Welt bestrahlet,", "tokens": ["Wann", "es", "das", "feuch\u00b7te", "Rund", "der", "dunck\u00b7len", "Welt", "be\u00b7strah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und durch der W\u00e4lder Schatten bricht!", "tokens": ["Und", "durch", "der", "W\u00e4l\u00b7der", "Schat\u00b7ten", "bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In welcher Harmonie sieht ein vergn\u00fcgt Gesicht", "tokens": ["In", "wel\u00b7cher", "Har\u00b7mo\u00b7nie", "sieht", "ein", "ver\u00b7gn\u00fcgt", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "ART", "VVPP", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So dann das, was man sieht! Die C\u00f6rper sind gemahlet", "tokens": ["So", "dann", "das", ",", "was", "man", "sieht", "!", "Die", "C\u00f6r\u00b7per", "sind", "ge\u00b7mah\u00b7let"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PDS", "$,", "PRELS", "PIS", "VVFIN", "$.", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In einem holden Grad von Glantz und Finsterni\u00df;", "tokens": ["In", "ei\u00b7nem", "hol\u00b7den", "Grad", "von", "Glantz", "und", "Fins\u00b7ter\u00b7ni\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Formen sind sowohl, als Farben, ungewi\u00df,", "tokens": ["Die", "For\u00b7men", "sind", "so\u00b7wohl", ",", "als", "Far\u00b7ben", ",", "un\u00b7ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "KOUS", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und alles reitzet unsre Brust", "tokens": ["Und", "al\u00b7les", "reit\u00b7zet", "uns\u00b7re", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zu einer still- und sanften Lust.", "tokens": ["Zu", "ei\u00b7ner", "still", "und", "sanf\u00b7ten", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}