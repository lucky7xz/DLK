{"dta.poem.21381": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XVIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! meine Seele ward betr\u00fcbt", "tokens": ["Ach", "!", "mei\u00b7ne", "See\u00b7le", "ward", "be\u00b7tr\u00fcbt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie des Odysseus Seele,", "tokens": ["Wie", "des", "O\u00b7dys\u00b7seus", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als er geh\u00f6rt, da\u00df Polyphem", "tokens": ["Als", "er", "ge\u00b7h\u00f6rt", ",", "da\u00df", "Po\u00b7ly\u00b7phem"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Felsblock schob vor die H\u00f6hle.", "tokens": ["Den", "Fels\u00b7block", "schob", "vor", "die", "H\u00f6h\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Es trat an den Wagen ein Corporal", "tokens": ["Es", "trat", "an", "den", "Wa\u00b7gen", "ein", "Cor\u00b7po\u00b7ral"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und frug uns: wie wir hie\u00dfen?", "tokens": ["Und", "frug", "uns", ":", "wie", "wir", "hie\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich hei\u00dfe Niemand, bin Augenarzt", "tokens": ["Ich", "hei\u00b7\u00dfe", "Nie\u00b7mand", ",", "bin", "Au\u00b7gen\u00b7arzt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "$,", "VAFIN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und steche den Staar den Riesen.", "tokens": ["Und", "ste\u00b7che", "den", "Staar", "den", "Rie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Im Wirthshaus ward mir noch schlimmer zu Muth,", "tokens": ["Im", "Wirths\u00b7haus", "ward", "mir", "noch", "schlim\u00b7mer", "zu", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Essen wollt mir nicht schmecken.", "tokens": ["Das", "Es\u00b7sen", "wollt", "mir", "nicht", "schme\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ging schlafen sogleich, doch schlief ich nicht,", "tokens": ["Ging", "schla\u00b7fen", "sog\u00b7leich", ",", "doch", "schlief", "ich", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mich dr\u00fcckten so schwer die Decken.", "tokens": ["Mich", "dr\u00fcck\u00b7ten", "so", "schwer", "die", "De\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Es war ein breites Federbett,", "tokens": ["Es", "war", "ein", "brei\u00b7tes", "Fe\u00b7der\u00b7bett", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gardinen von rothem Damaste,", "tokens": ["Gar\u00b7di\u00b7nen", "von", "ro\u00b7them", "Da\u00b7mas\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der Himmel von verblichenem Gold,", "tokens": ["Der", "Him\u00b7mel", "von", "ver\u00b7bli\u00b7che\u00b7nem", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mit einem schmutzigen Quaste.", "tokens": ["Mit", "ei\u00b7nem", "schmut\u00b7zi\u00b7gen", "Quas\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}