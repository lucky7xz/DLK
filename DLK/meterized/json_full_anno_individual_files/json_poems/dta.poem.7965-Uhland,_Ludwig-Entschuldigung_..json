{"dta.poem.7965": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Entschuldigung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Was ich in Liedern manchesmal berichte", "tokens": ["Was", "ich", "in", "Lie\u00b7dern", "man\u00b7ches\u00b7mal", "be\u00b7rich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von K\u00fcssen in vertrauter Abendstunde,", "tokens": ["Von", "K\u00fcs\u00b7sen", "in", "ver\u00b7trau\u00b7ter", "A\u00b7bends\u00b7tun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von der Umarmung wonnevollem Bunde,", "tokens": ["Von", "der", "Um\u00b7ar\u00b7mung", "won\u00b7ne\u00b7vol\u00b7lem", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach! Traum ist, leider, Alles und Gedichte.", "tokens": ["Ach", "!", "Traum", "ist", ",", "lei\u00b7der", ",", "Al\u00b7les", "und", "Ge\u00b7dich\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "VAFIN", "$,", "ADV", "$,", "PIS", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und du noch gehest mit mir in\u2019s Gerichte,", "tokens": ["Und", "du", "noch", "ge\u00b7hest", "mit", "mir", "in's", "Ge\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du z\u00fcrnest meinem prahlerischen Munde:", "tokens": ["Du", "z\u00fcr\u00b7nest", "mei\u00b7nem", "prah\u00b7le\u00b7ri\u00b7schen", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von nie gew\u00e4hrtem Gl\u00fccke geb\u2019 er Kunde,", "tokens": ["Von", "nie", "ge\u00b7w\u00e4hr\u00b7tem", "Gl\u00fc\u00b7cke", "geb'", "er", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das, selbst gew\u00e4hrt, zum Schweigen stets verpflichte.", "tokens": ["Das", ",", "selbst", "ge\u00b7w\u00e4hrt", ",", "zum", "Schwei\u00b7gen", "stets", "ver\u00b7pflich\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADV", "VVPP", "$,", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Geliebte, la\u00df den strengen Ernst sich mildern", "tokens": ["Ge\u00b7lieb\u00b7te", ",", "la\u00df", "den", "stren\u00b7gen", "Ernst", "sich", "mil\u00b7dern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "ART", "ADJA", "NN", "PRF", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und l\u00e4chle zu den leichten Dichtertr\u00e4umen,", "tokens": ["Und", "l\u00e4ch\u00b7le", "zu", "den", "leich\u00b7ten", "Dich\u00b7ter\u00b7tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem unbewu\u00dften Spiel, den Schattenbildern!", "tokens": ["Dem", "un\u00b7be\u00b7wu\u00df\u00b7ten", "Spiel", ",", "den", "Schat\u00b7ten\u00b7bil\u00b7dern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der S\u00e4nger ruhet schlummernd oft im K\u00fchlen,", "tokens": ["Der", "S\u00e4n\u00b7ger", "ru\u00b7het", "schlum\u00b7mernd", "oft", "im", "K\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Inde\u00df die Harfe h\u00e4nget unter B\u00e4umen", "tokens": ["In\u00b7de\u00df", "die", "Har\u00b7fe", "h\u00e4n\u00b7get", "un\u00b7ter", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und in den Saiten L\u00fcfte s\u00e4uselnd w\u00fchlen.", "tokens": ["Und", "in", "den", "Sai\u00b7ten", "L\u00fcf\u00b7te", "s\u00e4u\u00b7selnd", "w\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}