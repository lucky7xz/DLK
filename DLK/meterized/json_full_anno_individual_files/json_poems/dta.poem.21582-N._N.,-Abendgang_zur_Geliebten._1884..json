{"dta.poem.21582": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Abendgang zur Geliebten.  \n 1884.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.85", "da:0.14"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Nun ist der Abend kommen,", "tokens": ["Nun", "ist", "der", "A\u00b7bend", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sterne sind entglommen,", "tokens": ["Die", "Ster\u00b7ne", "sind", "ent\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Stra\u00dfen schlummern m\u00e4hlig ein.", "tokens": ["Die", "Stra\u00b7\u00dfen", "schlum\u00b7mern", "m\u00e4h\u00b7lig", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Abwerf\u2019 ich all\u2019 mein M\u00fchen", "tokens": ["Ab\u00b7wer\u00b7f'", "ich", "all'", "mein", "M\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und la\u00df in mir erbl\u00fchen", "tokens": ["Und", "la\u00df", "in", "mir", "er\u00b7bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Liebe Sehnsucht ganz allein.", "tokens": ["Der", "Lie\u00b7be", "Sehn\u00b7sucht", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Rings gr\u00fc\u00dfen von den Zweigen", "tokens": ["Rings", "gr\u00fc\u00b7\u00dfen", "von", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die V\u00f6gel und es neigen", "tokens": ["Die", "V\u00f6\u00b7gel", "und", "es", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sich fl\u00fcsternd Busch und Blume mir;", "tokens": ["Sich", "fl\u00fcs\u00b7ternd", "Busch", "und", "Blu\u00b7me", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "NN", "KON", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So festlich ist mein Wesen,", "tokens": ["So", "fest\u00b7lich", "ist", "mein", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie m\u00f6gen leicht es lesen,", "tokens": ["Sie", "m\u00f6\u00b7gen", "leicht", "es", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie meine Seele fliegt zu Dir.", "tokens": ["Wie", "mei\u00b7ne", "See\u00b7le", "fliegt", "zu", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Kinder, die am Wege", "tokens": ["Die", "Kin\u00b7der", ",", "die", "am", "We\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich tummeln durch\u2019s Gehege,", "tokens": ["Sich", "tum\u00b7meln", "durch's", "Ge\u00b7he\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Sie reichen l\u00e4chelnd mir die Hand.", "tokens": ["Sie", "rei\u00b7chen", "l\u00e4\u00b7chelnd", "mir", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Winde, die da wehen,", "tokens": ["Die", "Win\u00b7de", ",", "die", "da", "we\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Wolken, die da gehen,", "tokens": ["Die", "Wol\u00b7ken", ",", "die", "da", "ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie kn\u00fcpfen mir ein rosig Band.", "tokens": ["Sie", "kn\u00fcp\u00b7fen", "mir", "ein", "ro\u00b7sig", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie weit seid ihr entschwunden,", "tokens": ["Wie", "weit", "seid", "ihr", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr sorgenschweren Stunden,", "tokens": ["Ihr", "sor\u00b7gen\u00b7schwe\u00b7ren", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie fern, wie fern liegt Kampf und Streit;", "tokens": ["Wie", "fern", ",", "wie", "fern", "liegt", "Kampf", "und", "Streit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Welt ist so voll Frieden,", "tokens": ["Die", "Welt", "ist", "so", "voll", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als l\u00e4g\u2019 sie abgeschieden \u2014", "tokens": ["Als", "l\u00e4g'", "sie", "ab\u00b7ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein See in gr\u00fcner Einsamkeit.", "tokens": ["Ein", "See", "in", "gr\u00fc\u00b7ner", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun steh\u2019 ich an dem Hause,", "tokens": ["Nun", "steh'", "ich", "an", "dem", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor meines Gl\u00fcckes Klause,", "tokens": ["Vor", "mei\u00b7nes", "Gl\u00fc\u00b7ckes", "Klau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und meiner Freuden Inbrunst wird Gebet;", "tokens": ["Und", "mei\u00b7ner", "Freu\u00b7den", "In\u00b7brunst", "wird", "Ge\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VAFIN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "La\u00df jedes Herz hienieden", "tokens": ["La\u00df", "je\u00b7des", "Herz", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durch Liebe finden Frieden,", "tokens": ["Durch", "Lie\u00b7be", "fin\u00b7den", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du g\u00f6ttlich Feuer, das die Welt durchweht.", "tokens": ["Du", "g\u00f6tt\u00b7lich", "Feu\u00b7er", ",", "das", "die", "Welt", "durch\u00b7weht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}