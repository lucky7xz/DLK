{"textgrid.poem.53415": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Am Sonntag", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch diese Nacht hat sich verlohren.", "tokens": ["Auch", "die\u00b7se", "Nacht", "hat", "sich", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VAFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sonntag wird geschaut,", "tokens": ["Der", "Sonn\u00b7tag", "wird", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Gott zu seiner Ruh erkohren,", "tokens": ["Den", "Gott", "zu", "sei\u00b7ner", "Ruh", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als er die Welt gebaut,", "tokens": ["Als", "er", "die", "Welt", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wil, da\u00df er uns in gemein", "tokens": ["Und", "wil", ",", "da\u00df", "er", "uns", "in", "ge\u00b7mein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "$,", "KOUS", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sol gleichfalls heilig seyn.", "tokens": ["Sol", "gleich\u00b7falls", "hei\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So danck' ich, Vater, dir von Hertzen,", "tokens": ["So", "danck", "ich", ",", "Va\u00b7ter", ",", "dir", "von", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du mein armes Gut", "tokens": ["Da\u00df", "du", "mein", "ar\u00b7mes", "Gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mich bewahrt f\u00fcr Noth und Schmertzen,", "tokens": ["Und", "mich", "be\u00b7wahrt", "f\u00fcr", "Noth", "und", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch deiner Engel Hut,", "tokens": ["Durch", "dei\u00b7ner", "En\u00b7gel", "Hut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Dieberey, auch Mord und Brand", "tokens": ["Die", "Die\u00b7be\u00b7rey", ",", "auch", "Mord", "und", "Brand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Getreulich abgewand.", "tokens": ["Ge\u00b7treu\u00b7lich", "ab\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und nun du mich erleben lassen", "tokens": ["Und", "nun", "du", "mich", "er\u00b7le\u00b7ben", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "PRF", "VVINF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den werthen Sabbath-Tag,", "tokens": ["Den", "wert\u00b7hen", "Sab\u00b7ba\u00b7th\u00b7Tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So gib, da\u00df ich auch Kr\u00e4fte fassen", "tokens": ["So", "gib", ",", "da\u00df", "ich", "auch", "Kr\u00e4f\u00b7te", "fas\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "$,", "KOUS", "PPER", "ADV", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihn recht feyren mag,", "tokens": ["Und", "ihn", "recht", "fey\u00b7ren", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und schenck mir hierzu allermeist,", "tokens": ["Und", "schenck", "mir", "hier\u00b7zu", "al\u00b7ler\u00b7meist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Herr, deinen guten Geist.", "tokens": ["Herr", ",", "dei\u00b7nen", "gu\u00b7ten", "Geist", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der \u00f6ffne meines Hertzens Pforte,", "tokens": ["Der", "\u00f6ff\u00b7ne", "mei\u00b7nes", "Hert\u00b7zens", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und thu was ihm bewu\u00dft,", "tokens": ["Und", "thu", "was", "ihm", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich an deinem heilgen Worte", "tokens": ["Da\u00df", "ich", "an", "dei\u00b7nem", "heil\u00b7gen", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hab alle meine Lust,", "tokens": ["Hab", "al\u00b7le", "mei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und mir es lasse lieber seyn", "tokens": ["Und", "mir", "es", "las\u00b7se", "lie\u00b7ber", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PPER", "VVFIN", "ADV", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als Gold und Edelstein.", "tokens": ["Als", "Gold", "und", "E\u00b7del\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "La\u00df mit den Sinnen mich nicht wancken,", "tokens": ["La\u00df", "mit", "den", "Sin\u00b7nen", "mich", "nicht", "wan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nimm g\u00e4ntzlich mich von mir,", "tokens": ["Nimm", "g\u00e4ntz\u00b7lich", "mich", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sperr' ein den Umbschweiff der Gedancken", "tokens": ["Sperr'", "ein", "den", "U\u00b7mbschweiff", "der", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ART", "NN", "ART", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und richte sie zu dir,", "tokens": ["Und", "rich\u00b7te", "sie", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Damit die gantze Predigt frey", "tokens": ["Da\u00b7mit", "die", "gant\u00b7ze", "Pre\u00b7digt", "frey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von fremden Sorgen sey.", "tokens": ["Von", "frem\u00b7den", "Sor\u00b7gen", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mach dir mein Hertz f\u00fcr allen Dingen", "tokens": ["Mach", "dir", "mein", "Hertz", "f\u00fcr", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu deinem Heiligthum,", "tokens": ["Zu", "dei\u00b7nem", "Hei\u00b7lig\u00b7thum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df den gantzen Tag erklingen", "tokens": ["Und", "la\u00df", "den", "gant\u00b7zen", "Tag", "er\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von deinem Lob und Ruhm,", "tokens": ["Von", "dei\u00b7nem", "Lob", "und", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gib keiner S\u00fcnd und Frevelthat", "tokens": ["Gib", "kei\u00b7ner", "S\u00fcnd", "und", "Fre\u00b7vel\u00b7that"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In meiner Seelen statt.", "tokens": ["In", "mei\u00b7ner", "See\u00b7len", "statt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mein Hertz er\u00f6ffne sich dem Armen", "tokens": ["Mein", "Hertz", "er\u00b7\u00f6ff\u00b7ne", "sich", "dem", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In seiner grossen Noth,", "tokens": ["In", "sei\u00b7ner", "gros\u00b7sen", "Noth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich aus Christlichem Erbarmen", "tokens": ["Da\u00df", "ich", "aus", "Christ\u00b7li\u00b7chem", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm theile mit mein Brod,", "tokens": ["Ihm", "thei\u00b7le", "mit", "mein", "Brod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des Krancken pfleg' und mancherley", "tokens": ["Des", "Kran\u00b7cken", "pfleg'", "und", "man\u00b7cher\u00b7ley"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erweise Lieb und Treu.", "tokens": ["Er\u00b7wei\u00b7se", "Lieb", "und", "Treu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Werd' ich die Woche so anfangen,", "tokens": ["Werd'", "ich", "die", "Wo\u00b7che", "so", "an\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wird mein Werck darauff", "tokens": ["So", "wird", "mein", "Werck", "dar\u00b7auff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auff aller Wolfahrt Zweck gelangen,", "tokens": ["Auff", "al\u00b7ler", "Wol\u00b7fahrt", "Zweck", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis sich beschleust mein Lauff,", "tokens": ["Bis", "sich", "be\u00b7schleust", "mein", "Lauff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und ich den ew'gen Sabbat-Tag", "tokens": ["Und", "ich", "den", "ew'\u00b7gen", "Sab\u00b7bat\u00b7Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Himmel halten mag.", "tokens": ["Im", "Him\u00b7mel", "hal\u00b7ten", "mag."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["APPRART", "NN", "VVFIN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Auch diese Nacht hat sich verlohren.", "tokens": ["Auch", "die\u00b7se", "Nacht", "hat", "sich", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VAFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sonntag wird geschaut,", "tokens": ["Der", "Sonn\u00b7tag", "wird", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Gott zu seiner Ruh erkohren,", "tokens": ["Den", "Gott", "zu", "sei\u00b7ner", "Ruh", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als er die Welt gebaut,", "tokens": ["Als", "er", "die", "Welt", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wil, da\u00df er uns in gemein", "tokens": ["Und", "wil", ",", "da\u00df", "er", "uns", "in", "ge\u00b7mein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "$,", "KOUS", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sol gleichfalls heilig seyn.", "tokens": ["Sol", "gleich\u00b7falls", "hei\u00b7lig", 