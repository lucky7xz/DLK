{"dta.poem.18397": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Celadons klage-brief an Lenoren/  \n als er wider willen ihre  Conversa-  \n tion  eine zeitlang meiden muste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lenore/ dieses ist das allererste schreiben/", "tokens": ["Le\u00b7no\u00b7re", "/", "die\u00b7ses", "ist", "das", "al\u00b7le\u00b7rers\u00b7te", "schrei\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PDS", "VAFIN", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das dir/ vollkommnes kind/ dein treuer diener schickt/", "tokens": ["Das", "dir", "/", "voll\u00b7komm\u00b7nes", "kind", "/", "dein", "treu\u00b7er", "die\u00b7ner", "schickt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "$(", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Seit dem er mu\u00df entfernt von deinem antlitz bleiben/", "tokens": ["Seit", "dem", "er", "mu\u00df", "ent\u00b7fernt", "von", "dei\u00b7nem", "ant\u00b7litz", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VMFIN", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und umb und neben sich nichts als verdrus erblickt.", "tokens": ["Und", "umb", "und", "ne\u00b7ben", "sich", "nichts", "als", "ver\u00b7drus", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "KON", "APPR", "PRF", "PIS", "KOKOM", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach glaube/ wo du kanst die sinnen auf mich lencken/", "tokens": ["Ach", "glau\u00b7be", "/", "wo", "du", "kanst", "die", "sin\u00b7nen", "auf", "mich", "len\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$(", "PWAV", "PPER", "VMFIN", "ART", "ADJA", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wo mein nahmen noch in deinen ohren klingt/", "tokens": ["Und", "wo", "mein", "nah\u00b7men", "noch", "in", "dei\u00b7nen", "oh\u00b7ren", "klingt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df nichts/ als herbes leid und tausendfaches kr\u00e4ncken/", "tokens": ["Da\u00df", "nichts", "/", "als", "her\u00b7bes", "leid", "und", "tau\u00b7send\u00b7fa\u00b7ches", "kr\u00e4n\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$(", "KOUS", "NN", "ADJD", "KON", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In mein gequ\u00e4ltes hertz mit vollem hauffen dringt.", "tokens": ["In", "mein", "ge\u00b7qu\u00e4l\u00b7tes", "hertz", "mit", "vol\u00b7lem", "hauf\u00b7fen", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich stehe gantz betr\u00fcbt/ die augen voller thr\u00e4nen/", "tokens": ["Ich", "ste\u00b7he", "gantz", "be\u00b7tr\u00fcbt", "/", "die", "au\u00b7gen", "vol\u00b7ler", "thr\u00e4\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das hertze bebet mir in der bekl\u00e4mmten brust/", "tokens": ["Das", "hert\u00b7ze", "be\u00b7bet", "mir", "in", "der", "be\u00b7kl\u00e4mm\u00b7ten", "brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Die freude wandelt sich in ungemeines sehnen/", "tokens": ["Die", "freu\u00b7de", "wan\u00b7delt", "sich", "in", "un\u00b7ge\u00b7mei\u00b7nes", "seh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich schmecke gall und gifft statt der gehossten lust.", "tokens": ["Ich", "schme\u00b7cke", "gall", "und", "gifft", "statt", "der", "ge\u00b7hoss\u00b7ten", "lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ach \u00e4rmster/ sag\u2019 ich oft/ wo bistu hingerathen/", "tokens": ["Ach", "\u00e4rms\u00b7ter", "/", "sag'", "ich", "oft", "/", "wo", "bis\u00b7tu", "hin\u00b7ge\u00b7ra\u00b7then", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJD", "$(", "VVFIN", "PPER", "ADV", "$(", "PWAV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie wird es weiterhin um deine sachen stehn?", "tokens": ["Wie", "wird", "es", "wei\u00b7ter\u00b7hin", "um", "dei\u00b7ne", "sa\u00b7chen", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Denn das verh\u00e4ngnis sucht/ (o unerh\u00f6rte thaten!)", "tokens": ["Denn", "das", "ver\u00b7h\u00e4ng\u00b7nis", "sucht", "/", "(", "o", "un\u00b7er\u00b7h\u00f6r\u00b7te", "tha\u00b7ten", "!", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "$(", "$(", "FM", "ADJA", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Mit dir verstossenem recht grausam umzugehn.", "tokens": ["Mit", "dir", "ver\u00b7stos\u00b7se\u00b7nem", "recht", "grau\u00b7sam", "um\u00b7zu\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach schwacher/ magstu wol dem himmel widerstreben/", "tokens": ["Ach", "schwa\u00b7cher", "/", "mags\u00b7tu", "wol", "dem", "him\u00b7mel", "wi\u00b7der\u00b7stre\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJD", "$(", "VMFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der donnerwetter l\u00e4\u00dft zu deiner quaal aufziehn?", "tokens": ["Der", "don\u00b7ner\u00b7wet\u00b7ter", "l\u00e4\u00dft", "zu", "dei\u00b7ner", "qua\u00b7al", "auf\u00b7ziehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wer kan mich aus der angst/ aus diesem kummer heben?", "tokens": ["Wer", "kan", "mich", "aus", "der", "angst", "/", "aus", "die\u00b7sem", "kum\u00b7mer", "he\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "APPR", "ART", "NN", "$(", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ach weh mir/ da\u00df ich ietzt so gar verlassen bin?", "tokens": ["Ach", "weh", "mir", "/", "da\u00df", "ich", "ietzt", "so", "gar", "ver\u00b7las\u00b7sen", "bin", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PPER", "$(", "KOUS", "PPER", "ADV", "ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Di\u00df sind/ galantes kind/ die traurigen gedancken/", "tokens": ["Di\u00df", "sind", "/", "ga\u00b7lan\u00b7tes", "kind", "/", "die", "trau\u00b7ri\u00b7gen", "ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$(", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mit denen sich mein geist fast unaufh\u00f6rlich plagt;", "tokens": ["Mit", "de\u00b7nen", "sich", "mein", "geist", "fast", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "plagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Doch darff ich gleichwol nicht mit meinem unstern zancken/", "tokens": ["Doch", "darff", "ich", "gleich\u00b7wol", "nicht", "mit", "mei\u00b7nem", "uns\u00b7tern", "zan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Es wird mir aller trost von selbigem versagt.", "tokens": ["Es", "wird", "mir", "al\u00b7ler", "trost", "von", "sel\u00b7bi\u00b7gem", "ver\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "NN", "APPR", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Gedenck\u2019 ich nun zur\u00fcck\u2019 an di\u00df/ was schon vergangen/", "tokens": ["Ge\u00b7denck", "ich", "nun", "zu\u00b7r\u00fcck'", "an", "di\u00df", "/", "was", "schon", "ver\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKVZ", "APPR", "PDS", "$(", "PWS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und sehe meinen stand mit nassen augen an/", "tokens": ["Und", "se\u00b7he", "mei\u00b7nen", "stand", "mit", "nas\u00b7sen", "au\u00b7gen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So werd\u2019 ich/ glaub es nur/ mit todes- furcht umfangen/", "tokens": ["So", "werd'", "ich", "/", "glaub", "es", "nur", "/", "mit", "to\u00b7des", "furcht", "um\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$(", "VVFIN", "PPER", "ADV", "$(", "APPR", "TRUNC", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Weil niemand meinen schmertz vollkommen lindern kan.", "tokens": ["Weil", "nie\u00b7mand", "mei\u00b7nen", "schmertz", "voll\u00b7kom\u00b7men", "lin\u00b7dern", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ADJD", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Allein was m\u00fch\u2019 ich mich dir dieses zu entdecken?", "tokens": ["Al\u00b7lein", "was", "m\u00fch'", "ich", "mich", "dir", "die\u00b7ses", "zu", "ent\u00b7de\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VMFIN", "PPER", "PPER", "PPER", "PDS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die worte stillen doch die vielen plagen nicht/", "tokens": ["Die", "wor\u00b7te", "stil\u00b7len", "doch", "die", "vie\u00b7len", "pla\u00b7gen", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "PIS", "VVFIN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und dieses schlechte blat wird dich vielleicht erschrecken/", "tokens": ["Und", "die\u00b7ses", "schlech\u00b7te", "blat", "wird", "dich", "viel\u00b7leicht", "er\u00b7schre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Indem es sonst von nichts/ als meinen seuffzern spricht.", "tokens": ["In\u00b7dem", "es", "sonst", "von", "nichts", "/", "als", "mei\u00b7nen", "seuff\u00b7zern", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PIS", "$(", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ach sch\u00f6nste/ solt\u2019 ich nur vor deinen knien liegen/", "tokens": ["Ach", "sch\u00f6ns\u00b7te", "/", "solt'", "ich", "nur", "vor", "dei\u00b7nen", "kni\u00b7en", "lie\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$(", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und mein gebeugter fus in deinem zimmer stehn/", "tokens": ["Und", "mein", "ge\u00b7beug\u00b7ter", "fus", "in", "dei\u00b7nem", "zim\u00b7mer", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So w\u00fcrde deine gunst mein elend bald besiegen/", "tokens": ["So", "w\u00fcr\u00b7de", "dei\u00b7ne", "gunst", "mein", "e\u00b7lend", "bald", "be\u00b7sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPOSAT", "ADJD", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und die verdrie\u00dfligkeit wie schnee und ei\u00df vergehn.", "tokens": ["Und", "die", "ver\u00b7drie\u00df\u00b7lig\u00b7keit", "wie", "schnee", "und", "ei\u00df", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KOKOM", "NN", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ein angenehmer blick w\u00fcrd\u2019 alle pein vers\u00fcssen/", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7mer", "blick", "w\u00fcrd'", "al\u00b7le", "pein", "ver\u00b7s\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die mich bey solchem weh matt und entkr\u00e4fftet macht/", "tokens": ["Die", "mich", "bey", "sol\u00b7chem", "weh", "matt", "und", "ent\u00b7kr\u00e4ff\u00b7tet", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "PTKVZ", "KON", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und also w\u00fcrd\u2019 ich denn vor dir gestehen m\u00fcssen/", "tokens": ["Und", "al\u00b7so", "w\u00fcrd'", "ich", "denn", "vor", "dir", "ge\u00b7ste\u00b7hen", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Da\u00df die vertrauligkeit mich zur genesung bracht.", "tokens": ["Da\u00df", "die", "ver\u00b7trau\u00b7lig\u00b7keit", "mich", "zur", "ge\u00b7ne\u00b7sung", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "Ach! aber/ was sind das vor ungereimte grillen/", "tokens": ["Ach", "!", "a\u00b7ber", "/", "was", "sind", "das", "vor", "un\u00b7ge\u00b7reim\u00b7te", "gril\u00b7len", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "$(", "PWS", "VAFIN", "PDS", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ach arm er Celadon/ was bildestu dir ein?", "tokens": ["Ach", "arm", "er", "Ce\u00b7la\u00b7don", "/", "was", "bil\u00b7des\u00b7tu", "dir", "ein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJD", "PPER", "NE", "$(", "PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Niemand wird dir den wunsch/ den du begehrst/ erf\u00fcllen/", "tokens": ["Nie\u00b7mand", "wird", "dir", "den", "wunsch", "/", "den", "du", "be\u00b7gehrst", "/", "er\u00b7f\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ART", "NN", "$(", "ART", "PPER", "VVFIN", "$(", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.44": {"text": "Und deine sch\u00f6ne selbst wil unempfindlich seyn.", "tokens": ["Und", "dei\u00b7ne", "sch\u00f6\u00b7ne", "selbst", "wil", "un\u00b7emp\u00b7find\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "ADV", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Di\u00df bl\u00e4st die einsamkeit mir t\u00e4glich in die ohren/", "tokens": ["Di\u00