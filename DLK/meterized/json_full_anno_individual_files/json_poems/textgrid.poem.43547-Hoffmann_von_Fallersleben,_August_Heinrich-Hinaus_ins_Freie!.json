er": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Nach der Weidenfl\u00f6te sollen", "tokens": ["Nach", "der", "Wei\u00b7den\u00b7fl\u00f6\u00b7te", "sol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wir springen zum Reih'n.", "tokens": ["Wir", "sprin\u00b7gen", "zum", "Reih'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Wer wollte nicht tanzen", "tokens": ["Wer", "woll\u00b7te", "nicht", "tan\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dem Fr\u00fchling zu lieb?", "tokens": ["Dem", "Fr\u00fch\u00b7ling", "zu", "lieb", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKA", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der den schlimmen langen Winter", "tokens": ["Der", "den", "schlim\u00b7men", "lan\u00b7gen", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns endlich vertrieb.", "tokens": ["Uns", "end\u00b7lich", "ver\u00b7trieb", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "So kommet, so kommet", "tokens": ["So", "kom\u00b7met", ",", "so", "kom\u00b7met"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ins Freie hinaus!", "tokens": ["Ins", "Frei\u00b7e", "hin\u00b7aus",