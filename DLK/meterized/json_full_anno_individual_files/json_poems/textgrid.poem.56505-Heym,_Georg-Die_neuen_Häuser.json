{"textgrid.poem.56505": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die neuen H\u00e4user", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im gr\u00fcnen Himmel, der manchmal knallt", "tokens": ["Im", "gr\u00fc\u00b7nen", "Him\u00b7mel", ",", "der", "manch\u00b7mal", "knallt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor Frost im rostigen Westen,", "tokens": ["Vor", "Frost", "im", "ros\u00b7ti\u00b7gen", "Wes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo noch ein Baum mit den \u00c4sten", "tokens": ["Wo", "noch", "ein", "Baum", "mit", "den", "\u00c4s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schreit in den Abend, stehen sie pl\u00f6tzlich, frierend und kalt,", "tokens": ["Schreit", "in", "den", "A\u00b7bend", ",", "ste\u00b7hen", "sie", "pl\u00f6tz\u00b7lich", ",", "frie\u00b7rend", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "ADJD", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "+--+-+--+-+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Wie Pilze gewachsen, und strecken in ihren Gebresten", "tokens": ["Wie", "Pil\u00b7ze", "ge\u00b7wach\u00b7sen", ",", "und", "stre\u00b7cken", "in", "ih\u00b7ren", "Ge\u00b7bres\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVPP", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.6": {"text": "Ihre schwarzen und d\u00fcnnen Dachsparren himmelan,", "tokens": ["Ih\u00b7re", "schwar\u00b7zen", "und", "d\u00fcn\u00b7nen", "Dach\u00b7spar\u00b7ren", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KON", "ADJA", "NN", "ADV", "$,"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Klappernd in ihrer Mauern sch\u00e4bigem Kleid", "tokens": ["Klap\u00b7pernd", "in", "ih\u00b7rer", "Mau\u00b7ern", "sch\u00e4\u00b7bi\u00b7gem", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "---+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Wie ein armes Volk, das vor K\u00e4lte schreit.", "tokens": ["Wie", "ein", "ar\u00b7mes", "Volk", ",", "das", "vor", "K\u00e4l\u00b7te", "schreit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und die Diebe schleichen \u00fcber die Treppen hinan,", "tokens": ["Und", "die", "Die\u00b7be", "schlei\u00b7chen", "\u00fc\u00b7ber", "die", "Trep\u00b7pen", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Springen oben \u00fcber die B\u00f6den mit schlenkerndem Bein,", "tokens": ["Sprin\u00b7gen", "o\u00b7ben", "\u00fc\u00b7ber", "die", "B\u00f6\u00b7den", "mit", "schlen\u00b7kern\u00b7dem", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Und manchmal flackert heraus ihr Laternenschein.", "tokens": ["Und", "manch\u00b7mal", "fla\u00b7ckert", "he\u00b7raus", "ihr", "La\u00b7ter\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Im gr\u00fcnen Himmel, der manchmal knallt", "tokens": ["Im", "gr\u00fc\u00b7nen", "Him\u00b7mel", ",", "der", "manch\u00b7mal", "knallt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor Frost im rostigen Westen,", "tokens": ["Vor", "Frost", "im", "ros\u00b7ti\u00b7gen", "Wes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo noch ein Baum mit den \u00c4sten", "tokens": ["Wo", "noch", "ein", "Baum", "mit", "den", "\u00c4s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schreit in den Abend, stehen sie pl\u00f6tzlich, frierend und kalt,", "tokens": ["Schreit", "in", "den", "A\u00b7bend", ",", "ste\u00b7hen", "sie", "pl\u00f6tz\u00b7lich", ",", "frie\u00b7rend", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "wor