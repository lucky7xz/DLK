{"dta.poem.9663": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Blondille.  \n * * v. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mannes sorge macht Blondillen/ sonsten nichts nicht/ un-", "tokens": ["Man\u00b7nes", "sor\u00b7ge", "macht", "Blon\u00b7dil\u00b7len", "/", "sons\u00b7ten", "nichts", "nicht", "/", "un"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "NN", "VVFIN", "NN", "$(", "ADV", "PIS", "PTKNEG", "$(", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "gemach/", "tokens": ["ge\u00b7mach", "/"], "token_info": ["word", "punct"], "pos": ["ADV", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Wolte keiner zu ihr kommen/ lief sie selbst doch ihnen nach.", "tokens": ["Wol\u00b7te", "kei\u00b7ner", "zu", "ihr", "kom\u00b7men", "/", "lief", "sie", "selbst", "doch", "ih\u00b7nen", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "PPER", "VVINF", "$(", "VVFIN", "PPER", "ADV", "ADV", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Letzlich fand sich dennoch einer/ der sich trauen lie\u00df mit ihr/", "tokens": ["Letz\u00b7lich", "fand", "sich", "den\u00b7noch", "ei\u00b7ner", "/", "der", "sich", "trau\u00b7en", "lie\u00df", "mit", "ihr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "$(", "PRELS", "PRF", "VVINF", "VVFIN", "APPR", "PPOSAT", "$("], "meter": "+-+-+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.5": {"text": "Sie gab ihm aus gutem willen vor der zeit ein kind daf\u00fcr.", "tokens": ["Sie", "gab", "ihm", "aus", "gu\u00b7tem", "wil\u00b7len", "vor", "der", "zeit", "ein", "kind", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "PAV", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}}}}