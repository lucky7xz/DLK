{"dta.poem.6355": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann in verborgner Adern G\u00e4ngen", "tokens": ["Wann", "in", "ver\u00b7borg\u00b7ner", "A\u00b7dern", "G\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von unserm Blut", "tokens": ["Von", "un\u00b7serm", "Blut"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die stets gepresste Fluth", "tokens": ["Die", "stets", "ge\u00b7press\u00b7te", "Fluth"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nicht anders, als mit starckem Dr\u00e4ngen,", "tokens": ["Nicht", "an\u00b7ders", ",", "als", "mit", "star\u00b7ckem", "Dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jm Creise lauffen kan; entsteht dergleichen Gluht.", "tokens": ["Jm", "Crei\u00b7se", "lauf\u00b7fen", "kan", ";", "ent\u00b7steht", "derg\u00b7lei\u00b7chen", "Gluht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VMFIN", "$.", "VVFIN", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von so verschiednem Drang entstehn", "tokens": ["Von", "so", "ver\u00b7schied\u00b7nem", "Drang", "ent\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die G\u00e4hrung und ein schnelles Kochen,", "tokens": ["Die", "G\u00e4h\u00b7rung", "und", "ein", "schnel\u00b7les", "Ko\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Man f\u00fchlt ein sanfftes Feur durch unsern C\u00f6rper gehn,", "tokens": ["Man", "f\u00fchlt", "ein", "sanff\u00b7tes", "Feur", "durch", "un\u00b7sern", "C\u00f6r\u00b7per", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn das Gebl\u00fct, im Hertzen ausgebreitet,", "tokens": ["Wenn", "das", "Ge\u00b7bl\u00fct", ",", "im", "Hert\u00b7zen", "aus\u00b7ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die Geister, sonder Zwang, in alle Theile leitet.", "tokens": ["Die", "Geis\u00b7ter", ",", "son\u00b7der", "Zwang", ",", "in", "al\u00b7le", "Thei\u00b7le", "lei\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "NN", "$,", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das sanffte Regen seiner S\u00e4ffte", "tokens": ["Das", "sanff\u00b7te", "Re\u00b7gen", "sei\u00b7ner", "S\u00e4ff\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Gab uns Gesundheit, St\u00e4rck\u2019 und Kr\u00e4ffte.", "tokens": ["Gab", "uns", "Ge\u00b7sund\u00b7heit", ",", "St\u00e4rck", "und", "Kr\u00e4ff\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Allein man sp\u00fchrt des Fiebers wildes Pochen,", "tokens": ["Al\u00b7lein", "man", "sp\u00fchrt", "des", "Fie\u00b7bers", "wil\u00b7des", "Po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Des Blutes Stocken folgt ein \u00fcbereilter Schlag,", "tokens": ["Des", "Blu\u00b7tes", "Sto\u00b7cken", "folgt", "ein", "\u00fc\u00b7be\u00b7reil\u00b7ter", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}