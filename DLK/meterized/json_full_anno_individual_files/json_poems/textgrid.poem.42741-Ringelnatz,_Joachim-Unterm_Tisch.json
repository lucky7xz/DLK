{"textgrid.poem.42741": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Unterm Tisch", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein St\u00fcckchen Fromage de brie,", "tokens": ["Es", "war", "ein", "St\u00fcck\u00b7chen", "Fro\u00b7ma\u00b7ge", "de", "brie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das fiel untern Tisch. Man sah nicht wie.", "tokens": ["Das", "fiel", "un\u00b7tern", "Tisch", ".", "Man", "sah", "nicht", "wie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "$.", "PIS", "VVFIN", "PTKNEG", "PWAV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dort standen zwei Lackschuh mit silbernen Schnallen.", "tokens": ["Dort", "stan\u00b7den", "zwei", "Lack\u00b7schuh", "mit", "sil\u00b7ber\u00b7nen", "Schnal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die fanden an dem Fromage Gefallen", "tokens": ["Die", "fan\u00b7den", "an", "dem", "Fro\u00b7ma\u00b7ge", "Ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und traten nach einiger \u00dcberwindung", "tokens": ["Und", "tra\u00b7ten", "nach", "ei\u00b7ni\u00b7ger", "\u00dc\u00b7berw\u00b7in\u00b7dung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Mit ihm in ganz intime Verbindung.", "tokens": ["Mit", "ihm", "in", "ganz", "in\u00b7ti\u00b7me", "Ver\u00b7bin\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Als abends die beiden Schnallengezierten", "tokens": ["Als", "a\u00b7bends", "die", "bei\u00b7den", "Schnal\u00b7len\u00b7ge\u00b7zier\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "PIAT", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "In einer feudalen Gesellschaft soupierten,", "tokens": ["In", "ei\u00b7ner", "feu\u00b7da\u00b7len", "Ge\u00b7sell\u00b7schaft", "sou\u00b7pier\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Erhoben sich pl\u00f6tzlich zwei andere Schuhe", "tokens": ["Er\u00b7ho\u00b7ben", "sich", "pl\u00f6tz\u00b7lich", "zwei", "an\u00b7de\u00b7re", "Schu\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "CARD", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Und knarrten verlegen und baten um Ruhe", "tokens": ["Und", "knarr\u00b7ten", "ver\u00b7le\u00b7gen", "und", "ba\u00b7ten", "um", "Ru\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVINF", "KON", "VVFIN", "APPR", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "Und sagten, als alles ruhig war:", "tokens": ["Und", "sag\u00b7ten", ",", "als", "al\u00b7les", "ru\u00b7hig", "war", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "\u00bbverehrte, es \u2013 riecht hier so sonderbar.\u00ab", "tokens": ["\u00bb", "ver\u00b7ehr\u00b7te", ",", "es", "\u2013", "riecht", "hier", "so", "son\u00b7der\u00b7bar", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "PPER", "$(", "VVFIN", "ADV", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es war ein St\u00fcckchen Fromage de brie,", "tokens": ["Es", "war", "ein", "St\u00fcck\u00b7chen", "Fro\u00b7ma\u00b7ge", "de", "brie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das fiel untern Tisch. Man sah nicht wie.", "tokens": ["Das", "fiel", "un\u00b7tern", "Tisch", ".", "Man", "sah", "nicht", "wie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "$.", "PIS", "VVFIN", "PTKNEG", "PWAV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dort standen zwei Lackschuh mit silbernen Schnallen.", "tokens": ["Dort", "stan\u00b7den", "zwei", "Lack\u00b7schuh", "mit", "sil\u00b7ber\u00b7nen", "Schnal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die fanden an dem Fromage Gefallen", "tokens": ["Die", "fan\u00b7den", "an", "dem", "Fro\u00b7ma\u00b7ge", "Ge\u00b7fal\u00b7len"], "token_info": [