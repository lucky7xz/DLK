{"textgrid.poem.32291": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Phyllis an Damon", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lehre mich, o Damon, singen,", "tokens": ["Leh\u00b7re", "mich", ",", "o", "Da\u00b7mon", ",", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "FM", "NE", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singen, wie du trunken singst.", "tokens": ["Sin\u00b7gen", ",", "wie", "du", "trun\u00b7ken", "singst", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df auch mich dir Lieder bringen,", "tokens": ["La\u00df", "auch", "mich", "dir", "Lie\u00b7der", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie du mir begeistert bringst.", "tokens": ["Wie", "du", "mir", "be\u00b7geis\u00b7tert", "bringst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie du mich willst ewig singen,", "tokens": ["Wie", "du", "mich", "willst", "e\u00b7wig", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00f6cht' auch ich dich ewig singen.", "tokens": ["M\u00f6cht'", "auch", "ich", "dich", "e\u00b7wig", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch des Weines Feuerkr\u00e4fte,", "tokens": ["Durch", "des", "Wei\u00b7nes", "Feu\u00b7er\u00b7kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur durch sie singst du so sch\u00f6n.", "tokens": ["Nur", "durch", "sie", "singst", "du", "so", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Aber diese G\u00f6tters\u00e4fte", "tokens": ["A\u00b7ber", "die\u00b7se", "G\u00f6t\u00b7ter\u00b7s\u00e4f\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Darf ich schmachtend nur besehn.", "tokens": ["Darf", "ich", "schmach\u00b7tend", "nur", "be\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dir riet Venus Wein zu trinken,", "tokens": ["Dir", "riet", "Ve\u00b7nus", "Wein", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir riet sie, ihn nicht zu trinken.", "tokens": ["Mir", "riet", "sie", ",", "ihn", "nicht", "zu", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Was wird nun mein Lied beleben,", "tokens": ["Was", "wird", "nun", "mein", "Lied", "be\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Kann es dieser Trank nicht sein? \u2013", "tokens": ["Kann", "es", "die\u00b7ser", "Trank", "nicht", "sein", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PDAT", "NN", "PTKNEG", "VAINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie? Du willst mir K\u00fcsse geben?", "tokens": ["Wie", "?", "Du", "willst", "mir", "K\u00fcs\u00b7se", "ge\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fcsse, feuriger, als Wein? \u2013", "tokens": ["K\u00fcs\u00b7se", ",", "feu\u00b7ri\u00b7ger", ",", "als", "Wein", "?", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "KOUS", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Damon, ach! nach deinen K\u00fcssen", "tokens": ["Da\u00b7mon", ",", "ach", "!", "nach", "dei\u00b7nen", "K\u00fcs\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ITJ", "$.", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Werd' ich wohl verstummen m\u00fcssen.", "tokens": ["Werd'", "ich", "wohl", "ver\u00b7stum\u00b7men", "m\u00fcs\u00b7sen", "."], "token_inf