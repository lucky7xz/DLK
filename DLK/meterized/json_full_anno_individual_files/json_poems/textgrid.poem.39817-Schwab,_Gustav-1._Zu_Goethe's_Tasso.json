{"textgrid.poem.39817": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1. Zu Goethe's Tasso", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schufest uns in Worten eine Welt,", "tokens": ["Du", "schu\u00b7fest", "uns", "in", "Wor\u00b7ten", "ei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wer preist mit Worten w\u00fcrdig dich, o Held?", "tokens": ["Wer", "preist", "mit", "Wor\u00b7ten", "w\u00fcr\u00b7dig", "dich", ",", "o", "Held", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "ADJD", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was wir durch dich empfunden und geschaut,", "tokens": ["Was", "wir", "durch", "dich", "emp\u00b7fun\u00b7den", "und", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Macht sich in eitler Schilderung nicht laut.", "tokens": ["Macht", "sich", "in", "eit\u00b7ler", "Schil\u00b7de\u00b7rung", "nicht", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN", "PTKNEG", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Drum kurze Rede ziemt an diesem Ort,", "tokens": ["Drum", "kur\u00b7ze", "Re\u00b7de", "ziemt", "an", "die\u00b7sem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Denn ihn verkl\u00e4ren soll dein eigen Wort.", "tokens": ["Denn", "ihn", "ver\u00b7kl\u00e4\u00b7ren", "soll", "dein", "ei\u00b7gen", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VMFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du lie\u00dfest ", "tokens": ["Du", "lie\u00b7\u00dfest"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Verk\u00fcndiger der Kunst und der Natur.", "tokens": ["Ver\u00b7k\u00fcn\u00b7di\u00b7ger", "der", "Kunst", "und", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Hier, auf den Brettern, wo in Spiel und Schein", "tokens": ["Hier", ",", "auf", "den", "Bret\u00b7tern", ",", "wo", "in", "Spiel", "und", "Schein"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "$,", "PWAV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die T\u00e4uschung ew'ge Wahrheit gr\u00e4bt hinein,", "tokens": ["Die", "T\u00e4u\u00b7schung", "ew'\u00b7ge", "Wahr\u00b7heit", "gr\u00e4bt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Hier lebt unsterblich-frisch dein Dichtergeist,", "tokens": ["Hier", "lebt", "un\u00b7sterb\u00b7lich\u00b7frisch", "dein", "Dich\u00b7ter\u00b7geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und zeigt, was Sch\u00f6pferkraft auf Erden hei\u00dft.", "tokens": ["Und", "zeigt", ",", "was", "Sch\u00f6p\u00b7fer\u00b7kraft", "auf", "Er\u00b7den", "hei\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auch sie ward deiner Musen St\u00e4tte l\u00e4ngst:", "tokens": ["Auch", "sie", "ward", "dei\u00b7ner", "Mu\u00b7sen", "St\u00e4t\u00b7te", "l\u00e4ngst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPOSAT", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Scheu und bangem Flei\u00df versuchten wir", "tokens": ["Mit", "Scheu", "und", "ban\u00b7gem", "Flei\u00df", "ver\u00b7such\u00b7ten", "wir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Uns schon an deiner h\u00f6chsten Werke Zier,", "tokens": ["Uns", "schon", "an", "dei\u00b7ner", "h\u00f6chs\u00b7ten", "Wer\u00b7ke", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Selbst jenes Riesenbild der innern Welt", "tokens": ["Selbst", "je\u00b7nes", "Rie\u00b7sen\u00b7bild", "der", "in\u00b7nern", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir haben's zagend \u2013 dennoch dargestellt.", "tokens": ["Wir", "ha\u00b7ben's", "za\u00b7gend", "\u2013", "den\u00b7noch", "dar\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$(", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und heute, wo des Dichters Schmerzenskrampf,", "tokens": ["Und", "heu\u00b7te", ",", "wo", "des", "Dich\u00b7ters", "Schmer\u00b7zen\u00b7skrampf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Streit mit seinem weichen Selbst, der Kampf", "tokens": ["Der", "Streit", "mit", "sei\u00b7nem", "wei\u00b7chen", "Selbst", ",", "der", "Kampf"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit jenem Weltgeist, musenlos und kalt,", "tokens": ["Mit", "je\u00b7nem", "Welt\u00b7geist", ",", "mu\u00b7sen\u00b7los", "und", "kalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "In uns auf's neu' gewinnen soll Gestalt:", "tokens": ["In", "uns", "auf's", "neu'", "ge\u00b7win\u00b7nen", "soll", "Ge\u00b7stalt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "VVINF", "VMFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "O m\u00f6chte heut in Rede, Haltung, Blick", "tokens": ["O", "m\u00f6ch\u00b7te", "heut", "in", "Re\u00b7de", ",", "Hal\u00b7tung", ",", "Blick"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "VMFIN", "ADV", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Rein spiegeln sich das tragische Geschick,", "tokens": ["Rein", "spie\u00b7geln", "sich", "das", "tra\u00b7gi\u00b7sche", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df du, der es gesch\u00f6pft aus tiefer Brust,", "tokens": ["Da\u00df", "du", ",", "der", "es", "ge\u00b7sch\u00f6pft", "aus", "tie\u00b7fer", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Es schau'n und h\u00f6ren k\u00f6nntest recht mit Lust!", "tokens": ["Es", "schau'n", "und", "h\u00f6\u00b7ren", "k\u00f6nn\u00b7test", "recht", "mit", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVINF", "VMFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es war nicht ", "tokens": ["Es", "war", "nicht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "In deinen Becher nicht der Erde Spott.", "tokens": ["In", "dei\u00b7nen", "Be\u00b7cher", "nicht", "der", "Er\u00b7de", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Dichterleben, das von Wonne trof,", "tokens": ["Ein", "Dich\u00b7ter\u00b7le\u00b7ben", ",", "das", "von", "Won\u00b7ne", "trof", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du schl\u00fcrftest es an eines ", "tokens": ["Du", "schl\u00fcrf\u00b7test", "es", "an", "ei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und was du sangst, begl\u00e4nzte nah und fern", "tokens": ["Und", "was", "du", "sangst", ",", "be\u00b7gl\u00e4nz\u00b7te", "nah", "und", "fern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Aus tausend Augen heller Liebe Stern.", "tokens": ["Aus", "tau\u00b7send", "Au\u00b7gen", "hel\u00b7ler", "Lie\u00b7be", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Baum, erwachsen in der Jahre Ruh',", "tokens": ["Ein", "Baum", ",", "er\u00b7wach\u00b7sen", "in", "der", "Jah\u00b7re", "Ruh'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In langem Friedenstraume gr\u00fcntest du;", "tokens": ["In", "lan\u00b7gem", "Frie\u00b7dens\u00b7trau\u00b7me", "gr\u00fcn\u00b7test", "du", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dein moos'ger Stamm, von Bl\u00fcten \u00fcberdeckt,", "tokens": ["Dein", "moos'\u00b7ger", "Stamm", ",", "von", "Bl\u00fc\u00b7ten", "\u00fc\u00b7berd\u00b7eckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Liegt, erst aus morscher Wurzel hingestreckt.", "tokens": ["Liegt", ",", "erst", "aus", "mor\u00b7scher", "Wur\u00b7zel", "hin\u00b7ge\u00b7streckt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Jetzt aber rollt der Donner \u00fcber'm Hain,", "tokens": ["Jetzt", "a\u00b7ber", "rollt", "der", "Don\u00b7ner", "\u00fc\u00b7ber'm", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Es pfeift der Nord, es zuckt der Blitze Schein,", "tokens": ["Es", "pfeift", "der", "Nord", ",", "es", "zuckt", "der", "Blit\u00b7ze", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Dein Bild umh\u00fcllt nicht blo\u00df des Weihrauchs Dampf,", "tokens": ["Dein", "Bild", "um\u00b7h\u00fcllt", "nicht", "blo\u00df", "des", "Weih\u00b7rauchs", "Dampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Auch wolk'ger Staub vom schw\u00fclen Erdenkampf.", "tokens": ["Auch", "wol\u00b7k'\u00b7ger", "Staub", "vom", "schw\u00fc\u00b7len", "Er\u00b7den\u00b7kampf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch Wetter ziehn vor\u00fcber, und ihr Staub", "tokens": ["Doch", "Wet\u00b7ter", "ziehn", "vor\u00b7\u00fc\u00b7ber", ",", "und", "ihr", "Staub"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verweht, und Kunst wird keiner Zeiten Raub.", "tokens": ["Ver\u00b7weht", ",", "und", "Kunst", "wird", "kei\u00b7ner", "Zei\u00b7ten", "Raub", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "NN", "VAFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn deines Freunds Gesang, den ", "tokens": ["Wenn", "dei\u00b7nes", "Freunds", "Ge\u00b7sang", ",", "den"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$,", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In deinen Arm und in die Welt gesandt,", "tokens": ["In", "dei\u00b7nen", "Arm", "und", "in", "die", "Welt", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit heil'gen Lauten schmetternd in den Krieg", "tokens": ["Mit", "heil'\u00b7gen", "Lau\u00b7ten", "schmet\u00b7ternd", "in", "den", "Krieg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Meinung donnert, zu des Guten Sieg:", "tokens": ["Der", "Mei\u00b7nung", "don\u00b7nert", ",", "zu", "des", "Gu\u00b7ten", "Sieg", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Singt ", "tokens": ["Singt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Was dauernd keimt in jeder Menschenbrust.", "tokens": ["Was", "dau\u00b7ernd", "keimt", "in", "je\u00b7der", "Men\u00b7schen\u00b7brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Auch Hellas' gr\u00f6\u00dfte Dichter sangen so", "tokens": ["Auch", "Hel\u00b7las'", "gr\u00f6\u00df\u00b7te", "Dich\u00b7ter", "san\u00b7gen", "so"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und galten, wo man traurig war und froh;", "tokens": ["Und", "gal\u00b7ten", ",", "wo", "man", "trau\u00b7rig", "war", "und", "froh", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PIS", "ADJD", "VAFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Das Lied Homers klang selbst im B\u00fcrgerzwist", "tokens": ["Das", "Lied", "Ho\u00b7mers", "klang", "selbst", "im", "B\u00fcr\u00b7ger\u00b7zwist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART",