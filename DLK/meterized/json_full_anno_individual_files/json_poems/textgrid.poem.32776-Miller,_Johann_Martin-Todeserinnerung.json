{"textgrid.poem.32776": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Todeserinnerung", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was du, Gott, auf Erden schufest,", "tokens": ["Was", "du", ",", "Gott", ",", "auf", "Er\u00b7den", "schu\u00b7fest", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4gt das Bild der Sterblichkeit;", "tokens": ["Tr\u00e4gt", "das", "Bild", "der", "Sterb\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit jedem Tage rufest", "tokens": ["Und", "mit", "je\u00b7dem", "Ta\u00b7ge", "ru\u00b7fest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du Dem Menschen: ", "tokens": ["Du", "Dem", "Men\u00b7schen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Wenn die Sonnenstrahlen gl\u00fchen,", "tokens": ["Wenn", "die", "Son\u00b7nen\u00b7strah\u00b7len", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Saat der Reifung lacht,", "tokens": ["Und", "die", "Saat", "der", "Rei\u00b7fung", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mu\u00df der Blume Schmuck verbl\u00fchen,", "tokens": ["Mu\u00df", "der", "Blu\u00b7me", "Schmuck", "ver\u00b7bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die im Morgentau erwacht.", "tokens": ["Die", "im", "Mor\u00b7gen\u00b7tau", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn des Herbstes Fr\u00fcchte reifen,", "tokens": ["Wenn", "des", "Herbs\u00b7tes", "Fr\u00fcch\u00b7te", "rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O, so reifen sie dem Grab;", "tokens": ["O", ",", "so", "rei\u00b7fen", "sie", "dem", "Grab", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Winde stehen auf und streifen", "tokens": ["Win\u00b7de", "ste\u00b7hen", "auf", "und", "strei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie vom vollen Baum herab;", "tokens": ["Sie", "vom", "vol\u00b7len", "Baum", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann beginnt des Winters Stille,", "tokens": ["Dann", "be\u00b7ginnt", "des", "Win\u00b7ters", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn der Herbstwind ausgedroht,", "tokens": ["Wenn", "der", "Herbst\u00b7wind", "aus\u00b7ge\u00b7droht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und in wei\u00dfer Leichenh\u00fclle", "tokens": ["Und", "in", "wei\u00b7\u00dfer", "Lei\u00b7chen\u00b7h\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Liegt umher die Sch\u00f6pfung tot.", "tokens": ["Liegt", "um\u00b7her", "die", "Sch\u00f6p\u00b7fung", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar im neuen Feierkleide", "tokens": ["Zwar", "im", "neu\u00b7en", "Fei\u00b7er\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bricht der junge Fr\u00fchling an;", "tokens": ["Bricht", "der", "jun\u00b7ge", "Fr\u00fch\u00b7ling", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Gebirg und Thal und Heide", "tokens": ["Und", "Ge\u00b7birg", "und", "Thal", "und", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "KON", "NE"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Sind mit Blumen angethan;", "tokens": ["Sind", "mit", "Blu\u00b7men", "an\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "B\u00e4ume knospen auf und gr\u00fcnen,", "tokens": ["B\u00e4u\u00b7me", "knos\u00b7pen", "auf", "und", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wei\u00dfe Bl\u00fcte h\u00fcllt sie ein,", "tokens": ["Wei\u00b7\u00dfe", "Bl\u00fc\u00b7te", "h\u00fcllt", "sie", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber mitten unter ihnen", "tokens": ["A\u00b7ber", "mit\u00b7ten", "un\u00b7ter", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Welkt der sch\u00f6nste Baum im Hain.", "tokens": ["Welkt", "der", "sch\u00f6ns\u00b7te", "Baum", "im", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "V\u00f6gel singen uns im K\u00fchlen", "tokens": ["V\u00f6\u00b7gel", "sin\u00b7gen", "uns", "im", "K\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Seligkeit im Mai;", "tokens": ["Von", "der", "Se\u00b7lig\u00b7keit", "im", "Mai", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber, eh sie ganz ihn f\u00fchlen,", "tokens": ["A\u00b7ber", ",", "eh", "sie", "ganz", "ihn", "f\u00fch\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcrzet sie ein schnelles Blei;", "tokens": ["St\u00fcr\u00b7zet", "sie", "ein", "schnel\u00b7les", "Blei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00e4mmer h\u00fcpfen sonder Sorgen", "tokens": ["L\u00e4m\u00b7mer", "h\u00fcp\u00b7fen", "son\u00b7der", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durchs bebl\u00fcmte Rosenthal;", "tokens": ["Durchs", "be\u00b7bl\u00fcm\u00b7te", "Ro\u00b7sen\u00b7thal", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber \u2013 arme L\u00e4mmer! morgen", "tokens": ["A\u00b7ber", "\u2013", "ar\u00b7me", "L\u00e4m\u00b7mer", "!", "mor\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "$(", "ADJA", "NN", "$.", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "S\u00e4ttigt ihr den M\u00f6rderstahl.", "tokens": ["S\u00e4t\u00b7tigt", "ihr", "den", "M\u00f6r\u00b7der\u00b7stahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Kinder brechen, wo sie gehen,", "tokens": ["Kin\u00b7der", "bre\u00b7chen", ",", "wo", "sie", "ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blumen sich zu Kr\u00e4nzen ab;", "tokens": ["Blu\u00b7men", "sich", "zu", "Kr\u00e4n\u00b7zen", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber unvermutet stehen", "tokens": ["A\u00b7ber", "un\u00b7ver\u00b7mu\u00b7tet", "ste\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie auf der Gespielen Grab.", "tokens": ["Sie", "auf", "der", "Ge\u00b7spie\u00b7len", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wenn sich in erhellten Hallen", "tokens": ["Wenn", "sich", "in", "er\u00b7hell\u00b7ten", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "J\u00fcnglinge des Tanzes freun,", "tokens": ["J\u00fcng\u00b7lin\u00b7ge", "des", "Tan\u00b7zes", "freun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$,"], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Und die Pauke t\u00f6nt, erschallen", "tokens": ["Und", "die", "Pau\u00b7ke", "t\u00f6nt", ",", "er\u00b7schal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NE", "VVFIN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Pl\u00f6tzlich Todesglocken drein.", "tokens": ["Pl\u00f6tz\u00b7lich", "To\u00b7des\u00b7glo\u00b7cken", "drein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach, mit jedem Tage nahen", "tokens": ["Ach", ",", "mit", "je\u00b7dem", "Ta\u00b7ge", "na\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PIAT", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wir uns der Verg\u00e4nglichkeit!", "tokens": ["Wir", "uns", "der", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, uns alle zu empfahen,", "tokens": ["Ach", ",", "uns", "al\u00b7le", "zu", "em\u00b7pfa\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ein weites Grab bereit!", "tokens": ["Ist", "ein", "wei\u00b7tes", "Grab", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Darum la\u00dft uns himmlisch denken!", "tokens": ["Da\u00b7rum", "la\u00dft", "uns", "himm\u00b7lisch", "den\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Und von stiller Unschuld voll,", "tokens": ["Und", "von", "stil\u00b7ler", "Un\u00b7schuld", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unsern Geist zum Himmel lenken,", "tokens": ["Un\u00b7sern", "Geist", "zum", "Him\u00b7mel", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo er ewig bleiben soll!", "tokens": ["Wo", "er", "e\u00b7wig", "blei\u00b7ben", "soll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Was du, Gott, auf Erden schufest,", "tokens": ["Was", "du", ",", "Gott", ",", "auf", "Er\u00b7den", "schu\u00b7fest", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4gt das Bild der Sterblichkeit;", "tokens": ["Tr\u00e4gt", "das", "Bild", "der", "Sterb\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit jedem Tage rufest", "tokens": ["Und", "mit", "je\u00b7dem", "Ta\u00b7ge", "ru\u00b7fest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du Dem Menschen: ", "tokens": ["Du", "Dem", "Men\u00b7schen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Wenn die Sonnenstrahlen gl\u00fchen,", "tokens": ["Wenn", "die", "Son\u00b7nen\u00b7strah\u00b7len", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure":