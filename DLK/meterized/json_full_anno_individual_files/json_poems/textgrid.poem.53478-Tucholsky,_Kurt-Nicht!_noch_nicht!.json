{"textgrid.poem.53478": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Nicht! noch nicht!", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein leichter Suff umnebelt die Gedanken.", "tokens": ["Ein", "leich\u00b7ter", "Suff", "um\u00b7ne\u00b7belt", "die", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verdammt! Der Fr\u00fchling kommt zu fr\u00fch.", "tokens": ["Ver\u00b7dammt", "!", "Der", "Fr\u00fch\u00b7ling", "kommt", "zu", "fr\u00fch", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "ART", "NN", "VVFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Parapluie", "tokens": ["Der", "Pa\u00b7ra\u00b7plu\u00b7ie"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "steht tief im Schrank \u2013 die Zeitbegriffe schwanken.", "tokens": ["steht", "tief", "im", "Schrank", "\u2013", "die", "Zeit\u00b7be\u00b7grif\u00b7fe", "schwan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$(", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was wehen jetzt die warmen Fr\u00fchlingsl\u00fcfte?", "tokens": ["Was", "we\u00b7hen", "jetzt", "die", "war\u00b7men", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein lauer Wind ums\u00e4uselt still", "tokens": ["Ein", "lau\u00b7er", "Wind", "um\u00b7s\u00e4u\u00b7selt", "still"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "mich im April \u2013", "tokens": ["mich", "im", "Ap\u00b7ril", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "die Nase schnuppert ungewohnte D\u00fcfte.", "tokens": ["die", "Na\u00b7se", "schnup\u00b7pert", "un\u00b7ge\u00b7wohn\u00b7te", "D\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du lieber Gott, da ist doch nichts dahinter!", "tokens": ["Du", "lie\u00b7ber", "Gott", ",", "da", "ist", "doch", "nichts", "da\u00b7hin\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$,", "ADV", "VAFIN", "ADV", "PIS", "PAV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wie ein dicker B\u00e4r sich murrend schleckt,", "tokens": ["Und", "wie", "ein", "di\u00b7cker", "B\u00e4r", "sich", "mur\u00b7rend", "schleckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "zu fr\u00fch geweckt,", "tokens": ["zu", "fr\u00fch", "ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "so zieh ich mich zur\u00fcck und tr\u00e4ume Winter.", "tokens": ["