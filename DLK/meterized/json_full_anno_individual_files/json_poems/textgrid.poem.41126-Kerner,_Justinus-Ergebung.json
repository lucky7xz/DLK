{"textgrid.poem.41126": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Ergebung", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft meinst du, Herz! du k\u00f6nntst noch \u00fcberflie\u00dfen", "tokens": ["Oft", "meinst", "du", ",", "Herz", "!", "du", "k\u00f6nntst", "noch", "\u00fc\u00b7berf\u00b7lie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$.", "PPER", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von bunten Liedern, br\u00e4chte man dich nur", "tokens": ["Von", "bun\u00b7ten", "Lie\u00b7dern", ",", "br\u00e4ch\u00b7te", "man", "dich", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "PIS", "PRF", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus Deutschlands Froste noch auf eine Flur,", "tokens": ["Aus", "Deutschlands", "Fros\u00b7te", "noch", "auf", "ei\u00b7ne", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wo Sonn' und Erde hei\u00dfer sich begr\u00fc\u00dfen.", "tokens": ["Wo", "Sonn'", "und", "Er\u00b7de", "hei\u00b7\u00dfer", "sich", "be\u00b7gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADJA", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Erstarrtes Herz! wohl w\u00fcrdst du dort auch singen,", "tokens": ["Er\u00b7starr\u00b7tes", "Herz", "!", "wohl", "w\u0