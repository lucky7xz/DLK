{"textgrid.poem.49511": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Sans les illusions, que servient nos plaisirs", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aphroditens sch\u00f6nes Kind,", "tokens": ["A\u00b7phro\u00b7di\u00b7tens", "sch\u00f6\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unvorsichtig, leicht gesinnt,", "tokens": ["Un\u00b7vor\u00b7sich\u00b7tig", ",", "leicht", "ge\u00b7sinnt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stolperte bey finstrer Nacht", "tokens": ["Stol\u00b7per\u00b7te", "bey", "finst\u00b7rer", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Unversehns in einen Schacht.", "tokens": ["Un\u00b7ver\u00b7sehns", "in", "ei\u00b7nen", "Schacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als es nun Minerven rief,", "tokens": ["Als", "es", "nun", "Mi\u00b7ner\u00b7ven", "rief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Die im n\u00e4chsten Tempel schlief,", "tokens": ["Die", "im", "n\u00e4chs\u00b7ten", "Tem\u00b7pel", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kam sie, aber ohne Licht,", "tokens": ["Kam", "sie", ",", "a\u00b7ber", "oh\u00b7ne", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Liebgen, sprach sie, weine nicht,", "tokens": ["Lieb\u00b7gen", ",", "sprach", "sie", ",", "wei\u00b7ne", "nicht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Gerne z\u00fcndet' ich dir zwar;", "tokens": ["Ger\u00b7ne", "z\u00fcn\u00b7det'", "ich", "dir", "zwar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Aber s\u00e4hstu alles klar,", "tokens": ["A\u00b7ber", "s\u00e4h\u00b7stu", "al\u00b7les", "klar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "W\u00fcrdestu der Gott der Pein", "tokens": ["W\u00fcr\u00b7des\u00b7tu", "der", "Gott", "der", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "\u00d6ftrer, als der Freuden seyn!", "tokens": ["\u00d6ft\u00b7rer", ",", "als", "der", "Freu\u00b7den", "seyn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aphroditens sch\u00f6nes Kind,", "tokens": ["A\u00b7phro\u00b7di\u00b7tens", "sch\u00f6\u00b7nes", "Kind", ","], "token_info": ["word",