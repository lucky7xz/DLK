{"textgrid.poem.55938": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "R\u00f6mische Sarkophage", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was aber hindert uns zu glauben, da\u00df", "tokens": ["Was", "a\u00b7ber", "hin\u00b7dert", "uns", "zu", "glau\u00b7ben", ",", "da\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$,", "KOUS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "(so wie wir hingestellt sind und verteilt)", "tokens": ["(", "so", "wie", "wir", "hin\u00b7ge\u00b7stellt", "sind", "und", "ver\u00b7teilt", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOKOM", "PPER", "VVPP", "VAFIN", "KON", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "nicht eine kleine Zeit nur Drang und Ha\u00df", "tokens": ["nicht", "ei\u00b7ne", "klei\u00b7ne", "Zeit", "nur", "Drang", "und", "Ha\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und dies Verwirrende in uns verweilt,", "tokens": ["und", "dies", "Ver\u00b7wir\u00b7ren\u00b7de", "in", "uns", "ver\u00b7weilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "wie einst in dem verzierten Sarkophag", "tokens": ["wie", "einst", "in", "dem", "ver\u00b7zier\u00b7ten", "Sar\u00b7ko\u00b7phag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["