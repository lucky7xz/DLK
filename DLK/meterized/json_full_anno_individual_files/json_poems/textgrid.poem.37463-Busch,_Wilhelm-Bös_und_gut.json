{"textgrid.poem.37463": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "B\u00f6s und gut", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie kam ich nur aus jenem Frieden", "tokens": ["Wie", "kam", "ich", "nur", "aus", "je\u00b7nem", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Weltget\u00f6s?", "tokens": ["Ins", "Welt\u00b7ge\u00b7t\u00f6s", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Was einst vereint, hat sich geschieden,", "tokens": ["Was", "einst", "ver\u00b7eint", ",", "hat", "sich", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "$,", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und das ist b\u00f6s.", "tokens": ["Und", "das", "ist", "b\u00f6s", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Nun bin ich nicht geneigt zum Geben,", "tokens": ["Nun", "bin", "ich", "nicht", "ge\u00b7neigt", "zum", "Ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun hei\u00dft es: Nimm!", "tokens": ["Nun", "hei\u00dft", "es", ":", "Nimm", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ja, ich mu\u00df t\u00f6ten, um zu leben,", "tokens": ["Ja", ",", "ich", "mu\u00df", "t\u00f6\u00b7ten", ",", "um", "zu", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "VVINF", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und das ist schlimm.", "tokens": ["Und", "das", "ist", "schlimm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Doch eine Sehnsucht blieb zur\u00fccke,", "tokens": ["Doch", "ei\u00b7ne", "Sehn\u00b7sucht", "blieb", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die niemals ruht.", "tokens": ["Die", "nie\u00b7mals", "ruht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sie zieht mich heim zum alten Gl\u00fccke,", "tokens": ["Sie", "zieht", "mich", "heim", "zum", "al\u00b7ten", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und das ist gut.", "tokens": ["Und", "das", "ist", "gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Wie kam ich nur aus jenem Frieden", "tokens": ["Wie", "kam", "ich", "nur", "aus", "je\u00b7nem", "Frie\u00b7den"], "token_info": ["word