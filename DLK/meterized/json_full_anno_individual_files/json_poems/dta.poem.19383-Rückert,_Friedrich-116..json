{"dta.poem.19383": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "116.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Seele, s\u00fcndigst du, und denkst, Gott sieht dich nicht;", "tokens": ["O", "See\u00b7le", ",", "s\u00fcn\u00b7digst", "du", ",", "und", "denkst", ",", "Gott", "sieht", "dich", "nicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$,", "KON", "VVFIN", "$,", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ist die Blindheit gro\u00df, wie klein der Einsicht Licht!", "tokens": ["Wie", "ist", "die", "Blind\u00b7heit", "gro\u00df", ",", "wie", "klein", "der", "Ein\u00b7sicht", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "$,", "PWAV", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und s\u00fcndigst du und wei\u00dft, da\u00df es sein Blick vernahm,", "tokens": ["Und", "s\u00fcn\u00b7digst", "du", "und", "wei\u00dft", ",", "da\u00df", "es", "sein", "Blick", "ver\u00b7nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ist die Frechheit gro\u00df, wie klein ist deine Scham?", "tokens": ["Wie", "ist", "die", "Frech\u00b7heit", "gro\u00df", ",", "wie", "klein", "ist", "dei\u0