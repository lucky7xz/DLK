{"textgrid.poem.26275": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Ach, wie lange mu\u00df ich warten", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem Gartengange,", "tokens": ["In", "dem", "Gar\u00b7ten\u00b7gan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Um die funkelroten Bl\u00fcten", "tokens": ["Um", "die", "fun\u00b7kel\u00b7ro\u00b7ten", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Hibiskushecken", "tokens": ["Der", "Hi\u00b7bis\u00b7kus\u00b7he\u00b7cken"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Flattern Falter aus Verstecken,", "tokens": ["Flat\u00b7tern", "Fal\u00b7ter", "aus", "Ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die sich froh im Liebesdrange", "tokens": ["Die", "sich", "froh", "im", "Lie\u00b7bes\u00b7dran\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Spielend fangen und sich necken.", "tokens": ["Spie\u00b7lend", "fan\u00b7gen", "und", "sich", "ne\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, wie lange", "tokens": ["Ach", ",", "wie", "lan\u00b7ge"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Mu\u00df ich andere gl\u00fccklich sehen", "tokens": ["Mu\u00df", "ich", "an\u00b7de\u00b7re", "gl\u00fcck\u00b7lich", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PIS", "ADJD", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Und mu\u00df warten!", "tokens": ["Und", "mu\u00df", "war\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Darf, der Wolke gleich,", "tokens": ["Darf", ",", "der", "Wol\u00b7ke", "gleich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "ART", "NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Im besonnten Garten", "tokens": ["Im", "be\u00b7sonn\u00b7ten", "Gar\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Nur als Schatten gehen. \u2013", "tokens": ["Nur", "als", "Schat\u00b7ten", "ge\u00b7hen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "KOUS", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Wie die Wolke bleich.", "tokens": ["Wie", "die", "Wol\u00b7ke", "bleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "In dem Gartengange,", "tokens": ["In", "dem", "Gar\u00b7ten\u00b7gan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Um die funkelroten Bl\u00fct