{"textgrid.poem.39449": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie ist mir? Wonne blitzt von Gottes Throne,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ist mir? Wonne blitzt von Gottes Throne,", "tokens": ["Wie", "ist", "mir", "?", "Won\u00b7ne", "blitzt", "von", "Got\u00b7tes", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "NN", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und hat mit s\u00fc\u00dfen Banden mich umschlungen.", "tokens": ["Und", "hat", "mit", "s\u00fc\u00b7\u00dfen", "Ban\u00b7den", "mich", "um\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Sehnen ist die Himmel durchgedrungen:", "tokens": ["Mein", "Seh\u00b7nen", "ist", "die", "Him\u00b7mel", "durch\u00b7ge\u00b7drun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich seh' den Vater bei dem theuren Sohne.", "tokens": ["Ich", "seh'", "den", "Va\u00b7ter", "bei", "dem", "theu\u00b7ren", "Soh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hinan! hinan! auf da\u00df ich bei euch wohne,", "tokens": ["Hi\u00b7nan", "!", "hi\u00b7nan", "!", "auf", "da\u00df", "ich", "bei", "euch", "woh\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PTKVZ", "$.", "APPR", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Vom Zug der Liebe leicht emporgeschwungen!", "tokens": ["Vom", "Zug", "der", "Lie\u00b7be", "leicht", "em\u00b7por\u00b7ge\u00b7schwun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Heil'gen, die ihr treu mit mir gerungen,", "tokens": ["Ihr", "Heil'\u00b7gen", ",", "die", "ihr", "treu", "mit", "mir", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Glaubt, liebet, hofft, und einst empfaht die Krone. \u2013", "tokens": ["Glaubt", ",", "lie\u00b7bet", ",", "hofft", ",", "und", "einst", "em\u00b7pfaht", "die", "Kro\u00b7ne", ".", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "KON", "ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wie sie so auf Wolk' und Duft entschwindet,", "tokens": ["Und", "wie", "sie", "so", "auf", "Wol\u00b7k'", "und", "Duft", "ent\u00b7schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPR", "NE", "KON", "NN", "VVPP", "$,"], "meter": "-----+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Uml\u00e4cheln sie des Himmels j\u00fcngste S\u00f6hne;", "tokens": ["Um\u00b7l\u00e4\u00b7cheln", "sie", "des", "Him\u00b7mels", "j\u00fcngs\u00b7te", "S\u00f6h\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schon weichen unter ihrem Fu\u00df die Sonnen.", "tokens": ["Schon", "wei\u00b7chen", "un\u00b7ter", "ih\u00b7rem", "Fu\u00df", "die", "Son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Lichte wird ein neues Licht entz\u00fcndet,", "tokens": ["Im", "Lich\u00b7te", "wird", "ein", "neu\u00b7es", "Licht", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So strahlt die Braut, verkl\u00e4rt in reiner Sch\u00f6ne,", "tokens": ["So", "strahlt", "die", "Braut", ",", "ver\u00b7kl\u00e4rt", "in", "rei\u00b7ner", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ruht nun liebend an der Liebe Bronnen.", "tokens": ["Und", "ruht", "nun", "lie\u00b7bend", "an", "der", "Lie\u00b7be", "Bron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1"