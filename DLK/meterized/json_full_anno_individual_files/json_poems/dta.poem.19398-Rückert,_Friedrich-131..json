{"dta.poem.19398": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "131.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Selbliebe liebte gern sich selber ungest\u00f6rt,", "tokens": ["Sel\u00b7blie\u00b7be", "lieb\u00b7te", "gern", "sich", "sel\u00b7ber", "un\u00b7ge\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ist von allem, was darin sie st\u00f6rt, emp\u00f6rt.", "tokens": ["Und", "ist", "von", "al\u00b7lem", ",", "was", "da\u00b7rin", "sie", "st\u00f6rt", ",", "em\u00b7p\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIS", "$,", "PRELS", "PAV", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie m\u00f6chte seyn, um nur recht lieben sich zu k\u00f6nnen,", "tokens": ["Sie", "m\u00f6ch\u00b7te", "seyn", ",", "um", "nur", "recht", "lie\u00b7ben", "sich", "zu", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VAINF", "$,", "KOUI", "ADV", "ADJD", "VVFIN", "PRF", "PTKZU", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Recht liebensw\u00fcrdig, und sich jeden Vorzug g\u00f6nnen.", "tokens": ["Recht", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig", ",", "und", "sich", "je\u00b7den", "Vor\u00b7zug", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "KON", "PRF", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Entdecken mu\u00df sie da mit Schrecken manchen Flecken;", "tokens": ["Ent\u00b7de\u00b7cken", "mu\u00df", "sie", "da", "mit", "Schre\u00b7cken", "man\u00b7chen", "Fle\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "APPR", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was bleibt ihr denn, als ihn verdecken und verstecken?", "tokens": ["Was", "bleibt", "ihr", "denn", ",", "als", "ihn", "ver\u00b7de\u00b7cken", "und", "ver\u00b7ste\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Verstecken vor der Welt, da\u00df sch\u00f6n der Welt sie scheine;", "tokens": ["Ver\u00b7ste\u00b7cken", "vor", "der", "Welt", ",", "da\u00df", "sch\u00f6n", "der", "Welt", "sie", "schei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KOUS", "ADJD", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verdecken vor sich selbst, da\u00df sie es selber meine.", "tokens": ["Ver\u00b7de\u00b7cken", "vor", "sich", "selbst", ",", "da\u00df", "sie", "es", "sel\u00b7ber", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "ADV", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch meinen kann sie's nie recht ungest\u00f6rt und still,", "tokens": ["Doch", "mei\u00b7nen", "kann", "sie's", "nie", "recht", "un\u00b7ge\u00b7st\u00f6rt", "und", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VMFIN", "PIS", "ADV", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und meint stets, da\u00df die Welt nicht recht es meinen will.", "tokens": ["Und", "meint", "stets", ",", "da\u00df", "die", "Welt", "nicht", "recht", "es", "mei\u00b7nen", "will", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "ART", "NN", "PTKNEG", "ADJD", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Straft ihr die Heuchlerinn? sie straft die Heuchelei;", "tokens": ["Straft", "ihr", "die", "Heuch\u00b7le\u00b7rinn", "?", "sie", "straft", "die", "Heu\u00b7che\u00b7lei", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie selbst nur wei\u00df, wie schwer die hole Maske sei.", "tokens": ["Sie", "selbst", "nur", "wei\u00df", ",", "wie", "schwer", "die", "ho\u00b7le", "Mas\u00b7ke", "sei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "$,", "PWAV", "ADJD", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}