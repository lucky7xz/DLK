{"dta.poem.6991": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "cy:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kan es der C\u00f6rper seyn, der durch der Folgrung Krafft,", "tokens": ["Kan", "es", "der", "C\u00f6r\u00b7per", "seyn", ",", "der", "durch", "der", "Fol\u00b7grung", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VAINF", "$,", "PRELS", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch Gr\u00fcnde, welche nicht umst\u00f6\u00dflich,", "tokens": ["Durch", "Gr\u00fcn\u00b7de", ",", "wel\u00b7che", "nicht", "um\u00b7st\u00f6\u00df\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Schl\u00fcsse, die gantz unaufl\u00f6\u00dflich,", "tokens": ["Und", "Schl\u00fcs\u00b7se", ",", "die", "gantz", "un\u00b7au\u00b7fl\u00f6\u00df\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Formiret hat den Crei\u00df der Wissenschafft?", "tokens": ["For\u00b7mi\u00b7ret", "hat", "den", "Crei\u00df", "der", "Wis\u00b7sen\u00b7schafft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und von der Wahrheit, die er heget,", "tokens": ["Und", "von", "der", "Wahr\u00b7heit", ",", "die", "er", "he\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Grund geleget?", "tokens": ["Den", "Grund", "ge\u00b7le\u00b7get", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}