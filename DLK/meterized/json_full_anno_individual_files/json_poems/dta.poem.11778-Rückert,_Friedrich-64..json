{"dta.poem.11778": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "64.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was uranf\u00e4nglich ist, das ist auch unanf\u00e4nglich,", "tokens": ["Was", "u\u00b7ran\u00b7f\u00e4ng\u00b7lich", "ist", ",", "das", "ist", "auch", "un\u00b7an\u00b7f\u00e4ng\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PDS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und unanf\u00e4ngliches nothwendig unverg\u00e4nglich.", "tokens": ["Und", "un\u00b7an\u00b7f\u00e4ng\u00b7li\u00b7ches", "noth\u00b7wen\u00b7dig", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was irgend wo und wann hat selber angefangen,", "tokens": ["Was", "ir\u00b7gend", "wo", "und", "wann", "hat", "sel\u00b7ber", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PWAV", "KON", "PWAV", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kann nicht der Anfang seyn, und mu\u00df ein End' erlangen.", "tokens": ["Kann", "nicht", "der", "An\u00b7fang", "seyn", ",", "und", "mu\u00df", "ein", "End'", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "VAINF", "$,", "KON", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Anfang nur allein kann nie zu Ende gehn,", "tokens": ["Der", "An\u00b7fang", "nur", "al\u00b7lein", "kann", "nie", "zu", "En\u00b7de", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er aus Nichts entstand, Nichts ohn' ihn kann entstehn.", "tokens": ["Weil", "er", "aus", "Nichts", "ent\u00b7stand", ",", "Nichts", "ohn'", "ihn", "kann", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "VVFIN", "$,", "PIS", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Worin die Welt entsteht, besteht, und untergeht,", "tokens": ["Wo\u00b7rin", "die", "Welt", "ent\u00b7steht", ",", "be\u00b7steht", ",", "und", "un\u00b7ter\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und neu entsteht, ist das, was in sich selber steht;", "tokens": ["Und", "neu", "ent\u00b7steht", ",", "ist", "das", ",", "was", "in", "sich", "sel\u00b7ber", "steht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "VAFIN", "PDS", "$,", "PRELS", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was in sich selber kreist, und Alles kreisen macht,", "tokens": ["Was", "in", "sich", "sel\u00b7ber", "kreist", ",", "und", "Al\u00b7les", "krei\u00b7sen", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PRF", "ADV", "VVFIN", "$,", "KON", "PIS", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich selbst bewegend, Allbewegung hat gebracht.", "tokens": ["Sich", "selbst", "be\u00b7we\u00b7gend", ",", "All\u00b7be\u00b7we\u00b7gung", "hat", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "$,", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und ein Bewegtes, das als Hebel der Bewegung", "tokens": ["Und", "ein", "Be\u00b7weg\u00b7tes", ",", "das", "als", "He\u00b7bel", "der", "Be\u00b7we\u00b7gung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "KOUS", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In sich den Anfang f\u00fchlt, ist selbst Uranfangsregung.", "tokens": ["In", "sich", "den", "An\u00b7fang", "f\u00fchlt", ",", "ist", "selbst", "U\u00b7ran\u00b7fangs\u00b7re\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "VVFIN", "$,", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+--+----", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Drum wenn du f\u00fchlst in dir ein Uranf\u00e4ngliches,", "tokens": ["Drum", "wenn", "du", "f\u00fchlst", "in", "dir", "ein", "U\u00b7ran\u00b7f\u00e4ng\u00b7li\u00b7ches", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dem Gef\u00fchl hast du dein Unverg\u00e4ngliches.", "tokens": ["In", "dem", "Ge\u00b7f\u00fchl", "hast", "du", "dein", "Un\u00b7ver\u00b7g\u00e4ng\u00b7li\u00b7ches", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}