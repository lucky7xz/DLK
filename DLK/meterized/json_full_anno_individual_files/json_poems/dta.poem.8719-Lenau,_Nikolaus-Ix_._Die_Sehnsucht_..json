{"dta.poem.8719": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Ix .  \n  Die Sehnsucht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Haben wir auch sch\u00f6n getr\u00e4umet", "tokens": ["Ha\u00b7ben", "wir", "auch", "sch\u00f6n", "ge\u00b7tr\u00e4u\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von des Gl\u00fcckes Zauberlanden,", "tokens": ["Von", "des", "Gl\u00fc\u00b7ckes", "Zau\u00b7berl\u00b7an\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo sich ew'ge Freudenkr\u00e4nze", "tokens": ["Wo", "sich", "ew'\u00b7ge", "Freu\u00b7den\u00b7kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um die trunknen Schl\u00e4fe wanden,", "tokens": ["Um", "die", "trunk\u00b7nen", "Schl\u00e4\u00b7fe", "wan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wir wachen auf am Morgen,", "tokens": ["Und", "wir", "wa\u00b7chen", "auf", "am", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kehren zu des Lebens M\u00fchen", "tokens": ["Keh\u00b7ren", "zu", "des", "Le\u00b7bens", "M\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne Klagen wir zur\u00fccke;", "tokens": ["Oh\u00b7ne", "Kla\u00b7gen", "wir", "zu\u00b7r\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4ume m\u00fcssen ja verbl\u00fchen.", "tokens": ["Tr\u00e4u\u00b7me", "m\u00fcs\u00b7sen", "ja", "ver\u00b7bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Also waltet in dem Gasthof", "tokens": ["Al\u00b7so", "wal\u00b7tet", "in", "dem", "Gast\u00b7hof"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klara nach der alten Weise,", "tokens": ["Kla\u00b7ra", "nach", "der", "al\u00b7ten", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur ein seliges Erinnern", "tokens": ["Nur", "ein", "se\u00b7li\u00b7ges", "E\u00b7rin\u00b7nern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "An den Traum umschwebt sie leise.", "tokens": ["An", "den", "Traum", "um\u00b7schwebt", "sie", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit gewohnter holder Miene", "tokens": ["Mit", "ge\u00b7wohn\u00b7ter", "hol\u00b7der", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fc\u00dfet sie die frohen Zecher;", "tokens": ["Gr\u00fc\u00b7\u00dfet", "sie", "die", "fro\u00b7hen", "Ze\u00b7cher", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch am freundlichsten vor allen,", "tokens": ["Doch", "am", "freund\u00b7lichs\u00b7ten", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "APPR", "PIAT", "$,"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.4": {"text": "F\u00fcllet Einem sie den Becher.", "tokens": ["F\u00fcl\u00b7let", "Ei\u00b7nem", "sie", "den", "Be\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Oft auch sah man, wie die Jungfrau", "tokens": ["Oft", "auch", "sah", "man", ",", "wie", "die", "Jung\u00b7frau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und der Krieger lange sprachen;", "tokens": ["Und", "der", "Krie\u00b7ger", "lan\u00b7ge", "spra\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heinrich ist es, der gestanden", "tokens": ["Hein\u00b7rich", "ist", "es", ",", "der", "ge\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "$,", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bei des Prinzen Kerkerwachen;", "tokens": ["Bei", "des", "Prin\u00b7zen", "Ker\u00b7ker\u00b7wa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und er wei\u00df gar viel zu r\u00fchmen,", "tokens": ["Und", "er", "wei\u00df", "gar", "viel", "zu", "r\u00fch\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem sch\u00f6nen F\u00fcrstenjungen,", "tokens": ["Von", "dem", "sch\u00f6\u00b7nen", "F\u00fcrs\u00b7ten\u00b7jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie dem Stolzen nie das Ungl\u00fcck", "tokens": ["Wie", "dem", "Stol\u00b7zen", "nie", "das", "Un\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Klagelaut erzwungen.", "tokens": ["Ei\u00b7nen", "Kla\u00b7ge\u00b7laut", "er\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Eines aber hoch zu preisen,", "tokens": ["Ei\u00b7nes", "a\u00b7ber", "hoch", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Worte nie verga\u00dfen,", "tokens": ["Sei\u00b7ne", "Wor\u00b7te", "nie", "ver\u00b7ga\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Prinz den b\u00f6sen Hauptmann", "tokens": ["Wie", "der", "Prinz", "den", "b\u00f6\u00b7sen", "Haupt\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Chantereine einst angelassen.", "tokens": ["Chan\u00b7ter\u00b7ei\u00b7ne", "einst", "an\u00b7ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Dieser trat mit plumpem Trotze", "tokens": ["Die\u00b7ser", "trat", "mit", "plum\u00b7pem", "Trot\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor den Stillen, scheinbar Zahmen,", "tokens": ["Vor", "den", "Stil\u00b7len", ",", "schein\u00b7bar", "Zah\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihm den S\u00e4bel abzufordern", "tokens": ["Ihm", "den", "S\u00e4\u00b7bel", "ab\u00b7zu\u00b7for\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frech in K\u00f6nig Ludwigs Namen.", "tokens": ["Frech", "in", "K\u00f6\u00b7nig", "Lud\u00b7wigs", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Doch wie donnerte der J\u00fcngling:", "tokens": ["Doch", "wie", "don\u00b7ner\u00b7te", "der", "J\u00fcng\u00b7ling", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "\u201eich bin Johann Prinz von Polen!", "tokens": ["\u201e", "ich", "bin", "Jo\u00b7hann", "Prinz", "von", "Po\u00b7len", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "NE", "NN", "APPR", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u201el\u00fcstet ihn nach meinem Schwerte,", "tokens": ["\u201e", "l\u00fcs\u00b7tet", "ihn", "nach", "mei\u00b7nem", "Schwer\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201emags dein K\u00f6nig selber holen!\u201c", "tokens": ["\u201e", "mags", "dein", "K\u00f6\u00b7nig", "sel\u00b7ber", "ho\u00b7len", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Feig verzagend vor dem K\u00fchnen", "tokens": ["Feig", "ver\u00b7za\u00b7gend", "vor", "dem", "K\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sucht der Hauptmann seine Rotte", "tokens": ["Sucht", "der", "Haupt\u00b7mann", "sei\u00b7ne", "Rot\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zur Gewaltthat aufzustacheln", "tokens": ["Zur", "Ge\u00b7walt\u00b7that", "auf\u00b7zu\u00b7sta\u00b7cheln"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit Befehl und scharfem Spotte.", "tokens": ["Mit", "Be\u00b7fehl", "und", "schar\u00b7fem", "Spot\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ha! wie hat der Polenj\u00fcngling", "tokens": ["Ha", "!", "wie", "hat", "der", "Po\u00b7len\u00b7j\u00fcng\u00b7ling"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jezt sein tapfres Schwert geschwungen!", "tokens": ["Jezt", "sein", "tapf\u00b7res", "Schwert", "ge\u00b7schwun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ha! wie ist er auf den Hauptmann,", "tokens": ["Ha", "!", "wie", "ist", "er", "auf", "den", "Haupt\u00b7mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf die Knechte eingedrungen!", "tokens": ["Auf", "die", "Knech\u00b7te", "ein\u00b7ge\u00b7drun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und die Rotte feiler Schergen", "tokens": ["Und", "die", "Rot\u00b7te", "fei\u00b7ler", "Scher\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Taumelte zur\u00fcck, erschrocken,", "tokens": ["Tau\u00b7mel\u00b7te", "zu\u00b7r\u00fcck", ",", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Sturmwind auseinander", "tokens": ["Wie", "der", "Sturm\u00b7wind", "aus\u00b7ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jagt der Spreu geringe Flocken. \u2014", "tokens": ["Jagt", "der", "Spreu", "ge\u00b7rin\u00b7ge", "Flo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Schwellend hat bei solchen Reden", "tokens": ["Schwel\u00b7lend", "hat", "bei", "sol\u00b7chen", "Re\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klara's Busen sich erhoben,", "tokens": ["Kla\u00b7ra's", "Bu\u00b7sen", "sich", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00fc\u00dfer Klang ist's f\u00fcr die Jungfrau,", "tokens": ["S\u00fc\u00b7\u00dfer", "Klang", "ist's", "f\u00fcr", "die", "Jung\u00b7frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+----+", "measure": "unknown.measure.tri"}, "line.4": {"text": "H\u00f6rt sie den Geliebten loben. \u2014 \u2014", "tokens": ["H\u00f6rt", "sie", "den", "Ge\u00b7lieb\u00b7ten", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "War nun Klara gegen jeden", "tokens": ["War", "nun", "Kla\u00b7ra", "ge\u00b7gen", "je\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "NE", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Froh und freundlich tages\u00fcber,", "tokens": ["Froh", "und", "freund\u00b7lich", "ta\u00b7ges\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie endlich kann allein seyn,", "tokens": ["Wenn", "sie", "end\u00b7lich", "kann", "al\u00b7lein", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "ADV", "VAINF", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ist sie Abends um so tr\u00fcber.", "tokens": ["Ist", "sie", "A\u00b7bends", "um", "so", "tr\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Ist ihr auch das Gl\u00fcck der Liebe", "tokens": ["Ist", "ihr", "auch", "das", "Gl\u00fcck", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein Traum vor\u00fcbergangen,", "tokens": ["Wie", "ein", "Traum", "vor\u00b7\u00fc\u00b7be