elt", "Kla\u00b7ra", "still", "und", "ein\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihres Grams geheime Pfade.", "tokens": ["Ih\u00b7res", "Grams", "ge\u00b7hei\u00b7me", "Pfa\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Aber nicht vom stillen Meere,", "tokens": ["A\u00b7ber", "nicht", "vom", "stil\u00b7len", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht vom Meere, sturmgeschlagen,", "tokens": ["Nicht", "vom", "Mee\u00b7re", ",", "sturm\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Harret sie auch manche Jahre,", "tokens": ["Har\u00b7ret", "sie", "auch", "man\u00b7che", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird der Theure hergetragen.", "tokens": ["Wird", "der", "Theu\u00b7re", "her\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}