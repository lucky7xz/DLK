{"dta.poem.874": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "LxI.  Auff den Sontag de\u00df/ die in S\u00fcnden  \n Kranck/ ja Todliegende Menschen auffweck\u1ebd-  \n den Artztes oder  XXIV.  Sontag nach dem Fest der  \n H. dreyeinigkeit/  Matth.  9.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ichleb/ vnd bin doch tod/ die H\u00e4nde gantz erkalten/ ", "tokens": ["I\u00b7chleb", "/", "vnd", "bin", "doch", "tod", "/", "die", "H\u00e4n\u00b7de", "gantz", "er\u00b7kal\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KON", "VAFIN", "ADV", "NN", "$(", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die F\u00fc\u00df/ erstarren mir/ die Augen werden blind/", "tokens": ["Die", "F\u00fc\u00df", "/", "er\u00b7star\u00b7ren", "mir", "/", "die", "Au\u00b7gen", "wer\u00b7den", "blind", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "PPER", "$(", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Ohren h\u00f6ren nicht/ die H\u00e4nd vnsauber sind.", "tokens": ["Die", "Oh\u00b7ren", "h\u00f6\u00b7ren", "nicht", "/", "die", "H\u00e4nd", "vn\u00b7sau\u00b7ber", "sind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$(", "ART", "NN", "KON", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Vbertrettungen mich gantz durch-au\u00df verstalten/", "tokens": ["Mein", "Vbert\u00b7ret\u00b7tun\u00b7gen", "mich", "gantz", "durch\u00b7au\u00df", "ver\u00b7stal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "NE", "VVFIN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Bo\u00dfheit dicke-h\u00e4rt Sinn vnd Gem\u00fcth einhalten/", "tokens": ["Der", "Bo\u00df\u00b7heit", "di\u00b7cke\u00b7h\u00e4rt", "Sinn", "vnd", "Ge\u00b7m\u00fcth", "ein\u00b7hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vnd alle Gliedere beziht der S\u00fcnden-Grind/", "tokens": ["Vnd", "al\u00b7le", "Glie\u00b7de\u00b7re", "be\u00b7ziht", "der", "S\u00fcn\u00b7den\u00b7Grind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich seh gantz sche\u00fc\u00dflich au\u00df. Ja/ was man an mirfind/", "tokens": ["Ich", "seh", "gantz", "sche\u00fc\u00df\u00b7lich", "au\u00df", ".", "Ja", "/", "was", "man", "an", "mir\u00b7find", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$.", "PTKANT", "$(", "PWS", "PIS", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom Haupt/ bi\u00df auff die F\u00fc\u00df/ will gantz in mir veralten/", "tokens": ["Vom", "Haupt", "/", "bi\u00df", "auff", "die", "F\u00fc\u00df", "/", "will", "gantz", "in", "mir", "ver\u00b7al\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "KON", "APPR", "ART", "NN", "$(", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vnd gehn dem Grabe zu/ zur finstern Hellen-Grufft:", "tokens": ["Vnd", "gehn", "dem", "Gra\u00b7be", "zu", "/", "zur", "fins\u00b7tern", "Hel\u00b7len\u00b7G\u00b7rufft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKZU", "$(", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Meer-Sand schwere S\u00fcnd benimt mich fast der Lufft", "tokens": ["Die", "Meer\u00b7Sand", "schwe\u00b7re", "S\u00fcnd", "be\u00b7nimt", "mich", "fast", "der", "Lufft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch/ Jesu/ Tr\u00f6ster werth/ auff dich soll mein Hertz hoffen/", "tokens": ["Doch", "/", "Je\u00b7su", "/", "Tr\u00f6s\u00b7ter", "werth", "/", "auff", "dich", "soll", "mein", "Hertz", "hof\u00b7fen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "NE", "$(", "NN", "ADJD", "$(", "APPR", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du bist mein rechter Artzt/ komm mache mich gesund/", "tokens": ["Du", "bist", "mein", "rech\u00b7ter", "Artzt", "/", "komm", "ma\u00b7che", "mich", "ge\u00b7sund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$(", "VVFIN", "VVFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn soll ich r\u016fhmen dich mit Hertzen vnd mit Mund.", "tokens": ["Denn", "soll", "ich", "r\u016fh\u00b7men", "dich", "mit", "Hert\u00b7zen", "vnd", "mit", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O Gunst! Ichlebe schon. Der Himmel steht mir offen.", "tokens": ["O", "Gunst", "!", "I\u00b7chle\u00b7be", "schon", ".", "Der", "Him\u00b7mel", "steht", "mir", "of\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "ADV", "$.", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}