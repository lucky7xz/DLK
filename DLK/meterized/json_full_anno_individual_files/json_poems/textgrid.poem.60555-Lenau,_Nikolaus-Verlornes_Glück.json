{"textgrid.poem.60555": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Verlornes Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die B\u00e4ume rauschen hier noch immer,", "tokens": ["Die", "B\u00e4u\u00b7me", "rau\u00b7schen", "hier", "noch", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch sinds dieselben Bl\u00e4tter nimmer,", "tokens": ["Doch", "sinds", "die\u00b7sel\u00b7ben", "Bl\u00e4t\u00b7ter", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie einst in jener Sommernacht.", "tokens": ["Wie", "einst", "in", "je\u00b7ner", "Som\u00b7mer\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohin, du rauhes Erdenwetter,", "tokens": ["Wo\u00b7hin", ",", "du", "rau\u00b7hes", "Er\u00b7den\u00b7wet\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hast du die damals gr\u00fcnen Bl\u00e4tter,", "tokens": ["Hast", "du", "die", "da\u00b7mals", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wohin hast du mein Gl\u00fcck gebracht?", "tokens": ["Wo\u00b7hin", "hast", "du", "mein", "Gl\u00fcck", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie schritt mit mir durch diese B\u00e4ume,", "tokens": ["Sie", "schritt", "mit", "mir", "durch", "die\u00b7se", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr gleicht kein Bild begl\u00fcckter Tr\u00e4ume,", "tokens": ["Ihr", "gleicht", "kein", "Bild", "be\u00b7gl\u00fcck\u00b7ter", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So sch\u00f6n und doch so treu und klar;", "tokens": ["So", "sch\u00f6n", "und", "doch", "so", "treu", "und", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Mondlicht ruht' auf ihren Wangen,", "tokens": ["Das", "Mond\u00b7licht", "ruht'", "auf", "ih\u00b7ren", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und ihre s\u00fc\u00dfen Worte klangen:", "tokens": ["Und", "ih\u00b7re", "s\u00fc\u00b7\u00dfen", "Wor\u00b7te", "klan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra