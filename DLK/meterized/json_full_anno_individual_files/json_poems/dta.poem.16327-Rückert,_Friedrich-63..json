{"dta.poem.16327": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich schmelz' in Dankbarkeit und R\u00fchrung, wenn ich denke,", "tokens": ["Ich", "schmelz'", "in", "Dank\u00b7bar\u00b7keit", "und", "R\u00fch\u00b7rung", ",", "wenn", "ich", "den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich durch deine Kraft nach deinen Zielen lenke", "tokens": ["Da\u00df", "ich", "durch", "dei\u00b7ne", "Kraft", "nach", "dei\u00b7nen", "Zie\u00b7len", "len\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Schritte, die solang, so oft, so tief, so schwer", "tokens": ["Die", "Schrit\u00b7te", ",", "die", "so\u00b7lang", ",", "so", "oft", ",", "so", "tief", ",", "so", "schwer"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "$,", "ADV", "ADV", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gestrauchelt, und hinfort, hoff' ich, nicht straucheln mehr.", "tokens": ["Ge\u00b7strau\u00b7chelt", ",", "und", "hin\u00b7fort", ",", "hoff'", "ich", ",", "nicht", "strau\u00b7cheln", "mehr", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "ADV", "$,", "VVFIN", "PPER", "$,", "PTKNEG", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da\u00df ich nicht weiter kam durch meine Schuld, o Scham!", "tokens": ["Da\u00df", "ich", "nicht", "wei\u00b7ter", "kam", "durch", "mei\u00b7ne", "Schuld", ",", "o", "Scham", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Gl\u00fcck, da\u00df ich so weit durch deine Gnade kam.", "tokens": ["O", "Gl\u00fcck", ",", "da\u00df", "ich", "so", "weit", "durch", "dei\u00b7ne", "Gna\u00b7de", "kam", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}