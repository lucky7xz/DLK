{"textgrid.poem.65811": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "85", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch immer bleibt die Muse still bescheiden,", "tokens": ["Noch", "im\u00b7mer", "bleibt", "die", "Mu\u00b7se", "still", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn goldne Federn in ein Prunkgewand", "tokens": ["Wenn", "gold\u00b7ne", "Fe\u00b7dern", "in", "ein", "Prunk\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Ruhmes dich mit stolzen Z\u00fcgen kleiden,", "tokens": ["Des", "Ruh\u00b7mes", "dich", "mit", "stol\u00b7zen", "Z\u00fc\u00b7gen", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Schmuck, gewebt von aller Musen Hand.", "tokens": ["In", "Schmuck", ",", "ge\u00b7webt", "von", "al\u00b7ler", "Mu\u00b7sen", "Hand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVPP", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo andre gute Worte schreiben, denk' ich", "tokens": ["Wo", "and\u00b7re", "gu\u00b7te", "Wor\u00b7te", "schrei\u00b7ben", ",", "denk'", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PIS", "AD