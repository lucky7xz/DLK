{"textgrid.poem.56103": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Derselbe gro\u00dfe Engel, welcher einst", "tokens": ["Der\u00b7sel\u00b7be", "gro\u00b7\u00dfe", "En\u00b7gel", ",", "wel\u00b7cher", "einst"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ihr der Geb\u00e4rung Botschaft niederbrachte,", "tokens": ["ihr", "der", "Ge\u00b7b\u00e4\u00b7rung", "Bot\u00b7schaft", "nie\u00b7der\u00b7brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "stand da, abwartend da\u00df sie ihn beachte,", "tokens": ["stand", "da", ",", "ab\u00b7war\u00b7tend", "da\u00df", "sie", "ihn", "be\u00b7ach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJD", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "und sprach: Jetzt wird es Zeit, da\u00df du erscheinst.", "tokens": ["und", "sprach", ":", "Jetzt", "wird", "es", "Zeit", ",", "da\u00df", "du", "er\u00b7scheinst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und sie erschrak wie damals und erwies", "tokens": ["Und", "sie", "er\u00b7schrak", "wie", "da\u00b7mals", "und", "er\u00b7wies"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "sich wieder als die Magd, ihn tief bejahend.", "tokens": ["sich", "wie\u00b7der", "als", "die", "Magd", ",", "ihn", "tief", "be\u00b7ja\u00b7hend", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "KOUS", "ART", "NN", "$,", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Er aber strahlte und, unendlich nahend,", "tokens": ["Er", "a\u00b7ber", "strahl\u00b7te", "und", ",", "un\u00b7end\u00b7lich", "na\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "KON", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "schwand er wie in ihr Angesicht \u2013 und hie\u00df", "tokens": ["schwand", "er", "wie", "in", "ihr", "An\u00b7ge\u00b7sicht", "\u2013", "und", "hie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "KOKOM", "APPR", "PPOSAT", "NN", "$(", "KON", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "die weithin ausgegangenen Bekehrer", "tokens": ["die", "weit\u00b7hin", "aus\u00b7ge\u00b7gan\u00b7ge\u00b7nen", "Be\u00b7keh\u00b7rer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "zusammenkommen in das Haus am Hang,", "tokens": ["zu\u00b7sam\u00b7men\u00b7kom\u00b7men", "in", "das", "Haus", "am", "Hang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "das Haus des Abendmahls. Sie kamen schwerer", "tokens": ["das", "Haus", "des", "A\u00b7bend\u00b7mahls", ".", "Sie", "ka\u00b7men", "schwe\u00b7rer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "und traten bange ein: Da lag, entlang", "tokens": ["und", "tra\u00b7ten", "ban\u00b7ge", "ein", ":", "Da", "lag", ",", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$.", "ADV", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "die schmale Bettstatt, die in Untergang", "tokens": ["die", "schma\u00b7le", "Bett\u00b7statt", ",", "die", "in", "Un\u00b7ter\u00b7gang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "und Auserw\u00e4hlung r\u00e4tselhaft Getauchte,", "tokens": ["und", "Au\u00b7ser\u00b7w\u00e4h\u00b7lung", "r\u00e4t\u00b7sel\u00b7haft", "Ge\u00b7tauch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "ganz unversehrt, wie eine Ungebrauchte,", "tokens": ["ganz", "un\u00b7ver\u00b7sehrt", ",", "wie", "ei\u00b7ne", "Un\u00b7ge\u00b7brauch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "und achtete auf englischen Gesang.", "tokens": ["und", "ach\u00b7te\u00b7te", "auf", "eng\u00b7li\u00b7schen", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Nun da sie alle hinter ihren Kerzen", "tokens": ["Nun", "da", "sie", "al\u00b7le", "hin\u00b7ter", "ih\u00b7ren", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PIS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "abwarten sah, ri\u00df sie vom \u00dcberma\u00df", "tokens": ["ab\u00b7war\u00b7ten", "sah", ",", "ri\u00df", "sie", "vom", "\u00dc\u00b7berm\u00b7a\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVINF", "VVFIN", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "der Stimmen sich und schenkte noch von Herzen", "tokens": ["der", "Stim\u00b7men", "sich", "und", "schenk\u00b7te", "noch", "von", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "KON", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "die beiden Kleider fort, die sie besa\u00df,", "tokens": ["die", "bei\u00b7den", "Klei\u00b7der", "fort", ",", "die", "sie", "be\u00b7sa\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKVZ", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "und hob ihr Antlitz auf zu dem und dem...", "tokens": ["und", "hob", "ihr", "Ant\u00b7litz", "auf", "zu", "dem", "und", "dem", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "APPR", "ART", "KON", "ART", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "(o Ursprung