{"dta.poem.10442": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Nacht lag auf meinen Augen,", "tokens": ["Nacht", "lag", "auf", "mei\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Blei lag auf meinem Mund,", "tokens": ["Blei", "lag", "auf", "mei\u00b7nem", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit starrem Hirn und Herzen", "tokens": ["Mit", "star\u00b7rem", "Hirn", "und", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lag ich in Grabesgrund.", "tokens": ["Lag", "ich", "in", "Gra\u00b7bes\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie lang kann ich nicht sagen,", "tokens": ["Wie", "lang", "kann", "ich", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich geschlafen hab';", "tokens": ["Da\u00df", "ich", "ge\u00b7schla\u00b7fen", "hab'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wachte auf und h\u00f6rte", "tokens": ["Ich", "wach\u00b7te", "auf", "und", "h\u00f6r\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie's pochte an mein Grab.", "tokens": ["Wie's", "poch\u00b7te", "an", "mein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201ewillst du nicht aufstehn, Heinrich?", "tokens": ["\u201e", "willst", "du", "nicht", "auf\u00b7stehn", ",", "Hein\u00b7rich", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der ew'ge Tag bricht an,", "tokens": ["Der", "ew'\u00b7ge", "Tag", "bricht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Todten sind erstanden,", "tokens": ["Die", "Tod\u00b7ten", "sind", "er\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ew'ge Lust begann.\u201c", "tokens": ["Die", "ew'\u00b7ge", "Lust", "be\u00b7gann", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mein Lieb, ich kann nicht aufstehn,", "tokens": ["Mein", "Lieb", ",", "ich", "kann", "nicht", "auf\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Bin ja noch immer blind;", "tokens": ["Bin", "ja", "noch", "im\u00b7mer", "blind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch Weinen meine Augen", "tokens": ["Durch", "Wei\u00b7nen", "mei\u00b7ne", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "G\u00e4nzlich erloschen sind.", "tokens": ["G\u00e4nz\u00b7lich", "er\u00b7lo\u00b7schen", "sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "\u201eich will dir k\u00fcssen, Heinrich,", "tokens": ["\u201e", "ich", "will", "dir", "k\u00fcs\u00b7sen", ",", "Hein\u00b7rich", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "VVINF", "$,", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Vom Auge fort die Nacht;", "tokens": ["Vom", "Au\u00b7ge", "fort", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Engel sollst du schauen,", "tokens": ["Die", "En\u00b7gel", "sollst", "du", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und auch des Himmels Pracht.\u201c", "tokens": ["Und", "auch", "des", "Him\u00b7mels", "Pracht", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mein Lieb ich kann nicht aufstehn,", "tokens": ["Mein", "Lieb", "ich", "kann", "nicht", "auf\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Noch blutet's immerfort,", "tokens": ["Noch", "blu\u00b7tet's", "im\u00b7mer\u00b7fort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo du in's Herz mir stachest", "tokens": ["Wo", "du", "in's", "Herz", "mir", "sta\u00b7chest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit einem spitz'gen Wort'.", "tokens": ["Mit", "ei\u00b7nem", "spitz'\u00b7gen", "Wort'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u201eganz leise leg' ich, Heinrich,", "tokens": ["\u201e", "ganz", "lei\u00b7se", "leg'", "ich", ",", "Hein\u00b7rich", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "VVFIN", "PPER", "$,", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Dir meine Hand auf's Herz;", "tokens": ["Dir", "mei\u00b7ne", "Hand", "auf's", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann wird es nicht mehr bluten,", "tokens": ["Dann", "wird", "es", "nicht", "mehr", "blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geheilt ist all sein Schmerz.\u201c", "tokens": ["Ge\u00b7heilt", "ist", "all", "sein", "Schmerz", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Mein Lieb, ich kann nicht aufstehn,", "tokens": ["Mein", "Lieb", ",", "ich", "kann", "nicht", "auf\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Es blutet auch mein Haupt;", "tokens": ["Es", "blu\u00b7tet", "auch", "mein", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hab' ja hinein geschossen,", "tokens": ["Hab'", "ja", "hin\u00b7ein", "ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als du mir wurdest geraubt.", "tokens": ["Als", "du", "mir", "wur\u00b7dest", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "\u201emit meinen Locken, Heinrich,", "tokens": ["\u201e", "mit", "mei\u00b7nen", "Lo\u00b7cken", ",", "Hein\u00b7rich", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "$,", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Stopf' ich des Hauptes Wund',", "tokens": ["Stopf'", "ich", "des", "Haup\u00b7tes", "Wund'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dr\u00e4ng' zur\u00fcck den Blutstrom,", "tokens": ["Und", "dr\u00e4ng'", "zu\u00b7r\u00fcck", "den", "Bluts\u00b7trom", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und mache dein Haupt gesund.\u201c", "tokens": ["Und", "ma\u00b7che", "dein", "Haupt", "ge\u00b7sund", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Es bat so sanft, so lieblich,", "tokens": ["Es", "bat", "so", "sanft", ",", "so", "lieb\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich konnt' nicht widerstehn;", "tokens": ["Ich", "konnt'", "nicht", "wi\u00b7der\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wollte mich erheben,", "tokens": ["Ich", "woll\u00b7te", "mich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und zu der Liebsten gehn.", "tokens": ["Und", "zu", "der", "Liebs\u00b7ten", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Da brachen auf die Wunden,", "tokens": ["Da", "bra\u00b7chen", "auf", "die", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da st\u00fcrzt' mit wilder Macht", "tokens": ["Da", "st\u00fcrzt'", "mit", "wil\u00b7der", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus Kopf und Brust der Blutstrom,", "tokens": ["Aus", "Kopf", "und", "Brust", "der", "Bluts\u00b7trom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sieh! \u2014 ich bin erwacht.", "tokens": ["Und", "sieh", "!", "ich", "bin", "er\u00b7wacht", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "$(", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}