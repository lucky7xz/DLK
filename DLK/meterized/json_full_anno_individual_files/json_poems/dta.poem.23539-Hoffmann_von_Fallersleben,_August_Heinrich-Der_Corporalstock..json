{"dta.poem.23539": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Corporalstock.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von einem Helden will ich singen", "tokens": ["Von", "ei\u00b7nem", "Hel\u00b7den", "will", "ich", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der einst die ganze Welt bezwang:", "tokens": ["Der", "einst", "die", "gan\u00b7ze", "Welt", "be\u00b7zwang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So konnt' es keinem noch gelingen,", "tokens": ["So", "konnt'", "es", "kei\u00b7nem", "noch", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So glorreich wie es ihm gelang.", "tokens": ["So", "glor\u00b7reich", "wie", "es", "ihm", "ge\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Obschon im Waldesgr\u00fcn geboren", "tokens": ["Ob\u00b7schon", "im", "Wal\u00b7des\u00b7gr\u00fcn", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bei Amselschlag und Fr\u00fchlingswehn,", "tokens": ["Bei", "Am\u00b7sel\u00b7schlag", "und", "Fr\u00fch\u00b7lings\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So war er doch dazu erkoren,", "tokens": ["So", "war", "er", "doch", "da\u00b7zu", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Herren Hand in Hand zu gehn.", "tokens": ["Mit", "Her\u00b7ren", "Hand", "in", "Hand", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er ward gewiegt von F\u00fcrstenh\u00e4nden,", "tokens": ["Er", "ward", "ge\u00b7wiegt", "von", "F\u00fcrs\u00b7ten\u00b7h\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zopf und Kamasche pflegten sein;", "tokens": ["Zopf", "und", "Ka\u00b7ma\u00b7sche", "pfleg\u00b7ten", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "VAINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sie mussten viele Zeit verwenden", "tokens": ["Sie", "muss\u00b7ten", "vie\u00b7le", "Zeit", "ver\u00b7wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu seinem Wachsthum und Gedeihn.", "tokens": ["Zu", "sei\u00b7nem", "Wach\u00b7sthum", "und", "Ge\u00b7deihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann gab man ihn noch in die Lehre", "tokens": ["Dann", "gab", "man", "ihn", "noch", "in", "die", "Leh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu einem braven Corporal,", "tokens": ["Zu", "ei\u00b7nem", "bra\u00b7ven", "Cor\u00b7po\u00b7ral", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ward er voller Zucht und Ehre,", "tokens": ["Da", "ward", "er", "vol\u00b7ler", "Zucht", "und", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Leder z\u00e4h, und hart wie Stahl.", "tokens": ["Wie", "Le\u00b7der", "z\u00e4h", ",", "und", "hart", "wie", "Stahl", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$,", "KON", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er bracht' es nun in wenig Tagen", "tokens": ["Er", "bracht'", "es", "nun", "in", "we\u00b7nig", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu solcher hohen Trefflichkeit,", "tokens": ["Zu", "sol\u00b7cher", "ho\u00b7hen", "Treff\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Staunen, Schrecken, Angst und Zagen", "tokens": ["Da\u00df", "Stau\u00b7nen", ",", "Schre\u00b7cken", ",", "Angst", "und", "Za\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ergriff die ganze Christenheit.", "tokens": ["Er\u00b7griff", "die", "gan\u00b7ze", "Chris\u00b7ten\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er ward bekannt in allen Landen", "tokens": ["Er", "ward", "be\u00b7kannt", "in", "al\u00b7len", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo nur was Gro\u00dfes je geschah,", "tokens": ["Wo", "nur", "was", "Gro\u00b7\u00dfes", "je", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PWS", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ganze Regimenter standen", "tokens": ["Und", "gan\u00b7ze", "Re\u00b7gi\u00b7men\u00b7ter", "stan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor ihm wie Leichen lautlos da.", "tokens": ["Vor", "ihm", "wie", "Lei\u00b7chen", "laut\u00b7los", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KOKOM", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O weh, er ist nun Staub und Asche,", "tokens": ["O", "weh", ",", "er", "ist", "nun", "Staub", "und", "A\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "VAFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O weh, o weh, er ist nicht mehr!", "tokens": ["O", "weh", ",", "o", "weh", ",", "er", "ist", "nicht", "mehr", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "FM", "PTKVZ", "$,", "PPER", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dahin ist Zopf, dahin Kamasche!", "tokens": ["Da\u00b7hin", "ist", "Zopf", ",", "da\u00b7hin", "Ka\u00b7ma\u00b7sche", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "$,", "PAV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dahin sein ganzes gro\u00dfes Heer!", "tokens": ["Da\u00b7hin", "sein", "gan\u00b7zes", "gro\u00b7\u00dfes", "Heer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Kein Denkmal ist von ihm geblieben,", "tokens": ["Kein", "Denk\u00b7mal", "ist", "von", "ihm", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch war in jener guten Zeit", "tokens": ["Doch", "war", "in", "je\u00b7ner", "gu\u00b7ten", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf jedem R\u00fccken eingeschrieben", "tokens": ["Auf", "je\u00b7dem", "R\u00fc\u00b7cken", "ein\u00b7ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Ruhm und seine Tapferkeit.", "tokens": ["Sein", "Ruhm", "und", "sei\u00b7ne", "Tap\u00b7fer\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Uns aber liess er zum Verm\u00e4chtni\u00df", "tokens": ["Uns", "a\u00b7ber", "liess", "er", "zum", "Ver\u00b7m\u00e4cht\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den alten Corporal zur\u00fcck,", "tokens": ["Den", "al\u00b7ten", "Cor\u00b7po\u00b7ral", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der ruft uns allen ins Ged\u00e4chtni\u00df", "tokens": ["Der", "ruft", "uns", "al\u00b7len", "ins", "Ge\u00b7d\u00e4cht\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mitunter noch das alte Gl\u00fcck.", "tokens": ["Mi\u00b7tun\u00b7ter", "noch", "das", "al\u00b7te", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wir aber sind zu dumm geworden", "tokens": ["Wir", "a\u00b7ber", "sind", "zu", "dumm", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "PTKA", "ADJD", "VAPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr jene alte gute Zeit;", "tokens": ["F\u00fcr", "je\u00b7ne", "al\u00b7te", "gu\u00b7te", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sei im S\u00fcden, sei im Norden,", "tokens": ["Sie", "sei", "im", "S\u00fc\u00b7den", ",", "sei", "im", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur bleibe sie von uns recht weit!", "tokens": ["Nur", "blei\u00b7be", "sie", "von", "uns", "recht", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Auch Millionen werden flehen,", "tokens": ["Auch", "Mil\u00b7lion\u00b7en", "wer\u00b7den", "fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn Gott der Herr sitzt zu Gericht:", "tokens": ["Wenn", "Gott", "der", "Herr", "sitzt", "zu", "Ge\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "La\u00df alle Helden auferstehen,", "tokens": ["La\u00df", "al\u00b7le", "Hel\u00b7den", "auf\u00b7er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur diesen, diesen einen nicht \u2014", "tokens": ["Nur", "die\u00b7sen", ",", "die\u00b7sen", "ei\u00b7nen", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "$,", "PDAT", "ART", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}