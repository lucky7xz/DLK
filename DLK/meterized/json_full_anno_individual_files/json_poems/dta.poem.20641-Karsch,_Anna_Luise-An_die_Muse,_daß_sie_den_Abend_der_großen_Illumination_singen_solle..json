{"dta.poem.20641": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  die Muse,  \n da\u00df sie  \n  den Abend der gro\u00dfen Illumination singen solle.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die du das Feld des Krieges \u00fcberflogest,                 ", "tokens": ["Die", "du", "das", "Feld", "des", "Krie\u00b7ges", "\u00fc\u00b7berf\u00b7lo\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Schwefelduft und Kugelregen drangst,", "tokens": ["Durch", "Schwe\u00b7fel\u00b7duft", "und", "Ku\u00b7gel\u00b7re\u00b7gen", "drangst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Sieger, und mit ihm durch Ehrenpforten zogest,", "tokens": ["Zum", "Sie\u00b7ger", ",", "und", "mit", "ihm", "durch", "Eh\u00b7ren\u00b7pfor\u00b7ten", "zo\u00b7gest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und seines Einzugs Jubel sangst,", "tokens": ["Und", "sei\u00b7nes", "Ein\u00b7zugs", "Ju\u00b7bel", "sangst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Muse! singe nun auch k\u00fchn den stolzen Abend,", "tokens": ["O", "Mu\u00b7se", "!", "sin\u00b7ge", "nun", "auch", "k\u00fchn", "den", "stol\u00b7zen", "A\u00b7bend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "ADV", "ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der von des Sturmwinds Fl\u00fcgeln lo\u00df,", "tokens": ["Der", "von", "des", "Sturm\u00b7winds", "Fl\u00fc\u00b7geln", "lo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Sternenmantel um sich habend,", "tokens": ["Den", "Ster\u00b7nen\u00b7man\u00b7tel", "um", "sich", "ha\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Herabsah, auf ", "tokens": ["Her\u00b7ab\u00b7sah", ",", "auf"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und seinen Frieden ewig nannte,", "tokens": ["Und", "sei\u00b7nen", "Frie\u00b7den", "e\u00b7wig", "nann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und, von der Kunst beflammt, den Sph\u00e4ren \u00e4hnlich", "tokens": ["Und", ",", "von", "der", "Kunst", "be\u00b7flammt", ",", "den", "Sph\u00e4\u00b7ren", "\u00e4hn\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "brannte,", "tokens": ["brann\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Ganz \u00e4hnlich jenem Pomp der pr\u00e4chtigen Natur.", "tokens": ["Ganz", "\u00e4hn\u00b7lich", "je\u00b7nem", "Pomp", "der", "pr\u00e4ch\u00b7ti\u00b7gen", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PDAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}