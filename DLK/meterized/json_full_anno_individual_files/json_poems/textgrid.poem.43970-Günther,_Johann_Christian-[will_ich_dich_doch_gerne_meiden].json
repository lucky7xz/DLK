: {"text": "Bis der Gram mein Leben raube,", "tokens": ["Bis", "der", "Gram", "mein", "Le\u00b7ben", "rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis die Kr\u00e4fte sich verschreyn,", "tokens": ["Bis", "die", "Kr\u00e4f\u00b7te", "sich", "ver\u00b7schreyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und da soll ein Grab voll Laube", "tokens": ["Und", "da", "soll", "ein", "Grab", "voll", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "ART", "NN", "ADJD", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Milder als dein Herze seyn.", "tokens": ["Mil\u00b7der", "als", "dein", "Her\u00b7ze", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPOSAT", "VVFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Kan ich dich an Treu besch\u00e4men,", "tokens": ["Kan", "ich", "dich", "an", "Treu", "be\u00b7sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich noch dein Conterfey", "tokens": ["Will", "ich", "noch", "dein", "Con\u00b7ter\u00b7fey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Tod ans Herze nehmen,", "tokens": ["In", "dem", "Tod", "ans", "Her\u00b7ze", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word"