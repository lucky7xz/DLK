{"textgrid.poem.43970": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[will ich dich doch gerne meiden]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Will ich dich doch gerne meiden,", "tokens": ["Will", "ich", "dich", "doch", "ger\u00b7ne", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gieb mir nur noch einen Ku\u00df,", "tokens": ["Gieb", "mir", "nur", "noch", "ei\u00b7nen", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh ich sonst das Lezte leiden", "tokens": ["Eh", "ich", "sonst", "das", "Lez\u00b7te", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den Ring zerbrechen mu\u00df.", "tokens": ["Und", "den", "Ring", "zer\u00b7bre\u00b7chen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fchle doch die starcken Triebe", "tokens": ["F\u00fch\u00b7le", "doch", "die", "star\u00b7cken", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und des Herzens bange Qual!", "tokens": ["Und", "des", "Her\u00b7zens", "ban\u00b7ge", "Qual", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Also bitter schmeckt der Liebe", "tokens": ["Al\u00b7so", "bit\u00b7ter", "schmeckt", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "So ein sch\u00f6nes Henckermahl.", "tokens": ["So", "ein", "sch\u00f6\u00b7nes", "Hen\u00b7cker\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Las dich etwas Be\u00dfers k\u00fc\u00dfen,", "tokens": ["Las", "dich", "et\u00b7was", "Be\u00b7\u00dfers", "k\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles g\u00f6nn und w\u00fcntsch ich dir;", "tokens": ["Al\u00b7les", "g\u00f6nn", "und", "w\u00fcnt\u00b7sch", "ich", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "KON", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Aber frag auch dein Gewi\u00dfen,", "tokens": ["A\u00b7ber", "frag", "auch", "dein", "Ge\u00b7wi\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Zeuge bleibet mir.", "tokens": ["Die\u00b7ser", "Zeu\u00b7ge", "blei\u00b7bet", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lerne doch nur weiter dencken:", "tokens": ["Ler\u00b7ne", "doch", "nur", "wei\u00b7ter", "den\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "D\u00f6rft es dich auch einmahl reun?", "tokens": ["D\u00f6rft", "es", "dich", "auch", "ein\u00b7mahl", "reun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "D\u00f6rft auch mein versto\u00dfnes Kr\u00e4ncken", "tokens": ["D\u00f6rft", "auch", "mein", "ver\u00b7sto\u00df\u00b7nes", "Kr\u00e4n\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deines Ehstands H\u00f6lle seyn?", "tokens": ["Dei\u00b7nes", "Eh\u00b7stands", "H\u00f6l\u00b7le", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sieh, die Tropfen an den Bircken", "tokens": ["Sieh", ",", "die", "Trop\u00b7fen", "an", "den", "Bir\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Thun dir selbst ihr Mitleid kund;", "tokens": ["Thun", "dir", "selbst", "ihr", "Mit\u00b7leid", "kund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil verliebte Thr\u00e4nen w\u00fcrcken,", "tokens": ["Weil", "ver\u00b7lieb\u00b7te", "Thr\u00e4\u00b7nen", "w\u00fcr\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weinen sie um unsern Bund.", "tokens": ["Wei\u00b7nen", "sie", "um", "un\u00b7sern", "Bund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Diese z\u00e4hrenvolle Rinden", "tokens": ["Die\u00b7se", "z\u00e4h\u00b7ren\u00b7vol\u00b7le", "Rin\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rizt die Unschuld und mein Flehn,", "tokens": ["Rizt", "die", "Un\u00b7schuld", "und", "mein", "Flehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn sie haben dem Verbinden", "tokens": ["Denn", "sie", "ha\u00b7ben", "dem", "Ver\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der Trennung zugesehn.", "tokens": ["Und", "der", "Tren\u00b7nung", "zu\u00b7ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dieses r\u00fchrt die todten B\u00e4ume,", "tokens": ["Die\u00b7ses", "r\u00fchrt", "die", "tod\u00b7ten", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich, mein Kind, ach, r\u00fchrt es nicht;", "tokens": ["Dich", ",", "mein", "Kind", ",", "ach", ",", "r\u00fchrt", "es", "nicht", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "ITJ", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber da\u00df ich mich noch s\u00e4ume,", "tokens": ["A\u00b7ber", "da\u00df", "ich", "mich", "noch", "s\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da dein Scheiden gar nichts spricht,", "tokens": ["Da", "dein", "Schei\u00b7den", "gar", "nichts", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "G\u00f6nnt mir doch, ihr holden Lippen,", "tokens": ["G\u00f6nnt", "mir", "doch", ",", "ihr", "hol\u00b7den", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine kurze gute Nacht,", "tokens": ["Ei\u00b7ne", "kur\u00b7ze", "gu\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eh der Traum an solchen Klippen", "tokens": ["Eh", "der", "Traum", "an", "sol\u00b7chen", "Klip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mein Gem\u00fcthe scheitern macht.", "tokens": ["Mein", "Ge\u00b7m\u00fc\u00b7the", "schei\u00b7tern", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Gute Nacht, ihr liebsten Armen!", "tokens": ["Gu\u00b7te", "Nacht", ",", "ihr", "liebs\u00b7ten", "Ar\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Glieder M\u00fcdigkeit", "tokens": ["Mei\u00b7ner", "Glie\u00b7der", "M\u00fc\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird nicht mehr in euch erwarmen;", "tokens": ["Wird", "nicht", "mehr", "in", "euch", "er\u00b7war\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, wie qu\u00e4lt die alte Zeit!", "tokens": ["Ach", ",", "wie", "qu\u00e4lt", "die", "al\u00b7te", "Zeit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gute Nacht, ihr sch\u00f6nsten Br\u00fcste,", "tokens": ["Gu\u00b7te", "Nacht", ",", "ihr", "sch\u00f6ns\u00b7ten", "Br\u00fcs\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Macht nun andre H\u00e4nde voll;", "tokens": ["Macht", "nun", "and\u00b7re", "H\u00e4n\u00b7de", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jezo geh ich in die W\u00fcste,", "tokens": ["Je\u00b7zo", "geh", "ich", "in", "die", "W\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo mein Elend schlafen soll.", "tokens": ["Wo", "mein", "E\u00b7lend", "schla\u00b7fen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "In den W\u00e4ldern will ich irren,", "tokens": ["In", "den", "W\u00e4l\u00b7dern", "will", "ich", "ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor den Menschen will ich fliehn,", "tokens": ["Vor", "den", "Men\u00b7schen", "will", "ich", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit verwaisten Tauben girren,", "tokens": ["Mit", "ver\u00b7wais\u00b7ten", "Tau\u00b7ben", "gir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit verscheuchtem Wilde ziehn,", "tokens": ["Mit", "ver\u00b7scheuch\u00b7tem", "Wil\u00b7de", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis der Gram mein Leben raube,", "tokens": ["Bis", "der", "Gram", "mein", "Le\u00b7ben", "rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis die Kr\u00e4fte sich verschreyn,", "tokens": ["Bis", "die", "Kr\u00e4f\u00b7te", "sich", "ver\u00b7schreyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und da soll ein Grab voll Laube", "tokens": ["Und", "da", "soll", "ein", "Grab", "voll", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "ART", "NN", "ADJD", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Milder als dein Herze seyn.", "tokens": ["Mil\u00b7der", "als", "dein", "Her\u00b7ze", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPOSAT", "VVFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Kan ich dich an Treu besch\u00e4men,", "tokens": ["Kan", "ich", "dich", "an", "Treu", "be\u00b7sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich noch dein Conterfey", "tokens": ["Will", "ich", "noch", "dein", "Con\u00b7ter\u00b7fey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Tod ans Herze nehmen,", "tokens": ["In", "dem", "Tod", "ans", "Her\u00b7ze", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er recht beweglich sey;", "tokens": ["Da\u00df", "er", "recht", "be\u00b7weg\u00b7lich", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieht es niemand von den Leuten,", "tokens": ["Sieht", "es", "nie\u00b7mand", "von", "den", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sieht es doch der Himmel an,", "tokens": ["Sieht", "es", "doch", "der", "Him\u00b7mel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der dich bey gelegnen Zeiten", "tokens": ["Der", "dich", "bey", "ge\u00b7leg\u00b7nen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wohl damit noch strafen kan.", "tokens": ["Wohl", "da\u00b7mit", "noch", "stra\u00b7fen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wirstu einmahl durch die Str\u00e4uche", "tokens": ["Wirs\u00b7tu", "ein\u00b7mahl", "durch", "die", "Str\u00e4u\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halb verirrt spazieren gehn,", "tokens": ["Halb", "ver\u00b7irrt", "spa\u00b7zie\u00b7ren", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ey, so bleib bey meiner Leiche", "tokens": ["Ey", ",", "so", "bleib", "bey", "mei\u00b7ner", "Lei\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur mit andern Augen stehn;", "tokens": ["Nur", "mit", "an\u00b7dern", "Au\u00b7gen", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zeige sie dem neuen Schaze,", "tokens": ["Zei\u00b7ge", "sie", "dem", "neu\u00b7en", "Scha\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der dir das Geleite giebt,", "tokens": ["Der", "dir", "das", "Ge\u00b7lei\u00b7te", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und vermeld ihm auf dem Plaze:", "tokens": ["Und", "ver\u00b7meld", "ihm", "auf", "dem", "Pla\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dieser hat mich auch geliebt.", "tokens": ["Die\u00b7ser", "hat", "mich", "auch", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ach, wo bleibt ihr theuren Schw\u00fcre?", "tokens": ["Ach", ",", "wo", "bleibt", "ihr", "theu\u00b7ren", "Schw\u00fc\u00b7re", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, wo ist dein treuer Sinn,", "tokens": ["Ach", ",", "wo", "ist", "dein", "treu\u00b7er", "Sinn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den ich schmerzlicher verliere,", "tokens": ["Den", "ich", "schmerz\u00b7li\u00b7cher", "ver\u00b7lie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ich selbst gebohren bin?", "tokens": ["Als", "ich", "selbst", "ge\u00b7boh\u00b7ren", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimm das lezte Sehnsuchtszeichen;", "tokens": ["Nimm", "das", "lez\u00b7te", "Sehn\u00b7suchts\u00b7zei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nun, mein Kind, besinne dich!", "tokens": ["Nun", ",", "mein", "Kind", ",", "be\u00b7sin\u00b7ne", "dich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dieses kan dich nicht erweichen,", "tokens": ["Die\u00b7ses", "kan", "dich", "nicht", "er\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "wor