{"dta.poem.4110": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Lauberh\u00fctte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In deinem gr\u00fcnen Zelt!", "tokens": ["In", "dei\u00b7nem", "gr\u00fc\u00b7nen", "Zelt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hier seh' ich erst dich bl\u00fchen,", "tokens": ["Hier", "seh'", "ich", "erst", "dich", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier bl\u00fchet deine Welt.", "tokens": ["Hier", "bl\u00fc\u00b7het", "dei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mir ist's, als ob ich tr\u00e4te", "tokens": ["Mir", "ist's", ",", "als", "ob", "ich", "tr\u00e4\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In ein gelobtes Land,", "tokens": ["In", "ein", "ge\u00b7lob\u00b7tes", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ob der Lauf der Zeiten", "tokens": ["Als", "ob", "der", "Lauf", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich habe umgewandt.", "tokens": ["Sich", "ha\u00b7be", "um\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Entlaubt sind unsre B\u00e4ume,", "tokens": ["Ent\u00b7laubt", "sind", "uns\u00b7re", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verbl\u00fcht ist unser Feld:", "tokens": ["Ver\u00b7bl\u00fcht", "ist", "un\u00b7ser", "Feld", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier seh' ich Lenz und Sommer", "tokens": ["Hier", "seh'", "ich", "Lenz", "und", "Som\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als Br\u00fcder froh gesellt.", "tokens": ["Als", "Br\u00fc\u00b7der", "froh", "ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Herbst will auch nicht fehlen", "tokens": ["Der", "Herbst", "will", "auch", "nicht", "feh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In diesem sch\u00f6nen Haus,", "tokens": ["In", "die\u00b7sem", "sch\u00f6\u00b7nen", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sucht f\u00fcr seine Fr\u00fcchte", "tokens": ["Und", "sucht", "f\u00fcr", "sei\u00b7ne", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich Blumenzweiglein aus.", "tokens": ["Sich", "Blu\u00b7men\u00b7zwei\u00b7glein", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So pr\u00fcfen Duft und Schimmer", "tokens": ["So", "pr\u00fc\u00b7fen", "Duft", "und", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wetteifernd ihre Macht:", "tokens": ["Wett\u00b7ei\u00b7fernd", "ih\u00b7re", "Macht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es flammen hohe Kerzen", "tokens": ["Es", "flam\u00b7men", "ho\u00b7he", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Sterne durch die Nacht.", "tokens": ["Wie", "Ster\u00b7ne", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und aus den blanken Becken", "tokens": ["Und", "aus", "den", "blan\u00b7ken", "Be\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Steigt Weihrauch stolz empor:", "tokens": ["Steigt", "Weih\u00b7rauch", "stolz", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da trauert manche Rose,", "tokens": ["Da", "trau\u00b7ert", "man\u00b7che", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil sie den Rang verlor.", "tokens": ["Weil", "sie", "den", "Rang", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du siehst mich an, Geliebte,", "tokens": ["Du", "siehst", "mich", "an", ",", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und mir versagt das Wort:", "tokens": ["Und", "mir", "ver\u00b7sagt", "das", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du wirst mich nicht verstehen", "tokens": ["Du", "wirst", "mich", "nicht", "ver\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An diesem Zauberort.", "tokens": ["An", "die\u00b7sem", "Zau\u00b7ber\u00b7ort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wie, solltest du mir folgen", "tokens": ["Wie", ",", "soll\u00b7test", "du", "mir", "fol\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In tr\u00fcbe, kalte Luft,", "tokens": ["In", "tr\u00fc\u00b7be", ",", "kal\u00b7te", "Luft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus deinem Vaterlande", "tokens": ["Aus", "dei\u00b7nem", "Va\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Voll Gluth und Glanz und Duft?", "tokens": ["Voll", "Gluth", "und", "Glanz", "und", "Duft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}