{"textgrid.poem.60469": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie ward der Tod ein andrer,", "tokens": ["O", "wie", "ward", "der", "Tod", "ein", "an\u00b7drer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "ART", "NN", "ART", "ADJA", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Als die Griechen ihn geschildert!", "tokens": ["Als", "die", "Grie\u00b7chen", "ihn", "ge\u00b7schil\u00b7dert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem milden G\u00f6tterboten", "tokens": ["Aus", "dem", "mil\u00b7den", "G\u00f6t\u00b7ter\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist zum Schreckbild er verwildert.", "tokens": ["Ist", "zum", "Schreck\u00b7bild", "er", "ver\u00b7wil\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als ein Genius, der die Reise", "tokens": ["Als", "ein", "Ge\u00b7nius", ",", "der", "die", "Rei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sterblichen verk\u00fcnden soll,", "tokens": ["Sterb\u00b7li\u00b7chen", "ver\u00b7k\u00fcn\u00b7den", "soll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Hand zur Wange haltend", "tokens": ["Sei\u00b7ne", "Hand", "zur", "Wan\u00b7ge", "hal\u00b7tend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stand der Tod gedankenvoll;", "tokens": ["Stand", "der", "Tod", "ge\u00b7dan\u00b7ken\u00b7voll", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Oder zeigte, mildsymbolisch,", "tokens": ["O\u00b7der", "zeig\u00b7te", ",", "mild\u00b7sym\u00b7bo\u00b7lisch", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df die Erdenlust zu Ende,", "tokens": ["Da\u00df", "die", "Er\u00b7den\u00b7lust", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00f6schend die gest\u00fcrzte Fackel,", "tokens": ["L\u00f6\u00b7schend", "die", "ge\u00b7st\u00fcrz\u00b7te", "Fa\u00b7ckel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kreuzt' er dr\u00fcber seine H\u00e4nde.", "tokens": ["Kreuzt'", "er", "dr\u00fc\u00b7ber", "sei\u00b7ne", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Leise trat sein Fu\u00df die Psyche;", "tokens": ["Lei\u00b7se", "trat", "sein", "Fu\u00df", "die", "Psy\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie der Freund dem Freund ein Zeichen", "tokens": ["Wie", "der", "Freund", "dem", "Freund", "ein", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leise gibt, vom Festgelage", "tokens": ["Lei\u00b7se", "gibt", ",", "vom", "Fest\u00b7ge\u00b7la\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne St\u00f6rung fortzuschleichen.", "tokens": ["Oh\u00b7ne", "St\u00f6\u00b7rung", "fort\u00b7zu\u00b7schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schlaf und Tod als Zwillingsbr\u00fcder", "tokens": ["Schlaf", "und", "Tod", "als", "Zwil\u00b7lings\u00b7br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Standen oft auf ", "tokens": ["Stan\u00b7den", "oft", "auf"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "APPR"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Beiden, ach, so weit Verschiednen", "tokens": ["Bei\u00b7den", ",", "ach", ",", "so", "weit", "Ver\u00b7schied\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PIS", "$,", "ITJ", "$,", "ADV", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleiche Bildung gab die Milde.", "tokens": ["Glei\u00b7che", "Bil\u00b7dung", "gab", "die", "Mil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Zweifelhaft erschien der Genius,", "tokens": ["Zwei\u00b7fel\u00b7haft", "er\u00b7schien", "der", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Fragen sollte der Beschauer:", "tokens": ["Fra\u00b7gen", "soll\u00b7te", "der", "Be\u00b7schau\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ists der Schlaf und die Erholung?", "tokens": ["Ists", "der", "Schlaf", "und", "die", "Er\u00b7ho\u00b7lung", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ists das Sterben und die Trauer?", "tokens": ["Ists", "das", "Ster\u00b7ben", "und", "die", "Trau\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nur zuweilen ward gesondert,", "tokens": ["Nur", "zu\u00b7wei\u00b7len", "ward", "ge\u00b7son\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das herbre Bildnis trug,", "tokens": ["Und", "das", "herb\u00b7re", "Bild\u00b7nis", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der Blick den Tod erkenne,", "tokens": ["Da\u00df", "der", "Blick", "den", "Tod", "er\u00b7ken\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Falter, Kranz und Aschenkrug.", "tokens": ["Fal\u00b7ter", ",", "Kranz", "und", "A\u00b7schen\u00b7krug", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Dort den Charos sieht der Grieche", "tokens": ["Dort", "den", "Cha\u00b7ros", "sieht", "der", "Grie\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch in sp\u00e4ten, rauhern Zeiten", "tokens": ["Noch", "in", "sp\u00e4\u00b7ten", ",", "rau\u00b7hern", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der dunkeln Schar der Seinen", "tokens": ["Mit", "der", "dun\u00b7keln", "Schar", "der", "Sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber das Gebirge reiten;", "tokens": ["\u00dc\u00b7ber", "das", "Ge\u00b7bir\u00b7ge", "rei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ihm voraus die Jungen wandern,", "tokens": ["Ihm", "vo\u00b7raus", "die", "Jun\u00b7gen", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Alte kommen nachgeschlichen;", "tokens": ["Al\u00b7te", "kom\u00b7men", "nach\u00b7ge\u00b7schli\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und gereiht am Sattel sitzen", "tokens": ["Und", "ge\u00b7reiht", "am", "Sat\u00b7tel", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zarte Kinder, fr\u00fchverblichen. \u2013", "tokens": ["Zar\u00b7te", "Kin\u00b7der", ",", "fr\u00fch\u00b7ver\u00b7bli\u00b7chen", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Heiter kam er noch als Fiedler,", "tokens": ["Hei\u00b7ter", "kam", "er", "noch", "als", "Fied\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sein Gesinde trat den Reigen,", "tokens": ["Sein", "Ge\u00b7sin\u00b7de", "trat", "den", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zu Lust und Tanz von hinnen", "tokens": ["Und", "zu", "Lust", "und", "Tanz", "von", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "APPR", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rief sein Pfeifen, helles Geigen. \u2013 \u2013", "tokens": ["Rief", "sein", "Pfei\u00b7fen", ",", "hel\u00b7les", "Gei\u00b7gen", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ADJA", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Thanatos, ach, ward ein Krieger,", "tokens": ["Tha\u00b7na\u00b7tos", ",", "ach", ",", "ward", "ein", "Krie\u00b7ger", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ITJ", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "---++-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Auf die Opfer Speere schwingend;", "tokens": ["Auf", "die", "Op\u00b7fer", "Spee\u00b7re", "schwin\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein Athlet, auf glattem Boden", "tokens": ["Ein", "Ath\u00b7let", ",", "auf", "glat\u00b7tem", "Bo\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jeden Helden niederringend;", "tokens": ["Je\u00b7den", "Hel\u00b7den", "nie\u00b7der\u00b7rin\u00b7gend", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Thanatos, der edle Genius,", "tokens": ["Tha\u00b7na\u00b7tos", ",", "der", "ed\u00b7le", "Ge\u00b7nius", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Ist zum Sensenmann verbauert,", "tokens": ["Ist", "zum", "Sen\u00b7sen\u00b7mann", "ver\u00b7bau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4ht den Menschen, einen Grashalm,", "tokens": ["M\u00e4ht", "den", "Men\u00b7schen", ",", "ei\u00b7nen", "Gras\u00b7halm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der zur Erde niederschauert.", "tokens": ["Der", "zur", "Er\u00b7de", "nie\u00b7der\u00b7schau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Fischer, mit dem leisen K\u00f6der,", "tokens": ["Fi\u00b7scher", ",", "mit", "dem", "lei\u00b7sen", "K\u00f6\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Angelt er im Meer der Luft;", "tokens": ["An\u00b7gelt", "er", "im", "Meer", "der", "Luft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Legt uns Schlingen als ein Vogler,", "tokens": ["Legt", "uns", "Schlin\u00b7gen", "als", "ein", "Vog\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der mit falschen Stimmen ruft.", "tokens": ["Der", "mit", "fal\u00b7schen", "Stim\u00b7men", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Nur noch feindlich naht der Wilde,", "tokens": ["Nur", "noch", "feind\u00b7lich", "naht", "der", "Wil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drohend, ins Verderben lockend,", "tokens": ["Dro\u00b7hend", ",", "ins", "Ver\u00b7der\u00b7ben", "lo\u00b7ckend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch dem Menschen wie ein Kobold,", "tokens": ["Auch", "dem", "Men\u00b7schen", "wie", "ein", "Ko\u00b7bold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Irrwisch auf dem Halse hockend.", "tokens": ["Irr\u00b7wisch", "auf", "dem", "Hal\u00b7se", "ho\u00b7ckend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Gr\u00e4\u00dflich naht uns mit der Sense,", "tokens": ["Gr\u00e4\u00df\u00b7lich", "naht", "uns", "mit", "der", "Sen\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreck- und Vorbild, das Gerippe;", "tokens": ["Schreck", "und", "Vor\u00b7bild", ",", "das", "Ge\u00b7rip\u00b7pe", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr ein mildes L\u00e4cheln hat es", "tokens": ["F\u00fcr", "ein", "mil\u00b7des", "L\u00e4\u00b7cheln", "hat", "es"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine Wange, keine Lippe. \u2013", "tokens": ["Kei\u00b7ne", "Wan\u00b7ge", ",", "kei\u00b7ne", "Lip\u00b7pe", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "So in wechselnden Gestalten", "tokens": ["So", "in", "wech\u00b7seln\u00b7den", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht der Tod die Erdenrunde;", "tokens": ["Macht", "der", "Tod", "die", "Er\u00b7den\u00b7run\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heute aber geht im Heere", "tokens": ["Heu\u00b7te", "a\u00b7ber", "geht", "im", "Hee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sigismunds die Schreckenskunde:", "tokens": ["Si\u00b7gis\u00b7munds", "die", "Schre\u00b7cken\u00b7skun\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "\u00bbweil den Ziska, schlachterm\u00fcdet,", "tokens": ["\u00bb", "weil", "den", "Zis\u00b7ka", ",", "schlach\u00b7ter\u00b7m\u00fc\u00b7det", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "KOUS", "ART", "NE", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leichter Schlummer \u00fcberkommen,", "tokens": ["Leich\u00b7ter", "Schlum\u00b7mer", "\u00fc\u00b7ber\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat der Tod, ihn zu ersetzen,", "tokens": ["Hat", "der", "Tod", ",", "ihn", "zu", "er\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine R\u00fcstung umgenommen;", "tokens": ["Sei\u00b7ne", "R\u00fcs\u00b7tung", "um\u00b7ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Denn unwiderstehlich jeden,", "tokens": ["Denn", "un\u00b7wi\u00b7der\u00b7steh\u00b7lich", "je\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PIAT", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der ihm naht im Schlachtgebraus,", "tokens": ["Der", "ihm", "naht", "im", "Schlacht\u00b7ge\u00b7braus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Winkt der schwarze Helmbusch Ziskas", "tokens": ["Winkt", "der", "schwar\u00b7ze", "Helm\u00b7busch", "Zis\u00b7kas"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NE"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "In die ewge Nacht hinaus.\u00ab", "tokens": ["In", "die", "ew\u00b7ge", "Nacht", "hin\u00b7aus", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "O wie ward der Tod ein andrer,", "tokens": ["O", "wie", "ward", "der", "Tod", "ein", "an\u00b7drer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "ART", "NN", "ART", "ADJA", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Als die Griechen ihn geschildert!", "tokens": ["Als", "die", "Grie\u00b7chen", "ihn", "ge\u00b7schil\u00b7dert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem milden G\u00f6tterboten", "tokens": ["Aus", "dem", "mil\u00b7den", "G\u00f6t\u00b7ter\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist zum Schreckbild er verwildert.", "tokens": ["Ist", "zum", "Schreck\u00b7bild", "er", "ver\u00b7wil\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Als ein Genius, der die Reise", "tokens": ["Als", "ein", "Ge\u00b7nius", ",", "der", "die", "Rei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sterblichen verk\u00fcnden soll,", "tokens": ["Sterb\u00b7li\u00b7chen", "ver\u00b7k\u00fcn\u00b7den", "soll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Hand zur Wange haltend", "tokens": ["Sei\u00b7ne", "Hand", "zur", "Wan\u00b7ge", "hal\u00b7tend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stand der Tod gedankenvoll;", "tokens": ["Stand", "der", "Tod", "ge\u00b7dan\u00b7ken\u00b7voll", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Oder zeigte, mildsymbolisch,", "tokens": ["O\u00b7der", "zeig\u00b7te", ",", "mild\u00b7sym\u00b7bo\u00b7lisch", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df die Erdenlust zu Ende,", "tokens": ["Da\u00df", "die", "Er\u00b7den\u00b7lust", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00f6schend die gest\u00fcrzte Fackel,", "tokens": ["L\u00f6\u00b7schend", "die", "ge\u00b7st\u00fcrz\u00b7te", "Fa\u00b7ckel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kreuzt' er dr\u00fcber seine H\u00e4nde.", "tokens": ["Kreuzt'", "er", "dr\u00fc\u00b7ber", "sei\u00b7ne", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Leise trat sein Fu\u00df die Psyche;", "tokens": ["Lei\u00b7se", "trat", "sein", "Fu\u00df", "die", "Psy\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie der Freund dem Freund ein Zeichen", "tokens": ["Wie", "der", "Freund", "dem", "Freund", "ein", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leise gibt, vom Festgelage", "tokens": ["Lei\u00b7se", "gibt", ",", "vom", "Fest\u00b7ge\u00b7la\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne St\u00f6rung fortzuschleichen.", "tokens": ["Oh\u00b7ne", "St\u00f6\u00b7rung", "fort\u00b7zu\u00b7schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Schlaf und Tod als Zwillingsbr\u00fcder", "tokens": ["Schlaf", "und", "Tod", "als", "Zwil\u00b7lings\u00b7br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Standen oft auf ", "tokens": ["Stan\u00b7den", "oft", "auf"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "APPR"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Beiden, ach, so weit Verschiednen", "tokens": ["Bei\u00b7den", ",", "ach", ",", "so", "weit", "Ver\u00b7schied\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PIS", "$,", "ITJ", "$,", "ADV", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleiche Bildung gab die Milde.", "tokens": ["Glei\u00b7che", "Bil\u00b7dung", "gab", "die", "Mil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "Zweifelhaft erschien der Genius,", "tokens": ["Zwei\u00b7fel\u00b7haft", "er\u00b7schien", "der", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Fragen sollte der Beschauer:", "tokens": ["Fra\u00b7gen", "soll\u00b7te", "der", "Be\u00b7schau\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ists der Schlaf und die Erholung?", "tokens": ["Ists", "der", "Schlaf", "und", "die", "Er\u00b7ho\u00b7lung", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ists das Sterben und die Trauer?", "tokens": ["Ists", "das", "Ster\u00b7ben", "und", "die", "Trau\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "Nur zuweilen ward gesondert,", "tokens": ["Nur", "zu\u00b7wei\u00b7len", "ward", "ge\u00b7son\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das herbre Bildnis trug,", "tokens": ["Und", "das", "herb\u00b7re", "Bild\u00b7nis", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der Blick den Tod erkenne,", "tokens": ["Da\u00df", "der", "Blick", "den", "Tod", "er\u00b7ken\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Falter, Kranz und Aschenkrug.", "tokens": ["Fal\u00b7ter", ",", "Kranz", "und", "A\u00b7schen\u00b7krug", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Dort den Charos sieht der Grieche", "tokens": ["Dort", "den", "Cha\u00b7ros", "sieht", "der", "Grie\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch in sp\u00e4ten, rauhern Zeiten", "tokens": ["Noch", "in", "sp\u00e4\u00b7ten", ",", "rau\u00b7hern", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der dunkeln Schar der Seinen", "tokens": ["Mit", "der", "dun\u00b7keln", "Schar", "der", "Sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber das Gebirge reiten;", "tokens": ["\u00dc\u00b7ber", "das", "Ge\u00b7bir\u00b7ge", "rei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.27": {"line.1": {"text": "Ihm voraus die Jungen wandern,", "tokens": ["Ihm", "vo\u00b7raus", "die", "Jun\u00b7gen", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Alte kommen nachgeschlichen;", "tokens": ["Al\u00b7te", "kom\u00b7men", "nach\u00b7ge\u00b7schli\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und gereiht am Sattel sitzen", "tokens": ["Und", "ge\u00b7reiht", "am", "Sat\u00b7tel", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zarte Kinder, fr\u00fchverblichen. \u2013", "tokens": ["Zar\u00b7te", "Kin\u00b7der", ",", "fr\u00fch\u00b7ver\u00b7bli\u00b7chen", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.28": {"line.1": {"text": "Heiter kam er noch als Fiedler,", "tokens": ["Hei\u00b7ter", "kam", "er", "noch", "als", "Fied\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sein Gesinde trat den Reigen,", "tokens": ["Sein", "Ge\u00b7sin\u00b7de", "trat", "den", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zu Lust und Tanz von hinnen", "tokens": ["Und", "zu", "Lust", "und", "Tanz", "von", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "APPR", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rief sein Pfeifen, helles Geigen. \u2013 \u2013", "tokens": ["Rief", "sein", "Pfei\u00b7fen", ",", "hel\u00b7les", "Gei\u00b7gen", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ADJA", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.29": {"line.1": {"text": "Thanatos, ach, ward ein Krieger,", "tokens": ["Tha\u00b7na\u00b7tos", ",", "ach", ",", "ward", "ein", "Krie\u00b7ger", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ITJ", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "---++-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Auf die Opfer Speere schwingend;", "tokens": ["Auf", "die", "Op\u00b7fer", "Spee\u00b7re", "schwin\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein Athlet, auf glattem Boden", "tokens": ["Ein", "Ath\u00b7let", ",", "auf", "glat\u00b7tem", "Bo\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jeden Helden niederringend;", "tokens": ["Je\u00b7den", "Hel\u00b7den", "nie\u00b7der\u00b7rin\u00b7gend", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.30": {"line.1": {"text": "Thanatos, der edle Genius,", "tokens": ["Tha\u00b7na\u00b7tos", ",", "der", "ed\u00b7le", "Ge\u00b7nius", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "mea