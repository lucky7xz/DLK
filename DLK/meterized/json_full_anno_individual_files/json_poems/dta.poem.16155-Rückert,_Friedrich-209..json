{"dta.poem.16155": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "209.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn einer hat genug, soll er nach mehr nicht streben;", "tokens": ["Wenn", "ei\u00b7ner", "hat", "ge\u00b7nug", ",", "soll", "er", "nach", "mehr", "nicht", "stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "ADV", "$,", "VMFIN", "PPER", "APPR", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Allein das schwere ist genug zu haben eben.", "tokens": ["Al\u00b7lein", "das", "schwe\u00b7re", "ist", "ge\u00b7nug", "zu", "ha\u00b7ben", "e\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VAFIN", "ADV", "PTKZU", "VAINF", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nie hat genug ein Mann an dem was er gewann,", "tokens": ["Nie", "hat", "ge\u00b7nug", "ein", "Mann", "an", "dem", "was", "er", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "APPR", "ART", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lang er denkt, da\u00df er noch mehr gewinnen kann.", "tokens": ["So", "lang", "er", "denkt", ",", "da\u00df", "er", "noch", "mehr", "ge\u00b7win\u00b7nen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Kaum die Betrachtung hemmt sein th\u00f6richtes Beginnen,", "tokens": ["Kaum", "die", "Be\u00b7trach\u00b7tung", "hemmt", "sein", "th\u00f6\u00b7rich\u00b7tes", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df, wer viel hat, mehr kann verlieren als gewinnen.", "tokens": ["Da\u00df", ",", "wer", "viel", "hat", ",", "mehr", "kann", "ver\u00b7lie\u00b7ren", "als", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "ADV", "VAFIN", "$,", "ADV", "VMFIN", "VVINF", "KOKOM", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}