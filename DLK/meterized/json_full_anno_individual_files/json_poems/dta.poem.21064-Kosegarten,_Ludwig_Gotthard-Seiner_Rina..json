{"dta.poem.21064": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Seiner Rina.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Rina, reiche mir die Rechte!               ", "tokens": ["Ri\u00b7na", ",", "rei\u00b7che", "mir", "die", "Rech\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leite durch des Lebens N\u00e4chte", "tokens": ["Lei\u00b7te", "durch", "des", "Le\u00b7bens", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich ins helle Vaterland.", "tokens": ["Mich", "ins", "hel\u00b7le", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch der Erde Distelpfade", "tokens": ["Durch", "der", "Er\u00b7de", "Dis\u00b7tel\u00b7pfa\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Leite bis ans Grabgestade,", "tokens": ["Lei\u00b7te", "bis", "ans", "Grab\u00b7ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freundin, mich an sanfter Hand.", "tokens": ["Freun\u00b7din", ",", "mich", "an", "sanf\u00b7ter", "Hand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch nicht lauter n\u00e4chtlich Grauen", "tokens": ["Doch", "nicht", "lau\u00b7ter", "n\u00e4cht\u00b7lich", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADJD", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00fcllet unsrer Wallfahrt Auen;", "tokens": ["H\u00fcl\u00b7let", "uns\u00b7rer", "Wall\u00b7fahrt", "Au\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Disteln zeugt nicht jedes Feld.", "tokens": ["Dis\u00b7teln", "zeugt", "nicht", "je\u00b7des", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonnenschein und Regenschauer,", "tokens": ["Son\u00b7nen\u00b7schein", "und", "Re\u00b7gen\u00b7schau\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Licht und Schatten, Freud' und Trauer", "tokens": ["Licht", "und", "Schat\u00b7ten", ",", "Freud'", "und", "Trau\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wechseln stets auf unsrer Welt.", "tokens": ["Wech\u00b7seln", "stets", "auf", "uns\u00b7rer", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Alle Freuden, die uns spriessen,", "tokens": ["Al\u00b7le", "Freu\u00b7den", ",", "die", "uns", "spries\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rina, wollen wir geniessen;", "tokens": ["Ri\u00b7na", ",", "wol\u00b7len", "wir", "ge\u00b7nies\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedes Bl\u00fcmchen, das uns winkt,", "tokens": ["Je\u00b7des", "Bl\u00fcm\u00b7chen", ",", "das", "uns", "winkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jede K\u00fchlung, die uns f\u00e4chelt,", "tokens": ["Je\u00b7de", "K\u00fch\u00b7lung", ",", "die", "uns", "f\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jede Tr\u00f6stung, die uns l\u00e4chelt,", "tokens": ["Je\u00b7de", "Tr\u00f6s\u00b7tung", ",", "die", "uns", "l\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jeden Schimmer, der uns blinkt.", "tokens": ["Je\u00b7den", "Schim\u00b7mer", ",", "der", "uns", "blinkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit des Tages s\u00fcsser Fr\u00fche", "tokens": ["Mit", "des", "Ta\u00b7ges", "s\u00fcs\u00b7ser", "Fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wollen wir zu eurer M\u00fche", "tokens": ["Wol\u00b7len", "wir", "zu", "eu\u00b7rer", "M\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Muthig und getrost erstehn.", "tokens": ["Mut\u00b7hig", "und", "ge\u00b7trost", "er\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit des Abends linder K\u00fchle,", "tokens": ["Mit", "des", "A\u00b7bends", "lin\u00b7der", "K\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An dem rasch erstrittnen Ziele,", "tokens": ["An", "dem", "rasch", "er\u00b7stritt\u00b7nen", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll uns holde Ruh' umwehn.", "tokens": ["Soll", "uns", "hol\u00b7de", "Ruh'", "um\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ferner St\u00fcrme dumpfes Grollen,", "tokens": ["Fer\u00b7ner", "St\u00fcr\u00b7me", "dum\u00b7pfes", "Grol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aufgew\u00fchlter Wogen Grollen", "tokens": ["Auf\u00b7ge\u00b7w\u00fchl\u00b7ter", "Wo\u00b7gen", "Grol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "D\u00e4mpfe, Freundin, deine Huld.", "tokens": ["D\u00e4mp\u00b7fe", ",", "Freun\u00b7din", ",", "dei\u00b7ne", "Huld", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Reste meiner Flammenjugend", "tokens": ["Res\u00b7te", "mei\u00b7ner", "Flam\u00b7men\u00b7ju\u00b7gend"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Z\u00e4hme deine stille Tugend,", "tokens": ["Z\u00e4h\u00b7me", "dei\u00b7ne", "stil\u00b7le", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine siegende Geduld.", "tokens": ["Dei\u00b7ne", "sie\u00b7gen\u00b7de", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Sorgsam will ich meine Freuden", "tokens": ["Sorg\u00b7sam", "will", "ich", "mei\u00b7ne", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dir theilen, alle Leiden", "tokens": ["Mit", "dir", "thei\u00b7len", ",", "al\u00b7le", "Lei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "VVINF", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Z\u00e4rtlich tragen, die dir nahn.", "tokens": ["Z\u00e4rt\u00b7lich", "tra\u00b7gen", ",", "die", "dir", "nahn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "PRELS", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4nnlich will ich dich beschirmen,", "tokens": ["M\u00e4nn\u00b7lich", "will", "ich", "dich", "be\u00b7schir\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Will in Ungest\u00fcm und St\u00fcrmen", "tokens": ["Will", "in", "Un\u00b7ge\u00b7st\u00fcm", "und", "St\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dich mit festem Arm umfahn.", "tokens": ["Dich", "mit", "fes\u00b7tem", "Arm", "um\u00b7fahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wann erdonnern alle Festen,", "tokens": ["Wann", "er\u00b7don\u00b7nern", "al\u00b7le", "Fes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann erblitzen Ost und Westen,", "tokens": ["Wann", "er\u00b7blit\u00b7zen", "Ost", "und", "Wes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Halte fest dich, fest an mich!", "tokens": ["Hal\u00b7te", "fest", "dich", ",", "fest", "an", "mich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Pole schwanken, Achsen splittern,", "tokens": ["Po\u00b7le", "schwan\u00b7ken", ",", "Ach\u00b7sen", "split\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Erden taumeln, Himmel sch\u00fcttern \u2014", "tokens": ["Er\u00b7den", "tau\u00b7meln", ",", "Him\u00b7mel", "sch\u00fct\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine Rechte st\u00fctzet dich;", "tokens": ["Mei\u00b7ne", "Rech\u00b7te", "st\u00fct\u00b7zet", "dich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Gottes Rechte meine Rechte!", "tokens": ["Got\u00b7tes", "Rech\u00b7te", "mei\u00b7ne", "Rech\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unbesiegte, ungeschw\u00e4chte,", "tokens": ["Un\u00b7be\u00b7sieg\u00b7te", ",", "un\u00b7ge\u00b7schw\u00e4ch\u00b7te", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hehre Kraft, Religion!", "tokens": ["Heh\u00b7re", "Kraft", ",", "Re\u00b7li\u00b7gi\u00b7on", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$."], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Weihe, Himmelsbothin, weihe", "tokens": ["Wei\u00b7he", ",", "Him\u00b7mels\u00b7bo\u00b7thin", ",", "wei\u00b7he"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mich und meine Vielgetreue", "tokens": ["Mich", "und", "mei\u00b7ne", "Viel\u00b7ge\u00b7treu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir zur Tochter, dir zum Sohn!", "tokens": ["Dir", "zur", "Toch\u00b7ter", ",", "dir", "zum", "Sohn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Mit der schirmenden Egiden", "tokens": ["Mit", "der", "schir\u00b7men\u00b7den", "E\u00b7gi\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sicher' unsrer Tage Frieden,", "tokens": ["Si\u00b7cher'", "uns\u00b7rer", "Ta\u00b7ge", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4uter' unsre Leidenschaft,", "tokens": ["L\u00e4u\u00b7ter'", "uns\u00b7re", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schatt' uns in der Mittagsschw\u00fcle,", "tokens": ["Schatt'", "uns", "in", "der", "Mit\u00b7tags\u00b7schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00e4rm' uns in der Winterk\u00fchle,", "tokens": ["W\u00e4rm'", "uns", "in", "der", "Win\u00b7ter\u00b7k\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fl\u00fcgle uns zu That und Kraft!", "tokens": ["Fl\u00fcg\u00b7le", "uns", "zu", "That", "und", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "In des Lebens furchtbarn Engen,", "tokens": ["In", "des", "Le\u00b7bens", "furcht\u00b7barn", "En\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn uns Welt und Schicksal dr\u00e4ngen,", "tokens": ["Wenn", "uns", "Welt", "und", "Schick\u00b7sal", "dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwinge deinen weissen Stab.", "tokens": ["Schwin\u00b7ge", "dei\u00b7nen", "weis\u00b7sen", "Stab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeig' uns \u00fcber Sonn' und Sternen", "tokens": ["Zeig'", "uns", "\u00fc\u00b7ber", "Sonn'", "und", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jene lichten, weiten Fernen,", "tokens": ["Je\u00b7ne", "lich\u00b7ten", ",", "wei\u00b7ten", "Fer\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+