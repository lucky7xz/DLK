{"textgrid.poem.24625": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Tugend-Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sch\u00f6nheit ist ein Feind der frey und reinen Brust/", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "ist", "ein", "Feind", "der", "frey", "und", "rei\u00b7nen", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "ADJD", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Liebe/ die sie pflantzt/ ein Feind der Ruh und Lust.", "tokens": ["Die", "Lie\u00b7be", "/", "die", "sie", "pflantzt", "/", "ein", "Feind", "der", "Ruh", "und", "Lust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$(", "ART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch diese Feinde kan die Kranckheit \u00fcberwinden/", "tokens": ["Doch", "die\u00b7se", "Fein\u00b7de", "kan", "die", "Kran\u00b7ck\u00b7heit", "\u00fc\u00b7berw\u00b7in\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Die Tugend aber nicht/ in der wir Ruhe finden.", "tokens": ["Die", "Tu\u00b7gend", "a\u00b7ber", "nicht", "/", "in", "der", "wir", "Ru\u00b7he", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "$(", "APPR", "PRELS", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Drum liebt ein edler Geist zwar einen sch\u00f6nen Leib/", "tokens": ["Drum", "liebt", "ein", "ed\u00b7ler", "Geist", "zwar", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Leib", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jedoch entflammt ihn mehr ein Engel-gleiches Weib/", "tokens": ["Je\u00b7doch", "ent\u00b7flammt", "ihn", "mehr", "ein", "En\u00b7gel\u00b7glei\u00b7ches", "Weib", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und will in der Natur er die Natur besprechen/", "tokens": ["Und", "will", "in", "der", "Na\u00b7tur", "er", "die", "Na\u00b7tur", "be\u00b7spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ART", "NN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mu\u00df ihm der Tugend-Hand der Sch\u00f6nheit Rosen brechen.", "tokens": ["Mu\u00df", "ihm", "der", "Tu\u00b7gen\u00b7dHand", "der", "Sch\u00f6n\u00b7heit", "Ro\u00b7sen", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Himmel schlichtete den sonderbahren Streit/", "tokens": ["Der", "Him\u00b7mel", "schlich\u00b7te\u00b7te", "den", "son\u00b7der\u00b7bah\u00b7ren", "Streit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sprach: der Tugend nur bleibt die Vortrefflichkeit.", "tokens": ["Und", "sprach", ":", "der", "Tu\u00b7gend", "nur", "bleibt", "die", "Vor\u00b7treff\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch zancket nicht darum/ vereinigt euch zusammen:", "tokens": ["Doch", "zan\u00b7cket", "nicht", "da\u00b7rum", "/", "ver\u00b7ei\u00b7nigt", "euch", "zu\u00b7sam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PAV", "$(", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Menschen-Liebe mu\u00df durch euch vollkommen flammen.", "tokens": ["Der", "Men\u00b7schen\u00b7Lie\u00b7be", "mu\u00df", "durch", "euch", "voll\u00b7kom\u00b7men", "flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er rief: was ich gesagt/ sey itzo Sonnen-klar/", "tokens": ["Er", "rief", ":", "was", "ich", "ge\u00b7sagt", "/", "sey", "it\u00b7zo", "Son\u00b7nen\u00b7klar", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "PPER", "VVPP", "$(", "VAFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und suchte gleich darauf ein ihm beliebtes Paar.", "tokens": ["Und", "such\u00b7te", "gleich", "da\u00b7rauf", "ein", "ihm", "be\u00b7lieb\u00b7tes", "Paar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "ART", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem gab er sie/ und sprach: den s\u00fc\u00dfen Brand zu heilen/", "tokens": ["Dem", "gab", "er", "sie", "/", "und", "sprach", ":", "den", "s\u00fc\u00b7\u00dfen", "Brand", "zu", "hei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "$(", "KON", "VVFIN", "$.", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Solt ihr euch beyde nun in diese viere theilen.", "tokens": ["Solt", "ihr", "euch", "bey\u00b7de", "nun", "in", "die\u00b7se", "vie\u00b7re", "thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PIS", "ADV", "APPR", "PDAT", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich nehme sprach die Braut/ die Tugend in mein Hertz:", "tokens": ["Ich", "neh\u00b7me", "sprach", "die", "Braut", "/", "die", "Tu\u00b7gend", "in", "mein", "Hertz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "ART", "NN", "$(", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie ist der Seelen-Schatz/ der Engel Lust und Schertz;", "tokens": ["Sie", "ist", "der", "See\u00b7len\u00b7Schatz", "/", "der", "En\u00b7gel", "Lust", "und", "Schertz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Sch\u00f6nheit an den Leib. Wer will die Liebe haben?", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "an", "den", "Leib", ".", "Wer", "will", "die", "Lie\u00b7be", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "PWS", "VMFIN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die beyden ersten sind bey mir des Himmels-Gaben.", "tokens": ["Die", "bey\u00b7den", "ers\u00b7ten", "sind", "bey", "mir", "des", "Him\u00b7mels\u00b7Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "VAFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "So kom mein Edler Wolff/ nim beydes Lieben an.", "tokens": ["So", "kom", "mein", "Ed\u00b7ler", "Wolff", "/", "nim", "bey\u00b7des", "Lie\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NE", "$(", "ADV", "PIAT", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich find an deinem Thun/ was mich entz\u00fccken kan:", "tokens": ["Ich", "find", "an", "dei\u00b7nem", "Thun", "/", "was", "mich", "ent\u00b7z\u00fc\u00b7cken", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gelehrsamkeit/ Verstand/ die Anmuht in Geberden:", "tokens": ["Ge\u00b7lehr\u00b7sam\u00b7keit", "/", "Ver\u00b7stand", "/", "die", "An\u00b7muht", "in", "Ge\u00b7ber\u00b7den", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wo durch ich sch\u00e4tzbar kan vor vielen Weibern werden.", "tokens": ["Wo", "durch", "ich", "sch\u00e4tz\u00b7bar", "kan", "vor", "vie\u00b7len", "Wei\u00b7bern", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "ADJD", "VMFIN", "APPR", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die erste Liebe soll nach meiner Tugend gehn;", "tokens": ["Die", "ers\u00b7te", "Lie\u00b7be", "soll", "nach", "mei\u00b7ner", "Tu\u00b7gend", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du wirst dem Gl\u00fcck dadurch un\u00fcberwindlich sehn.", "tokens": ["Du", "wirst", "dem", "Gl\u00fcck", "da\u00b7durch", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PAV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Tugend ist ein Schmuck an Frauen hochzusch\u00e4tzen/", "tokens": ["Die", "Tu\u00b7gend", "ist", "ein", "Schmuck", "an", "Frau\u00b7en", "hoch\u00b7zu\u00b7sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Atlas in dem Creutz/ ein Leitstern im Ergetzen.", "tokens": ["Ein", "At\u00b7las", "in", "dem", "Creutz", "/", "ein", "Leits\u00b7tern", "im", "Er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Der Sch\u00f6nheit Liebe nimmt dein Hertz in die Gewalt/", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "Lie\u00b7be", "nimmt", "dein", "Hertz", "in", "die", "Ge\u00b7walt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und giebt der Augen Lust beliebten Aufenthalt.", "tokens": ["Und", "giebt", "der", "Au\u00b7gen", "Lust", "be\u00b7lieb\u00b7ten", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie soll die Dienerin von iener Liebe bleiben:", "tokens": ["Sie", "soll", "die", "Die\u00b7ne\u00b7rin", "von", "ie\u00b7ner", "Lie\u00b7be", "blei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In Tugend die Natur vergn\u00fcgen und vertreiben.", "tokens": ["In", "Tu\u00b7gend", "die", "Na\u00b7tur", "ver\u00b7gn\u00fc\u00b7gen", "und", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Die ungemeine Braut sprach so viel sch\u00f6nes aus.", "tokens": ["Die", "un\u00b7ge\u00b7mei\u00b7ne", "Braut", "sprach", "so", "viel", "sch\u00f6\u00b7nes", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PIAT", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn zog der Br\u00e4utigam in ihrer Tugend-Haus/", "tokens": ["Denn", "zog", "der", "Br\u00e4u\u00b7ti\u00b7gam", "in", "ih\u00b7rer", "Tu\u00b7gen\u00b7dHaus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NE", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und hat der liebe Geist/ den ihre Sch\u00f6nheit r\u00fchret/", "tokens": ["Und", "hat", "der", "lie\u00b7be", "Geist", "/", "den", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In ihre keusche Brust durch Priesters-Hand gef\u00fchret.", "tokens": ["In", "ih\u00b7re", "keu\u00b7sche", "Brust", "durch", "Pries\u00b7ter\u00b7sHand", "ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Drauf kam er Seegens-Mund und Schlo\u00df die kluge That:", "tokens": ["Drauf", "kam", "er", "See\u00b7gens\u00b7Mund", "und", "Schlo\u00df", "die", "klu\u00b7ge", "That", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verliebt/ doch keusches Paar/ das nebst der Tugend hat", "tokens": ["Ver\u00b7liebt", "/", "doch", "keu\u00b7sches", "Paar", "/", "das", "nebst", "der", "Tu\u00b7gend", "hat"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$(", "ADV", "ADJA", "NN", "$(", "ART", "APPR", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In Sch\u00f6nheit sich verliebt/ du wirst in kurtzen schauen/", "tokens": ["In", "Sch\u00f6n\u00b7heit", "sich", "ver\u00b7liebt", "/", "du", "wirst", "in", "kurt\u00b7zen", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVPP", "$(", "PPER", "VAFIN", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie durch sein sch\u00f6n Gesch\u00f6pff Gott will Gesch\u00f6pffe bauen.", "tokens": ["Wie", "durch", "sein", "sch\u00f6n", "Ge\u00b7sch\u00f6pff", "Gott", "will", "Ge\u00b7sch\u00f6pf\u00b7fe", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "ADJD", "NN", "NN", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.i