{"textgrid.poem.39446": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein starker J\u00fcngling, k\u00fchn zur That und schnell,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein starker J\u00fcngling, k\u00fchn zur That und schnell,", "tokens": ["Ein", "star\u00b7ker", "J\u00fcng\u00b7ling", ",", "k\u00fchn", "zur", "That", "und", "schnell", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "APPRART", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Entrei\u00dft Johannes sich bewohnten St\u00e4tten.", "tokens": ["Ent\u00b7rei\u00dft", "Jo\u00b7han\u00b7nes", "sich", "be\u00b7wohn\u00b7ten", "St\u00e4t\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PRF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er liebt, in \u00f6de Kl\u00fcfte sich zu betten,", "tokens": ["Er", "liebt", ",", "in", "\u00f6\u00b7de", "Kl\u00fcf\u00b7te", "sich", "zu", "bet\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die H\u00fcften g\u00fcrtet ihm ein rauhes Fell.", "tokens": ["Die", "H\u00fcf\u00b7ten", "g\u00fcr\u00b7tet", "ihm", "ein", "rau\u00b7hes", "Fell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Einf\u00e4ltig wird sein Sinn, sein Auge hell;", "tokens": ["Ein\u00b7f\u00e4l\u00b7tig", "wird", "sein", "Sinn", ",", "sein", "Au\u00b7ge", "hell", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nichts Niedres kann ihn an die Erde ketten,", "tokens": ["Nichts", "Nie\u00b7dres", "kann", "ihn", "an", "die", "Er\u00b7de", "ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sein Geschlecht vom Untergang zu retten,", "tokens": ["Und", "sein", "Ge\u00b7schlecht", "vom", "Un\u00b7ter\u00b7gang", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sucht er in sich der Gottheit Lebensquell.", "tokens": ["Sucht", "er", "in", "sich", "der", "Got\u00b7theit", "Le\u00b7bens\u00b7quell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er sitzt am Felsen, dessen Born ihn tr\u00e4nket,", "tokens": ["Er", "sitzt", "am", "Fel\u00b7sen", ",", "des\u00b7sen", "Born", "ihn", "tr\u00e4n\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PRELAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da steigt vor seiner Seel' empor ein Bild,", "tokens": ["Da", "steigt", "vor", "sei\u00b7ner", "Seel'", "em\u00b7por", "ein", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das er mit sel'gem Staunen \u00fcberdenket.", "tokens": ["Das", "er", "mit", "sel'\u00b7gem", "Stau\u00b7nen", "\u00fc\u00b7ber\u00b7den\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es ist des Menschen Sohn, so gro\u00df als mild.", "tokens": ["Es", "ist", "des", "Men\u00b7schen", "Sohn", ",", "so", "gro\u00df", "als", "mild", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der ernste Seher h\u00e4lt sein Haupt gesenket:", "tokens": ["Der", "erns\u00b7te", "Se\u00b7her", "h\u00e4lt", "sein", "Haupt", "ge\u00b7sen\u00b7ket", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach, gegen dich, wie bin ich streng' und wild!", "tokens": ["Ach", ",", "ge\u00b7gen", "dich", ",", "wie", "bin", "ich", "streng'", "und", "wild", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "PPER", "$,", "PWAV", "VAFIN", "PPER", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein starker J\u00fcngling, k\u00fchn zur That und schnell,", "tokens": ["Ein", "s