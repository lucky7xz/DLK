{"dta.poem.15050": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von den vortrefflichen Umst\u00e4nden  \n f\u00fcr die Poesie unter den Kaisern aus  \n dem schw\u00e4bischen Hause.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198476", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nu daz die geste riche", "tokens": ["Nu", "daz", "die", "ges\u00b7te", "ri\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "den turnei sus geliche", "tokens": ["den", "tur\u00b7nei", "sus", "ge\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "FM", "FM", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "geteilet heten under in", "tokens": ["ge\u00b7tei\u00b7let", "he\u00b7ten", "un\u00b7der", "in"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VVINF", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "do vvart geleit dar uf ir sin", "tokens": ["do", "vvart", "ge\u00b7leit", "dar", "uf", "ir", "sin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "vnd ir vvilleclich gedanc", "tokens": ["vnd", "ir", "vvil\u00b7le\u00b7clich", "ge\u00b7danc"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "das iedermann an vveinic tranc", "tokens": ["das", "ie\u00b7der\u00b7mann", "an", "vvei\u00b7nic", "tranc"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "vnd az ein edel murzel", "tokens": ["vnd", "az", "ein", "e\u00b7del", "mur\u00b7zel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "darnach si beide vvurden snel", "tokens": ["dar\u00b7nach", "si", "bei\u00b7de", "vvur\u00b7den", "snel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "das si kerten uf den plan", "tokens": ["das", "si", "ker\u00b7ten", "uf", "den", "plan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "von Persia der Soldan", "tokens": ["von", "Per\u00b7sia", "der", "Sol\u00b7dan"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "vz der veste kam gezogt", "tokens": ["vz", "der", "ves\u00b7te", "kam", "ge\u00b7zogt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "so reit der keiserliche Vogt", "tokens": ["so", "reit", "der", "kei\u00b7ser\u00b7li\u00b7che", "Vogt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "im engegen von dem mer", "tokens": ["im", "en\u00b7ge\u00b7gen", "von", "dem", "mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "APPR", "ART", "NN"], "meter": "+----+-", "measure": "dactylic.init"}, "line.14": {"text": "ir zvveier k&#250;neclichez her", "tokens": ["ir", "zv\u00b7vei\u00b7er", "k", "&#250;", "ne\u00b7cli\u00b7chez", "her"], "token_info": ["word", "word", "word", "XML_entity", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "$(", "NE", "APZR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "vnd ir zvvo schoenen parte", "tokens": ["vnd", "ir", "zv\u00b7vo", "schoe\u00b7nen", "par\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "gezieret vvaren harte", "tokens": ["ge\u00b7zie\u00b7ret", "vva\u00b7ren", "har\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "VVINF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "mit siden unde mit golde licht", "tokens": ["mit", "si\u00b7den", "un\u00b7de", "mit", "gol\u00b7de", "licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.18": {"text": "von endian von Vztricht", "tokens": ["von", "en\u00b7di\u00b7an", "von", "Vztricht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "von kriechen unde von heidenlant", "tokens": ["von", "krie\u00b7chen", "un\u00b7de", "von", "hei\u00b7den\u00b7lant"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "vvas in ze stiure dar gesant", "tokens": ["vvas", "in", "ze", "stiu\u00b7re", "dar", "ge\u00b7sant"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "vil manec edel samit", "tokens": ["vil", "ma\u00b7nec", "e\u00b7del", "sa\u00b7mit"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.22": {"text": "die vverden k&#250;nge in vviderstrit", "tokens": ["die", "vver\u00b7den", "k", "&#250;", "nge", "in", "vvi\u00b7der\u00b7strit"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "VVFIN", "APPR", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "haten sich gegestet", "tokens": ["ha\u00b7ten", "sich", "ge\u00b7ge\u00b7stet"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PRF", "VVPP"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.24": {"text": "vnd an ir lip gebestet", "tokens": ["vnd", "an", "ir", "lip", "ge\u00b7be\u00b7stet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADJD", "VVPP"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.25": {"text": "tiure unde selzene vvat", "tokens": ["tiu\u00b7re", "un\u00b7de", "sel\u00b7ze\u00b7ne", "vvat"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.26": {"text": "der purpur unde der phat", "tokens": ["der", "pur\u00b7pur", "un\u00b7de", "der", "phat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.27": {"text": "der zendal unde das paldekin", "tokens": ["der", "zen\u00b7dal", "un\u00b7de", "das", "pal\u00b7de\u00b7kin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.28": {"text": "die varen vvunneclichen schin", "tokens": ["die", "va\u00b7ren", "vvun\u00b7ne\u00b7cli\u00b7chen", "schin"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "ir liehten ougen reine", "tokens": ["ir", "lieh\u00b7ten", "ou\u00b7gen", "rei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "das golt und das gesteine", "tokens": ["das", "golt", "und", "das", "ge\u00b7stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "das silber und das stahelvverc", "tokens": ["das", "sil\u00b7ber", "und", "das", "sta\u00b7helv\u00b7verc"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PDS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "mit glanze do tal unde berg", "tokens": ["mit", "glan\u00b7ze", "do", "tal", "un\u00b7de", "berg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}