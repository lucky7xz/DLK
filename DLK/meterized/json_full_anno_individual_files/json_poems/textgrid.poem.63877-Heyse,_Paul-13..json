{"textgrid.poem.63877": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sind mir die Schuppen vom Auge gefallen, was kann ich daf\u00fcr?", "tokens": ["Es", "sind", "mir", "die", "Schup\u00b7pen", "vom", "Au\u00b7ge", "ge\u00b7fal\u00b7len", ",", "was", "kann", "ich", "da\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$,", "PWS", "VMFIN", "PPER", "PAV", "$."], "meter": "-+--+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "Will nun in der Welt mir so Manches mi\u00dffallen, was kann ich daf\u00fcr?", "tokens": ["Will", "nun", "in", "der", "Welt", "mir", "so", "Man\u00b7ches", "mi\u00df\u00b7fal\u00b7len", ",", "was", "kann", "ich", "da\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN", "PPER", "ADV", "PIS", "VVINF", "$,", "PWS", "VMFIN", "PPER", "PAV", "$."], "meter": "-+--+--+--+-++--+", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "Ich opferte sch\u00f6nen und leuchtenden G\u00f6ttern, und schmerzt es mich jetzt,", "tokens": ["Ich", "op\u00b7fer\u00b7te", "sch\u00f6\u00b7nen", "und", "leuch\u00b7ten\u00b7den", "G\u00f6t\u00b7tern", ",", "und", "schmerzt", "es", "mich", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "KON", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+--+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "Zu sehen, wie rasch ihre Tempel verfallen, was kann ich daf\u00fcr?", "tokens": ["Zu", "se\u00b7hen", ",", "wie", "rasch", "ih\u00b7re", "Tem\u00b7pel", "ver\u00b7fal\u00b7len", ",", "was", "kann", "ich", "da\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "VVINF", "$,", "PWS", "VMFIN", "PPER", "PAV", "$."], "meter": "-+--+--+--+-++--+", "measure": "amphibrach.tetra.plus"}, "line.5": {"text": "Es macht im Theater Gemeines und Plattes und Mystik sich breit,", "tokens": ["Es", "macht", "im", "The\u00b7a\u00b7ter", "Ge\u00b7mei\u00b7nes", "und", "Plat\u00b7tes", "und", "Mys\u00b7tik", "sich", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NN", "KON", "NN", "KON", "NN", "PRF", "ADJD", "$,"], "meter": "-+--+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.6": {"text": "Und scheinen entweiht mir die heiligen Hallen, was kann ich daf\u00fcr?", "tokens": ["Und", "schei\u00b7nen", "ent\u00b7weiht", "mir", "die", "hei\u00b7li\u00b7gen", "Hal\u00b7len", ",", "was", "kann", "ich", "da\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PWS", "VMFIN", "PPER", "PAV", "$."], "meter": "-+--+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.7": {"text": "Orchestergel\u00e4rm zu rhetorischen Phrasen gilt heut f\u00fcr Musik.", "tokens": ["Or\u00b7ches\u00b7ter\u00b7ge\u00b7l\u00e4rm", "zu", "rhe\u00b7to\u00b7ri\u00b7schen", "Phra\u00b7sen", "gilt", "heut", "f\u00fcr", "Mu\u00b7sik", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Mich lockt der ", "tokens": ["Mich", "lockt", "der"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Die Lyriker halten ihr Klimpern und Klingeln f\u00fcr Seelengesang,", "tokens": ["Die", "Ly\u00b7ri\u00b7ker", "hal\u00b7ten", "ihr", "Klim\u00b7pern", "und", "Klin\u00b7geln", "f\u00fcr", "See\u00b7len\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.10": {"text": "Und d\u00fcnket mich kindisch ihr Stammeln und Lallen, was kann ich daf\u00fcr?", "tokens": ["Und", "d\u00fcn\u00b7ket", "mich", "kin\u00b7disch", "ihr", "Stam\u00b7meln", "und", "Lal\u00b7len", ",", "was", "kann", "ich", "da\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "KON", "NN", "$,", "PWS", "VMFIN", "PPER", "PAV", "$."], "meter": "-+--+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.11": {"text": "Die Pleinairisten und Naturalisten, wenn beide zumal", "tokens": ["Die", "Plei\u00b7nai\u00b7ris\u00b7ten", "und", "Na\u00b7tu\u00b7ra\u00b7lis\u00b7ten", ",", "wenn", "bei\u00b7de", "zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$,", "KOUS", "PIS", "ADV"], "meter": "-+-+-+-+-+--+--+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Mir scheinen absurden Doktrinen verfallen, was kann ich daf\u00fcr?", "tokens": ["Mir", "schei\u00b7nen", "ab\u00b7sur\u00b7den", "Dokt\u00b7ri\u00b7nen", "ver\u00b7fal\u00b7len", ",", "was", "kann", "ich", "da\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "VVINF", "$,", "PWS", "VMFIN", "PPER", "PAV", "$."], "meter": "-+-+--++--+-++--+", "measure": "iambic.octa.plus.relaxed"}, "line.13": {"text": "Sie zucken die Achseln und r\u00fcmpfen die Nase und nennen's senil,", "tokens": ["Sie", "zu\u00b7cken", "die", "Ach\u00b7seln", "und", "r\u00fcmp\u00b7fen", "die", "Na\u00b7se", "und", "nen\u00b7nen's", "se\u00b7nil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "KON", "NE", "NE", "$,"], "meter": "-+--+--+--+-+-+-+", "measure": "amphibrach.tetra.plus"}, "line.14": {"text": "Im St\u00fcrmen und Dr\u00e4ngen nicht mitzukrawallen; was kann ich daf\u00fcr?", "tokens": ["Im", "St\u00fcr\u00b7men", "und", "Dr\u00e4n\u00b7gen", "nicht", "mit\u00b7zu\u00b7kra\u00b7wal\u00b7len", ";", "was", "kann", "ich", "da\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "PTKNEG", "VVINF", "$.", "PWS", "VMFIN", "PPER", "PAV", "$."], "meter": "-+--+--+--+-++--+", "measure": "amphibrach.tetra.plus"}, "line.15": {"text": "Denn spr\u00e4ch' auch einer mit Engelszungen, es w\u00fcrde sein Wort", "tokens": ["Denn", "spr\u00e4ch'", "auch", "ei\u00b7ner", "mit", "En\u00b7gels\u00b7zun\u00b7gen", ",", "es", "w\u00fcr\u00b7de", "sein", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "APPR", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+--+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Wie eines W\u00fcstenpred'gers verhallen; was kann ich daf\u00fcr?", "tokens": ["Wie", "ei\u00b7nes", "W\u00fcs\u00b7ten\u00b7pre\u00b7d'\u00b7gers", "ver\u00b7hal\u00b7len", ";", "was", "kann", "ich", "da\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$.", "PWS", "VMFIN", "PPER", "PAV", "$."], "meter": "-+-+--+--+-++--+", "measure": "iambic.septa.relaxed"}, "line.17": {"text": "Doch schn\u00f6d' ist's freilich, nur stets zu sprechen und heimlich dabei", "tokens": ["Doch", "schn\u00f6d'", "ist's", "frei\u00b7lich", ",", "nur", "stets", "zu", "spre\u00b7chen", "und", "heim\u00b7lich", "da\u00b7bei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "$,", "ADV", "ADV", "PTKZU", "VVINF", "KON", "ADJD", "PAV"], "meter": "-+-+--+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Ohnm\u00e4chtig die Faust in der Tasche zu ballen: was kann ich daf\u00fcr!", "tokens": ["Ohn\u00b7m\u00e4ch\u00b7tig", "die", "Faust", "in", "der", "Ta\u00b7sche", "zu", "bal\u00b7len", ":", "was", "kann", "ich", "da\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$.", "PWS", "VMFIN", "PPER", "PAV", "$."], "meter": "-+--+--+--+-++--+", "measure": "amphibrach.tetra.plus"}}, "stanza.2": {"line.1": {"text": "Es sind mir die Schuppen vom Auge gefallen, was kann ich daf\u00fcr?", "tokens": ["Es", "sind", "mir", "die", "Schup\u00b7pen", "vom", "Au\u00b7ge", "ge\u00b7fal\u00b7len", ",", "was", "kann", "ich", "da\u00b7f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$,", "PWS", "VMFIN", "PPER", "PAV", "$."], "meter": "-+--