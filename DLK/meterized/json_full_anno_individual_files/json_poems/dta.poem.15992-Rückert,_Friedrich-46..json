{"dta.poem.15992": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Weise sollte seyn ein K\u00f6nig, und zum Lohne", "tokens": ["Der", "Wei\u00b7se", "soll\u00b7te", "seyn", "ein", "K\u00f6\u00b7nig", ",", "und", "zum", "Loh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "ART", "NN", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Weisheit tragen sollt' er auf dem Haupt die Krone.", "tokens": ["Der", "Weis\u00b7heit", "tra\u00b7gen", "sollt'", "er", "auf", "dem", "Haupt", "die", "Kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch soviel Weise giebts, wir hoffen's, in den Landen,", "tokens": ["Doch", "so\u00b7viel", "Wei\u00b7se", "giebts", ",", "wir", "hof\u00b7fen's", ",", "in", "den", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,", "PPER", "VVFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df K\u00f6nigstellen gnug dazu nicht sind vorhanden.", "tokens": ["Da\u00df", "K\u00f6\u00b7nig\u00b7stel\u00b7len", "gnug", "da\u00b7zu", "nicht", "sind", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PAV", "PTKNEG", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auch Schade w\u00e4r' es um den Weisen, wenn ein Kaiser", "tokens": ["Auch", "Scha\u00b7de", "w\u00e4r'", "es", "um", "den", "Wei\u00b7sen", ",", "wenn", "ein", "Kai\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "APPR", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er oder K\u00f6nig w\u00fcrd', und bliebe nicht ein Weiser.", "tokens": ["Er", "o\u00b7der", "K\u00f6\u00b7nig", "w\u00fcrd'", ",", "und", "blie\u00b7be", "nicht", "ein", "Wei\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "NN", "VAFIN", "$,", "KON", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch sollt' ein K\u00f6nig nur allein der Weise seyn,", "tokens": ["Doch", "sollt'", "ein", "K\u00f6\u00b7nig", "nur", "al\u00b7lein", "der", "Wei\u00b7se", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADV", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der's auch als K\u00f6nig blieb, das K\u00f6nigthum gieng' ein.", "tokens": ["Der's", "auch", "als", "K\u00f6\u00b7nig", "blieb", ",", "das", "K\u00f6\u00b7nig\u00b7thum", "gieng'", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "KOUS", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}