{"textgrid.poem.41208": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ohne Lustempfindung", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ohne Lustempfindung", "tokens": ["Oh\u00b7ne", "Lus\u00b7temp\u00b7fin\u00b7dung"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sieht ein Krieger niemal", "tokens": ["Sieht", "ein", "Krie\u00b7ger", "nie\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Auf sein hangend Schwert,", "tokens": ["Auf", "sein", "han\u00b7gend", "Schwert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Das mit ihm am ersten", "tokens": ["Das", "mit", "ihm", "am", "ers\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "PPER", "APPRART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Seiner Ehrentage", "tokens": ["Sei\u00b7ner", "Eh\u00b7ren\u00b7ta\u00b7ge"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Sieg und hohen Heldenruhm erfocht.", "tokens": ["Sieg", "und", "ho\u00b7hen", "Hel\u00b7den\u00b7ruhm", "er\u00b7focht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ohne Lustempfindung", "tokens": ["Oh\u00b7ne", "Lus\u00b7temp\u00b7fin\u00b7dung"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Seh' ich Barde niemal", "tokens": ["Seh'", "ich", "Bar\u00b7de", "nie\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Auf mein hangend Spiel.", "tokens": ["Auf", "mein", "han\u00b7gend", "Spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVPP", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ossians erhab'ne", "tokens": ["Os\u00b7si\u00b7ans", "er\u00b7hab'\u00b7ne"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Lieder nachzustimmen", "tokens": ["Lie\u00b7der", "nach\u00b7zu\u00b7stim\u00b7men"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Rang es, und errang mir einen Gleim", "tokens": ["Rang", "es", ",", "und", "er\u00b7rang", "mir", "ei\u00b7nen", "Gleim"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "F\u00fcrstengold umstralet", "tokens": ["F\u00fcrs\u00b7ten\u00b7gold", "um\u00b7stra\u00b7let"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "In geschm\u00fcckten Hallen", "tokens": ["In", "ge\u00b7schm\u00fcck\u00b7ten", "Hal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Seinen Scheitel nicht.", "tokens": ["Sei\u00b7nen", "Schei\u00b7tel", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Weder Krieg, noch Frieden,", "tokens": ["We\u00b7der", "Krieg", ",", "noch", "Frie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Geht von seinen Lippen", "tokens": ["Geht", "von", "sei\u00b7nen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ueber untergeb'ne V\u00f6lker aus.", "tokens": ["Ue\u00b7ber", "un\u00b7ter\u00b7ge\u00b7b'\u00b7ne", "V\u00f6l\u00b7ker", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Keine Siegestr\u00fcmmer", "tokens": ["Kei\u00b7ne", "Sie\u00b7ge\u00b7str\u00fcm\u00b7mer"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ragt in Blutgefilden", "tokens": ["Ragt", "in", "Blut\u00b7ge\u00b7fil\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Seinem Namen auf.", "tokens": ["Sei\u00b7nem", "Na\u00b7men", "auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Kein Bedr\u00fcckter eilet", "tokens": ["Kein", "Be\u00b7dr\u00fcck\u00b7ter", "ei\u00b7let"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Unter hochgebornen", "tokens": ["Un\u00b7ter", "hoch\u00b7ge\u00b7bor\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Reichen Herrscherg\u00fcnstlingen ihm zu.", "tokens": ["Rei\u00b7chen", "Herr\u00b7scher\u00b7g\u00fcnst\u00b7lin\u00b7gen", "ihm", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Aber seinen Scheitel", "tokens": ["A\u00b7ber", "sei\u00b7nen", "Schei\u00b7tel"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Laubt ein unentweihter", "tokens": ["Laubt", "ein", "un\u00b7ent\u00b7weih\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Eichenspr\u00f6\u00dfling auf;", "tokens": ["Ei\u00b7chen\u00b7spr\u00f6\u00df\u00b7ling", "auf", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Aber Lob und Ehre", "tokens": ["A\u00b7ber", "Lob", "und", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Geht von seinen Lippen", "tokens": ["Geht", "von", "sei\u00b7nen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ueber Th\u00e4ter ed'ler Thaten aus.", "tokens": ["Ue\u00b7ber", "Th\u00e4\u00b7ter", "ed'\u00b7ler", "Tha\u00b7ten", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Aber seinen Namen", "tokens": ["A\u00b7ber", "sei\u00b7nen", "Na\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ziert in seinen Liedern", "tokens": ["Ziert", "in", "sei\u00b7nen", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ewigheller Ruhm;", "tokens": ["E\u00b7wig\u00b7hel\u00b7ler", "Ruhm", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Aber Bardenfreunde", "tokens": ["A\u00b7ber", "Bar\u00b7den\u00b7freun\u00b7de"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Jauchzen unter hohen", "tokens": ["Jauch\u00b7zen", "un\u00b7ter", "ho\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Deutschen Harfenk\u00f6nigen ihm zu.", "tokens": ["Deut\u00b7schen", "Har\u00b7fen\u00b7k\u00f6\u00b7ni\u00b7gen", "ihm", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Wie der Quelle Lispel", "tokens": ["Wie", "der", "Quel\u00b7le", "Lis\u00b7pel"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mit der Weste S\u00e4useln", "tokens": ["Mit", "der", "Wes\u00b7te", "S\u00e4u\u00b7seln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sich im Lenze paart;", "tokens": ["Sich", "im", "Len\u00b7ze", "paart", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Also, wenn den Barden", "tokens": ["Al\u00b7so", ",", "wenn", "den", "Bar\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Holder Scherz erheitert,", "tokens": ["Hol\u00b7der", "Scherz", "er\u00b7hei\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Rieselt in die Saiten sein Gesang", "tokens": ["Rie\u00b7selt", "in", "die", "Sai\u00b7ten", "sein", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Wie der Donner Krachen", "tokens": ["Wie", "der", "Don\u00b7ner", "Kra\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mit des Nordes Brausen", "tokens": ["Mit", "des", "Nor\u00b7des", "Brau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sich in Wetter paart;", "tokens": ["Sich", "in", "Wet\u00b7ter", "paart", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Also, wenn den Barden", "tokens": ["Al\u00b7so", ",", "wenn", "den", "Bar\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Hedenernst verd\u00fcstert,", "tokens": ["He\u00b7de\u00b7nernst", "ver\u00b7d\u00fcs\u00b7tert", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "St\u00fcrmet in die Saiten sein Gesang", "tokens": ["St\u00fcr\u00b7met", "in", "die", "Sai\u00b7ten", "sein", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Als in Todeseb'nen", "tokens": ["Als", "in", "To\u00b7des\u00b7e\u00b7b'\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Friedrich's Heere standen,", "tokens": ["Fried\u00b7rich's", "Hee\u00b7re", "stan\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "War der Barde mit.", "tokens": ["War", "der", "Bar\u00b7de", "mit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wie die Griffe rauschten,", "tokens": ["Wie", "die", "Grif\u00b7fe", "rauschten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Schwoll der Muth im Busen,", "tokens": ["Schwoll", "der", "Muth", "im", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Schlug der Waffenhagel weit umher.", "tokens": ["Schlug", "der", "Waf\u00b7fen\u00b7ha\u00b7gel", "weit", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Aber lieber, lieber", "tokens": ["A\u00b7ber", "lie\u00b7ber", ",", "lie\u00b7ber"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "ADV", "$,", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Weilet er an B\u00e4chen,", "tokens": ["Wei\u00b7let", "er", "an", "B\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wo kein Eisen klirrt;", "tokens": ["Wo", "kein", "Ei\u00b7sen", "klirrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "In der Espe Schatten", "tokens": ["In", "der", "Es\u00b7pe", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sitzet er, und singet", "tokens": ["Sit\u00b7zet", "er", ",", "und", "sin\u00b7get"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Br\u00fcdern menschenfreundliches Gef\u00fchl.", "tokens": ["Br\u00fc\u00b7dern", "men\u00b7schen\u00b7freund\u00b7li\u00b7ches", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Denn sein Herz ist heiter,", "tokens": ["Denn", "sein", "Herz", "ist", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wie die d\u00fcnstelose,", "tokens": ["Wie", "die", "d\u00fcns\u00b7te\u00b7lo\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Stille Sommernacht,", "tokens": ["Stil\u00b7le", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Heiter und gef\u00e4llig,", "tokens": ["Hei\u00b7ter", "und", "ge\u00b7f\u00e4l\u00b7lig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und der Freundschaft offen,", "tokens": ["Und", "der", "Freund\u00b7schaft", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und so mild, so mild, wie reifer Herbst", "tokens": ["Und", "so", "mild", ",", "so", "mild", ",", "wie", "rei\u00b7fer", "Herbst"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "PWAV", "ADJD", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Allen Harfens\u00f6hnen", "tokens": ["Al\u00b7len", "Har\u00b7fen\u00b7s\u00f6h\u00b7nen"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "G\u00f6nnet er den Beifall,", "tokens": ["G\u00f6n\u00b7net", "er", "den", "Bei\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Den ihr Lied ersingt.", "tokens": ["Den", "ihr", "Lied", "er\u00b7singt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Jauchzen ihnen V\u00f6lker,", "tokens": ["Jauch\u00b7zen", "ih\u00b7nen", "V\u00f6l\u00b7ker", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "