{"dta.poem.8797": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XvIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin wie Weib dem Mann, wie Mann dem Weibe dir!", "tokens": ["Ich", "bin", "wie", "Weib", "dem", "Mann", ",", "wie", "Mann", "dem", "Wei\u00b7be", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "ART", "NN", "$,", "PWAV", "NN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wen darfst du lieben sonst, da von der Lippe weg", "tokens": ["Wen", "darfst", "du", "lie\u00b7ben", "sonst", ",", "da", "von", "der", "Lip\u00b7pe", "weg"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "ADV", "$,", "KOUS", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit ew'gen K\u00fcssen ich den Tod vertreibe dir?", "tokens": ["Mit", "ew'\u00b7gen", "K\u00fcs\u00b7sen", "ich", "den", "Tod", "ver\u00b7trei\u00b7be", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich bin dir Rosenduft, dir Nachtigallgesang,", "tokens": ["Ich", "bin", "dir", "Ro\u00b7sen\u00b7duft", ",", "dir", "Nach\u00b7ti\u00b7gall\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich bin der Sonne Pfeil, des Mondes Scheibe dir!", "tokens": ["Ich", "bin", "der", "Son\u00b7ne", "Pfeil", ",", "des", "Mon\u00b7des", "Schei\u00b7be", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was willst du noch? Was blickt die Sehnsucht noch umher?", "tokens": ["Was", "willst", "du", "noch", "?", "Was", "blickt", "die", "Sehn\u00b7sucht", "noch", "um\u00b7her", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "$.", "PWS", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wirf Alles, Alles hin, du wei\u00dft, ich bleibe dir!", "tokens": ["Wirf", "Al\u00b7les", ",", "Al\u00b7les", "hin", ",", "du", "wei\u00dft", ",", "ich", "blei\u00b7be", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "$,", "PIS", "PTKVZ", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}