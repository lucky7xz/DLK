{"dta.poem.15770": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie kann im Gegensatz der Werke der Natur", "tokens": ["Wie", "kann", "im", "Ge\u00b7gen\u00b7satz", "der", "Wer\u00b7ke", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "APPRART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Menschen schwache Kunst ihr Werk aufstellen nur!", "tokens": ["Des", "Men\u00b7schen", "schwa\u00b7che", "Kunst", "ihr", "Werk", "auf\u00b7stel\u00b7len", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ihn selber hat Natur als Kunstwerk aufgestellt,", "tokens": ["Ihn", "sel\u00b7ber", "hat", "Na\u00b7tur", "als", "Kunst\u00b7werk", "auf\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "NN", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihm Kunstwerkbildungstrieb lebendig eingesellt.", "tokens": ["Ihm", "Kunst\u00b7werk\u00b7bil\u00b7dungs\u00b7trieb", "le\u00b7ben\u00b7dig", "ein\u00b7ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und was durch diesen Trieb die Kunst hervorgebracht,", "tokens": ["Und", "was", "durch", "die\u00b7sen", "Trieb", "die", "Kunst", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PDAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist mittelbar, Natur, ein Werk nur deiner Macht.", "tokens": ["Ist", "mit\u00b7tel\u00b7bar", ",", "Na\u00b7tur", ",", "ein", "Werk", "nur", "dei\u00b7ner", "Macht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "NN", "$,", "ART", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was r\u00fchmet sich der Mensch, da\u00df er dein Werkzeug ist,", "tokens": ["Was", "r\u00fch\u00b7met", "sich", "der", "Mensch", ",", "da\u00df", "er", "dein", "Werk\u00b7zeug", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ART", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo du Werkmeisterin, Werkstoff und Werkstatt bist!", "tokens": ["Wo", "du", "Werk\u00b7meis\u00b7te\u00b7rin", ",", "Werk\u00b7stoff", "und", "Werk\u00b7statt", "bist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "$,", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}