{"textgrid.poem.24058": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Traum im Walde", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein lichter, gr\u00fcner Schleier \u00fcber mir,", "tokens": ["Ein", "lich\u00b7ter", ",", "gr\u00fc\u00b7ner", "Schlei\u00b7er", "\u00fc\u00b7ber", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und um mich her ein lichter gr\u00fcner Schleier ...", "tokens": ["Und", "um", "mich", "her", "ein", "lich\u00b7ter", "gr\u00fc\u00b7ner", "Schlei\u00b7er", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PRF", "APZR", "ART", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Es singt und klingt aus weiter, weiter Ferne", "tokens": ["Es", "singt", "und", "klingt", "aus", "wei\u00b7ter", ",", "wei\u00b7ter", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ADV", "$,", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Musik, vergehend, weich ...", "tokens": ["Mu\u00b7sik", ",", "ver\u00b7ge\u00b7hend", ",", "weich", "..."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "ADJD", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Durch die Maschen des Schleiers flirrt und blinkt", "tokens": ["Durch", "die", "Ma\u00b7schen", "des", "Schlei\u00b7ers", "flirrt", "und", "blinkt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Ein goldiger Schein.", "tokens": ["Ein", "gol\u00b7di\u00b7ger", "Schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Der malt sich in Kringeln,", "tokens": ["Der", "malt", "sich", "in", "Krin\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "In tanzenden, huschenden, bebenden Tupfen", "tokens": ["In", "tan\u00b7zen\u00b7den", ",", "hu\u00b7schen\u00b7den", ",", "be\u00b7ben\u00b7den", "Tup\u00b7fen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Hell aufs dunkelgr\u00fcne Moos.", "tokens": ["Hell", "aufs", "dun\u00b7kel\u00b7gr\u00fc\u00b7ne", "Moos", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was singt das ferne, ferne Lied ...?", "tokens": ["Was", "singt", "das", "fer\u00b7ne", ",", "fer\u00b7ne", "Lied", "...", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lauschen will ich ...", "tokens": ["Lau\u00b7schen", "will", "ich", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Holde, weiche Frauenstimme,", "tokens": ["Hol\u00b7de", ",", "wei\u00b7che", "Frau\u00b7en\u00b7stim\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leise, leise ... Wiegenliedsang ....", "tokens": ["Lei\u00b7se", ",", "lei\u00b7se", "...", "Wie\u00b7gen\u00b7lie\u00b7dsang", "...."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$(", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Schlage die Augen auf, gl\u00fcckliches Kind;", "tokens": ["Schla\u00b7ge", "die", "Au\u00b7gen", "auf", ",", "gl\u00fcck\u00b7li\u00b7ches", "Kind", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Siehe, liebreich schimmern zwei g\u00fctige", "tokens": ["Sie\u00b7he", ",", "lieb\u00b7reich", "schim\u00b7mern", "zwei", "g\u00fc\u00b7ti\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "ADJD", "VVFIN", "CARD", "ADJA"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Sterne der wachenden Liebe nieder,", "tokens": ["Ster\u00b7ne", "der", "wa\u00b7chen\u00b7den", "Lie\u00b7be", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Schlafe, schlafe du gl\u00fcckliches Kind,", "tokens": ["Schla\u00b7fe", ",", "schla\u00b7fe", "du", "gl\u00fcck\u00b7li\u00b7ches", "Kind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Umsungen vom Liede der Mutterliebe ...", "tokens": ["Um\u00b7sun\u00b7gen", "vom", "Lie\u00b7de", "der", "Mut\u00b7ter\u00b7lie\u00b7be", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Wehend teilt sich der gr\u00fcne Schleier:", "tokens": ["We\u00b7hend", "teilt", "sich", "der", "gr\u00fc\u00b7ne", "Schlei\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wie eine Wolke umh\u00fcllt er ein Weib.", "tokens": ["Wie", "ei\u00b7ne", "Wol\u00b7ke", "um\u00b7h\u00fcllt", "er", "ein", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das naht mit schwebend langsamem Schritt.", "tokens": ["Das", "naht", "mit", "schwe\u00b7bend", "lang\u00b7sa\u00b7mem", "Schritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Bist du das Gl\u00fcck, Weib, bist du die Liebe? ...", "tokens": ["Bist", "du", "das", "Gl\u00fcck", ",", "Weib", ",", "bist", "du", "die", "Lie\u00b7be", "?", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "NN", "$,", "VAFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}}, "stanza.5": {"line.1": {"text": "Selige Milde str\u00f6mt aus den blauen,", "tokens": ["Se\u00b7li\u00b7ge", "Mil\u00b7de", "str\u00f6mt", "aus", "den", "blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Himmlisch g\u00fctigen Augen mir", "tokens": ["Himm\u00b7lisch", "g\u00fc\u00b7ti\u00b7gen", "Au\u00b7gen", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "PPER"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "L\u00f6send ins Herz ...", "tokens": ["L\u00f6\u00b7send", "ins", "Herz", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Bist du die Liebe, Weib? ...", "tokens": ["Bist", "du", "die", "Lie\u00b7be", ",", "Weib", "?", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wie es klingt und duftet ...", "tokens": ["Wie", "es", "klingt", "und", "duf\u00b7tet", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Was hebt mich empor?", "tokens": ["Was", "hebt", "mich", "em\u00b7por", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ein Quillen und Schwellen in mir:", "tokens": ["Ein", "Quil\u00b7len", "und", "Schwel\u00b7len", "in", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "S\u00fc\u00dfes Singen,", "tokens": ["S\u00fc\u00b7\u00dfes", "Sin\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ferne, nahe;", "tokens": ["Fer\u00b7ne", ",", "na\u00b7he", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Geigen schwirren, lang auss\u00e4uselnd;", "tokens": ["Gei\u00b7gen", "schwir\u00b7ren", ",", "lang", "aus\u00b7s\u00e4u\u00b7selnd", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bl\u00fcten schaukeln herab durch warme,", "tokens": ["Bl\u00fc\u00b7ten", "schau\u00b7keln", "her\u00b7ab", "durch", "war\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "ADJA", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Wogende D\u00fcfte, \u2013 ah, der Atem", "tokens": ["Wo\u00b7gen\u00b7de", "D\u00fcf\u00b7te", ",", "\u2013", "ah", ",", "der", "A\u00b7tem"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "$(", "ITJ", "$,", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Der Frau mir nahe.", "tokens": ["Der", "Frau", "mir", "na\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Ihre Blicke str\u00f6men wie hei\u00dfe Fluten", "tokens": ["Ih\u00b7re", "Bli\u00b7cke", "str\u00f6\u00b7men", "wie", "hei\u00b7\u00dfe", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.11": {"text": "Gl\u00fchend mir ins Herz, \u2013", "tokens": ["Gl\u00fc\u00b7hend", "mir", "ins", "Herz", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "PPER", "APPRART", "NN", "$,", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Ein Ku\u00df", "tokens": ["Ein", "Ku\u00df"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Auf meinen bebenden Lippen ...", "tokens": ["Auf", "mei\u00b7nen", "be\u00b7ben\u00b7den", "Lip\u00b7pen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Bist du die Liebe, Weib?", "tokens": ["Bist", "du", "die", "Lie\u00b7be", ",", "Weib", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da klingts wie Wiegenliedsang so weich,", "tokens": ["Da", "klingts", "wie", "Wie\u00b7gen\u00b7lie\u00b7dsang", "so", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Beruhigend, seliger Wehmut voll", "tokens": ["Be\u00b7ru\u00b7hi\u00b7gend", ",", "se\u00b7li\u00b7ger", "Weh\u00b7mut", "voll"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "ADJA", "NN", "ADJD"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von den Lippen der Frau:", "tokens": ["Von", "den", "Lip\u00b7pen", "der", "Frau", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "\u00bbve