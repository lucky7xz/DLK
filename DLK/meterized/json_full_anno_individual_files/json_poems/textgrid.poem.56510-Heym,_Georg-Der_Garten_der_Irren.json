{"textgrid.poem.56510": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Garten der Irren", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am roten Teiche stehen viele Schatten", "tokens": ["Am", "ro\u00b7ten", "Tei\u00b7che", "ste\u00b7hen", "vie\u00b7le", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bei d\u00fcnner B\u00e4ume schw\u00e4chlichem Gesichte,", "tokens": ["Bei", "d\u00fcn\u00b7ner", "B\u00e4u\u00b7me", "schw\u00e4ch\u00b7li\u00b7chem", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Stille fort. Nur selten da\u00df sich einer", "tokens": ["In", "Stil\u00b7le", "fort", ".", "Nur", "sel\u00b7ten", "da\u00df", "sich", "ei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKVZ", "$.", "ADV", "ADJD", "KOUS", "PRF", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herunter zu dem tr\u00fcben Wasser b\u00fccket.", "tokens": ["Her\u00b7un\u00b7ter", "zu", "dem", "tr\u00fc\u00b7ben", "Was\u00b7ser", "b\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und manche gehn in den entleerten Hecken", "tokens": ["Und", "man\u00b7che", "gehn", "in", "den", "ent\u00b7leer\u00b7ten", "He\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos"