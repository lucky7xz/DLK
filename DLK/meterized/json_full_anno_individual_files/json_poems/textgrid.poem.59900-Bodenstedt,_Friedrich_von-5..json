{"textgrid.poem.59900": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ei, du n\u00e4rrisches Herz,", "tokens": ["Ei", ",", "du", "n\u00e4r\u00b7ri\u00b7sches", "Herz", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Da\u00df dich klagend gebeugst hast!", "tokens": ["Da\u00df", "dich", "kla\u00b7gend", "ge\u00b7beugst", "hast", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVPP", "VAFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Du bejammerst den Schmerz,", "tokens": ["Du", "be\u00b7jam\u00b7merst", "den", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Den du selber erzeugt hast!", "tokens": ["Den", "du", "sel\u00b7ber", "er\u00b7zeugt", "hast", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Du verzweifelst in Gefahr heut", "tokens": ["Du", "ver\u00b7zwei\u00b7felst", "in", "Ge\u00b7fahr", "heut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und suchst doch selbst die Gefahr!", "tokens": ["Und", "suchst", "doch", "selbst", "die", "Ge\u00b7fahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-