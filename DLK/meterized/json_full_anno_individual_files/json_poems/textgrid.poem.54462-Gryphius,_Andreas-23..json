{"textgrid.poem.54462": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem der sternen F\u00fcrst von vns begint zu weichen/", "tokens": ["In", "dem", "der", "ster\u00b7nen", "F\u00fcrst", "von", "vns", "be\u00b7gint", "zu", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dem der Sommer stirbt/ in dem das gr\u00fcne kleidt", "tokens": ["In", "dem", "der", "Som\u00b7mer", "stirbt", "/", "in", "dem", "das", "gr\u00fc\u00b7ne", "kleidt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$(", "APPR", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der wiesen durch den frost des herbstes wird gemayt.", "tokens": ["Der", "wie\u00b7sen", "durch", "den", "frost", "des", "herbs\u00b7tes", "wird", "ge\u00b7mayt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd vor dem Scorpion die matten ba\u00fcm erbl\u00e4ichen.", "tokens": ["Vnd", "vor", "dem", "Scor\u00b7pi\u00b7on", "die", "mat\u00b7ten", "ba\u00fcm", "er\u00b7bl\u00e4i\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "met