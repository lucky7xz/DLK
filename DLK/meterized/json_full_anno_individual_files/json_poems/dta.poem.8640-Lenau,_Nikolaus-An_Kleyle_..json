d", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VVFIN", "ART", "NE", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Wir sind vorbei. Der Sturm lenkt sein Gefieder", "tokens": ["Wir", "sind", "vor\u00b7bei", ".", "Der", "Sturm", "lenkt", "sein", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum dunkeln Horste der Vergangenheit,", "tokens": ["Zum", "dun\u00b7keln", "Hors\u00b7te", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Wehmuth sinkt an meinen Busen wieder,", "tokens": ["Und", "Weh\u00b7muth", "sinkt", "an", "mei\u00b7nen", "Bu\u00b7sen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die stille Freundin meiner Einsamkeit.", "tokens": ["Die", "stil\u00b7le", "Freun\u00b7din", "mei\u00b7ner", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}