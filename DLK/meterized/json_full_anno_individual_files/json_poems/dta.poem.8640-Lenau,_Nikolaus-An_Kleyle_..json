{"dta.poem.8640": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An Kleyle .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Vergib, vergib, Geliebter, dem Gesange,", "tokens": ["Ver\u00b7gib", ",", "ver\u00b7gib", ",", "Ge\u00b7lieb\u00b7ter", ",", "dem", "Ge\u00b7san\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVIMP", "$,", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der deines Schmerzes leisen Schlummer st\u00f6rt,", "tokens": ["Der", "dei\u00b7nes", "Schmer\u00b7zes", "lei\u00b7sen", "Schlum\u00b7mer", "st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der dir Erinnerungen, s\u00fc\u00df und bange,", "tokens": ["Der", "dir", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", ",", "s\u00fc\u00df", "und", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herauf aus ihrer stillen Gruft beschw\u00f6rt!", "tokens": ["Her\u00b7auf", "aus", "ih\u00b7rer", "stil\u00b7len", "Gruft", "be\u00b7schw\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gedenkst du noch des Abends, den die G\u00f6tter", "tokens": ["Ge\u00b7denkst", "du", "noch", "des", "A\u00b7bends", ",", "den", "die", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf uns herabgestreut aus milder Hand,", "tokens": ["Auf", "uns", "her\u00b7ab\u00b7ge\u00b7streut", "aus", "mil\u00b7der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So bl\u00fchend, leicht, wie junge Rosenbl\u00e4tter,", "tokens": ["So", "bl\u00fc\u00b7hend", ",", "leicht", ",", "wie", "jun\u00b7ge", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADJD", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denkst du des Abends noch am Leithastrand?", "tokens": ["Denkst", "du", "des", "A\u00b7bends", "noch", "am", "Leit\u00b7has\u00b7trand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Im Haine sprang von Baum zu Baum die R\u00f6the,", "tokens": ["Im", "Hai\u00b7ne", "sprang", "von", "Baum", "zu", "Baum", "die", "R\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "NE", "APPR", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie wiegte sich auf Wipfeln, mischte froh", "tokens": ["Sie", "wieg\u00b7te", "sich", "auf", "Wip\u00b7feln", ",", "mischte", "froh"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sich in den Wellentanz, der zum Gefl\u00f6te", "tokens": ["Sich", "in", "den", "Wel\u00b7len\u00b7tanz", ",", "der", "zum", "Ge\u00b7fl\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Nachtigallen rasch vor\u00fcberfloh.", "tokens": ["Der", "Nach\u00b7ti\u00b7gal\u00b7len", "rasch", "vor\u00b7\u00fc\u00b7ber\u00b7floh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wir aber schritten traulich durch die Schatten,", "tokens": ["Wir", "a\u00b7ber", "schrit\u00b7ten", "trau\u00b7lich", "durch", "die", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und, s\u00fc\u00df geschw\u00e4tzig, uns zur Seite ging", "tokens": ["Und", ",", "s\u00fc\u00df", "ge\u00b7schw\u00e4t\u00b7zig", ",", "uns", "zur", "Sei\u00b7te", "ging"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "ADJD", "ADJD", "$,", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Hoffnung, sprach vom Himmel treuer Gatten,", "tokens": ["Die", "Hoff\u00b7nung", ",", "sprach", "vom", "Him\u00b7mel", "treu\u00b7er", "Gat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wies dir von Lottchens Hand den g\u00fcldnen Ring.", "tokens": ["Wies", "dir", "von", "Lott\u00b7chens", "Hand", "den", "g\u00fcld\u00b7nen", "Ring", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NE", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Schon sah mein Blick, der in die Zukunft sp\u00e4hte,", "tokens": ["Schon", "sah", "mein", "Blick", ",", "der", "in", "die", "Zu\u00b7kunft", "sp\u00e4h\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In langen Reihen Wonnetage zieh'n;", "tokens": ["In", "lan\u00b7gen", "Rei\u00b7hen", "Won\u00b7ne\u00b7ta\u00b7ge", "zieh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schon baut' ich k\u00fchn mit leichtem Traumger\u00e4the", "tokens": ["Schon", "baut'", "ich", "k\u00fchn", "mit", "leich\u00b7tem", "Traum\u00b7ge\u00b7r\u00e4\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein fr\u00fch zerfallnes Gl\u00fcck an deines hin. \u2014", "tokens": ["Mein", "fr\u00fch", "zer\u00b7fall\u00b7nes", "Gl\u00fcck", "an", "dei\u00b7nes", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "APPR", "PPOSAT", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sanft senkten sich in feierliches Schweigen", "tokens": ["Sanft", "senk\u00b7ten", "sich", "in", "fei\u00b7er\u00b7li\u00b7ches", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Z\u00fcge der Natur, kein L\u00fcftchen sprach,", "tokens": ["Die", "Z\u00fc\u00b7ge", "der", "Na\u00b7tur", ",", "kein", "L\u00fcft\u00b7chen", "sprach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie schien ihr g\u00f6ttlich Angesicht zu neigen,", "tokens": ["Sie", "schien", "ihr", "g\u00f6tt\u00b7lich", "An\u00b7ge\u00b7sicht", "zu", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als s\u00e4nne still sie einer Freude nach.", "tokens": ["Als", "s\u00e4n\u00b7ne", "still", "sie", "ei\u00b7ner", "Freu\u00b7de", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ADJD", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Sterne tauchten aus dem Aethermeere,", "tokens": ["Die", "Ster\u00b7ne", "tauch\u00b7ten", "aus", "dem", "A\u00b7e\u00b7ther\u00b7mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Weste Hauch erwachte nun im Hain,", "tokens": ["Der", "Wes\u00b7te", "Hauch", "er\u00b7wach\u00b7te", "nun", "im", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Blume trank des Himmels leise Z\u00e4hre,", "tokens": ["Die", "Blu\u00b7me", "trank", "des", "Him\u00b7mels", "lei\u00b7se", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und selig irrten wir im Mondenschein. \u2014 \u2014", "tokens": ["Und", "se\u00b7lig", "irr\u00b7ten", "wir", "im", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Doch kommt ein Sturm jezt \u00fcber meine Saiten,", "tokens": ["Doch", "kommt", "ein", "Sturm", "jezt", "\u00fc\u00b7ber", "mei\u00b7ne", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Rei\u00dft wild mir von der Leier jenen Tag,", "tokens": ["Rei\u00dft", "wild", "mir", "von", "der", "Lei\u00b7er", "je\u00b7nen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den sch\u00f6nen Tag mit allen Seligkeiten, \u2014", "tokens": ["Den", "sch\u00f6\u00b7nen", "Tag", "mit", "al\u00b7len", "Se\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Pocht mir an's Herz mit rauhem Fl\u00fcgelschlag.", "tokens": ["Pocht", "mir", "an's", "Herz", "mit", "rau\u00b7hem", "Fl\u00fc\u00b7gel\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Herein! herein! du finsterer Geselle!", "tokens": ["Her\u00b7ein", "!", "her\u00b7ein", "!", "du", "fins\u00b7te\u00b7rer", "Ge\u00b7sel\u00b7le", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$.", "PTKVZ", "$.", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du bist in meiner Brust kein neuer Gast;", "tokens": ["Du", "bist", "in", "mei\u00b7ner", "Brust", "kein", "neu\u00b7er", "Gast", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich \u00f6ffne dir die tr\u00fcmmervolle Zelle,", "tokens": ["Ich", "\u00f6ff\u00b7ne", "dir", "die", "tr\u00fcm\u00b7mer\u00b7vol\u00b7le", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In welcher dein Geschlecht schon oft gerast!", "tokens": ["In", "wel\u00b7cher", "dein", "Ge\u00b7schlecht", "schon", "oft", "ge\u00b7rast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPOSAT", "NN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Des Abends, Freund, gedenk' ich, jenes andern!", "tokens": ["Des", "A\u00b7bends", ",", "Freund", ",", "ge\u00b7denk'", "ich", ",", "je\u00b7nes", "an\u00b7dern", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "VVFIN", "PPER", "$,", "PDAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich seh' im winterlichen D\u00e4mmerlicht", "tokens": ["Ich", "seh'", "im", "win\u00b7ter\u00b7li\u00b7chen", "D\u00e4m\u00b7mer\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zur Kirche hin den langen Brautzug wandern,", "tokens": ["Zur", "Kir\u00b7che", "hin", "den", "lan\u00b7gen", "Braut\u00b7zug", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo die Geliebte Treu' und Herz dir bricht.", "tokens": ["Wo", "die", "Ge\u00b7lieb\u00b7te", "Treu'", "und", "Herz", "dir", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Der Priester sprach den Segen ob dem Paare,", "tokens": ["Der", "Pries\u00b7ter", "sprach", "den", "Se\u00b7gen", "ob", "dem", "Paa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir schien ein Mordgew\u00f6lb das Heiligthum,", "tokens": ["Mir", "schien", "ein", "Mord\u00b7ge\u00b7w\u00f6lb", "das", "Hei\u00b7lig\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich sah die Hoffnung fallen am Altare,", "tokens": ["Ich", "sah", "die", "Hoff\u00b7nung", "fal\u00b7len", "am", "Al\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie ward die s\u00fc\u00dfe Schw\u00e4tzerinn so stumm! \u2014", "tokens": ["Wie", "ward", "die", "s\u00fc\u00b7\u00dfe", "Schw\u00e4t\u00b7ze\u00b7rinn", "so", "stumm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Befl\u00fcgle dich, mein Lied! denn immer tr\u00fcber\u201a", "tokens": ["Be\u00b7fl\u00fcg\u00b7le", "dich", ",", "mein", "Lied", "!", "denn", "im\u00b7mer", "tr\u00fc\u00b7ber", "\u201a"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und thr\u00e4nenvoller stets wird deine Bahn;", "tokens": ["Und", "thr\u00e4\u00b7nen\u00b7vol\u00b7ler", "stets", "wird", "dei\u00b7ne", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O f\u00fchre schnell den Freund mir da vor\u00fcber\u201a", "tokens": ["O", "f\u00