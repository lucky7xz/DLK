{"dta.poem.15904": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "109.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Freund, la\u00df uns nur nicht so schnell bei Seite schieben", "tokens": ["Mein", "Freund", ",", "la\u00df", "uns", "nur", "nicht", "so", "schnell", "bei", "Sei\u00b7te", "schie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "PPER", "ADV", "PTKNEG", "ADV", "ADJD", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die alten Dichter, weils die neuern h\u00f6her trieben.", "tokens": ["Die", "al\u00b7ten", "Dich\u00b7ter", ",", "weils", "die", "neu\u00b7ern", "h\u00f6\u00b7her", "trie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "ART", "ADJA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gar mancher, den man jetzt so vornehm \u00fcberguckt,", "tokens": ["Gar", "man\u00b7cher", ",", "den", "man", "jetzt", "so", "vor\u00b7nehm", "\u00fc\u00b7ber\u00b7guckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "PIS", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Achsel mitleidsvoll bei seinem Namen zuckt,", "tokens": ["Die", "Ach\u00b7sel", "mit\u00b7le\u00b7ids\u00b7voll", "bei", "sei\u00b7nem", "Na\u00b7men", "zuckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Ist, wenn du bringst die Zeit in Anschlag, gar nicht schlecht,", "tokens": ["Ist", ",", "wenn", "du", "bringst", "die", "Zeit", "in", "An\u00b7schlag", ",", "gar", "nicht", "schlecht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und, wenn du absiehst von der Zeit, nicht minder echt", "tokens": ["Und", ",", "wenn", "du", "ab\u00b7siehst", "von", "der", "Zeit", ",", "nicht", "min\u00b7der", "echt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Als mancher, der da nun so hoch die Saiten stimmt,", "tokens": ["Als", "man\u00b7cher", ",", "der", "da", "nun", "so", "hoch", "die", "Sai\u00b7ten", "stimmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "ADV", "ADV", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er so leicht wie Kork auf Beifallswogen schwimmt;", "tokens": ["Weil", "er", "so", "leicht", "wie", "Kork", "auf", "Bei\u00b7falls\u00b7wo\u00b7gen", "schwimmt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KOKOM", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und kann sogar noch jetzt gefallen, wie wol kaum", "tokens": ["Und", "kann", "so\u00b7gar", "noch", "jetzt", "ge\u00b7fal\u00b7len", ",", "wie", "wol", "kaum"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "ADV", "ADV", "VVPP", "$,", "PWAV", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird jener k\u00f6nnen, wann zergangen einst der Schaum.", "tokens": ["Wird", "je\u00b7ner", "k\u00f6n\u00b7nen", ",", "wann", "zer\u00b7gan\u00b7gen", "einst", "der", "Schaum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "VMFIN", "$,", "PWAV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Hier ist nichts was entz\u00fcckt, doch auch nichts was verletzt,", "tokens": ["Hier", "ist", "nichts", "was", "ent\u00b7z\u00fcckt", ",", "doch", "auch", "nichts", "was", "ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PWS", "VVFIN", "$,", "ADV", "ADV", "PIS", "PIS", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und, wenn du m\u00e4\u00dfig bist, genug was dich ergetzt.", "tokens": ["Und", ",", "wenn", "du", "m\u00e4\u00b7\u00dfig", "bist", ",", "ge\u00b7nug", "was", "dich", "er\u00b7getzt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "ADV", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}