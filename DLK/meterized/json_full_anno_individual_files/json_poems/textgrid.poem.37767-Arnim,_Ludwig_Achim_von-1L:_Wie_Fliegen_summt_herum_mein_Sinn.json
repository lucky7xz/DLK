{"textgrid.poem.37767": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie Fliegen summt herum mein Sinn", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Fliegen summt herum mein Sinn", "tokens": ["Wie", "Flie\u00b7gen", "summt", "he\u00b7rum", "mein", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "APZR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wiegt sich leicht auf Halmen,", "tokens": ["Und", "wiegt", "sich", "leicht", "auf", "Hal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als wollt' er sie zermalmen", "tokens": ["Als", "wollt'", "er", "sie", "zer\u00b7mal\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Lachen spielt mir \u00fcber's Kinn.", "tokens": ["Und", "La\u00b7chen", "spielt", "mir", "\u00fc\u00b7ber's", "Kinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich that, als z\u00f6g ich fort von ihr", "tokens": ["Ich", "that", ",", "als", "z\u00f6g", "ich", "fort", "von", "ihr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Hut beschatten Rosen,", "tokens": ["Den", "Hut", "be\u00b7schat\u00b7ten", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So trat ich zu der Losen", "tokens": ["So", "trat", "ich", "zu", "der", "Lo\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sprach: \u00bbIch ziehe fort von hier.", "tokens": ["Und", "sprach", ":", "\u00bb", "Ich", "zie\u00b7he", "fort", "von", "hier", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PTKVZ", "APPR", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mich zieht, mich treibt, ich wei\u00df nicht was,", "tokens": ["Mich", "zieht", ",", "mich", "treibt", ",", "ich", "wei\u00df", "nicht", "was", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In allen meinen Adern;", "tokens": ["In", "al\u00b7len", "mei\u00b7nen", "A\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich f\u00fchl ein stockend Hadern,", "tokens": ["Ich", "f\u00fchl", "ein", "sto\u00b7ckend", "Ha\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ha f\u00fchlt den Puls, die Wangen bla\u00df.", "tokens": ["Ha", "f\u00fchlt", "den", "Puls", ",", "die", "Wan\u00b7gen", "bla\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nach Welschland schweift mein feiner Sinn,", "tokens": ["Nach", "Wel\u00b7schland", "schweift", "mein", "fei\u00b7ner", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin von Luft getragen,", "tokens": ["Ich", "bin", "von", "Luft", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Wolken ziehn den Wagen,", "tokens": ["Die", "Wol\u00b7ken", "ziehn", "den", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es rollet laut mein Sinn darin.", "tokens": ["Es", "rol\u00b7let", "laut", "mein", "Sinn", "da\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hinab, hinab im Thr\u00e4nenstrom", "tokens": ["Hin\u00b7ab", ",", "hin\u00b7ab", "im", "Thr\u00e4\u00b7nen\u00b7strom"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zerflie\u00dfen meine Augen,", "tokens": ["Zer\u00b7flie\u00b7\u00dfen", "mei\u00b7ne", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was k\u00f6nnen sie mir taugen,", "tokens": ["Was", "k\u00f6n\u00b7nen", "sie", "mir", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn sie nicht sehn das hohe Rom.\u00ab", "tokens": ["Wenn", "sie", "nicht", "sehn", "das", "ho\u00b7he", "Rom", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "ART", "ADJA", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie sah mich an aus losem Schlaf,", "tokens": ["Sie", "sah", "mich", "an", "aus", "lo\u00b7sem", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Misst mich mit gro\u00dfen Augen,", "tokens": ["Misst", "mich", "mit", "gro\u00b7\u00dfen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mu\u00df in die H\u00e4ndchen hauchen,", "tokens": ["Mu\u00df", "in", "die", "H\u00e4nd\u00b7chen", "hau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um klar zu sehn, was mich betraf.", "tokens": ["Um", "klar", "zu", "sehn", ",", "was", "mich", "be\u00b7traf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PTKZU", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dann springt sie von der Rasenbank", "tokens": ["Dann", "springt", "sie", "von", "der", "Ra\u00b7sen\u00b7bank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar leicht auf meinen R\u00fccken,", "tokens": ["Gar", "leicht", "auf", "mei\u00b7nen", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich will mich boshaft b\u00fccken,", "tokens": ["Ich", "will", "mich", "bos\u00b7haft", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch Sie mir nicht vom R\u00fccken sank.", "tokens": ["Doch", "Sie", "mir", "nicht", "vom", "R\u00fc\u00b7cken", "sank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie singt mit hellem, hellem Ton:", "tokens": ["Sie", "singt", "mit", "hel\u00b7lem", ",", "hel\u00b7lem", "Ton", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wandern wir nun alle", "tokens": ["So", "wan\u00b7dern", "wir", "nun", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im hellen Morgenschalle", "tokens": ["Im", "hel\u00b7len", "Mor\u00b7gen\u00b7schal\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu unsres Papstes goldnem Thron.", "tokens": ["Zu", "uns\u00b7res", "Paps\u00b7tes", "gold\u00b7nem", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich k\u00fcsse sein Pant\u00f6ffelein,", "tokens": ["Ich", "k\u00fcs\u00b7se", "sein", "Pan\u00b7t\u00f6f\u00b7fe\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Er bittet mich um K\u00fcsse,", "tokens": ["Er", "bit\u00b7tet", "mich", "um", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Damit er sicher wisse,", "tokens": ["Da\u00b7mit", "er", "si\u00b7cher", "wis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ob ich auch eine Christin rein.", "tokens": ["Ob", "ich", "auch", "ei\u00b7ne", "Chris\u00b7tin", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wohlauf, wohlan mein Pegasus,", "tokens": ["Wohl\u00b7auf", ",", "wo\u00b7hlan", "mein", "Pe\u00b7ga\u00b7sus", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich will Dich sch\u00f6n umfassen,", "tokens": ["Ich", "will", "Dich", "sch\u00f6n", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sollst mich nicht fallen lassen,", "tokens": ["Sollst", "mich", "nicht", "fal\u00b7len", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Nach Rom ich heut noch reiten mu\u00df.", "tokens": ["Nach", "Rom", "ich", "heut", "noch", "rei\u00b7ten", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "ADV", "ADV", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Es flieget neben uns die Welt,", "tokens": ["Es", "flie\u00b7get", "ne\u00b7ben", "uns", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die W\u00e4lder untertauchen,", "tokens": ["Die", "W\u00e4l\u00b7der", "un\u00b7ter\u00b7tau\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von Flammen bunt sie rauchen,", "tokens": ["Von", "Flam\u00b7men", "bunt", "sie", "rau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als w\u00e4r es heut f\u00fcr uns bestellt.", "tokens": ["Als", "w\u00e4r", "es", "heut", "f\u00fcr", "uns", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Sie singet wie das Morgenblau", "tokens": ["Sie", "sin\u00b7get", "wie", "das", "Mor\u00b7gen\u00b7blau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus allen tausend Orten,", "tokens": ["Aus", "al\u00b7len", "tau\u00b7send", "Or\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie wei\u00df von keinen Worten,", "tokens": ["Sie", "wei\u00df", "von", "kei\u00b7nen", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch spricht zu ihr die bunte Au.", "tokens": ["Doch", "spricht", "zu", "ihr", "die", "bun\u00b7te", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Uns hebt aus S\u00fcd ein s\u00fc\u00dfer Duft", "tokens": ["Uns", "hebt", "aus", "S\u00fcd", "ein", "s\u00fc\u00b7\u00dfer", "Duft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verspielt in ihren Haaren,", "tokens": ["Ver\u00b7spielt", "in", "ih\u00b7ren", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und aller Tr\u00e4ume Schaaren", "tokens": ["Und", "al\u00b7ler", "Tr\u00e4u\u00b7me", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie kommen mit der neuen Luft.", "tokens": ["Sie", "kom\u00b7men", "mit", "der", "neu\u00b7en", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Der Wald ist frei, der Abend mein,", "tokens": ["Der", "Wald", "ist", "frei", ",", "der", "A\u00b7bend", "mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Leg Dich ins Gras ganz schnelle,", "tokens": ["Leg", "Dich", "ins", "Gras", "ganz", "schnel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Br\u00fcnnlein rieselt helle,", "tokens": ["Ein", "Br\u00fcnn\u00b7lein", "rie\u00b7selt", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Mond sieht sich so froh darein.", "tokens": ["Der", "Mond", "sieht", "sich", "so", "froh", "da\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "ADJD", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Sie legt das K\u00f6pfchen in die Hand", "tokens": ["Sie", "legt", "das", "K\u00f6pf\u00b7chen", "in", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "w