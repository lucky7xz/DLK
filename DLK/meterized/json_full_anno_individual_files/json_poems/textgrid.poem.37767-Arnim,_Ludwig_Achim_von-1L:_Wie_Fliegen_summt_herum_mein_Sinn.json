, "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es rollet laut mein Sinn darin.", "tokens": ["Es", "rol\u00b7let", "laut", "mein", "Sinn", "da\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.22": {"line.1": {"text": "Hinab, hinab im Thr\u00e4nenstrom", "tokens": ["Hin\u00b7ab", ",", "hin\u00b7ab", "im", "Thr\u00e4\u00b7nen\u00b7strom"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zerflie\u00dfen meine Augen,", "tokens": ["Zer\u00b7flie\u00b7\u00dfen", "mei\u00b7ne", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was k\u00f6nnen sie mir taugen,", "tokens": ["Was", "k\u00f6n\u00b7nen", "sie", "mir", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn sie nicht sehn das hohe Rom.\u00ab", "tokens": ["Wenn", "sie", "nicht", "sehn", "das", "ho\u00b7he", "Rom", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "ART", "ADJA", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.23": {"line.1": {"text": "Sie sah mich an aus losem Schlaf,", "tokens": ["Sie", "sah", "mich", "an", "aus", "lo\u00b7sem", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": 