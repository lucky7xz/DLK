{"dta.poem.3131": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "176.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Streben sei, o Sohn, ein innres Gutes frei", "tokens": ["Dein", "Stre\u00b7ben", "sei", ",", "o", "Sohn", ",", "ein", "inn\u00b7res", "Gu\u00b7tes", "frei"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "FM", "NN", "$,", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu machen so, da\u00df es ein \u00e4u\u00dfres Sch\u00f6nes sei.", "tokens": ["Zu", "ma\u00b7chen", "so", ",", "da\u00df", "es", "ein", "\u00e4u\u00df\u00b7res", "Sch\u00f6\u00b7nes", "sei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Warum soll glei\u00dfnerisch ein Schlechter sich bestreben,", "tokens": ["Wa\u00b7rum", "soll", "glei\u00df\u00b7ne\u00b7risch", "ein", "Schlech\u00b7ter", "sich", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADJD", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit falschem Scheine sich des Guten zu umgeben,", "tokens": ["Mit", "fal\u00b7schem", "Schei\u00b7ne", "sich", "des", "Gu\u00b7ten", "zu", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Guter aber sich im Gegentheil beflei\u00dfen,", "tokens": ["Ein", "Gu\u00b7ter", "a\u00b7ber", "sich", "im", "Ge\u00b7gen\u00b7theil", "be\u00b7flei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu scheinen schlechter als er ist, um nicht zu glei\u00dfen?", "tokens": ["Zu", "schei\u00b7nen", "schlech\u00b7ter", "als", "er", "ist", ",", "um", "nicht", "zu", "glei\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "KOUS", "PPER", "VAFIN", "$,", "KOUI", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Durch besser Scheinen wird kein Schlechter besser werden,", "tokens": ["Durch", "bes\u00b7ser", "Schei\u00b7nen", "wird", "kein", "Schlech\u00b7ter", "bes\u00b7ser", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PIAT", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ungestraft kann sich kein Guter schlecht geberden.", "tokens": ["Doch", "un\u00b7ge\u00b7straft", "kann", "sich", "kein", "Gu\u00b7ter", "schlecht", "ge\u00b7ber\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PRF", "PIAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wenn du mit herbem Trotz dein Gutes eigensinnig", "tokens": ["Wenn", "du", "mit", "her\u00b7bem", "Trotz", "dein", "Gu\u00b7tes", "ei\u00b7gen\u00b7sin\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dir verschlie\u00dfen kannst, so ist es nicht recht innig.", "tokens": ["In", "dir", "ver\u00b7schlie\u00b7\u00dfen", "kannst", ",", "so", "ist", "es", "nicht", "recht", "in\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "VMFIN", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Denn, w\u00e4re voll sein Drang, so br\u00e4ch' es aus der H\u00fclle,", "tokens": ["Denn", ",", "w\u00e4\u00b7re", "voll", "sein", "Drang", ",", "so", "br\u00e4ch'", "es", "aus", "der", "H\u00fcl\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "ADJD", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie aus der Knospe bricht der Rose Liebesf\u00fclle.", "tokens": ["Wie", "aus", "der", "Knos\u00b7pe", "bricht", "der", "Ro\u00b7se", "Lie\u00b7bes\u00b7f\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Knospe aber, die sich dumpf verstockt, und wagt", "tokens": ["Die", "Knos\u00b7pe", "a\u00b7ber", ",", "die", "sich", "dumpf", "ver\u00b7stockt", ",", "und", "wagt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PRF", "ADJD", "VVPP", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht aufzugehn, ist wol im Kern vom Wurm genagt.", "tokens": ["Nicht", "auf\u00b7zu\u00b7gehn", ",", "ist", "wol", "im", "Kern", "vom", "Wurm", "ge\u00b7nagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "VAFIN", "ADV", "APPRART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Drum wenn kein Wurm dich nagt des Hochmuths in der Brust,", "tokens": ["Drum", "wenn", "kein", "Wurm", "dich", "nagt", "des", "Hoch\u00b7muths", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PIAT", "NN", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So bl\u00fch auf unverzagt, dir und der Welt zur Lust!", "tokens": ["So", "bl\u00fch", "auf", "un\u00b7ver\u00b7zagt", ",", "dir", "und", "der", "Welt", "zur", "Lust", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJD", "$,", "PPER", "KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Nur nichtig ist der Schein, doch wichtig die Erscheinung,", "tokens": ["Nur", "nich\u00b7tig", "ist", "der", "Schein", ",", "doch", "wich\u00b7tig", "die", "Er\u00b7schei\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vollkommen ist allein des Seyns und Scheins Vereinung.", "tokens": ["Voll\u00b7kom\u00b7men", "ist", "al\u00b7lein", "des", "Seyns", "und", "Scheins", "Ver\u00b7ei\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "ART", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Mach ein Gedicht aus dir, das dann nur ist gelungen,", "tokens": ["Mach", "ein", "Ge\u00b7dicht", "aus", "dir", ",", "das", "dann", "nur", "ist", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "$,", "PRELS", "ADV", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn aus dem Vollgehalt die Wohlgestalt entsprungen.", "tokens": ["Wenn", "aus", "dem", "Voll\u00b7ge\u00b7halt", "die", "Wohl\u00b7ge\u00b7stalt", "ent\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}