{"textgrid.poem.41532": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Montage in der Karwoche", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie stehst du doch so d\u00fcrr und kahl,", "tokens": ["Wie", "stehst", "du", "doch", "so", "d\u00fcrr", "und", "kahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die trocknen Adern leer,", "tokens": ["Die", "trock\u00b7nen", "A\u00b7dern", "leer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Feigenbaum!", "tokens": ["O", "Fei\u00b7gen\u00b7baum", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ein Totenkranz von Bl\u00e4ttern fahl", "tokens": ["Ein", "To\u00b7ten\u00b7kranz", "von", "Bl\u00e4t\u00b7tern", "fahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00e4ngt rasselnd um dich her,", "tokens": ["H\u00e4ngt", "ras\u00b7selnd", "um", "dich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie Wellenschaum;", "tokens": ["Wie", "Wel\u00b7len\u00b7schaum", ";"], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "O Mensch! ich mu\u00df hier stehn, ich mu\u00df", "tokens": ["O", "Mensch", "!", "ich", "mu\u00df", "hier", "stehn", ",", "ich", "mu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "PPER", "VMFIN", "ADV", "VVINF", "$,", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dich gr\u00fc\u00dfen mit dem Todesgru\u00df,", "tokens": ["Dich", "gr\u00fc\u00b7\u00dfen", "mit", "dem", "To\u00b7des\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df du das Leben fassest,", "tokens": ["Da\u00df", "du", "das", "Le\u00b7ben", "fas\u00b7sest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Es nicht entlassest.", "tokens": ["Es", "nicht", "ent\u00b7las\u00b7sest", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wie halt' ich denn das Leben fest,", "tokens": ["Wie", "halt'", "ich", "denn", "das", "Le\u00b7ben", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df es mir nicht entrinnt,", "tokens": ["Da\u00df", "es", "mir", "nicht", "ent\u00b7rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Feigenbaum!", "tokens": ["O", "Fei\u00b7gen\u00b7baum", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "O Mensch! der Wille ist das Best',", "tokens": ["O", "Mensch", "!", "der", "Wil\u00b7le", "ist", "das", "Best'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die wahre Treu' gewinnt.", "tokens": ["Die", "wah\u00b7re", "Treu'", "ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "H\u00e4ltst du im Zaum", "tokens": ["H\u00e4ltst", "du", "im", "Zaum"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Die Hoffart und die Zweifelsucht,", "tokens": ["Die", "Hof\u00b7fart", "und", "die", "Zwei\u00b7fel\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die Lauheit auch in guter Zucht:", "tokens": ["Die", "Lau\u00b7heit", "auch", "in", "gu\u00b7ter", "Zucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mu\u00df dir in diesem Treiben", "tokens": ["Mu\u00df", "dir", "in", "die\u00b7sem", "Trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Das Leben bleiben.", "tokens": ["Das", "Le\u00b7ben", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wie bist du denn so v\u00f6llig tot,", "tokens": ["Wie", "bist", "du", "denn", "so", "v\u00f6l\u00b7lig", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So ganz und gar dahin,", "tokens": ["So", "ganz", "und", "gar", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Feigenbaum!", "tokens": ["O", "Fei\u00b7gen\u00b7baum", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "O Mensch! wie \u00fcpp'ges Morgenrot", "tokens": ["O", "Mensch", "!", "wie", "\u00fcpp'\u00b7ges", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lie\u00df ich mein Leben ziehn", "tokens": ["Lie\u00df", "ich", "mein", "Le\u00b7ben", "ziehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Am Erdensaum,", "tokens": ["Am", "Er\u00b7den\u00b7saum", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und weh! und dachte nicht der Frucht.", "tokens": ["Und", "weh", "!", "und", "dach\u00b7te", "nicht", "der", "Frucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$.", "KON", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da hat mich Gott der Herr verflucht,", "tokens": ["Da", "hat", "mich", "Gott", "der", "Herr", "ver\u00b7flucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df ich mu\u00df allem Leben", "tokens": ["Da\u00df", "ich", "mu\u00df", "al\u00b7lem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "PIS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ein Zeugnis geben.", "tokens": ["Ein", "Zeug\u00b7nis", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Wer hat dir solches zubereit'", "tokens": ["Wer", "hat", "dir", "sol\u00b7ches", "zu\u00b7be\u00b7reit'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch heimlichen Verrat,", "tokens": ["Durch", "heim\u00b7li\u00b7chen", "Ver\u00b7rat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Feigenbaum!", "tokens": ["O", "Fei\u00b7gen\u00b7baum", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "O Mensch! des Herren Aug' sieht weit,", "tokens": ["O", "Mensch", "!", "des", "Her\u00b7ren", "Aug'", "sieht", "weit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "NN", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es sieht des W\u00fcrmleins Pfad", "tokens": ["Es", "sieht", "des", "W\u00fcrm\u00b7leins", "Pfad"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In Blattes Flaum;", "tokens": ["In", "Blat\u00b7tes", "Flaum", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ihm kannst du nichts entdecken, noch", "tokens": ["Ihm", "kannst", "du", "nichts", "ent\u00b7de\u00b7cken", ",", "noch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "VVINF", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Entziehn, er sieht und wei\u00df es doch;", "tokens": ["Ent\u00b7ziehn", ",", "er", "sieht", "und", "wei\u00df", "es", "doch", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Es lag schon auf der Waage", "tokens": ["Es", "lag", "schon", "auf", "der", "Waa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Am ersten Tage.", "tokens": ["Am", "ers\u00b7ten", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Du starbest wohl vor langer Zeit,", "tokens": ["Du", "star\u00b7best", "wohl", "vor", "lan\u00b7ger", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil du so d\u00fcrr und leer,", "tokens": ["Weil", "du", "so", "d\u00fcrr", "und", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Feigenbaum!", "tokens": ["O", "Fei\u00b7gen\u00b7baum", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "O Mensch! des Herren Hand reicht weit,", "tokens": ["O", "Mensch", "!", "des", "Her\u00b7ren", "Hand", "reicht", "weit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "NN", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ist so schnell und schwer,", "tokens": ["Und", "ist", "so", "schnell", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Du siehst es kaum.", "tokens": ["Du", "siehst", "es", "kaum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Er nimmt dir seines Lebens Hauch:", "tokens": ["Er", "nimmt", "dir", "sei\u00b7nes", "Le\u00b7bens", "Hauch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Du mu\u00dft vergehn wie Dunst und Rauch;", "tokens": ["Du", "mu\u00dft", "ver\u00b7gehn", "wie", "Dunst", "und", "Rauch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er braucht nicht Wort noch Stunden,", "tokens": ["Er", "braucht", "nicht", "Wort", "noch", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Du bist verschwunden.", "tokens": ["Du", "bist", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Wo bleibt denn seine gro\u00dfe Huld,", "tokens": ["Wo", "bleibt", "denn", "sei\u00b7ne", "gro\u00b7\u00dfe", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was fruchtet denn die Reu'", "tokens": ["Was", "fruch\u00b7tet", "denn", "die", "Reu'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Feigenbaum!", "tokens": ["O", "Fei\u00b7gen\u00b7baum", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "O Mensch! gedenk an deine Schuld,", "tokens": ["O", "Mensch", "!", "ge\u00b7denk", "an", "dei\u00b7ne", "Schuld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gedenk an seine Treu'.", "tokens": ["Ge\u00b7denk", "an", "sei\u00b7ne", "Treu'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Schau, in den Raum", "tokens": ["Schau", ",", "in", "den", "Raum"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Hat er mich gnadenvoll gestellt,", "tokens": ["Hat", "er", "mich", "gna\u00b7den\u00b7voll", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df ich durch seine weite Welt", "tokens": ["Da\u00df", "ich", "durch", "sei\u00b7ne", "wei\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Aus meines Elends Tiefe", "tokens": ["Aus", "mei\u00b7nes", "E\u00b7lends", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Dir warnend riefe.", "tokens": ["Dir", "war\u00b7nend", "rie\u00b7fe", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Steht denn kein Hoffen mehr bei dir,", "tokens": ["Steht", "denn", "kein", "Hof\u00b7fen", "mehr", "bei", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Hoffen in der Not,", "tokens": ["Kein", "Hof\u00b7fen", "in", "der", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Feigenbaum!", "tokens": ["O", "Fei\u00b7gen\u00b7baum", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "O Mensch! kein Hoff