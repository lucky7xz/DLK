{"dta.poem.14957": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Von Engeln und von Bengeln.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.71", "af:0.28"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Den andern war's, wenn die Aue", "tokens": ["Den", "an\u00b7dern", "wa\u00b7r's", ",", "wenn", "die", "Au\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "VAFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Noch d\u00e4mmert' im Fr\u00fchlingsschein,", "tokens": ["Noch", "d\u00e4m\u00b7mert'", "im", "Fr\u00fch\u00b7lings\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als z\u00f6ge ein Engel durch's Blaue", "tokens": ["Als", "z\u00f6\u00b7ge", "ein", "En\u00b7gel", "durch's", "Blau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und rief' die Gesellen sein.", "tokens": ["Und", "rie\u00b7f'", "die", "Ge\u00b7sel\u00b7len", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}