{"textgrid.poem.24553": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen Freund als er disputirte/ in Nahmen eines andern", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein munteres Gem\u00fcth/ ein aufgeweckter Geist", "tokens": ["Ein", "mun\u00b7te\u00b7res", "Ge\u00b7m\u00fcth", "/", "ein", "auf\u00b7ge\u00b7weck\u00b7ter", "Geist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Liebt bey den B\u00fcchern auch wohl einen guten Degen;", "tokens": ["Liebt", "bey", "den", "B\u00fc\u00b7chern", "auch", "wohl", "ei\u00b7nen", "gu\u00b7ten", "De\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein wohl beritten Pferd: weil beydes l\u00f6blich hei\u00dft.", "tokens": ["Ein", "wohl", "be\u00b7rit\u00b7ten", "Pferd", ":", "weil", "bey\u00b7des", "l\u00f6b\u00b7lich", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$.", "KOUS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bi\u00dfhero hast du nun den dreyen obgelegen.", "tokens": ["Bi\u00df\u00b7he\u00b7ro", "hast", "du", "nun", "den", "drey\u00b7en", "ob\u00b7ge\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "CARD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "meas