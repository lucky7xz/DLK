{"textgrid.poem.51050": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ob sie geschehn? Das ist hier nicht zu fragen;", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob sie geschehn? Das ist hier nicht zu fragen;", "tokens": ["Ob", "sie", "ge\u00b7schehn", "?", "Das", "ist", "hier", "nicht", "zu", "fra\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$.", "PDS", "VAFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Perle jeder Fabel ist der Sinn,", "tokens": ["Die", "Per\u00b7le", "je\u00b7der", "Fa\u00b7bel", "ist", "der", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Mark der Wahrheit ruht hier frisch darin,", "tokens": ["Das", "Mark", "der", "Wahr\u00b7heit", "ruht", "hier", "frisch", "da\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der reife Kern von allen V\u00f6lkersagen.", "tokens": ["Der", "rei\u00b7fe", "Kern", "von", "al\u00b7len", "V\u00f6l\u00b7ker\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es war der erste Schu\u00df ein Alleswagen,", "tokens": ["Es", "war", "der", "ers\u00b7te", "Schu\u00df", "ein", "Al\u00b7les\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kind, Leib und Gut, an k\u00f6stlichen Gewinn:", "tokens": ["Kind", ",", "Leib", "und", "Gut", ",", "an", "k\u00f6st\u00b7li\u00b7chen", "Ge\u00b7winn", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJD", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbblick her, Tyrann! was ich nur hab und bin,", "tokens": ["\u00bb", "blick", "her", ",", "Ty\u00b7rann", "!", "was", "ich", "nur", "hab", "und", "bin", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "$,", "NE", "$.", "PWS", "PPER", "ADV", "VAFIN", "KON", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Will ich beim ersten in die Schanze schlagen!", "tokens": ["Will", "ich", "beim", "ers\u00b7ten", "in", "die", "Schan\u00b7ze", "schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "ADJA", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und du stehst leer und heillos, wie du bist,", "tokens": ["Und", "du", "stehst", "leer", "und", "heil\u00b7los", ",", "wie", "du", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und l\u00e4ssest f\u00fchllos dir am Herzen r\u00fctteln", "tokens": ["