{"dta.poem.19962": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Auf der Wanderung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Morgen kommt mit lichtem Grusse,", "tokens": ["Mor\u00b7gen", "kommt", "mit", "lich\u00b7tem", "Grus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und Natur beginnt ein Fest.", "tokens": ["Und", "Na\u00b7tur", "be\u00b7ginnt", "ein", "Fest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mancher noch mit hei\u00dfem Kusse", "tokens": ["Man\u00b7cher", "noch", "mit", "hei\u00b7\u00dfem", "Kus\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An das Herz was Liebes pre\u00dft.", "tokens": ["An", "das", "Herz", "was", "Lie\u00b7bes", "pre\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PWS", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber irre und verlassen", "tokens": ["A\u00b7ber", "ir\u00b7re", "und", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treibt es mich durch Land und Meer.", "tokens": ["Treibt", "es", "mich", "durch", "Land", "und", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ich innig m\u00f6cht' umfassen", "tokens": ["Was", "ich", "in\u00b7nig", "m\u00f6cht'", "um\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADJD", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchrt nicht Mond nicht Sonne her.", "tokens": ["F\u00fchrt", "nicht", "Mond", "nicht", "Son\u00b7ne", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "NN", "PTKNEG", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In der Blume seh' ichs bl\u00fchen,", "tokens": ["In", "der", "Blu\u00b7me", "seh'", "ichs", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6r's im Nachtigall-Gesang,", "tokens": ["H\u00f6r's", "im", "Nach\u00b7ti\u00b7gall\u00b7Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den Sternen seh' ichs ziehen", "tokens": ["Mit", "den", "Ster\u00b7nen", "seh'", "ichs", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Still und mild das Thal entlang.", "tokens": ["Still", "und", "mild", "das", "Thal", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch umsonst blickt voll von Thr\u00e4nen", "tokens": ["Doch", "um\u00b7sonst", "blickt", "voll", "von", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "APPR", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Auge nach ihm himmelw\u00e4rts.", "tokens": ["Au\u00b7ge", "nach", "ihm", "him\u00b7mel\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Ungestillt in bangem Sehnen", "tokens": ["Un\u00b7ge\u00b7stillt", "in", "ban\u00b7gem", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stirbt dahin die\u00df warme Herz.", "tokens": ["Stirbt", "da\u00b7hin", "die\u00df", "war\u00b7me", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "PDS", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}