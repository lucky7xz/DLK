{"dta.poem.6143": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Subtilen Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da zwischen C\u00f6rpern nun ein Leer unm\u00fcglich;", "tokens": ["Da", "zwi\u00b7schen", "C\u00f6r\u00b7pern", "nun", "ein", "Leer", "un\u00b7m\u00fcg\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So w\u00fcrckt ein Stoff, der nicht in unsre Sinne dringt,", "tokens": ["So", "w\u00fcrckt", "ein", "Stoff", ",", "der", "nicht", "in", "uns\u00b7re", "Sin\u00b7ne", "dringt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und welchen die Bewegung doch gar f\u00fcglich", "tokens": ["Und", "wel\u00b7chen", "die", "Be\u00b7we\u00b7gung", "doch", "gar", "f\u00fcg\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAT", "ART", "NN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An alle Orte bringt;", "tokens": ["An", "al\u00b7le", "Or\u00b7te", "bringt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die wunderbaren Ding\u2019, die unsre Augen", "tokens": ["Die", "wun\u00b7der\u00b7ba\u00b7ren", "Ding'", ",", "die", "uns\u00b7re", "Au\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nicht gnugsam zu bewundern taugen.", "tokens": ["Nicht", "gnug\u00b7sam", "zu", "be\u00b7wun\u00b7dern", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Furcht des ", "tokens": ["Die", "Furcht", "des"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Indem uns die Erfahrung lehret,", "tokens": ["In\u00b7dem", "uns", "die", "Er\u00b7fah\u00b7rung", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df alle C\u00f6rper hol durchr\u00f6hret,", "tokens": ["Da\u00df", "al\u00b7le", "C\u00f6r\u00b7per", "hol", "durch\u00b7r\u00f6h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und von dem Himmels-Stoff, der so gar d\u00fcun und fein,", "tokens": ["Und", "von", "dem", "Him\u00b7mels\u00b7Stoff", ",", "der", "so", "gar", "d\u00fcun", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auch stets bereit sie durchzudringen;", "tokens": ["Auch", "stets", "be\u00b7reit", "sie", "durch\u00b7zu\u00b7drin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Gar leichtlich durchzudringen seyn.", "tokens": ["Gar", "leicht\u00b7lich", "durch\u00b7zu\u00b7drin\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}