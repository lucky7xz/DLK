{"dta.poem.10658": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma Ovveni.  \n  An die so sich schmincken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Die jhr mit Farb anstreicht euch ewre Zierlichkeit/", "tokens": ["Die", "jhr", "mit", "Farb", "an\u00b7streicht", "euch", "ew\u00b7re", "Zier\u00b7lich\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bekennet recht/ da\u00df jhr nur Staub vnd Asche seid.", "tokens": ["Be\u00b7ken\u00b7net", "recht", "/", "da\u00df", "jhr", "nur", "Staub", "vnd", "A\u00b7sche", "seid", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$(", "KOUS", "PPER", "ADV", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}