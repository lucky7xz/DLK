{"dta.poem.14304": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Doch steht ein Thurm dort \u00fcber dem Gew\u00fchle,", "tokens": ["Doch", "steht", "ein", "Thurm", "dort", "\u00fc\u00b7ber", "dem", "Ge\u00b7w\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der andre Zeiten sah und bess're Br\u00fcder,", "tokens": ["Der", "and\u00b7re", "Zei\u00b7ten", "sah", "und", "bess'\u00b7re", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Kreuz treu halten seine Riesenglieder,", "tokens": ["Das", "Kreuz", "treu", "hal\u00b7ten", "sei\u00b7ne", "Rie\u00b7sen\u00b7glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie auch der Menschlein Fluth den Fels umsp\u00fcle.", "tokens": ["Wie", "auch", "der", "Mensc\u00b7hlein", "Fluth", "den", "Fels", "um\u00b7sp\u00fc\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}