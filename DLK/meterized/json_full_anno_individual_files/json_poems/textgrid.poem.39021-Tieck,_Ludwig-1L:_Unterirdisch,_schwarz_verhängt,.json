{"textgrid.poem.39021": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unterirdisch, schwarz verh\u00e4ngt,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unterirdisch, schwarz verh\u00e4ngt,", "tokens": ["Un\u00b7ter\u00b7ir\u00b7disch", ",", "schwarz", "ver\u00b7h\u00e4ngt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur von wen'gen Lichtern hell,", "tokens": ["Nur", "von", "wen'\u00b7gen", "Lich\u00b7tern", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist rings der d\u00fcstre Dom von Todtensch\u00e4deln,", "tokens": ["Ist", "rings", "der", "d\u00fcst\u00b7re", "Dom", "von", "Tod\u00b7ten\u00b7sch\u00e4\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gerippen und allem Graus erf\u00fcllt, \u2013", "tokens": ["Ge\u00b7rip\u00b7pen", "und", "al\u00b7lem", "Graus", "er\u00b7f\u00fcllt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "PIS", "NN", "VVPP", "$,", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hier redet begeistert von der Vernichtung,", "tokens": ["Hier", "re\u00b7det", "be\u00b7geis\u00b7tert", "von", "der", "Ver\u00b7nich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure":