{"textgrid.poem.65783": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "57", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Sklave, der ich bin, wie w\u00e4r' ich freier,", "tokens": ["Dein", "Skla\u00b7ve", ",", "der", "ich", "bin", ",", "wie", "w\u00e4r'", "ich", "frei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als wenn ich Stund' und Zeit wahrnehme, die du liebst?", "tokens": ["Als", "wenn", "ich", "Stund'", "und", "Zeit", "wahr\u00b7neh\u00b7me", ",", "die", "du", "liebst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sonst acht' ich keinen Dienst und keine Stunde teuer,", "tokens": ["Sonst", "acht'", "ich", "kei\u00b7nen", "Dienst", "und", "kei\u00b7ne", "Stun\u00b7de", "teu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "KON", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als wenn du etwas mir zu dienen gibst:", "tokens": ["Als", "wenn", "du", "et\u00b7was", "mir", "zu", "die\u00b7nen", "gibst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PIS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Noch wag' ich, Stund' auf Stund' am Seiger nach dir z\u00e4hlend,", "tokens": ["Noch", "wag'", "ich", ",", "Stund'", "auf", "Stund'", "am", "Sei\u00b7ger", "nach", "dir", "z\u00e4h\u00b7lend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "APPR", "NN", "APPRART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein F\u00fcrst, die endlos lange Zeit zu schm\u00e4hn;", "tokens": ["Mein", "F\u00fcrst", ",", "die", "end\u00b7los", "lan\u00b7ge", "Zeit", "zu", "schm\u00e4hn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Trennung Bitterkeiten mir verhehlend,", "tokens": ["Der", "Tren\u00b7nung", "Bit\u00b7ter\u00b7kei\u00b7ten", "mir", "ver\u00b7heh\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wenn scheidend dich dein Knecht nur einmal gr\u00fc\u00dfen sehn:", "tokens": ["Wenn", "schei\u00b7dend", "dich", "dein", "Knecht", "nur", "ein\u00b7mal", "gr\u00fc\u00b7\u00dfen", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Noch gr\u00fcbl' ich eifers\u00fcchtig nach der Spur,", "tokens": ["Noch", "gr\u00fcbl'", "ich", "ei\u00b7fer\u00b7s\u00fcch\u00b7tig", "nach", "der", "Spur", ","], "token_info": ["word", "word", "word", "word", "w