{"dta.poem.6558": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4tt' ihn von Jugend an die Kranckheit eingenommen;", "tokens": ["H\u00e4tt'", "ihn", "von", "Ju\u00b7gend", "an", "die", "Kran\u00b7ck\u00b7heit", "ein\u00b7ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So w\u00e4r ihm alles gelb auf Erden vorgekommen:", "tokens": ["So", "w\u00e4r", "ihm", "al\u00b7les", "gelb", "auf", "Er\u00b7den", "vor\u00b7ge\u00b7kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wir, die wir gesund, da von der Jugend an", "tokens": ["Und", "wir", ",", "die", "wir", "ge\u00b7sund", ",", "da", "von", "der", "Ju\u00b7gend", "an"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "PPER", "ADJD", "$,", "KOUS", "APPR", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Farben blo\u00df bey uns beurtheilt seyn,", "tokens": ["Die", "Far\u00b7ben", "blo\u00df", "bey", "uns", "beurt\u00b7heilt", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So stimmen wir darin mit niemand \u00fcberein.", "tokens": ["So", "stim\u00b7men", "wir", "da\u00b7rin", "mit", "nie\u00b7mand", "\u00fc\u00b7be\u00b7re\u00b7in", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "APPR", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Der Werckzeug Unterscheid erregt bey jedermann", "tokens": ["Der", "Wer\u00b7ck\u00b7zeug", "Un\u00b7ter\u00b7scheid", "er\u00b7regt", "bey", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVPP", "APPR", "PIS"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Die Mannigfaltigkeit, die man kaum z\u00e4hlen kan.", "tokens": ["Die", "Man\u00b7nig\u00b7fal\u00b7tig\u00b7keit", ",", "die", "man", "kaum", "z\u00e4h\u00b7len", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In Farben hat uns nichts vereinen k\u00f6nnen,", "tokens": ["In", "Far\u00b7ben", "hat", "uns", "nichts", "ver\u00b7ei\u00b7nen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PIS", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Als da\u00df wir sie auf eine Weise nennen.", "tokens": ["Als", "da\u00df", "wir", "sie", "auf", "ei\u00b7ne", "Wei\u00b7se", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die Menschen werden wol auf Erden", "tokens": ["Die", "Men\u00b7schen", "wer\u00b7den", "wol", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "In diesen Punct nicht leicht vereinigt werden:", "tokens": ["In", "die\u00b7sen", "Punct", "nicht", "leicht", "ver\u00b7ei\u00b7nigt", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKNEG", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ob sie sich gleich bestreben und befleissen", "tokens": ["Ob", "sie", "sich", "gleich", "be\u00b7stre\u00b7ben", "und", "be\u00b7fleis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die Dinge ", "tokens": ["Die", "Din\u00b7ge"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}}}}}