{"dta.poem.3267": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Chloen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Weis Chloe mein geheim Verlangen?", "tokens": ["Weis", "Chloe", "mein", "ge\u00b7heim", "Ver\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Verrieth mein Auge mich vielleicht,", "tokens": ["Ver\u00b7rieth", "mein", "Au\u00b7ge", "mich", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das nach den Rosen ihrer Wangen", "tokens": ["Das", "nach", "den", "Ro\u00b7sen", "ih\u00b7rer", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch manchen Umweg l\u00fcstern schleicht?", "tokens": ["Durch", "man\u00b7chen", "Um\u00b7weg", "l\u00fcs\u00b7tern", "schleicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jhr Blick begegnet meinem Blicke:", "tokens": ["Ihr", "Blick", "be\u00b7geg\u00b7net", "mei\u00b7nem", "Bli\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jhr Auge sieht mich schalkhaft an,", "tokens": ["Ihr", "Au\u00b7ge", "sieht", "mich", "schalk\u00b7haft", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Oft nur im Flug und schnell zur\u00fccke;", "tokens": ["Oft", "nur", "im", "Flug", "und", "schnell", "zu\u00b7r\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch da\u00df ich es bemerken kann.", "tokens": ["Doch", "da\u00df", "ich", "es", "be\u00b7mer\u00b7ken", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Oft blitzen, von Gefahr begleitet,", "tokens": ["Oft", "blit\u00b7zen", ",", "von", "Ge\u00b7fahr", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die blauen Augen frey auf mich,", "tokens": ["Die", "blau\u00b7en", "Au\u00b7gen", "frey", "auf", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus welchen Amor mich bestreitet,", "tokens": ["Aus", "wel\u00b7chen", "A\u00b7mor", "mich", "be\u00b7strei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der stets aus ihnen siegreich wich.", "tokens": ["Der", "stets", "aus", "ih\u00b7nen", "sieg\u00b7reich", "wich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich kann die Grazien darinnen", "tokens": ["Ich", "kann", "die", "Gra\u00b7zi\u00b7en", "da\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein schmeichelnd L\u00e4cheln bilden sehn:", "tokens": ["Ein", "schmei\u00b7chelnd", "L\u00e4\u00b7cheln", "bil\u00b7den", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das \u00fcberraschet meine Sinnen;", "tokens": ["Das", "\u00fc\u00b7berr\u00b7a\u00b7schet", "mei\u00b7ne", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie kann das Herz ihm widerstehn?", "tokens": ["Wie", "kann", "das", "Herz", "ihm", "wi\u00b7der\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Kein Schnee gleicht ihres Armes Weisse,", "tokens": ["Kein", "Schnee", "gleicht", "ih\u00b7res", "Ar\u00b7mes", "Weis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der vor dem Fenster in der Luft,", "tokens": ["Der", "vor", "dem", "Fens\u00b7ter", "in", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit einem ungewohnten Fleisse,", "tokens": ["Mit", "ei\u00b7nem", "un\u00b7ge\u00b7wohn\u00b7ten", "Fleis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So sinnreich meiner Sehnsucht ruft!", "tokens": ["So", "sinn\u00b7reich", "mei\u00b7ner", "Sehn\u00b7sucht", "ruft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun schaut sie r\u00fcckw\u00e4rts, doch gestrecket,", "tokens": ["Nun", "schaut", "sie", "r\u00fcck\u00b7w\u00e4rts", ",", "doch", "ge\u00b7stre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis sich die volle Brust emp\u00f6rt,", "tokens": ["Bis", "sich", "die", "vol\u00b7le", "Brust", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und halb entwischt, und, unverdecket,", "tokens": ["Und", "halb", "ent\u00b7wischt", ",", "und", ",", "un\u00b7ver\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,", "KON", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auch eines Cato Runzeln st\u00f6rt.", "tokens": ["Auch", "ei\u00b7nes", "Ca\u00b7to", "Run\u00b7zeln", "st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich aber steh und strampf und gl\u00fche,", "tokens": ["Ich", "a\u00b7ber", "steh", "und", "strampf", "und", "gl\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "KON", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Flieg in Gedancken hin zu ihr,", "tokens": ["Flieg", "in", "Ge\u00b7dan\u00b7cken", "hin", "zu", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sehe, mit verlohrner M\u00fche,", "tokens": ["Und", "se\u00b7he", ",", "mit", "ver\u00b7lohr\u00b7ner", "M\u00fc\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich unst\u00e4t, aber immer hier:", "tokens": ["Mich", "un\u00b7st\u00e4t", ",", "a\u00b7ber", "im\u00b7mer", "hier", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil, bis mich Gl\u00fcck und Freundschaft retten,", "tokens": ["Weil", ",", "bis", "mich", "Gl\u00fcck", "und", "Freund\u00b7schaft", "ret\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die oft ein langer Schlaf bef\u00e4llt;", "tokens": ["Die", "oft", "ein", "lan\u00b7ger", "Schlaf", "be\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mich hier mit diamantnen Ketten", "tokens": ["Mich", "hier", "mit", "di\u00b7a\u00b7mant\u00b7nen", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das Schicksal angefesselt h\u00e4lt.", "tokens": ["Das", "Schick\u00b7sal", "an\u00b7ge\u00b7fes\u00b7selt", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}