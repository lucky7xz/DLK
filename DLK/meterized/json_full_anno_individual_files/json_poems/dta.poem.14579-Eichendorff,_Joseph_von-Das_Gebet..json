{"dta.poem.14579": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das Gebet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es breitet diese einz'ge Stunde", "tokens": ["Es", "brei\u00b7tet", "die\u00b7se", "einz'\u00b7ge", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich \u00fcber's ganze Leben still,", "tokens": ["Sich", "\u00fc\u00b7ber's", "gan\u00b7ze", "Le\u00b7ben", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Legt bl\u00fchend sich um Deine Wunde,", "tokens": ["Legt", "bl\u00fc\u00b7hend", "sich", "um", "Dei\u00b7ne", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die niemals wieder heilen will.", "tokens": ["Die", "nie\u00b7mals", "wie\u00b7der", "hei\u00b7len", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Treu bleibt der Himmel stets dem Treuen,", "tokens": ["Treu", "bleibt", "der", "Him\u00b7mel", "stets", "dem", "Treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zur Erd' das Ird'sche niedergeht,", "tokens": ["Zur", "Erd'", "das", "Ird'\u00b7sche", "nie\u00b7der\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zum Himmel \u00fcber Zaubereien", "tokens": ["Zum", "Him\u00b7mel", "\u00fc\u00b7ber", "Zau\u00b7be\u00b7rei\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Geht ewig siegreich das Gebet.", "tokens": ["Geht", "e\u00b7wig", "sieg\u00b7reich", "das", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}