{"textgrid.poem.43888": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Kind, was zweifelstu an meiner Redligkeit,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Kind, was zweifelstu an meiner Redligkeit,", "tokens": ["Mein", "Kind", ",", "was", "zwei\u00b7fel\u00b7stu", "an", "mei\u00b7ner", "Red\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihres gleichen doch in deiner Brust versp\u00fcret?", "tokens": ["Die", "ih\u00b7res", "glei\u00b7chen", "doch", "in", "dei\u00b7ner", "Brust", "ver\u00b7sp\u00fc\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo meiner Adern Blut nur einen Tropfen f\u00fchret,", "tokens": ["Wo", "mei\u00b7ner", "A\u00b7dern", "Blut", "nur", "ei\u00b7nen", "Trop\u00b7fen", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der sich nicht tausendmahl vor dich zu sterben freut,", "tokens": ["Der", "sich", "nicht", "tau\u00b7send\u00b7mahl", "vor", "dich", "zu", "ster\u00b7ben", "freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PTKNEG", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So w\u00fcntsch ich ihm den Fluch, den Ebals Felsen dr\u00e4ut", "tokens": ["So", "w\u00fcnt\u00b7sch", "ich", "ihm", "den", "Fluch", ",", "den", "E\u00b7bals", "Fel\u00b7sen", "dr\u00e4ut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,", "ART", "NN", "NN", "VVFIN"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Und Cains Fu\u00df erf\u00e4hrt; der Stern, so mich regieret,", "tokens": ["Und", "Cains", "Fu\u00df", "er\u00b7f\u00e4hrt", ";", "der", "Stern", ",", "so", "mich", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VVFIN", "$.", "ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Und de\u00dfen Trieb in mir die reine Glut gebiehret,", "tokens": ["Und", "de\u00b7\u00dfen", "Trieb", "in", "mir", "die", "rei\u00b7ne", "Glut", "ge\u00b7bieh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Folgt nicht wie ein Planet dem Wechsel dieser Zeit.", "tokens": ["Folgt", "nicht", "wie", "ein", "Pla\u00b7net", "dem", "Wech\u00b7sel", "die\u00b7ser", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "KOKOM", "ART", "NN", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Mein Sinnbild ist ein Ring, der Denckspruch: Sonder Ende;", "tokens": ["Mein", "Sinn\u00b7bild", "ist", "ein", "Ring", ",", "der", "Denck\u00b7spruch", ":", "Son\u00b7der", "En\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$.", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn wer nicht ewig liebt, der liebet nimmermehr.", "tokens": ["Denn", "wer", "nicht", "e\u00b7wig", "liebt", ",", "der", "lie\u00b7bet", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "ADJD", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Engel, giebstu nun dem Argwohn kein Geh\u00f6r,", "tokens": ["Mein", "En\u00b7gel", ",", "giebs\u00b7tu", "nun", "dem", "Arg\u00b7wohn", "kein", "Ge\u00b7h\u00f6r", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So lege mir dein Herz in die getreuen H\u00e4nde.", "tokens": ["So", "le\u00b7ge", "mir", "dein", "Herz", "in", "die", "ge\u00b7treu\u00b7en", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich sichre, diesen Schaz wird deinem Saladin", "tokens": ["Ich", "sich\u00b7re", ",", "die\u00b7sen", "Schaz", "wird", "dei\u00b7nem", "Sa\u00b7la\u00b7din"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "P