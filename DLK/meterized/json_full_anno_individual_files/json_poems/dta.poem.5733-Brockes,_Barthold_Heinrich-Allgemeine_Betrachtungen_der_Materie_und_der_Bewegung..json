{"dta.poem.5733": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Allgemeine Betrachtungen der Materie  \n und der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man hat auch bey der alten Welt", "tokens": ["Man", "hat", "auch", "bey", "der", "al\u00b7ten", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jhn unter ", "tokens": ["Jhn", "un\u00b7ter"], "token_info": ["word", "word"], "pos": ["PPER", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Von welchem die Poeten viel gesungen,", "tokens": ["Von", "wel\u00b7chem", "die", "Po\u00b7et\u00b7en", "viel", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df er best\u00e4ndige Gestallten", "tokens": ["Da\u00df", "er", "be\u00b7st\u00e4n\u00b7di\u00b7ge", "Ge\u00b7stall\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nie habe lange Zeit behalten,", "tokens": ["Nie", "ha\u00b7be", "lan\u00b7ge", "Zeit", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wol aber aus den Banden, die ihn drungen,", "tokens": ["Wol", "a\u00b7ber", "aus", "den", "Ban\u00b7den", ",", "die", "ihn", "drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PAV", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Sich selbst befreyt und weggeschwungen.", "tokens": ["Sich", "selbst", "be\u00b7freyt", "und", "weg\u00b7ge\u00b7schwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bald breitet er sich aus im Flu\u00df, wird eine Fluht;", "tokens": ["Bald", "brei\u00b7tet", "er", "sich", "aus", "im", "Flu\u00df", ",", "wird", "ei\u00b7ne", "Fluht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "APPRART", "NN", "$,", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bald wird er zur verzehrnden Gluht;", "tokens": ["Bald", "wird", "er", "zur", "ver\u00b7zehrn\u00b7den", "Gluht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bald zischt er in der Lufft, ergrimmten Schlangen gleich;", "tokens": ["Bald", "zischt", "er", "in", "der", "Lufft", ",", "er\u00b7grimm\u00b7ten", "Schlan\u00b7gen", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bald ist er hart Metall; bald in den Pflanzen weich;", "tokens": ["Bald", "ist", "er", "hart", "Me\u00b7tall", ";", "bald", "in", "den", "Pflan\u00b7zen", "weich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "NN", "$.", "ADV", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bis da\u00df er wiederum, ersch\u00f6pfft von Aenderung,", "tokens": ["Bis", "da\u00df", "er", "wie\u00b7de\u00b7rum", ",", "er\u00b7sch\u00f6pfft", "von", "A\u00b7en\u00b7de\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "In seiner ersten Form gantz fertig und geneiget", "tokens": ["In", "sei\u00b7ner", "ers\u00b7ten", "Form", "gantz", "fer\u00b7tig", "und", "ge\u00b7nei\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Zu abermahliger Verwandelung,", "tokens": ["Zu", "a\u00b7ber\u00b7mah\u00b7li\u00b7ger", "Ver\u00b7wan\u00b7de\u00b7lung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Sich unsern Augen wieder zeiget.", "tokens": ["Sich", "un\u00b7sern", "Au\u00b7gen", "wie\u00b7der", "zei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}