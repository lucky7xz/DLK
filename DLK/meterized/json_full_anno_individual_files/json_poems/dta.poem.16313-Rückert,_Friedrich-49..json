{"dta.poem.16313": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Restchen Leben ist wie das Zigarrenendchen,", "tokens": ["Das", "Rest\u00b7chen", "Le\u00b7ben", "ist", "wie", "das", "Zi\u00b7gar\u00b7re\u00b7nend\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je n\u00e4her schon am Mund, je duftiger das Br\u00e4ndchen.", "tokens": ["Je", "n\u00e4\u00b7her", "schon", "am", "Mund", ",", "je", "duf\u00b7ti\u00b7ger", "das", "Br\u00e4nd\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPRART", "NN", "$,", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du m\u00f6chtest mit der Lipp' es erst recht scharf nun fassen,", "tokens": ["Du", "m\u00f6ch\u00b7test", "mit", "der", "Lipp'", "es", "erst", "recht", "scharf", "nun", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "PPER", "ADV", "ADV", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein es brennt am Mund, du mu\u00dft es fallen lassen.", "tokens": ["Al\u00b7lein", "es", "brennt", "am", "Mund", ",", "du", "mu\u00dft", "es", "fal\u00b7len", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos"