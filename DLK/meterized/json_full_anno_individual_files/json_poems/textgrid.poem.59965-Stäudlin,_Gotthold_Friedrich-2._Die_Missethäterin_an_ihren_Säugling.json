{"textgrid.poem.59965": {"metadata": {"author": {"name": "St\u00e4udlin, Gotthold Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2. Die Misseth\u00e4terin an ihren S\u00e4ugling", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O weh mir armen Mutter!", "tokens": ["O", "weh", "mir", "ar\u00b7men", "Mut\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O ungl\u00fcckselig Kind,", "tokens": ["O", "un\u00b7gl\u00fcck\u00b7se\u00b7lig", "Kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da\u00df in der Wehenstunde", "tokens": ["Da\u00df", "in", "der", "We\u00b7hen\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir nicht verschmachtet sind!", "tokens": ["Wir", "nicht", "ver\u00b7schmach\u00b7tet", "sind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O schlage nicht dein Auge", "tokens": ["O", "schla\u00b7ge", "nicht", "dein", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So froh zum Morgenrot!", "tokens": ["So", "froh", "zum", "Mor\u00b7gen\u00b7rot", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Weib, das dich geboren", "tokens": ["Das", "Weib", ",", "das", "dich", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "War ihres Gatten Tod!", "tokens": ["War", "ih\u00b7res", "Gat\u00b7ten", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O blicke nicht so suchend", "tokens": ["O", "bli\u00b7cke", "nicht", "so", "su\u00b7chend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus deiner Wieg' umher:", "tokens": ["Aus", "dei\u00b7ner", "Wieg'", "um\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den du so gierig suchest,", "tokens": ["Den", "du", "so", "gie\u00b7rig", "su\u00b7chest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Vater ist nicht mehr!", "tokens": ["Dein", "Va\u00b7ter", "ist", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er liebte fremde Dirnen", "tokens": ["Er", "lieb\u00b7te", "frem\u00b7de", "Dir\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Samt der verha\u00dften Brut,", "tokens": ["Samt", "der", "ver\u00b7ha\u00df\u00b7ten", "Brut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mehr als die Angetraute,", "tokens": ["Mehr", "als", "die", "An\u00b7ge\u00b7trau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mehr als sein eigen Blut;", "tokens": ["Mehr", "als", "sein", "ei\u00b7gen", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ach Gott! da \u00fcbermannte", "tokens": ["Ach", "Gott", "!", "da", "\u00fc\u00b7berm\u00b7ann\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ITJ", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich Eifersucht und Schmerz;", "tokens": ["Mich", "Ei\u00b7fer\u00b7sucht", "und", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dies blanke Messer stie\u00df ich", "tokens": ["Dies", "blan\u00b7ke", "Mes\u00b7ser", "stie\u00df", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Schlafenden ins Herz!", "tokens": ["Dem", "Schla\u00b7fen\u00b7den", "ins", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mit deines Vaters Blute", "tokens": ["Mit", "dei\u00b7nes", "Va\u00b7ters", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00e4rbt' ich dies Messer rot!", "tokens": ["F\u00e4rbt'", "ich", "dies", "Mes\u00b7ser", "rot", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit meinem Blute f\u00e4rb' ich", "tokens": ["Mit", "mei\u00b7nem", "Blu\u00b7te", "f\u00e4rb'", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Henkereisen rot!", "tokens": ["Das", "Hen\u00b7ker\u00b7ei\u00b7sen", "rot", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Neun lange Jammermonde", "tokens": ["Neun", "lan\u00b7ge", "Jam\u00b7mer\u00b7mon\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wardst du f\u00fcr Schmach und Not", "tokens": ["Wardst", "du", "f\u00fcr", "Schmach", "und", "Not"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In diesem Kerker reifer,", "tokens": ["In", "die\u00b7sem", "Ker\u00b7ker", "rei\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich aber f\u00fcr den Tod!", "tokens": ["Ich", "a\u00b7ber", "f\u00fcr", "den", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Noch eh' ich meinen Namen", "tokens": ["Noch", "eh'", "ich", "mei\u00b7nen", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich stammeln h\u00f6ren kann,", "tokens": ["Dich", "stam\u00b7meln", "h\u00f6\u00b7ren", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schleppt mich zum Blutgerichte", "tokens": ["Schleppt", "mich", "zum", "Blut\u00b7ge\u00b7rich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Rache schon hinan.", "tokens": ["Die", "Ra\u00b7che", "schon", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Bei Menschen, armes W\u00fcrmchen!", "tokens": ["Bei", "Men\u00b7schen", ",", "ar\u00b7mes", "W\u00fcrm\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lass' ich dich nun allein!", "tokens": ["Lass'", "ich", "dich", "nun", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie werden taub wie Steine", "tokens": ["Sie", "wer\u00b7den", "taub", "wie", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei deinem Jammer sein!", "tokens": ["Bei", "dei\u00b7nem", "Jam\u00b7mer", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dir Herz und Pforte schlie\u00dfen", "tokens": ["Dir", "Herz", "und", "Pfor\u00b7te", "schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und, statt des Trostes, gar", "tokens": ["Und", ",", "statt", "des", "Tros\u00b7tes", ",", "gar"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "KOUI", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dich foltern mit der Frage,", "tokens": ["Dich", "fol\u00b7tern", "mit", "der", "Fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer deine Mutter war?", "tokens": ["Wer", "dei\u00b7ne", "Mut\u00b7ter", "war", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und weh dir, wenn du Rache", "tokens": ["Und", "weh", "dir", ",", "wenn", "du", "Ra\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gleich deiner Mutter \u00fcbst,", "tokens": ["Gleich", "dei\u00b7ner", "Mut\u00b7ter", "\u00fcbst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir selbst, des gro\u00dfen R\u00e4chers", "tokens": ["Dir", "selbst", ",", "des", "gro\u00b7\u00dfen", "R\u00e4\u00b7chers"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Uneingedenk, sie giebst!", "tokens": ["Un\u00b7ein\u00b7ge\u00b7denk", ",", "sie", "giebst", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Drum Thr\u00e4nen und Gebete", "tokens": ["Drum", "Thr\u00e4\u00b7nen", "und", "Ge\u00b7be\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Segen \u00fcber dich!", "tokens": ["Und", "Se\u00b7gen", "\u00fc\u00b7ber", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihn, den ich selbst mir raubte,", "tokens": ["Ihn", ",", "den", "ich", "selbst", "mir", "raub\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Segen \u00fcber dich!", "tokens": ["Den", "Se\u00b7gen", "\u00fc\u00b7ber", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "An diesen Mutterbusen", "tokens": ["An", "die\u00b7sen", "Mut\u00b7ter\u00b7bu\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Komm' dann zum letztenmal", "tokens": ["Komm'", "dann", "zum", "letz\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schl\u00fcrfe du dir Labung", "tokens": ["Und", "schl\u00fcr\u00b7fe", "du", "dir", "La\u00b7bung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus dieser Brust voll Qual!", "tokens": ["Aus", "die\u00b7ser", "Brust", "voll", "Qual", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Vergebens streckst du wieder", "tokens": ["Ver\u00b7ge\u00b7bens", "streckst", "du", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die H\u00e4ndchen aus nach mir", "tokens": ["Die", "H\u00e4nd\u00b7chen", "aus", "nach", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nimmermehr wird Labung", "tokens": ["Und", "nim\u00b7mer\u00b7mehr", "wird", "La\u00b7bung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An diesem Busen dir!", "tokens": ["An", "die\u00b7sem", "Bu\u00b7sen", "dir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Ach! wer wird k\u00fcnftig Vater,", "tokens": ["Ach", "!", "wer", "wird", "k\u00fcnf\u00b7tig", "Va\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer wird die Mutter sein?", "tokens": ["Wer", "wird", "die", "Mut\u00b7ter", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihn deckt ein Kirchhofh\u00fcgel,", "tokens": ["Ihn", "deckt", "ein", "Kirch\u00b7hof\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und mich der Rabenstein.", "tokens": ["Und", "mich", "der", "Ra\u00b7ben\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "O weh mir armen Mutter!", "tokens": ["O", "weh", "mir", "ar\u00b7men", "Mut\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O ungl\u00fcckselig Kind,", "tokens": ["O", "un\u00b7gl\u00fcck\u00b7se\u00b7lig", "Kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da\u00df in der Wehenstunde", "tokens": ["Da\u00df", "in", "der", "We\u00b7hen\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir nicht verschmachtet sind!", "tokens": ["Wir", "nicht", "ver\u00b7schmach\u00b7tet", "sind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "O schlage nicht dein Auge", "tokens": ["O", "schla\u00b7ge", "nicht", "dein", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-", "measu