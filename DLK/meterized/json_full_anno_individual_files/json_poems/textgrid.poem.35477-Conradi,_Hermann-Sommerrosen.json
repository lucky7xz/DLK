{"textgrid.poem.35477": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Sommerrosen", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wollte dich mit Rosen \u00fcbersch\u00fctten,", "tokens": ["Ich", "woll\u00b7te", "dich", "mit", "Ro\u00b7sen", "\u00fc\u00b7ber\u00b7sch\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit roten Rosen dein goldbraunes Haar", "tokens": ["Mit", "ro\u00b7ten", "Ro\u00b7sen", "dein", "gold\u00b7brau\u00b7nes", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+++-+", "measure": "zehnsilber"}, "line.3": {"text": "Und deines Mieders Knospenrundung schm\u00fccken ...", "tokens": ["Und", "dei\u00b7nes", "Mie\u00b7ders", "Knos\u00b7pen\u00b7run\u00b7dung", "schm\u00fc\u00b7cken", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als noch der Lenz mit s\u00fc\u00dfem Veilchenodem,", "tokens": ["Als", "noch", "der", "Lenz", "mit", "s\u00fc\u00b7\u00dfem", "Veil\u00b7che\u00b7no\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein milder Sieger, durch die Lande schritt,", "tokens": ["Ein", "mil\u00b7der", "Sie\u00b7ger", ",", "durch", "die", "Lan\u00b7de", "schritt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sprach ich zu dir: Geliebte! Hat sein Mund", "tokens": ["Sprach", "ich", "zu", "dir", ":", "Ge\u00b7lieb\u00b7te", "!", "Hat", "sein", "Mund"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPER", "$.", "NN", "$.", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit letztem hei\u00dfen Abschiedsku\u00df die Rose,", "tokens": ["Mit", "letz\u00b7tem", "hei\u00b7\u00dfen", "Ab\u00b7schieds\u00b7ku\u00df", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die rote Sommerrose, aufgebrochen,", "tokens": ["Die", "ro\u00b7te", "Som\u00b7mer\u00b7ro\u00b7se", ",", "auf\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dann will ich zu dir kommen und mit Rosen,", "tokens": ["Dann", "will", "ich", "zu", "dir", "kom\u00b7men", "und", "mit", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mit roten Rosen deine Sch\u00f6nheit kr\u00f6nen ...", "tokens": ["Mit", "ro\u00b7ten", "Ro\u00b7sen", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "kr\u00f6\u00b7nen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun kam der Sommer ... Und der Rosen F\u00fclle", "tokens": ["Nun", "kam", "der", "Som\u00b7mer", "...", "Und", "der", "Ro\u00b7sen", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seh' ich allorts und alle Stunde bl\u00fchen ...", "tokens": ["Seh'", "ich", "al\u00b7lorts", "und", "al\u00b7le", "Stun\u00b7de", "bl\u00fc\u00b7hen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die ganze Welt scheint ihrer Macht verfallen,", "tokens": ["Die", "gan\u00b7ze", "Welt", "scheint", "ih\u00b7rer", "Macht", "ver\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ihre Keusche wirbt Vasallen um Vasallen ...", "tokens": ["Und", "ih\u00b7re", "Keu\u00b7sche", "wirbt", "Va\u00b7sal\u00b7len", "um", "Va\u00b7sal\u00b7len", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Selbst einen Bettler sah ich heute l\u00e4cheln,", "tokens": ["Selbst", "ei\u00b7nen", "Bett\u00b7ler", "sah", "ich", "heu\u00b7te", "l\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als sein vertr\u00e4nter Blick von ungef\u00e4hr", "tokens": ["Als", "sein", "ver\u00b7tr\u00e4n\u00b7ter", "Blick", "von", "un\u00b7ge\u00b7f\u00e4hr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf einen Korb mit roten Rosen fiel ...", "tokens": ["Auf", "ei\u00b7nen", "Korb", "mit", "ro\u00b7ten", "Ro\u00b7sen", "fiel", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich kauf' sie in der ganzen Stadt zusammen", "tokens": ["Ich", "kauf'", "sie", "in", "der", "gan\u00b7zen", "Stadt", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sch\u00fctte sie auf tote Liebesflammen ...", "tokens": ["Und", "sch\u00fct\u00b7te", "sie", "auf", "to\u00b7te", "Lie\u00b7bes\u00b7flam\u00b7men", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "pu