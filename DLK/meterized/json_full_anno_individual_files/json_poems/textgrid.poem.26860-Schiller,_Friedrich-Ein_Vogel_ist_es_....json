{"textgrid.poem.26860": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein Vogel ist es ...", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Vogel ist es, und an Schnelle", "tokens": ["Ein", "Vo\u00b7gel", "ist", "es", ",", "und", "an", "Schnel\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "VAFIN", "PPER", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Buhlt es mit eines Adlers Flug,", "tokens": ["Buhlt", "es", "mit", "ei\u00b7nes", "Ad\u00b7lers", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Fisch ists und zerteilt die Welle,", "tokens": ["Ein", "Fisch", "ists", "und", "zer\u00b7teilt", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die noch kein gr\u00f6\u00dfres Untier trug,", "tokens": ["Die", "noch", "kein", "gr\u00f6\u00df\u00b7res", "Un\u00b7tier", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Elefant ists, welcher T\u00fcrme", "tokens": ["Ein", "E\u00b7le\u00b7fant", "ists", ",", "wel\u00b7cher", "T\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "PWAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf seinem schweren R\u00fccken tr\u00e4gt,", "tokens": ["Auf", "sei\u00b7nem", "schwe\u00b7ren", "R\u00fc\u00b7cken", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Spinnen kriechendem Gew\u00fcrme", "tokens": ["Der", "Spin\u00b7nen", "krie\u00b7chen\u00b7dem", "Ge\u00b7w\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gleicht es, wenn es die F\u00fc\u00dfe regt,", "tokens": ["Gleicht", "es", ",", "wenn", "es", "die", "F\u00fc\u00b7\u00dfe", "regt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Und hat es fest sich eingebissen", "tokens": ["Und", "hat", "es", "fest", "sich", "ein\u00b7ge\u00b7bis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mit seinem spitzgen Eisenzahn,", "tokens": ["Mit", "sei\u00b7nem", "spitz\u00b7gen", "Ei\u00b7sen\u00b7zahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So stehts gleichwie auf festen F\u00fc\u00dfen", "tokens": ["So", "stehts", "gleich\u00b7wie", "auf", "fes\u00b7ten", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und trotzt dem w\u00fctenden Orkan.", "tokens": ["Und", "trotzt", "dem", "w\u00fc\u00b7ten\u00b7den", "Or\u00b