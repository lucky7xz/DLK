{"textgrid.poem.63296": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Berliner in Italien", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ganze Welt ist voll von Berlinern.", "tokens": ["Die", "gan\u00b7ze", "Welt", "ist", "voll", "von", "Ber\u00b7li\u00b7nern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Deutschland, Deutschland \u00fcberall in der Welt.", "tokens": ["Deutschland", ",", "Deutschland", "\u00fc\u00b7be\u00b7rall", "in", "der", "Welt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ich sah sie auf der Promenade in Nervi sich gegenseitig bedienern,", "tokens": ["Ich", "sah", "sie", "auf", "der", "Pro\u00b7me\u00b7na\u00b7de", "in", "Ner\u00b7vi", "sich", "ge\u00b7gen\u00b7sei\u00b7tig", "be\u00b7die\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "NE", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+--+--+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Und sie waren als Statisten beim Empfang des italienischen K\u00f6nigs", "tokens": ["Und", "sie", "wa\u00b7ren", "als", "Sta\u00b7tis\u00b7ten", "beim", "Emp\u00b7fang", "des", "i\u00b7ta\u00b7li\u00b7e\u00b7ni\u00b7schen", "K\u00f6\u00b7nigs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "KOKOM", "NN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "--+-+-+---+-+---+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "in Mailand aufgestellt.", "tokens": ["in", "Mai\u00b7land", "auf\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da konnten sie einmal wieder aus vollem Herzen Hurra schreien.", "tokens": ["Da", "konn\u00b7ten", "sie", "ein\u00b7mal", "wie\u00b7der", "aus", "vol\u00b7lem", "Her\u00b7zen", "Hur\u00b7ra", "schrei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "NE", "VVFIN", "$."], "meter": "-+--+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "So 'n K\u00f6nig, und sei er noch so klein, is doch janz was anderes", "tokens": ["So", "'n", "K\u00f6\u00b7nig", ",", "und", "sei", "er", "noch", "so", "klein", ",", "is", "doch", "janz", "was", "an\u00b7de\u00b7res"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "FM", "ADV", "ADV", "PWS", "PIS"], "meter": "--+--+-+-+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "als so 'ne miekrige Republik.", "tokens": ["als", "so", "'ne", "miek\u00b7ri\u00b7ge", "Re\u00b7pub\u00b7lik", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "In Bellaggio wandeln sie unter Palmen und Zypressen zu zweien,", "tokens": ["In", "Bel\u00b7lag\u00b7gio", "wan\u00b7deln", "sie", "un\u00b7ter", "Pal\u00b7men", "und", "Zyp\u00b7res\u00b7sen", "zu", "zwei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und aus dem Grandhotel t\u00f6nt (fabelhaft echt Italienisch;", "tokens": ["Und", "aus", "dem", "Grand\u00b7ho\u00b7tel", "t\u00f6nt", "(", "fa\u00b7bel\u00b7haft", "echt", "I\u00b7ta\u00b7li\u00b7e\u00b7nisch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$(", "ADJD", "ADJD", "NN", "$."], "meter": "-+-++-+-+-+-+-+-", "measure": "unknown.measure.octa.plus"}}, "stanza.6": {"line.1": {"text": "Pensionspreis t\u00e4glich 200 Lire) die Jazzmusik.", "tokens": ["Pen\u00b7si\u00b7ons\u00b7preis", "t\u00e4g\u00b7lich", "200", "Li\u00b7re", ")", "die", "Jazz\u00b7mu\u00b7sik", "."], "token_info": ["word", "word", "number", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "CARD", "NN", "$(", "ART", "NN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Wie h\u00fcbsch in Bologna die Jungens mit den schwarzen Mussolinhemden!", "tokens": ["Wie", "h\u00fcbsch", "in", "Bo\u00b7log\u00b7na", "die", "Jun\u00b7gens", "mit", "den", "schwar\u00b7zen", "Mus\u00b7so\u00b7lin\u00b7hem\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "NE", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+--", "measure": "iambic.octa.plus"}, "line.2": {"text": "Wie malerisch die Bettler am Kirchentor!", "tokens": ["Wie", "ma\u00b7le\u00b7risch", "die", "Bett\u00b7ler", "am", "Kir\u00b7chen\u00b7tor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die und die Fl\u00f6he finden einen Fremden", "tokens": ["Die", "und", "die", "Fl\u00f6\u00b7he", "fin\u00b7den", "ei\u00b7nen", "Frem\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus hunderttausend Eingebornen hervor.", "tokens": ["Aus", "hun\u00b7dert\u00b7tau\u00b7send", "Ein\u00b7ge\u00b7bor\u00b7nen", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.8": {"line.1": {"text": "In Genua am Hafen aus engen mit W\u00e4sche verhangenen Gassen winken", "tokens": ["In", "Ge\u00b7nua", "am", "Ha\u00b7fen", "aus", "en\u00b7gen", "mit", "W\u00e4\u00b7sche", "ver\u00b7han\u00b7ge\u00b7nen", "Gas\u00b7sen", "win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPRART", "NN", "APPR", "ADJA", "APPR", "NN", "ADJA", "NN", "VVINF"], "meter": "--+-+--+--+--+--+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Schwarz\u00e4ugige M\u00e4dchen und sind bereit,", "tokens": ["Schwarz\u00b7\u00e4u\u00b7gi\u00b7ge", "M\u00e4d\u00b7chen", "und", "sind", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Gegen entsprechendes Honorar sich abzuschminken.", "tokens": ["Ge\u00b7gen", "ent\u00b7spre\u00b7chen\u00b7des", "Ho\u00b7no\u00b7rar", "sich", "ab\u00b7zu\u00b7schmin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVIZU", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.4": {"text": "O du fr\u00f6hliche, o du selige Fr\u00fchlingszeit.", "tokens": ["O", "du", "fr\u00f6h\u00b7li\u00b7che", ",", "o", "du", "se\u00b7li\u00b7ge", "Fr\u00fch\u00b7lings\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "$,", "FM", "PPER", "ADJA", "NN", "$."], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Dagegen das Kolosseum, die ollen Klamotten, die verstaubten Geschichten,", "tokens": ["Da\u00b7ge\u00b7gen", "das", "Ko\u00b7los\u00b7se\u00b7um", ",", "die", "ol\u00b7len", "Kla\u00b7mot\u00b7ten", ",", "die", "ver\u00b7staub\u00b7ten", "Ge\u00b7schich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+--+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Das haben wir zu Hause auf halb bebautem Gel\u00e4nde auch, nu jewi\u00df.", "tokens": ["Das", "ha\u00b7ben", "wir", "zu", "Hau\u00b7se", "auf", "halb", "be\u00b7bau\u00b7tem", "Ge\u00b7l\u00e4n\u00b7de", "auch", ",", "nu", "je\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "NN", "APPR", "ADJD", "ADJA", "NN", "ADV", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+--+--+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Den schiefen Turm von Pisa sollten sie mal jrade richten.", "tokens": ["Den", "schie\u00b7fen", "Turm", "von", "Pi\u00b7sa", "soll\u00b7ten", "sie", "mal", "jra\u00b7de", "rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Mussolini hat dazu den n\u00f6tigen Schmi\u00df.", "tokens": ["Mus\u00b7so\u00b7li\u00b7ni", "hat", "da\u00b7zu", "den", "n\u00f6\u00b7ti\u00b7gen", "Sch\u00b7mi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.10": {"line.1": {"text": "\u00dcber diesem Lande schweben egal weg die Musen,", "tokens": ["\u00dc\u00b7ber", "die\u00b7sem", "Lan\u00b7de", "schwe\u00b7ben", "e\u00b7gal", "weg", "die", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Man sehe sich die Brera und die Uffizien an.", "tokens": ["Man", "se\u00b7he", "sich", "die", "Bre\u00b7ra", "und", "die", "Uf\u00b7fi\u00b7zi\u00b7en", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ART", "NE", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Die mageren Weiber von Botticelli kann ich nich verknusen,", "tokens": ["Die", "ma\u00b7ge\u00b7ren", "Wei\u00b7ber", "von", "Bot\u00b7ti\u00b7cel\u00b7li", "kann", "ich", "nich", "ver\u00b7knu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+-+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Aber Rubens, des is mein Mann.", "tokens": ["A\u00b7ber", "Ru\u00b7bens", ",", "des", "is", "mein", "Mann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "ART", "FM", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Wohin man sieht, spuckt einer oder verrichtet sonst eine Notdurft:", "tokens": ["Wo\u00b7hin", "man", "sieht", ",", "spuckt", "ei\u00b7ner", "o\u00b7der", "ver\u00b7rich\u00b7tet", "sonst", "ei\u00b7ne", "Not\u00b7durft", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "VVFIN", "ART", "KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+--+--+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.12": {"line.1": {"text": "es ist ein echt volkst\u00fcmliches Treiben.", "tokens": ["es", "ist", "ein", "echt", "volks\u00b7t\u00fcm\u00b7li\u00b7ches", "Trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Pr\u00e4chtig dies Monument Vittorio Emmanueles in Rom: goldbronziert", "tokens": ["Pr\u00e4ch\u00b7tig", "dies", "Mo\u00b7nu\u00b7ment", "Vit\u00b7to\u00b7rio", "Em\u00b7ma\u00b7nu\u00b7e\u00b7les", "in", "Rom", ":", "gold\u00b7bron\u00b7ziert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "PDS", "NN", "NE", "NE", "APPR", "NE", "$.", "VVFIN"], "meter": "+-+-+-+-+-+-+--+--+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.14": {"line.1": {"text": "und die S\u00e4ulenhalle aus wei\u00dfem Gips.", "tokens": ["und", "die", "S\u00e4u\u00b7len\u00b7hal\u00b7le", "aus", "wei\u00b7\u00dfem", "Gips", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.15": {"line.1": {"text": "Daf\u00fcr kann mir das ganze Forum jestohlen