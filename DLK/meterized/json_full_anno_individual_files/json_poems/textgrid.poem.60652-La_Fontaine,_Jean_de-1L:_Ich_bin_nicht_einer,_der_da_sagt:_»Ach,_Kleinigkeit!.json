{"textgrid.poem.60652": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin nicht einer, der da sagt: \u00bbAch, Kleinigkeit!", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin nicht einer, der da sagt: \u00bbAch, Kleinigkeit!", "tokens": ["Ich", "bin", "nicht", "ei\u00b7ner", ",", "der", "da", "sagt", ":", "\u00bb", "Ach", ",", "Klei\u00b7nig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "$,", "PRELS", "ADV", "VVFIN", "$.", "$(", "ITJ", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur eine Frau, die sich ertr\u00e4nkt!\u00ab", "tokens": ["Nur", "ei\u00b7ne", "Frau", ",", "die", "sich", "er\u00b7tr\u00e4nkt", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sage, das ist viel, und bin bereit", "tokens": ["Ich", "sa\u00b7ge", ",", "das", "ist", "viel", ",", "und", "bin", "be\u00b7reit"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PDS", "VAFIN", "ADV", "$,", "KON", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um sie zu trauern, weil das Weib uns Freude schenkt. \u2013", "tokens": ["Um", "sie", "zu", "trau\u00b7ern", ",", "weil", "das", "Weib", "uns", "Freu\u00b7de", "schenkt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF", "$,", "KOUS", "ART", "NN", "PPER", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Vorgesagte ist nicht \u00fcberfl\u00fcssig", "tokens": ["Das", "Vor\u00b7ge\u00b7sag\u00b7te", "ist", "nicht", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7sig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr meine Fabel, denn sie meldet dies,", "tokens": ["F\u00fcr", "mei\u00b7ne", "Fa\u00b7bel", ",", "denn", "sie", "mel\u00b7det", "dies", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "PDS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df eine Frau, des Lebens \u00fcberdr\u00fcssig,", "tokens": ["Da\u00df", "ei\u00b7ne", "Frau", ",", "des", "Le\u00b7bens", "\u00fc\u00b7berd\u00b7r\u00fcs\u00b7sig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sich eines Tages in die Fluten fallen lie\u00df.", "tokens": ["Sich", "ei\u00b7nes", "Ta\u00b7ges", "in", "die", "Flu\u00b7ten", "fal\u00b7len", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "APPR", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihr Gatte wollte wenigstens die Leiche retten,", "tokens": ["Ihr", "Gat\u00b7te", "woll\u00b7te", "we\u00b7nigs\u00b7tens", "die", "Lei\u00b7che", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Um sie mit Kirchenehren einzubetten.", "tokens": ["Um", "sie", "mit", "Kir\u00b7che\u00b7neh\u00b7ren", "ein\u00b7zu\u00b7bet\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Doch von den Leuten, die zur Unfallzeit", "tokens": ["Doch", "von", "den", "Leu\u00b7ten", ",", "die", "zur", "Un\u00b7fall\u00b7zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Am Ufer gingen, hatte keiner wahrgenommen,", "tokens": ["Am", "U\u00b7fer", "gin\u00b7gen", ",", "hat\u00b7te", "kei\u00b7ner", "wahr\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wo ihre Leiche hingeschwommen.", "tokens": ["Wo", "ih\u00b7re", "Lei\u00b7che", "hin\u00b7ge\u00b7schwom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Mit Rat jedoch war jeder gern bereit.", "tokens": ["Mit", "Rat", "je\u00b7doch", "war", "je\u00b7der", "gern", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VAFIN", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "\u00bbes liegt,\u00ab sprach einer, \u00bbwohl in der Natur", "tokens": ["\u00bb", "es", "liegt", ",", "\u00ab", "sprach", "ei\u00b7ner", ",", "\u00bb", "wohl", "in", "der", "Na\u00b7tur"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "$,", "$(", "VVFIN", "PIS", "$,", "$(", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Der Dinge, da\u00df sie mit der Str\u00f6mung nur", "tokens": ["Der", "Din\u00b7ge", ",", "da\u00df", "sie", "mit", "der", "Str\u00f6\u00b7mung", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Flu\u00dfabw\u00e4rts treiben konnte.\u00ab Sprach ein andrer: \u00bbNein!", "tokens": ["Flu\u00df\u00b7ab\u00b7w\u00e4rts", "trei\u00b7ben", "konn\u00b7te", ".", "\u00ab", "Sprach", "ein", "an\u00b7drer", ":", "\u00bb", "Nein", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$.", "$(", "NN", "ART", "ADJA", "$.", "$(", "PTKANT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df man sie aufw\u00e4rts sucht, scheint richtiger mir zu sein.", "tokens": ["Da\u00df", "man", "sie", "auf\u00b7w\u00e4rts", "sucht", ",", "scheint", "rich\u00b7ti\u00b7ger", "mir", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "ADJD", "PPER", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Wie stark auch das Gef\u00e4lle und die Kraft,", "tokens": ["Wie", "stark", "auch", "das", "Ge\u00b7f\u00e4l\u00b7le", "und", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Mit der das Wasser alles vorw\u00e4rts schafft,", "tokens": ["Mit", "der", "das", "Was\u00b7ser", "al\u00b7les", "vor\u00b7w\u00e4rts", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Der Geist des Widerspruches wird das Weib bestimmen,", "tokens": ["Der", "Geist", "des", "Wi\u00b7der\u00b7spru\u00b7ches", "wird", "das", "Weib", "be\u00b7stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Gegen die Str\u00f6mung anzuschwimmen.\u00ab", "tokens": ["Ge\u00b7gen", "die", "Str\u00f6\u00b7mung", "an\u00b7zu\u00b7schwim\u00b7men", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVIZU", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Mann trieb seinen Spott wohl nicht zur rechten Zeit,", "tokens": ["Der", "Mann", "trieb", "sei\u00b7nen", "Spott", "wohl", "nicht", "zur", "rech\u00b7ten", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch wei\u00df ich nicht, ob's allen richtig scheint,", "tokens": ["Auch", "wei\u00df", "ich", "nicht", ",", "ob's", "al\u00b7len", "rich\u00b7tig", "scheint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was er vom Widerspruch gemeint.", "tokens": ["Was", "er", "vom", "Wi\u00b7der\u00b7spruch", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Fehler oder nur ein b\u00f6ser Hang,", "tokens": ["Ein", "Feh\u00b7ler", "o\u00b7der", "nur", "ein", "b\u00f6\u00b7ser", "Hang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So geht doch ", "tokens": ["So", "geht", "doch"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Wer widerspricht, der widerspricht sein Leben lang!", "tokens": ["Wer", "wi\u00b7der\u00b7spricht", ",", "der", "wi\u00b7der\u00b7spricht", "sein", "Le\u00b7ben", "lang", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und nicht nur von der Wiege bis zum Grabe,", "tokens": ["Und", "nicht", "nur", "von", "der", "Wie\u00b7ge", "bis", "zum", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "APPR", "ART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nein, noch im Tode \u00fcbt er diese Gabe.", "tokens": ["Nein", ",", "noch", "im", "To\u00b7de", "\u00fcbt", "er", "die\u00b7se", "Ga\u00b7be", "."], "token_info"