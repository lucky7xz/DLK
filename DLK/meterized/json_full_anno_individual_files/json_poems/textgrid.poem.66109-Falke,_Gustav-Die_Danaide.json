{"textgrid.poem.66109": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Danaide", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Tag verd\u00e4mmert, steigen Sterne,", "tokens": ["Wenn", "der", "Tag", "ver\u00b7d\u00e4m\u00b7mert", ",", "stei\u00b7gen", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Morgenrot f\u00fchrt neuen Tag herein,", "tokens": ["Mor\u00b7gen\u00b7rot", "f\u00fchrt", "neu\u00b7en", "Tag", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Endlos rauscht es aus der dunklen Ferne,", "tokens": ["End\u00b7los", "rauscht", "es", "aus", "der", "dunk\u00b7len", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ach, wann wird die letzte Welle sein?", "tokens": ["Ach", ",", "wann", "wird", "die", "letz\u00b7te", "Wel\u00b7le", "sein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sind denn unersch\u00f6pflich jene Schalen?", "tokens": ["Sind", "denn", "un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "je\u00b7ne", "Scha\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "PDA