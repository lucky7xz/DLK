{"textgrid.poem.66059": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Von weissen Rosen", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Gl\u00fcck teilt seine Rosen aus,", "tokens": ["Das", "Gl\u00fcck", "teilt", "sei\u00b7ne", "Ro\u00b7sen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Macht auch wohl mal ein Kr\u00e4nzlein draus,", "tokens": ["Macht", "auch", "wohl", "mal", "ein", "Kr\u00e4nz\u00b7lein", "draus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADV", "ART", "NN", "PAV", "$,"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.3": {"text": "Aus roten, die gleich Sonnen gl\u00fchn,", "tokens": ["Aus", "ro\u00b7ten", ",", "die", "gleich", "Son\u00b7nen", "gl\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "PRELS", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus wei\u00dfen, die gleich Sternen bl\u00fchn.", "tokens": ["Aus", "wei\u00b7\u00dfen", ",", "die", "gleich", "Ster\u00b7nen", "bl\u00fchn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "PRELS", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der roten viel am Wege stehn,", "tokens": ["Der", "ro\u00b7ten", "viel", "am", "We\u00b7ge", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die wei\u00dfen mu\u00df es suchen gehn.", "tokens": ["Die", "wei\u00b7\u00dfen", "mu\u00df", "es", "su\u00b7chen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Viel flinke H\u00e4nde schickt es aus,", "tokens": ["Viel", "flin\u00b7ke", "H\u00e4n\u00b7de", "schickt", "es", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hilfsenglein sucht von Haus zu Haus:", "tokens": ["Hilf\u00b7sen\u00b7glein", "sucht", "von", "Haus", "zu", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ein Wunsch, von Herz zu Herz gedacht,", "tokens": ["Ein", "Wunsch", ",", "von", "Herz", "zu", "Herz", "ge\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Seufzer in verschwiegener Nacht,", "tokens": ["Ein", "Seuf\u00b7zer", "in", "ver\u00b7schwie\u00b7ge\u00b7ner", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ein Tr\u00e4nlein, oder was es sei,", "tokens": ["Ein", "Tr\u00e4n\u00b7lein", ",", "o\u00b7der", "was", "es", "sei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gib acht, flieg nicht daran vorbei!", "tokens": ["Gib", "acht", ",", "flieg", "nicht", "da\u00b7ran", "vor\u00b7bei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "$,", "VVFIN", "PTKNEG", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Garten ist voll wei\u00dfen Gl\u00fccks.", "tokens": ["Mein", "Gar\u00b7ten", "ist", "voll", "wei\u00b7\u00dfen", "Gl\u00fccks", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Englein siehts: Wie lieblich! Pfl\u00fccks", "tokens": ["Das", "En\u00b7glein", "siehts", ":", "Wie", "lieb\u00b7lich", "!", "Pfl\u00fccks"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PWAV", "ADJD", "$.", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr sie, f\u00fcr dies in Bl\u00fcte steht:", "tokens": ["F\u00fcr", "sie", ",", "f\u00fcr", "dies", "in", "Bl\u00fc\u00b7te", "steht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PDS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Morgengru\u00df. Ein Nachtgebet.", "tokens": ["Ein", "Mor\u00b7gen\u00b7gru\u00df", ".", "Ein", "Nacht\u00b7ge\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Habdichlieb! Ein Denkedein! \u2013", "tokens": ["Ein", "Hab\u00b7dich\u00b7lieb", "!", "Ein", "Den\u00b7ke\u00b7dein", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihm zittern vor Freude die Fl\u00fcgelein.", "tokens": ["Ihm", "zit\u00b7tern", "vor", "Freu\u00b7de", "die", "Fl\u00fc\u00b7ge\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Und alle Rosen, die es fand,", "tokens": ["Und", "al\u00b7le", "Ro\u00b7sen", ",", "die", "es", "fand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nimmt es in seine wei\u00dfe Hand,", "tokens": ["Nimmt", "es", "in", "sei\u00b7ne", "wei\u00b7\u00dfe", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wo es nur ein R\u00f6slein nahm,", "tokens": ["Und", "wo", "es", "nur", "ein", "R\u00f6s\u00b7lein", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sogleich ein anderes wiederkam.", "tokens": ["Sog\u00b7leich", "ein", "an\u00b7de\u00b7res", "wie\u00b7der\u00b7kam", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PIS", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "So findets immer einen Flor", "tokens": ["So", "fin\u00b7dets", "im\u00b7mer", "ei\u00b7nen", "Flor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr dich erbl\u00fchter Rosen vor.", "tokens": ["F\u00fcr", "dich", "er\u00b7bl\u00fch\u00b7ter", "Ro\u00b7sen", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da macht das Gl\u00fcck die Augen gro\u00df,", "tokens": ["Da", "macht", "das", "Gl\u00fcck", "die", "Au\u00b7gen", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat einen \u00fcberreichen Scho\u00df:", "tokens": ["Hat", "ei\u00b7nen", "\u00fc\u00b7berr\u00b7ei\u00b7chen", "Scho\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das langt ja bis zum j\u00fcngsten Tag,", "tokens": ["Das", "langt", "ja", "bis", "zum", "j\u00fcng\u00b7sten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Obs M\u00e4del den erleben mag?", "tokens": ["Obs", "M\u00e4\u00b7del", "den", "er\u00b7le\u00b7ben", "mag", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und geht es eh zum Himmel ein,", "tokens": ["Und", "geht", "es", "eh", "zum", "Him\u00b7mel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bringts lauter Rosen mit hinein!", "tokens": ["Bringts", "lau\u00b7ter", "Ro\u00b7sen", "mit", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "APPR", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und sinnend siehts, n\u00e4rrischer Traum,", "tokens": ["Und", "sin\u00b7nend", "siehts", ",", "n\u00e4r\u00b7ri\u00b7scher", "Traum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Es schon vorweg im Himmelsraum;", "tokens": ["Es", "schon", "vor\u00b7weg", "im", "Him\u00b7mels\u00b7raum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gar lieblich gehts mit seinem Kranz", "tokens": ["Gar", "lieb\u00b7lich", "gehts", "mit", "sei\u00b7nem", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00fcberstrahlt der Engel Glanz.", "tokens": ["Und", "\u00fc\u00b7bers\u00b7trahlt", "der", "En\u00b7gel", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Sch\u00fcrzlein hat es, wei\u00dfe Pracht,", "tokens": ["Im", "Sch\u00fcrz\u00b7lein", "hat", "es", ",", "wei\u00b7\u00dfe", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein H\u00e4uflein Rosen mitgebracht.", "tokens": ["Ein", "H\u00e4uf\u00b7lein", "Ro\u00b7sen", "mit\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Als unversehns vor Gott es steht,", "tokens": ["Als", "un\u00b7ver\u00b7sehns", "vor", "Gott", "es", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Schreck ihm durch die Glieder geht.", "tokens": ["Ein", "Schreck", "ihm", "durch", "die", "Glie\u00b7der", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Rosen fallen ihm aus dem Scho\u00df,", "tokens": ["Die", "Ro\u00b7sen", "fal\u00b7len", "ihm", "aus", "dem", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sogleich geschieht ein Wunder gro\u00df:", "tokens": ["Sog\u00b7leich", "ge\u00b7schieht", "ein", "Wun\u00b7der", "gro\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was eben wei\u00dfe Bl\u00fcte war,", "tokens": ["Was", "e\u00b7ben", "wei\u00b7\u00dfe", "Bl\u00fc\u00b7te", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird eine lichte Bubenschar:", "tokens": ["Wird", "ei\u00b7ne", "lich\u00b7te", "Bu\u00b7ben\u00b7schar", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ein Morgengru\u00df. Ein Nachtgebet.", "tokens": ["Ein", "Mor\u00b7gen\u00b7gru\u00df", ".", "Ein", "Nacht\u00b7ge\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Wunsch, der sich versch\u00e4mt verr\u00e4t.", "tokens": ["Ein", "Wunsch", ",", "der", "sich", "ver\u00b7sch\u00e4mt", "ver\u00b7r\u00e4t", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Tr\u00e4nlein, still in sich hinein.", "tokens": ["Ein", "Tr\u00e4n\u00b7lein", ",", "still", "in", "sich", "hin\u00b7ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Habdichlieb. Ein Denkedein.", "tokens": ["Ein", "Hab\u00b7dich\u00b7lieb", ".", "Ein", "Den\u00b7ke\u00b7dein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die knien, ein lieblicher Kranz, mit stumm", "tokens": ["Die", "kni\u00b7en", ",", "ein", "lieb\u00b7li\u00b7cher", "Kranz", ",", "mit", "stumm"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "ART", "ADJA", "NN", "$,", "APPR", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Gefalteten H\u00e4ndchen um sie herum.", "tokens": ["Ge\u00b7fal\u00b7te\u00b7ten", "H\u00e4nd\u00b7chen", "um", "sie", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Der Herr, halb l\u00e4chelnd, halb ger\u00fchrt,", "tokens": ["Der", "Herr", ",", "halb", "l\u00e4\u00b7chelnd", ",", "halb", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ADJD", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein seltsam Herzbewegen sp\u00fcrt.", "tokens": ["Ein", "selt\u00b7sam", "Herz\u00b7be\u00b7we\u00b7gen", "sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ist kein Laut im Himmel drin,", "tokens": ["Und", "ist", "kein", "Laut", "im", "Him\u00b7mel", "drin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "APPR", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sehn alle auf die Holde hin.", "tokens": ["Sehn", "al\u00b7le", "auf", "die", "Hol\u00b7de", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die steht verwirrt, versch\u00e4mt \u2013 da f\u00e4llt", "tokens": ["Die", "steht", "ver\u00b7wirrt", ",", "ver\u00b7sch\u00e4mt", "\u2013", "da", "f\u00e4llt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "$,", "VVPP", "$(", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetr