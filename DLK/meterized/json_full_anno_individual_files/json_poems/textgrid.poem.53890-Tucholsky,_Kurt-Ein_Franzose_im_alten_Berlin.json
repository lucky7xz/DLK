{"textgrid.poem.53890": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Ein Franzose im alten Berlin", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Wappen von Berlin ist ein B\u00e4r, der auf seinen Hinterpfoten sch\u00f6n macht.", "tokens": ["Das", "Wap\u00b7pen", "von", "Ber\u00b7lin", "ist", "ein", "B\u00e4r", ",", "der", "auf", "sei\u00b7nen", "Hin\u00b7ter\u00b7pfo\u00b7ten", "sch\u00f6n", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+--++--+--+-+-+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": " im Walde singt die Nachtigall,", "tokens": ["im", "Wal\u00b7de", "singt", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": " die Pilze bl\u00fchn am Bachesrande;", "tokens": ["die", "Pil\u00b7ze", "bl\u00fchn", "am", "Ba\u00b7ches\u00b7ran\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": " es zieht der s\u00fc\u00dfe Fr\u00fchlingsschall", "tokens": ["es", "zieht", "der", "s\u00fc\u00b7\u00dfe", "Fr\u00fch\u00b7lings\u00b7schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": " wohlklingend durch die deutschen Lande.", "tokens": ["wohl\u00b7klin\u00b7gend", "durch", "die", "deu