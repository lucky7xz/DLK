{"textgrid.poem.41852": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Leben, la\u00df von dir hinweg mich scheiden", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Leben, la\u00df von dir hinweg mich scheiden", "tokens": ["O", "Le\u00b7ben", ",", "la\u00df", "von", "dir", "hin\u00b7weg", "mich", "schei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVIMP", "APPR", "PPER", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur Heimath, die dem Pilger Ruhe beut;", "tokens": ["Zur", "Hei\u00b7math", ",", "die", "dem", "Pil\u00b7ger", "Ru\u00b7he", "beut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich wei\u00df ein Grab, da schlummern meine Freuden,", "tokens": ["Ich", "wei\u00df", "ein", "Grab", ",", "da", "schlum\u00b7mern", "mei\u00b7ne", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da bl\u00fcht allein, was Tr\u00f6stung mir verleiht.", "tokens": ["Da", "bl\u00fcht", "al\u00b7lein", ",", "was", "Tr\u00f6s\u00b7tung", "mir", "ver\u00b7leiht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PWS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gar friedlich ist der stille Platz bereitet,", "tokens": ["Gar", "fried\u00b7lich", "ist", "der", "stil\u00b7le", "Platz", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und lau das Wehn, das fl\u00fcsternd ihn umschwebt;", "tokens": ["Und", "lau", "das", "Wehn", ",", "das", "fl\u00fcs\u00b7ternd", "ihn", "um\u00b7schwebt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Lebend'ges Gr\u00fcn ist weich umhergebreitet,", "tokens": ["Le\u00b7ben\u00b7d'\u00b7ges", "Gr\u00fcn", "ist", "weich", "um\u00b7her\u00b7ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Mit Blumen rings sein duft'ger Rand umwebt.", "tokens": ["Mit", "Blu\u00b7men", "rings", "sein", "duft'\u00b7ger", "Rand", "um\u00b7webt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hold s\u00e4uselt dort mit mattbegl\u00e4nztem Fl\u00fcgel", "tokens": ["Hold", "s\u00e4u\u00b7selt", "dort", "mit", "matt\u00b7be\u00b7gl\u00e4nz\u00b7tem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Abendschein die Wehmuth um den Tod,", "tokens": ["Im", "A\u00b7bend\u00b7schein", "die", "Weh\u00b7muth", "um", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und leuchtend spielt am frischbekr\u00e4nzten H\u00fcgel", "tokens": ["Und", "leuch\u00b7tend", "spielt", "am", "frischbe\u00b7kr\u00e4nz\u00b7ten", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Hoffnung Schein im hellen Morgenroth.", "tokens": ["Der", "Hoff\u00b7nung", "Schein", "im", "hel\u00b7len", "Mor\u00b7gen\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und prangend hebt, wo meine Thr\u00e4nen thauen,", "tokens": ["Und", "pran\u00b7gend", "hebt", ",", "wo", "mei\u00b7ne", "Thr\u00e4\u00b7nen", "thau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Blumenkelch sich aus der stillen Gruft,", "tokens": ["Ein", "Blu\u00b7men\u00b7kelch", "sich", "aus", "der", "stil\u00b7len", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der ist gar mild und freundlich anzuschauen", "tokens": ["Der", "ist", "gar", "mild", "und", "freund\u00b7lich", "an\u00b7zu\u00b7schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "VVIZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und labt mein Herz mit wunderbarem Duft.", "tokens": ["Und", "labt", "mein", "Herz", "mit", "wun\u00b7der\u00b7ba\u00b7rem", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ihn pfleg' ich stets mit liebevoller Treue,", "tokens": ["Ihn", "pfleg'", "ich", "stets", "mit", "lie\u00b7be\u00b7vol\u00b7ler", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein frisches Leben nur ist mein Gewinn;", "tokens": ["Sein", "fri\u00b7sches", "Le\u00b7ben", "nur", "ist", "mein", "Ge\u00b7winn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er ist's allein, an dem ich mich erfreue,", "tokens": ["Er", "ist's", "al\u00b7lein", ",", "an", "dem", "ich", "mich", "er\u00b7freu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn er verwelkt, dann welk' auch ich dahin.", "tokens": ["Wenn", "er", "ver\u00b7welkt", ",", "dann", "wel\u00b7k'", "auch", "ich", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "ADV", "VVFIN", "ADV", "PPER", "PAV", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Denn in dem Reiz, der bl\u00fchend ihn umwaltet,", "tokens": ["Denn", "in", "dem", "Reiz", ",", "der", "bl\u00fc\u00b7hend", "ihn", "um\u00b7wal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und in dem Duft, der z\u00fcchtig ihn umflie\u00dft,", "tokens": ["Und", "in", "dem", "Duft", ",", "der", "z\u00fcch\u00b7tig", "ihn", "um\u00b7flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hat sich das Bild der Heiligen entfaltet,", "tokens": ["Hat", "sich", "das", "Bild", "der", "Hei\u00b7li\u00b7gen", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die lange schon der H\u00fcgel mir verschlie\u00dft.", "tokens": ["Die", "lan\u00b7ge", "schon", "der", "H\u00fc\u00b7gel", "mir", "ver\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wohl mu\u00df ich fern von meiner Freude stehen,", "tokens": ["Wohl", "mu\u00df", "ich", "fern", "von", "mei\u00b7ner", "Freu\u00b7de", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nimme