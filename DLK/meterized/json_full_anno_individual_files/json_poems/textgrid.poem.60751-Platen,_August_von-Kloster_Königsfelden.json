{"textgrid.poem.60751": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Kloster K\u00f6nigsfelden", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Kapelle W\u00f6lbung trat ich ein,", "tokens": ["In", "der", "Ka\u00b7pel\u00b7le", "W\u00f6l\u00b7bung", "trat", "ich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ver\u00f6det feiernd nun ins Ketzers Land;", "tokens": ["Ver\u00b7\u00f6\u00b7det", "fei\u00b7ernd", "nun", "ins", "Ket\u00b7zers", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Priester opfert mehr hier Brot und Wein,", "tokens": ["Kein", "Pries\u00b7ter", "op\u00b7fert", "mehr", "hier", "Brot", "und", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kein wei\u00dfer Knabe geht ihm fromm zur Hand.", "tokens": ["Kein", "wei\u00b7\u00dfer", "Kna\u00b7be", "geht", "ihm", "fromm", "zur", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Schlicht ist die Wand und ohne Schmuck und Gold,", "tokens": ["Schlicht", "ist", "die", "Wand", "und", "oh\u00b7ne", "Schmuck", "und", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "KON", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch stellt in Bildern sie den tapfern Chor,", "tokens": ["Doch", "stellt", "in", "Bil\u00b7dern", "sie", "den", "tap\u00b7fern", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den gegen Sempach f\u00fchrte Leopold,", "tokens": ["Den", "ge\u00b7gen", "Sem\u00b7pach", "f\u00fchr\u00b7te", "Leo\u00b7pold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und der des Heldentods sich freute, vor.", "tokens": ["Und", "der", "des", "Hel\u00b7den\u00b7tods", "sich", "freu\u00b7te", ",", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "PRF", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bei Jedem seht ihr Wappen, Nam und Schild,", "tokens": ["Bei", "Je\u00b7dem", "seht", "ihr", "Wap\u00b7pen", ",", "Nam", "und", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPOSAT", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und knieend flehn sie hier um Gottes Huld;", "tokens": ["Und", "kni\u00b7e\u00b7end", "flehn", "sie", "hier", "um", "Got\u00b7tes", "Huld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "NN", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In ihrer Mitte hangt des F\u00fchrers Bild:", "tokens": ["In", "ih\u00b7rer", "Mit\u00b7te", "hangt", "des", "F\u00fch\u00b7rers", "Bild", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du stolzes Herz, du hast geb\u00fc\u00dft die Schuld!", "tokens": ["Du", "stol\u00b7zes", "Herz", ",", "du", "hast", "ge\u00b7b\u00fc\u00dft", "die", "Schuld", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "VAFIN", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du hast erfahren, was ein Volk vermag,", "tokens": ["Du", "hast", "er\u00b7fah\u00b7ren", ",", "was", "ein", "Volk", "ver\u00b7mag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das f\u00fcr den eignen Herd die Fahne tr\u00e4gt:", "tokens": ["Das", "f\u00fcr", "den", "eig\u00b7nen", "Herd", "die", "Fah\u00b7ne", "tr\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So sterbe Jeder bis auf diesen Tag,", "tokens": ["So", "ster\u00b7be", "Je\u00b7der", "bis", "auf", "die\u00b7sen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wer einen freien Mann in Ketten schl\u00e4gt!", "tokens": ["Wer", "ei\u00b7nen", "frei\u00b7en", "Mann", "in", "Ket\u00b7ten", "schl\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und hier, wo sonst sich ein Altar erhub,", "tokens": ["Und", "hier", ",", "wo", "sonst", "sich", "ein", "Al\u00b7tar", "er\u00b7hub", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "ADV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erlag ein andrer m\u00e4chtiger Tyrann:", "tokens": ["Er\u00b7lag", "ein", "an\u00b7drer", "m\u00e4ch\u00b7ti\u00b7ger", "Ty\u00b7rann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im falschen Busen seines Ohms begrub", "tokens": ["Im", "fal\u00b7schen", "Bu\u00b7sen", "sei\u00b7nes", "Ohms", "be\u00b7grub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den vaterm\u00f6rderischen Dolch Johann.", "tokens": ["Den", "va\u00b7ter\u00b7m\u00f6r\u00b7de\u00b7ri\u00b7schen", "Dolch", "Jo\u00b7hann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Im Tode brach hier Alberts harter Sinn,", "tokens": ["Im", "To\u00b7de", "brach", "hier", "Al\u00b7berts", "har\u00b7ter", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der seinem Volk Freiheit verhielt und Recht;", "tokens": ["Der", "sei\u00b7nem", "Volk", "Frei\u00b7heit", "ver\u00b7hielt", "und", "Recht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVFIN", "KON", "NN", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Allein der Ungarn stolze K\u00f6nigin", "tokens": ["Al\u00b7lein", "der", "Un\u00b7garn", "stol\u00b7ze", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verdarb die M\u00f6rder und ihr ganz Geschlecht.", "tokens": ["Ver\u00b7darb", "die", "M\u00f6r\u00b7der", "und", "ihr", "ganz", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Selbst Greis und S\u00e4ugling unterlag der Wut;", "tokens": ["Selbst", "Greis", "und", "S\u00e4ug\u00b7ling", "un\u00b7ter\u00b7lag", "der", "Wut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es schwur die K\u00f6nigin, als w\u00e4r's in Tau,", "tokens": ["Es", "schwur", "die", "K\u00f6\u00b7ni\u00b7gin", ",", "als", "w\u00e4r's", "in", "Tau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOKOM", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu baden sich in ihrer Feinde Blut:", "tokens": ["Zu", "ba\u00b7den", "sich", "in", "ih\u00b7rer", "Fein\u00b7de", "Blut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PRF", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hebt sich so wild der Busen einer Frau?", "tokens": ["Hebt", "sich", "so", "wild", "der", "Bu\u00b7sen", "ei\u00b7ner", "Frau", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Dies Kloster bauend, wo der Vater starb,", "tokens": ["Dies", "Klos\u00b7ter", "bau\u00b7end", ",", "wo", "der", "Va\u00b7ter", "starb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVPP", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Belud Alt\u00e4re sie mit fremdem Raub,", "tokens": ["Be\u00b7lud", "Al\u00b7t\u00e4\u00b7re", "sie", "mit", "frem\u00b7dem", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo im Gebet sie um den Himmel warb;", "tokens": ["Wo", "im", "Ge\u00b7bet", "sie", "um", "den", "Him\u00b7mel", "warb", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch solchen Taten ist der Himmel taub!", "tokens": ["Doch", "sol\u00b7chen", "Ta\u00b7ten", "ist", "der", "Him\u00b7mel", "taub", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT