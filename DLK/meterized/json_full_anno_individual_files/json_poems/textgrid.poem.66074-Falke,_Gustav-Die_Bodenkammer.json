{"textgrid.poem.66074": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Bodenkammer", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war auf unsrer Bodenkammer,", "tokens": ["Das", "war", "auf", "uns\u00b7rer", "Bo\u00b7den\u00b7kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo schr\u00e4g das Dach dar\u00fcber lief.", "tokens": ["Wo", "schr\u00e4g", "das", "Dach", "da\u00b7r\u00fc\u00b7ber", "lief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, was verschlo\u00df die rostige Klammer", "tokens": ["Ach", ",", "was", "ver\u00b7schlo\u00df", "die", "ros\u00b7ti\u00b7ge", "Klam\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der schweren T\u00fcr! Von keinem Brief", "tokens": ["Der", "schwe\u00b7ren", "T\u00fcr", "!", "Von", "kei\u00b7nem", "Brief"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wurd je ein Siegel weggebrochen", "tokens": ["Wurd", "je", "ein", "Sie\u00b7gel", "weg\u00b7ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit so erh\u00f6htem Herzenspochen,", "tokens": ["Mit", "so", "er\u00b7h\u00f6h\u00b7tem", "Her\u00b7zen\u00b7spo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als wir zum Paradies dort oben", "tokens": ["Als", "wir", "zum", "Pa\u00b7ra\u00b7dies", "dort", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die schwere Luke keuchend hoben.", "tokens": ["Die", "schwe\u00b7re", "Lu\u00b7ke", "keu\u00b7chend", "ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da gab es einen Tannenbaum,", "tokens": ["Da", "gab", "es", "ei\u00b7nen", "Tan\u00b7nen\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom letzten Fest noch aufgehoben,", "tokens": ["Vom", "letz\u00b7ten", "Fest", "noch", "auf\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der fuhr als Schlitten durch den Raum,", "tokens": ["Der", "fuhr", "als", "Schlit\u00b7ten", "durch", "den", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOUS", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Staub und trockne Nadeln stoben.", "tokens": ["Da\u00df", "Staub", "und", "trock\u00b7ne", "Na\u00b7deln", "sto\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da gab es eine W\u00e4schemangel,", "tokens": ["Da", "gab", "es", "ei\u00b7ne", "W\u00e4\u00b7sche\u00b7man\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die rollend an zu kreischen fing,", "tokens": ["Die", "rol\u00b7lend", "an", "zu", "krei\u00b7schen", "fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie noch in keiner rostigen Angel", "tokens": ["Wie", "noch", "in", "kei\u00b7ner", "ros\u00b7ti\u00b7gen", "An\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Je eine alte T\u00fcre ging.", "tokens": ["Je", "ei\u00b7ne", "al\u00b7te", "T\u00fc\u00b7re", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da gab es eine leere Kiste,", "tokens": ["Da", "gab", "es", "ei\u00b7ne", "lee\u00b7re", "Kis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ganz wie gemacht zum H\u00f6hlenhaus,", "tokens": ["Ganz", "wie", "ge\u00b7macht", "zum", "H\u00f6h\u00b7len\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein morsches Ding, ein Wurmgeniste,", "tokens": ["Ein", "mor\u00b7sches", "Ding", ",", "ein", "Wurm\u00b7ge\u00b7nis\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Spinnen tapeziertens aus.", "tokens": ["Und", "Spin\u00b7nen", "ta\u00b7pe\u00b7zier\u00b7tens", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch Kindersinn hat Zauberkraft", "tokens": ["Doch", "Kin\u00b7der\u00b7sinn", "hat", "Zau\u00b7ber\u00b7kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mehr als die Lampe Aladins,", "tokens": ["Mehr", "als", "die", "Lam\u00b7pe", "A\u00b7la\u00b7dins", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Wunsch \u2013 da baut sich Schaft an Schaft", "tokens": ["Ein", "Wunsch", "\u2013", "da", "baut", "sich", "Schaft", "an", "Schaft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "VVFIN", "PRF", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das sch\u00f6nste Schlo\u00df, F\u00fcrsten beziehns.", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Schlo\u00df", ",", "F\u00fcrs\u00b7ten", "be\u00b7ziehns", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "PTKVZ", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Dann war da eine alte Wanne,", "tokens": ["Dann", "war", "da", "ei\u00b7ne", "al\u00b7te", "Wan\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die, leck, schon l\u00e4ngst kein Wasser sah,", "tokens": ["Die", ",", "leck", ",", "schon", "l\u00e4ngst", "kein", "Was\u00b7ser", "sah", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "$,", "ADV", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und eine alte G\u00e4rtnerkanne,", "tokens": ["Und", "ei\u00b7ne", "al\u00b7te", "G\u00e4rt\u00b7ner\u00b7kan\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durchlocht und ganz zerbeult, war da.", "tokens": ["Durch\u00b7locht", "und", "ganz", "zer\u00b7beult", ",", "war", "da", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VVPP", "$,", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das war ein Trommeln und Trompeten!", "tokens": ["Das", "war", "ein", "Trom\u00b7meln", "und", "Trom\u00b7pe\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Herr Hauptmann und Herr General!", "tokens": ["Herr", "Haupt\u00b7mann", "und", "Herr", "Ge\u00b7ne\u00b7ral", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Das ganze Bataillon antreten!", "tokens": ["Das", "gan\u00b7ze", "Ba\u00b7tail\u00b7lon", "an\u00b7tre\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Zu Pferde stieg der Feldmarschall.", "tokens": ["Zu", "Pfer\u00b7de", "stieg", "der", "Feld\u00b7mar\u00b7schall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und ja das Pferd! Kein Bl\u00fccher dr\u00fcckte", "tokens": ["Und", "ja", "das", "Pferd", "!", "Kein", "Bl\u00fc\u00b7cher", "dr\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "$.", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein bessres unterm Siegeslaub.", "tokens": ["Ein", "bess\u00b7res", "un\u00b7term", "Sie\u00b7ges\u00b7laub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort flo\u00df der Rhein, der \u00fcberbr\u00fcckte,", "tokens": ["Dort", "flo\u00df", "der", "Rhein", ",", "der", "\u00fc\u00b7berb\u00b7r\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dort der Kartoffelsack war Kaub.", "tokens": ["Dort", "der", "Kar\u00b7tof\u00b7fel\u00b7sack", "war", "Kaub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der alte brave Schaukelschecke", "tokens": ["Der", "al\u00b7te", "bra\u00b7ve", "Schau\u00b7kel\u00b7sche\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Fiel vom Galopp in pleine carri\u00e8re", "tokens": ["Fiel", "vom", "Ga\u00b7lopp", "in", "plei\u00b7ne", "car\u00b7ri\u00e8\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "FM", "FM"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Und mitten in der Feinde Heer,", "tokens": ["Und", "mit\u00b7ten", "in", "der", "Fein\u00b7de", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kam er dabei auch nicht vom Flecke.", "tokens": ["Kam", "er", "da\u00b7bei", "auch", "nicht", "vom", "Fle\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PAV", "ADV", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach Gott, was schlossen diese W\u00e4nde", "tokens": ["Ach", "Gott", ",", "was", "schlos\u00b7sen", "die\u00b7se", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "PWS", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht alles ein, die ganze Welt,", "tokens": ["Nicht", "al\u00b7les", "ein", ",", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von einem bis zum andern Ende,", "tokens": ["Von", "ei\u00b7nem", "bis", "zum", "an\u00b7dern", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "War zwischen ihnen aufgestellt.", "tokens": ["War", "zwi\u00b7schen", "ih\u00b7nen", "auf\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im schiefen Dach das kleine Fenster", "tokens": ["Im", "schie\u00b7fen", "Dach", "das", "klei\u00b7ne", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Warf Licht in ein unendlich Land,", "tokens": ["Warf", "Licht", "in", "ein", "un\u00b7end\u00b7lich", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Wo R\u00e4uber, K\u00f6nige und Gespenster", "tokens": ["Wo", "R\u00e4u\u00b7ber", ",", "K\u00f6\u00b7ni\u00b7ge", "und", "Ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.8": {"text": "Das Kind in jedem Winkel fand.", "tokens": ["Das", "Kind", "in", "je\u00b7dem", "Win\u00b7kel", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O k\u00f6nnt ich einmal noch im Leben", "tokens": ["O", "k\u00f6nnt", "ich", "ein\u00b7mal", "noch", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die knarrenden Stufen da hinan,", "tokens": ["Die", "knar\u00b7ren\u00b7den", "Stu\u00b7fen", "da", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die alte schwere Luke heben", "tokens": ["Die", "al\u00b7te", "schwe\u00b7re", "Lu\u00b7ke", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NE", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und in der Bodenkammer dann", "tokens": ["Und", "in", "der", "Bo\u00b7den\u00b7kam\u00b7mer", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch einmal auf dem Schaukelpferde", "tokens": ["Noch", "ein\u00b7mal", "auf", "dem", "Schau\u00b7kel\u00b7pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Napoleon in \u00c4gypten sein", "tokens": ["Na\u00b7po\u00b7le\u00b7on", "in", "\u00c4\u00b7gyp\u00b7ten", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "PPOSAT"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und mit tyrannischer Geberde", "tokens": ["Und", "mit", "ty\u00b7ran\u00b7ni\u00b7scher", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Welt in Grund und Boden schrein.", "tokens": ["Die", "Welt", "in", "Grund", "und", "Bo\u00b7den", "schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"],