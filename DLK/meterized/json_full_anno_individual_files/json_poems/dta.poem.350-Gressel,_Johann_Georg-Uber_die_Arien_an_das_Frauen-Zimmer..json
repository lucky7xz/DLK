{"dta.poem.350": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uber die  Arien  an das Frauen-Zimmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Nehmt ihr Sch\u00f6nen diese Lieder", "tokens": ["Nehmt", "ihr", "Sch\u00f6\u00b7nen", "die\u00b7se", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit geneigten H\u00e4nden an/", "tokens": ["Mit", "ge\u00b7neig\u00b7ten", "H\u00e4n\u00b7den", "an", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eros findet sich hier wieder/", "tokens": ["E\u00b7ros", "fin\u00b7det", "sich", "hier", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADV", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn man ihn nicht finden kan.", "tokens": ["Wenn", "man", "ihn", "nicht", "fin\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Er wird euch in diesen Myrthen", "tokens": ["Er", "wird", "euch", "in", "die\u00b7sen", "Myr\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Mit beliebter Lust bewirthen.", "tokens": ["Mit", "be\u00b7lieb\u00b7ter", "Lust", "be\u00b7wirt\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eure H\u00e4nde/ Mund und Br\u00fcste/", "tokens": ["Eu\u00b7re", "H\u00e4n\u00b7de", "/", "Mund", "und", "Br\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die/ die ", "tokens": ["Die", "/", "die"], "token_info": ["word", "punct", "word"], "pos": ["ART", "$(", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Lieben dieses Kraut der L\u00fcste/", "tokens": ["Lie\u00b7ben", "die\u00b7ses", "Kraut", "der", "L\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "PDAT", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So das letzte Labsahl giebt.", "tokens": ["So", "das", "letz\u00b7te", "Lab\u00b7sahl", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach so liebt/ die Gluth von oben", "tokens": ["Ach", "so", "liebt", "/", "die", "Gluth", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "$(", "ART", "NN", "APPR", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird den guten Endschlu\u00df loben", "tokens": ["Wird", "den", "gu\u00b7ten", "End\u00b7schlu\u00df", "lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Endlich schliesset meiue Leiche", "tokens": ["End\u00b7lich", "schlies\u00b7set", "mei\u00b7ue", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Angenehme G\u00f6tter-Art", "tokens": ["An\u00b7ge\u00b7neh\u00b7me", "G\u00f6t\u00b7ter\u00b7Art"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein iu gr\u00fcne Myrthen-Str\u00e4uche;", "tokens": ["Ein", "i\u00b7u", "gr\u00fc\u00b7ne", "Myr\u00b7then\u00b7Str\u00e4u\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Alsdenn bin ich wohl verwahrt/", "tokens": ["Als\u00b7denn", "bin", "ich", "wohl", "ver\u00b7wahrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und erlange vor die Lieder", "tokens": ["Und", "er\u00b7lan\u00b7ge", "vor", "die", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Etwas zur Belohnung wieder.", "tokens": ["Et\u00b7was", "zur", "Be\u00b7loh\u00b7nung", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}