{"dta.poem.19450": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Nachtger\u00e4usche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Melde mir die Nachtger\u00e4usche, Muse,", "tokens": ["Mel\u00b7de", "mir", "die", "Nacht\u00b7ge\u00b7r\u00e4u\u00b7sche", ",", "Mu\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die ans Ohr des Schlummerlosen fluthen!", "tokens": ["Die", "ans", "Ohr", "des", "Schlum\u00b7mer\u00b7lo\u00b7sen", "flut\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Erst das traute Wachtgebell der Hunde,", "tokens": ["Erst", "das", "trau\u00b7te", "Wacht\u00b7ge\u00b7bell", "der", "Hun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dann der abgez\u00e4hlte Schlag der Stunde,", "tokens": ["Dann", "der", "ab\u00b7ge\u00b7z\u00e4hl\u00b7te", "Schlag", "der", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Dann ein Fischer-Zwiegespr\u00e4ch am Ufer,", "tokens": ["Dann", "ein", "Fi\u00b7scher\u00b7Zwie\u00b7ge\u00b7spr\u00e4ch", "am", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dann? Nichts weiter als der ungewisse", "tokens": ["Dann", "?", "Nichts", "wei\u00b7ter", "als", "der", "un\u00b7ge\u00b7wis\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "PIS", "ADV", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Geisterlaut der ungebrochnen Stille,", "tokens": ["Geis\u00b7ter\u00b7laut", "der", "un\u00b7ge\u00b7broch\u00b7nen", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wie das Athmen eines jungen Busens,", "tokens": ["Wie", "das", "Ath\u00b7men", "ei\u00b7nes", "jun\u00b7gen", "Bu\u00b7sens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Wie das Murmeln eines tiefen Brunnens,", "tokens": ["Wie", "das", "Mur\u00b7meln", "ei\u00b7nes", "tie\u00b7fen", "Brun\u00b7nens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Wie das Schlagen eines dumpfen Ruders,", "tokens": ["Wie", "das", "Schla\u00b7gen", "ei\u00b7nes", "dum\u00b7pfen", "Ru\u00b7ders", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Dann des Schlummers leise leise Tritte.", "tokens": ["Dann", "des", "Schlum\u00b7mers", "lei\u00b7se", "lei\u00b7se", "Trit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}