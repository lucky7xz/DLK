{"dta.poem.5477": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00e4serne Kugel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf einer Kugel, die von Glas, und auf gewisse Art\nverg\u00fcldet,", "tokens": ["Auf", "ei\u00b7ner", "Ku\u00b7gel", ",", "die", "von", "Glas", ",", "und", "auf", "ge\u00b7wis\u00b7se", "Art", "ver\u00b7g\u00fcl\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "NN", "$,", "KON", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Sah ich die Vorw\u00fcrff \u00fcberall, Bewundrungs-w\u00fcrdig-", "tokens": ["Sah", "ich", "die", "Vor\u00b7w\u00fcrff", "\u00fc\u00b7be\u00b7rall", ",", "Be\u00b7wun\u00b7drungs\u00b7w\u00fcr\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit unverwendten Blicken, an. Unglaublich ist, wie", "tokens": ["Mit", "un\u00b7ver\u00b7wend\u00b7ten", "Bli\u00b7cken", ",", "an", ".", "Un\u00b7glaub\u00b7lich", "ist", ",", "wie"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PTKVZ", "$.", "ADJD", "VAFIN", "$,", "PWAV"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie nett und zierlich alle C\u00f6rper verkleinert und formiret", "tokens": ["Wie", "nett", "und", "zier\u00b7lich", "al\u00b7le", "C\u00f6r\u00b7per", "ver\u00b7klei\u00b7nert", "und", "for\u00b7mi\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "PIAT", "NN", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+--+--+-+", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Indem ich es erstaunt betrachte, f\u00e4llt mir nicht sonder", "tokens": ["In\u00b7dem", "ich", "es", "er\u00b7staunt", "be\u00b7trach\u00b7te", ",", "f\u00e4llt", "mir", "nicht", "son\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df diese glatt\u2019 und runde Kugel ein Bild von einem Au-", "tokens": ["Da\u00df", "die\u00b7se", "glatt'", "und", "run\u00b7de", "Ku\u00b7gel", "ein", "Bild", "von", "ei\u00b7nem", "Au"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "VVFIN", "KON", "ADJA", "NN", "ART", "NN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Der Unterscheid steckt blos darin, da\u00df von den C\u00f6rperli-", "tokens": ["Der", "Un\u00b7ter\u00b7scheid", "steckt", "blos", "da\u00b7rin", ",", "da\u00df", "von", "den", "C\u00f6\u00b7rper\u00b7li"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PAV", "$,", "KOUS", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Bilder auf der Kugel nur formirt, und gleichsam", "tokens": ["Die", "Bil\u00b7der", "auf", "der", "Ku\u00b7gel", "nur", "for\u00b7mirt", ",", "und", "gleich\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADV", "VVPP", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da sie hingegen in das Auge, ja g\u00e4ntzlich durch dasselbe", "tokens": ["Da", "sie", "hin\u00b7ge\u00b7gen", "in", "das", "Au\u00b7ge", ",", "ja", "g\u00e4ntz\u00b7lich", "durch", "das\u00b7sel\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "$,", "ADV", "ADJD", "APPR", "PDAT"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Und auch die untre R\u00fcnde r\u00fchren, ja durch ein Nervgen", "tokens": ["Und", "auch", "die", "un\u00b7tre", "R\u00fcn\u00b7de", "r\u00fch\u00b7ren", ",", "ja", "durch", "ein", "Nerv\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVINF", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.7": {"text": "Wodurch im menschlichen Gehirne, so dann Betrachtun-", "tokens": ["Wo\u00b7durch", "im", "menschli\u00b7chen", "Ge\u00b7hir\u00b7ne", ",", "so", "dann", "Be\u00b7trach\u00b7tun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "$,", "ADV", "ADV", "TRUNC"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Wenn nur das Nervgen nicht verstopft. Weil sonst nicht", "tokens": ["Wenn", "nur", "das", "Nerv\u00b7gen", "nicht", "ver\u00b7stopft", ".", "Weil", "sonst", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$.", "KOUS", "ADV", "PTKNEG"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Der Menschen Augen was sie sehn, zu sehn und zu be-", "tokens": ["Der", "Men\u00b7schen", "Au\u00b7gen", "was", "sie", "sehn", ",", "zu", "sehn", "und", "zu", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PWS", "PPER", "VVINF", "$,", "PTKZU", "VVINF", "KON", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn nun beym Sehen, ohne Dencken, die Menschen kei-", "tokens": ["Wenn", "nun", "beym", "Se\u00b7hen", ",", "oh\u00b7ne", "Den\u00b7cken", ",", "die", "Men\u00b7schen", "kei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "NN", "$,", "KOUI", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "So fiel, so dir als mir zur Lehre, mir folgende Vermah-", "tokens": ["So", "fiel", ",", "so", "dir", "als", "mir", "zur", "Leh\u00b7re", ",", "mir", "fol\u00b7gen\u00b7de", "Ver\u00b7mah"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "PPER", "KOUS", "PPER", "APPRART", "NN", "$,", "PPER", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+---+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "La\u00df doch, bey aller Pracht der Wunder hier auf", "tokens": ["La\u00df", "doch", ",", "bey", "al\u00b7ler", "Pracht", "der", "Wun\u00b7der", "hier", "auf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "APPR", "PIAT", "NN", "ART", "NN", "ADV", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Dein Auge, lieber Mensch, kein Ochsen-Auge werden!", "tokens": ["Dein", "Au\u00b7ge", ",", "lie\u00b7ber", "Mensch", ",", "kein", "Och\u00b7sen\u00b7Au\u00b7ge", "wer\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "NN", "$,", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach nein!", "tokens": ["Ach", "nein", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKANT", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "La\u00df es, zu GOttes Ruhm, ein Menschen Auge seyn!", "tokens": ["La\u00df", "es", ",", "zu", "Got\u00b7tes", "Ruhm", ",", "ein", "Men\u00b7schen", "Au\u00b7ge", "seyn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "APPR", "NN", "NN", "$,", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er\u00f6fne die an ihm befindlichen Can\u00e4le,", "tokens": ["Er\u00b7\u00f6f\u00b7ne", "die", "an", "ihm", "be\u00b7find\u00b7li\u00b7chen", "Ca\u00b7n\u00e4\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und la\u00df die Pracht von den erschaffnen Dingen,", "tokens": ["Und", "la\u00df", "die", "Pracht", "von", "den", "er\u00b7schaff\u00b7nen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Durch sie, sich ins Gehirn, den Sitz der Seele,", "tokens": ["Durch", "sie", ",", "sich", "ins", "Ge\u00b7hirn", ",", "den", "Sitz", "der", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRF", "APPRART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ja in die Seele selber dringen.", "tokens": ["Ja", "in", "die", "See\u00b7le", "sel\u00b7ber", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "La\u00df durch Gewohnheit dir die Th\u00fcren nicht verriegeln,", "tokens": ["La\u00df", "durch", "Ge\u00b7wohn\u00b7heit", "dir", "die", "Th\u00fc\u00b7ren", "nicht", "ver\u00b7rie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "NN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wodurch die Welt mit dir, du mit der Welt, vereint,", "tokens": ["Wo\u00b7durch", "die", "Welt", "mit", "dir", ",", "du", "mit", "der", "Welt", ",", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPER", "$,", "PPER", "APPR", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch die der GOTTHEIT Glantz, als wie von hellen", "tokens": ["Durch", "die", "der", "GoT\u00b7T\u00b7HEIT", "Glantz", ",", "als", "wie", "von", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "NN", "$,", "KOUS", "KOKOM", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus seinen Wercken wiederscheint!", "tokens": ["Aus", "sei\u00b7nen", "Wer\u00b7cken", "wie\u00b7der\u00b7scheint", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wir sehen ja das eigentliche Licht,", "tokens": ["Wir", "se\u00b7hen", "ja", "das", "ei\u00b7gent\u00b7li\u00b7che", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ohn einen Gegenschlag von festen C\u00f6rpern, nicht.", "tokens": ["Ohn", "ei\u00b7nen", "Ge\u00b7gen\u00b7schlag", "von", "fes\u00b7ten", "C\u00f6r\u00b7pern", ",", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So kann man auch der GOttheit Lieb und Macht", "tokens": ["So", "kann", "man", "auch", "der", "Got\u00b7theit", "Lieb", "und", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ohn seiner Creaturen Pracht", "tokens": ["Ohn", "sei\u00b7ner", "Crea\u00b7tu\u00b7ren", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Unm\u00f6glich sehn, erkennen und verehren.", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "sehn", ",", "er\u00b7ken\u00b7nen", "und", "ver\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Creaturen sinds allein,", "tokens": ["Die", "Crea\u00b7tu\u00b7ren", "sinds", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die uns von seiner GOttheit Schein", "tokens": ["Die", "uns", "von", "sei\u00b7ner", "Got\u00b7theit", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die herrliche Beschaffenheit erkl\u00e4ren.", "tokens": ["Die", "herr\u00b7li\u00b7che", "Be\u00b7schaf\u00b7fen\u00b7heit", "er\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Drey Dinge braucht ein Thier zum Sehen: das ", "tokens": ["Drey", "Din\u00b7ge", "braucht", "ein", "Thier", "zum", "Se\u00b7hen", ":", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$.", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Wer aber als ein Mensch will sehen, mu\u00df das ", "tokens": ["Wer", "a\u00b7ber", "als", "ein", "Mensch", "will", "se\u00b7hen", ",", "mu\u00df", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ADV", "KOUS", "ART", "NN", "VMFIN", "VVINF", "$,", "VMFIN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Annoch zu diesen dreyen lencken,", "tokens": ["An\u00b7noch", "zu", "die\u00b7sen", "drey\u00b7en", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "CARD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und diese Seelen-Kraft noch zu den andern f\u00fcgen;", "tokens": ["Und", "die\u00b7se", "See\u00b7len\u00b7Kraft", "noch", "zu", "den", "an\u00b7dern", "f\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ADV", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sonst hat der Mensch von allem, was auf Erden,", "tokens": ["Sonst", "hat", "der", "Mensch", "von", "al\u00b7lem", ",", "was", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PIS", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Kein\u2019 eigentliche Lust, kein menschliches Vergn\u00fcgen,", "tokens": ["Kein'", "ei\u00b7gent\u00b7li\u00b7che", "Lust", ",", "kein", "menschli\u00b7ches", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und GOtt kann nicht gedanckt noch angebehtet werden.", "tokens": ["Und", "Gott", "kann", "nicht", "ge\u00b7danckt", "noch", "an\u00b7ge\u00b7beh\u00b7tet", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "PTKNEG", "VVPP", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}