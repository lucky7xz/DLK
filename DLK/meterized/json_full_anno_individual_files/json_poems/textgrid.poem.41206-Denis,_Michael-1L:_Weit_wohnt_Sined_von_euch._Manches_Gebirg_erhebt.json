.30": {"line.1": {"text": "Sohn der Moldau! dein Herz gleichet der Sommernacht,", "tokens": ["Sohn", "der", "Mol\u00b7dau", "!", "dein", "Herz", "glei\u00b7chet", "der", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wenn der schweigende Mond \u00fcber bethauete", "tokens": ["Wenn", "der", "schwei\u00b7gen\u00b7de", "Mond", "\u00fc\u00b7ber", "be\u00b7thau\u00b7e\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ADJA"], "meter": "--+--++---+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Segenreiche Gefilde", "tokens": ["Se\u00b7gen\u00b7rei\u00b7che", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Voll und dunstlos hernieder h\u00e4ngt.", "tokens": ["Voll", "und", "dunst\u00b7los", "her\u00b7nie\u00b7der", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PTKVZ", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.31": {"line.1": {"text": "Lang umflo\u00df mich sein Licht; aber nun leuchtet es", "tokens": ["Lang", "um\u00b7flo\u00df", "mich", "sein", "Licht", ";", "a\u00b7ber", "nun", "leuch\u00b7tet", "es"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "ADV", "ADV", "VVFIN", "PPER"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Fernen H\u00fcgeln, seitdem unsrer Beherrscherinn", "tokens": ["Fer\u00b7nen", "H\u00fc\u00b7geln", ",", "seit\u00b7dem", "uns\u00b7rer", "Be\u00b7herr\u00b7sche\u00b7rinn"], "token_info": ["word", "word", "pu