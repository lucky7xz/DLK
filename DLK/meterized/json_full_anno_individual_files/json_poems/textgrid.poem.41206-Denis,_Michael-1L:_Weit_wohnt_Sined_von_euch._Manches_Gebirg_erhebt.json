{"textgrid.poem.41206": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weit wohnt Sined von euch. Manches Gebirg erhebt", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weit wohnt Sined von euch. Manches Gebirg erhebt", "tokens": ["Weit", "wohnt", "Si\u00b7ned", "von", "euch", ".", "Man\u00b7ches", "Ge\u00b7birg", "er\u00b7hebt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "NE", "APPR", "PPER", "$.", "PIAT", "NN", "VVFIN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Uns zu trennen sein Haupt m\u00e4chtig, und mancher Flu\u00df", "tokens": ["Uns", "zu", "tren\u00b7nen", "sein", "Haupt", "m\u00e4ch\u00b7tig", ",", "und", "man\u00b7cher", "Flu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "PPOSAT", "NN", "ADJD", "$,", "KON", "PIAT", "NN"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Rauscht entzwischen, o W\u00e4lder,", "tokens": ["Rauscht", "ent\u00b7zwi\u00b7schen", ",", "o", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "FM", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Die mein kindlicher Fu\u00df betrat!", "tokens": ["Die", "mein", "kind\u00b7li\u00b7cher", "Fu\u00df", "be\u00b7trat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "In den Tagen des Herbst's, wenn sich der Abend br\u00e4unt.", "tokens": ["In", "den", "Ta\u00b7gen", "des", "Herbst's", ",", "wenn", "sich", "der", "A\u00b7bend", "br\u00e4unt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NE", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Irr' ich einsam den Hain, irr' ich die Fluren durch,", "tokens": ["Irr'", "ich", "ein\u00b7sam", "den", "Hain", ",", "irr'", "ich", "die", "Flu\u00b7ren", "durch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dann zeucht meine Gedanken", "tokens": ["Dann", "zeucht", "mei\u00b7ne", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Euer freundliches Bild an sich.", "tokens": ["Eu\u00b7er", "freund\u00b7li\u00b7ches", "Bild", "an", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PRF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Ja, dann seyd ihr vor mir, W\u00e4lder mit seufzenden", "tokens": ["Ja", ",", "dann", "seyd", "ihr", "vor", "mir", ",", "W\u00e4l\u00b7der", "mit", "seuf\u00b7zen\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "PPER", "APPR", "PPER", "$,", "NN", "APPR", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Tannen! bist du vor mir, sprudelnder Erlenbach!", "tokens": ["Tan\u00b7nen", "!", "bist", "du", "vor", "mir", ",", "spru\u00b7deln\u00b7der", "Er\u00b7len\u00b7bach", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "APPR", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und ihr Teiche voll Schilfes,", "tokens": ["Und", "ihr", "Tei\u00b7che", "voll", "Schil\u00b7fes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Von dem k\u00fchlenden West' umrauscht!", "tokens": ["Von", "dem", "k\u00fch\u00b7len\u00b7den", "West'", "um\u00b7rauscht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Wo mein Leben begann, wo sich Allvater mir", "tokens": ["Wo", "mein", "Le\u00b7ben", "be\u00b7gann", ",", "wo", "sich", "All\u00b7va\u00b7ter", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$,", "PWAV", "PRF", "NN", "PPER"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Im Gestirne der Nacht, in der gebl\u00fcmten Flur", "tokens": ["Im", "Ge\u00b7stir\u00b7ne", "der", "Nacht", ",", "in", "der", "ge\u00b7bl\u00fcm\u00b7ten", "Flur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Offenbarte, der Fr\u00fchling", "tokens": ["Of\u00b7fen\u00b7bar\u00b7te", ",", "der", "Fr\u00fch\u00b7ling"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Meiner Tage mir heiter flo\u00df.", "tokens": ["Mei\u00b7ner", "Ta\u00b7ge", "mir", "hei\u00b7ter", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Wo mich, wenn mich ein Spiel unter die d\u00e4mmernden", "tokens": ["Wo", "mich", ",", "wenn", "mich", "ein", "Spiel", "un\u00b7ter", "die", "d\u00e4m\u00b7mern\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-++--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Eichenschatten berief, heiligen Schauer hub,", "tokens": ["Ei\u00b7chen\u00b7schat\u00b7ten", "be\u00b7rief", ",", "hei\u00b7li\u00b7gen", "Schau\u00b7er", "hub", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Da\u00df den bebenden H\u00e4nden", "tokens": ["Da\u00df", "den", "be\u00b7ben\u00b7den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Pl\u00f6tzlich jeglicher Tand entfiel,", "tokens": ["Pl\u00f6tz\u00b7lich", "jeg\u00b7li\u00b7cher", "Tand", "ent\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Von den Lippen ein Laut str\u00f6mte, dem Liede gleich,", "tokens": ["Von", "den", "Lip\u00b7pen", "ein", "Laut", "str\u00f6m\u00b7te", ",", "dem", "Lie\u00b7de", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "APPR", "VVFIN", "$,", "ART", "NN", "ADV", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Das im Busche des Herbst's furchtsam ein Wipfelkind,", "tokens": ["Das", "im", "Bu\u00b7sche", "des", "Herbst's", "furcht\u00b7sam", "ein", "Wip\u00b7fel\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "ART", "NN", "ADJD", "ART", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "J\u00fcngst vom Neste geflogen,", "tokens": ["J\u00fcngst", "vom", "Nes\u00b7te", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Mit noch stammelnder Kehle wagt.", "tokens": ["Mit", "noch", "stam\u00b7meln\u00b7der", "Keh\u00b7le", "wagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "O dann wird es in mir still, wie der Abend ist,", "tokens": ["O", "dann", "wird", "es", "in", "mir", "still", ",", "wie", "der", "A\u00b7bend", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VAFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,", "PWAV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wenn sein mildes Gesicht keine der Wolken tr\u00fcbt,", "tokens": ["Wenn", "sein", "mil\u00b7des", "Ge\u00b7sicht", "kei\u00b7ne", "der", "Wol\u00b7ken", "tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PIAT", "ART", "NN", "VVFIN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "W\u00e4lder schweigen, aus Westen", "tokens": ["W\u00e4l\u00b7der", "schwei\u00b7gen", ",", "aus", "Wes\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$,", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Noch die scheidende Sonne blickt.", "tokens": ["Noch", "die", "schei\u00b7den\u00b7de", "Son\u00b7ne", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Nur der sanftere Wunsch, freundliche Gegenden,", "tokens": ["Nur", "der", "sanf\u00b7te\u00b7re", "Wunsch", ",", "freund\u00b7li\u00b7che", "Ge\u00b7gen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Wo mein Leben begann! immer um euch zu seyn,", "tokens": ["Wo", "mein", "Le\u00b7ben", "be\u00b7gann", "!", "im\u00b7mer", "um", "euch", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$.", "ADV", "APPR", "PPER", "PTKZU", "VAINF", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Bis mein Leben verhauchet,", "tokens": ["Bis", "mein", "Le\u00b7ben", "ver\u00b7hau\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Der nur lispelt im Herzen noch.", "tokens": ["Der", "nur", "lis\u00b7pelt", "im", "Her\u00b7zen", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPRART", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Aber, lispelnder Wunsch! w\u00e4rest du mehr als Wunsch,", "tokens": ["A\u00b7ber", ",", "lis\u00b7peln\u00b7der", "Wunsch", "!", "w\u00e4\u00b7rest", "du", "mehr", "als", "Wunsch", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$.", "VAFIN", "PPER", "PIAT", "KOKOM", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Niemal h\u00e4tte mein Aug' Sie, die Gebieterin,", "tokens": ["Nie\u00b7mal", "h\u00e4t\u00b7te", "mein", "Aug'", "Sie", ",", "die", "Ge\u00b7bie\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Sie die Mutter der Menschen,", "tokens": ["Sie", "die", "Mut\u00b7ter", "der", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sie, das Wunder der Zeit gesehn;", "tokens": ["Sie", ",", "das", "Wun\u00b7der", "der", "Zeit", "ge\u00b7sehn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Niemal h\u00e4tte mein Ohr Worte der Huld von ihr", "tokens": ["Nie\u00b7mal", "h\u00e4t\u00b7te", "mein", "Ohr", "Wor\u00b7te", "der", "Huld", "von", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "ART", "NN", "APPR", "PPOSAT"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Freudetrunken geh\u00f6rt, niemal mein Harfenspiel", "tokens": ["Freu\u00b7de\u00b7trun\u00b7ken", "ge\u00b7h\u00f6rt", ",", "nie\u00b7mal", "mein", "Har\u00b7fen\u00b7spiel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ADV", "PPOSAT", "NN"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ihrem Lobe gestimmet", "tokens": ["Ih\u00b7rem", "Lo\u00b7be", "ge\u00b7stim\u00b7met"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ruhm im Erbe von Teut erjauchzt.", "tokens": ["Ruhm", "im", "Er\u00b7be", "von", "Teut", "er\u00b7jauchzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Niemal w\u00e4re mein Blick ihrem erhabenen", "tokens": ["Nie\u00b7mal", "w\u00e4\u00b7re", "mein", "Blick", "ih\u00b7rem", "er\u00b7ha\u00b7be\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPOSAT", "ADJA"], "meter": "+-+-+++--+--", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ersterzeugten gefolgt, wann er in Tausende", "tokens": ["E\u00b7rster\u00b7zeug\u00b7ten", "ge\u00b7folgt", ",", "wann", "er", "in", "Tau\u00b7sen\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "PWAV", "PPER", "APPR", "NN"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.3": {"text": "Muth verbreitet, mich h\u00e4tte", "tokens": ["Muth", "ver\u00b7brei\u00b7tet", ",", "mich", "h\u00e4t\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$,", "PPER", "VAFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Joseph's Barden kein Mund genannt.", "tokens": ["Jo\u00b7se\u00b7ph's", "Bar\u00b7den", "kein", "Mund", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Niemal h\u00e4tt' ich auch dich, Diener Theresien's,", "tokens": ["Nie\u00b7mal", "h\u00e4tt'", "ich", "auch", "dich", ",", "Die\u00b7ner", "The\u00b7re\u00b7si\u00b7en's", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "$,", "NN", "NE", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Joseph's Diener! geseh'n, niemal dein Herz gekannt,", "tokens": ["Jo\u00b7se\u00b7ph's", "Die\u00b7ner", "!", "ge\u00b7seh'n", ",", "nie\u00b7mal", "dein", "Herz", "ge\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVPP", "$,", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+---+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Mann der Treue, des Rathes,", "tokens": ["Mann", "der", "Treu\u00b7e", ",", "des", "Ra\u00b7thes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Bardeng\u00f6nner und Menschenfreund!", "tokens": ["Bar\u00b7den\u00b7g\u00f6n\u00b7ner", "und", "Men\u00b7schen\u00b7freund", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.13": {"line.1": {"text": "Sohn der Moldau! dein Herz gleichet der Sommernacht,", "tokens": ["Sohn", "der", "Mol\u00b7dau", "!", "dein", "Herz", "glei\u00b7chet", "der", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wenn der schweigende Mond \u00fcber bethauete", "tokens": ["Wenn", "der", "schwei\u00b7gen\u00b7de", "Mond", "\u00fc\u00b7ber", "be\u00b7thau\u00b7e\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ADJA"], "meter": "--+--++---+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Segenreiche Gefilde", "tokens": ["Se\u00b7gen\u00b7rei\u00b7che", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Voll und dunstlos hernieder h\u00e4ngt.", "tokens": ["Voll", "und", "dunst\u00b7los", "her\u00b7nie\u00b7der", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PTKVZ", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.14": {"line.1": {"text": "Lang umflo\u00df mich sein Licht; aber nun leuchtet es", "tokens": ["Lang", "um\u00b7flo\u00df", "mich", "sein", "Licht", ";", "a\u00b7ber", "nun", "leuch\u00b7tet", "es"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "ADV", "ADV", "VVFIN", "PPER"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Fernen H\u00fcgeln, seitdem unsrer Beherrscherinn", "tokens": ["Fer\u00b7nen", "H\u00fc\u00b7geln", ",", "seit\u00b7dem", "uns\u00b7rer", "Be\u00b7herr\u00b7sche\u00b7rinn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Muttersorge dich deinem", "tokens": ["Mut\u00b7ter\u00b7sor\u00b7ge", "dich", "dei\u00b7nem"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "PPOSAT"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Vaterlande zur\u00fccke gab", "tokens": ["Va\u00b7ter\u00b7lan\u00b7de", "zu\u00b7r\u00fc\u00b7cke", "gab"], "token_info": ["word", "word", "word"], "pos": ["NN", "PTKVZ", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.15": {"line.1": {"text": "Dunkel w\u00e4r' es um mich, kl\u00e4nge dem Ohre nicht", "tokens": ["Dun\u00b7kel", "w\u00e4r'", "es", "um", "mich", ",", "kl\u00e4n\u00b7ge", "dem", "Oh\u00b7re", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "APPR", "PPER", "$,", "VVFIN", "ART", "NN", "PTKNEG"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Deine Stimme noch jetzt, tr\u00fcg' ich im Herzen nicht", "tokens": ["Dei\u00b7ne", "Stim\u00b7me", "noch", "jetzt", ",", "tr\u00fcg'", "ich", "im", "Her\u00b7zen", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "$,", "VVFIN", "PPER", "APPRART", "NN", "PTKNEG"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Unaustilgbar gezeichnet", "tokens": ["Un\u00b7au\u00b7stilg\u00b7bar", "ge\u00b7zeich\u00b7net"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Dein erhellendes Angesicht.", "tokens": ["Dein", "er\u00b7hel\u00b7len\u00b7des", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.16": {"line.1": {"text": "Freund von Sined! Es beut Titan uns Licht, der Baum", "tokens": ["Freund", "von", "Si\u00b7ned", "!", "Es", "beut", "Ti\u00b7tan", "uns", "Licht", ",", "der", "Baum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NE", "$.", "PPER", "VVFIN", "NN", "PPER", "NN", "$,", "ART", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Schatten, Labung der Quell, D\u00fcfte der Bl\u00fcthenstrauch,", "tokens": ["Schat\u00b7ten", ",", "La\u00b7bung", "der", "Quell", ",", "D\u00fcf\u00b7te", "der", "Bl\u00fc\u00b7then\u00b7strauch", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Und ein Barde den Ausbruch", "tokens": ["Und", "ein", "Bar\u00b7de", "den", "Aus\u00b7bruch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Seines Herzen, ein feurig Lied.", "tokens": ["Sei\u00b7nes", "Her\u00b7zen", ",", "ein", "feu\u00b7rig", "Lied", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJD", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.17": {"line.1": {"text": "Nimm es, was dir den Herbst Sined im Haine sang,", "tokens": ["Nimm", "es", ",", "was", "dir", "den", "Herbst", "Si\u00b7ned", "im", "Hai\u00b7ne", "sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "PWS", "PPER", "ART", "NN", "NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, indem sich dein Bild lebhaft vor ihm entwarf,", "tokens": ["Der", ",", "in\u00b7dem", "sich", "dein", "Bild", "leb\u00b7haft", "vor", "ihm", "ent\u00b7warf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PRF", "PPOSAT", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Selbst der freundlichen Gegend,", "tokens": ["Selbst", "der", "freund\u00b7li\u00b7chen", "Ge\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wo sein Leben begann, verga\u00df.", "tokens": ["Wo", "sein", "Le\u00b7ben", "be\u00b7gann", ",", "ver\u00b7ga\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.18": {"line.1": {"text": "Weit wohnt Sined von euch. Manches Gebirg erhebt", "tokens": ["Weit", "wohnt", "Si\u00b7ned", "von", "euch", ".", "Man\u00b7ches", "Ge\u00b7birg", "er\u00b7hebt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "NE", "APPR", "PPER", "$.", "PIAT", "NN", "VVFIN"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Uns zu trennen sein Haupt m\u00e4chtig, und mancher Flu\u00df", "tokens": ["Uns", "zu", "tren\u00b7nen", "sein", "Haupt", "m\u00e4ch\u00b7tig", ",", "und", "man\u00b7cher", "Flu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "PPOSAT", "NN", "ADJD", "$,", "KON", "PIAT", "NN"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Rauscht entzwischen, o W\u00e4lder,", "tokens": ["Rauscht", "ent\u00b7zwi\u00b7schen", ",", "o", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "FM", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Die mein kindlicher Fu\u00df betrat!", "tokens": ["Die", "mein", "kind\u00b7li\u00b7cher", "Fu\u00df", "be\u00b7trat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.19": {"line.1": {"text": "In den Tagen des Herbst's, wenn sich der Abend br\u00e4unt.", "tokens": ["In", "den", "Ta\u00b7gen", "des", "Herbst's", ",", "wenn", "sich", "der", "A\u00b7bend", "br\u00e4unt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NE", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Irr' ich einsam den Hain, irr' ich die Fluren durch,", "tokens": ["Irr'", "ich", "ein\u00b7sam", "den", "Hain", ",", "irr'", "ich", "die", "Flu\u00b7ren", "durch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dann zeucht meine Gedanken", "tokens": ["Dann", "zeucht", "mei\u00b7ne", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Euer freundliches Bild an sich.", "tokens": ["Eu\u00b7er", "freund\u00b7li\u00b7ches", "Bild", "an", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PRF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.20": {"line.1": {"text": "Ja, dann seyd ihr vor mir, W\u00e4lder mit seufzenden", "tokens": ["Ja", ",", "dann", "seyd", "ihr", "vor", "mir", ",", "W\u00e4l\u00b7der", "mit", "seuf\u00b7zen\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "PPER", "APPR", "PPER", "$,", "NN", "APPR", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Tannen! bist du vor mir, sprudelnder Erlenbach!", "tokens": ["Tan\u00b7nen", "!", "bist", "du", "vor", "mir", ",", "spru\u00b7deln\u00b7der", "Er\u00b7len\u00b7bach", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "APPR", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und ihr Teiche voll Schilfes,", "tokens": ["Und", "ihr", "Tei\u00b7che", "voll", "Schil\u00b7fes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Von dem k\u00fchlenden West' umrauscht!", "tokens": ["Von", "dem", "k\u00fch\u00b7len\u00b7den", "West'", "um\u00b7rauscht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.21": {"line.1": {"text": "Wo mein Leben begann, wo sich Allvater mir", "tokens": ["Wo", "mein", "Le\u00b7ben", "be\u00b7gann", ",", "wo", "sich", "All\u00b7va\u00b7ter", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$,", "PWAV", "PRF", "NN", "PPER"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Im Gestirne der Nacht, in der gebl\u00fcmten Flur", "tokens": ["Im", "Ge\u00b7stir\u00b7ne", "der", "Nacht", ",", "in", "der", "ge\u00b7bl\u00fcm\u00b7ten", "Flur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Offenbarte, der Fr\u00fchling", "tokens": ["Of\u00b7fen\u00b7bar\u00b7te", ",", "der", "Fr\u00fch\u00b7ling"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Meiner Tage mir heiter flo\u00df.", "tokens": ["Mei\u00b7ner", "Ta\u00b7ge", "mir", "hei\u00b7ter", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.22": {"line.1": {"text": "Wo mich, wenn mich ein Spiel unter die d\u00e4mmernden", "tokens": ["Wo", "mich", ",", "wenn", "mich", "ein", "Spiel", "un\u00b7ter", "die", "d\u00e4m\u00b7mern\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-++--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Eichenschatten berief, heiligen Schauer hub,", "tokens": ["Ei\u00b7chen\u00b7schat\u00b7ten", "be\u00b7rief", ",", "hei\u00b7li\u00b7gen", "Schau\u00b7er", "hub", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Da\u00df den bebenden H\u00e4nden", "tokens": ["Da\u00df", "den", "be\u00b7ben\u00b7den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Pl\u00f6tzlich jeglicher Tand entfiel,", "tokens": ["Pl\u00f6tz\u00b7lich", "jeg\u00b7li\u00b7cher", "Tand", "ent\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.23": {"line.1": {"text": "Von den Lippen ein Laut str\u00f6mte, dem Liede gleich,", "tokens": ["Von", "den", "Lip\u00b7pen", "ein", "Laut", "str\u00f6m\u00b7te", ",", "dem", "Lie\u00b7de", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "APPR", "VVFIN", "$,", "ART", "NN", "ADV", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Das im Busche des Herbst's furchtsam ein Wipfelkind,", "tokens": ["Das", "im", "Bu\u00b7sche", "des", "Herbst's", "furcht\u00b7sam", "ein", "Wip\u00b7fel\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "ART", "NN", "ADJD", "ART", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "J\u00fcngst vom Neste geflogen,", "tokens": ["J\u00fcngst", "vom", "Nes\u00b7te", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Mit noch stammelnder Kehle wagt.", "tokens": ["Mit", "noch", "stam\u00b7meln\u00b7der", "Keh\u00b7le", "wagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.24": {"line.1": {"text": "O dann wird es in mir still, wie der Abend ist,", "tokens": ["O", "dann", "wird", "es", "in", "mir", "still", ",", "wie", "der", "A\u00b7bend", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VAFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,", "PWAV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wenn sein mildes Gesicht keine der Wolken tr\u00fcbt,", "tokens": ["Wenn", "sein", "m