um", "Vier\u00b7tel", "sechs", ")", "\u2013", "zwei", "d\u00fcn\u00b7ne", "Kell\u00b7ner", "tra\u00b7gen"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "NN", "CARD", "$(", "$(", "CARD", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "das Eis, das jeder zu verzehren hat.", "tokens": ["das", "Eis", ",", "das", "je\u00b7der", "zu", "ver\u00b7zeh\u00b7ren", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und in der Untergrundbahn Kellerr\u00e4umen", "tokens": ["Und", "in", "der", "Un\u00b7ter\u00b7grund\u00b7bahn", "Kel\u00b7ler\u00b7r\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "ruft einer: \u00bbWolln Sie nich den Korb wechziehn?\u00ab", "tokens": ["ruft", "ei\u00b7ner", ":", "\u00bb", "Wolln", "Sie", "nich", "den", "Korb", "wech\u00b7ziehn", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIS", "$.", "$(", "PWAV", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "\u00bbsie L\u00fcmmel!\u00ab hallt es noch in meinen Tr\u00e4umen . . .", "tokens": ["\u00bb", "sie", "L\u00fcm\u00b7mel", "!", "\u00ab", "hallt", "es", "noch", "in", "mei\u00b7nen", "Tr\u00e4u\u00b7men", ".", ".", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPER", "NN", "$.", "$(", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was macht wohl jetzt, im Augenblick, Berlin?", "tokens": ["Was", "macht", "wohl", "jetzt", ",", "im", "Au\u00b7gen\u00b7blick", ",", "Ber\u00b7lin", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "$,", "APPRART", "NN", "$,", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kaufleute schuften. Alle Uhren treiben.", "tokens": ["Kauf\u00b7leu\u00b7te", "schuf\u00b7ten", ".", "Al\u00b7le", "Uh\u00b7ren", "trei\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und alle Welt hat Dienst. Kein Mensch flaniert.", "tokens": ["Und", "al\u00b7le", "Welt", "hat", "Dienst", ".", "Kein", "Mensch", "fla\u00b7niert", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN", "$.", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Redakteur darf einen Aufsatz schreiben", "tokens": ["Ein", "Re\u00b7dak\u00b7teur", "darf", "ei\u00b7nen", "Auf\u00b7satz", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "auf Poincar\u00e9, der doch nicht inseriert.", "tokens": ["auf", "Poin\u00b7ca\u00b7r\u00e9", ",", "der", "doch", "nicht", "in\u00b7se\u00b7riert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Damen gehen shopping voller Eile", "tokens": ["Die", "Da\u00b7men", "ge\u00b7hen", "shop\u00b7ping", "vol\u00b7ler", "Ei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und wackeln emsig mit dem Hinterteile . . .", "tokens": ["und", "wa\u00b7ckeln", "em\u00b7sig", "mit", "dem", "Hin\u00b7ter\u00b7tei\u00b7le", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auch dieser Platz war einmal ohne Tadel;", "tokens": ["Auch", "die\u00b7ser", "Platz", "war", "ein\u00b7mal", "oh\u00b7ne", "Ta\u00b7del", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VAFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "hier wohnte fr\u00fcher guter, alter Adel.", "tokens": ["hier", "wohn\u00b7te", "fr\u00fc\u00b7her", "gu\u00b7ter", ",", "al\u00b7ter", "A\u00b7del", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Jetzt k\u00fcmmert sich kein feiner Mann um ihn.", "tokens": ["Jetzt", "k\u00fcm\u00b7mert", "sich", "kein", "fei\u00b7ner", "Mann", "um", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Vielleicht aus Neugier jener oder dieser . . .", "tokens": ["Viel\u00b7leicht", "aus", "Neu\u00b7gier", "je\u00b7ner", "o\u00b7der", "die\u00b7ser", ".", ".", "."], "token_info": ["word", "word