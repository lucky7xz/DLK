{"textgrid.poem.39617": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Wechsel der Dynastie in den Philosophen-Schulen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erst stand im h\u00f6chsten Rang das Ich,", "tokens": ["Erst", "stand", "im", "h\u00f6chs\u00b7ten", "Rang", "das", "Ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Litt Du und Er kaum neben sich,", "tokens": ["Litt", "Du", "und", "Er", "kaum", "ne\u00b7ben", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "PPER", "ADV", "APPR", "PRF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und jedes Nicht-Ich schien ihm nichtig;", "tokens": ["Und", "je\u00b7des", "Nicht\u00b7Ich", "schien", "ihm", "nich\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Ich macht' alle Dinge richtig.", "tokens": ["Das", "Ich", "macht'", "al\u00b7le", "Din\u00b7ge", "rich\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "PIAT", "NN", "ADJD", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "So schlug es manchen Purzelbaum", "tokens": ["So", "schlug", "es", "man\u00b7chen", "Pur\u00b7zel\u00b7baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im methaphysisch leeren Raum.", "tokens": ["Im", "me\u00b7tha\u00b7phy\u00b7sisch", "lee\u00b7ren", "Raum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nachdem es lang von sich gesprochen,", "tokens": ["Nach\u00b7dem", "es", "lang", "von", "sich", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ward ihm zuletzt der Hals gebrochen.", "tokens": ["Ward", "ihm", "zu\u00b7letzt", "der", "Hals", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der unbarmherzige Begriff", "tokens": ["Der", "un\u00b7barm\u00b7her\u00b7zi\u00b7ge", "Be\u00b7griff"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Erdro\u00dfelt' es mit hartem Griff.", "tokens": ["Er\u00b7dro\u00b7\u00df\u00b7elt'", "es", "mit", "har\u00b7tem", "Griff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Der lehrt: was wirklich, sei vern\u00fcnftig;", "tokens": ["Der", "lehrt", ":", "was", "wirk\u00b7lich", ",", "sei", "ver\u00b7n\u00fcnf\u00b7tig", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "PWS", "ADJD", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Das macht ihn bei Philistern z\u00fcnftig. \u2013", "tokens": ["Das", "macht", "ihn", "bei", "Phi\u00b7lis\u00b7tern", "z\u00fcnf\u00b7tig", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wer sagt uns, welcher neue Kniff", "tokens": ["Wer", "sagt", "uns", ",", "wel\u00b7cher", "neu\u00b7e", "Kniff"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Vom Thron wird sto\u00dfen den Begriff?", "tokens": ["Vom", "Thron", "wird", "sto\u00b7\u00dfen", "den", "Be\u00b7griff", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Erst stand im h\u00f6chsten Rang das Ich,", "tokens": ["Erst", "stand", "im", "h\u00f6chs\u00b7ten", "Rang", "das", "Ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Litt Du und Er kaum neben sich,", "tokens": ["Litt", "Du", "und", "Er", "kaum", "ne\u00b7ben", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "PPER", "ADV", "APPR", "PRF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und jedes Nicht-Ich schien ihm nichtig;", "tokens": ["Und", "je\u00b7des", "Nicht\u00b7Ich", "schien", "ihm", "nich\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Ich macht' alle Dinge richtig.", "toke