{"dta.poem.10111": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die herrliche Schau-B\u00fchne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es senckte sich der Erden Theil, worauf Hammonia ge-\ngr\u00fcndet,", "tokens": ["Es", "senck\u00b7te", "sich", "der", "Er\u00b7den", "Theil", ",", "wo\u00b7rauf", "Ham\u00b7mo\u00b7nia", "ge", "gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "NN", "$,", "PWAV", "NE", "TRUNC", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Des Abends von der Sonnen abw\u00e4rts; wodurch denn, was", "tokens": ["Des", "A\u00b7bends", "von", "der", "Son\u00b7nen", "ab\u00b7w\u00e4rts", ";", "wo\u00b7durch", "denn", ",", "was"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$.", "PWAV", "ADV", "$,", "PWS"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "In noch versch\u00f6nertem, vermehrten, und buntern Glantz", "tokens": ["In", "noch", "ver\u00b7sch\u00f6\u00b7ner\u00b7tem", ",", "ver\u00b7mehr\u00b7ten", ",", "und", "bun\u00b7tern", "Glantz"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "$,", "VVFIN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Indem man durch verl\u00e4ngte Schatten die C\u00f6rper doppelt", "tokens": ["In\u00b7dem", "man", "durch", "ver\u00b7l\u00e4ng\u00b7te", "Schat\u00b7ten", "die", "C\u00f6r\u00b7per", "dop\u00b7pelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ADJA", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Es war, durch erst gefallnen Regen, Lufft, Erde, Laub", "tokens": ["Es", "war", ",", "durch", "erst", "ge\u00b7fall\u00b7nen", "Re\u00b7gen", ",", "Lufft", ",", "Er\u00b7de", ",", "Laub"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "$,", "APPR", "ADV", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Und, in den noch verhandnen Tropffen, sah man der Sonnen", "tokens": ["Und", ",", "in", "den", "noch", "ver\u00b7hand\u00b7nen", "Tropf\u00b7fen", ",", "sah", "man", "der", "Son\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "ART", "ADV", "ADJA", "NN", "$,", "VVFIN", "PIS", "ART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Daher denn alles gl\u00e4ntzt\u2019 und gl\u00fchte. Die sch\u00f6nste Schau-", "tokens": ["Da\u00b7her", "denn", "al\u00b7les", "gl\u00e4ntzt'", "und", "gl\u00fch\u00b7te", ".", "Die", "sch\u00f6ns\u00b7te", "Schau"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "ADV", "PIS", "VVFIN", "KON", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ward meinen halb entz\u00fcckten Augen, im hellen Schimmer,", "tokens": ["Ward", "mei\u00b7nen", "halb", "ent\u00b7z\u00fcck\u00b7ten", "Au\u00b7gen", ",", "im", "hel\u00b7len", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJD", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Aufs wenigst hat kein einzigs mahl", "tokens": ["Aufs", "we\u00b7nigst", "hat", "kein", "ein\u00b7zigs", "mahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADV", "VAFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein menschlich Aug\u2019 ein herrlicher Portal", "tokens": ["Ein", "menschlich", "Aug'", "ein", "herr\u00b7li\u00b7cher", "Por\u00b7tal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Von einem Schau-Platz, ie gesehn, und kann auch nimmer-", "tokens": ["Von", "ei\u00b7nem", "Schau\u00b7Platz", ",", "ie", "ge\u00b7sehn", ",", "und", "kann", "auch", "nim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVPP", "$,", "KON", "VMFIN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was gr\u00f6ssers und was pr\u00e4chtigers, als dieser Bau, gefun-", "tokens": ["Was", "gr\u00f6s\u00b7sers", "und", "was", "pr\u00e4ch\u00b7ti\u00b7gers", ",", "als", "die\u00b7ser", "Bau", ",", "ge\u00b7fun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWS", "ADJA", "KON", "PWS", "ADJA", "$,", "KOUS", "PDAT", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Noch auf der Welt gesehen seyn. Denn kurtz: Es war", "tokens": ["Noch", "auf", "der", "Welt", "ge\u00b7se\u00b7hen", "seyn", ".", "Denn", "kurtz", ":", "Es", "war"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "VAINF", "$.", "KON", "ADJD", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den selbst des Sch\u00f6pfers grosses Wort gef\u00e4rbt, am Firma-", "tokens": ["Den", "selbst", "des", "Sch\u00f6p\u00b7fers", "gros\u00b7ses", "Wort", "ge\u00b7f\u00e4rbt", ",", "am", "Fir\u00b7ma"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "ADJA", "NN", "VVPP", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er strahlt\u2019 in voller Pracht und Gluht, es schien von allen", "tokens": ["Er", "strahlt'", "in", "vol\u00b7ler", "Pracht", "und", "Gluht", ",", "es", "schien", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "APPR", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Farben Pracht und Sch\u00f6nheit sich in diesem Kreise zu", "tokens": ["Der", "Far\u00b7ben", "Pracht", "und", "Sch\u00f6n\u00b7heit", "sich", "in", "die\u00b7sem", "Krei\u00b7se", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN", "PRF", "APPR", "PDAT", "NN", "PTKZU"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "Unglaublich war der bunte Schimmer, unglaublich der ge-", "tokens": ["Un\u00b7glaub\u00b7lich", "war", "der", "bun\u00b7te", "Schim\u00b7mer", ",", "un\u00b7glaub\u00b7lich", "der", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$,", "ADJD", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.14": {"text": "Man sah ihn, wie man selten siehet, in seiner halben R\u00fcn-", "tokens": ["Man", "sah", "ihn", ",", "wie", "man", "sel\u00b7ten", "sie\u00b7het", ",", "in", "sei\u00b7ner", "hal\u00b7ben", "R\u00fcn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "$,", "PWAV", "PIS", "ADJD", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "TRUNC"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Durch dieses pr\u00e4chtige Portal nun war nicht minder", "tokens": ["Durch", "die\u00b7ses", "pr\u00e4ch\u00b7ti\u00b7ge", "Por\u00b7tal", "nun", "war", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADV", "VAFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In der bestrahlt-und feuchten Landschaft, ein heller Schau-", "tokens": ["In", "der", "be\u00b7strahl\u00b7t\u00b7und", "feuch\u00b7ten", "Land\u00b7schaft", ",", "ein", "hel\u00b7ler", "Schau"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das niedre Licht, die langen Schatten, vereinten sich, zu", "tokens": ["Das", "nied\u00b7re", "Licht", ",", "die", "lan\u00b7gen", "Schat\u00b7ten", ",", "ver\u00b7ein\u00b7ten", "sich", ",", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "PRF", "$,", "APPR"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "In solchem angenehmen Glantz, in solcher sanften Har-", "tokens": ["In", "sol\u00b7chem", "an\u00b7ge\u00b7neh\u00b7men", "Glantz", ",", "in", "sol\u00b7cher", "sanf\u00b7ten", "Ha\u00b7r"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,", "APPR", "PIAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Da\u00df, wo nicht selbst vom Paradiese das Urbild, minstens", "tokens": ["Da\u00df", ",", "wo", "nicht", "selbst", "vom", "Pa\u00b7ra\u00b7die\u00b7se", "das", "Ur\u00b7bild", ",", "mins\u00b7tens"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "$,", "PWAV", "PTKNEG", "ADV", "APPRART", "NN", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Sich dem Gesicht zu zeigen schien. Der Himmel und die", "tokens": ["Sich", "dem", "Ge\u00b7sicht", "zu", "zei\u00b7gen", "schien", ".", "Der", "Him\u00b7mel", "und", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$.", "ART", "NN", "KON", "ART"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.7": {"text": "In ihrem Schmuck uns gleichsam an. Ein Aug\u2019 und Hertz", "tokens": ["In", "ih\u00b7rem", "Schmuck", "uns", "gleich\u00b7sam", "an", ".", "Ein", "Aug'", "und", "Hertz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADJD", "PTKVZ", "$.", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das, durch der Sonnen Strahlen, gelblich, ja gleichsam", "tokens": ["Das", ",", "durch", "der", "Son\u00b7nen", "Strah\u00b7len", ",", "gelb\u00b7lich", ",", "ja", "gleich\u00b7sam"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PDS", "$,", "APPR", "ART", "NN", "NN", "$,", "ADJD", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "War, da es recht als wie ein Licht, durchs Aug\u2019 uns in die", "tokens": ["War", ",", "da", "es", "recht", "als", "wie", "ein", "Licht", ",", "durchs", "Aug'", "uns", "in", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "ADV", "KOUS", "KOKOM", "ART", "NN", "$,", "APPRART", "NN", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.10": {"text": "Nicht nur den fast entz\u00fcckten Blicken; der Seelen ein so lieb-", "tokens": ["Nicht", "nur", "den", "fast", "ent\u00b7z\u00fcck\u00b7ten", "Bli\u00b7cken", ";", "der", "See\u00b7len", "ein", "so", "lie\u00b7b"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "ADV", "ADJA", "NN", "$.", "ART", "NN", "ART", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Da\u00df sich ihr gantzes Wesen fast, durch ein ununterbrochnes", "tokens": ["Da\u00df", "sich", "ihr", "gant\u00b7zes", "We\u00b7sen", "fast", ",", "durch", "ein", "un\u00b7un\u00b7ter\u00b7broch\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJA", "NN", "ADV", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "In einer reinen Lust verlohr. Sie w\u00fcnschte, gantz von", "tokens": ["In", "ei\u00b7ner", "rei\u00b7nen", "Lust", "ver\u00b7lohr", ".", "Sie", "w\u00fcnschte", ",", "gantz", "von"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$.", "PPER", "VVFIN", "$,", "ADV", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "In aller Sch\u00f6nheit Quell und Ursprung, im Danck sich herr-", "tokens": ["In", "al\u00b7ler", "Sch\u00f6n\u00b7heit", "Quell", "und", "Ur\u00b7sprung", ",", "im", "Danck", "sich", "herr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN", "KON", "NN", "$,", "APPRART", "NN", "PRF", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Lob, Ehr-Furcht, Andacht, Lieb\u2019 und Danck, und Ehr", "tokens": ["Lob", ",", "Ehr\u00b7Furcht", ",", "An\u00b7dacht", ",", "Lieb'", "und", "Danck", ",", "und", "Ehr"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,", "KON", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sey Dir allein, o grosser Sch\u00f6pfer! rieff ich, mit Thr\u00e4nen", "tokens": ["Sey", "Dir", "al\u00b7lein", ",", "o", "gros\u00b7ser", "Sch\u00f6p\u00b7fer", "!", "rieff", "ich", ",", "mit", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$,", "FM", "ADJA", "NN", "$.", "VVFIN", "PPER", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Worin ein Freuden-Feuer brannt\u2019. Ach m\u00f6gte mein ge-", "tokens": ["Wo\u00b7rin", "ein", "Freu\u00b7den\u00b7Feu\u00b7er", "brannt'", ".", "Ach", "m\u00f6g\u00b7te", "mein", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$.", "NN", "VMFIN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob dem, was mir so wol gef\u00e4llt, Dir, HERR, durch Den", "tokens": ["Ob", "dem", ",", "was", "mir", "so", "wol", "ge\u00b7f\u00e4llt", ",", "Dir", ",", "HeRR", ",", "durch", "Den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "$,", "PWS", "PPER", "ADV", "ADV", "VVPP", "$,", "PPER", "$,", "NN", "$,", "APPR", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und was ich sehe, bin und seh\u2019, aus Lieb\u2019 auch zu gefallen", "tokens": ["Und", "was", "ich", "se\u00b7he", ",", "bin", "und", "seh'", ",", "aus", "Lieb'", "auch", "zu", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "KON", "VVFIN", "$,", "APPR", "NN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Ach m\u00f6gt\u2019 ich Dich in Deinen Wercken doch stets, Dir wol-", "tokens": ["Ach", "m\u00f6gt'", "ich", "Dich", "in", "Dei\u00b7nen", "Wer\u00b7cken", "doch", "stets", ",", "Dir", "wol"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "ADV", "ADV", "$,", "PPER", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Der angenehm beb\u00fcschten Wiesen hell-gr\u00fcn-bestrahlte", "tokens": ["Der", "an\u00b7ge\u00b7nehm", "be\u00b7b\u00fcschten", "Wie\u00b7sen", "hell\u00b7gr\u00fcn\u00b7be\u00b7strahl\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "ADJA"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ward durch die an dem Horizont no