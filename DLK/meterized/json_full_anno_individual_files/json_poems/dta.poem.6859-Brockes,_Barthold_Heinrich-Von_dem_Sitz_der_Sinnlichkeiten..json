{"dta.poem.6859": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seele, die hiermit durchs Werckzeug sich besch\u00e4fftigt,", "tokens": ["Die", "See\u00b7le", ",", "die", "hier\u00b7mit", "durchs", "Wer\u00b7ck\u00b7zeug", "sich", "be\u00b7sch\u00e4ff\u00b7tigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PAV", "APPRART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Will, da\u00df die Sinnlichkeit in unserm C\u00f6rper sey,", "tokens": ["Will", ",", "da\u00df", "die", "Sinn\u00b7lich\u00b7keit", "in", "un\u00b7serm", "C\u00f6r\u00b7per", "sey", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Statt da\u00df sie von dem Fehl der dummen Kindheit frey", "tokens": ["Statt", "da\u00df", "sie", "von", "dem", "Fehl", "der", "dum\u00b7men", "Kind\u00b7heit", "frey"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sich entfernen sollt, wird sie darin bekr\u00e4fftigt.", "tokens": ["Und", "sich", "ent\u00b7fer\u00b7nen", "sollt", ",", "wird", "sie", "da\u00b7rin", "be\u00b7kr\u00e4ff\u00b7tigt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVINF", "VMFIN", "$,", "VAFIN", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und da sie alles glaubt von aussen her zu kriegen,", "tokens": ["Und", "da", "sie", "al\u00b7les", "glaubt", "von", "aus\u00b7sen", "her", "zu", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "VVFIN", "APPR", "NE", "APZR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "H\u00e4lt sie sich stets ger\u00fchrt von \u00e4usserlichen Z\u00fcgen.", "tokens": ["H\u00e4lt", "sie", "sich", "stets", "ge\u00b7r\u00fchrt", "von", "\u00e4us\u00b7ser\u00b7li\u00b7chen", "Z\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Indem man f\u00fchlt, da\u00df uns des tieffen Himmels Zelt,", "tokens": ["In\u00b7dem", "man", "f\u00fchlt", ",", "da\u00df", "uns", "des", "tief\u00b7fen", "Him\u00b7mels", "Zelt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zusammt dem weiten Reich der fl\u00fcssenden Crystallen,", "tokens": ["Zu\u00b7sammt", "dem", "wei\u00b7ten", "Reich", "der", "fl\u00fcs\u00b7sen\u00b7den", "Crys\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Geb\u00fcrge, Th\u00e4ler, Wald und Feld,", "tokens": ["Ge\u00b7b\u00fcr\u00b7ge", ",", "Th\u00e4\u00b7ler", ",", "Wald", "und", "Feld", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Auf einmal in die Augen fallen;", "tokens": ["Auf", "ein\u00b7mal", "in", "die", "Au\u00b7gen", "fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}