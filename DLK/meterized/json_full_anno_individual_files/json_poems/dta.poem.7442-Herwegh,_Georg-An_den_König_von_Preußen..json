{"dta.poem.7442": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An den K\u00f6nig von Preu\u00dfen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Einst hat ein bessrer Mann gewagt,", "tokens": ["Einst", "hat", "ein", "bess\u00b7rer", "Mann", "ge\u00b7wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinem Lied vor Dich zu treten;", "tokens": ["Mit", "sei\u00b7nem", "Lied", "vor", "Dich", "zu", "tre\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du kennst Ihn, der so unverzagt", "tokens": ["Du", "kennst", "Ihn", ",", "der", "so", "un\u00b7ver\u00b7zagt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Tyrannei bei Dir verklagt", "tokens": ["Die", "Ty\u00b7ran\u00b7nei", "bei", "Dir", "ver\u00b7klagt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Dich um Deinen Schutz gebeten,", "tokens": ["Und", "Dich", "um", "Dei\u00b7nen", "Schutz", "ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Um Schutz f\u00fcr jenes arme Land,", "tokens": ["Um", "Schutz", "f\u00fcr", "je\u00b7nes", "ar\u00b7me", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das blutend vor dem Himmel stand", "tokens": ["Das", "blu\u00b7tend", "vor", "dem", "Him\u00b7mel", "stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und keine, keine H\u00fclfe fand,", "tokens": ["Und", "kei\u00b7ne", ",", "kei\u00b7ne", "H\u00fcl\u00b7fe", "fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Als die Verzweiflung der Poeten.", "tokens": ["Als", "die", "Ver\u00b7zwei\u00b7flung", "der", "Po\u00b7et\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O lebt' Er noch, er w\u00fcrde heut", "tokens": ["O", "lebt'", "Er", "noch", ",", "er", "w\u00fcr\u00b7de", "heut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich aus dem s\u00fc\u00dfen Schlummer st\u00f6ren;", "tokens": ["Dich", "aus", "dem", "s\u00fc\u00b7\u00dfen", "Schlum\u00b7mer", "st\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ob alle Welt Dir Weihrauch streut", "tokens": ["Ob", "al\u00b7le", "Welt", "Dir", "Weih\u00b7rauch", "streut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und jeden Siegerkranz Dir beut,", "tokens": ["Und", "je\u00b7den", "Sie\u00b7ger\u00b7kranz", "Dir", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er spr\u00e4ch' dem falschen Jubel Hohn", "tokens": ["Er", "spr\u00e4ch'", "dem", "fal\u00b7schen", "Ju\u00b7bel", "Hohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nahte zornig Deinem Thron;", "tokens": ["Und", "nah\u00b7te", "zor\u00b7nig", "Dei\u00b7nem", "Thron", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Tot ist der Vater, und der Sohn,", "tokens": ["Tot", "ist", "der", "Va\u00b7ter", ",", "und", "der", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der M\u00e4chtige, m\u00fc\u00dft' Ihn h\u00f6ren.", "tokens": ["Der", "M\u00e4ch\u00b7ti\u00b7ge", ",", "m\u00fc\u00dft'", "Ihn", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Und Polen ist durch uns verloren;", "tokens": ["Und", "Po\u00b7len", "ist", "durch", "uns", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In Ehrfurcht tret' ", "tokens": ["In", "Ehr\u00b7furcht", "tret'"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wirf nach dem Dichter nicht den Speer,", "tokens": ["Wirf", "nach", "dem", "Dich\u00b7ter", "nicht", "den", "Speer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Weil eine H\u00fctte ihn geboren,", "tokens": ["Weil", "ei\u00b7ne", "H\u00fct\u00b7te", "ihn", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Weil er vor Dir, dem F\u00fcrst, den Mut,", "tokens": ["Weil", "er", "vor", "Dir", ",", "dem", "F\u00fcrst", ",", "den", "Mut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu flehen f\u00fcr Dein eigen Gut,", "tokens": ["Zu", "fle\u00b7hen", "f\u00fcr", "Dein", "ei\u00b7gen", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu flehen f\u00fcr Dein eigen Blut,", "tokens": ["Zu", "fle\u00b7hen", "f\u00fcr", "Dein", "ei\u00b7gen", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "F\u00fcr's deutsche Volk, dem Du geschworen!", "tokens": ["F\u00fcr's", "deut\u00b7sche", "Volk", ",", "dem", "Du", "ge\u00b7schwo\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieh, wie die Jugend sich verzehrt", "tokens": ["Sieh", ",", "wie", "die", "Ju\u00b7gend", "sich", "ver\u00b7zehrt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Gluten eines Meleager,", "tokens": ["In", "Glu\u00b7ten", "ei\u00b7nes", "Me\u00b7le\u00b7a\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sie nach Kampf und That begehrt \u2014", "tokens": ["Wie", "sie", "nach", "Kampf", "und", "That", "be\u00b7gehrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O dr\u00fcck' in ihre Hand ein Schwert,", "tokens": ["O", "dr\u00fcck", "in", "ih\u00b7re", "Hand", "ein", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fchr' aus den St\u00e4dten sie ins Lager!", "tokens": ["F\u00fchr'", "aus", "den", "St\u00e4d\u00b7ten", "sie", "ins", "La\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und frage nicht, ", "tokens": ["Und", "fra\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die Feinde kommen mit dem Wind:", "tokens": ["Die", "Fein\u00b7de", "kom\u00b7men", "mit", "dem", "Wind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Beh\u00fct' uns vor dem Frankenkind", "tokens": ["Be\u00b7h\u00fct'", "uns", "vor", "dem", "Fran\u00b7ken\u00b7kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und vor dem Czaren, Deinem Schwager!", "tokens": ["Und", "vor", "dem", "Cza\u00b7ren", ",", "Dei\u00b7nem", "Schwa\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Sehnsucht Deutschlands steht nach Dir,", "tokens": ["Die", "Sehn\u00b7sucht", "Deutschlands", "steht", "nach", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Fest, wie nach Norden blickt die Nadel;", "tokens": ["Fest", ",", "wie", "nach", "Nor\u00b7den", "blickt", "die", "Na\u00b7del", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O F\u00fcrst, entfalte Dein Panier;", "tokens": ["O", "F\u00fcrst", ",", "ent\u00b7fal\u00b7te", "Dein", "Pa\u00b7nier", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch ist es Zeit, noch folgen wir,", "tokens": ["Noch", "ist", "es", "Zeit", ",", "noch", "fol\u00b7gen", "wir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch soll verstummen jeder Tadel!", "tokens": ["Noch", "soll", "ver\u00b7stum\u00b7men", "je\u00b7der", "Ta\u00b7del", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcrwahr, f\u00fcrwahr, Du thust nicht Recht,", "tokens": ["F\u00fcr\u00b7wahr", ",", "f\u00fcr\u00b7wahr", ",", "Du", "thust", "nicht", "Recht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "$,", "PPER", "VVFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn Du ein moderndes Geschlecht,", "tokens": ["Wenn", "Du", "ein", "mo\u00b7dern\u00b7des", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn Du zu W\u00fcrden hebst den Knecht;", "tokens": ["Wenn", "Du", "zu", "W\u00fcr\u00b7den", "hebst", "den", "Knecht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nur wer ein Adler, sei von Adel!", "tokens": ["Nur", "wer", "ein", "Ad\u00b7ler", ",", "sei", "von", "A\u00b7del", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ART", "NN", "$,", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Lass', was den W\u00fcrmern l\u00e4ngst verfiel,", "tokens": ["Lass'", ",", "was", "den", "W\u00fcr\u00b7mern", "l\u00e4ngst", "ver\u00b7fiel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Frieden bei den W\u00fcrmern liegen;", "tokens": ["In", "Frie\u00b7den", "bei", "den", "W\u00fcr\u00b7mern", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dir ward ein weiter, h\u00f6her Ziel,", "tokens": ["Dir", "ward", "ein", "wei\u00b7ter", ",", "h\u00f6\u00b7her", "Ziel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir ward ein sch\u00f6ner Ritterspiel,", "tokens": ["Dir", "ward", "ein", "sch\u00f6\u00b7ner", "Rit\u00b7ter\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als krumme Lanzen grad' zu biegen.", "tokens": ["Als", "krum\u00b7me", "Lan\u00b7zen", "grad'", "zu", "bie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sei in des Herren Hand ein Blitz,", "tokens": ["Sei", "in", "des", "Her\u00b7ren", "Hand", "ein", "Blitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schlag' in der Feinde schn\u00f6den Witz,", "tokens": ["Schlag'", "in", "der", "Fein\u00b7de", "schn\u00f6\u00b7den", "Witz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Schon tagt ein neues Austerlitz,", "tokens": ["Schon", "tagt", "ein", "neu\u00b7es", "Aus\u00b7ter\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "M\u00f6gst ", "tokens": ["M\u00f6gst"], "token_info": ["word"], "pos": ["VMFIN"], "meter": "+", "measure": "single.up"}}, "stanza.7": {"line.1": {"text": "Das rathlos auseinanderirrt,", "tokens": ["Das", "rath\u00b7los", "aus\u00b7ein\u00b7an\u00b7de\u00b7rirrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Volk soll Dir entgegenflammen;", "tokens": ["Mein", "Volk", "soll", "Dir", "ent\u00b7ge\u00b7gen\u00b7flam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Steh' auf und sprich: \u201eIch bin der Hirt,", "tokens": ["Steh'", "auf", "und", "sprich", ":", "\