 "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Tod im heiligen Streite.", "tokens": ["Zum", "Tod", "im", "hei\u00b7li\u00b7gen", "Strei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Du bist der Stern, auf den man schaut,", "tokens": ["Du", "bist", "der", "Stern", ",", "auf", "den", "man", "schaut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der letzte F\u00fcrst, auf den man baut;", "tokens": ["Der", "letz\u00b7te", "F\u00fcrst", ",", "auf", "den", "man", "baut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O eil' Dich! eh' der Morgen graut,", "tokens": ["O", "eil'", "Dich", "!", "eh'", "der", "Mor\u00b7gen", "graut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sind schon die Freunde in der Weite.", "tokens": ["Sind", "schon", "die", "Freun\u00b7de", "in", "der", "Wei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Nun schweig', du ehernes Gedicht!", "tokens": ["Nun", "schweig'", ",", "du", "e\u00b7her\u00b7nes", "Ge\u00b7dicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des F\u00fcrsten Mund wird bitter schmollen.", "tokens": ["Des", "F\u00fcrs\u00b7ten", "Mund", "wird", "bit\u00b7ter", "schmol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df, man h\u00f6rt die S\u00e4nger nicht,", "tokens": ["Ich", "wei\u00df", ",", "man", "h\u00f6rt", "die", "S\u00e4n\u00b7ger", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PIS", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man stellt die Freien vor Gericht", "tokens": ["Man", "stellt", "die", "Frei\u00b7en", "vor", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wirft sie in die Schar der Tollen.", "tokens": ["Und", "wirft", "sie", "in", "die", "Schar", "der", "Tol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gleichviel \u2014 wie er auch immer schmollt,", "tokens": ["Gleich\u00b7viel", "wie", "er", "auch", "im\u00b7mer", "schmollt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PWAV", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich hab' gethan, was ich gesollt;", "tokens": ["Ich", "hab'", "ge\u00b7than", ",", "was", "ich", "ge\u00b7sollt",