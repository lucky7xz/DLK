{"textgrid.poem.41096": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An dieselbe", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Winzerlied! wie bist du bald verklungen!", "tokens": ["O", "Win\u00b7zer\u00b7lied", "!", "wie", "bist", "du", "bald", "ver\u00b7klun\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es tr\u00e4gt die Traube und den Schnee die Rebe,", "tokens": ["Es", "tr\u00e4gt", "die", "Trau\u00b7be", "und", "den", "Schnee", "die", "Re\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Vogel hat sein letztes Lied gesungen.", "tokens": ["Der", "Vo\u00b7gel", "hat", "sein", "letz\u00b7tes", "Lied", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der \u00e4u\u00dfern Welt auch ich Ade nun gebe.", "tokens": ["Der", "\u00e4u\u00b7\u00dfern", "Welt", "auch", "ich", "A\u00b7de", "nun", "ge\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PPER", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Komm \u00fcber mich nun, Tod! und stille Nacht!", "tokens": ["Komm", "\u00fc\u00b7ber", "mich", "nun", ",", "Tod", "!", "und", "stil\u00b7le", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "$,", "NN", "$.", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und nimm von mir der lauten Erde Kummer!", "tokens": ["Und", "nimm", "von", "mir", "der", "lau\u00b7ten", "Er\u00b7de", "Kum\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Indem ich also still bei mir gedacht,", "tokens": ["In\u00b7dem", "ich", "al\u00b7so", "still", "bei", "mir", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schon war es Mitternacht, sank ich in Schlummer.", "tokens": ["Schon", "war", "es", "Mit\u00b7ter\u00b7nacht", ",", "sank", "ich", "in", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da ward in mir ein Leuchten angefacht,", "tokens": ["Da", "ward", "in", "mir", "ein", "Leuch\u00b7ten", "an\u00b7ge\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie ich's noch nie bei ird'scher Sonn' gesehen,", "tokens": ["Wie", "ich's", "noch", "nie", "bei", "ird'\u00b7scher", "Sonn'", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", 