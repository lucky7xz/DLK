{"textgrid.poem.33731": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "13. Friedensbild", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn \u00fcber Eichen Sturm und Donner schnauben,", "tokens": ["Wenn", "\u00fc\u00b7ber", "Ei\u00b7chen", "Sturm", "und", "Don\u00b7ner", "schnau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Singt unter Blumen ungest\u00f6rt die Grille;", "tokens": ["Singt", "un\u00b7ter", "Blu\u00b7men", "un\u00b7ge\u00b7st\u00f6rt", "die", "Gril\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Bergtal lebt und webt noch die Idylle,", "tokens": ["Im", "Berg\u00b7tal", "lebt", "und", "webt", "noch", "die", "I\u00b7dyl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn rings die L\u00e4nder Krieg und Pest durchrauben.", "tokens": ["Wenn", "rings", "die", "L\u00e4n\u00b7der", "Krieg", "und", "Pest", "durch\u00b7rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O sieh, da herrscht noch Sitte, Treu und Glauben;", "tokens": ["O", "sieh", ",", "da", "herrscht", "noch", "Sit\u00b7te", ",", "Treu", "und", "Glau\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ADV", "VVFIN", "ADV", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Kinder f\u00fchrt ein Patriarchenwille,", "tokens": ["Die", "Kin\u00b7der", "f\u00fchrt", "ein", "Pat\u00b7ri\u00b7ar\u00b7chen\u00b7wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Tag ist Arbeit und die Nacht ist Stille,", "tokens": ["Der", "Tag", "ist", "Ar\u00b7beit", "und", "die", "Nacht", "ist", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Hausdach nisten Storch und wei\u00dfe Tauben.", "tokens": ["Am", "Haus\u00b7dach", "nis\u00b7ten", "Storch", "und", "wei\u00b7\u00dfe", "Tau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Wanduhr pickt, und alles schl\u00e4ft \u2013 doch n\u00e4her", "tokens": ["Die", "Wan\u00b7duhr", "pickt", ",", "und", "al\u00b7les", "schl\u00e4ft", "\u2013", "doch", "n\u00e4\u00b7her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PIS", "VVFIN", "$(", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und n\u00e4her t\u00f6nt schon Echo von Gesch\u00fctzen,", "tokens": ["Und", "n\u00e4\u00b7her", "t\u00f6nt", "schon", "E\u00