{"textgrid.poem.46640": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[sie wollen unter anderm Trost]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie wollen unter anderm Trost", "tokens": ["Sie", "wol\u00b7len", "un\u00b7ter", "an\u00b7derm", "Trost"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PIS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch diesen Trost mir bieten,", "tokens": ["Auch", "die\u00b7sen", "Trost", "mir", "bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von Kindern, ihrer Mutter Noth,", "tokens": ["Von", "Kin\u00b7dern", ",", "ih\u00b7rer", "Mut\u00b7ter", "Noth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die, lebend leidiger als todt,", "tokens": ["Die", ",", "le\u00b7bend", "lei\u00b7di\u00b7ger", "als", "todt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Miswuchsen und misriethen.", "tokens": ["Mis\u00b7wuch\u00b7sen", "und", "mis\u00b7rie\u00b7then", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Tr\u00f6ster wissen nicht, wie weh", "tokens": ["Die", "Tr\u00f6s\u00b7ter", "wis\u00b7sen", "nicht", ",", "wie", "weh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie meinem Herzen thaten.", "tokens": ["Sie", "mei\u00b7nem", "Her\u00b7zen", "tha\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein frischer Hirsch, mein schlankes Reh!", "tokens": ["Mein", "fri\u00b7scher", "Hirsch", ",", "mein", "schlan\u00b7kes", "Reh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich denken, da\u00df ihr konntet je", "tokens": ["Ich", "den\u00b7ken", ",", "da\u00df", "ihr", "konn\u00b7tet", "je"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "KOUS", "PPER", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Miswachsen und misrathen!", "tokens": ["Mis\u00b7wach\u00b7sen", "und", "mis\u00b7ra\u00b7then", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie wollen unter anderm Trost", "tokens": ["Sie", "wol\u00b7len", "un\u00b7ter", "an\u00b7derm", "Trost"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PIS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch diesen Trost mir bieten,", "tokens": ["Auch", "die\u00b7sen", "Trost", "mir", "bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von Kindern, ihrer Mutter Noth,", "tokens": ["Von", "Kin\u00b7der