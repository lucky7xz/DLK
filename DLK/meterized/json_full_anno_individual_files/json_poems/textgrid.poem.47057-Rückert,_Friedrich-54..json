{"textgrid.poem.47057": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fcck, Heil und Segen dir und jeder Quelle", "tokens": ["Gl\u00fcck", ",", "Heil", "und", "Se\u00b7gen", "dir", "und", "je\u00b7der", "Quel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "PPER", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An dir, daraus ich sog Genusses Wogen,", "tokens": ["An", "dir", ",", "da\u00b7raus", "ich", "sog", "Ge\u00b7nus\u00b7ses", "Wo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PAV", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Berauschung in des Armes offnem Bogen,", "tokens": ["Be\u00b7rau\u00b7schung", "in", "des", "Ar\u00b7mes", "off\u00b7nem", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entz\u00fcckung aus des offnen Auges Helle.", "tokens": ["Ent\u00b7z\u00fc\u00b7ckung", "aus", "des", "off\u00b7nen", "Au\u00b7ges", "Hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gl\u00fcck, Heil und Segen dir und jeder Stelle,", "tokens": ["Gl\u00fcck", ",", "Heil", "und", "Se\u00b7gen", "dir", "und", "je\u00b7der", "Stel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "PPER", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo du mich in dein s\u00fc\u00dfes Netz gezogen,", "tokens": ["Wo", "du", "mich", "in", "dein", "s\u00fc\u00b7\u00dfes", "Netz", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo du begl\u00fcckt mich, wo du mich betrogen,", "tokens": ["Wo", "du", "be\u00b7gl\u00fcckt", "mich", ",", "wo", "du", "mich", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn Trug ist ja der Liebe Spielgeselle.", "tokens": ["Denn", "Trug", "ist", "ja", "der", "Lie\u00b7be", "Spiel\u00b7ge\u00b7sel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df nicht, ob ein Blick, der je ins Leben", "tokens": ["Ich", "wei\u00df", "nicht", ",", "ob", "ein", "Blick", ",", "der", "je", "ins", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir ging, aus deinem Leben sei gekommen,", "tokens": ["Mir", "ging", ",", "aus", "dei\u00b7nem", "Le\u00b7ben", "sei", "ge\u00b7kom\u00b7men", ","], "token_info": ["word",