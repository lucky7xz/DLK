{"textgrid.poem.54162": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Dreh dich hin, dreh dich her \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Zeitungsverleger M\u00fclvo\u00df, als welcher ein krummer Jid,", "tokens": ["Der", "Zei\u00b7tungs\u00b7ver\u00b7le\u00b7ger", "M\u00fcl\u00b7vo\u00df", ",", "als", "wel\u00b7cher", "ein", "krum\u00b7mer", "Jid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "KOUS", "PRELS", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "sprach: \u00bbWissen Se \u2013 ich bin n\u00e4mlich Antisemit!", "tokens": ["sprach", ":", "\u00bb", "Wis\u00b7sen", "Se", "\u2013", "ich", "bin", "n\u00e4m\u00b7lich", "An\u00b7ti\u00b7se\u00b7mit", "!"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "$(", "NE", "NE", "$(", "PPER", "VAFIN", "ADV", "NE", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie haben eben keinen Sinn f\u00fcr Wehrhaftigkeit!", "tokens": ["Sie", "ha\u00b7ben", "e\u00b7ben", "kei\u00b7nen", "Sinn", "f\u00fcr", "Wehr\u00b7haf\u00b7tig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Ich und mein Blatt, wir gehen mit unsrer Zeit!", "tokens": ["Ich", "und", "mein", "Blatt", ",", "wir", "ge\u00b7hen", "mit", "uns\u00b7rer", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Mit der Zeit mu\u00df man mitgehn!\u00ab", "tokens": ["Mit", "der", "Zeit", "mu\u00df", "man", "mit\u00b7gehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PIS", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und es erhob sich ein Wispern im Bl\u00e4tterwalde.", "tokens": ["Und", "es", "er\u00b7hob", "sich", "ein", "Wis\u00b7pern", "im", "Bl\u00e4t\u00b7ter\u00b7wal\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und jeder Mitarbeiter f\u00fchlte: Warte nur, balde . . .!", "tokens": ["Und", "je\u00b7der", "Mit\u00b7ar\u00b7bei\u00b7ter", "f\u00fchl\u00b7te", ":", "War\u00b7te", "nur", ",", "bal\u00b7de", ".", ".", ".", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct", "punct", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$.", "NN", "ADV", "$,", "ADV", "$.", "$.", "$.", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und die Redakteure bildeten sich im Kunstfliegen aus,", "tokens": ["Und", "die", "Re\u00b7dak\u00b7teu\u00b7re", "bil\u00b7de\u00b7ten", "sich", "im", "Kunst\u00b7flie\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "--+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "und je j\u00fcdischer einer hie\u00df, desto rau\u00dferer flog er raus.", "tokens": ["und", "je", "j\u00fc\u00b7di\u00b7scher", "ei\u00b7ner", "hie\u00df", ",", "des\u00b7to", "rau\u00b7\u00dfe\u00b7rer", "flog", "er", "raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PIS", "VVFIN", "$,", "ADV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Mit der Zeit mu\u00df man mitgehn.", "tokens": ["Mit", "der", "Zeit", "mu\u00df", "man", "mit\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und siehe, es entdeckten manche Spitzen der Verlegerei,", "tokens": ["Und", "sie\u00b7he", ",", "es", "ent\u00b7deck\u00b7ten", "man\u00b7che", "Spit\u00b7zen", "der", "Ver\u00b7le\u00b7ge\u00b7rei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "PPER", "VVFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "da\u00df es mit dem Militarismus gar nicht so b\u00f6se sei.", "tokens": ["da\u00df", "es", "mit", "dem", "Mi\u00b7li\u00b7ta\u00b7ris\u00b7mus", "gar", "nicht", "so", "b\u00f6\u00b7se", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADV", "PTKNEG", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Denn wer nicht reiten kann, der ist entweder Pazifist,", "tokens": ["Denn", "wer", "nicht", "rei\u00b7ten", "kann", ",", "der", "ist", "ent\u00b7we\u00b7der", "Pa\u00b7zi\u00b7fist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "VVFIN", "VMFIN", "$,", "PRELS", "VAFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "oder er bewundert alles, was ein Kommi\u00dfknopp ist.", "tokens": ["o\u00b7der", "er", "be\u00b7wun\u00b7dert", "al\u00b7les", ",", "was", "ein", "Kom\u00b7mi\u00df\u00b7knopp", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIS", "$,", "PRELS", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+---+-", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Mit der Zeit mu\u00df man stramm stehn.", "tokens": ["Mit", "der", "Zeit", "mu\u00df", "man", "stramm", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber denkt denn der Druckereibesitzer von solchem Blatt,", "tokens": ["A\u00b7ber", "denkt", "denn", "der", "Dru\u00b7cke\u00b7rei\u00b7be\u00b7sit\u00b7zer", "von", "sol\u00b7chem", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+--+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "da\u00df der Adolf Hitler so ein kurzes Ged\u00e4chtnis hat?", "tokens": ["da\u00df", "der", "A\u00b7dolf", "Hit\u00b7ler", "so", "ein", "kur\u00b7zes", "Ge\u00b7d\u00e4cht\u00b7nis", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "NN", "ADV", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Und nimmt nichts mehr krumm?", "tokens": ["Und", "nimmt", "nichts", "mehr", "krumm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Dumm ist er ja. Aber so dumm . . .!", "tokens": ["Dumm", "ist", "er", "ja", ".", "A\u00b7ber", "so", "dumm", ".", ".", ".", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "$.", "KON", "ADV", "ADJD", "$.", "$.", "$.", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Und das ist das Besch\u00e4mende an diesem Gesindel, das den Faschismus st\u00fctzt:", "tokens": ["Und", "das", "ist", "das", "Be\u00b7sch\u00e4\u00b7men\u00b7de", "an", "die\u00b7sem", "Ge\u00b7sin\u00b7del", ",", "das", "den", "Fa\u00b7schis\u00b7mus", "st\u00fctzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "APPR", "PDAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+---+-+-+--+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "da\u00df ihm der Umfall auch nicht das geringste n\u00fctzt.", "tokens": ["da\u00df", "ihm", "der", "Um\u00b7fall", "auch", "nicht", "das", "ge\u00b7rings\u00b7te", "n\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "PTKNEG", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Mit der Zeit werden sie eingehn.", "tokens": ["Mit", "der", "Zeit", "wer\u00b7den", "sie", "ein\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Der Zeitungsverleger M\u00fclvo\u00df, als welcher ein krummer Jid,"