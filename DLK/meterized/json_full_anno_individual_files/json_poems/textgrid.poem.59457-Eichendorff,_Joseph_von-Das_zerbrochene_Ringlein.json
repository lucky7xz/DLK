{"textgrid.poem.59457": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das zerbrochene Ringlein", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einem k\u00fchlen Grunde", "tokens": ["In", "ei\u00b7nem", "k\u00fch\u00b7len", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da geht ein M\u00fchlenrad,", "tokens": ["Da", "geht", "ein", "M\u00fch\u00b7len\u00b7rad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Liebste ist verschwunden,", "tokens": ["Mein", "Liebs\u00b7te", "ist", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die dort gewohnet hat.", "tokens": ["Die", "dort", "ge\u00b7woh\u00b7net", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie hat mir Treu versprochen,", "tokens": ["Sie", "hat", "mir", "Treu", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gab mir ein'n Ring dabei,", "tokens": ["Gab", "mir", "ein'n", "Ring", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat die Treu gebrochen,", "tokens": ["Sie", "hat", "die", "Treu", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Ringlein sprang entzwei.", "tokens": ["Mein", "Rin\u00b7glein", "sprang", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich m\u00f6cht als Spielmann reisen", "tokens": ["Ich", "m\u00f6cht", "als", "Spiel\u00b7mann", "rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "KOUS", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weit in die Welt hinaus,", "tokens": ["Weit", "in", "die", "Welt", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und singen meine Weisen,", "tokens": ["Und", "sin\u00b7gen", "mei\u00b7ne", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und gehn von Haus zu Haus.", "tokens": ["Und", "gehn", "von", "Haus", "zu", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich m\u00f6cht als Reiter fliegen", "tokens": ["Ich", "m\u00f6cht", "als", "Rei\u00b7ter", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "KOUS", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl in die blut'ge Schlacht,", "tokens": ["Wohl", "in", "die", "blut'\u00b7ge", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um stille Feuer liegen", "tokens": ["Um", "stil\u00b7le", "Feu\u00b7er", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Feld bei dunkler Nacht.", "tokens": ["Im", "Feld", "bei", "dunk\u00b7ler", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "H\u00f6r ich das M\u00fchlrad gehen:", "tokens": ["H\u00f6r", "ich", "das", "M\u00fchl\u00b7rad", "ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich wei\u00df nicht, was ich will \u2013", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "ich", "will", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich m\u00f6cht am liebsten sterben,", "tokens": ["Ich", "m\u00f6cht", "am", "liebs\u00b7ten", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da w\u00e4r's auf einmal still!", "tokens": ["Da", "w\u00e4r's", "auf", "ein\u00b7mal", "still", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "In