{"dta.poem.756": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlII.  An  Eugenien.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich als ein Wandersmann dafern die tr\u00fcbe Nacht/ ", "tokens": ["Gleich", "als", "ein", "Wan\u00b7ders\u00b7mann", "da\u00b7fern", "die", "tr\u00fc\u00b7be", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "KOUS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit dicker finsternu\u00df/ Luft/ Erd/ vnd See verdecket/ ", "tokens": ["Mit", "di\u00b7cker", "fins\u00b7ter\u00b7nu\u00df", "/", "Luft", "/", "Erd", "/", "vnd", "See", "ver\u00b7de\u00b7cket", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "NN", "$(", "NN", "$(", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Betr\u00fcbt jrr\u2019t hin vnd her/ vnd mit viel furcht erschre-", "tokens": ["Be\u00b7tr\u00fcbt", "jrr't", "hin", "vnd", "her", "/", "vnd", "mit", "viel", "furcht", "er\u00b7schre"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$(", "KON", "APPR", "PIAT", "NN", "TRUNC"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "cket/", "tokens": ["cket", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Nicht wei\u00df wohin er geht/ noch was er l\u00e4st vnd macht:", "tokens": ["Nicht", "wei\u00df", "wo\u00b7hin", "er", "geht", "/", "noch", "was", "er", "l\u00e4st", "vnd", "macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PWAV", "PPER", "VVFIN", "$(", "ADV", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So eben ists mit mir/ doch wenn der Mond erwacht", "tokens": ["So", "e\u00b7ben", "ists", "mit", "mir", "/", "doch", "wenn", "der", "Mond", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "APPR", "PPER", "$(", "ADV", "KOUS", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vnd seiner stralen kertz im Wolckenhau\u00df\u2019 anstecket:", "tokens": ["Vnd", "sei\u00b7ner", "stra\u00b7len", "kertz", "im", "Wol\u00b7cken\u00b7hau\u00df'", "an\u00b7ste\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald findt er weg\u2019 vnd raht: so wird mein ", "tokens": ["Bald", "findt", "er", "weg'", "vnd", "raht", ":", "so", "wird", "mein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "NN", "$.", "ADV", "VAFIN", "PPOSAT"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Nun mich der newe trost au\u00df ewrem brieff anlacht.", "tokens": ["Nun", "mich", "der", "ne\u00b7we", "trost", "au\u00df", "ew\u00b7rem", "brieff", "an\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Doch warumb heist jhr mich di\u00df sch\u00f6ne pfand verbrennen?", "tokens": ["Doch", "wa\u00b7rumb", "heist", "jhr", "mich", "di\u00df", "sch\u00f6\u00b7ne", "pfand", "ver\u00b7bren\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "PPER", "PRF", "PDS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wolt jhr in meiner nacht mich bey der glut' erkennen? ", "tokens": ["Wolt", "jhr", "in", "mei\u00b7ner", "nacht", "mich", "bey", "der", "glut'", "er\u00b7ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Di\u00df/ meines hertzen few\u2019r entdeckt ja wer ich sey:", "tokens": ["Di\u00df", "/", "mei\u00b7nes", "hert\u00b7zen", "fe\u00b7w'r", "ent\u00b7deckt", "ja", "wer", "ich", "sey", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$(", "PPOSAT", "NN", "ADJD", "VVPP", "ADV", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Sol sch\u00f6nste/ di\u00df papier nur meine brust ber\u00fchren:", "tokens": ["Sol", "sch\u00f6ns\u00b7te", "/", "di\u00df", "pa\u00b7pier", "nur", "mei\u00b7ne", "brust", "be\u00b7r\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "$(", "PDS", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird es alsobald in aschen sich verlieren:", "tokens": ["So", "wird", "es", "al\u00b7so\u00b7bald", "in", "asc\u00b7hen", "sich", "ver\u00b7lie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo von der flam\u2019 es nicht wird durch mein weinen frey.", "tokens": ["Wo", "von", "der", "flam'", "es", "nicht", "wird", "durch", "mein", "wei\u00b7nen", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "VVFIN", "PPER", "PTKNEG", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}