{"dta.poem.969": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "Entf\u00fchrung", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Zieh mit mir geliebtes kind                 ", "tokens": ["Zieh", "mit", "mir", "ge\u00b7lieb\u00b7tes", "kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In die w&#228;lder ferner kunde                 ", "tokens": ["In", "die", "w", "&#228;", "lder", "fer\u00b7ner", "kun\u00b7de"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "$(", "VVIMP", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und behalt als angebind                 ", "tokens": ["Und", "be\u00b7halt", "als", "an\u00b7ge\u00b7bind"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOUS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur mein lied in deinem munde               ", "tokens": ["Nur", "mein", "lied", "in", "dei\u00b7nem", "mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Baden wir im sanften blau                 ", "tokens": ["Ba\u00b7den", "wir", "im", "sanf\u00b7ten", "blau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der mit duft umh&#252;llten gr&#228;nzen                 ", "tokens": ["Der", "mit", "duft", "umh", "&#252;", "ll\u00b7ten", "gr", "&#228;", "nzen"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "XML_entity", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$(", "VVINF"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Werden unsre leiber gl&#228;nzen                 ", "tokens": ["Wer\u00b7den", "uns\u00b7re", "lei\u00b7ber", "gl", "&#228;", "nzen"], "token_info": ["word", "word", "word", "word", "XML_entity", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "NE", "$(", "VVINF"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Klarer scheinen als der tau               ", "tokens": ["Kla\u00b7rer", "schei\u00b7nen", "als", "der", "tau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KOKOM", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In der luft sich silbern fein                 ", "tokens": ["In", "der", "luft", "sich", "sil\u00b7bern", "fein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F&#228;den uns zu schleiern spinnen                 ", "tokens": ["F", "&#228;", "den", "uns", "zu", "schlei\u00b7ern", "spin\u00b7nen"], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "ART", "PPER", "PTKZU", "VVINF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf dem rasen bleichen linnen                 ", "tokens": ["Auf", "dem", "ra\u00b7sen", "blei\u00b7chen", "lin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zart wie schnee und sternenschein               ", "tokens": ["Zart", "wie", "schnee", "und", "ster\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Unter b&#228;umen um den see                 ", "tokens": ["Un\u00b7ter", "b", "&#228;", "u\u00b7men", "um", "den", "see"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["APPR", "XY", "$(", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Schweben wir vereint uns freuend                 ", "tokens": ["Schwe\u00b7ben", "wir", "ver\u00b7eint", "uns", "freu\u00b7end"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.3": {"text": "Sachte singend blumen streuend                 ", "tokens": ["Sach\u00b7te", "sin\u00b7gend", "blu\u00b7men", "streu\u00b7end"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weisse nelken weissen klee               ", "tokens": ["Weis\u00b7se", "nel\u00b7ken", "weis\u00b7sen", "klee"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVINF", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}