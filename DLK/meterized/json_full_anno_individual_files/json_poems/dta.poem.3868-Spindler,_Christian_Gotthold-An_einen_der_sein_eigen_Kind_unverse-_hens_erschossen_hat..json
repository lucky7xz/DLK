{"dta.poem.3868": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "An einen/ der sein eigen Kind unverse-  \n hens erschossen hat.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Gehet nur, bethr\u00e4nte Zeilen,", "tokens": ["Ge\u00b7het", "nur", ",", "be\u00b7thr\u00e4n\u00b7te", "Zei\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bringt das bange Mitleid an;", "tokens": ["Bringt", "das", "ban\u00b7ge", "Mit\u00b7leid", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Man wird euch Geh\u00f6r ertheilen,", "tokens": ["Man", "wird", "euch", "Ge\u00b7h\u00f6r", "er\u00b7thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gehet zu dem frommen Mann,", "tokens": ["Ge\u00b7het", "zu", "dem", "from\u00b7men", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zu den Jacob jener Stadt,", "tokens": ["Zu", "den", "Ja\u00b7cob", "je\u00b7ner", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der sein Kind erschossen hat,", "tokens": ["Der", "sein", "Kind", "er\u00b7schos\u00b7sen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der durch unvermuthet Sch\u00fcssen", "tokens": ["Der", "durch", "un\u00b7ver\u00b7mu\u00b7thet", "Sch\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seinen Sohn ert\u00f6dten m\u00fcssen.", "tokens": ["Sei\u00b7nen", "Sohn", "er\u00b7t\u00f6d\u00b7ten", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Armes Schaaf von seinem Hauffen,", "tokens": ["Ar\u00b7mes", "Schaaf", "von", "sei\u00b7nem", "Hauf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun erstarrter Benjamin!", "tokens": ["Nun", "er\u00b7starr\u00b7ter", "Ben\u00b7ja\u00b7min", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Must du denn entgegen lauffen?", "tokens": ["Must", "du", "denn", "ent\u00b7ge\u00b7gen", "lauf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKVZ", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rei\u00dft dich eine Kugel hin!", "tokens": ["Rei\u00dft", "dich", "ei\u00b7ne", "Ku\u00b7gel", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird der h\u00f6chst verha\u00dfte Ort", "tokens": ["Wird", "der", "h\u00f6chst", "ver\u00b7ha\u00df\u00b7te", "Ort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Unschulds- voll zum Vater-Mord?", "tokens": ["Un\u00b7schulds", "voll", "zum", "Va\u00b7ter\u00b7Mord", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["TRUNC", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Must du in belaubten Buchen", "tokens": ["Must", "du", "in", "be\u00b7laub\u00b7ten", "Bu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deinen Tod so pl\u00f6tzlich suchen?", "tokens": ["Dei\u00b7nen", "Tod", "so", "pl\u00f6tz\u00b7lich", "su\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, ach leider! ists geschehen,", "tokens": ["Ja", ",", "ach", "lei\u00b7der", "!", "ists", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ITJ", "ADV", "$.", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frommer Vater! fasse dich;", "tokens": ["From\u00b7mer", "Va\u00b7ter", "!", "fas\u00b7se", "dich", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df der bangen Seuffzer Flehen,", "tokens": ["La\u00df", "der", "ban\u00b7gen", "Seuff\u00b7zer", "Fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kr\u00e4ncket es gleich inniglich,", "tokens": ["Kr\u00e4n\u00b7cket", "es", "gleich", "in\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Mu\u00df die Unschuld doch allein", "tokens": ["Mu\u00df", "die", "Un\u00b7schuld", "doch", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir zur besten Tr\u00f6stung seyn.", "tokens": ["Dir", "zur", "bes\u00b7ten", "Tr\u00f6s\u00b7tung", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Di\u00df dient auch zum Trost der Deinen,", "tokens": ["Di\u00df", "dient", "auch", "zum", "Trost", "der", "Dei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Darum hemmt das bange Weinen.", "tokens": ["Da\u00b7rum", "hemmt", "das", "ban\u00b7ge", "Wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Unvermuthet bange F\u00e4lle", "tokens": ["Un\u00b7ver\u00b7mu\u00b7thet", "ban\u00b7ge", "F\u00e4l\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommen von weit h\u00f6hrer Macht,", "tokens": ["Kom\u00b7men", "von", "weit", "h\u00f6h\u00b7rer", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Scheint uns heut der Himmel helle,", "tokens": ["Scheint", "uns", "heut", "der", "Him\u00b7mel", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgen kommt die Ungl\u00fccks-Nacht.", "tokens": ["Mor\u00b7gen", "kommt", "die", "Un\u00b7gl\u00fccks\u00b7Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Niemand wei\u00df des Zufalls Spuhr,", "tokens": ["Nie\u00b7mand", "wei\u00df", "des", "Zu\u00b7falls", "Spuhr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch die Vorsicht kennt sie nur,", "tokens": ["Doch", "die", "Vor\u00b7sicht", "kennt", "sie", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ihr gn\u00e4digstes Erweisen,", "tokens": ["Und", "ihr", "gn\u00e4\u00b7digs\u00b7tes", "Er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Mu\u00df man letzt an Ende preisen.", "tokens": ["Mu\u00df", "man", "letzt", "an", "En\u00b7de", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}