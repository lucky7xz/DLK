{"dta.poem.13610": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Lockung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rst du nicht die B\u00e4ume rauschen", "tokens": ["H\u00f6rst", "du", "nicht", "die", "B\u00e4u\u00b7me", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drau\u00dfen durch die stille Rund'?", "tokens": ["Drau\u00b7\u00dfen", "durch", "die", "stil\u00b7le", "Rund'", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lockt's dich nicht hinabzulauschen", "tokens": ["Lockt's", "dich", "nicht", "hin\u00b7ab\u00b7zu\u00b7lau\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPER", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem S\u00f6ller in den Grund,", "tokens": ["Von", "dem", "S\u00f6l\u00b7ler", "in", "den", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo die vielen B\u00e4che gehen", "tokens": ["Wo", "die", "vie\u00b7len", "B\u00e4\u00b7che", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wunderbar im Mondenschein", "tokens": ["Wun\u00b7der\u00b7bar", "im", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die stillen Schl\u00f6sser sehen", "tokens": ["Und", "die", "stil\u00b7len", "Schl\u00f6s\u00b7ser", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In den Flu\u00df vom hohen Stein.", "tokens": ["In", "den", "Flu\u00df", "vom", "ho\u00b7hen", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kennst du noch die irren Lieder", "tokens": ["Kennst", "du", "noch", "die", "ir\u00b7ren", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der alten sch\u00f6nen Zeit?", "tokens": ["Aus", "der", "al\u00b7ten", "sch\u00f6\u00b7nen", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie erwachen alle wieder", "tokens": ["Sie", "er\u00b7wa\u00b7chen", "al\u00b7le", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nachts in Waldeseinsamkeit,", "tokens": ["Nachts", "in", "Wal\u00b7de\u00b7sein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die B\u00e4ume tr\u00e4umend lauschen", "tokens": ["Wenn", "die", "B\u00e4u\u00b7me", "tr\u00e4u\u00b7mend", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Flieder duftet schw\u00fcl", "tokens": ["Und", "der", "Flie\u00b7der", "duf\u00b7tet", "schw\u00fcl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und im Flu\u00df die Nixen rauschen \u2014", "tokens": ["Und", "im", "Flu\u00df", "die", "Ni\u00b7xen", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Komm herab, hier ist's so k\u00fchl.", "tokens": ["Komm", "her\u00b7ab", ",", "hier", "ist's", "so", "k\u00fchl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADV", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}