{"dta.poem.5501": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df es ein wahres Wunder ist; allein er hat doch seine", "tokens": ["Da\u00df", "es", "ein", "wah\u00b7res", "Wun\u00b7der", "ist", ";", "al\u00b7lein", "er", "hat", "doch", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN", "$.", "ADV", "PPER", "VAFIN", "ADV", "PPOSAT"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Schrancken", "tokens": ["Schran\u00b7cken"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wor\u00fcber er nicht kommen kann. Wer nun die forschen-", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "er", "nicht", "kom\u00b7men", "kann", ".", "Wer", "nun", "die", "for\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PTKNEG", "VVINF", "VMFIN", "$.", "PWS", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "den Gedancken", "tokens": ["den", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.5": {"text": "Aus ihrem Kraft-Kreis treiben will, und, mehr als wo-", "tokens": ["Aus", "ih\u00b7rem", "Kraft\u00b7Kreis", "trei\u00b7ben", "will", ",", "und", ",", "mehr", "als", "wo"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$,", "KON", "$,", "PIAT", "KOKOM", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "zu sie bestimmt,", "tokens": ["zu", "sie", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Den Engeln, ja der GOttheit gleich, damit zu fassen", "tokens": ["Den", "En\u00b7geln", ",", "ja", "der", "Got\u00b7theit", "gleich", ",", "da\u00b7mit", "zu", "fas\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "ADV", "$,", "PAV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "unternimmt,", "tokens": ["un\u00b7ter\u00b7nimmt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Wird, wie der Lucifer, gest\u00fcrtzt. Ach, la\u00dft uns dieses wol", "tokens": ["Wird", ",", "wie", "der", "Lu\u00b7ci\u00b7fer", ",", "ge\u00b7st\u00fcrtzt", ".", "Ach", ",", "la\u00dft", "uns", "die\u00b7ses", "wol"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "PWAV", "ART", "NE", "$,", "VVPP", "$.", "ITJ", "$,", "VVIMP", "PPER", "PDS", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "erwegen!", "tokens": ["er\u00b7we\u00b7gen", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Ich habs erfahren, da\u00df daran weit mehr, als wie man", "tokens": ["Ich", "habs", "er\u00b7fah\u00b7ren", ",", "da\u00df", "da\u00b7ran", "weit", "mehr", ",", "als", "wie", "man"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NE", "VVINF", "$,", "KOUS", "PAV", "ADJD", "ADV", "$,", "KOUS", "PWAV", "PIS"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "meint, gelegen.", "tokens": ["meint", ",", "ge\u00b7le\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Will unser sinckendes Gem\u00fcth, will unser\u2019 angefochtne", "tokens": ["Will", "un\u00b7ser", "sin\u00b7cken\u00b7des", "Ge\u00b7m\u00fcth", ",", "will", "un\u00b7ser'", "an\u00b7ge\u00b7focht\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "$,", "VMFIN", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "Seelen", "tokens": ["See\u00b7len"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Ein Zweiffel, der un\u00fcberwindlich, mit Angst, bi\u00df zur Ver-", "tokens": ["Ein", "Zweif\u00b7fel", ",", "der", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", ",", "mit", "Angst", ",", "bi\u00df", "zur", "Ver"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "$,", "APPR", "NN", "$,", "KOUS", "APPRART", "TRUNC"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "zweiflung qv\u00e4hlen;", "tokens": ["zwei\u00b7flung", "qv\u00e4h\u00b7len", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "So sprecht in wahrer Selbst-Erk\u00e4nntni\u00df; halt ein, mein", "tokens": ["So", "sprecht", "in", "wah\u00b7rer", "Selbst\u00b7Er\u00b7k\u00e4nnt\u00b7ni\u00df", ";", "halt", "ein", ",", "mein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$.", "VVFIN", "PTKVZ", "$,", "PPOSAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Geist! hier ist dein Ziel!", "tokens": ["Geist", "!", "hier", "ist", "dein", "Ziel", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.19": {"text": "Wilst du, was nicht zu fassen, fassen; die\u00df ist verwegen", "tokens": ["Wilst", "du", ",", "was", "nicht", "zu", "fas\u00b7sen", ",", "fas\u00b7sen", ";", "die\u00df", "ist", "ver\u00b7we\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "PRELS", "PTKNEG", "PTKZU", "VVINF", "$,", "VVINF", "$.", "PDS", "VAFIN", "ADJD"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "und zu viel!", "tokens": ["und", "zu", "viel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Drum denck\u2019 in Demuth an die Wahrheit: ", "tokens": ["Drum", "denck", "in", "De\u00b7muth", "an", "die", "Wahr\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "will und kann allein", "tokens": ["will", "und", "kann", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "KON", "VMFIN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.23": {"text": "Beroundert, nicht begriffen, seyn.", "tokens": ["Be\u00b7ro\u00b7un\u00b7dert", ",", "nicht", "be\u00b7grif\u00b7fen", ",", "seyn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "PTKNEG", "VVPP", "$,", "VAINF", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}}}}