{"dta.poem.12046": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fcckselig bist du, wenn auf Folgrungen und Schl\u00fcssen", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", "bist", "du", ",", "wenn", "auf", "Fol\u00b7grun\u00b7gen", "und", "Schl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Das Beste so du wei\u00dft, du nicht hast gr\u00fcnden m\u00fcssen.", "tokens": ["Das", "Bes\u00b7te", "so", "du", "wei\u00dft", ",", "du", "nicht", "hast", "gr\u00fcn\u00b7den", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VVFIN", "$,", "PPER", "PTKNEG", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So brauchst du gegen die dich auch nicht zu ereifern,", "tokens": ["So", "brauchst", "du", "ge\u00b7gen", "die", "dich", "auch", "nicht", "zu", "er\u00b7ei\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PRELS", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+---+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die mit unreifem Witz bek\u00e4mpfen deinen reifern.", "tokens": ["Die", "mit", "un\u00b7rei\u00b7fem", "Witz", "be\u00b7k\u00e4mp\u00b7fen", "dei\u00b7nen", "rei\u00b7fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Schwank ist Gedankenbau, und nur die Ueberzeugung,", "tokens": ["Schwank", "ist", "Ge\u00b7dan\u00b7ken\u00b7bau", ",", "und", "nur", "die", "Ue\u00b7ber\u00b7zeu\u00b7gung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die auf sich selber ruht, bef\u00fcrchtet keine Beugung.", "tokens": ["Die", "auf", "sich", "sel\u00b7ber", "ruht", ",", "be\u00b7f\u00fcrch\u00b7tet", "kei\u00b7ne", "Beu\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "ADV", "VVFIN", "$,", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}