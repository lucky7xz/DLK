{"textgrid.poem.49322": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Diogenes", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diogenes der pflag ohn' alle Ross' und Wagen", "tokens": ["Dio\u00b7ge\u00b7nes", "der", "pflag", "ohn'", "al\u00b7le", "Ross'", "und", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Sein ganzes Hausger\u00e4t stets mit sich umzutragen,", "tokens": ["Sein", "gan\u00b7zes", "Haus\u00b7ge\u00b7r\u00e4t", "stets", "mit", "sich", "um\u00b7zu\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "APPR", "PRF", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das war ein' alte Tasch', ein Gerstenmus, ein Stab,", "tokens": ["Das", "war", "ein'", "al\u00b7te", "Ta\u00b7sch'", ",", "ein", "Gers\u00b7ten\u00b7mus", ",", "ein", "Stab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dazu ein Becherlein, das nannt' er all' sein Hab.", "tokens": ["Da\u00b7zu", "ein", "Be\u00b7cher\u00b7lein", ",", "das", "nannt'", "er", "all'", "sein", "Hab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,", "PDS", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und wann er solches trug, so klagt' er schier mit Z\u00e4hren,", "tokens": ["Und", "wann", "er", "sol\u00b7ches", "trug", ",", "so", "klagt'", "er", "schier", "mit", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PIS", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es th\u00e4te die\u00df Ger\u00e4t ihn gar zu sehr beschweren,", "tokens": ["Es", "th\u00e4\u00b7te", "die\u00df", "Ge\u00b7r\u00e4t", "ihn", "gar", "zu", "sehr", "be\u00b7schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "NN", "PPER", "ADV", "PTKA", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er meinte seinen Stab, Beibeutel und Pokal,", "tokens": ["Er", "mein\u00b7te", "sei\u00b7nen", "Stab", ",", "Bei\u00b7beu\u00b7tel", "und", "Po\u00b7kal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der doch nur b\u00fcchen war, den Plunder allzumal.", "tokens": ["Der", "doch", "nur", "b\u00fc\u00b7chen", "war", ",", "den", "Plun\u00b7der", "all\u00b7zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVINF", "VAFIN", "$,", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Als er nun einst am Flu\u00df aus Ungeschicht that hinken", "tokens": ["Als", "er", "nun", "einst", "am", "Flu\u00df", "aus", "Un\u00b7ge\u00b7schicht", "that", "hin\u00b7ken"], "token_in