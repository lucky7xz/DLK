{"dta.poem.2619": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Letztes Ehren-Ged\u00e4chtnus/  \n  Fr. R. K. v. T. g. v. T. 18. Junii  1669.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Welt-Krei\u00df war halb Tag/ und noch halb Nacht zu\nnennen/", "tokens": ["Der", "Welt\u00b7Krei\u00df", "war", "halb", "Tag", "/", "und", "noch", "halb", "Nacht", "zu", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "NN", "$(", "KON", "ADV", "ADJD", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sternen g\u00f6ldnes Heer verlohr sich allgemach/", "tokens": ["Der", "Ster\u00b7nen", "g\u00f6ld\u00b7nes", "Heer", "ver\u00b7lohr", "sich", "all\u00b7ge\u00b7mach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PRF", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Indem der graue Tag schon durch die Wolcken brach/", "tokens": ["In\u00b7dem", "der", "grau\u00b7e", "Tag", "schon", "durch", "die", "Wol\u00b7cken", "brach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und durch die D\u00e4mmerung Aurora war zu kennen/", "tokens": ["Und", "durch", "die", "D\u00e4m\u00b7me\u00b7rung", "Au\u00b7ro\u00b7ra", "war", "zu", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NE", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als mich ein schneller Zug/ und eine starcke Hand/", "tokens": ["Als", "mich", "ein", "schnel\u00b7ler", "Zug", "/", "und", "ei\u00b7ne", "star\u00b7cke", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$(", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So ich nicht melden kan/ ri\u00df in ein ander Land.", "tokens": ["So", "ich", "nicht", "mel\u00b7den", "kan", "/", "ri\u00df", "in", "ein", "an\u00b7der", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKNEG", "VVINF", "VMFIN", "$(", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mir gieng die reine Lufft was frischer ins Gesichte/", "tokens": ["Mir", "gieng", "die", "rei\u00b7ne", "Lufft", "was", "fri\u00b7scher", "ins", "Ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PWS", "ADJA", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich sahe hohe Berg und Felsen vor mir stehn/", "tokens": ["Ich", "sa\u00b7he", "ho\u00b7he", "Berg", "und", "Fel\u00b7sen", "vor", "mir", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KON", "NN", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mich hie\u00df ein frembdes Wort behertzt noch weiter gehn/", "tokens": ["Mich", "hie\u00df", "ein", "fremb\u00b7des", "Wort", "be\u00b7hertzt", "noch", "wei\u00b7ter", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zu dem die Sonne schien in ihrem Rosen-Lichte/", "tokens": ["Zu", "dem", "die", "Son\u00b7ne", "schien", "in", "ih\u00b7rem", "Ro\u00b7sen\u00b7Lich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df ich von ferne noch das Schnee-Gebirg erblickt/", "tokens": ["Da\u00df", "ich", "von", "fer\u00b7ne", "noch", "das", "Schnee\u00b7Ge\u00b7birg", "er\u00b7blickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und unversehens auf den Eych-Berg bin ger\u00fcckt.", "tokens": ["Und", "un\u00b7ver\u00b7se\u00b7hens", "auf", "den", "Ey\u00b7ch\u00b7Berg", "bin", "ge\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.13": {"text": "Das Lust-Haus der Natur/ so sonst von Liebligkeiten", "tokens": ["Das", "Lust\u00b7Haus", "der", "Na\u00b7tur", "/", "so", "sonst", "von", "Lieb\u00b7lig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$(", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und allem Uberflu\u00df verlangter Anmuth reich/", "tokens": ["Und", "al\u00b7lem", "U\u00b7ber\u00b7flu\u00df", "ver\u00b7lang\u00b7ter", "An\u00b7muth", "reich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "NN", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sah vielmehr einer Heck/ und \u00f6den W\u00fcsten gleich/", "tokens": ["Sah", "viel\u00b7mehr", "ei\u00b7ner", "Heck", "/", "und", "\u00f6\u00b7den", "W\u00fcs\u00b7ten", "gleich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$(", "KON", "ADJA", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So da\u00df ich bey mir selbst must in Gedancken streiten/", "tokens": ["So", "da\u00df", "ich", "bey", "mir", "selbst", "must", "in", "Ge\u00b7dan\u00b7cken", "strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "PPER", "ADV", "VMFIN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ob mich ein Nacht-Gesicht und falscher Schein beth\u00f6rt/", "tokens": ["Ob", "mich", "ein", "Nacht\u00b7Ge\u00b7sicht", "und", "fal\u00b7scher", "Schein", "be\u00b7th\u00f6rt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und ob mein bl\u00f6des Ohr nur ein ", "tokens": ["Und", "ob", "mein", "bl\u00f6\u00b7des", "Ohr", "nur", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "ADV", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die gantze Gegend war mit Trauren \u00fcberschwemmet/", "tokens": ["Die", "gant\u00b7ze", "Ge\u00b7gend", "war", "mit", "Trau\u00b7ren", "\u00fc\u00b7bersc\u00b7hwem\u00b7met", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der kalte Bober-Strom/ der sonst den Ort umbarmt/", "tokens": ["Der", "kal\u00b7te", "Bo\u00b7ber\u00b7Strom", "/", "der", "sonst", "den", "Ort", "um\u00b7barmt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Flo\u00df langsam und betr\u00fcbt/ als ob es ihn erbarmt/", "tokens": ["Flo\u00df", "lang\u00b7sam", "und", "be\u00b7tr\u00fcbt", "/", "als", "ob", "es", "ihn", "er\u00b7barmt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "VVPP", "$(", "KOKOM", "KOUS", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df keine Kunst noch Macht den Schlu\u00df des Himmels hemmet/", "tokens": ["Da\u00df", "kei\u00b7ne", "Kunst", "noch", "Macht", "den", "Schlu\u00df", "des", "Him\u00b7mels", "hem\u00b7met", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Eichen/ derer Haupt vom Alter heilig war/", "tokens": ["Die", "Ei\u00b7chen", "/", "de\u00b7rer", "Haupt", "vom", "Al\u00b7ter", "hei\u00b7lig", "war", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "NN", "APPRART", "NN", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Verdorrten an Gestalt/ verlohren Safft und Haar.", "tokens": ["Ver\u00b7dorr\u00b7ten", "an", "Ge\u00b7stalt", "/", "ver\u00b7loh\u00b7ren", "Safft", "und", "Haar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$(", "VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Man sah die Dryaden mehr keine T\u00e4ntze hegen/", "tokens": ["Man", "sah", "die", "Dry\u00b7a\u00b7den", "mehr", "kei\u00b7ne", "T\u00e4nt\u00b7ze", "he\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und umb die Mittags-Zeit geniessen Schatten-Lust/", "tokens": ["Und", "umb", "die", "Mit\u00b7tags\u00b7Zeit", "ge\u00b7nies\u00b7sen", "Schat\u00b7ten\u00b7Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Chloris hatte nicht bebl\u00fcmet ihre Brust/", "tokens": ["Die", "Chlo\u00b7ris", "hat\u00b7te", "nicht", "be\u00b7bl\u00fc\u00b7met", "ih\u00b7re", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Statt g\u00f6ldner Schmergeln wuchs Napell auf allen Wegen:", "tokens": ["Statt", "g\u00f6ld\u00b7ner", "Schmer\u00b7geln", "wuchs", "Na\u00b7pell", "auf", "al\u00b7len", "We\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "NE", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der B\u00e4che Silbern Fu\u00df erstarrt/ und gieng entz\u00fcckt/", "tokens": ["Der", "B\u00e4\u00b7che", "Sil\u00b7bern", "Fu\u00df", "er\u00b7starrt", "/", "und", "gieng", "ent\u00b7z\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "VVPP", "$(", "KON", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Kein Vogel hat den Ort mit seinem Klang erquickt.", "tokens": ["Kein", "Vo\u00b7gel", "hat", "den", "Ort", "mit", "sei\u00b7nem", "Klang", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Indem ich bey mir selbst der Sachen Lauff erwogen/", "tokens": ["In\u00b7dem", "ich", "bey", "mir", "selbst", "der", "Sa\u00b7chen", "Lauff", "er\u00b7wo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und schliesse/ da\u00df ein Fall und Leid verhanden sey/", "tokens": ["Und", "schlies\u00b7se", "/", "da\u00df", "ein", "Fall", "und", "Leid", "ver\u00b7han\u00b7den", "sey", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "ART", "NN", "KON", "NN", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Kommt dem best\u00fcrtzten Ohr ein traurlg Winseln bey/", "tokens": ["Kommt", "dem", "be\u00b7st\u00fcrtz\u00b7ten", "Ohr", "ein", "traurlg", "Win\u00b7seln", "bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "APPR", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Und mir ein seufftzend Ach! und Girren nachgezo gen:", "tokens": ["Und", "mir", "ein", "seufft\u00b7zend", "Ach", "!", "und", "Gir\u00b7ren", "nach\u00b7ge\u00b7zo", "gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJD", "ITJ", "$.", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Stimme/ so halb tod/ und halb lebendig schien/", "tokens": ["Die", "Stim\u00b7me", "/", "so", "halb", "tod", "/", "und", "halb", "le\u00b7ben\u00b7dig", "schien", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADJD", "NN", "$(", "KON", "ADJD", "ADJD", "VVFIN", "$("], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Gab endlich zum Verstand mir diese Worte hin;", "tokens": ["Gab", "end\u00b7lich", "zum", "Ver\u00b7stand", "mir", "die\u00b7se", "Wor\u00b7te", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "PPER", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Unsch\u00e4tzbahrer Verlust! der Ruhm von unsern Feldern/", "tokens": ["Un\u00b7sch\u00e4tz\u00b7bah\u00b7rer", "Ver\u00b7lust", "!", "der", "Ruhm", "von", "un\u00b7sern", "Fel\u00b7dern", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.38": {"text": "Die Sonne/ die da hat die Gegend hier belebt/", "tokens": ["Die", "Son\u00b7ne", "/", "die", "da", "hat", "die", "Ge\u00b7gend", "hier", "be\u00b7lebt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die hat der Parcen Hand des Todes Garn gewebt;", "tokens": ["Die", "hat", "der", "Par\u00b7cen", "Hand", "des", "To\u00b7des", "Garn", "ge\u00b7webt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die Pallas/ die/ zum Theil in diesen heilgen W\u00e4ldern", "tokens": ["Die", "Pal\u00b7las", "/", "die", "/", "zum", "Theil", "in", "die\u00b7sen", "heil\u00b7gen", "W\u00e4l\u00b7dern"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "$(", "APPRART", "NN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Doch meist in Blumenau den Helicon gebaut/", "tokens": ["Doch", "meist", "in", "Blu\u00b7me\u00b7nau", "den", "He\u00b7li\u00b7con", "ge\u00b7baut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-