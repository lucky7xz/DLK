word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Die beiden halt in Ehren,", "tokens": ["Die", "bei\u00b7den", "halt", "in", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So wird im Zeitenlauf", "tokens": ["So", "wird", "im", "Zei\u00b7ten\u00b7lauf"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Feind dich je versehren;", "tokens": ["Kein", "Feind", "dich", "je", "ver\u00b7seh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gl\u00fcckauf, mein Volk, Gl\u00fcckauf!\u00ab", "tokens": ["Gl\u00fcc\u00b7kauf", ",", "mein", "Volk", ",", "Gl\u00fcc\u00b7kauf", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Er sang's, und steigend wallte", "tokens": ["Er", "sang's", ",", "und", "stei\u00b7gend", "wall\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Nebel um ihn her,", "tokens": ["Der", "Ne\u00b7bel", "um", "ihn", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und als das Lied verhallte,", "tokens": ["Und", "als", "das", "Lied", "ver\u00b7hall\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gewahrt' ich ihn nicht mehr.", "tokens": ["Ge\u00b7wahrt'", "ich", "ihn", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}