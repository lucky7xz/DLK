{"dta.poem.549": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Cupido  z\u00fcrnet \u00fcber der  Stellandra  und  Elio-  \n dors  H\u00e4rtigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Bin ich derjenige/ der da die Wunder macht/", "tokens": ["Bin", "ich", "der\u00b7je\u00b7ni\u00b7ge", "/", "der", "da", "die", "Wun\u00b7der", "macht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDS", "$(", "ART", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df in den Flammen steht fast alle Creatur/", "tokens": ["Da\u00df", "in", "den", "Flam\u00b7men", "steht", "fast", "al\u00b7le", "Crea\u00b7tur", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "So nur durch H\u00fclffe der Natur", "tokens": ["So", "nur", "durch", "H\u00fclf\u00b7fe", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An dieses Licht gebracht?", "tokens": ["An", "die\u00b7ses", "Licht", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja/ Ja/ ich soll es seyn/ ich seh den ", "tokens": ["Ja", "/", "Ja", "/", "ich", "soll", "es", "seyn", "/", "ich", "seh", "den"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$(", "PTKANT", "$(", "PPER", "VMFIN", "PPER", "VAINF", "$(", "PPER", "VVFIN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der stoltze ", "tokens": ["Der", "stolt\u00b7ze"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Da\u00df er sich hei\u00df entz\u00fcndt zu ", "tokens": ["Da\u00df", "er", "sich", "hei\u00df", "ent\u00b7z\u00fcndt", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVPP", "PTKZU"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ich hab es auch gemacht/", "tokens": ["Ich", "hab", "es", "auch", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Und als ein br\u00fcnstig Hirsch ", "tokens": ["Und", "als", "ein", "br\u00fcns\u00b7tig", "Hirsch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Da\u00df sich ", "tokens": ["Da\u00df", "sich"], "token_info": ["word", "word"], "pos": ["KOUS", "PRF"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Da\u00df sich ", "tokens": ["Da\u00df", "sich"], "token_info": ["word", "word"], "pos": ["KOUS", "PRF"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Mit der ", "tokens": ["Mit", "der"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Da\u00df sich die Mutter selbst mit dem ", "tokens": ["Da\u00df", "sich", "die", "Mut\u00b7ter", "selbst", "mit", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "APPR", "ART"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Da\u00df gegen ", "tokens": ["Da\u00df", "ge\u00b7gen"], "token_info": ["word", "word"], "pos": ["KOUS", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.20": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.22": {"text": "Da\u00df von der ", "tokens": ["Da\u00df", "von", "der"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPR", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.23": {"text": "Da\u00df seine Schwester selbst ", "tokens": ["Da\u00df", "sei\u00b7ne", "Schwes\u00b7ter", "selbst"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Da\u00df bey dem ", "tokens": ["Da\u00df", "bey", "dem"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Da\u00df von ", "tokens": ["Da\u00df", "von"], "token_info": ["word", "word"], "pos": ["KOUS", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.27": {"text": "Der Wunder sind noch mehr; ich hab auch ausge\u00fcbet/", "tokens": ["Der", "Wun\u00b7der", "sind", "noch", "mehr", ";", "ich", "hab", "auch", "aus\u00b7ge\u00b7\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$.", "PPER", "VAFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da\u00df der ", "tokens": ["Da\u00df", "der"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.29": {"text": "Da\u00df sich die Sonnen Blum nach ", "tokens": ["Da\u00df", "sich", "die", "Son\u00b7nen", "Blum", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "NE", "APPR"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.30": {"text": "Da\u00df sich der Reben Stock um seinen Ulmbaum flichtet.", "tokens": ["Da\u00df", "sich", "der", "Re\u00b7ben", "Stock", "um", "sei\u00b7nen", "Ulm\u00b7baum", "flich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da\u00df vor den Hunds-Stern sich im w\u00fcsten ", "tokens": ["Da\u00df", "vor", "den", "Hunds\u00b7S\u00b7tern", "sich", "im", "w\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "PRF", "APPRART", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Der Geissen wilde Schaar mit staaren Augen beuget/", "tokens": ["Der", "Geis\u00b7sen", "wil\u00b7de", "Schaar", "mit", "staa\u00b7ren", "Au\u00b7gen", "beu\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Da\u00df zu dem Agtstein sich das leichte Stroh hinneiget/", "tokens": ["Da\u00df", "zu", "dem", "Agt\u00b7stein", "sich", "das", "leich\u00b7te", "Stroh", "hin\u00b7nei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PRF", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Da\u00df man den ", "tokens": ["Da\u00df", "man", "den"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIS", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.35": {"text": "Da\u00df mit dem Gold Quecksilber sich vermischet.", "tokens": ["Da\u00df", "mit", "dem", "Gold", "Queck\u00b7sil\u00b7ber", "sich", "ver\u00b7mi\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.36": {"text": "Da\u00df sich das Schuppen-Vieh im Meer erhitzt", "tokens": ["Da\u00df", "sich", "das", "Schup\u00b7pen\u00b7Vieh", "im", "Meer", "er\u00b7hitzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Und vor grosser Liebe schwitzt/", "tokens": ["Und", "vor", "gros\u00b7ser", "Lie\u00b7be", "schwitzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Da\u00df sich ", "tokens": ["Da\u00df", "sich"], "token_info": ["word", "word"], "pos": ["KOUS", "PRF"], "meter": "+-", "measure": "trochaic.single"}, "line.39": {"text": "Da\u00df Mensch und ", "tokens": ["Da\u00df", "Mensch", "und"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "KON"], "meter": "-+-", "measure": "amphibrach.single"}, "line.40": {"text": "Da\u00df der Forell vor heisser Liebe zischet.", "tokens": ["Da\u00df", "der", "Fo\u00b7rell", "vor", "heis\u00b7ser", "Lie\u00b7be", "zi\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Der Erden Eingeweid/ das gl\u00e4ntzende Metall", "tokens": ["Der", "Er\u00b7den", "Ein\u00b7ge\u00b7weid", "/", "das", "gl\u00e4nt\u00b7zen\u00b7de", "Me\u00b7tall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "F\u00fchlt meine heissen Triebe/", "tokens": ["F\u00fchlt", "mei\u00b7ne", "heis\u00b7sen", "Trie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Man siehts am Blumen \u00fcberall/", "tokens": ["Man", "siehts", "am", "Blu\u00b7men", "\u00fc\u00b7be\u00b7rall", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Da\u00df sie bek\u00fcmmert sind aus Liebe.", "tokens": ["Da\u00df", "sie", "be\u00b7k\u00fcm\u00b7mert", "sind", "aus", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Die Sterne an den blauen Himmels-Bogen/", "tokens": ["Die", "Ster\u00b7ne", "an", "den", "blau\u00b7en", "Him\u00b7mels\u00b7Bo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Die werden durch geheimen Zug den Menschen zugezogen/", "tokens": ["Die", "wer\u00b7den", "durch", "ge\u00b7hei\u00b7men", "Zug", "den", "Men\u00b7schen", "zu\u00b7ge\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.47": {"text": "Den ich denselben einverleib/ wenn meine Macht die Wirckung thut/", "tokens": ["Den", "ich", "den\u00b7sel\u00b7ben", "ein\u00b7ver\u00b7leib", "/", "wenn", "mei\u00b7ne", "Macht", "die", "Wir\u00b7ckung", "thut", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PDAT", "NN", "$(", "KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.48": {"text": "Doch Nein/ ich bin es nicht/", "tokens": ["Doch", "Nein", "/", "ich", "bin", "es", "nicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$(", "PPER", "VAFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.49": {"text": "Ich zwinge niemand mehr zu meiner s\u00fcssen Dienstbarkeit/", "tokens": ["Ich", "zwin\u00b7ge", "nie\u00b7mand", "mehr", "zu", "mei\u00b7ner", "s\u00fcs\u00b7sen", "Dienst\u00b7bar\u00b