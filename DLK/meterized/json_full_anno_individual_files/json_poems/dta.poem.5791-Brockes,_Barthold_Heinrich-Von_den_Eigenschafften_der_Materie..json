{"dta.poem.5791": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jedoch betrachtet man die\u00df ernstlich; wird man sehn,", "tokens": ["Je\u00b7doch", "be\u00b7trach\u00b7tet", "man", "die\u00df", "ernst\u00b7lich", ";", "wird", "man", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PDS", "ADJD", "$.", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie falsch es ist, wenn man den C\u00f6rpern ein Bewegen,", "tokens": ["Wie", "falsch", "es", "ist", ",", "wenn", "man", "den", "C\u00f6r\u00b7pern", "ein", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "$,", "KOUS", "PIS", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das unaufh\u00f6rlich sey, will zugestehn.", "tokens": ["Das", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "sey", ",", "will", "zu\u00b7ge\u00b7stehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VAFIN", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die C\u00f6rper k\u00f6nnen ja, ohn\u2019 einigs Leer,", "tokens": ["Die", "C\u00f6r\u00b7per", "k\u00f6n\u00b7nen", "ja", ",", "ohn'", "ei\u00b7nigs", "Leer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "$,", "KOUI", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als wie in einem weichen Meer,", "tokens": ["Als", "wie", "in", "ei\u00b7nem", "wei\u00b7chen", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich in die R\u00fcnd\u2019 und Circkel-weise regen.", "tokens": ["Sich", "in", "die", "R\u00fcnd'", "und", "Cir\u00b7ckel\u00b7wei\u00b7se", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "KON", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}