{"dta.poem.612": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Nahmens-Tag einer vornehmen  \n Frauen/ im Nahmen ihrer Schwester.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Heut ist dein Nahmens-Tag/ ich w\u00fcnsche Gl\u00fcck dazu/", "tokens": ["Heut", "ist", "dein", "Nah\u00b7mens\u00b7Tag", "/", "ich", "w\u00fcn\u00b7sche", "Gl\u00fcck", "da\u00b7zu", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "NN", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Frau Schwester/ da\u00df du magst den Tag noch offt", "tokens": ["Frau", "Schwes\u00b7ter", "/", "da\u00df", "du", "magst", "den", "Tag", "noch", "offt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$(", "KOUS", "PPER", "VMFIN", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "erleben/", "tokens": ["er\u00b7le\u00b7ben", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Der Himmel gebe dir beliebte Lust und Ruh/", "tokens": ["Der", "Him\u00b7mel", "ge\u00b7be", "dir", "be\u00b7lieb\u00b7te", "Lust", "und", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und ich will dir mein Hertz als eigen \u00fcbergeben:", "tokens": ["Und", 