{"dta.poem.16100": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "154.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du die Nacht durchschl\u00e4fst, bedarfst du keines Lichts,", "tokens": ["Wenn", "du", "die", "Nacht", "durch\u00b7schl\u00e4fst", ",", "be\u00b7darfst", "du", "kei\u00b7nes", "Lichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wenn du wachen mu\u00dft, ist n\u00f6thiger dir nichts.", "tokens": ["Doch", "wenn", "du", "wa\u00b7chen", "mu\u00dft", ",", "ist", "n\u00f6\u00b7thi\u00b7ger", "dir", "nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VMFIN", "$,", "VAFIN", "ADJD", "PPER", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es ist ein Herzensfreund, der in Weltk\u00fcmmernissen", "tokens": ["Es", "ist", "ein", "Her\u00b7zens\u00b7freund", ",", "der", "in", "Welt\u00b7k\u00fcm\u00b7mer\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich tr\u00f6stet; m\u00f6chtest du dis Licht im Dunkel missen?", "tokens": ["Dich", "tr\u00f6s\u00b7tet", ";", "m\u00f6ch\u00b7test", "du", "dis", "Licht", "im", "Dun\u00b7kel", "mis\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VMFIN", "PPER", "PDS", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}