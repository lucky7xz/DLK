{"textgrid.poem.34234": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Im Gro\u00dfen, wie im Kleinen", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trittst du in ein Gemach, worin die bescheid'ne Reseda", "tokens": ["Trittst", "du", "in", "ein", "Ge\u00b7mach", ",", "wo\u00b7rin", "die", "be\u00b7schei\u00b7d'\u00b7ne", "Re\u00b7se\u00b7da"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+--+-+-+--+--+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Freundlich gepflegt wird, wie s\u00fc\u00df str\u00f6mt dir entgegen der Duft!", "tokens": ["Freund\u00b7lich", "ge\u00b7pflegt", "wird", ",", "wie", "s\u00fc\u00df", "str\u00f6mt", "dir", "ent\u00b7ge\u00b7gen", "der", "Duft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wenn du aber darin ein Paar Minuten verweiltest,", "tokens": ["Wenn", "du", "a\u00b7ber", "da\u00b7rin", "ein", "Paar", "Mi\u00b7nu\u00b7ten", "ver\u00b7weil\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sp\u00fcrst du ihn nicht mehr: warum geht's uns doch so mit der Welt?", "tokens": ["Sp\u00fcrst", "du", "ihn", "nicht", "mehr", ":", "wa\u00b7rum", "geht's", "uns", "doch", "so", "mit", "der", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"