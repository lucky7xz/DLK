{"textgrid.poem.50637": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Eitelkeit weltlicher Lust' und Ehren", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df eine F\u00fcrstinn mich umfing mit hei\u00dfen K\u00fcssen,", "tokens": ["Da\u00df", "ei\u00b7ne", "F\u00fcrs\u00b7tinn", "mich", "um\u00b7fing", "mit", "hei\u00b7\u00dfen", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mich ein K\u00f6nig ", "tokens": ["Und", "mich", "ein", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Ich schmeckte, was ich f\u00fchlt', und f\u00fchlte, was ich h\u00f6rt'.", "tokens": ["Ich", "schmeck\u00b7te", ",", "was", "ich", "f\u00fchlt'", ",", "und", "f\u00fchl\u00b7te", ",", "was", "ich", "h\u00f6rt'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "KON", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als aber ich erwacht' und nach den sanften Zeichen", "tokens": ["Als", "a\u00b7ber", "ich", "er\u00b7wacht'", "und", "nach", "den", "sanf\u00b7ten", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "VVFIN", "KON",