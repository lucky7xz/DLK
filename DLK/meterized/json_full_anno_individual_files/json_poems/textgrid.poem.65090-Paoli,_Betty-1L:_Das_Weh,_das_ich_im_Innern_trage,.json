{"textgrid.poem.65090": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Weh, das ich im Innern trage,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Weh, das ich im Innern trage,", "tokens": ["Das", "Weh", ",", "das", "ich", "im", "In\u00b7nern", "tra\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Treu wahr' ich es, entschloss'nen Sinnes,", "tokens": ["Treu", "wahr'", "ich", "es", ",", "ent\u00b7schloss'\u00b7nen", "Sin\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als letzten Rest gl\u00fccksel'ger Tage,", "tokens": ["Als", "letz\u00b7ten", "Rest", "gl\u00fcck\u00b7sel'\u00b7ger", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als B\u00fcrgschaft ewigen Gewinnes.", "tokens": ["Als", "B\u00fcrg\u00b7schaft", "e\u00b7wi\u00b7gen", "Ge\u00b7win\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob auch dem bl\u00f6den Aug' des Thoren,", "tokens": ["Ob", "auch", "dem", "bl\u00f6\u00b7den", "Aug'", "des", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measur