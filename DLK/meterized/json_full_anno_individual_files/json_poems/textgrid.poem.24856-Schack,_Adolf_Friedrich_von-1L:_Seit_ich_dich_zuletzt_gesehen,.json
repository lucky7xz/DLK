{"textgrid.poem.24856": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seit ich dich zuletzt gesehen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit ich dich zuletzt gesehen,", "tokens": ["Seit", "ich", "dich", "zu\u00b7letzt", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4dchen, sprich, was ist geschehen?", "tokens": ["M\u00e4d\u00b7chen", ",", "sprich", ",", "was", "ist", "ge\u00b7sche\u00b7hen", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "PWS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich erkenn' ich wieder kaum;", "tokens": ["Dich", "er\u00b7kenn'", "ich", "wie\u00b7der", "kaum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo ist nun dein muntres Lachen?", "tokens": ["Wo", "ist", "nun", "dein", "mun\u00b7tres", "La\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.5": {"text": "Wo dein Scherz schon beim Erwachen", "tokens": ["Wo", "dein", "Scherz", "schon", "beim", "Er\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus der N\u00e4chte heitrem Traum?", "tokens": ["Aus", "der", "N\u00e4ch\