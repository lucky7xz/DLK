{"dta.poem.10337": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ix.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Myrthen und Rosen, lieblich und hold,", "tokens": ["Mit", "Myr\u00b7then", "und", "Ro\u00b7sen", ",", "lieb\u00b7lich", "und", "hold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit duft'gen Zypressen und Flittergold,", "tokens": ["Mit", "duft'\u00b7gen", "Zyp\u00b7res\u00b7sen", "und", "Flit\u00b7ter\u00b7gold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "M\u00f6cht' ich zieren die\u00df Buch wie 'nen Todtenschrein,", "tokens": ["M\u00f6cht'", "ich", "zie\u00b7ren", "die\u00df", "Buch", "wie", "'nen", "Tod\u00b7ten\u00b7schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PDS", "NN", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und sargen meine Lieder hinein.", "tokens": ["Und", "sar\u00b7gen", "mei\u00b7ne", "Lie\u00b7der", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "O k\u00f6nnt' ich die Liebe sargen hinzu!", "tokens": ["O", "k\u00f6nnt'", "ich", "die", "Lie\u00b7be", "sar\u00b7gen", "hin\u00b7zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "++--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Auf dem Grabe der Liebe w\u00e4chst Bl\u00fcmlein der Ruh,", "tokens": ["Auf", "dem", "Gra\u00b7be", "der", "Lie\u00b7be", "w\u00e4chst", "Bl\u00fcm\u00b7lein", "der", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "NN", "ART", "NN", "$,"], "meter": "--+--+-++--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da bl\u00fcht es hervor, da pfl\u00fcckt man es ab, \u2014", "tokens": ["Da", "bl\u00fcht", "es", "her\u00b7vor", ",", "da", "pfl\u00fcckt", "man", "es", "ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PIS", "PPER", "PTKVZ", "$,", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch mir bl\u00fcht's nur, wenn ich selber im Grab.", "tokens": ["Doch", "mir", "bl\u00fcht's", "nur", ",", "wenn", "ich", "sel\u00b7ber", "im", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Hier sind nun die Lieder, die einst so wild,", "tokens": ["Hier", "sind", "nun", "die", "Lie\u00b7der", ",", "die", "einst", "so", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie ein Lavastrom, der dem Aetna entquillt,", "tokens": ["Wie", "ein", "La\u00b7vast\u00b7rom", ",", "der", "dem", "A\u00b7et\u00b7na", "ent\u00b7quillt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ART", "NE", "VVFIN", "$,"], "meter": "-++-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Hervorgest\u00fcrzt aus dem tiefsten Gem\u00fcth,", "tokens": ["Her\u00b7vor\u00b7ge\u00b7st\u00fcrzt", "aus", "dem", "tiefs\u00b7ten", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und rings viel blitzende Funken verspr\u00fch't!", "tokens": ["Und", "rings", "viel", "blit\u00b7zen\u00b7de", "Fun\u00b7ken", "ver\u00b7spr\u00fch't", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Nun liegen sie stumm und todtengleich,", "tokens": ["Nun", "lie\u00b7gen", "sie", "stumm", "und", "tod\u00b7ten\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nun starren sie kalt und nebelbleich.", "tokens": ["Nun", "star\u00b7ren", "sie", "kalt", "und", "ne\u00b7bel\u00b7bleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch auf's neu' die alte Gluth sie belebt,", "tokens": ["Doch", "auf's", "neu'", "die", "al\u00b7te", "Gluth", "sie", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "ART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn der Liebe Geist einst \u00fcber sie schwebt.", "tokens": ["Wenn", "der", "Lie\u00b7be", "Geist", "einst", "\u00fc\u00b7ber", "sie", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Und es wird mir im Herzen viel Ahnung laut:", "tokens": ["Und", "es", "wird", "mir", "im", "Her\u00b7zen", "viel", "Ah\u00b7nung", "laut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "APPRART", "NN", "PIAT", "NN", "ADJD", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Der Liebe Geist einst \u00fcber sie thaut;", "tokens": ["Der", "Lie\u00b7be", "Geist", "einst", "\u00fc\u00b7ber", "sie", "thaut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Einst kommt die\u00df Buch in deine Hand,", "tokens": ["Einst", "kommt", "die\u00df", "Buch", "in", "dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du s\u00fc\u00dfes Lieb im fernen Land.", "tokens": ["Du", "s\u00fc\u00b7\u00dfes", "Lieb", "im", "fer\u00b7nen", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann l\u00f6st sich des Liedes Zauberbann,", "tokens": ["Dann", "l\u00f6st", "sich", "des", "Lie\u00b7des", "Zau\u00b7ber\u00b7bann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die blassen Buchstaben schaun dich an,", "tokens": ["Die", "blas\u00b7sen", "Buch\u00b7sta\u00b7ben", "schaun", "dich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Sie schauen dir flehend in's sch\u00f6ne Aug',", "tokens": ["Sie", "schau\u00b7en", "dir", "fle\u00b7hend", "in's", "sch\u00f6\u00b7ne", "Aug'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und fl\u00fcstern mit Wehmuth und Liebeshauch.", "tokens": ["Und", "fl\u00fcs\u00b7tern", "mit", "Weh\u00b7muth", "und", "Lie\u00b7bes\u00b7hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}