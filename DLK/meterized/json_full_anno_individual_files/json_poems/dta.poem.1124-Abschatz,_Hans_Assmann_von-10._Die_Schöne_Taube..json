{"dta.poem.1124": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "10. Die Sch\u00f6ne Taube.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Weil deiner Sch\u00f6nheit Ruff die gantze Welt betaubt/ ", "tokens": ["Weil", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "Ruff", "die", "gant\u00b7ze", "Welt", "be\u00b7taubt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil Amors sein Gescho\u00df aus deinen Augen kracht/", "tokens": ["Weil", "A\u00b7mors", "sein", "Ge\u00b7scho\u00df", "aus", "dei\u00b7nen", "Au\u00b7gen", "kracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil meiner Thr\u00e4nen Nil auff deinem Fel\u00df zerstaubt/", "tokens": ["Weil", "mei\u00b7ner", "Thr\u00e4\u00b7nen", "Nil", "auff", "dei\u00b7nem", "Fel\u00df", "zer\u00b7staubt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NE", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bistu vielleicht dadurch/ o Nymphe/ taub gemacht.", "tokens": ["Bis\u00b7tu", "viel\u00b7leicht", "da\u00b7durch", "/", "o", "Nym\u00b7phe", "/", "taub", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "PAV", "$(", "FM", "NN", "$(", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Betrug verf\u00fchret den/ der allzu leichtlich glaubt;", "tokens": ["Be\u00b7trug", "ver\u00b7f\u00fch\u00b7ret", "den", "/", "der", "all\u00b7zu", "leicht\u00b7lich", "glaubt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "$(", "ART", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dir wird durch geiles Wort kein Gifft zum Hertzen bracht/", "tokens": ["Dir", "wird", "durch", "gei\u00b7les", "Wort", "kein", "Gifft", "zum", "Hert\u00b7zen", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gleichwie die Schlange (wo di\u00df Gleichnis ist erlaubt/)", "tokens": ["Gleich\u00b7wie", "die", "Schlan\u00b7ge", "(", "wo", "di\u00df", "Gleich\u00b7nis", "ist", "er\u00b7laubt", "/", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "$(", "PWAV", "PDS", "NN", "VAFIN", "VVPP", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit zugestopfftem Ohr auff kein Beschweren acht.", "tokens": ["Mit", "zu\u00b7ge\u00b7stopff\u00b7tem", "Ohr", "auff", "kein", "Be\u00b7schwe\u00b7ren", "acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PIAT", "NN", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Allein/ du h\u00f6rst auch nicht die Warheit meiner Klagen.", "tokens": ["Al\u00b7lein", "/", "du", "h\u00f6rst", "auch", "nicht", "die", "War\u00b7heit", "mei\u00b7ner", "Kla\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPER", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gedult! ich bleibe doch/ wie vor/ in dich entbrannt.", "tokens": ["Ge\u00b7dult", "!", "ich", "blei\u00b7be", "doch", "/", "wie", "vor", "/", "in", "dich", "ent\u00b7brannt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ADV", "$(", "KOKOM", "APPR", "$(", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dein taubes Ohr ersezt der Finger ihr Verstand.", "tokens": ["Dein", "tau\u00b7bes", "Ohr", "er\u00b7sezt", "der", "Fin\u00b7ger", "ihr", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dein Auge mag mein Hertz/ ich will dein Auge fragen.", "tokens": ["Dein", "Au\u00b7ge", "mag", "mein", "Hertz", "/", "ich", "will", "dein", "Au\u00b7ge", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPOSAT", "NN", "$(", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Lichter/ welche so/ wie deine Sonnen blincken/", "tokens": ["Die", "Lich\u00b7ter", "/", "wel\u00b7che", "so", "/", "wie", "dei\u00b7ne", "Son\u00b7nen", "blin\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "ADV", "$(", "KOKOM", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die sprechen ohne Wort/ befehlen uns durch Wincken.", "tokens": ["Die", "spre\u00b7chen", "oh\u00b7ne", "Wort", "/", "be\u00b7feh\u00b7len", "uns", "durch", "Win\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$(", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}