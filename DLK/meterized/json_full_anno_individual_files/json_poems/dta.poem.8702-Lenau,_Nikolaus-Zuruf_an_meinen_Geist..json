{"dta.poem.8702": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Zuruf an meinen Geist.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Auf schwingt der Aar sich \u00fcber dem Schlachtgefild,", "tokens": ["Auf", "schwingt", "der", "Aar", "sich", "\u00fc\u00b7ber", "dem", "Schlacht\u00b7ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Senkt bald herab sein Aug' auf die Leichen, bald,", "tokens": ["Senkt", "bald", "her\u00b7ab", "sein", "Aug'", "auf", "die", "Lei\u00b7chen", ",", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zerreissend k\u00fchn den Wolkenvorhang,", "tokens": ["Zer\u00b7reis\u00b7send", "k\u00fchn", "den", "Wol\u00b7ken\u00b7vor\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Blickt er hinauf in die goldne Sonne;", "tokens": ["Blickt", "er", "hin\u00b7auf", "in", "die", "gold\u00b7ne", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "So schwing' empor dich Geist, und verweile jezt", "tokens": ["So", "schwing'", "em\u00b7por", "dich", "Geist", ",", "und", "ver\u00b7wei\u00b7le", "jezt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "PPER", "NN", "$,", "KON", "VVFIN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Beim Tode, jezt durchdringe die Wolke, die", "tokens": ["Beim", "To\u00b7de", ",", "jezt", "durch\u00b7drin\u00b7ge", "die", "Wol\u00b7ke", ",", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Sonnenstrahl der Auferstehung", "tokens": ["Den", "Son\u00b7nen\u00b7strahl", "der", "Auf\u00b7er\u00b7ste\u00b7hung"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fallen nicht l\u00e4\u00dft in die offnen Gr\u00e4ber!", "tokens": ["Fal\u00b7len", "nicht", "l\u00e4\u00dft", "in", "die", "off\u00b7nen", "Gr\u00e4\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}