": "Sprach ich kurz und schlicht, \u2013", "tokens": ["Sprach", "ich", "kurz", "und", "schlicht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "ADJD", "KON", "VVFIN", "$,", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "\u00bbjeder kann es anders meinen,", "tokens": ["\u00bb", "je\u00b7der", "kann", "es", "an\u00b7ders", "mei\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder etwa nicht?\u00ab", "tokens": ["O\u00b7der", "et\u00b7wa", "nicht", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "PTKNEG", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbnein!\u00ab So schrie er wutentbronnen,", "tokens": ["\u00bb", "nein", "!", "\u00ab", "So", "schrie", "er", "wu\u00b7tent\u00b7bron\u00b7nen", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$.", "$(", "ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbfreiheit ist 's Panier,", "tokens": ["\u00bb", "frei\u00b7heit", "ist", "'s", "Pa\u00b7nier", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Du bist nicht wie ich gesonnen!\u00ab", "tokens": ["Du", "bist", "nicht", "wie", "ich", "ge\u00b7son\u00b7nen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PWAV", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er scho\u00df nach mir.", "tokens": [