{"dta.poem.9564": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die \u00fcber ihren entlauffenen sohn erz\u00fcrnete  \n Venus.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was will der heisse zorn? was das verg\u00e4llte dr\u00e4uen?", "tokens": ["Was", "will", "der", "heis\u00b7se", "zorn", "?", "was", "das", "ver\u00b7g\u00e4ll\u00b7te", "dr\u00e4u\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "$.", "PWS", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was die ruthe dort in deiner rechten hand?", "tokens": ["Und", "was", "die", "ru\u00b7the", "dort", "in", "dei\u00b7ner", "rech\u00b7ten", "hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo kommstu so verwirrt/ so hitzig hergerant?", "tokens": ["Wo", "komms\u00b7tu", "so", "ver\u00b7wirrt", "/", "so", "hit\u00b7zig", "her\u00b7ge\u00b7rant", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ADJD", "$(", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie? Venus/ h\u00f6rstu nicht? sie schwieg; bald h\u00f6rt\u2019 ich schreyen:", "tokens": ["Wie", "?", "Ve\u00b7nus", "/", "h\u00f6rs\u00b7tu", "nicht", "?", "sie", "schwieg", ";", "bald", "h\u00f6rt'", "ich", "schre\u00b7yen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "NN", "$(", "VVFIN", "PTKNEG", "$.", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "Du abgefeimter dieb! lernstu dich noch nicht scheuen?", "tokens": ["Du", "ab\u00b7ge\u00b7feim\u00b7ter", "dieb", "!", "lerns\u00b7tu", "dich", "noch", "nicht", "scheu\u00b7en", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "VVFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich habe dir vorl\u00e4ngst die ruthe zuerkannt;", "tokens": ["Ich", "ha\u00b7be", "dir", "vor\u00b7l\u00e4ngst", "die", "ru\u00b7the", "zu\u00b7er\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nun aber solt du seyn aus meiner gunst verbannt.", "tokens": ["Nun", "a\u00b7ber", "solt", "du", "seyn", "aus", "mei\u00b7ner", "gunst", "ver\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VAINF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Halt schelm! halt b\u00f6sewicht! es soll dir wohl gereuen!", "tokens": ["Halt", "schelm", "!", "halt", "b\u00f6\u00b7se\u00b7wicht", "!", "es", "soll", "dir", "wohl", "ge\u00b7reu\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "VVFIN", "ADJD", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich rieff ihr weiter zu: Was? suchstu deinen sohn?", "tokens": ["Ich", "rieff", "ihr", "wei\u00b7ter", "zu", ":", "Was", "?", "suchs\u00b7tu", "dei\u00b7nen", "sohn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "PWS", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er baut bey Solimen gleich itzt den liebes-thron/", "tokens": ["Er", "baut", "bey", "So\u00b7li\u00b7men", "gleich", "itzt", "den", "lie\u00b7bes\u00b7thron", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "ADV", "ART", "NN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Und m\u00fchet sich durch sie die hertzen zu entz\u00fcnden:", "tokens": ["Und", "m\u00fc\u00b7het", "sich", "durch", "sie", "die", "hert\u00b7zen", "zu", "ent\u00b7z\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.12": {"text": "Bald reitzt er durch ihr aug/ bald wieder durch den mund/", "tokens": ["Bald", "reitzt", "er", "durch", "ihr", "aug", "/", "bald", "wie\u00b7der", "durch", "den", "mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$(", "ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ach Venus! r\u00e4che mich; er hat mich auch verwundt/", "tokens": ["Ach", "Ve\u00b7nus", "!", "r\u00e4\u00b7che", "mich", ";", "er", "hat", "mich", "auch", "ver\u00b7wundt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Lauff hin zur Solime/ da da wirst du ihn finden.", "tokens": ["Lauff", "hin", "zur", "So\u00b7li\u00b7me", "/", "da", "da", "wirst", "du", "ihn", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPRART", "NN", "$(", "ADV", "ADV", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}}}}