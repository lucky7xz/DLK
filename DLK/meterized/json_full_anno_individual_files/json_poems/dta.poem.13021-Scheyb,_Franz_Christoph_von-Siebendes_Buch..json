{"dta.poem.13021": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hierauf fiel jemand ein, (die schimmerte von Gold)", "tokens": ["Hier\u00b7auf", "fiel", "je\u00b7mand", "ein", ",", "(", "die", "schim\u00b7mer\u00b7te", "von", "Gold", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "PTKVZ", "$,", "$(", "ART", "ADJA", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ewas \u201e, sprach sie, bist du dann nur deinen Sinnen hold?", "tokens": ["\u201e", "was", "\u201e", ",", "sprach", "sie", ",", "bist", "du", "dann", "nur", "dei\u00b7nen", "Sin\u00b7nen", "hold", "?"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "$(", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "115\u201dIsts m\u00f6glich, da\u00df dein Herz dem Mund nicht wiederspreche?", "tokens": ["\"", "Ists", "m\u00f6g\u00b7lich", ",", "da\u00df", "dein", "Herz", "dem", "Mund", "nicht", "wie\u00b7der\u00b7spre\u00b7che", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ees kommt mir vor, da\u00df es dir an Beweis gebreche.", "tokens": ["\u201e", "es", "kommt", "mir", "vor", ",", "da\u00df", "es", "dir", "an", "Be\u00b7weis", "ge\u00b7bre\u00b7che", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eschreckst du vielleicht nur die, die leicht zu schrecken seynd?", "tokens": ["\u201e", "schreckst", "du", "viel\u00b7leicht", "nur", "die", ",", "die", "leicht", "zu", "schre\u00b7cken", "seynd", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "ADV", "ART", "$,", "PRELS", "ADJD", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eerkl\u00e4r uns deine Rach! wie strittst du mit dem Feind?", "tokens": ["\u201e", "er\u00b7kl\u00e4r", "uns", "dei\u00b7ne", "Rach", "!", "wie", "strittst", "du", "mit", "dem", "Feind", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "PPOSAT", "NN", "$.", "PWAV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}