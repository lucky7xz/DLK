{"dta.poem.19490": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Aus der H\u00f6he.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Schreitend meinen H\u00f6henpfad,", "tokens": ["Schrei\u00b7tend", "mei\u00b7nen", "H\u00f6\u00b7hen\u00b7pfad", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seh' ich statt lebend'ger Fluth", "tokens": ["Seh'", "ich", "statt", "le\u00b7ben\u00b7d'\u00b7ger", "Fluth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Unter mir des Eises Flur,", "tokens": ["Un\u00b7ter", "mir", "des", "Ei\u00b7ses", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Drauf der Wettlauf Tausender", "tokens": ["Drauf", "der", "Wett\u00b7lauf", "Tau\u00b7sen\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unerm\u00fcdlich sich erg\u00f6tzt.", "tokens": ["Un\u00b7er\u00b7m\u00fcd\u00b7lich", "sich", "er\u00b7g\u00f6tzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Horch! Ein dunkel Geisterlied,", "tokens": ["Horch", "!", "Ein", "dun\u00b7kel", "Geis\u00b7ter\u00b7lied", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie des Bienenkorbs Gesums:", "tokens": ["Wie", "des", "Bie\u00b7nen\u00b7korbs", "Ge\u00b7sums", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dr\u00f6hnend sonder Unterbruch", "tokens": ["Dr\u00f6h\u00b7nend", "son\u00b7der", "Un\u00b7ter\u00b7bruch"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Durch die reine Winterluft", "tokens": ["Durch", "die", "rei\u00b7ne", "Win\u00b7ter\u00b7luft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Des gest\u00e4hlten Schuhes Ton \u2014", "tokens": ["Des", "ge\u00b7st\u00e4hl\u00b7ten", "Schu\u00b7hes", "Ton"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Meiner Jugend einz'ge Lust", "tokens": ["Mei\u00b7ner", "Ju\u00b7gend", "einz'\u00b7ge", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "L\u00e4utet dumpf zu mir empor.", "tokens": ["L\u00e4u\u00b7tet", "dumpf", "zu", "mir", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}