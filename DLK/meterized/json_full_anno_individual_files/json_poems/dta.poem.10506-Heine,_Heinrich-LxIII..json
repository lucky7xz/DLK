{"dta.poem.10506": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Wer zum erstenmale liebt,", "tokens": ["Wer", "zum", "ers\u00b7ten\u00b7ma\u00b7le", "liebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sey's auch gl\u00fccklos, ist ein Gott;", "tokens": ["Sey's", "auch", "gl\u00fcc\u00b7klos", ",", "ist", "ein", "Gott", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "$,", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber wer zum zweitenmale", "tokens": ["A\u00b7ber", "wer", "zum", "zwei\u00b7ten\u00b7ma\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00fccklos liebt, der ist ein Narr.", "tokens": ["Gl\u00fcc\u00b7klos", "liebt", ",", "der", "ist", "ein", "Narr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "PRELS", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich, ein solcher Narr, ich liebe", "tokens": ["Ich", ",", "ein", "sol\u00b7cher", "Narr", ",", "ich", "lie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "ART", "PIAT", "NN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder ohne Gegenliebe!", "tokens": ["Wie\u00b7der", "oh\u00b7ne", "Ge\u00b7gen\u00b7lie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonne, Mond und Sterne lachen,", "tokens": ["Son\u00b7ne", ",", "Mond", "und", "Ster\u00b7ne", "la\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich lache mit \u2014 und sterbe.", "tokens": ["Und", "ich", "la\u00b7che", "mit", "und", "ster\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "$(", "KON", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}