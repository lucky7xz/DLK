{"dta.poem.3388": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die Narcisse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hat die Sonne dich doch, duftiges Mayenkind,", "tokens": ["Hat", "die", "Son\u00b7ne", "dich", "doch", ",", "duf\u00b7ti\u00b7ges", "Ma\u00b7yen\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Deiner Kammer entlockt, wo du so traulich schliefst,", "tokens": ["Dei\u00b7ner", "Kam\u00b7mer", "ent\u00b7lockt", ",", "wo", "du", "so", "trau\u00b7lich", "schliefst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "PWAV", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wie im Schoosse der Mutter", "tokens": ["Wie", "im", "Schoos\u00b7se", "der", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Leisaufathmend der S\u00e4ugling schl\u00e4ft?", "tokens": ["Leis\u00b7au\u00b7fath\u00b7mend", "der", "S\u00e4ug\u00b7ling", "schl\u00e4ft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Warum weiltest du nicht, z\u00fcchtige Dufterin,", "tokens": ["Wa\u00b7rum", "weil\u00b7test", "du", "nicht", ",", "z\u00fcch\u00b7ti\u00b7ge", "Duf\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKNEG", "$,", "ADJA", "NN", "$,"], "meter": "----+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In dem schirmenden Schooss? Dr\u00e4ngtest dich k\u00fchn", "tokens": ["In", "dem", "schir\u00b7men\u00b7den", "Scho\u00b7oss", "?", "Dr\u00e4ng\u00b7test", "dich", "k\u00fchn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "VVFIN", "PPER", "VVINF"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "hervor,", "tokens": ["her\u00b7vor", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Und entschleierst den Winden", "tokens": ["Und", "ent\u00b7schlei\u00b7erst", "den", "Win\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Deine bebende Schwanenbrust?", "tokens": ["Dei\u00b7ne", "be\u00b7ben\u00b7de", "Schwa\u00b7nen\u00b7brust", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Ach, du l\u00e4chelst mich an: \u201eIst mein Gewand nicht", "tokens": ["Ach", ",", "du", "l\u00e4\u00b7chelst", "mich", "an", ":", "\u201e", "Ist", "mein", "Ge\u00b7wand", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "VAFIN", "PPOSAT", "NN", "PTKNEG"], "meter": "+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "weiss?", "tokens": ["weiss", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "\u201ebl\u00fcht mein Mund nicht so roth? Bin ich nicht", "tokens": ["\u201e", "bl\u00fcht", "mein", "Mund", "nicht", "so", "roth", "?", "Bin", "ich", "nicht"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "ADJD", "$.", "VAFIN", "PPER", "PTKNEG"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "lieb und hold?", "tokens": ["lieb", "und", "hold", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "\u201ewarum sollt' ich nicht kommen,", "tokens": ["\u201e", "wa\u00b7rum", "sollt'", "ich", "nicht", "kom\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201eda mir winken der May und Lenz?\u201c", "tokens": ["\u201e", "da", "mir", "win\u00b7ken", "der", "May", "und", "Lenz", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPER", "VVFIN", "ART", "NN", "KON", "NN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "L\u00e4chle immer \u2014 o weh! kennest, so jung! so zart!", "tokens": ["L\u00e4ch\u00b7le", "im\u00b7mer", "o", "weh", "!", "ken\u00b7nest", ",", "so", "jung", "!", "so", "zart", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$(", "FM", "PTKVZ", "$.", "VVFIN", "$,", "ADV", "ADJD", "$.", "ADV", "ADJD", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Kennest Sitte der Welt, weisst um ihr Elend nicht!", "tokens": ["Ken\u00b7nest", "Sit\u00b7te", "der", "Welt", ",", "weisst", "um", "ihr", "E\u00b7lend", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wetter werden dich geisseln,", "tokens": ["Wet\u00b7ter", "wer\u00b7den", "dich", "geis\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Regen st\u00e4upen vor Abendzeit!", "tokens": ["Re\u00b7gen", "st\u00e4u\u00b7pen", "vor", "A\u00b7bend\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Warum neigst du dein Haupt? Bist ja so lieb! so", "tokens": ["Wa\u00b7rum", "neigst", "du", "dein", "Haupt", "?", "Bist", "ja", "so", "lieb", "!", "so"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "VAFIN", "ADV", "ADV", "ADJD", "$.", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "hold?", "tokens": ["hold", "?"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Dein jungfr\u00e4ulicher Schooss rosig! die Brust so weiss!", "tokens": ["Dein", "jung\u00b7fr\u00e4u\u00b7li\u00b7cher", "Scho\u00b7oss", "ro\u00b7sig", "!", "die", "Brust", "so", "weiss", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$.", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Frevler werden dich sch\u00e4nden,", "tokens": ["Frev\u00b7ler", "wer\u00b7den", "dich", "sch\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "R\u00e4uber lauern der Unschuld auf.", "tokens": ["R\u00e4u\u00b7ber", "lau\u00b7ern", "der", "Un\u00b7schuld", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "O des perlenden Thau's, der dir im Antlitz bebt!", "tokens": ["O", "des", "per\u00b7len\u00b7den", "Thau's", ",", "der", "dir", "im", "Ant\u00b7litz", "bebt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Jammer k\u00fcndet er an, bebt er im Menschenblick!", "tokens": ["Jam\u00b7mer", "k\u00fcn\u00b7det", "er", "an", ",", "bebt", "er", "im", "Men\u00b7schen\u00b7blick", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Diese Perlen sind Thr\u00e4nen \u2014", "tokens": ["Die\u00b7se", "Per\u00b7len", "sind", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "NN", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Weine, weine; der M\u00f6rder harrt. \u2014 \u2014", "tokens": ["Wei\u00b7ne", ",", "wei\u00b7ne", ";", "der", "M\u00f6r\u00b7der", "harrt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "ART", "NN", "VVFIN", "$.", "$(", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Als der S\u00e4nger noch sang, siehe! da streckte sich", "tokens": ["Als", "der", "S\u00e4n\u00b7ger", "noch", "sang", ",", "sie\u00b7he", "!", "da", "streck\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "$,", "VVIMP", "$.", "ADV", "VVFIN", "PRF"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Eine gierige Hand, raffte die Holde weg,", "tokens": ["Ei\u00b7ne", "gie\u00b7ri\u00b7ge", "Hand", ",", "raff\u00b7te", "die", "Hol\u00b7de", "weg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und zerpfl\u00fcckte die Bl\u00e4tter,", "tokens": ["Und", "zer\u00b7pfl\u00fcck\u00b7te", "die", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dass sie stoben den Garten durch.", "tokens": ["Dass", "sie", "sto\u00b7ben", "den", "Gar\u00b7ten", "durch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}