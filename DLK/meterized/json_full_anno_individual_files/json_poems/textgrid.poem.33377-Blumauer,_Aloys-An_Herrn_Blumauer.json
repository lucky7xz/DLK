{"textgrid.poem.33377": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Blumauer", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein deutscher Druck der Hand, Freund! sey mein Dank", "tokens": ["Ein", "deut\u00b7scher", "Druck", "der", "Hand", ",", "Freund", "!", "sey", "mein", "Dank"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "NN", "$.", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr deinen launigen Gesang.", "tokens": ["F\u00fcr", "dei\u00b7nen", "lau\u00b7ni\u00b7gen", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zufrieden mit dem Beifall edler Seelen,", "tokens": ["Zu\u00b7frie\u00b7den", "mit", "dem", "Bei\u00b7fall", "ed\u00b7ler", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Soll heuchlerische Mi\u00dfgunst dich nicht qu\u00e4len;", "tokens": ["Soll", "heuch\u00b7le\u00b7ri\u00b7sche", "Mi\u00df\u00b7gunst", "dich", "nicht", "qu\u00e4\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verh\u00f6hn' des Aberglaubens Zischen, Joseph sitzt", "tokens": ["Ver\u00b7h\u00f6hn'", "des", "A\u00b7berg\u00b7lau\u00b7bens", "Zi\u00b7schen", ",", "Jo\u00b7se\u00b7ph", "sitzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "NN", "$,", "NE", "VVFIN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Nun auf dem Throne, der die Wahrheit sch\u00fctzt.", "tokens": ["Nun", "auf", "dem", "Thro\u00b7ne", ",", "der", "die", "Wahr\u00b7heit", "sch\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Heuchelei, die H\u00f6lle in dem Herzen,", "tokens": ["Die", "Heu\u00b7che\u00b7lei", ",", "die", "H\u00f6l\u00b7le", "in", "dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Himmel in dem Munde, mag in deinen Scherzen", "tokens": ["Den", "Him\u00b7mel", "in", "dem", "Mun\u00b7de", ",", "mag", "in", "dei\u00b7nen", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Verbrechen finden; sie mag boshaft dumm,", "tokens": ["Ver\u00b7bre\u00b7chen", "fin\u00b7den", ";", "sie", "mag", "bos\u00b7haft", "dumm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "PPER", "VMFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das Pferd von Troja mit dem Papste Rom's vergleichen", "tokens": ["Das", "Pferd", "von", "Tro\u00b7ja", "mit", "dem", "Paps\u00b7te", "Rom's", "ver\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "APPR", "ART", "NN", "NE", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein Blick von Joseph wird der Mi\u00dfgunst Nacht verscheuchen;", "tokens": ["Ein", "Blick", "von", "Jo\u00b7se\u00b7ph", "wird", "der", "Mi\u00df\u00b7gunst", "Nacht", "ver\u00b7scheu\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VAFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Ein Blick von ihm macht selbst die Bosheit stumm.", "tokens": ["Ein", "Blick", "von", "ihm", "macht", "selbst", "die", "Bos\u00b7heit", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "K\u00fchn kannst du, Freund, der Wahrheit treu, bekennen,", "tokens": ["K\u00fchn", "kannst", "du", ",", "Freund", ",", "der", "Wahr\u00b7heit", "treu", ",", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Da\u00df manches Glied von uns'rer heiligen Klerisei", "tokens": ["Da\u00df", "man\u00b7ches", "Glied", "von", "un\u00b7s'\u00b7rer", "hei\u00b7li\u00b7gen", "Kle\u00b7ri\u00b7sei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Gerade, wie dein Eremit aus Argos, sei:", "tokens": ["Ge\u00b7ra\u00b7de", ",", "wie", "dein", "E\u00b7re\u00b7mit", "aus", "Ar\u00b7gos", ",", "sei", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPOSAT", "NN", "APPR", "NE", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Allein, des Papstes Ankunft Troja's Pferd zu nennen,", "tokens": ["Al\u00b7lein", ",", "des", "Paps\u00b7tes", "An\u00b7kunft", "Tro\u00b7ja's", "Pferd", "zu", "nen\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "ADJA", "NN", "NE", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Fiel niemals dir, nur unser'm P\u00f6bel bei;", "tokens": ["Fiel", "nie\u00b7mals", "dir", ",", "nur", "un\u00b7ser'm", "P\u00f6\u00b7bel", "bei", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "$,", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Er frage nur, wie alt dein Pferd von Troja sei,", "tokens": ["Er", "fra\u00b7ge", "nur", ",", "wie", "alt", "dein", "Pferd", "von", "Tro\u00b7ja", "sei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "APPR", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und sieh, er wird err\u00f6thend dir bekennen,", "tokens": ["Und", "sieh", ",", "er", "wird", "er\u00b7r\u00f6\u00b7thend", "dir", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Da\u00df dein Gedanke nicht so neu,", "tokens": ["Da\u00df", "dein", "Ge\u00b7dan\u00b7ke", "nicht", "so", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und da\u00df nicht du, nur er, Pasquins Trompete sei.", "tokens": ["Und", "da\u00df", "nicht", "du", ",", "nur", "er", ",", "Pas\u00b7quins", "Trom\u00b7pe\u00b7te", "sei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PTKNEG", "PPER", "$,", "ADV", "PPER", "$,", "NN", "NN", "VAFIN", "$."], "meter": "-+-----+-+-+", "measure": "dactylic.init"}, "line.22": {"text": "Das Ro\u00df von Troja brachte nur Verderben", "tokens": ["Das", "Ro\u00df", "von", "Tro\u00b7ja", "brach\u00b7te", "nur", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Und Unheil in die Stadt: entfernt von List,", "tokens": ["Und", "Un\u00b7heil", "in", "die", "Stadt", ":", "ent\u00b7fernt", "von", "List", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$.", "ADJD", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Kam Pius nur, Theresens Erben", "tokens": ["Kam", "Pius", "nur", ",", "The\u00b7re\u00b7sens", "Er\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "ADV", "$,", "NE", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Zu segnen, ihn, der vieler V\u00f6lker Vater ist.", "tokens": ["Zu", "seg\u00b7nen", ",", "ihn", ",", "der", "vie\u00b7ler", "V\u00f6l\u00b7ker", "Va\u00b7ter", "ist", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PPER", "$,", "PRELS", "PIAT", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der dir, Verfolger Christ! zur Strafe", "tokens": ["Der", "dir", ",", "Ver\u00b7fol\u00b7ger", "Christ", "!", "zur", "Stra\u00b7fe"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "$,", "ADJA", "NN", "$.", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Die Juden wieder Menschen werden hie\u00df,", "tokens": ["Die", "Ju\u00b7den", "wie\u00b7der", "Men\u00b7schen", "wer\u00b7den", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Der seiner Kirche Sohn, nicht Sklave,", "tokens": ["Der", "sei\u00b7ner", "Kir\u00b7che", "Sohn", ",", "nicht", "Skla\u00b7ve", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "$,", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Betrog'ne M\u00e4dchen aus dem Kerker ri\u00df,", "tokens": ["Be\u00b7tro\u00b7g'\u00b7ne", "M\u00e4d\u00b7chen", "aus", "dem", "Ker\u00b7ker", "ri\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Und M\u00f6nche, f\u00fcr die Welt verloren,", "tokens": ["Und", "M\u00f6n\u00b7che", ",", "f\u00fcr", "die", "Welt", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Zu B\u00fcrgern machte, halb der Menschheit Grab", "tokens": ["Zu", "B\u00fcr\u00b7gern", "mach\u00b7te", ",", "halb", "der", "Menschheit", "Grab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "ADJD", "ART", "NN", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.32": {"text": "Vertilgte, B\u00f6hmens Volke, freigeboren", "tokens": ["Ver\u00b7tilg\u00b7te", ",", "B\u00f6h\u00b7mens", "Vol\u00b7ke", ",", "frei\u00b7ge\u00b7bo\u00b7ren"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "NE", "NN", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Wie seine stolzen Herr'n, die Freiheit wieder gab;", "tokens": ["Wie", "sei\u00b7ne", "stol\u00b7zen", "Herr'n", ",", "die", "Frei\u00b7heit", "wie\u00b7der", "gab", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der, wie einst Gott, die Menschen nach dem Werke,", "tokens": ["Der", ",", "wie", "einst", "Gott", ",", "die", "Men\u00b7schen", "nach", "dem", "Wer\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ADV", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Nicht nach dem Glauben mi\u00dft;", "tokens": ["Nicht", "nach", "dem", "Glau\u00b7ben", "mi\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.36": {"text": "Den segnen, der dies that, und seiner Reiche St\u00e4rke", "tokens": ["Den", "seg\u00b7nen", ",", "der", "dies", "that", ",", "und", "sei\u00b7ner", "Rei\u00b7che", "St\u00e4r\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PRELS", "PDS", "VVFIN", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Als F\u00fcrst bewundern, und als F\u00fcrst und Christ,", "tokens": ["Als", "F\u00fcrst", "be\u00b7wun\u00b7dern", ",", "und", "als", "F\u00fcrst", "und", "Christ", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$,", "KON", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Ihn und sein Volk beneiden, so kam Pius, kehrte", "tokens": ["Ihn", "und", "sein", "Volk", "be\u00b7nei\u00b7den", ",", "so", "kam", "Pius", ",", "kehr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "KON", "PPOSAT", "NN", "VVINF", "$,", "ADV", "VVFIN", "NE", "$,", "VVFIN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Auch so nach Rom zur\u00fcck und lehrte", "tokens": ["Auch", "so", "nach", "Rom", "zu\u00b7r\u00fcck", "und", "lehr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NE", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Selbst Schwache nun, da\u00df Rom f\u00fcr einen Staat,", "tokens": ["Selbst", "Schwa\u00b7che", "nun", ",", "da\u00df", "Rom", "f\u00fcr", "ei\u00b7nen", "Staat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADV", "$,", "KOUS", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Wo Nestor Kaunitz wacht, nichts f\u00fcrchterliches hat.", "tokens": ["Wo", "Nes\u00b7tor", "Kau\u00b7nitz", "wacht", ",", "nichts", "f\u00fcrch\u00b7ter\u00b7li\u00b7ches", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NE", "VVFIN", "$,", "PIS", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Der P\u00f6bel, welcher rasend ihm entgegen eilte,", "tokens": ["Der", "P\u00f6\u00b7bel", ",", "wel\u00b7cher", "ra\u00b7send", "ihm", "ent\u00b7ge\u00b7gen", "eil\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PPER", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ist selber Schuld, da\u00df Pius seine Gl\u00e4ubige", "tokens": ["Ist", "sel\u00b7ber", "Schuld", ",", "da\u00df", "Pius", "sei\u00b7ne", "Gl\u00e4u\u00b7bi\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "NN", "$,", "KOUS", "NE", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.44": {"text": "Mit Segen und vollkommnem Abla\u00df t\u00f6dtete,", "tokens": ["Mit", "Se\u00b7gen", "und", "voll\u00b7komm\u00b7nem", "Ab\u00b7la\u00df", "t\u00f6d\u00b7te\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Da der Apostel einst auch ohne Abla\u00df heilte,", "tokens": ["Da", "der", "A\u00b7pos\u00b7tel", "einst", "auch", "oh\u00b7ne", "Ab\u00b7la\u00df", "heil\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "wor