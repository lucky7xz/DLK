{"dta.poem.1161": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "47. Die Sch\u00f6ne Blasse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mein gantz entf\u00e4rbter Schatz/ mein mattes Liljen-Kind/ ", "tokens": ["Mein", "gantz", "ent\u00b7f\u00e4rb\u00b7ter", "Schatz", "/", "mein", "mat\u00b7tes", "Lil\u00b7jen\u00b7Kind", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das mit Zinober vor und Purpur hat geprahlet/", "tokens": ["Das", "mit", "Zi\u00b7no\u00b7ber", "vor", "und", "Pur\u00b7pur", "hat", "ge\u00b7prah\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "PTKVZ", "KON", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Izt auff der Wangen Au/ die man erstorben findt/", "tokens": ["Izt", "auff", "der", "Wan\u00b7gen", "Au", "/", "die", "man", "ers\u00b7tor\u00b7ben", "findt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$(", "PRELS", "PIS", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die bange Todten-Farb\u2019 in blasse Bl\u00e4tter mahlet/", "tokens": ["Die", "ban\u00b7ge", "Tod\u00b7ten\u00b7Fa\u00b7rb'", "in", "blas\u00b7se", "Bl\u00e4t\u00b7ter", "mah\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Vor hat mich deine R\u00f6th\u2019 in hei\u00dfer Glutt entz\u00fcndt/", "tokens": ["Vor", "hat", "mich", "dei\u00b7ne", "R\u00f6\u00b7th'", "in", "hei\u00b7\u00dfer", "Glutt", "ent\u00b7z\u00fcndt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Da Schmertz und Beyleid izt aus deiner Bleiche strahlet;", "tokens": ["Da", "Schmertz", "und", "Bey\u00b7leid", "izt", "aus", "dei\u00b7ner", "Blei\u00b7che", "strah\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gleich wie mich Sonn und Gold vorhin mit dir verbindt/", "tokens": ["Gleich", "wie", "mich", "Sonn", "und", "Gold", "vor\u00b7hin", "mit", "dir", "ver\u00b7bindt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "NN", "KON", "NN", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So h\u00e4lt mich izt dein Bley noch fester angepfahlet.", "tokens": ["So", "h\u00e4lt", "mich", "izt", "dein", "Bley", "noch", "fes\u00b7ter", "an\u00b7ge\u00b7pfah\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sieht man die Funcken nicht aus deinem Antlitz schimmern/", "tokens": ["Sieht", "man", "die", "Fun\u00b7cken", "nicht", "aus", "dei\u00b7nem", "Ant\u00b7litz", "schim\u00b7mern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So treff ich doch bey dir die alten Kohlen an/", "tokens": ["So", "treff", "ich", "doch", "bey", "dir", "die", "al\u00b7ten", "Koh\u00b7len", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die mit verborgner Glutt in bleicher Asche glimmern.", "tokens": ["Die", "mit", "ver\u00b7borg\u00b7ner", "Glutt", "in", "blei\u00b7cher", "A\u00b7sche", "glim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du bist es/ die der Welt ein Vorbild zeigen kan/", "tokens": ["Du", "bist", "es", "/", "die", "der", "Welt", "ein", "Vor\u00b7bild", "zei\u00b7gen", "kan", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$(", "PRELS", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie man doch auch den Tag bey tr\u00fcber Zeit erkennet/", "tokens": ["Wie", "man", "doch", "auch", "den", "Tag", "bey", "tr\u00fc\u00b7ber", "Zeit", "er\u00b7ken\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bey blassem Schatten auch die Sonne schw\u00e4rzt und brennet.", "tokens": ["Bey", "blas\u00b7sem", "Schat\u00b7ten", "auch", "die", "Son\u00b7ne", "schw\u00e4rzt", "und", "bren\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}