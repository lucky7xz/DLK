{"dta.poem.4036": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fcckkehr in die Heimath .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.85", "nl:0.14"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr milden L\u00fcfte, Boten Italiens!", "tokens": ["Ihr", "mil\u00b7den", "L\u00fcf\u00b7te", ",", "Bo\u00b7ten", "I\u00b7ta\u00b7li\u00b7ens", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "NN", "NE", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und du mit deinen Pappeln, geliebter Strom!", "tokens": ["Und", "du", "mit", "dei\u00b7nen", "Pap\u00b7peln", ",", "ge\u00b7lieb\u00b7ter", "Strom", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihr wogenden Gebirg'! o all' ihr", "tokens": ["Ihr", "wo\u00b7gen\u00b7den", "Ge\u00b7bir\u00b7g'", "!", "o", "all'", "ihr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "FM", "PIAT", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sonnigen Gipfel! so seyd ihr's wieder.", "tokens": ["Son\u00b7ni\u00b7gen", "Gip\u00b7fel", "!", "so", "seyd", "ih\u00b7r's", "wie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Du stiller Ort! in Tr\u00e4umen erschienst du fern,", "tokens": ["Du", "stil\u00b7ler", "Ort", "!", "in", "Tr\u00e4u\u00b7men", "er\u00b7schienst", "du", "fern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "APPR", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nach hoffnungslosem Tage dem Sehnenden,", "tokens": ["Nach", "hoff\u00b7nungs\u00b7lo\u00b7sem", "Ta\u00b7ge", "dem", "Seh\u00b7nen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und du, mein Haus, und ihr Gespielen,", "tokens": ["Und", "du", ",", "mein", "Haus", ",", "und", "ihr", "Ge\u00b7spie\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "B\u00e4ume des H\u00fcgels, ihr wohlbekannten!", "tokens": ["B\u00e4u\u00b7me", "des", "H\u00fc\u00b7gels", ",", "ihr", "wohl\u00b7be\u00b7kann\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wie lang' ist's, o wie lange! des Kindes Ruh'", "tokens": ["Wie", "lang'", "ist's", ",", "o", "wie", "lan\u00b7ge", "!", "des", "Kin\u00b7des", "Ruh'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "$,", "FM", "KOKOM", "ADV", "$.", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ist hin, und hin ist Jugend und Lieb' und Gl\u00fcck,", "tokens": ["Ist", "hin", ",", "und", "hin", "ist", "Ju\u00b7gend", "und", "Lieb'", "und", "Gl\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "KON", "ADV", "VAFIN", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch du, mein Vaterland, du Heilig-", "tokens": ["Doch", "du", ",", "mein", "Va\u00b7ter\u00b7land", ",", "du", "Hei\u00b7lig"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Duldendes, siehe, du bist geblieben!", "tokens": ["Dul\u00b7den\u00b7des", ",", "sie\u00b7he", ",", "du", "bist", "ge\u00b7blie\u00b7ben", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und darum, da\u00df sie dulden mit dir, mit dir", "tokens": ["Und", "da\u00b7rum", ",", "da\u00df", "sie", "dul\u00b7den", "mit", "dir", ",", "mit", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PAV", "$,", "KOUS", "PPER", "VVINF", "APPR", "PPER", "$,", "APPR", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sich freu'n, erziehst du, Theures! die Deinen auch,", "tokens": ["Sich", "freu'n", ",", "er\u00b7ziehst", "du", ",", "Theu\u00b7res", "!", "die", "Dei\u00b7nen", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$,", "VVFIN", "PPER", "$,", "NE", "$.", "ART", "PPOSAT", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und mahnst in Tr\u00e4umen, wenn sie ferne", "tokens": ["Und", "mahnst", "in", "Tr\u00e4u\u00b7men", ",", "wenn", "sie", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "$,", "KOUS", "PPER", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schweifen und irren, die Ungetreuen.", "tokens": ["Schwei\u00b7fen", "und", "ir\u00b7ren", ",", "die", "Un\u00b7ge\u00b7treu\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$,", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}