{"dta.poem.1118": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "4. Die Sch\u00f6ne mit hohem R\u00fccken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ists? Verirrte sich die kluge Meister-Hand? ", "tokens": ["Wie", "ists", "?", "Ver\u00b7irr\u00b7te", "sich", "die", "klu\u00b7ge", "Meis\u00b7ter\u00b7Hand", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$.", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schlieff irgend die Natur/ die alles pr\u00e4gt und heget/", "tokens": ["Schlieff", "ir\u00b7gend", "die", "Na\u00b7tur", "/", "die", "al\u00b7les", "pr\u00e4gt", "und", "he\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$(", "ART", "PIS", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ward sie durch Zorn/ ward sie durch Eyfersucht beweget/", "tokens": ["Ward", "sie", "durch", "Zorn", "/", "ward", "sie", "durch", "Ey\u00b7fer\u00b7sucht", "be\u00b7we\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "$(", "VAFIN", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sie dich ohne Schuld versezt in solchem Stand?", "tokens": ["Da\u00df", "sie", "dich", "oh\u00b7ne", "Schuld", "ver\u00b7sezt", "in", "sol\u00b7chem", "Stand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVPP", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als sie dein Angesicht gantz \u00fcberirrdisch fand/", "tokens": ["Als", "sie", "dein", "An\u00b7ge\u00b7sicht", "gantz", "\u00fc\u00b7be\u00b7rirr\u00b7disch", "fand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So hat sie eine Last dem R\u00fccken auffgeleget:", "tokens": ["So", "hat", "sie", "ei\u00b7ne", "Last", "dem", "R\u00fc\u00b7cken", "auff\u00b7ge\u00b7le\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Weil deiner Augen Paar des Himmels Feuer tr\u00e4get/", "tokens": ["Weil", "dei\u00b7ner", "Au\u00b7gen", "Paar", "des", "Him\u00b7mels", "Feu\u00b7er", "tr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So zeigt das Hintertheil der Erde finstres Land.", "tokens": ["So", "zeigt", "das", "Hin\u00b7ter\u00b7theil", "der", "Er\u00b7de", "finst\u00b7res", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach ja/ sie irrte sich durch des Verh\u00e4ngni\u00df Trieb:", "tokens": ["Ach", "ja", "/", "sie", "irr\u00b7te", "sich", "durch", "des", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "Trieb", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$(", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denn sie die Liebe nackt und fliegend bilden wolte/", "tokens": ["Denn", "sie", "die", "Lie\u00b7be", "nackt", "und", "flie\u00b7gend", "bil\u00b7den", "wol\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ADJD", "KON", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nur da\u00df ihr zu viel Stoff zu Fl\u00fcgeln \u00fcberblieb:", "tokens": ["Nur", "da\u00df", "ihr", "zu", "viel", "Stoff", "zu", "Fl\u00fc\u00b7geln", "\u00fc\u00b7berb\u00b7lieb", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "PIAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und/ hat sie nicht gefehlt/ so sag\u2019 ich/ da\u00df es solte", "tokens": ["Und", "/", "hat", "sie", "nicht", "ge\u00b7fehlt", "/", "so", "sag'", "ich", "/", "da\u00df", "es", "sol\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$(", "VAFIN", "PPER", "PTKNEG", "VVPP", "$(", "ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Des Atlas Bildnis seyn/ den M\u00e4nnern darzuthun:", "tokens": ["Des", "At\u00b7las", "Bild\u00b7nis", "seyn", "/", "den", "M\u00e4n\u00b7nern", "dar\u00b7zu\u00b7thun", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAINF", "$(", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df auch der Welt-Krei\u00df k\u00f6nnt auff einer Frauen ruhn.", "tokens": ["Da\u00df", "auch", "der", "Welt\u00b7Krei\u00df", "k\u00f6nnt", "auff", "ei\u00b7ner", "Frau\u00b7en", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}