{"textgrid.poem.43014": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "2. Ein heil'ges Fest", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O dieser Tag der h\u00f6chsten Feier,", "tokens": ["O", "die\u00b7ser", "Tag", "der", "h\u00f6chs\u00b7ten", "Fei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der mir das Herz im Busen bricht;", "tokens": ["Der", "mir", "das", "Herz", "im", "Bu\u00b7sen", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der h\u00f6hnend durch der Zukunft Schleier", "tokens": ["Der", "h\u00f6h\u00b7nend", "durch", "der", "Zu\u00b7kunft", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir zeigt des Schmerzes Angesicht!", "tokens": ["Mir", "zeigt", "des", "Schmer\u00b7zes", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Schmerz, der nicht in leichtem Beben,", "tokens": ["Ein", "Schmerz", ",", "der", "nicht", "in", "leich\u00b7tem", "Be\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In fl\u00fcchtigem Vor\u00fcberschweben,", "tokens": ["In", "fl\u00fcch\u00b7ti\u00b7gem", "Vor\u00b7\u00fc\u00b7bersc\u00b7hwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die schwarze Trauerfahne tr\u00e4gt; \u2013", "tokens": ["Die", "schwar\u00b7ze", "Trau\u00b7er\u00b7fah\u00b7ne", "tr\u00e4gt", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nein, der ein ganzes, reiches Leben", "tokens": ["Nein", ",", "der", "ein", "gan\u00b7zes", ",", "rei\u00b7ches", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "PRELS", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mit schonungsloser Hand zerschl\u00e4gt!", "tokens": ["Mit", "scho\u00b7nungs\u00b7lo\u00b7ser", "Hand", "zer\u00b7schl\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht ahnt's der Kranz in meinen Locken,", "tokens": ["Nicht", "ahnt's", "der", "Kranz", "in", "mei\u00b7nen", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich dem Tode angetraut;", "tokens": ["Da\u00df", "ich", "dem", "To\u00b7de", "an\u00b7ge\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht ahnen es die Kirchenglocken,", "tokens": ["Nicht", "ah\u00b7nen", "es", "die", "Kir\u00b7chen\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu l\u00e4uten einer Grabesbraut! \u2013", "tokens": ["Zu", "l\u00e4u\u00b7ten", "ei\u00b7ner", "Gra\u00b7bes\u00b7braut", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Umsonst mit euern milden T\u00f6nen", "tokens": ["Um\u00b7sonst", "mit", "eu\u00b7ern", "mil\u00b7den", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wollt ihr dem Leben mich vers\u00f6hnen;", "tokens": ["Wollt", "ihr", "dem", "Le\u00b7ben", "mich", "ver\u00b7s\u00f6h\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mich lockt kein festlich heit'rer Klang!", "tokens": ["Mich", "lockt", "kein", "fest\u00b7lich", "heit'\u00b7rer", "Klang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nur meinen Schmerz kann er verh\u00f6hnen;", "tokens": ["Nur", "mei\u00b7nen", "Schmerz", "kann", "er", "ver\u00b7h\u00f6h\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nur feiern meinen Untergang!", "tokens": ["Nur", "fei\u00b7ern", "mei\u00b7nen", "Un\u00b7ter\u00b7gang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Verkauft ein ganzes reiches Leben,", "tokens": ["Ver\u00b7kauft", "ein", "gan\u00b7zes", "rei\u00b7ches", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das seines Werths sich kaum bewu\u00dft,", "tokens": ["Das", "sei\u00b7nes", "Werths", "sich", "kaum", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Tr\u00e4umen, die das Herz durchbeben,", "tokens": ["Mit", "Tr\u00e4u\u00b7men", ",", "die", "das", "Herz", "durch\u00b7be\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In wilder ahnungsvoller Lust!", "tokens": ["In", "wil\u00b7der", "ah\u00b7nungs\u00b7vol\u00b7ler", "Lust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein gl\u00fchend Schwelgen, s\u00fc\u00dfes Bangen,", "tokens": ["Ein", "gl\u00fc\u00b7hend", "Schwel\u00b7gen", ",", "s\u00fc\u00b7\u00dfes", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein fiebrisch zitterndes Verlangen,", "tokens": ["Ein", "fie\u00b7brisch", "zit\u00b7tern\u00b7des", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das um das Gl\u00fcck gebiet'risch fleht,", "tokens": ["Das", "um", "das", "Gl\u00fcck", "ge\u00b7biet'\u00b7risch", "fleht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bis von dem kalten Tod umfangen", "tokens": ["Bis", "von", "dem", "kal\u00b7ten", "Tod", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Das Leben und der Traum verweht!", "tokens": ["Das", "Le\u00b7ben", "und", "der", "Traum", "ver\u00b7weht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du Herr der Welt, du Lebensw\u00fcrger,", "tokens": ["Du", "Herr", "der", "Welt", ",", "du", "Le\u00b7bens\u00b7w\u00fcr\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O falsches, glei\u00dfendes Metall!", "tokens": ["O", "fal\u00b7sches", ",", "glei\u00b7\u00dfen\u00b7des", "Me\u00b7tall", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verlockst du selbst des Himmels B\u00fcrger,", "tokens": ["Ver\u00b7lockst", "du", "selbst", "des", "Him\u00b7mels", "B\u00fcr\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den stolzen ", "tokens": ["Den", "stol\u00b7zen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Die sich nach ew'gen Himmeln sehnen,", "tokens": ["Die", "sich", "nach", "ew'\u00b7gen", "Him\u00b7meln", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die k\u00fchn sich unverg\u00e4nglich w\u00e4hnen,", "tokens": ["Die", "k\u00fchn", "sich", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "w\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Verkaufen dir ein ew'ges Sein.", "tokens": ["Ver\u00b7kau\u00b7fen", "dir", "ein", "ew'\u00b7ges", "Sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Erl\u00f6scht, ihr Kerzen am Altare!", "tokens": ["Er\u00b7l\u00f6scht", ",", "ihr", "Ker\u00b7zen", "am", "Al\u00b7ta\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erl\u00f6scht, wie meiner Seele Licht!", "tokens": ["Er\u00b7l\u00f6scht", ",", "wie", "mei\u00b7ner", "See\u00b7le", "Licht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Brautbett wird zur Todtenbahre,", "tokens": ["Das", "Braut\u00b7bett", "wird", "zur", "Tod\u00b7ten\u00b7bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um die man Grabeskr\u00e4nze flicht.", "tokens": ["Um", "die", "man", "Gra\u00b7bes\u00b7kr\u00e4n\u00b7ze", "flicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es tritt auf allen meinen Wegen", "tokens": ["Es", "tritt", "auf", "al\u00b7len", "mei\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verzweiflung spottend mir entgegen,", "tokens": ["Ver\u00b7zwei\u00b7flung", "spot\u00b7tend", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit irrem Blick, mit wildem Haar;", "tokens": ["Mit", "ir\u00b7rem", "Blick", ",", "mit", "wil\u00b7dem", "Haar", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verzweiflung sprach den Hochzeitsegen,", "tokens": ["Ver\u00b7zwei\u00b7flung", "sprach", "den", "Hoch\u00b7zeit\u00b7se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sprach ihren Fluch am Traualtar!", "tokens": ["Sprach", "ih\u00b7ren", "Fluch", "am", "Trau\u00b7al\u00b7tar", "!"], "token_info": ["word", 