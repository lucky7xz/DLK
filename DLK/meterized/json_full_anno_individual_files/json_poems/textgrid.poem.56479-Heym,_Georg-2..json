word", "word", "word"], "pos": ["ART", "NN", "NE", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "K\u00fc\u00dft ihrer Reiherschwingen langer Zug.", "tokens": ["K\u00fc\u00dft", "ih\u00b7rer", "Rei\u00b7her\u00b7schwin\u00b7gen", "lan\u00b7ger", "Zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Die Silberst\u00e4dte, die im Monde gl\u00fchen,", "tokens": ["Die", "Sil\u00b7ber\u00b7st\u00e4d\u00b7te", ",", "die", "im", "Mon\u00b7de", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umarmen sie mit ihres Sommers Pracht,", "tokens": ["Um\u00b7ar\u00b7men", "sie", "mit", "ih\u00b7res", "Som\u00b7mers", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo schon im Ost wie gro\u00dfe Rosen bl\u00fchen", "tokens": ["Wo", "schon", "im", "Ost", "wie", "gro\u00b7\u00dfe", "Ro\u00b7sen", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "NN", "KOKOM", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Morgenr\u00f6ten in die Mitternacht.", "tokens": ["Die", "Mor\u00b7gen\u00b7r\u00f6\u00b7ten", "in", "die", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Der gro\u00dfen St\u00e4dte 