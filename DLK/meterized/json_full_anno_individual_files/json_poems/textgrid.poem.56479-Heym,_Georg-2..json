{"textgrid.poem.56479": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der gro\u00dfen St\u00e4dte n\u00e4chtliche Emporen", "tokens": ["Der", "gro\u00b7\u00dfen", "St\u00e4d\u00b7te", "n\u00e4cht\u00b7li\u00b7che", "Em\u00b7po\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stehn rings am Rand, wie gelbe Br\u00e4nde weit.", "tokens": ["Stehn", "rings", "am", "Rand", ",", "wie", "gel\u00b7be", "Br\u00e4n\u00b7de", "weit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$,", "PWAV", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und mit der Fackel scheucht aus ihren Toren", "tokens": ["Und", "mit", "der", "Fa\u00b7ckel", "scheucht", "aus", "ih\u00b7ren", "To\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Tod die Toten in die Dunkelheit.", "tokens": ["Der", "Tod", "die", "To\u00b7ten", "in", "die", "Dun\u00b7kel\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie fahren aus wie gro\u00dfer Rauch und schwirren", "tokens": ["Sie", "fah\u00b7ren", "aus", "wie", "gro\u00b7\u00dfer", "Rauch", "und", "schwir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KOKOM", "ADJA", "NN", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit leisen Klagen durch das Distelfeld.", "tokens": ["Mit", "lei\u00b7sen", "Kla\u00b7gen", "durch", "das", "Dis\u00b7tel\u00b7feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Am Kreuzweg hocken sie zuhauf und irren", "tokens": ["Am", "Kreuz\u00b7weg", "ho\u00b7cken", "sie", "zu\u00b7hauf", "und", "ir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Heimatlosen gleich in schwarzer Welt.", "tokens": ["Den", "Hei\u00b7mat\u00b7lo\u00b7sen", "gleich", "in", "schwar\u00b7zer", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie schaun zur\u00fcck von einem kahlen Baume,", "tokens": ["Sie", "schaun", "zu\u00b7r\u00fcck", "von", "ei\u00b7nem", "kah\u00b7len", "Bau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf den der Wind sie warf. Doch ihre Stadt", "tokens": ["Auf", "den", "der", "Wind", "sie", "warf", ".", "Doch", "ih\u00b7re", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "PPER", "VVFIN", "$.", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist zu f\u00fcr sie. Und in dem leeren Raume", "tokens": ["Ist", "zu", "f\u00fcr", "sie", ".", "Und", "in", "dem", "lee\u00b7ren", "Rau\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "APPR", "PPER", "$.", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Treibt Sturm sie um den Baum, wie V\u00f6gel matt.", "tokens": ["Treibt", "Sturm", "sie", "um", "den", "Baum", ",", "wie", "V\u00f6\u00b7gel", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "APPR", "ART", "NN", "$,", "PWAV", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wo ist die Totenstadt? Sie wollen schlafen.", "tokens": ["Wo", "ist", "die", "To\u00b7ten\u00b7stadt", "?", "Sie", "wol\u00b7len", "schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$.", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da tut sich auf im ernsten Abendrot", "tokens": ["Da", "tut", "sich", "auf", "im", "erns\u00b7ten", "A\u00b7ben\u00b7drot"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Unterwelt, der stillen St\u00e4dte Hafen,", "tokens": ["Die", "Un\u00b7ter\u00b7welt", ",", "der", "stil\u00b7len", "St\u00e4d\u00b7te", "Ha\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo schwarze Segel ziehen, Boot an Boot.", "tokens": ["Wo", "schwar\u00b7ze", "Se\u00b7gel", "zie\u00b7hen", ",", "Boot", "an", "Boot", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$,", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und schwarze Fahnen wehn die langen Gassen", "tokens": ["Und", "schwar\u00b7ze", "Fah\u00b7nen", "wehn", "die", "lan\u00b7gen", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der ausgestorbnen St\u00e4dte, die verstummt", "tokens": ["Der", "aus\u00b7ge\u00b7storb\u00b7nen", "St\u00e4d\u00b7te", ",", "die", "ver\u00b7stummt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Fluch von wei\u00dfen Himmeln und verlassen,", "tokens": ["Im", "Fluch", "von", "wei\u00b7\u00dfen", "Him\u00b7meln", "und", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo ewig eine stumpfe Glocke brummt.", "tokens": ["Wo", "e\u00b7wig", "ei\u00b7ne", "stump\u00b7fe", "Glo\u00b7cke", "brummt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die schwarzen Br\u00fccken werfen ungeheuer", "tokens": ["Die", "schwar\u00b7zen", "Br\u00fc\u00b7cken", "wer\u00b7fen", "un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Abendschatten auf den dunklen Strom.", "tokens": ["Die", "A\u00b7bend\u00b7schat\u00b7ten", "auf", "den", "dunk\u00b7len", "Strom", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und riesiger Lagunen rotes Feuer", "tokens": ["Und", "rie\u00b7si\u00b7ger", "La\u00b7gu\u00b7nen", "ro\u00b7tes", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verbrennt die Luft mit purpurnem Arom.", "tokens": ["Ver\u00b7brennt", "die", "Luft", "mit", "pur\u00b7pur\u00b7nem", "A\u00b7rom", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Kan\u00e4le alle, die die Stadt durchschwimmen,", "tokens": ["Ka\u00b7n\u00e4\u00b7le", "al\u00b7le", ",", "die", "die", "Stadt", "durch\u00b7schwim\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind von den Lilienw\u00e4ldern sanft ums\u00e4umt.", "tokens": ["Sind", "von", "den", "Li\u00b7li\u00b7en\u00b7w\u00e4l\u00b7dern", "sanft", "um\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Am Bug der K\u00e4hne, wo die Lampen glimmen,", "tokens": ["Am", "Bug", "der", "K\u00e4h\u00b7ne", ",", "wo", "die", "Lam\u00b7pen", "glim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stehn gro\u00df die Schiffer, und der Abend tr\u00e4umt", "tokens": ["Stehn", "gro\u00df", "die", "Schif\u00b7fer", ",", "und", "der", "A\u00b7bend", "tr\u00e4umt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wie zarte goldene Kronen um die Stirnen.", "tokens": ["Wie", "zar\u00b7te", "gol\u00b7de\u00b7ne", "Kro\u00b7nen", "um", "die", "Stir\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der tiefen Augen dunkler Edelstein", "tokens": ["Der", "tie\u00b7fen", "Au\u00b7gen", "dunk\u00b7ler", "E\u00b7del\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Umschlie\u00dft des hohen Himmels blasse Firnen,", "tokens": ["Um\u00b7schlie\u00dft", "des", "ho\u00b7hen", "Him\u00b7mels", "blas\u00b7se", "Fir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo weidet schon der Mond im gr\u00fcnen Schein.", "tokens": ["Wo", "wei\u00b7det", "schon", "der", "Mond", "im", "gr\u00fc\u00b7nen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Die Toten schaun aus ihrem Winterbaume", "tokens": ["Die", "To\u00b7ten", "schaun", "aus", "ih\u00b7rem", "Win\u00b7ter\u00b7bau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Schl\u00e4fern zu in ihrem sanften Reich.", "tokens": ["Den", "Schl\u00e4\u00b7fern", "zu", "in", "ih\u00b7rem", "sanf\u00b7ten", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und das Verlangen fa\u00dft sie nach dem Saume", "tokens": ["Und", "das", "Ver\u00b7lan\u00b7gen", "fa\u00dft", "sie", "nach", "dem", "Sau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des roten Himmels und dem Abend weich.", "tokens": ["Des", "ro\u00b7ten", "Him\u00b7mels", "und", "dem", "A\u00b7bend", "weich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Da st\u00fcrzt sie Hermes, der die Nacht ersch\u00fcttert", "tokens": ["Da", "st\u00fcrzt", "sie", "Her\u00b7mes", ",", "der", "die", "Nacht", "er\u00b7sch\u00fct\u00b7tert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE", "$,", "PRELS", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit starkem Flug, ein bl\u00e4ulicher Komet,", "tokens": ["