{"textgrid.poem.43809": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Honigkuchenmann", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Keine Puppe will ich haben \u2013", "tokens": ["Kei\u00b7ne", "Pup\u00b7pe", "will", "ich", "ha\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VAFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Puppen geh'n mich gar nichts an.", "tokens": ["Pup\u00b7pen", "geh'n", "mich", "gar", "nichts", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was erfreu'n mich kann und laben,", "tokens": ["Was", "er\u00b7freu'n", "mich", "kann", "und", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "VMFIN", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ein Honigkuchenmann,", "tokens": ["Ist", "ein", "Ho\u00b7nig\u00b7ku\u00b7chen\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So ein Mann mit Leib und Kleid,", "tokens": ["So", "ein", "Mann", "mit", "Leib", "und", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch und durch von S\u00fc\u00dfigkeit.", "tokens": ["Durch", "und", "durch", "von", "S\u00fc\u00b7\u00dfig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "APPR", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Stattlicher als eine Puppe", "tokens": ["Statt\u00b7li\u00b7cher", "als", "ei\u00b7ne", "Pup\u00b7pe"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht ein Honigkerl sich an,", "tokens": ["Sieht", "ein", "Ho\u00b7nig\u00b7kerl", "sich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine ganze Puppengruppe", "tokens": ["Ei\u00b7ne", "gan\u00b7ze", "Pup\u00b7pen\u00b7grup\u00b7pe"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mich nicht so erfreuen kann.", "tokens": ["Mich", "nicht", "so", "er\u00b7freu\u00b7en", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber seh' ich recht dich an,", "tokens": ["A\u00b7ber", "seh'", "ich", "recht", "dich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dauerst du mich, lieber Mann.", "tokens": ["Dau\u00b7erst", "du", "mich", ",", "lie\u00b7ber", "Mann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "PRF", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn du bist zum Tod erkoren \u2013", "tokens": ["Denn", "du", "bist", "zum", "Tod", "er\u00b7ko\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPRART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bin ich dir auch noch so gut,", "tokens": ["Bin", "ich", "dir", "auch", "noch", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob du hast ein Bein verloren,", "tokens": ["Ob", "du", "hast", "ein", "Bein", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob das andre weh dir thut:", "tokens": ["Ob", "das", "and\u00b7re", "weh", "dir", "thut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Armer Honigkuchenmann,", "tokens": ["Ar\u00b7mer", "Ho\u00b7nig\u00b7ku\u00b7chen\u00b7mann", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hilft dir nichts, du mu\u00dft doch dran!", "tokens": ["Hilft", "dir", "nichts", ",", "du", "mu\u00dft", "doch", "dran", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "PPER", "VMFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Keine Puppe will ich haben \u2013", "tokens": ["Kei\u00b7ne", "Pup\u00b7pe", "will", "ich", "ha\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMF