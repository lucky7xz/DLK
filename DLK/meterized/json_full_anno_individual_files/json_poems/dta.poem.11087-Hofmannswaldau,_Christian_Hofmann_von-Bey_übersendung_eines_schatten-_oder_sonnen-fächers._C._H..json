{"dta.poem.11087": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Bey \u00fcbersendung eines schatten-  \n oder sonnen-f\u00e4chers.  \n  C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den selbst die sehnsucht fast zum schatten hat gemacht,", "tokens": ["Den", "selbst", "die", "sehn\u00b7sucht", "fast", "zum", "schat\u00b7ten", "hat", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "VVFIN", "ADV", "APPRART", "ADJA", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der \u00fcbersendet dir hier einen sonnen-schatten.", "tokens": ["Der", "\u00fc\u00b7ber\u00b7sen\u00b7det", "dir", "hier", "ei\u00b7nen", "son\u00b7nen\u00b7schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie nun beym schatten licht, und bey dem tage nacht;", "tokens": ["Wie", "nun", "beym", "schat\u00b7ten", "licht", ",", "und", "bey", "dem", "ta\u00b7ge", "nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "ADJA", "NN", "$,", "KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wollst du, sonne! dich mit diesem gleichfalls gatten!", "tokens": ["So", "wollst", "du", ",", "son\u00b7ne", "!", "dich", "mit", "die\u00b7sem", "gleich\u00b7falls", "gat\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "VVFIN", "$.", "PRF", "APPR", "PDAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der schatten folgt dem licht\u2019, ich folge, sonne! dir;", "tokens": ["Der", "schat\u00b7ten", "folgt", "dem", "licht'", ",", "ich", "fol\u00b7ge", ",", "son\u00b7ne", "!", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "$,", "VVFIN", "$.", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch brennen unerh\u00f6rt mich deines lichtes blicke:", "tokens": ["Doch", "bren\u00b7nen", "un\u00b7er\u00b7h\u00f6rt", "mich", "dei\u00b7nes", "lich\u00b7tes", "bli\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "ADJD", "PPER", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "H\u00e4ltst du hinf\u00fcro nun dir diesen schatten f\u00fcr;", "tokens": ["H\u00e4ltst", "du", "hin\u00b7f\u00fc\u00b7ro", "nun", "dir", "die\u00b7sen", "schat\u00b7ten", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PPER", "PDAT", "ADJA", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So find ich bey dem licht im schatten mein gel\u00fccke.", "tokens": ["So", "find", "ich", "bey", "dem", "licht", "im", "schat\u00b7ten", "mein", "ge\u00b7l\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPRART", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der schatten k\u00fchlt den brand, das licht erwecket ihn;", "tokens": ["Der", "schat\u00b7ten", "k\u00fchlt", "den", "brand", ",", "das", "licht", "er\u00b7we\u00b7cket", "ihn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Doch du, o sonne! kanst durch licht und schatten k\u00fchlen.", "tokens": ["Doch", "du", ",", "o", "son\u00b7ne", "!", "kanst", "durch", "licht", "und", "schat\u00b7ten", "k\u00fch\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "FM", "$.", "VMFIN", "APPR", "NN", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wo deine strahlen sich der gegen-gunst hinziehn,", "tokens": ["Wo", "dei\u00b7ne", "strah\u00b7len", "sich", "der", "ge\u00b7gen\u00b7gunst", "hin\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "VVFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und wo du schatten machst, ist fr\u00fchling-lust zu f\u00fchlen.", "tokens": ["Und", "wo", "du", "schat\u00b7ten", "machst", ",", "ist", "fr\u00fch\u00b7ling\u00b7lust", "zu", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "VVFIN", "$,", "VAFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich flehe dich nun auch, mein licht und schatten, an,", "tokens": ["Ich", "fle\u00b7he", "dich", "nun", "auch", ",", "mein", "licht", "und", "schat\u00b7ten", ",", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "PPOSAT", "NN", "KON", "VVFIN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dein licht zertreibe mir das schatten-werck der sinnen,", "tokens": ["Dein", "licht", "zer\u00b7trei\u00b7be", "mir", "das", "schat\u00b7ten\u00b7\u00b7werck", "der", "sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ja alles, was mir sonst die lust verfinstern kan!", "tokens": ["Ja", "al\u00b7les", ",", "was", "mir", "sonst", "die", "lust", "ver\u00b7fins\u00b7tern", "kan", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIS", "$,", "PWS", "PPER", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und in dem schatten la\u00df mich deine gunst gewinnen!", "tokens": ["Und", "in", "dem", "schat\u00b7ten", "la\u00df", "mich", "dei\u00b7ne", "gunst", "ge\u00b7win\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch deine gunst mu\u00df selbst nicht nur ein schatten seyn.", "tokens": ["Doch", "dei\u00b7ne", "gunst", "mu\u00df", "selbst", "nicht", "nur", "ein", "schat\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ADV", "PTKNEG", "ADV", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Soll ich nun meinen zweck und deine gunst erlangen;", "tokens": ["Soll", "ich", "nun", "mei\u00b7nen", "zweck", "und", "dei\u00b7ne", "gunst", "er\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So lasse diesen schlu\u00df, o sonne! mich erfreun:", "tokens": ["So", "las\u00b7se", "die\u00b7sen", "schlu\u00df", ",", "o", "son\u00b7ne", "!", "mich", "er\u00b7freun", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "$,", "FM", "FM", "$.", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df sich dein schatten will mit h\u00e4nden lassen fangen!", "tokens": ["Da\u00df", "sich", "dein", "schat\u00b7ten", "will", "mit", "h\u00e4n\u00b7den", "las\u00b7sen", "fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "VVINF", "VMFIN", "APPR", "ADJA", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}