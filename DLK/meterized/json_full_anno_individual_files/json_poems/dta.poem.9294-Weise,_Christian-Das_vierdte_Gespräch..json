{"dta.poem.9294": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das vierdte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "5. Ach h\u00e4tt\u2019 ich nur an meinem hertzen/", "tokens": ["Ach", "h\u00e4tt'", "ich", "nur", "an", "mei\u00b7nem", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein kleines fenster offen stehn/", "tokens": ["Ein", "klei\u00b7nes", "fens\u00b7ter", "of\u00b7fen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df treu und falschheit/ lieb und schmertzen", "tokens": ["Da\u00df", "treu", "und", "falschheit", "/", "lieb", "und", "schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KON", "NN", "$(", "ADJD", "KON", "VVINF"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Jhr k\u00f6nten unter augen gehn.", "tokens": ["Ihr", "k\u00f6n\u00b7ten", "un\u00b7ter", "au\u00b7gen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die worte werden/ wenn sie kommen", "tokens": ["Die", "wor\u00b7te", "wer\u00b7den", "/", "wenn", "sie", "kom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAINF", "$(", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vor s\u00fcndendeckel aufgenommen.", "tokens": ["Vor", "s\u00fcn\u00b7den\u00b7de\u00b7ckel", "auf\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}