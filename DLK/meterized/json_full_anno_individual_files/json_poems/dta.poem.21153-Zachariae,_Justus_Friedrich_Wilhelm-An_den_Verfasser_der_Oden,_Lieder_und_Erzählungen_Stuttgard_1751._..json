{"dta.poem.21153": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An den Verfasser  \n  der Oden, Lieder und Erz\u00e4hlungen Stuttgard 1751. .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der du mit k\u00fchnem Schwung, gleich einem thraci-\nschen Adler,", "tokens": ["Der", "du", "mit", "k\u00fch\u00b7nem", "Schwung", ",", "gleich", "ei\u00b7nem", "thra\u00b7ci", "schen", "Ad\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "$,", "ADV", "ART", "TRUNC", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Fern von gemeinen H\u00f6hn der sklavischen S\u00e4nger dich", "tokens": ["Fern", "von", "ge\u00b7mei\u00b7nen", "H\u00f6hn", "der", "skla\u00b7vi\u00b7schen", "S\u00e4n\u00b7ger", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "PPER"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "O Freund, verachte den Schwarm, der niedre Ketten", "tokens": ["O", "Freund", ",", "ver\u00b7ach\u00b7te", "den", "Schwarm", ",", "der", "nied\u00b7re", "Ket\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Womit das Vorurtheil ihn angeschmiedet hat.", "tokens": ["Wo\u00b7mit", "das", "Vor\u00b7urt\u00b7heil", "ihn", "an\u00b7ge\u00b7schmie\u00b7det", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Umsonst beneidet er des S\u00e4ngers muthige Frey-", "tokens": ["Um\u00b7sonst", "be\u00b7nei\u00b7det", "er", "des", "S\u00e4n\u00b7gers", "mut\u00b7hi\u00b7ge", "Frey"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Der nie das Laster schont, wenn es auch Purpur beklei-", "tokens": ["Der", "nie", "das", "Las\u00b7ter", "schont", ",", "wenn", "es", "auch", "Pur\u00b7pur", "be\u00b7klei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ADV", "NN", "TRUNC"], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Poetenp\u00f6bel wird nie zu dieser Freyheit sich schwingen;", "tokens": ["Poe\u00b7ten\u00b7p\u00f6\u00b7bel", "wird", "nie", "zu", "die\u00b7ser", "Frey\u00b7heit", "sich", "schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "PDAT", "NN", "PRF", "VVINF", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Jhn blendet noch zu sehr der Titel, und die Macht.", "tokens": ["Jhn", "blen\u00b7det", "noch", "zu", "sehr", "der", "Ti\u00b7tel", ",", "und", "die", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKA", "ADV", "ART", "NN", "$,", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch, Huber, wenn du dich mit deinen freym\u00fc-", "tokens": ["Doch", ",", "Hu\u00b7ber", ",", "wenn", "du", "dich", "mit", "dei\u00b7nen", "frey\u00b7m\u00fc"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "NE", "$,", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Vom unterth\u00e4nigen Schwarm der kriechenden Reimer", "tokens": ["Vom", "un\u00b7ter\u00b7th\u00e4\u00b7ni\u00b7gen", "Schwarm", "der", "krie\u00b7chen\u00b7den", "Rei\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O so vergi\u00df nicht, o Freund, da\u00df du in Deutschland", "tokens": ["O", "so", "ver\u00b7gi\u00df", "nicht", ",", "o", "Freund", ",", "da\u00df", "du", "in", "Deutschland"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVIMP", "PTKNEG", "$,", "FM", "NN", "$,", "KOUS", "PPER", "APPR", "NE"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Das nicht die Freyheit kennt, die einen Britten hebt.", "tokens": ["Das", "nicht", "die", "Frey\u00b7heit", "kennt", ",", "die", "ei\u00b7nen", "Brit\u00b7ten", "hebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "NN", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nicht hohen Stand zu scheun, und keinen Reich-", "tokens": ["Nicht", "ho\u00b7hen", "Stand", "zu", "scheun", ",", "und", "kei\u00b7nen", "Reich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADJA", "NN", "PTKZU", "VVINF", "$,", "KON", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom Laster nicht verfolgt, vom Laster sicher zu schrei-", "tokens": ["Vom", "Las\u00b7ter", "nicht", "ver\u00b7folgt", ",", "vom", "Las\u00b7ter", "si\u00b7cher", "zu", "schrei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKNEG", "VVPP", "$,", "APPRART", "NN", "ADJD", "APPR", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Die Freyheit herrschet allein auf jener gl\u00fccklichen In-", "tokens": ["Die", "Frey\u00b7heit", "herr\u00b7schet", "al\u00b7lein", "auf", "je\u00b7ner", "gl\u00fcck\u00b7li\u00b7chen", "In"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PDAT", "ADJA", "TRUNC"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wo man Unsterblichkeit auch mit Guineen lohnt.", "tokens": ["Wo", "man", "U\u00b7nsterb\u00b7lich\u00b7keit", "auch", "mit", "Gui\u00b7neen", "lohnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}}}}