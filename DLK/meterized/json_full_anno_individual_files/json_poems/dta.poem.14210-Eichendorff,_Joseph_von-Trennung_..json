{"dta.poem.14210": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Trennung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Denkst Du noch jenes Abends, still vor Sehnen,", "tokens": ["Denkst", "Du", "noch", "je\u00b7nes", "A\u00b7bends", ",", "still", "vor", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PDAT", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo wir zum Letztenmal im Park beisammen?", "tokens": ["Wo", "wir", "zum", "Letz\u00b7ten\u00b7mal", "im", "Park", "bei\u00b7sam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "K\u00fchl standen rings des Abendrothes Flammen,", "tokens": ["K\u00fchl", "stan\u00b7den", "rings", "des", "A\u00b7ben\u00b7dro\u00b7thes", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich scherzte wild \u2014 Du l\u00e4cheltest durch Thr\u00e4nen.", "tokens": ["Ich", "scherz\u00b7te", "wild", "Du", "l\u00e4\u00b7chel\u00b7test", "durch", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$(", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "So spielt der Wahnsinn lieblich mit den Schmerzen", "tokens": ["So", "spielt", "der", "Wahn\u00b7sinn", "lieb\u00b7lich", "mit", "den", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "An j\u00e4her Schl\u00fcfte Rand, die nach ihm trachten;", "tokens": ["An", "j\u00e4\u00b7her", "Schl\u00fcf\u00b7te", "Rand", ",", "die", "nach", "ihm", "trach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,", "PRELS", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Er mag der lauernden Gefahr nicht achten;", "tokens": ["Er", "mag", "der", "lau\u00b7ern\u00b7den", "Ge\u00b7fahr", "nicht", "ach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er hat den Tod ja schon im \u00f6den Herzen.", "tokens": ["Er", "hat", "den", "Tod", "ja", "schon", "im", "\u00f6\u00b7den", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}