{"dta.poem.2973": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Zukunft steht verh\u00fcllt schon in der Gegenwart,", "tokens": ["Die", "Zu\u00b7kunft", "steht", "ver\u00b7h\u00fcllt", "schon", "in", "der", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo sie der stumpfe Blick des Menschen nicht gewahrt.", "tokens": ["Wo", "sie", "der", "stump\u00b7fe", "Blick", "des", "Men\u00b7schen", "nicht", "ge\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wir alle streben zwar zu heben ihren Flor,", "tokens": ["Wir", "al\u00b7le", "stre\u00b7ben", "zwar", "zu", "he\u00b7ben", "ih\u00b7ren", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ADV", "PTKZU", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch staunen werden wir, wann sie nun tritt hervor.", "tokens": ["Doch", "stau\u00b7nen", "wer\u00b7den", "wir", ",", "wann", "sie", "nun", "tritt", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie hat, mein Ahnen spricht, ein ander Angesicht,", "tokens": ["Sie", "hat", ",", "mein", "Ah\u00b7nen", "spricht", ",", "ein", "an\u00b7der", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPOSAT", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als mancher glaubt, der nun f\u00fcr seinen Abgott ficht.", "tokens": ["Als", "man\u00b7cher", "glaubt", ",", "der", "nun", "f\u00fcr", "sei\u00b7nen", "Ab\u00b7gott", "ficht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie l\u00e4chelt und sie z\u00fcrnt, wie ihrs euch nicht la\u00dft tr\u00e4umen,", "tokens": ["Sie", "l\u00e4\u00b7chelt", "und", "sie", "z\u00fcrnt", ",", "wie", "ihrs", "euch", "nicht", "la\u00dft", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PPER", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Blick von ihr wird euch und euern Wahn wegr\u00e4umen.", "tokens": ["Ein", "Blick", "von", "ihr", "wird", "euch", "und", "eu\u00b7ern", "Wahn", "weg\u00b7r\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VAFIN", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das sei euch profezeit: sie gleicht in nichts der Zeit,", "tokens": ["Das", "sei", "euch", "pro\u00b7fe\u00b7zeit", ":", "sie", "gleicht", "in", "nichts", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "NN", "$.", "PPER", "VVFIN", "APPR", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am allerwenigsten doch der Vergangenheit.", "tokens": ["Am", "al\u00b7ler\u00b7we\u00b7nigs\u00b7ten", "doch", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "ADV", "ART", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "O weh, betrogner Gast, der du der G\u00f6ttin Glast", "tokens": ["O", "weh", ",", "be\u00b7trog\u00b7ner", "Gast", ",", "der", "du", "der", "G\u00f6t\u00b7tin", "Glast"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit solchem nebligen Gespenst verwechselt hast.", "tokens": ["Mit", "sol\u00b7chem", "neb\u00b7li\u00b7gen", "Ge\u00b7spenst", "ver\u00b7wech\u00b7selt", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Du bist des Lohnes baar, da sie im Reich erschienen,", "tokens": ["Du", "bist", "des", "Loh\u00b7nes", "baar", ",", "da", "sie", "im", "Reich", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$,", "KOUS", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil du ihr dientest zwar, doch wolltest ihr nicht dienen.", "tokens": ["Weil", "du", "ihr", "dien\u00b7test", "zwar", ",", "doch", "woll\u00b7test", "ihr", "nicht", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADV", "$,", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}