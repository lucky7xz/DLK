{"textgrid.poem.53131": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Meyer geht aus meyen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Meyer geht aus meyen", "tokens": ["Ein", "Me\u00b7yer", "geht", "aus", "me\u00b7yen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bey dieser Vorjahrs-Zeit,", "tokens": ["Bey", "die\u00b7ser", "Vor\u00b7jahr\u00b7sZeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der in den Artzeneyen", "tokens": ["Der", "in", "den", "Art\u00b7ze\u00b7ne\u00b7yen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Weis gr\u00fcndlich wol Bescheid.", "tokens": ["Weis", "gr\u00fcnd\u00b7lich", "wol", "Be\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er steiget auff vnd nieder", "tokens": ["Er", "stei\u00b7get", "auff", "vnd", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "KON", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Berg, Th\u00e4ler, Wiesen, Stein'", "tokens": ["Berg", ",", "Th\u00e4\u00b7ler", ",", "Wie\u00b7sen", ",", "Stein'"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd samlet hin vnd wieder", "tokens": ["Vnd", "sam\u00b7let", "hin", "vnd", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viel sch\u00f6ner Kr\u00e4uter ein.", "tokens": ["Viel", "sch\u00f6\u00b7ner", "Kr\u00e4u\u00b7ter", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch wei\u00df er nichts zu finden,", "tokens": ["Doch", "wei\u00df", "er", "nichts", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wodurch sein Hertzens-Weh", "tokens": ["Wo\u00b7durch", "sein", "Hert\u00b7zens\u00b7Weh"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihm endlich k\u00f6nte schwinden,", "tokens": ["Ihm", "end\u00b7lich", "k\u00f6n\u00b7te", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ohn durch den s\u00fcssen Klee.", "tokens": ["Ohn", "durch", "den", "s\u00fcs\u00b7sen", "Klee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hie an des Pregels B\u00e4chen", "tokens": ["Hie", "an", "des", "Pre\u00b7gels", "B\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "W\u00e4chst dieses sch\u00f6ne Kraut,", "tokens": ["W\u00e4chst", "die\u00b7ses", "sch\u00f6\u00b7ne", "Kraut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Meyer kan nicht sprechen", "tokens": ["Der", "Me\u00b7yer", "kan", "nicht", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Wort, als er es schawt.", "tokens": ["Ein", "Wort", ",", "als", "er", "es", "schawt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O, seufftzet er im Hertzen:", "tokens": ["O", ",", "seufft\u00b7zet", "er", "im", "Hert\u00b7zen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was geh' ich armer an?", "tokens": ["Was", "geh'", "ich", "ar\u00b7mer", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach seht, was tausent Schmertzen", "tokens": ["Ach", "seht", ",", "was", "tau\u00b7sent", "Schmert\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "$,", "PRELS", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Klee mir machen kan.", "tokens": ["Der", "Klee", "mir", "ma\u00b7chen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O Kleeblat, aller Herden", "tokens": ["O", "Klee\u00b7blat", ",", "al\u00b7ler", "Her\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd meine S\u00fcssigkeit,", "tokens": ["Vnd", "mei\u00b7ne", "S\u00fcs\u00b7sig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O m\u00f6chtest du doch werden", "tokens": ["O", "m\u00f6ch\u00b7test", "du", "doch", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADV", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von mir bald abgemeyt!", "tokens": ["Von", "mir", "bald", "ab\u00b7ge\u00b7meyt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich bin bem\u00fcht gewesen", "tokens": ["Ich", "bin", "be\u00b7m\u00fcht", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "VAPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gar fern in Oesterreich", "tokens": ["Gar", "fern", "in", "O\u00b7es\u00b7ter\u00b7reich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Viel Blumen auff-zu-lesen,", "tokens": ["Viel", "Blu\u00b7men", "auf\u00b7fzu\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur dir war keine gleich.", "tokens": ["Nur", "dir", "war", "kei\u00b7ne", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PIAT", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich nehme selbs bezeuget,", "tokens": ["Ich", "neh\u00b7me", "selbs", "be\u00b7zeu\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was sich an Pracht vnd Zier", "tokens": ["Was", "sich", "an", "Pracht", "vnd", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Pregel hie er\u00e4uget,", "tokens": ["Am", "Pre\u00b7gel", "hie", "er\u00b7\u00e4u\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich lasse nicht von dir.", "tokens": ["Ich", "las\u00b7se", "nicht", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Mein' Officin verlanget,", "tokens": ["Mein'", "Of\u00b7fi\u00b7cin", "ver\u00b7lan\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Klee, nach dir allein,", "tokens": ["O", "Klee", ",", "nach", "dir", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn sie mit dir nicht pranget,", "tokens": ["Wenn", "sie", "mit", "dir", "nicht", "pran\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was k\u00fcmmert mich jhr Schein?", "tokens": ["Was", "k\u00fcm\u00b7mert", "mich", "jhr", "Schein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ihr Hirten in den Heyden,", "tokens": ["Ihr", "Hir\u00b7ten", "in", "den", "Hey\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Sch\u00e4hffer hin vnd hehr,", "tokens": ["Ihr", "Sch\u00e4hf\u00b7fer", "hin", "vnd", "hehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sonst m\u00f6gt jhr sicher weiden,", "tokens": ["Sonst", "m\u00f6gt", "jhr", "si\u00b7cher", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Macht hie mir kein beschwehr.", "tokens": ["Macht", "hie", "mir", "kein", "be\u00b7schwehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "PIAT", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Meit ewre Trifft mit trewen,", "tokens": ["Meit", "ew\u00b7re", "Trifft", "mit", "tre\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "VVINF", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.2": {"text": "Nur da\u00df sie's nicht versehn", "tokens": ["Nur", "da\u00df", "sie's", "nicht", "ver\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd mir den Klee abmeyen,", "tokens": ["Vnd", "mir", "den", "Klee", "ab\u00b7me\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Es ist vmb mich geschehn.", "tokens": ["Es", "ist", "vmb", "mich", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Dein wird der Himmel pflegen,", "tokens": ["Dein", "wird", "der", "Him\u00b7mel", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Kleeblat, meine Rhue,", "tokens": ["O", "Klee\u00b7blat", ",", "mei\u00b7ne", "Rhue", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df weder Frost noch Regen", "tokens": ["Da\u00df", "we\u00b7der", "Frost", "noch", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KON", "NN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch Sturm dir Schaden thue.", "tokens": ["Noch", "Sturm", "dir", "Scha\u00b7den", "thue", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Mich sol nach dir verlangen", "tokens": ["Mich", "sol", "nach", "dir", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In trewer Lieb' vnd Pflicht,", "tokens": ["In", "tre\u00b7wer", "Lieb'", "vnd", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn mich die Nacht vmbfangen,", "tokens": ["Wenn", "mich", "die", "Nacht", "vmb\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd wenn der Tag anbricht.", "tokens": ["Vnd", "wenn", "der", "Tag", "an\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Dein Wunsch ist angenommen,", "tokens": ["Dein", "Wunsch", "ist", "an\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wol dir, o Meyer, wol!", "tokens": ["Wol", "dir", ",", "o", "Me\u00b7yer", ",", "wol", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "$,", "FM", "NE", "$,", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die liebe Stund ist kommen,", "tokens": ["Die", "lie\u00b7be", "Stund", "ist", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So dich begn\u00fcgen sol.", "tokens": ["So", "dich", "be\u00b7gn\u00fc\u00b7gen", "sol", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Dein Klee wil dich erfrewen,", "tokens": ["Dein", "Klee", "wil", "dich", "er\u00b7fre\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du solst, wie dir bewust,", "tokens": ["Du", "solst", ",", "wie", "dir", "be\u00b7wust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihm seine Zier abmeyen", "tokens": ["Ihm", "sei\u00b7ne", "Zier", "ab\u00b7me\u00b7yen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nach aller Gn\u00fcg vnd Lust.", "tokens": ["Nach", "al\u00b7ler", "Gn\u00fcg", "vnd", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Er ist vnd bleibt dein eigen,", "tokens": ["Er", "ist", "vnd", "bleibt", "dein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mey, Meyer, jhn mit macht,", "tokens": ["Mey", ",", "Me\u00b7yer", ",", "jhn", "mit", "macht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PPER", "APPR", "VVFIN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Die Jahre werden zeigen,", "tokens": ["Die", "Jah\u00b7re", "wer\u00b7den", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie viel du eingebracht.", "tokens": ["Wie", "viel", "du", "ein\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Ein Meyer geht aus meyen", "tokens": ["Ein", "Me\u00b7yer", "geht", "aus", "me\u00b7yen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bey dieser Vorjahrs-Zeit,", "tokens": ["Bey", "die\u00b7ser", "Vor\u00b7jahr\u00b7sZeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der in den Artzeneyen", "tokens": ["Der", "in", "den", "Art\u00b7ze\u00b7ne\u00b7yen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Weis gr\u00fcndlich wol Bescheid.", "tokens": ["Weis", "gr\u00fcnd\u00b7lich", "wol", "Be\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Er steiget auff vnd nieder", "tokens": ["Er", "stei\u00b7get", "auff", "vnd", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "KON", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Berg, Th\u00e4ler, Wiesen, Stein'", "tokens": ["Berg", ",", "Th\u00e4\u00b7ler", ",", "Wie\u00b7sen", ",", "Stein'"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd samlet hin vnd wieder", "tokens": ["Vnd", "sam\u