{"textgrid.poem.60602": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr, sieh, ich bin verdrossen,", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, sieh, ich bin verdrossen,", "tokens": ["Herr", ",", "sieh", ",", "ich", "bin", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu thun, was dir gef\u00e4llt!", "tokens": ["Zu", "thun", ",", "was", "dir", "ge\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Herz hinkt unentschlossen", "tokens": ["Mein", "Herz", "hinkt", "un\u00b7ent\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch zwischen Gott und Welt.", "tokens": ["Noch", "zwi\u00b7schen", "Gott", "und", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich dr\u00fccken schn\u00f6de Ketten", "tokens": ["Mich", "dr\u00fc\u00b7cken", "schn\u00f6\u00b7de", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und alter S\u00fcnden Sklaverey:", "tokens": ["Und", "al\u00b7ter", "S\u00fcn\u00b7den", "Skla\u00b7ve\u00b7rey", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verzeuch nicht, mich zu retten,", "tokens": ["Ver\u00b7zeuch", "nicht", ",", "mich", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und mach, o Gott, mich frey!", "tokens": ["Und", "mach", ",", "o", "Gott", ",", "mich", "frey", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "FM", "NN", "$,", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Geh auf in meiner Seele,", "tokens": ["Geh", "auf", "in", "mei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geh auf mit vollem Glanz!", "tokens": ["Geh", "auf", "mit", "vol\u00b7lem", "Glanz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Damit ich dich erw\u00e4hle,", "tokens": ["Da\u00b7mit", "ich", "dich", "er\u00b7w\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So zeige dich mir ganz!", "tokens": ["So", "zei\u00b7ge", "dich", "mir", "ganz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie schrecklich du dem S\u00fcnder,", "tokens": ["Wie", "schreck\u00b7lich", "du", "dem", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O heiligstes der Wesen, seyst,", "tokens": ["O", "hei\u00b7ligs\u00b7tes", "der", "We\u00b7sen", ",", "seyst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADJA", "ART", "NN", "$,", "VAFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Du Vater deiner Kinder,", "tokens": ["Du", "Va\u00b7ter", "dei\u00b7ner", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vollkommner h\u00f6chster Geist!", "tokens": ["Voll\u00b7komm\u00b7ner", "h\u00f6chs\u00b7ter", "Geist", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sollt in gewohnten S\u00fcnden,", "tokens": ["Sollt", "in", "ge\u00b7wohn\u00b7ten", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Eh ich vers\u00f6hnet bin,", "tokens": ["Eh", "ich", "ver\u00b7s\u00f6h\u00b7net", "bin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein gro\u00dfer Tag mich finden,", "tokens": ["Dein", "gro\u00b7\u00dfer", "Tag", "mich", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O Gott, wo fl\u00f6h ich hin:", "tokens": ["O", "Gott", ",", "wo", "fl\u00f6h", "ich", "hin", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wann unter Ungewittern", "tokens": ["Wann", "un\u00b7ter", "Un\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Berge taumeln, wie vom Wind,", "tokens": ["Die", "Ber\u00b7ge", "tau\u00b7meln", ",", "wie", "vom", "Wind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Erd und Himmel zittern,", "tokens": ["Und", "Erd", "und", "Him\u00b7mel", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Sonnen finster sind!", "tokens": ["Und", "Son\u00b7nen", "fins\u00b7ter", "sind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ringt jammernd eure H\u00e4nde,", "tokens": ["Ringt", "jam\u00b7mernd", "eu\u00b7re", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ihr auf Erden lebt!", "tokens": ["Die", "ihr", "auf", "Er\u00b7den", "lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie f\u00fchlt ihr kommend Ende,", "tokens": ["Sie", "f\u00fchlt", "ihr", "kom\u00b7mend", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie \u00e4ngstet sich und bebt.", "tokens": ["Sie", "\u00e4ngs\u00b7tet", "sich", "und", "bebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie rauchen angez\u00fcndet,", "tokens": ["Sie", "rau\u00b7chen", "an\u00b7ge\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Wohnungen der Missethat,", "tokens": ["Die", "Woh\u00b7nun\u00b7gen", "der", "Mis\u00b7se\u00b7that", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da der die Welt gegr\u00fcndet,", "tokens": ["Da", "der", "die", "Welt", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich ihr als Richter naht.", "tokens": ["Sich", "ihr", "als", "Rich\u00b7ter", "naht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er k\u00f6mmt, und Blitze r\u00f6then", "tokens": ["Er", "k\u00f6mmt", ",", "und", "Blit\u00b7ze", "r\u00f6\u00b7then"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Arm des Menschensohns!", "tokens": ["Den", "Arm", "des", "Men\u00b7schen\u00b7sohns", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Herr! deine Blicke t\u00f6dten", "tokens": ["Herr", "!", "dei\u00b7ne", "Bli\u00b7cke", "t\u00f6d\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die Feinde deines Throns:", "tokens": ["Die", "Fein\u00b7de", "dei\u00b7nes", "Throns", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und Erd und Himmel fliehet", "tokens": ["Und", "Erd", "und", "Him\u00b7mel", "flie\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vor deinem furchtbarn Angesicht,", "tokens": ["Vor", "dei\u00b7nem", "furcht\u00b7barn", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wer nach ihnen siehet,", "tokens": ["Und", "wer", "nach", "ih\u00b7nen", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sieht ihre St\u00e4te nicht.", "tokens": ["Sieht", "ih\u00b7re", "St\u00e4\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ihr neuen Himmel, schweiget!", "tokens": ["Ihr", "neu\u00b7en", "Him\u00b7mel", ",", "schwei\u00b7get", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der S\u00fcnder wird verklagt;", "tokens": ["Der", "S\u00fcn\u00b7der", "wird", "ver\u00b7klagt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sein Gewissen zeuget,", "tokens": ["Und", "sein", "Ge\u00b7wis\u00b7sen", "zeu\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das an der Seele nagt,", "tokens": ["Das", "an", "der", "See\u00b7le", "nagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er bebt, er weicht zur\u00fccke:", "tokens": ["Er", "bebt", ",", "er", "weicht", "zu\u00b7r\u00fc\u00b7cke", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Weh ihm! die ganze H\u00f6lle gl\u00fcht", "tokens": ["Weh", "ihm", "!", "die", "gan\u00b7ze", "H\u00f6l\u00b7le", "gl\u00fcht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In seinem finstern Blicke,", "tokens": ["In", "sei\u00b7nem", "fins\u00b7tern", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der seinen Richter flieht.", "tokens": ["Der", "sei\u00b7nen", "Rich\u00b7ter", "flieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Was helfen Ruhm und Siege?", "tokens": ["Was", "hel\u00b7fen", "Ruhm", "und", "Sie\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was hilft dem S\u00fcnder itzt", "tokens": ["Was", "hilft", "dem", "S\u00fcn\u00b7der", "itzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Marmors feile L\u00fcge,", "tokens": ["Des", "Mar\u00b7mors", "fei\u00b7le", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die auf dem Grabmaal blitzt,", "tokens": ["Die", "auf", "dem", "Grab\u00b7maal", "blitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In dieser gro\u00dfen Scene,", "tokens": ["In", "die\u00b7ser", "gro\u00b7\u00dfen", "Sce\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo der sich unter Engel mischt,", "tokens": ["Wo", "der", "sich", "un\u00b7ter", "En\u00b7gel", "mischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PRF", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der eine fromme Thr\u00e4ne", "tokens": ["Der", "ei\u00b7ne", "from\u00b7me", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem Armen abgewischt?", "tokens": ["Dem", "Ar\u00b7men", "ab\u00b7ge\u00b7wischt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wann die verdammte Rotte,", "tokens": ["Wann", "die", "ver\u00b7damm\u00b7te", "Rot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verfolgt vom Donner, eilt,", "tokens": ["Ver\u00b7folgt", "vom", "Don\u00b7ner", ",", "eilt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, ewig fern von Gotte,", "tokens": ["Und", ",", "e\u00b7wig", "fern", "von", "Got\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gequ\u00e4lt und l\u00e4sternd heult:", "tokens": ["Ge\u00b7qu\u00e4lt", "und", "l\u00e4s\u00b7ternd", "heult", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Am Tage deiner Rache,", "tokens": ["Am", "Ta\u00b7ge", "dei\u00b7ner", "Ra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gott! Mittler! nimm dich meiner an,", "tokens": ["Gott", "!", "Mitt\u00b7ler", "!", "nimm", "dich", "mei\u00b7ner", "an", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "VVIMP", "PPER", "PPOSAT", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und f\u00fchre meine Sache,", "tokens": ["Und", "f\u00fch\u00b7re", "mei\u00b7ne", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie du am Kreuz gethan!", "tokens": ["Wie", "du", "am", "Kreuz", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Herr, sieh, ich bin verdrossen,", "tokens": ["Herr", ",", "sieh", ",", "ich", "bin", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu thun, was dir gef\u00e4llt!", "tokens": ["Zu", "thun", ",", "was", "dir", "ge\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Herz hinkt unentschlossen", "tokens": ["Mein", "Herz", "hinkt", "un\u00b7ent\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch zwischen Gott und Welt.", "tokens": ["Noch", "zwi\u00b7schen", "Gott", "und", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich dr\u00fccken schn\u00f6de Ketten", "tokens": ["Mich", "dr\u00fc\u00b7cken", "schn\u00f6\u00b7de", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und alter S\u00fcnden Sklaverey:", "tokens": ["Und", "al\u00b7ter", "S\u00fcn\u00b7den", "Skla\u00b7ve\u00b7rey", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verzeuch nicht, mich zu retten,", "tokens": ["Ver\u00b7zeuch", "nicht", ",", "mich", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und mach, o Gott, mich frey!", "tokens": ["Und", "mach", ",", "o", "Gott", ",", "mich", "frey", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "FM", "NN", "$,", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Geh auf in meiner Seele,", "tokens": ["Geh", "auf", "in", "mei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geh auf mit vollem Glanz!", "tokens": ["Geh", "auf", "mit", "vol\u00b7lem", "Glanz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Damit ich dich erw\u00e4hle,", "tokens": ["Da\u00b7mit", "ich", "dich", "er\u00b7w\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So zeige dich mir ganz!", "tokens": ["So", "zei\u00b7ge", "dich", "mir", "ganz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie schrecklich du dem S\u00fcnder,", "tokens": ["Wie", "schreck\u00b7lich", "du", "dem", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O heiligstes der Wesen, seyst,", "tokens": ["O", "hei\u00b7ligs\u00b7tes", "der", "We\u00b7sen", ",", "seyst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADJA", "ART", "NN", "$,", "VAFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Du Vater deiner Kinder,", "tokens": ["Du", "Va\u00b7ter", "dei\u00b7ner", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vollkommner h\u00f6chster Geist!", "tokens": ["Voll\u00b7komm\u00b7ner", "h\u00f6chs\u00b7ter", "Geist", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sollt in gewohnten S\u00fcnden,", "tokens": ["Sollt", "in", "ge\u00b7wohn\u00b7ten", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Eh ich vers\u00f6hnet bin,", "tokens": ["Eh", "ich", "ver\u00b7s\u00f6h\u00b7net", "bin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein gro\u00dfer Tag mich finden,", "tokens": ["Dein", "gro\u00b7\u00dfer", "Tag", "mich", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O Gott, wo fl\u00f6h ich hin:", "tokens": ["O", "Gott", ",", "wo", "fl\u00f6h", "ich", "hin", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wann unter Ungewittern", "tokens": ["Wann", "un\u00b7ter", "Un\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Berge taumeln, wie vom Wind,", "tokens": ["Die", "Ber\u00b7ge", "tau\u00b7meln", ",", "wie", "vom", "Wind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Erd und Himmel zittern,", "tokens": ["Und", "Erd", "und", "Him\u00b7mel", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Sonnen finster sind!", "tokens": ["Und", "Son\u00b7nen", "fins\u00b7ter", "sind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ringt jammernd eure H\u00e4nde,", "tokens": ["Ringt", "jam\u00b7mernd", "eu\u00b7re", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ihr auf Erden lebt!", "tokens": ["Die", "ihr", "auf", "Er\u00b7den", "lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie f\u00fchlt ihr kommend Ende,", "tokens": ["Sie", "f\u00fchlt", "ihr", "kom\u00b7mend", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie \u00e4ngstet sich und bebt.", "tokens": ["Sie", "\u00e4ngs\u00b7tet", "sich", "und", "bebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie rauchen angez\u00fcndet,", "tokens": ["Sie", "rau\u00b7chen", "an\u00b7ge\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Wohnungen der Missethat,", "tokens": ["Die", "Woh\u00b7nun\u00b7gen", "der", "Mis\u00b7se\u00b7that", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da der die Welt gegr\u00fcndet,", "tokens": ["Da", "der", "die", "Welt", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich ihr als Richter naht.", "tokens": ["Sich", "ihr", "als", "Rich\u00b7ter", "naht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Er k\u00f6mmt, und Blitze r\u00f6then", "tokens": ["Er", "k\u00f6mmt", ",", "und", "Blit\u00b7ze", "r\u00f6\u00b7then"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Arm des Menschensohns!", "tokens": ["Den", "Arm", "des", "Men\u00b7schen\u00b7sohns", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Herr! deine Blicke t\u00f6dten", "tokens": ["Herr", "!", "dei\u00b7ne", "Bli\u00b7cke", "t\u00f6d\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die Feinde deines Throns:", "tokens": ["Die", "Fein\u00b7de", "dei\u00b7nes", "Throns", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und Erd und Himmel fliehet", "tokens": ["Und", "Erd", "und", "Him\u00b7mel", "flie\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vor deinem furchtbarn Angesicht,", "tokens": ["Vor", "dei\u00b7nem", "furcht\u00b7barn", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wer nach ihnen siehet,", "tokens": ["Und", "wer", "nach", "ih\u00b7nen", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sieht ihre St\u00e4te nicht.", "tokens": ["Sieht", "ih\u00b7re", "St\u00e4\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Ihr neuen Himmel, schweiget!", "tokens": ["Ihr", "neu\u00b7en", "Him\u00b7mel", ",", "schwei\u00b7get", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der S\u00fcnder wird verklagt;", "tokens": ["Der", "S\u00fcn\u00b7der", "wird", "ver\u00b7klagt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sein Gewissen zeuget,", "tokens": ["Und", "sein", "Ge\u00b7wis\u00b7sen", "zeu\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das an der Seele nagt,", "tokens": ["Das", "an", "der", "See\u00b7le", "nagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er bebt, er weicht zur\u00fccke:", "tokens": ["Er", "bebt", ",", "er", "weicht", "zu\u00b7r\u00fc\u00b7cke", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Weh ihm! die ganze H\u00f6lle gl\u00fcht", "tokens": ["Weh", "ihm", "!", "die", "gan\u00b7ze", "H\u00f6l\u00b7le", "gl\u00fcht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In seinem finstern Blicke,", "tokens": ["In", "sei\u00b7nem", "fins\u00b7tern", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der seinen Richter flieht.", "tokens": ["Der", "sei\u00b7nen", "Rich\u00b7ter", "flieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Was helfen Ruhm und Siege?", "tokens": ["Was", "hel\u00b7fen", "Ruhm", "und", "Sie\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was hilft dem S\u00fcnder itzt", "tokens": ["Was", "hilft", "dem", "S\u00fcn\u00b7der", "itzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Marmors feile L\u00fcge,", "tokens": ["Des", "Mar\u00b7mors", "fei\u00b7le", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die auf dem Grabmaal blitzt,", "tokens": ["Die", "auf", "dem", "Grab\u00b7maal", "blitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In dieser gro\u00dfen Scene,", "tokens": ["In", "die\u00b7ser", "gro\u00b7\u00dfen", "Sce\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo der sich unter Engel mischt,", "tokens": ["Wo", "der", "sich", "un\u00b7ter", "En\u00b7gel", "mischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PRF", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der eine fromme Thr\u00e4ne", "tokens": ["Der", "ei\u00b7ne", "from\u00b7me", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem Armen abgewischt?", "tokens": ["Dem", "Ar\u00b7men", "ab\u00b7ge\u00b7wischt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Wann die verdammte Rotte,", "tokens": ["Wann", "die", "ver\u00b7damm\u00b7te", "Rot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verfolgt vom Donner, eilt,", "tokens": ["Ver\u00b7folgt", "vom", "Don\u00b7ner", ",", "eilt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, ewig fern von Gotte,", "tokens": ["Und", ",", "e\u00b7wig", "fern", "von", "Got\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gequ\u00e4lt und l\u00e4sternd heult:", "tokens": ["Ge\u00b7qu\u00e4lt", "und", "l\u00e4s\u00b7ternd", "heult", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Am Tage deiner Rache,", "tokens": ["Am", "Ta\u00b7ge", "dei\u00b7ner", "Ra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gott! Mittler! nimm dich meiner an,", "tokens": ["Gott", "!", "Mitt\u00b7ler", "!", "nimm", "dich", "mei\u00b7ner", "an", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "VVIMP", "PPER", "PPOSAT", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und f\u00fchre meine Sache,", "tokens": ["Und", "f\u00fch\u00b7re", "mei\u00b7ne", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie du am Kreuz gethan!", "tokens": ["Wie", "du", "am", "Kreuz", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}