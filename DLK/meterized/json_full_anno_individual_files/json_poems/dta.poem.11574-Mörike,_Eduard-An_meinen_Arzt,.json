{"dta.poem.11574": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An meinen Arzt,", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Siehe! da st\u00fcnd' ich wieder auf meinen F\u00fc\u00dfen! und blicke", "tokens": ["Sie\u00b7he", "!", "da", "st\u00fcnd'", "ich", "wie\u00b7der", "auf", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", "!", "und", "bli\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "$.", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$.", "KON", "VVFIN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Froh erstaunt in die Welt, die mir im R\u00fccken sch\u00f6n", "tokens": ["Froh", "er\u00b7staunt", "in", "die", "Welt", ",", "die", "mir", "im", "R\u00fc\u00b7cken", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "ADJD"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Aber ich spreche von Danke dir nicht: du liesest ihn besser", "tokens": ["A\u00b7ber", "ich", "spre\u00b7che", "von", "Dan\u00b7ke", "dir", "nicht", ":", "du", "lie\u00b7sest", "ihn", "bes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PTKANT", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Mir im Auge, du f\u00fchlst hier ihn im Drucke der Hand.", "tokens": ["Mir", "im", "Au\u00b7ge", ",", "du", "f\u00fchlst", "hier", "ihn", "im", "Dru\u00b7cke", "der", "Hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "PPER", "VVFIN", "ADV", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "\u2014 Ich gl\u00fcckseliger Thor, da\u00df ich meine, du solltest ver-", "tokens": ["Ich", "gl\u00fcck\u00b7se\u00b7li\u00b7ger", "Thor", ",", "da\u00df", "ich", "mei\u00b7ne", ",", "du", "soll\u00b7test", "ver"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "ADJA", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "TRUNC"], "meter": "+---+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Ueber dich selber seyn, oder ger\u00fchrt, so wie ich!", "tokens": ["Ue\u00b7ber", "dich", "sel\u00b7ber", "seyn", ",", "o\u00b7der", "ge\u00b7r\u00fchrt", ",", "so", "wie", "ich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VAINF", "$,", "KON", "VVPP", "$,", "ADV", "KOKOM", "PPER", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Doch daran erkennen wir dich! \u2014 Den schwindelnden", "tokens": ["Doch", "da\u00b7ran", "er\u00b7ken\u00b7nen", "wir", "dich", "!", "Den", "schwin\u00b7deln\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PRF", "$.", "$(", "ART", "ADJA"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.8": {"text": "Herrlich meisternd f\u00e4hrt ruhig der Schiffer an's Land,", "tokens": ["Herr\u00b7lich", "meis\u00b7ternd", "f\u00e4hrt", "ru\u00b7hig", "der", "Schif\u00b7fer", "an's", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ADJD", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.9": {"text": "Wirft in den Kahn das Ruder, das, ach! so Viele ge-", "tokens": ["Wirft", "in", "den", "Kahn", "das", "Ru\u00b7der", ",", "das", ",", "ach", "!", "so", "Vie\u00b7le", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "PDS", "$,", "ITJ", "$.", "ADV", "PIAT", "TRUNC"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Laut aufjauchzen sie ihm, aber er achtet es kaum,", "tokens": ["Laut", "auf\u00b7jauch\u00b7zen", "sie", "ihm", ",", "a\u00b7ber", "er", "ach\u00b7tet", "es", "kaum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "PPER", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Kettet das Schiff an den Pflock; und Abends sizt er beim", "tokens": ["Ket\u00b7tet", "das", "Schiff", "an", "den", "Pflock", ";", "und", "A\u00b7bends", "sizt", "er", "beim"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$.", "KON", "NN", "VVFIN", "PPER", "APPRART"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.12": {"text": "Wie ein anderer Mann, f\u00fcllet sein Pfeifchen und ruht.", "tokens": ["Wie", "ein", "an\u00b7de\u00b7rer", "Mann", ",", "f\u00fcl\u00b7let", "sein", "Pfei\u00b7fchen", "und", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}}}}}