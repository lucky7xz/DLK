{"dta.poem.23662": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Breslauer Schillerfest 10.Nov.1836.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es lebe die Zeit die neue!", "tokens": ["Es", "le\u00b7be", "die", "Zeit", "die", "neu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und keiner bereue", "tokens": ["Und", "kei\u00b7ner", "be\u00b7re\u00b7ue"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIS", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Zeit die neue,", "tokens": ["Die", "Zeit", "die", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Doch jeden erfreue", "tokens": ["Doch", "je\u00b7den", "er\u00b7freu\u00b7e"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIS", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die Zeit die neue!", "tokens": ["Die", "Zeit", "die", "neu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ich beschw\u00f6r' euch bei den Perr\u00fccken und Z\u00f6pfen,", "tokens": ["Ich", "be\u00b7schw\u00f6r'", "euch", "bei", "den", "Per\u00b7r\u00fc\u00b7cken", "und", "Z\u00f6p\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Bei den Atlasr\u00f6cken mit gro\u00dfen Kn\u00f6pfen,", "tokens": ["Bei", "den", "At\u00b7las\u00b7r\u00f6\u00b7cken", "mit", "gro\u00b7\u00dfen", "Kn\u00f6p\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.8": {"text": "Bei den runden bepuderten ernsten K\u00f6pfen,", "tokens": ["Bei", "den", "run\u00b7den", "be\u00b7pu\u00b7der\u00b7ten", "erns\u00b7ten", "K\u00f6p\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Bei dem Reifrockknix und dem Fischbeinmieder,", "tokens": ["Bei", "dem", "Rei\u00b7frock\u00b7knix", "und", "dem", "Fischbein\u00b7mie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Bei dem versch\u00e4mten Aufschlag der Augenlieder,", "tokens": ["Bei", "dem", "ver\u00b7sch\u00e4m\u00b7ten", "Auf\u00b7schlag", "der", "Au\u00b7gen\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "---+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Bei der Feiertagsruhe aller Glieder,", "tokens": ["Bei", "der", "Fei\u00b7er\u00b7tags\u00b7ru\u00b7he", "al\u00b7ler", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Bei den Tressen und Litzen,", "tokens": ["Bei", "den", "Tres\u00b7sen", "und", "Lit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.13": {"text": "Manschetten und Spitzen,", "tokens": ["Man\u00b7schet\u00b7ten", "und", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Bei den seidenen Str\u00fcmpfen mit falschen Waden,", "tokens": ["Bei", "den", "sei\u00b7de\u00b7nen", "Str\u00fcmp\u00b7fen", "mit", "fal\u00b7schen", "Wa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Bei den Sch\u00f6nheitspflastern, Schminken, Pomaden,", "tokens": ["Bei", "den", "Sch\u00f6n\u00b7heits\u00b7pflas\u00b7tern", ",", "Schmin\u00b7ken", ",", "Po\u00b7ma\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Bei der Weitschnurigkeit", "tokens": ["Bei", "der", "Weit\u00b7schnu\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.17": {"text": "Und Breitspurigkeit", "tokens": ["Und", "Breit\u00b7spu\u00b7rig\u00b7keit"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.18": {"text": "Aller alten und jungen", "tokens": ["Al\u00b7ler", "al\u00b7ten", "und", "jun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "ADJA", "KON", "ADJA"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.19": {"text": "Herzen und Zungen \u2014", "tokens": ["Her\u00b7zen", "und", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Wer k\u00f6nnt' es wagen,", "tokens": ["Wer", "k\u00f6nnt'", "es", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Das Verlorene zu beklagen,", "tokens": ["Das", "Ver\u00b7lo\u00b7re\u00b7ne", "zu", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Und w\u00fcnschen, unserem Leben und Treiben", "tokens": ["Und", "w\u00fcn\u00b7schen", ",", "un\u00b7se\u00b7rem", "Le\u00b7ben", "und", "Trei\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Das Langweilige wieder einzuverleiben?", "tokens": ["Das", "Lang\u00b7wei\u00b7li\u00b7ge", "wie\u00b7der", "ein\u00b7zu\u00b7ver\u00b7lei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVIZU", "$."], "meter": "-++--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Wie der Staub verweht durch das Feld,", "tokens": ["Wie", "der", "Staub", "ver\u00b7weht", "durch", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.25": {"text": "Ist der Puder hinweg aus der Welt,", "tokens": ["Ist", "der", "Pu\u00b7der", "hin\u00b7weg", "aus", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.26": {"text": "Und was er verh\u00fcllt und unkenntlich gemacht,", "tokens": ["Und", "was", "er", "ver\u00b7h\u00fcllt", "und", "un\u00b7kennt\u00b7lich", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVPP", "KON", "ADJD", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.27": {"text": "Ist rein und lauter ans Licht gebracht.", "tokens": ["Ist", "rein", "und", "lau\u00b7ter", "ans", "Licht", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Die alte Zeit musste verloren gehn,", "tokens": ["Die", "al\u00b7te", "Zeit", "muss\u00b7te", "ver\u00b7lo\u00b7ren", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Schon weil sie Schillern musste geboren sehn.", "tokens": ["Schon", "weil", "sie", "Schil\u00b7lern", "muss\u00b7te", "ge\u00b7bo\u00b7ren", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "NN", "VMFIN", "VVPP", "VVINF", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Die alte Zeit ist die gerichtete,", "tokens": ["Die", "al\u00b7te", "Zeit", "ist", "die", "ge\u00b7rich\u00b7te\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "die vernichtete,", "tokens": ["die", "ver\u00b7nich\u00b7te\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.32": {"text": "Weil Schiller dichtete.", "tokens": ["Weil", "Schil\u00b7ler", "dich\u00b7te\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Doch wir wollen vom Alten", "tokens": ["Doch", "wir", "wol\u00b7len", "vom", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.34": {"text": "Alles Gute behalten.", "tokens": ["Al\u00b7les", "Gu\u00b7te", "be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.35": {"text": "Wir behalten heute zu unserem Feste", "tokens": ["Wir", "be\u00b7hal\u00b7ten", "heu\u00b7te", "zu", "un\u00b7se\u00b7rem", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "das Beste \u2014", "tokens": ["das", "Bes\u00b7te"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.37": {"text": "All' ihr Versammelten wisst es:", "tokens": ["All'", "ihr", "Ver\u00b7sam\u00b7mel\u00b7ten", "wisst", "es", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}