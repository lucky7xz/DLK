{"dta.poem.14955": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Von Engeln und von Bengeln.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und mit dem Fl\u00fcgelkleide", "tokens": ["Und", "mit", "dem", "Fl\u00fc\u00b7gel\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie lie\u00dfen den Fl\u00fcgelschnack,", "tokens": ["Sie", "lie\u00b7\u00dfen", "den", "Fl\u00fc\u00b7gel\u00b7schnack", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das war keine kleine Freude:", "tokens": ["Das", "war", "kei\u00b7ne", "klei\u00b7ne", "Freu\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun stattlich in Hosen und Frack!", "tokens": ["Nun", "statt\u00b7lich", "in", "Ho\u00b7sen", "und", "Frack", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}