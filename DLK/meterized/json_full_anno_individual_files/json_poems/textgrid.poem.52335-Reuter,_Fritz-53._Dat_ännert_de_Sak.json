{"textgrid.poem.52335": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "53. Dat \u00e4nnert de Sak", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbjehann, min S\u00e4hn\u00ab, seggt Vader Br\u00fcmmer,", "tokens": ["\u00bb", "je\u00b7hann", ",", "min", "S\u00e4hn", "\u00ab", ",", "seggt", "Va\u00b7der", "Br\u00fcm\u00b7mer", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPOSAT", "NN", "$(", "$,", "VVFIN", "NE", "NE", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "\u00bbmit minen Bein ward't d\u00e4glich slimmer,", "tokens": ["\u00bb", "mit", "mi\u00b7nen", "Bein", "ward't", "d\u00e4g\u00b7lich", "slim\u00b7mer", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ick ward all olt un ward all swack,", "tokens": ["Ick", "ward", "all", "olt", "un", "ward", "all", "swack", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "FM", "FM", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Un Mutter, de ward ok all spack;", "tokens": ["Un", "Mut\u00b7ter", ",", "de", "ward", "ok", "all", "spack", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NE", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Du sallst de Hauw nu negstens krigen,", "tokens": ["Du", "sallst", "de", "Hauw", "nu", "negs\u00b7tens", "kri\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Un denn, min S\u00e4hn, denn sallst du frigen.\u00ab", "tokens": ["Un", "denn", ",", "min", "S\u00e4hn", ",", "denn", "sallst", "du", "fri\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "VVINF", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "\u00bbne, dormit lat Hei mi tau Weg'.\u00ab", "tokens": ["\u00bb", "ne", ",", "dor\u00b7mit", "lat", "Hei", "mi", "tau", "Weg'", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "\u00bbmin S\u00e4hn! Allein her\u00fcm tau nussen,", "tokens": ["\u00bb", "min", "S\u00e4hn", "!", "Al\u00b7lein", "he\u00b7r\u00fcm", "tau", "nus\u00b7sen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$.", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dat gl\u00f6w mi tau, s\u00fch, dat hett keinen Dag.", "tokens": ["Dat", "gl\u00f6w", "mi", "tau", ",", "s\u00fch", ",", "dat", "hett", "kei\u00b7nen", "Dag", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "ADJD", "$,", "PAV", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Du b\u00fcst so sch\u00f6n all ut den Dreck rut wussen,", "tokens": ["Du", "b\u00fcst", "so", "sch\u00f6n", "all", "ut", "den", "Dreck", "rut", "wus\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PIAT", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Du kriggst den Hof un kriggst de Hauw", "tokens": ["Du", "kriggst", "den", "Hof", "un", "kriggst", "de", "Hauw"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "FM", "VVFIN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mit Infentorium, Veih und allen.", "tokens": ["Mit", "In\u00b7fen\u00b7to\u00b7ri\u00b7um", ",", "Veih", "und", "al\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NE", "KON", "PIAT", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Mak du man irst dorvon de Prauw,", "tokens": ["Mak", "du", "man", "irst", "dor\u00b7von", "de", "Prauw", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PIS", "ADV", "PAV", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Dat Frigen sall di woll gefallen.\u00ab \u2013", "tokens": ["Dat", "Fri\u00b7gen", "sall", "di", "woll", "ge\u00b7fal\u00b7len", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "NE", "NE", "NE", "ADV", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "\u00bbne, Vader, ne! Dor helpt kein Reden,", "tokens": ["\u00bb", "ne", ",", "Va\u00b7der", ",", "ne", "!", "Dor", "helpt", "kein", "Re\u00b7den", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "NE", "$,", "NE", "$.", "NE", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Mit Frigen lat Hei mi taufreden.\u00ab", "tokens": ["Mit", "Fri\u00b7gen", "lat", "Hei", "mi", "tauf\u00b7re\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "FM", "FM", "FM", "FM", "$.", "$("], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "\u00bbjehann, min S\u00e4hn\u00ab, seggt nu de Ollsch,", "tokens": ["\u00bb", "je\u00b7hann", ",", "min", "S\u00e4hn", "\u00ab", ",", "seggt", "nu", "de", "Ollsch", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPOSAT", "NN", "$(", "$,", "VVFIN", "ADV", "NE", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.18": {"text": "\u00bbs\u00fch, wer nich frigt, de bliwwt perdollsch.", "tokens": ["\u00bb", "s\u00fch", ",", "wer", "nich", "frigt", ",", "de", "bliwwt", "per\u00b7doll\u00b7sch", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PWS", "PTKNEG", "ADJD", "$,", "NE", "VVFIN", "ADJD", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.19": {"text": "Gl\u00f6w Vadern tau, de weit Bescheid,", "tokens": ["Gl\u00f6w", "Va\u00b7dern", "tau", ",", "de", "weit", "Be\u00b7scheid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "NE", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Du gl\u00f6wst nich, wat dat Frigen deiht!", "tokens": ["Du", "gl\u00f6wst", "nich", ",", "wat", "dat", "Fri\u00b7gen", "deiht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Dat's grad, as wenn w' dat rugste Fahlen", "tokens": ["Dat's", "grad", ",", "as", "wenn", "w'", "dat", "rugs\u00b7te", "Fah\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "NE", "KOUS", "NE", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Von buten nah den Stall rin halen:", "tokens": ["Von", "bu\u00b7ten", "nah", "den", "Stall", "rin", "ha\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADJD", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Irst is dat wild un sihr unb\u00e4nnig,", "tokens": ["Irst", "is", "dat", "wild", "un", "sihr", "un\u00b7b\u00e4n\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "FM", "FM", "FM", "ADJD", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.24": {"text": "Un nahsten ward dat ganz verst\u00e4nnig;", "tokens": ["Un", "nahs\u00b7ten", "ward", "dat", "ganz", "ver\u00b7st\u00e4n\u00b7nig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VAFIN", "ART", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Mit Tom und Sadel ward't regiert,", "tokens": ["Mit", "Tom", "und", "Sa\u00b7del", "ward't", "re\u00b7giert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "D\u00f6rch Strigeln, Straken ward't en Pird.", "tokens": ["D\u00f6rch", "Stri\u00b7geln", ",", "Stra\u00b7ken", "ward't", "en", "Pird", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "$,", "NN", "VVFIN", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Un twor, je ruger 't Fahlen is,", "tokens": ["Un", "twor", ",", "je", "ru\u00b7ger", "'t", "Fah\u00b7len", "is", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "ADJA", "FM", "FM", "FM", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.28": {"text": "Je glatter ward dat Pird gewi\u00df.", "tokens": ["Je", "glat\u00b7ter", "ward", "dat", "Pird", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "D\u00f6rch Strigeln ward dat Fahlen zirlich,", "tokens": ["D\u00f6rch", "Stri\u00b7geln", "ward", "dat", "Fah\u00b7len", "zir\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "D\u00f6rch Frigen ward de Minsch manierlich.\u00ab", "tokens": ["D\u00f6rch", "Fri\u00b7gen", "ward", "de", "Minsch", "ma\u00b7nier\u00b7lich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "NN", "VAFIN", "NE", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "\u00bbne, Mutter, 't is mi tau schanierlich.\u00ab", "tokens": ["\u00bb", "ne", ",", "Mut\u00b7ter", ",", "'t", "is", "mi", "tau", "scha\u00b7nier\u00b7lich", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "$,", "NN", "$,", "FM", "FM", "FM", "FM", "ADJD", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "\u00bbdu Dummerjan!\u00ab seggt Vader Br\u00fcmmer,", "tokens": ["\u00bb", "du", "Dum\u00b7mer\u00b7jan", "!", "\u00ab", "seggt", "Va\u00b7der", "Br\u00fcm\u00b7mer", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NE", "$.", "$(", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "\u00bbwat redst du von schanierlich \u00fcmmer!", "tokens": ["\u00bb", "wat", "redst", "du", "von", "scha\u00b7nier\u00b7lich", "\u00fcm\u00b7mer", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "APPR", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Meinst, dat dat Frigen di wat deiht?", "tokens": ["Meinst", ",", "dat", "dat", "Fri\u00b7gen", "di", "wat", "deiht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "ART", "NN", "FM", "FM", "FM", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.35": {"text": "Wat is dat f\u00f6r 'ne \u00d6tigkeit!", "tokens": ["Wat", "is", "dat", "f\u00f6r", "'ne", "\u00d6\u00b7tig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.nl", "FM.nl", "FM.nl", "FM.nl", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "So'n Zagheit is doch f\u00fcrchterlich.", "tokens": ["So'n", "Zag\u00b7heit", "is", "doch", "f\u00fcrch\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "FM", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "So wat an minen Jungen tau erlewen!", "tokens": ["So", "wat", "an", "mi\u00b7nen", "Jun\u00b7gen", "tau", "er\u00b7le\u00b7wen", "!"], "token_info": ["w