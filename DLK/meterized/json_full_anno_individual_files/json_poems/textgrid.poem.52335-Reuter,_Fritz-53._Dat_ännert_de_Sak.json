, "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Hei brukt nich up mi in tau schrigen.\u00ab", "tokens": ["Hei", "brukt", "nich", "up", "mi", "in", "tau", "schri\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "NE", "NE", "APPR", "NE", "VVFIN", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.43": {"text": "\u00bbja, Vader, dat was ok en anner Ding,", "tokens": ["\u00bb", "ja", ",", "Va\u00b7der", ",", "dat", "was", "ok", "en", "an\u00b7ner", "Ding", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "NE", "$,", "FM", "FM", "FM", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Hei ded jo ok uns' Moder frigen.\u00ab", "tokens": ["Hei", "ded", "jo", "ok", "un\u00b7s'", "Mo\u00b7der", "fri\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}