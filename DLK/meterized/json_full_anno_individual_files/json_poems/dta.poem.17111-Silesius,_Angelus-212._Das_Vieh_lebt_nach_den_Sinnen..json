{"dta.poem.17111": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "212.  Das Vieh lebt nach den Sinnen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wer nach den Sinnen lebt/ den sch\u00e4tz ich f\u00fcr ein Vieh:", "tokens": ["Wer", "nach", "den", "Sin\u00b7nen", "lebt", "/", "den", "sch\u00e4tz", "ich", "f\u00fcr", "ein", "Vieh", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$(", "ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer aber G\u00f6ttlich wird/ dem beug ich meine Knie.", "tokens": ["Wer", "a\u00b7ber", "G\u00f6tt\u00b7lich", "wird", "/", "dem", "beug", "ich", "mei\u00b7ne", "Knie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VAFIN", "$(", "ART", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}