{"dta.poem.22703": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "7. Der Fall Ev \u00e6  ist Ursache da\u00df GOtt  \n Mensch worden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Der Ewge GOttes Sohn kombt her in diese W\u00fcsten/", "tokens": ["Der", "Ew\u00b7ge", "Got\u00b7tes", "Sohn", "kombt", "her", "in", "die\u00b7se", "W\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "ADV", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Un\u0303 n\u00e4hrt sich wie ein Kind an einer Jungfrau Br\u00fcste\u0303.", "tokens": ["U\u00f1", "n\u00e4hrt", "sich", "wie", "ein", "Kind", "an", "ei\u00b7ner", "Jung\u00b7frau", "Br\u00fcst\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer hat jhm dieses weh verursacht und gemacht?", "tokens": ["Wer", "hat", "jhm", "die\u00b7ses", "weh", "ver\u00b7ur\u00b7sacht", "und", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PDAT", "ADJD", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein abgefallnes Weib hat jhn darzu gebracht.", "tokens": ["Ein", "ab\u00b7ge\u00b7fall\u00b7nes", "Weib", "hat", "jhn", "dar\u00b7zu", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "pu