{"textgrid.poem.54174": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Beschlagnahmefreies Gedicht", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin klein.", "tokens": ["Ich", "bin", "klein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Mein Herz ist rein.", "tokens": ["Mein", "Herz", "ist", "rein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Soll niemand drin wohnen als nach Belieben auszuf\u00fcllen allein.", "tokens": ["Soll", "nie\u00b7mand", "drin", "woh\u00b7nen", "als", "nach", "Be\u00b7lie\u00b7ben", "aus\u00b7zu\u00b7f\u00fcl\u00b7len", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VVINF", "KOKOM", "APPR", "NN", "VVIZU", "ADV", "$."], "meter": "-+--+--+-+-+-+--+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Lieb Vaterland, magst ruhig sein,", "tokens": ["Lieb", "Va\u00b7ter\u00b7land", ",", "magst", "ru\u00b7hig", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "fest steht, da\u00df Ponds Creme das beste f\u00fcr die Haut ist.", "tokens": ["fest", "steht", ",", "da\u00df", "Ponds", "Cre\u00b7me", "das", "bes\u00b7te", "f\u00fcr", "die", "Haut", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "KOUS", "NE", "NE", "ART", "ADJA", "APPR", "ART", "NN", "