t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "so preu\u00dfisch-blau, so lind und sanft;", "tokens": ["so", "preu\u00b7\u00dfischblau", ",", "so", "lind", "und", "sanft", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.15": {"text": "da kam ein kleines Schaf daher,", "tokens": ["da", "kam", "ein", "klei\u00b7nes", "Schaf", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "jetzt steht da gar kein Veilchen mehr,", "tokens": ["jetzt", "steht", "da", "gar", "kein", "Veil\u00b7chen", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Hurra.", "tokens": ["Hur\u00b7ra", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Ein Richter steht im Walde,", "tokens": ["Ein", "Rich\u00b7ter", "steht", "im", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "so still und stumm.", "tokens": ["so", "still", "und", "stumm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Er war republikanisch bis zuletzt,", "tokens": ["Er", "war", "re\u00b7pub\u00b7li\u00b7ka\u00b7nisch"