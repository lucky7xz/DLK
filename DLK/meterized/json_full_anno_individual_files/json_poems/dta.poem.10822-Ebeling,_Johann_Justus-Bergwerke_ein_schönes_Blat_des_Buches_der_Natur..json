{"dta.poem.10822": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Bergwerke ein sch\u00f6nes Blat des  \n Buches der Natur.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Reich allwo das Gold entsteht, wo\nSilber und Metal entspringt,", "tokens": ["Das", "Reich", "all\u00b7wo", "das", "Gold", "ent\u00b7steht", ",", "wo", "Sil\u00b7ber", "und", "Me\u00b7tal", "ent\u00b7springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVFIN", "$,", "PWAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Wo uns die Tieffe Edelstein, und andre", "tokens": ["Wo", "uns", "die", "Tief\u00b7fe", "E\u00b7del\u00b7stein", ",", "und", "and\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "$,", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist in dem Buche der Natur, ein Blat das sch\u00f6n", "tokens": ["Ist", "in", "dem", "Bu\u00b7che", "der", "Na\u00b7tur", ",", "ein", "Blat", "das", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und das mit g\u00fcldnen Lettern ist, mit silbern Z\u00fcgen", "tokens": ["Und", "das", "mit", "g\u00fcld\u00b7nen", "Let\u00b7tern", "ist", ",", "mit", "sil\u00b7bern", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDS", "APPR", "ADJA", "NN", "VAFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit sch\u00f6nen Farben ausgeziert, mit Edelsteinen aus-", "tokens": ["Mit", "sch\u00f6\u00b7nen", "Far\u00b7ben", "aus\u00b7ge\u00b7ziert", ",", "mit", "E\u00b7del\u00b7stei\u00b7nen", "aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Wenn man mit Andacht darauf sieht, und es nur", "tokens": ["Wenn", "man", "mit", "An\u00b7dacht", "da\u00b7rauf", "sieht", ",", "und", "es", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "NN", "PAV", "VVFIN", "$,", "KON", "PPER", "ADV"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "So deucht mir da\u00df ein jeder wird, aus allen Zeilen", "tokens": ["So", "deucht", "mir", "da\u00df", "ein", "je\u00b7der", "wird", ",", "aus", "al\u00b7len", "Zei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "ART", "PIS", "VAFIN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es ist lein GOtt der alles schaft, ein wei-\nses und allm\u00e4chtig Wesen.", "tokens": ["Es", "ist", "lein", "Gott", "der", "al\u00b7les", "schaft", ",", "ein", "wei", "ses", "und", "all\u00b7m\u00e4ch\u00b7tig", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "ART", "PIS", "ADJD", "$,", "ART", "TRUNC", "ADJA", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}}}}