{"textgrid.poem.50593": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Unterschied der Sch\u00f6nheit", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Ich f\u00fchle die Gefahr und weich' umsonst zur\u00fcck;", "tokens": ["Ich", "f\u00fch\u00b7le", "die", "Ge\u00b7fahr", "und", "weich'", "um\u00b7sonst", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hergegen wenn ich viel mit ", "tokens": ["Her\u00b7ge\u00b7gen", "wenn", "ich", "viel", "mit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So nimmt sie erst, und, eh' ich's merke, mich gefangen.", "tokens": ["So", "nimmt", "sie", "erst", ",", "und", ",", "eh'", "ich's", "mer\u00b7ke", ",", "mich", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KON", "$,", "KOUS", "PIS", "VVFIN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dort ", "tokens": ["Dort"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Weil Doris ", "tokens": ["Weil", "Do\u00b7ris"], "token_info": ["word", "word"], "pos": ["KOUS", "NE"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Die Sch\u00f6nheit zeigt sich dort im freien Feld' und liegt", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "zeigt", "sich", "dort", "im", "frei\u00b7en", "Feld'", "und", "liegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPRART", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In Chloris Angesicht, als hinterm Busch, verhohlen.", "tokens": ["In", "Chlo\u00b7ris", "An\u00b7ge\u00b7sicht", ",", "als", "hin\u00b7term", "Busch", ",", "ver\u00b7hoh\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "KOUS", "APPRART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Ich f\u00fchle die Gefahr und weich' umsonst zur\u00fcck;", "tokens": ["Ich", "f\u00fch\u00b7le", "die", "Ge\u00b7fahr", "und", "weich'", "um\u00b7sonst", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hergegen wenn ich viel mit ", "tokens": ["Her\u00b7ge\u00b7gen", "wenn", "ich", "viel", "mit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So nimmt sie erst, und, eh' ich's merke, mich gefangen.", "tokens": ["So", "nimmt", "sie", "erst", ",", "und", ",", "eh'", "ich's", "mer\u00b7ke", ",", "mich", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"