{"textgrid.poem.34001": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Unsere Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist die Zeit des stummen Weltgerichts;", "tokens": ["Es", "ist", "die", "Zeit", "des", "stum\u00b7men", "Welt\u00b7ge\u00b7richts", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In Wasserfluten nicht und nicht in Flammen:", "tokens": ["In", "Was\u00b7ser\u00b7flu\u00b7ten", "nicht", "und", "nicht", "in", "Flam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "KON", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Form der Welt bricht in sich selbst zusammen,", "tokens": ["Die", "Form", "der", "Welt", "bricht", "in", "sich", "selbst", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und d\u00e4mmernd tritt die neue aus dem Nichts.", "tokens": ["Und", "d\u00e4m\u00b7mernd", "tritt", "die", "neu\u00b7e", "aus", "dem", "Nichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Dichter zeigt im Spiegel des Gedichts,", "tokens": ["Der", "Dich\u00b7ter", "zeigt", "im", "Spie\u00b7gel", "des", "Ge\u00b7dichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie Tag und Nacht im Morgenroth verschwammen,", "tokens": ["Wie", "Tag", "und", "Nacht", "im", "Mor\u00b7gen\u00b7roth", "ver\u00b7schwam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch wird er nicht beschw\u00f6ren, nicht verdammen,", "tokens": ["Doch", "wird", "er", "nicht", "be\u00b7schw\u00f6\u00b7ren", ",", "nicht", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der keusche Priester am Altar des Lichts.", "tokens": ["Der", "keu\u00b7sche", "Pries\u00b7ter", "am", "Al\u00b7tar", "des", "Lichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er soll mit reiner Hand des Lebens pflegen,", "tokens": ["Er", "soll", "mit", "rei\u00b7ner", "Hand", "des", "Le\u00b7bens", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und, wie er f\u00fcr des Fr\u00fchlings erste Bl\u00fcte", "tokens": ["Und", ",", "wie", "er", "f\u00fcr", "des", "Fr\u00fch\u00b7lings", "ers\u00b7te", "Bl\u00fc\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Auge hat, und sie mit Liebe bricht:", "tokens": ["Ein", "Au\u00b7ge", "hat", ",", "und", "sie", "mit", "Lie\u00b7be", "bricht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "KON", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So darf er auch des Herbstes letzten Segen", "tokens": ["So", "darf", "er", "auch", "des", "Herbs\u00b7tes", "letz\u00b7ten", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht \u00fcberseh'n, und die zu sp\u00e4t ergl\u00fchte", "tokens": ["Nicht", "\u00fc\u00b7ber\u00b7seh'n", ",", "und", "die", "zu", "sp\u00e4t", "er\u00b7gl\u00fch\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "$,", "KON", "ART", "PTKA", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht kalt verschm\u00e4hen, wenn den Kranz er flicht.", "tokens": ["Nicht", "kalt", "ver\u00b7schm\u00e4\u00b7hen", ",", "wenn", "den", "Kranz", "er", "flicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVINF", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es ist die Zeit des stummen Weltgerichts;", "tokens": ["Es", "ist", "die", "Zeit", "des", "stum\u00b7men", "Welt\u00b7ge\u00b7richts", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "po