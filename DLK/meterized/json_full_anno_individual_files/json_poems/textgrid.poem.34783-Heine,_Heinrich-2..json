{"textgrid.poem.34783": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcberall, wo du auch wandelst,", "tokens": ["\u00dc\u00b7be\u00b7rall", ",", "wo", "du", "auch", "wan\u00b7delst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaust du mich zu allen Stunden,", "tokens": ["Schaust", "du", "mich", "zu", "al\u00b7len", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und je mehr du mich mi\u00dfhandelst,", "tokens": ["Und", "je", "mehr", "du", "mich", "mi\u00df\u00b7han\u00b7delst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPER", "PRF", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Treuer bleib ich dir verbunden.", "tokens": ["Treu\u00b7er", "bleib", "ich", "dir", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn mich fesselt holde Bosheit,", "tokens": ["Denn", "mich", "fes\u00b7selt", "hol\u00b7de", "Bos\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie mich G\u00fcte stets vertrieben;", "tokens": ["Wie", "mich", "G\u00fc\u00b7te", "stets", "ver\u0