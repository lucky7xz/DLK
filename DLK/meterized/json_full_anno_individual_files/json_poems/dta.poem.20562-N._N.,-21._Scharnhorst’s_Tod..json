{"dta.poem.20562": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "21.  \n  Scharnhorst\u2019s Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.99"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "Dich preis' ich, Gott, Dein Walten", "tokens": ["Dich", "preis'", "ich", ",", "Gott", ",", "Dein", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Deine ew\u2019ge Macht!", "tokens": ["Und", "Dei\u00b7ne", "ew'\u00b7ge", "Macht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Arm hat mich erhalten", "tokens": ["Dein", "Arm", "hat", "mich", "er\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "K\u00fcrzlich in L\u00fctzner Schlacht! \u2014", "tokens": ["K\u00fcrz\u00b7lich", "in", "L\u00fctz\u00b7ner", "Schlacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ich ", "tokens": ["Ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Am Knie; hab\u2019 mich erkundet.", "tokens": ["Am", "Knie", ";", "hab'", "mich", "er\u00b7kun\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Wund\u2019, hei\u00dft\u2019s, ist gef\u00e4hrlich.", "tokens": ["Die", "Wund'", ",", "hei\u00dft's", ",", "ist", "ge\u00b7f\u00e4hr\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sei\u2019s denn! \u2014 Ich bin entbehrlich,", "tokens": ["Sei's", "denn", "!", "Ich", "bin", "ent\u00b7behr\u00b7lich", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "$.", "$(", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn, nachdem Bund geschlossen", "tokens": ["Wenn", ",", "nach\u00b7dem", "Bund", "ge\u00b7schlos\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und uns zum Kampfgenossen", "tokens": ["Und", "uns", "zum", "Kampf\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Haus Oest\u2019reich sich bestimmt,", "tokens": ["Haus", "O\u00b7est'\u00b7reich", "sich", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Mich Gott dann zu sich nimmt. \u2014", "tokens": ["Mich", "Gott", "dann", "zu", "sich", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "ADV", "APPR", "PRF", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Mit ", "tokens": ["Mit"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Ich, mit Jhm zu der Fehden", "tokens": ["Ich", ",", "mit", "Jhm", "zu", "der", "Feh\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "APPR", "PPER", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.15": {"text": "Verabreden den Plan! \u2014", "tokens": ["Ver\u00b7ab\u00b7re\u00b7den", "den", "Plan", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Er wird dann der Kumpan", "tokens": ["Er", "wird", "dann", "der", "Kum\u00b7pan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Von unserm alten Bl\u00fccher!", "tokens": ["Von", "un\u00b7serm", "al\u00b7ten", "Bl\u00fc\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Das Ding gelingt ganz sicher!", "tokens": ["Das", "Ding", "ge\u00b7lingt", "ganz", "si\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}