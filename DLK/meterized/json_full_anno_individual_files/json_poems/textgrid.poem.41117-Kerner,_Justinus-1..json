{"textgrid.poem.41117": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer deinen Tod, du Bruderherz! gesehen,", "tokens": ["Wer", "dei\u00b7nen", "Tod", ",", "du", "Bru\u00b7der\u00b7herz", "!", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "$,", "PPER", "NN", "$.", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem wird das Sterben Lust;", "tokens": ["Dem", "wird", "das", "Ster\u00b7ben", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gleich Melodie aus fernen Himmelsh\u00f6hen", "tokens": ["Gleich", "Me\u00b7lo\u00b7die", "aus", "fer\u00b7nen", "Him\u00b7mels\u00b7h\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "H\u00f6rt' ich das letzte Atmen deiner Brust,", "tokens": ["H\u00f6rt'", "ich", "das", "letz\u00b7te", "At\u00b7men", "dei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein sanftes S\u00e4useln noch \u2013 dann war's geschehen.", "tokens": ["Ein", "sanf\u00b7tes", "S\u00e4u\u00b7seln", "noch", "\u2013", "dann", "wa\u00b7r's", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$(", "ADV", "VAFIN", "VVPP"