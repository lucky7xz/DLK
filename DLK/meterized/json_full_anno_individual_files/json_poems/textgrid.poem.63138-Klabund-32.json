{"textgrid.poem.63138": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "32", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df mich einmal eine Nacht", "tokens": ["La\u00df", "mich", "ein\u00b7mal", "ei\u00b7ne", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne b\u00f6se Tr\u00e4ume schlafen,", "tokens": ["Oh\u00b7ne", "b\u00f6\u00b7se", "Tr\u00e4u\u00b7me", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der du mich aufs Meer gebracht:", "tokens": ["Der", "du", "mich", "aufs", "Meer", "ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchr mich in den lichten Hafen!", "tokens": ["F\u00fchr", "mich", "in", "den", "lich\u00b7ten", "Ha\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo die gro\u00dfen Schiffe ruhn,", "tokens": ["Wo", "die", "gro\u00b7\u00dfen", "Schif\u00b7fe", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Lauten silbern klingen,", "tokens": ["Wo", "die", "Lau\u00b7ten", "sil\u00b7bern", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo auf wei\u00dfen, seidnen Schuhn", "tokens": ["Wo", "auf", "wei\u00b7\u00dfen", ",", "seid\u00b7nen", "Schuhn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heilige Kellnerinnen springen.", "tokens": ["Hei\u00b7li\u00b7ge", "Kell\u00b7ne\u00b7rin\u00b7nen", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Wo es keine Ausfahrt gibt,", "tokens": ["Wo", "es", "kei\u00b7ne", "Aus\u00b7fahrt", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo wir alle jene trafen,", "tokens": ["Wo", "wir", "al\u00b7le", "je\u00b7ne", "tra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die wir himmlisch einst geliebt \u2013", "tokens": ["Die", "wir", "himm\u00b7lisch", "einst", "ge\u00b7liebt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADV", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df mich schlafen... la\u00df mich schlafen...", "tokens": ["La\u00df", "mich", "schla\u00b7fen", "...", "la\u00df", "mich", "schla\u00b7fen", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$(", "VVIMP", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1