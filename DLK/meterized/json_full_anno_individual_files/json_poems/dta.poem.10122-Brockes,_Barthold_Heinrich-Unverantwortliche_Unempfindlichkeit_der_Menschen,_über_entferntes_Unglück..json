{"dta.poem.10122": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unverantwortliche Unempfindlichkeit  \n der Menschen, \u00fcber entferntes  \n Ungl\u00fcck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gef\u00fchl-los menschliches Geschlechte,", "tokens": ["Ge\u00b7f\u00fchl\u00b7los", "menschli\u00b7ches", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mehr unempfindlich, als ein Stein!", "tokens": ["Mehr", "un\u00b7emp\u00b7find\u00b7lich", ",", "als", "ein", "Stein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit welchem Fug, mit welchem Rechte", "tokens": ["Mit", "wel\u00b7chem", "Fug", ",", "mit", "wel\u00b7chem", "Rech\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verlangest du begl\u00fcckt zu seyn?", "tokens": ["Ver\u00b7lan\u00b7gest", "du", "be\u00b7gl\u00fcckt", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vortrefflich sind dir alle Dinge,", "tokens": ["Vor\u00b7treff\u00b7lich", "sind", "dir", "al\u00b7le", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So lange du sie nur nicht hast:", "tokens": ["So", "lan\u00b7ge", "du", "sie", "nur", "nicht", "hast", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "ADV", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kaum sind sie dein, sind sie geringe,", "tokens": ["Kaum", "sind", "sie", "dein", ",", "sind", "sie", "ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "$,", "VAFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ja werden dir offt gar zur Last.", "tokens": ["Ja", "wer\u00b7den", "dir", "offt", "gar", "zur", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Ein Mittel, uns ein Ding zu nehmen,", "tokens": ["Ein", "Mit\u00b7tel", ",", "uns", "ein", "Ding", "zu", "neh\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ist, wenn man uns dasselbe schenckt.", "tokens": ["Ist", ",", "wenn", "man", "uns", "das\u00b7sel\u00b7be", "schenckt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PIS", "PRF", "PDAT", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Denn, wenn wir alles \u00fcberk\u00e4men,", "tokens": ["Denn", ",", "wenn", "wir", "al\u00b7les", "\u00fc\u00b7ber\u00b7k\u00e4\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Verliert mans, wenn man dran nicht denckt.", "tokens": ["Ver\u00b7liert", "mans", ",", "wenn", "man", "dran", "nicht", "denckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PIS", "PAV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Um dieser Plag\u2019 uns zu entziehen,", "tokens": ["Um", "die\u00b7ser", "Plag'", "uns", "zu", "ent\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Um danckbar und vergn\u00fcgt zu seyn;", "tokens": ["Um", "dan\u00b7ck\u00b7bar", "und", "ver\u00b7gn\u00fcgt", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "KON", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Will ich anietzo mich bem\u00fchen,", "tokens": ["Will", "ich", "an\u00b7iet\u00b7zo", "mich", "be\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Nur erstlich die entfernte Pein,", "tokens": ["Nur", "erst\u00b7lich", "die", "ent\u00b7fern\u00b7te", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Die uns iedoch betreffen k\u00f6nnen:", "tokens": ["Die", "uns", "ie\u00b7doch", "be\u00b7tref\u00b7fen", "k\u00f6n\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Auch nachmahls das, so in der That", "tokens": ["Auch", "nach\u00b7mahls", "das", ",", "so", "in", "der", "That"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PDS", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Des Sch\u00f6pfers G\u00fct\u2019 uns wollen g\u00f6nnen,", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "G\u00fct'", "uns", "wol\u00b7len", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und man von Jhm empfangen hat,", "tokens": ["Und", "man", "von", "Jhm", "emp\u00b7fan\u00b7gen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Mit frohem Ernst zu \u00fcberlegen.", "tokens": ["Mit", "fro\u00b7hem", "Ernst", "zu", "\u00fc\u00b7berl\u00b7e\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Vom Ungl\u00fcck will ich viererley,", "tokens": ["Vom", "Un\u00b7gl\u00fcck", "will", "ich", "vie\u00b7rer\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Wof\u00fcr uns GOTT bewahrt, erwegen:", "tokens": ["Wo\u00b7f\u00fcr", "uns", "GoTT", "be\u00b7wahrt", ",", "er\u00b7we\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "NE", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Krieg, Hunger, Kranckheit, Selaverey.", "tokens": ["Krieg", ",", "Hun\u00b7ger", ",", "Kran\u00b7ck\u00b7heit", ",", "Se\u00b7la\u00b7ve\u00b7rey", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Gieb, h\u00f6chster Herrscher, Deinen Segen,", "tokens": ["Gieb", ",", "h\u00f6chs\u00b7ter", "Herr\u00b7scher", ",", "Dei\u00b7nen", "Se\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Da\u00df es nicht ohne Nutzen sey!", "tokens": ["Da\u00df", "es", "nicht", "oh\u00b7ne", "Nut\u00b7zen", "sey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}