{"textgrid.poem.35273": {"metadata": {"author": {"name": "Rubiner, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Fr\u00fchen", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O die Erde wegrollen! Aufrei\u00dfen die schlammige Erdkugel, L\u00f6cher eintreiben, Sch\u00e4chte zum Licht!", "tokens": ["O", "die", "Er\u00b7de", "weg\u00b7rol\u00b7len", "!", "Auf\u00b7rei\u00b7\u00dfen", "die", "schlam\u00b7mi\u00b7ge", "Erd\u00b7ku\u00b7gel", ",", "L\u00f6\u00b7cher", "ein\u00b7trei\u00b7ben", ",", "Sch\u00e4ch\u00b7te", "zum", "Licht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVINF", "$.", "NN", "ART", "ADJA", "NN", "$,", "NN", "VVINF", "$,", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--++--+--+--+-+--+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Auseinanderballen den Erdklumpen, der feuchte Dunkelheit \u00fcber die Augen schattet!", "tokens": ["Aus\u00b7ein\u00b7an\u00b7der\u00b7bal\u00b7len", "den", "Erd\u00b7klum\u00b7pen", ",", "der", "feuch\u00b7te", "Dun\u00b7kel\u00b7heit", "\u00fc\u00b7ber", "die", "Au\u00b7gen", "schat\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+-+-+-+--+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Hinein in die Erde, Sturmlauf, Ihr Br\u00fcder, an die starre gefr\u00e4\u00dfige Mord-Erde,", "tokens": ["Hin\u00b7ein", "in", "die", "Er\u00b7de", ",", "Sturm\u00b7lauf", ",", "Ihr", "Br\u00fc\u00b7der", ",", "an", "die", "star\u00b7re", "ge\u00b7fr\u00e4\u00b7\u00dfi\u00b7ge", "Mord\u00b7Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "NN", "$,", "PPOSAT", "NN", "$,", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+-+--+---+-", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "O die Erde zersprengen zu Milliarden Staubplaneten in Brand,", "tokens": ["O", "die", "Er\u00b7de", "zer\u00b7spren\u00b7gen", "zu", "Mil\u00b7li\u00b7ar\u00b7den", "Staub\u00b7pla\u00b7ne\u00b7ten", "in", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "APPR", "NN", "NN", "APPR", "NN", "$,"], "meter": "+-+--+--+-+-+-+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "Die Erde sprengen mit einem Ruck der g\u00f6ttlichen Hand in alle H\u00f6hlungen des schimmernden Himmels:", "tokens": ["Die", "Er\u00b7de", "spren\u00b7gen", "mit", "ei\u00b7nem", "Ruck", "der", "g\u00f6tt\u00b7li\u00b7chen", "Hand", "in", "al\u00b7le", "H\u00f6h\u00b7lun\u00b7gen", "des", "schim\u00b7mern\u00b7den", "Him\u00b7mels", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+-+-+---+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "O Gottes brennender Finger sein, der das Tr\u00e4ge winzig zerst\u00e4ubt,", "tokens": ["O", "Got\u00b7tes", "bren\u00b7nen\u00b7der", "Fin\u00b7ger", "sein", ",", "der", "das", "Tr\u00e4\u00b7ge", "win\u00b7zig", "zer\u00b7st\u00e4ubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "VAINF", "$,", "PRELS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+--+-+--+", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Augen wollten Licht nicht sehen. Ohren h\u00f6rten keinen Hall.", "tokens": ["Au\u00b7gen", "woll\u00b7ten", "Licht", "nicht", "se\u00b7hen", ".", "Oh\u00b7ren", "h\u00f6r\u00b7ten", "kei\u00b7nen", "Hall", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "NN", "PTKNEG", "VVINF", "$.", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Tr\u00e4ge Erde war versto\u00dfen, Feindschaft schuf den neuen Ball.", "tokens": ["Tr\u00e4\u00b7ge", "Er\u00b7de", "war", "ver\u00b7sto\u00b7\u00dfen", ",", "Feind\u00b7schaft", "schuf", "den", "neu\u00b7en", "Ball", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "VVPP", "$,", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Die Menschenkugel zersprang.", "tokens": ["Die", "Men\u00b7schen\u00b7ku\u00b7gel", "zer\u00b7sprang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "O seht den g\u00f6ttlichen Lichtschein um Euch, dann dauert der Krieg nicht mehr lang!", "tokens": ["O", "seht", "den", "g\u00f6tt\u00b7li\u00b7chen", "Licht\u00b7schein", "um", "Euch", ",", "dann", "dau\u00b7ert", "der", "Krieg", "nicht", "mehr", "lang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER", "$,", "ADV", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+--+--+-+--+--+", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "O die Erde wegrol