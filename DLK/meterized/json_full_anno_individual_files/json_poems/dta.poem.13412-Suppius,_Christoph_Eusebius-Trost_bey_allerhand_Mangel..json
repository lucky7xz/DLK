{"dta.poem.13412": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Trost bey allerhand Mangel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Gl\u00fccke friert! das Holz ist theuer,", "tokens": ["Mein", "Gl\u00fc\u00b7cke", "friert", "!", "das", "Holz", "ist", "theu\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich habe keinen Kreuzer Geld!", "tokens": ["Ich", "ha\u00b7be", "kei\u00b7nen", "Kreu\u00b7zer", "Geld", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Ungl\u00fcck ist der Sonnen Feuer", "tokens": ["Zum", "Un\u00b7gl\u00fcck", "ist", "der", "Son\u00b7nen", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch schwach in dem Kamin der Welt,", "tokens": ["Noch", "schwach", "in", "dem", "Ka\u00b7min", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch will ich mich darum nicht h\u00e4rmen,", "tokens": ["Doch", "will", "ich", "mich", "da\u00b7rum", "nicht", "h\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "PAV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich wei\u00df noch diesen guten Rath,", "tokens": ["Ich", "wei\u00df", "noch", "die\u00b7sen", "gu\u00b7ten", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer weder Geld noch Holz im Winter hat,", "tokens": ["Wer", "we\u00b7der", "Geld", "noch", "Holz", "im", "Win\u00b7ter", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "KON", "NN", "ADV", "NN", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der kann die Glieder ja bey fremden Feuer w\u00e4rmen.", "tokens": ["Der", "kann", "die", "Glie\u00b7der", "ja", "bey", "frem\u00b7den", "Feu\u00b7er", "w\u00e4r\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Allein, wo soll ich dieses finden,", "tokens": ["Al\u00b7lein", ",", "wo", "soll", "ich", "die\u00b7ses", "fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "VMFIN", "PPER", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df man kein baares Geld bedarf?", "tokens": ["Da\u00df", "man", "kein", "baa\u00b7res", "Geld", "be\u00b7darf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich drehe mich nach allen Winden,", "tokens": ["Ich", "dre\u00b7he", "mich", "nach", "al\u00b7len", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ihre Luft ist gar zu scharf;", "tokens": ["Doch", "ih\u00b7re", "Luft", "ist", "gar", "zu", "scharf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach Norden, S\u00fcden, Osten, Westen", "tokens": ["Nach", "Nor\u00b7den", ",", "S\u00fc\u00b7den", ",", "Os\u00b7ten", ",", "Wes\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wend ich die kalte Nas herum,", "tokens": ["Wend", "ich", "die", "kal\u00b7te", "Nas", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Allein, was hilfts? ihr Abstand bleibet krumm,", "tokens": ["Al\u00b7lein", ",", "was", "hilfts", "?", "ihr", "Ab\u00b7stand", "blei\u00b7bet", "krumm", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Man fragt an keinem Ort nach ungebetnen G\u00e4sten.", "tokens": ["Man", "fragt", "an", "kei\u00b7nem", "Ort", "nach", "un\u00b7ge\u00b7bet\u00b7nen", "G\u00e4s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PIAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich will einmahl mein Gl\u00fcck versuchen,", "tokens": ["Ich", "will", "ein\u00b7mahl", "mein", "Gl\u00fcck", "ver\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und hier in einen Gasthof gehn;", "tokens": ["Und", "hier", "in", "ei\u00b7nen", "Gast\u00b7hof", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Herr Caupo f\u00e4ngt schon an zu fluchen,", "tokens": ["Herr", "Cau\u00b7po", "f\u00e4ngt", "schon", "an", "zu", "flu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ADV", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er spricht: Bleibt vor der Th\u00fcre stehn;", "tokens": ["Er", "spricht", ":", "Bleibt", "vor", "der", "Th\u00fc\u00b7re", "stehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Geld ist die Losung, die ich f\u00fchre!", "tokens": ["Geld", "ist", "die", "Lo\u00b7sung", ",", "die", "ich", "f\u00fch\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das ist mein wahres Element,", "tokens": ["Das", "ist", "mein", "wah\u00b7res", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer es nicht hat, und mich dazu nicht kennt,", "tokens": ["Wer", "es", "nicht", "hat", ",", "und", "mich", "da\u00b7zu", "nicht", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VAFIN", "$,", "KON", "PRF", "PAV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den wei\u00df ich wieder fort von meiner Stubenth\u00fcre.", "tokens": ["Den", "wei\u00df", "ich", "wie\u00b7der", "fort", "von", "mei\u00b7ner", "Stu\u00b7bent\u00b7h\u00fc\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was ist dabey nun anzufangen?", "tokens": ["Was", "ist", "da\u00b7bey", "nun", "an\u00b7zu\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PAV", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geduld! wenn eine Grobheit kr\u00e4nkt;", "tokens": ["Ge\u00b7duld", "!", "wenn", "ei\u00b7ne", "Grob\u00b7heit", "kr\u00e4nkt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort ist ein Schild herausgehangen,", "tokens": ["Dort", "ist", "ein", "Schild", "her\u00b7aus\u00b7ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo man Wein, Wind und Wasser schenkt,", "tokens": ["Wo", "man", "Wein", ",", "Wind", "und", "Was\u00b7ser", "schenkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Da mu\u00df ich hin! mit einem Glase", "tokens": ["Da", "mu\u00df", "ich", "hin", "!", "mit", "ei\u00b7nem", "Gla\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PTKVZ", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kommt man mir ins Gesicht gerannt:", "tokens": ["Kommt", "man", "mir", "ins", "Ge\u00b7sicht", "ge\u00b7rannt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Was will der Herr? hier hat man allerhand?", "tokens": ["Was", "will", "der", "Herr", "?", "hier", "hat", "man", "al\u00b7ler\u00b7hand", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "$.", "ADV", "VAFIN", "PIS", "PIS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nichts? o! zieht wieder ab mit eurer rothen Nase.", "tokens": ["Nichts", "?", "o", "!", "zieht", "wie\u00b7der", "ab", "mit", "eu\u00b7rer", "ro\u00b7then", "Na\u00b7se", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$.", "FM", "$.", "VVFIN", "ADV", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}}, "stanza.5": {"line.1": {"text": "Wo soll man sich doch hinverf\u00fcgen,", "tokens": ["Wo", "soll", "man", "sich", "doch", "hin\u00b7ver\u00b7f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da starren Gliedern wohl geschieht?", "tokens": ["Da", "star\u00b7ren", "Glie\u00b7dern", "wohl", "ge\u00b7schieht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bey denen B\u00fcchern krumm zu liegen,", "tokens": ["Bey", "de\u00b7nen", "B\u00fc\u00b7chern", "krumm", "zu", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Erhitzt sehr selten das Gebl\u00fct,", "tokens": ["Er\u00b7hitzt", "sehr", "sel\u00b7ten", "das", "Ge\u00b7bl\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In derer Philosophen Schriften", "tokens": ["In", "de\u00b7rer", "Phi\u00b7lo\u00b7so\u00b7phen", "Schrif\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDS", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Trifft man oft wenig Feuer an,", "tokens": ["Trifft", "man", "oft", "we\u00b7nig", "Feu\u00b7er", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Geh ich zu dem, was Kircher ", "tokens": ["Geh", "ich", "zu", "dem", ",", "was", "Kir\u00b7cher"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PPER", "APPR", "ART", "$,", "PWS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So ist es auch nicht warm in seinen Erdengr\u00fcften.", "tokens": ["So", "ist", "es", "auch", "nicht", "warm", "in", "sei\u00b7nen", "Er\u00b7den\u00b7gr\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Mein Frost wird Damons Herze r\u00fchren!", "tokens": ["Mein", "Frost", "wird", "Da\u00b7mons", "Her\u00b7ze", "r\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NE", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur fort! er ist von altem Schrot;", "tokens": ["Nur", "fort", "!", "er", "ist", "von", "al\u00b7tem", "Schrot", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was sagen seine Stubenth\u00fcren?", "tokens": ["Was", "sa\u00b7gen", "sei\u00b7ne", "Stu\u00b7bent\u00b7h\u00fc\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Freund! nicht aber in der Noth;", "tokens": ["Ein", "Freund", "!", "nicht", "a\u00b7ber", "in", "der", "Noth", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PTKNEG", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich klopfe mit den steifen Knochen,", "tokens": ["Ich", "klop\u00b7fe", "mit", "den", "stei\u00b7fen", "Kno\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Herr Bruder! Damon! aufgemacht!", "tokens": ["Herr", "Bru\u00b7der", "!", "Da\u00b7mon", "!", "auf\u00b7ge\u00b7macht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$.", "NE", "$.", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was schallt zur\u00fcck? Sein Diener! gute Nacht!", "tokens": ["Was", "schallt", "zu\u00b7r\u00fcck", "?", "Sein", "Die\u00b7ner", "!", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKVZ", "$.", "PPOSAT", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wir sehn uns morgen wohl! heut hab ich mich ver-", "tokens": ["Wir", "sehn", "uns", "mor\u00b7gen", "wohl", "!", "heut", "hab", "ich", "mich", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$.", "ADV", "VAFIN", "PPER", "PRF", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Gro\u00dfmuth l\u00e4sst die Falschheit fahren,", "tokens": ["Die", "Gro\u00df\u00b7muth", "l\u00e4sst", "die", "Falschheit", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie kommt vor eines G\u00f6nners Haus,", "tokens": ["Sie", "kommt", "vor", "ei\u00b7nes", "G\u00f6n\u00b7ners", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da guckt der alte Greis von Jahren", "tokens": ["Da", "guckt", "der", "al\u00b7te", "Greis", "von", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verdrie\u00dflich zu der Th\u00fcr heraus:", "tokens": ["Ver\u00b7drie\u00df\u00b7lich", "zu", "der", "Th\u00fcr", "he\u00b7raus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es ist mir lieb, da\u00df sie erschienen;", "tokens": ["Es", "ist", "mir", "lieb", ",", "da\u00df", "sie", "er\u00b7schie\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was w\u00e4re denn wohl ihr Begehr?", "tokens": ["Was", "w\u00e4\u00b7re", "denn", "wohl", "ihr", "Be\u00b7gehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich bitte nur \u2012 \u2012 Sie kommen wieder her!", "tokens": ["Ich", "bit\u00b7te", "nur", "\u2012", "\u2012", "Sie", "kom\u00b7men", "wie\u00b7der", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "$(", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich will ein andermahl von Herzen gerne dienen.", "tokens": ["Ich", "will", "ein", "an\u00b7der\u00b7mahl", "von", "Her\u00b7zen", "ger\u00b7ne", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADV", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Armseeliger! wohin? erwege!", "tokens": ["Arm\u00b7see\u00b7li\u00b7ger", "!", "wo\u00b7hin", "?", "er\u00b7we\u00b7ge", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "PWAV", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Freund, kein Geld und kein Patron!", "tokens": ["Kein", "Freund", ",", "kein", "Geld", "und", "kein", "Pat\u00b7ron", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Welch eine kalte Leibespflege", "tokens": ["Welch", "ei\u00b7ne", "kal\u00b7te", "Lei\u00b7bes\u00b7pfle\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor einen armen Musensohn!", "tokens": ["Vor", "ei\u00b7nen", "ar\u00b7men", "Mu\u00b7sen\u00b7sohn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erw\u00e4rme denn die starren Glieder", "tokens": ["Er\u00b7w\u00e4r\u00b7me", "denn", "die", "star\u00b7ren", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In deiner dunkeln Einsamkeit,", "tokens": ["In", "dei\u00b7ner", "dun\u00b7keln", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Geduld erhitzet auch! und mit der Zeit", "tokens": ["Ge\u00b7duld", "er\u00b7hit\u00b7zet", "auch", "!", "und", "mit", "der", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "$.", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "W\u00e4rmt ja das Himmelsfeur das Haus der Erden", "tokens": ["W\u00e4rmt", "ja", "das", "Him\u00b7mels\u00b7feur", "das", "Haus", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Mein Gl\u00fcck wird warm! mich friert nicht weiter,", "tokens": ["Mein", "Gl\u00fcck", "wird", "warm", "!", "mich", "friert", "nicht", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer standhaft ist, verlacht die Welt;", "tokens": ["Wer", "stand\u00b7haft", "ist", ",", "ver\u00b7lacht", "die", "Welt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Himmel wird schon wieder heiter,", "tokens": ["Der", "Him\u00b7mel", "wird", "schon", "wie\u00b7der", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der w\u00e4rmet j\u00e4hrlich ohne Geld,", "tokens": ["Der", "w\u0