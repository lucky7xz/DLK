{"dta.poem.8108": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zema, du kamst! t\u00f6ne das Lied zu dem Psalter,", "tokens": ["Ze\u00b7ma", ",", "du", "kamst", "!", "t\u00f6\u00b7ne", "das", "Lied", "zu", "dem", "Psal\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "$.", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--++--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Zema, du kamst! so ergiesse, durch des Festes", "tokens": ["Ze\u00b7ma", ",", "du", "kamst", "!", "so", "er\u00b7gies\u00b7se", ",", "durch", "des", "Fes\u00b7tes"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Lauben, sich der Gesang des Bundes,", "tokens": ["Lau\u00b7ben", ",", "sich", "der", "Ge\u00b7sang", "des", "Bun\u00b7des", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRF", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Zema, du starbst! und erstandst!", "tokens": ["Ze\u00b7ma", ",", "du", "starbst", "!", "und", "er\u00b7standst", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "$.", "KON", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}