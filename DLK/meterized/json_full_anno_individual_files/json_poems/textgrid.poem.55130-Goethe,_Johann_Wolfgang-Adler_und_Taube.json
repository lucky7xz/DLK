{"textgrid.poem.55130": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Adler und Taube", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Adlersj\u00fcngling hob die Fl\u00fcgel", "tokens": ["Ein", "Ad\u00b7lers\u00b7j\u00fcng\u00b7ling", "hob", "die", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Raub aus;", "tokens": ["Nach", "Raub", "aus", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Ihn traf des J\u00e4gers Pfeil und schnitt", "tokens": ["Ihn", "traf", "des", "J\u00e4\u00b7gers", "Pfeil", "und", "schnitt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der rechten Schwinge Sennkraft ab.", "tokens": ["Der", "rech\u00b7ten", "Schwin\u00b7ge", "Senn\u00b7kraft", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er st\u00fcrzt' hinab in einen Myrtenhain,", "tokens": ["Er", "st\u00fcrzt'", "hin\u00b7ab", "in", "ei\u00b7nen", "Myr\u00b7ten\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Fra\u00df seinen Schmerz drei Tage lang", "tokens": ["Fra\u00df", "sei\u00b7nen", "Schmerz", "drei", "Ta\u00b7ge", "lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "CARD", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und zuckt' an Qual", "tokens": ["Und", "zuckt'", "an", "Qual"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Drei lange, lange N\u00e4chte lang.", "tokens": ["Drei", "lan\u00b7ge", ",", "lan\u00b7ge", "N\u00e4ch\u00b7te", "lang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "ADV", "$,", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zuletzt heilt ihn", "tokens": ["Zu\u00b7letzt", "heilt", "ihn"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-++-", "measure": "unknown.measure.di"}, "line.10": {"text": "Allgegenw\u00e4rt'ger Balsam", "tokens": ["All\u00b7ge\u00b7gen\u00b7w\u00e4rt'\u00b7ger", "Bal\u00b7sam"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.11": {"text": "Allheilender Natur.", "tokens": ["All\u00b7hei\u00b7len\u00b7der", "Na\u00b7tur", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Er schleicht aus dem Geb\u00fcsch hervor", "tokens": ["Er", "schleicht", "aus", "dem", "Ge\u00b7b\u00fcsch", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und reckt die Fl\u00fcgel \u2013 ach!", "tokens": ["Und", "reckt", "die", "Fl\u00fc\u00b7gel", "\u2013", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$(", "ITJ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Die Schwingkraft weggeschnitten \u2013", "tokens": ["Die", "Schwing\u00b7kraft", "weg\u00b7ge\u00b7schnit\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Hebt sich m\u00fchsam kaum", "tokens": ["Hebt", "sich", "m\u00fch\u00b7sam", "kaum"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Am Boden weg", "tokens": ["Am", "Bo\u00b7den", "weg"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Unw\u00fcrd'gem Raubbed\u00fcrfnis nach,", "tokens": ["Un\u00b7w\u00fcrd'\u00b7gem", "Raub\u00b7be\u00b7d\u00fcrf\u00b7nis", "nach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.18": {"text": "Und ruht tieftrauernd", "tokens": ["Und", "ruht", "tief\u00b7trau\u00b7ernd"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Auf dem niedern Fels am Bach;", "tokens": ["Auf", "dem", "nie\u00b7dern", "Fels", "am", "Bach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Er blickt zur Eich hinauf,", "tokens": ["Er", "blickt", "zur", "Eich", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Hinauf zum Himmel,", "tokens": ["Hin\u00b7auf", "zum", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Und eine Tr\u00e4ne f\u00fcllt sein hohes Aug.", "tokens": ["Und", "ei\u00b7ne", "Tr\u00e4\u00b7ne", "f\u00fcllt", "sein", "ho\u00b7hes", "Aug."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da kommt mutwillig durch die Myrten\u00e4ste", "tokens": ["Da", "kommt", "mut\u00b7wil\u00b7lig", "durch", "die", "Myr\u00b7ten\u00b7\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dahergerauscht ein Taubenpaar,", "tokens": ["Da\u00b7her\u00b7ge\u00b7rauscht", "ein", "Tau\u00b7ben\u00b7paar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00e4\u00dft sich herab und wandelt nickend", "tokens": ["L\u00e4\u00dft", "sich", "her\u00b7ab", "und", "wan\u00b7delt", "ni\u00b7ckend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "KON", "VVFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00dcber goldnen Sand am Bach", "tokens": ["\u00dc\u00b7ber", "gold\u00b7nen", "Sand", "am", "Bach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ruckt einander an;", "tokens": ["Und", "ruckt", "ein\u00b7an\u00b7der", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ihr r\u00f6tlich Auge buhlt umher,", "tokens": ["Ihr", "r\u00f6t\u00b7lich", "Au\u00b7ge", "buhlt", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erblickt den Innigtrauernden.", "tokens": ["Er\u00b7blickt", "den", "In\u00b7nig\u00b7trau\u00b7ern\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "Der Tauber schwingt neugiergesellig sich", "tokens": ["Der", "Tau\u00b7ber", "schwingt", "neu\u00b7gier\u00b7ge\u00b7sel\u00b7lig", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Zum nahen Busch und blickt", "tokens": ["Zum", "na\u00b7hen", "Busch", "und", "blickt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Mit Selbstgef\u00e4lligkeit ihn freundlich an.", "tokens": ["Mit", "Selbst\u00b7ge\u00b7f\u00e4l\u00b7lig\u00b7keit", "ihn", "freund\u00b7lich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "\u00bbdu trauerst\u00ab, liebelt er,", "tokens": ["\u00bb", "du", "trau\u00b7erst", "\u00ab", ",", "lie\u00b7belt", "er", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$(", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "\u00bbsei guten Mutes, Freund!", "tokens": ["\u00bb", "sei", "gu\u00b7ten", "Mu\u00b7tes", ",", "Freund", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VAFIN", "ADJA", "NN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Hast du zur ruhigen Gl\u00fcckseligkeit", "tokens": ["Hast", "du", "zur", "ru\u00b7hi\u00b7gen", "Gl\u00fcck\u00b7se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Nicht alles hier?", "tokens": ["Nicht", "al\u00b7les", "hier", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Kannst du dich nicht des goldnen Zweiges freun,", "tokens": ["Kannst", "du", "dich", "nicht", "des", "gold\u00b7nen", "Zwei\u00b7ges", "freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Der vor des Tages Glut dich sch\u00fctzt?", "tokens": ["Der", "vor", "des", "Ta\u00b7ges", "Glut", "dich", "sch\u00fctzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Kannst du der Abendsonne Schein", "tokens": ["Kannst", "du", "der", "A\u00b7bend\u00b7son\u00b7ne", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Auf weichem Moos am Bache nicht", "tokens": ["Auf", "wei\u00b7chem", "Moos", "am", "Ba\u00b7che", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die Brust entgegenheben?", "tokens": ["Die", "Brust", "ent\u00b7ge\u00b7gen\u00b7he\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Du wandelst durch der Blumen frischen Tau,", "tokens": ["Du", "wan\u00b7delst", "durch", "der", "Blu\u00b7men", "fri\u00b7schen", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Pfl\u00fcckst aus dem \u00dcberflu\u00df", "tokens": ["Pfl\u00fcckst", "aus", "dem", "\u00dc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.22": {"text": "Des Waldgeb\u00fcsches dir", "tokens": ["Des", "Wald\u00b7ge\u00b7b\u00fc\u00b7sches", "dir"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Gelegne Speise, letzest", "tokens": ["Ge\u00b7leg\u00b7ne", "Spei\u00b7se", ",", "let\u00b7zest"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Den leichten Durst am Silberquell", "tokens": ["Den", "leich\u00b7ten", "Durst", "am", "Sil\u00b7ber\u00b7quell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "O Freund, das wahre Gl\u00fcck", "tokens": ["O", "Freund", ",", "das", "wah\u00b7re", "Gl\u00fcck"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Ist die Gen\u00fcgsamkeit,", "tokens": ["Ist", "die", "Ge\u00b7n\u00fcg\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.27": {"text": "Und die Gen\u00fcgsamkeit", "tokens": ["Und", "die", "Ge\u00b7n\u00fcg\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Hat \u00fcberall genug.\u00ab", "tokens": ["Hat", "\u00fc\u00b7be\u00b7rall", "ge\u00b7nug", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "\u00bbo Weise!\u00ab sprach der Adler, und tief ernst", "tokens": ["\u00bb", "o", "Wei\u00b7se", "!", "\u00ab", "sprach", "der", "Ad\u00b7ler", ",", "und", "tief", "ernst"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "FM", "NN", "$.", "$(", "VVFIN", "ART", "NN", "$,", "KON", "ADJD", "ADJD"], "meter": "-+-+-+--++", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Versinkt er tiefer in sich selbst,", "tokens": ["Ver\u00b7sinkt", "er", "tie\u00b7fer", "in", "sich", "selbst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "\u00bbo Weisheit ! Du redst wie eine Taube!\u00ab", "tokens": ["\u00bb", "o", "Weis\u00b7heit", "!", "Du", "redst", "wie", "ei\u00b7ne", "Tau\u00b7be", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "NN", "$.", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein Adlersj\u00fcngling hob die Fl\u00fcgel", "tokens": ["Ein", "Ad\u00b7lers\u00b7j\u00fcng\u00b7ling", "hob", "die", "Fl\u00fc\u00b7gel"], "