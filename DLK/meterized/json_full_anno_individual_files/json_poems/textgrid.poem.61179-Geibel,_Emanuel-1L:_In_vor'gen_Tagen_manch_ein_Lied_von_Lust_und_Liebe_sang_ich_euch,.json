{"textgrid.poem.61179": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: In vor'gen Tagen manch ein Lied von Lust und Liebe sang ich euch,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In vor'gen Tagen manch ein Lied von Lust und Liebe sang ich euch,", "tokens": ["In", "vor'\u00b7gen", "Ta\u00b7gen", "manch", "ein", "Lied", "von", "Lust", "und", "Lie\u00b7be", "sang", "ich", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PIAT", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "So wie's zur sch\u00f6nen Rosenzeit der Vogel singt im Waldgestr\u00e4uch;", "tokens": ["So", "wie's", "zur", "sch\u00f6\u00b7nen", "Ro\u00b7sen\u00b7zeit", "der", "Vo\u00b7gel", "singt", "im", "Wald\u00b7ges\u00b7tr\u00e4uch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Die Jugend floh, die Lust verlosch, da stellt' ich alles Singen ein,", "tokens": ["Die", "Ju\u00b7gend", "floh", ",", "die", "Lust", "ver\u00b7losch", ",", "da", "stellt'", "ich", "al\u00b7les", "Sin\u00b7gen", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und alten Sagen forscht' ich nach in Spaniens Pomeranzenhain.", "tokens": ["Und", "al\u00b7ten", "Sa\u00b7gen", "forscht'", "ich", "nach", "in", "Spa\u00b7ni\u00b7ens", "Po\u00b7me\u00b7ran\u00b7zen\u00b7hain", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "APPR", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+----+-+-+", "measure": "unknown.measure.septa"}}, "stanza.2": {"line.1": {"text": "Da kam ein Beben in die Welt, hohlbrausend wuchs der Zeiten Sturm,", "tokens": ["Da", "kam", "ein", "Be\u00b7ben", "in", "die", "Welt", ",", "hohl\u00b7brau\u00b7send", "wuchs", "der", "Zei\u00b7ten", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,", "ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Die Eiche bog ihr knotig Haupt, in seinen Festen brach der Turm;", "tokens": ["Die", "Ei\u00b7che", "bog", "ihr", "kno\u00b7tig", "Haupt", ",", "in", "sei\u00b7nen", "Fes\u00b7ten", "brach", "der", "Turm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und als ich nun vom Pergament die Augen hob und sah umher,", "tokens": ["Und", "als", "ich", "nun", "vom", "Per\u00b7ga\u00b7ment", "die", "Au\u00b7gen", "hob", "und", "sah", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Da schien der Osten feuerrot, im Westen hing's gewitterschwer.", "tokens": ["Da", "schien", "der", "Os\u00b7ten", "feu\u00b7er\u00b7rot", ",", "im", "Wes\u00b7ten", "hin\u00b7g's", "ge\u00b7wit\u00b7ter\u00b7schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$,", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Und rings die V\u00f6lker sah ich stehn im Widerschein des Flammenlichts,", "tokens": ["Und", "rings", "die", "V\u00f6l\u00b7ker", "sah", "ich", "stehn", "im", "Wi\u00b7der\u00b7schein", "des", "Flam\u00b7men\u00b7lichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Gewappnet und erwartungsvoll, als harrten sie des Weltgerichts;", "tokens": ["Ge\u00b7wapp\u00b7net", "und", "er\u00b7war\u00b7tungs\u00b7voll", ",", "als", "harr\u00b7ten", "sie", "des", "Welt\u00b7ge\u00b7richts", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "$,", "KOUS", "ADJA", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Doch murrt' es auch nur dumpf und fern, ich sah, da\u00df \n                                    ", "tokens": ["Doch", "murrt'", "es", "auch", "nur", "dumpf", "und", "fern", ",", "ich", "sah", ",", "da\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,", "PPER", "VVFIN", "$,", "KOUS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Nacht und Licht, von Geist und Stoff, ein Kampf von Gott und Antichrist.", "tokens": ["Von", "Nacht", "und", "Licht", ",", "von", "Geist", "und", "Stoff", ",", "ein", "Kampf", "von", "Gott", "und", "An\u00b7ti\u00b7ch\u00b7rist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}}, "stanza.4": {"line.1": {"text": "Und m\u00e4chtig fa\u00dfte mich Begier, mitauszufechten solchen Streit,", "tokens": ["Und", "m\u00e4ch\u00b7tig", "fa\u00df\u00b7te", "mich", "Be\u00b7gier", ",", "mit\u00b7aus\u00b7zu\u00b7fech\u00b7ten", "sol\u00b7chen", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "NN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Doch was vermag ein einz'ger Arm, ein schwacher Arm in unsrer Zeit?", "tokens": ["Doch", "was", "ver\u00b7mag", "ein", "einz'\u00b7ger", "Arm", ",", "ein", "schwa\u00b7cher", "Arm", "in", "uns\u00b7rer", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Da sprach mein Herz: Es ist der Reim des S\u00e4ngers Wehr in Ernst und Scherz,", "tokens": ["Da", "sprach", "mein", "Herz", ":", "Es", "ist", "der", "Reim", "des", "S\u00e4n\u00b7gers", "Wehr", "in", "Ernst", "und", "Scherz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und da von Erz die Zeiten sind, so sei'n die Lieder auch von Erz.", "tokens": ["Und", "da", "von", "Erz", "die", "Zei\u00b7ten", "sind", ",", "so", "sei'n", "die", "Lie\u00b7der", "auch", "von", "Erz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "ART", "NN", "VAFIN", "$,", "ADV", "APPR", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Wohlauf, wohlauf denn, mein Gesang, und wandle klingend deinen Schritt!", "tokens": ["Wohl\u00b7auf", ",", "wohl\u00b7auf", "denn", ",", "mein", "Ge\u00b7sang", ",", "und", "wand\u00b7le", "klin\u00b7gend", "dei\u00b7nen", "Schritt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ADV", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Ich geb' als werten Talisman das Kreuz dir in die Schlachten mit;", "tokens": ["Ich", "geb'", "als", "wer\u00b7ten", "Ta\u00b7lis\u00b7man", "das", "Kreuz", "dir", "in", "die", "Schlach\u00b7ten", "mit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "VAFIN", "NE", "ART", "NN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Der Freiheit R\u00f6slein hell im Schild, des Geistes Schwert in fester Hand,", "tokens": ["Der", "Frei\u00b7heit", "R\u00f6s\u00b7lein", "hell", "im", "Schild", ",", "des", "Geis\u00b7tes", "Schwert", "in", "fes\u00b7ter", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "APPRART", "NN", "$,", "ART", "NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "So schreit, ein wackrer Rittersmann, geharnischt durch das deutsche Land.", "tokens": ["So", "schreit", ",", "ein", "wack\u00b7rer", "Rit\u00b7ters\u00b7mann", ",", "ge\u00b7har\u00b7nischt", "durch", "das", "deut\u00b7sche", "Land", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ART", "ADJA", "NN", "$,", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Und l\u00e4chelt ihr, da\u00df meine Brust so sicheres Vertrauen hegt,", "tokens": ["Und", "l\u00e4\u00b7chelt", "ihr", ",", "da\u00df", "mei\u00b7ne", "Brust", "so", "si\u00b7che\u00b7res", "Ver\u00b7trau\u00b7en", "hegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Bedenkt: Es ist das Dichterherz die Glocke, die die Stunde schl\u00e4gt;", "tokens": ["Be\u00b7denkt", ":", "Es", "ist", "das", "Dich\u00b7ter\u00b7herz", "die", "Glo\u00b7cke", ",", "die", "die", "Stun\u00b7de", "schl\u00e4gt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "In ihm versammelt sich der Hall, der murmelnd l\u00e4uft von Haus zu Haus,", "tokens": ["In", "ihm", "ver\u00b7sam\u00b7melt", "sich", "der", "Hall", ",", "der", "mur\u00b7melnd", "l\u00e4uft", "von", "Haus", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PRF", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und vollen Schwunges sendet's ihn melodisch in die Welt hinaus.", "tokens": ["Und", "vol\u00b7len", "Schwun\u00b7ges", "sen\u00b7det's", "ihn", "me\u00b7lo\u00b7disch", "in", "die", "Welt", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.7": {"line.1": {"text": "In vor'gen Tagen manch ein Lied von Lust und Liebe sang ich euch,", "tokens": ["In", "vor'\u00b7gen", "Ta\u00b7gen", "manch", "ein", "Lied", "von", "Lust", "und", "Lie\u00b7be", "sang", "ich", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PIAT", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "So wie's zur sch\u00f6nen Rosenzeit der Vogel singt im Waldgestr\u00e4uch;", "tokens": ["So", "wie's", "zur", "sch\u00f6\u00b7nen", "Ro\u00b7sen\u00b7zeit", "der", "Vo\u00b7gel", "singt", "im", "Wald\u00b7ges\u00b7tr\u00e4uch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Die Jugend floh, die Lust verlosch, da stellt' ich alles Singen ein,", "tokens": ["Die", "Ju\u00b7gend", "floh", ",", "die", "Lust", "ver\u00b7losch", ",", "da", "stellt'", "ich", "al\u00b7les", "Sin\u00b7gen", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und alten Sagen forscht' ich nach in Spaniens Pomeranzenhain.", "tokens": ["Und", "al\u00b7ten", "Sa\u00b7gen", "forscht'", "ich", "nach", "in", "Spa\u00b7ni\u00b7ens", "Po\u00b7me\u00b7ran\u00b7zen\u00b7hain", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "APPR", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+----+-+-+", "measure": "unknown.measure.septa"}}, "stanza.8": {"line.1": {"text": "Da kam ein Beben in die Welt, hohlbrausend wuchs der Zeiten Sturm,", "tokens": ["Da", "kam", "ein", "Be\u00b7ben", "in", "die", "Welt", ",", "hohl\u00b7brau\u00b7send", "wuchs", "der", "Zei\u00b7ten", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,", "ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Die Eiche bog ihr knotig Haupt, in seinen Festen brach der Turm;", "tokens": ["Die", "Ei\u00b7che", "bog", "ihr", "kno\u00b7tig", "Haupt", ",", "in", "sei\u00b7nen", "Fes\u00b7ten", "brach", "der", "Turm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und als ich nun vom Pergament die Augen hob und sah umher,", "tokens": ["Und", "als", "ich", "nun", "vom", "Per\u00b7ga\u00b7ment", "die", "Au\u00b7gen", "hob", "und", "sah", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Da schien der Osten feuerrot, im Westen hing's gewitterschwer.", "tokens": ["Da", "schien", "der", "Os\u00b7ten", "feu\u00b7er\u00b7rot", ",", "im", "Wes\u00b7ten", "hin\u00b7g's", "ge\u00b7wit\u00b7ter\u00b7schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$,", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.9": {"line.1": {"text": "Und rings die V\u00f6lker sah ich stehn im Widerschein des Flammenlichts,", "tokens": ["Und", "rings", "die", "V\u00f6l\u00b7ker", "sah", "ich", "stehn", "im", "Wi\u00b7der\u00b7schein", "des", "Flam\u00b7men\u00b7lichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Gewappnet und erwartungsvoll, als harrten sie des Weltgerichts;", "tokens": ["Ge\u00b7wapp\u00b7net", "und", "er\u00b7war\u00b7tungs\u00b7voll", ",", "als", "harr\u00b7ten", "sie", "des", "Welt\u00b7ge\u00b7richts", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "$,", "KOUS", "ADJA", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Doch murrt' es auch nur dumpf und fern, ich sah, da\u00df \n                                    ", "tokens": ["Doch", "murrt'", "es", "auch", "nur", "dumpf", "und", "fern", ",", "ich", "sah", ",", "da\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,", "PPER", "VVFIN", "$,", "KOUS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Nacht und Licht, von Geist und Stoff, ein Kampf von Gott und Antichrist.", "tokens": ["Von", "Nacht", "und", "Licht", ",", "von", "Geist", "und", "Stoff", ",", "ein", "Kampf", "von", "Gott", "und", "An\u00b7ti\u00b7ch\u00b7rist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}}, "stanza.10": {"line.1": {"text": "Und m\u00e4chtig fa\u00dfte mich Begier, mitauszufechten solchen Streit,", "tokens": ["Und", "m\u00e4ch\u00b7tig", "fa\u00df\u00b7te", "mich", "Be\u00b7gier", ",", "mit\u00b7aus\u00b7zu\u00b7fech\u00b7ten", "sol\u00b7chen", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "NN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Doch was vermag ein einz'ger Arm, ein schwacher Arm in unsrer Zeit?", "tokens": ["Doch", "was", "ver\u00b7mag", "ein", "einz'\u00b7ger", "Arm", ",", "ein", "schwa\u00b7cher", "Arm", "in", "uns\u00b7rer", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Da sprach mein Herz: Es ist der Reim des S\u00e4ngers Wehr in Ernst und Scherz,", "tokens": ["Da", "sprach", "mein", "Herz", ":", "Es", "ist", "der", "Reim", "des", "S\u00e4n\u00b7gers", "Wehr", "in", "Ernst", "und", "Scherz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und da von Erz die Zeiten sind, so sei'n die Lieder auch von Erz.", "tokens": ["Und", "da", "von", "Erz", "die", "Zei\u00b7ten", "sind", ",", "so", "sei'n", "die", "Lie\u00b7der", "auch", "von", "Erz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "ART", "NN", "VAFIN", "$,", "ADV", "APPR", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.11": {"line.1": {"text": "Wohlauf, wohlauf denn, mein Gesang, und wandle klingend deinen Schritt!", "tokens": ["Wohl\u00b7auf", ",", "wohl\u00b7auf", "denn", ",", "mein", "Ge\u00b7sang", ",", "und", "wand\u00b7le", "klin\u0