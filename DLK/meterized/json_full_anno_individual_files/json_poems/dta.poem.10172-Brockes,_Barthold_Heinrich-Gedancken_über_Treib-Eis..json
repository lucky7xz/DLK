{"dta.poem.10172": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gedancken \u00fcber Treib-Eis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So wie ein schneller Blitz, aus fahrender Carossen", "tokens": ["So", "wie", "ein", "schnel\u00b7ler", "Blitz", ",", "aus", "fah\u00b7ren\u00b7der", "Ca\u00b7ros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bestrahltem Spiegel-Glas\u2019, uns schnell ins Ange", "tokens": ["Be\u00b7strahl\u00b7tem", "Spie\u00b7gel\u00b7Glas'", ",", "uns", "schnell", "ins", "An\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So blitzt ein glattes Eis, wenn es die Sonn\u2019 erhellt,", "tokens": ["So", "blitzt", "ein", "glat\u00b7tes", "Eis", ",", "wenn", "es", "die", "Sonn'", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und es im regen Flu\u00df gemach herab geflossen.", "tokens": ["Und", "es", "im", "re\u00b7gen", "Flu\u00df", "ge\u00b7mach", "her\u00b7ab", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "ADJA", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "So wie in einer heitern Nacht,", "tokens": ["So", "wie", "in", "ei\u00b7ner", "hei\u00b7tern", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wann alles durch der Schatten Heer verdunckelt,", "tokens": ["Wann", "al\u00b7les", "durch", "der", "Schat\u00b7ten", "Heer", "ver\u00b7dun\u00b7ckelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Stern der ersten Gr\u00f6ss\u2019, in einer reinen Pracht,", "tokens": ["Ein", "Stern", "der", "ers\u00b7ten", "Gr\u00f6ss'", ",", "in", "ei\u00b7ner", "rei\u00b7nen", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mehr Blitzt und strahlt, als gl\u00e4ntzt und funckelt;", "tokens": ["Mehr", "Blitzt", "und", "strahlt", ",", "als", "gl\u00e4ntzt", "und", "fun\u00b7ckelt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "VVFIN", "$,", "KOUS", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So scheinen offt bestrahltes Eises Schollen,", "tokens": ["So", "schei\u00b7nen", "offt", "be\u00b7strahl\u00b7tes", "Ei\u00b7ses", "Schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn sie im schweren Strohm beweglich vorw\u00e4rts rollen,", "tokens": ["Wenn", "sie", "im", "schwe\u00b7ren", "Strohm", "be\u00b7weg\u00b7lich", "vor\u00b7w\u00e4rts", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bey Tag\u2019 ein strahlendes Gestirn zu seyn.", "tokens": ["Bey", "Tag'", "ein", "strah\u00b7len\u00b7des", "Ge\u00b7stirn", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Welch heller Glantz, der mir ins Auge steigt,", "tokens": ["Welch", "hel\u00b7ler", "Glantz", ",", "der", "mir", "ins", "Au\u00b7ge", "steigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Aus diesem in die H\u00f6h\u2019 geschobnen glatten Eise,", "tokens": ["Aus", "die\u00b7sem", "in", "die", "H\u00f6h'", "ge\u00b7schob\u00b7nen", "glat\u00b7ten", "Ei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "APPR", "ART", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mir, ihres Sch\u00f6pfers Macht zum Preise,", "tokens": ["Mir", ",", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "Macht", "zum", "Prei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die Herrlichkeit des Lichts der Sonnen zeigt.", "tokens": ["Die", "Herr\u00b7lich\u00b7keit", "des", "Lichts", "der", "Son\u00b7nen", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}