{"textgrid.poem.48845": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "70. An Valerien", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Itzt hat Latona gleich das zweimal sechste Mal", "tokens": ["Itzt", "hat", "La\u00b7to\u00b7na", "gleich", "das", "zwei\u00b7mal", "sechs\u00b7te", "Mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NE", "ADV", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ihr Silber voll gemacht, und Delius ingleichen,", "tokens": ["ihr", "Sil\u00b7ber", "voll", "ge\u00b7macht", ",", "und", "De\u00b7lius", "in\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVPP", "$,", "KON", "NE", "ADJD", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "nach dem er ist gerannt durch alle Himmelszeichen,", "tokens": ["nach", "dem", "er", "ist", "ge\u00b7rannt", "durch", "al\u00b7le", "Him\u00b7mels\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "f\u00e4ngt fornen wieder an die Tage seiner Zal,", "tokens": ["f\u00e4ngt", "for\u00b7nen", "wie\u00b7der", "an", "die", "Ta\u00b7ge", "sei\u00b7ner", "Zal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "seit mir, ", "tokens": ["seit", "mir", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "in mein Gesichte fiel, das nun fast will verbleichen.", "tokens": ["in", "mein", "Ge\u00b7sich\u00b7te", "fiel", ",", "das", "nun", "fast", "will", "ver\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach wol mir von der Zeit, mir aller Wolfart reichen,", "tokens": ["Ach", "wol", "mir", "von", "der", "Zeit", ",", "mir", "al\u00b7ler", "Wol\u00b7fart", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PPER", "APPR", "ART", "NN", "$,", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "es reden es f\u00fcr mich See, Feld, Wald, Berg und Tal.", "tokens": ["es", "re\u00b7den", "es", "f\u00fcr", "mich", "See", ",", "Feld", ",", "Wald", ",", "Berg", "und", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+-++-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Tage, der sind viel, viel, viel der s\u00fc\u00dfen Stunden,", "tokens": ["Der", "Ta\u00b7ge", ",", "der", "sind", "viel", ",", "viel", ",", "viel", "der", "s\u00fc\u00b7\u00dfen", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "$,", "ADV", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "doch viel mehr ist der Lust, die ich um dich empfunden,", "tokens": ["doch", "viel", "mehr", "ist", "der", "Lust", ",", "die", "ich", "um", "dich", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "o du mein langer Preis. Nun, da ich scheiden soll,", "tokens": ["o", "du", "mein", "lan\u00b7ger", "Preis", ".", "Nun", ",", "da", "ich", "schei\u00b7den", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "PPER", "PPOSAT", "ADJA", "NN", "$.", "ADV", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "setz' ich der Freuden Zal entgegen meiner Qualen:", "tokens": ["setz'", "ich", "der", "Freu\u00b7den", "Zal", "ent\u00b7ge\u00b7gen", "mei\u00b7ner", "Qua\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich mach' es, wie ichs will, so mangelt mirs an Zalen", "tokens": ["Ich", "mach'", "es", ",", "wie", "ichs", "will", ",", "so", "man\u00b7gelt", "mirs", "an", "Za\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PIS", "VMFIN", "$,", "ADV", "VVFIN", "NE", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine