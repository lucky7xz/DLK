{"dta.poem.2256": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lobgedichte  \n auf die Pamela.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wollt ihr die Gottheit f\u00fcrchten lernen;", "tokens": ["Wollt", "ihr", "die", "Got\u00b7theit", "f\u00fcrch\u00b7ten", "ler\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Hochmuth meiden, Laster fliehn,", "tokens": ["Den", "Hoch\u00b7muth", "mei\u00b7den", ",", "Las\u00b7ter", "fliehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und euch von ihrer Straf\u2019 entfernen;", "tokens": ["Und", "euch", "von", "ih\u00b7rer", "Straf'", "ent\u00b7fer\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wollt ihr die Kinder wohl erziehn;", "tokens": ["Wollt", "ihr", "die", "Kin\u00b7der", "wohl", "er\u00b7ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wollt ihr die Aeltern ehren, lieben;", "tokens": ["Wollt", "ihr", "die", "A\u00b7el\u00b7tern", "eh\u00b7ren", ",", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die N\u00e4chstenlieb\u2019 am D\u00fcrftgen \u00fcben;", "tokens": ["Die", "N\u00e4chs\u00b7ten\u00b7lieb'", "am", "D\u00fcrft\u00b7gen", "\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Beym Reichthum reich, durch Wohlthun, seyn;", "tokens": ["Beym", "Reicht\u00b7hum", "reich", ",", "durch", "Wohl\u00b7thun", ",", "seyn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "APPR", "NN", "$,", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wollt ihr so Freund- als Herrschafts-Pflichten,", "tokens": ["Wollt", "ihr", "so", "Freun\u00b7d", "als", "Herr\u00b7schafts\u00b7Pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "TRUNC", "KON", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "Auch treuer Diener Amt, verrichten;", "tokens": ["Auch", "treu\u00b7er", "Die\u00b7ner", "Amt", ",", "ver\u00b7rich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wollt ihr euch eures Lebens freun;", "tokens": ["Wollt", "ihr", "euch", "eu\u00b7res", "Le\u00b7bens", "freun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wollt ihr ein gut Exempel geben;", "tokens": ["Wollt", "ihr", "ein", "gut", "Ex\u00b7em\u00b7pel", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Auch in der Ehe gl\u00fccklich leben;", "tokens": ["Auch", "in", "der", "E\u00b7he", "gl\u00fcck\u00b7lich", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Kurz: will ein jeder auf der Erden", "tokens": ["Kurz", ":", "will", "ein", "je\u00b7der", "auf", "der", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$.", "VMFIN", "ART", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Vergn\u00fcgt, geehrt, geliebet werden;", "tokens": ["Ver\u00b7gn\u00fcgt", ",", "ge\u00b7ehrt", ",", "ge\u00b7lie\u00b7bet", "wer\u00b7den", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Die Lehr\u2019 ist, nebst dem Beyspiel, da:", "tokens": ["Die", "Lehr'", "ist", ",", "nebst", "dem", "Bey\u00b7spiel", ",", "da", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "APPR", "ART", "NN", "$,", "KOUS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Man les\u2019 und folge Pamela.", "tokens": ["Man", "les'", "und", "fol\u00b7ge", "Pa\u00b7me\u00b7la", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}