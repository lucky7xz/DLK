{"dta.poem.12047": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "67.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du den Formeln siehst ins Herz, nicht aufs Gewand,", "tokens": ["Wenn", "du", "den", "For\u00b7meln", "siehst", "ins", "Herz", ",", "nicht", "aufs", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPRART", "NN", "$,", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Formeln, die der Geist zu seinem Spiel erfand;", "tokens": ["Den", "For\u00b7meln", ",", "die", "der", "Geist", "zu", "sei\u00b7nem", "Spiel", "er\u00b7fand", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So kannst du dir getrost aus allerlei Systemen", "tokens": ["So", "kannst", "du", "dir", "ge\u00b7trost", "aus", "al\u00b7ler\u00b7lei", "Sys\u00b7te\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVPP", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-++--", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Den Kern der Nahrung, wie aus H\u00fclsen K\u00f6rner, nehmen.", "tokens": ["Den", "Kern", "der", "Nah\u00b7rung", ",", "wie", "aus", "H\u00fcl\u00b7sen", "K\u00f6r\u00b7ner", ",", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PWAV", "APPR", "NN", "NE", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie sie's begr\u00fcnden dir, entwickeln und ableiten,", "tokens": ["Wie", "sie's", "be\u00b7gr\u00fcn\u00b7den", "dir", ",", "ent\u00b7wi\u00b7ckeln", "und", "ab\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PPER", "$,", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie sind im Sinn dir eins, die sich in Worten streiten.", "tokens": ["Sie", "sind", "im", "Sinn", "dir", "eins", ",", "die", "sich", "in", "Wor\u00b7ten", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PPER", "PIS", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie f\u00f6rdern nur zu Tag den Vorrath ihrer Brust,", "tokens": ["Sie", "f\u00f6r\u00b7dern", "nur", "zu", "Tag", "den", "Vor\u00b7rath", "ih\u00b7rer", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den aufzunehmen du in dir schon haben must.", "tokens": ["Den", "auf\u00b7zu\u00b7neh\u00b7men", "du", "in", "dir", "schon", "ha\u00b7ben", "must", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPER", "ADV", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}