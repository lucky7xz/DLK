{"textgrid.poem.37093": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die k\u00fcnftige Geliebte", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Br\u00e4chte der n\u00e4chste Fr\u00fchling meinem Arm dich,", "tokens": ["Br\u00e4ch\u00b7te", "der", "n\u00e4chs\u00b7te", "Fr\u00fch\u00b7ling", "mei\u00b7nem", "Arm", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PPOSAT", "NN", "PPER", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "T\u00f6nten V\u00f6gel aus Bl\u00fcthen mir das Brautlied;", "tokens": ["T\u00f6n\u00b7ten", "V\u00f6\u00b7gel", "aus", "Bl\u00fc\u00b7then", "mir", "das", "Braut\u00b7lied", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "PPER", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dann, Geliebte, h\u00e4tt' ich den Himmel schon auf Erden gefunden!", "tokens": ["Dann", ",", "Ge\u00b7lieb\u00b7te", ",", "h\u00e4tt'", "ich", "den", "Him\u00b7mel", "schon", "auf", "Er\u00b7den", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VAFIN", "PPER", "ART", "NN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "--+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "G\u00f6tter! Sie wird die Welt zum Eden zaubern,", "tokens": ["G\u00f6t\u00b7ter", "!", "Sie", "wird", "die", "Welt", "zum", "E\u00b7den", "zau\u00b7bern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wird die Fluren in G\u00e4rten Gottes wandeln,", "tokens": ["Wird", "die", "Flu\u00b7ren", "in", "G\u00e4r\u00b7ten", "Got\u00b7tes", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wird, auf meinem Schoo\u00dfe gewiegt, den Fr\u00fchlingsabend befl\u00fcgeln!", "tokens": ["Wird", ",", "auf", "mei\u00b7nem", "Schoo\u00b7\u00dfe", "ge\u00b7wiegt", ",", "den", "Fr\u00fch\u00b7lings\u00b7a\u00b7bend", "be\u00b7fl\u00fc\u00b7geln", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,", "ART", "NN", "VVINF", "$."], "meter": "--+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "G\u00f6tter! Ich werd an ihrer Brust entschlummern,", "tokens": ["G\u00f6t\u00b7ter", "!", "Ich", "werd", "an", "ih\u00b7rer", "Brust", "ent\u00b7schlum\u00b7mern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Werd im Traume mit ihrem Busen spielen;", "tokens": ["Werd", "im", "Trau\u00b7me", "mit", "ih\u00b7rem", "Bu\u00b7sen", "spie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Werde, wachgeschimmert vom May, in ihren Armen erwachen!", "tokens": ["Wer\u00b7de", ",", "wach\u00b7ge\u00b7schim\u00b7mert", "vom", "May", ",", "in", "ih\u00b7ren", "Ar\u00b7men", "er\u00b7wa\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "VVFIN", "APPRART", "NN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+--+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Soll ich dich finden? Komm, du Engel Gottes,", "tokens": ["Soll", "ich", "dich", "fin\u00b7den", "?", "Komm", ",", "du", "En\u00b7gel", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$.", "VVFIN", "$,", "PPER", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Komm, mein Leben zu heitern! Wenig Freuden", "tokens": ["Komm", ",", "mein", "Le\u00b7ben", "zu", "hei\u00b7tern", "!", "We\u00b7nig", "Freu\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$.", "PIAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Spro\u00dfen auf den Ufern des Lebens! Komm, mein Leben zu heitern!", "tokens": ["Spro\u00b7\u00dfen", "auf", "den", "U\u00b7fern", "des", "Le\u00b7bens", "!", "Komm", ",", "mein", "Le\u00b7ben", "zu", "hei\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$.", "VVFIN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-+-+--+-", "measure": "trochaic.septa.relaxed"}}, "stanza.5": {"line.1": {"text": "Br\u00e4chte der n\u00e4chste Fr\u00fchling meinem Arm dich,", "tokens": ["Br\u00e4ch\u00b7te", "der", "n\u00e4chs\u00b7te", "Fr\u00fch\u00b7ling", "mei\u00b7nem", "Arm", "dich", ","], "token_info": ["word", "word", "word", "word",