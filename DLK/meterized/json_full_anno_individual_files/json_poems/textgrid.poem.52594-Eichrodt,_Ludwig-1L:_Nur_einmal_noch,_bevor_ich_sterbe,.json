{"textgrid.poem.52594": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nur einmal noch, bevor ich sterbe,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur einmal noch, bevor ich sterbe,", "tokens": ["Nur", "ein\u00b7mal", "noch", ",", "be\u00b7vor", "ich", "ster\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "M\u00f6cht ich die Holde wieder sehen,", "tokens": ["M\u00f6cht", "ich", "die", "Hol\u00b7de", "wie\u00b7der", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die einst mir Liebe zugeschworen,", "tokens": ["Die", "einst", "mir", "Lie\u00b7be", "zu\u00b7ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dann verr\u00e4therisch entfloh!", "tokens": ["Und", "dann", "ver\u00b7r\u00e4\u00b7the\u00b7risch", "ent\u00b7floh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es war ein Weib so r\u00e4thselhaften", "tokens": ["Es", "war", "ein", "Weib", "so", "r\u00e4th\u00b7sel\u00b7haf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und s\u00fc\u00dfen Wesens doch so g\u00f6ttlich,", "tokens": ["Und", "s\u00fc\u00b7\u00dfen", "We\u00b7sens", "doch", "so", "g\u00f6tt\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Aus den geheimni\u00dftiefen Augen", "tokens": ["Aus", "den", "ge\u00b7heim\u00b7ni\u00df\u00b7tie\u00b7fen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Werd ich verwundet, nimmer klug.", "tokens": ["Werd", "ich", "ver\u00b7wun\u00b7det", ",", "nim\u00b7mer", "klug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wohin, warum ist sie geflohen,", "tokens": ["Wo\u00b7hin", ",", "wa\u00b7rum", "ist", "sie", "ge\u00b7flo\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Leidenschaft nur war und Liebe?", "tokens": ["Die", "Lei\u00b7den\u00b7schaft", "nur", "war", "und", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O d\u00fcrft ich, einmal noch, erschauen", "tokens": ["O", "d\u00fcrft", "ich", ",", "ein\u00b7mal", "noch", ",", "er\u00b7schau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "VMFIN", "PPER", "$,", "ADV", "ADV", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Blumenhaupt, mein Elfenkind!", "tokens": ["Mein