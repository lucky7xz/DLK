{"textgrid.poem.34997": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Diesseits und jenseits des Rheins", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sanftes Rasen, wildes Kosen,", "tokens": ["Sanf\u00b7tes", "Ra\u00b7sen", ",", "wil\u00b7des", "Ko\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00e4ndeln mit den gl\u00fchnden Rosen,", "tokens": ["T\u00e4n\u00b7deln", "mit", "den", "gl\u00fchn\u00b7den", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Holde L\u00fcge, s\u00fc\u00dfer Dunst,", "tokens": ["Hol\u00b7de", "L\u00fc\u00b7ge", ",", "s\u00fc\u00b7\u00dfer", "Dunst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Veredlung roher Brunst,", "tokens": ["Die", "Ver\u00b7ed\u00b7lung", "ro\u00b7her", "Brunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kurz, der Liebe heitre Kunst \u2013", "tokens": ["Kurz", ",", "der", "Lie\u00b7be", "heit\u00b7re", "Kunst", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da seid Meister ihr, Franzosen!", "tokens": ["Da", "seid", "Meis\u00b7ter", "ihr", ",", "Fran\u00b7zo\u00b7sen", "!"], "token_info": ["wor