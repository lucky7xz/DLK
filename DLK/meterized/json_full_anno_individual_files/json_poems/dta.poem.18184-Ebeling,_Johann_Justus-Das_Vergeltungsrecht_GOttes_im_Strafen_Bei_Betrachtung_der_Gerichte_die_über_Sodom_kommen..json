{"dta.poem.18184": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Das  \n Vergeltungsrecht GOttes  \n  im Strafen  \n Bei Betrachtung der Gerichte die \u00fcber  \n Sodom kommen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die gerechten Strafgerichte g\u00f6ttlicher", "tokens": ["Die", "ge\u00b7rech\u00b7ten", "Straf\u00b7ge\u00b7rich\u00b7te", "g\u00f6tt\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Gerechtigkeit,", "tokens": ["Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Kommen immer \u00fcber B\u00f6se, nach", "tokens": ["Kom\u00b7men", "im\u00b7mer", "\u00fc\u00b7ber", "B\u00f6\u00b7se", ",", "nach"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ADV", "APPR", "NN", "$,", "APPR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "verflo\u00dfner Gnaden Zeit,", "tokens": ["ver\u00b7flo\u00df\u00b7ner", "Gna\u00b7den", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da GOtt ihnen das vergilt, was sie gegen ihn ver-", "tokens": ["Da", "Gott", "ih\u00b7nen", "das", "ver\u00b7gilt", ",", "was", "sie", "ge\u00b7gen", "ihn", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "PDS", "VVFIN", "$,", "PRELS", "PPER", "APPR", "PPER", "TRUNC"], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "brochen,", "tokens": ["bro\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Da sie durch die Bosheit sich, selbst das Urtheil", "tokens": ["Da", "sie", "durch", "die", "Bos\u00b7heit", "sich", ",", "selbst", "das", "Ur\u00b7theil"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PRF", "$,", "ADV", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "schon gesprochen.", "tokens": ["schon", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Wie die S\u00fcnde; so die Straffe: da\u00df ist das Ver-", "tokens": ["Wie", "die", "S\u00fcn\u00b7de", ";", "so", "die", "Straf\u00b7fe", ":", "da\u00df", "ist", "das", "Ver"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$.", "ADV", "ART", "NN", "$.", "KOUS", "VAFIN", "ART", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "geltungsrecht,", "tokens": ["gel\u00b7tungs\u00b7recht", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Welches ", "tokens": ["Wel\u00b7ches"], "token_info": ["word"], "pos": ["PWS"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "S\u00fcnden-Knecht", "tokens": ["S\u00fcn\u00b7den\u00b7Knecht"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Durch gerechte Rache bringt, da\u00df man dabei kl\u00e4r-", "tokens": ["Durch", "ge\u00b7rech\u00b7te", "Ra\u00b7che", "bringt", ",", "da\u00df", "man", "da\u00b7bei", "kl\u00e4r"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PIS", "PAV", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "lich siehet,", "tokens": ["lich", "sie\u00b7het", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Wie des H\u00f6chsten Machtgericht, S\u00fcnder so zur", "tokens": ["Wie", "des", "H\u00f6chs\u00b7ten", "Macht\u00b7ge\u00b7richt", ",", "S\u00fcn\u00b7der", "so", "zur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "NN", "ADV", "APPRART"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Straffe ziehet", "tokens": ["Straf\u00b7fe", "zie\u00b7het"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Nach der Aehnlichkeit der Thaten; wie es oft den", "tokens": ["Nach", "der", "A\u00b7ehn\u00b7lich\u00b7keit", "der", "Tha\u00b7ten", ";", "wie", "es", "oft", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$.", "PWAV", "PPER", "ADV", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Lohn so giebt,", "tokens": ["Lohn", "so", "giebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Denen b\u00f6sen Leibes-Gliedern, die die S\u00fcnde aus-", "tokens": ["De\u00b7nen", "b\u00f6\u00b7sen", "Lei\u00b7bes\u00b7Glie\u00b7dern", ",", "die", "die", "S\u00fcn\u00b7de", "aus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.20": {"text": "ge\u00fcbt.", "tokens": ["ge\u00b7\u00fcbt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "Ofte straft ", "tokens": ["Of\u00b7te", "straft"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "geschehen,", "tokens": ["ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Oft zu einer solchen Zeit, da die Welt das B\u00f6\u00df", "tokens": ["Oft", "zu", "ei\u00b7ner", "sol\u00b7chen", "Zeit", ",", "da", "die", "Welt", "das", "B\u00f6\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "PIAT", "NN", "$,", "KOUS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "gesehen:", "tokens": ["ge\u00b7se\u00b7hen", ":"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Ofte wird dasselbe Uebel, wiederum an dem voll-", "tokens": ["Of\u00b7te", "wird", "das\u00b7sel\u00b7be", "Ue\u00b7bel", ",", "wie\u00b7de\u00b7rum", "an", "dem", "voll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PDAT", "NN", "$,", "ADV", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.26": {"text": "bracht,", "tokens": ["bracht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.27": {"text": "Und auf seinen Kopf vergolten, der dasselbe aus-", "tokens": ["Und", "auf", "sei\u00b7nen", "Kopf", "ver\u00b7gol\u00b7ten", ",", "der", "das\u00b7sel\u00b7be", "aus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "PDAT", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.28": {"text": "gedacht:", "tokens": ["ge\u00b7dacht", ":"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.29": {"text": "Wer dem N\u00e4chsten Gruben gr\u00e4bt, mu\u00df oft selbst", "tokens": ["Wer", "dem", "N\u00e4chs\u00b7ten", "Gru\u00b7ben", "gr\u00e4bt", ",", "mu\u00df", "oft", "selbst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,", "VMFIN", "ADV", "ADV"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.30": {"text": "darin versinken,", "tokens": ["da\u00b7rin", "ver\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.31": {"text": "Wer den Bruder Angel legt, mu\u00df oft selbsten da-", "tokens": ["Wer", "den", "Bru\u00b7der", "An\u00b7gel", "legt", ",", "mu\u00df", "oft", "selbs\u00b7ten", "da"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "NN", "VVFIN", "$,", "VMFIN", "ADV", "ADJA", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.32": {"text": "von hinken;", "tokens": ["von", "hin\u00b7ken", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.33": {"text": "Wer der Unschuld Bande schmiedet, die nichts \u00fcbels", "tokens": ["Wer", "der", "Un\u00b7schuld", "Ban\u00b7de", "schmie\u00b7det", ",", "die", "nichts", "\u00fc\u00b7bels"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "NN", "VVFIN", "$,", "PRELS", "PIS", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.34": {"text": "hat gethan,", "tokens": ["hat", "ge\u00b7than", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.35": {"text": "Dem sind oft dieselben Fesseln, selbst zur Straffe", "tokens": ["Dem", "sind", "oft", "die\u00b7sel\u00b7ben", "Fes\u00b7seln", ",", "selbst", "zur", "Straf\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "PDAT", "NN", "$,", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.36": {"text": "angethan.", "tokens": ["an\u00b7ge\u00b7than", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.37": {"text": "Da\u00df ist das Vergeltungsrecht, wunderbahrer Straf-", "tokens": ["Da\u00df", "ist", "das", "Ver\u00b7gel\u00b7tungs\u00b7recht", ",", "wun\u00b7der\u00b7bah\u00b7rer", "Straf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "VAFIN", "ART", "NN", "$,", "ADJA", "TRUNC"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.38": {"text": "gerichte,", "tokens": ["ge\u00b7rich\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.39": {"text": "Dadurch er die S\u00fcnde tilgt, und das B\u00f6se macht", "tokens": ["Da\u00b7durch", "er", "die", "S\u00fcn\u00b7de", "tilgt", ",", "und", "das", "B\u00f6\u00b7se", "macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "zu nichte.", "tokens": ["zu", "nich\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKA", "PIS", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.41": {"text": "In der Welt sind viel Exempel, daran man mit", "tokens": ["In", "der", "Welt", "sind", "viel", "Ex\u00b7em\u00b7pel", ",", "da\u00b7ran", "man", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PIAT", "NN", "$,", "PAV", "PIS", "APPR"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.42": {"text": "Schrekken lernt,", "tokens": ["Schrek\u00b7ken", "lernt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.43": {"text": "Wie es solchen S\u00fcnder gehet, der sich erst von", "tokens": ["Wie", "es", "sol\u00b7chen", "S\u00fcn\u00b7der", "ge\u00b7het", ",", "der", "sich", "erst", "von"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVFIN", "$,", "PRELS", "PRF", "ADV", "APPR"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.44": {"text": "Gott entfernt,", "tokens": ["Gott", "ent\u00b7fernt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.45": {"text": "Und durch b\u00f6se Lust verw\u00f6hnt, in dem Laster Ko-", "tokens": ["Und", "durch", "b\u00f6\u00b7se", "Lust", "ver\u00b7w\u00f6hnt", ",", "in", "dem", "Las\u00b7ter", "Ko"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "$,", "APPR", "ART", "NN", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.46": {"text": "the w\u00fchlet,", "tokens": ["the", "w\u00fch\u00b7let", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.47": {"text": "Der denn wie er hat gethan, auch gerechte Straffe", "tokens": ["Der", "denn", "wie", "er", "hat", "ge\u00b7than", ",", "auch", "ge\u00b7rech\u00b7te", "Straf\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "KON", "PWAV", "PPER", "VAFIN", "VVPP", "$,", "ADV", "ADJA", "NN"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.48": {"text": "f\u00fchlet.", "tokens": ["f\u00fch\u00b7let", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.49": {"text": "Nur ein Beispiel anzusehen, denkt nach Sodoma", "tokens": ["Nur", "ein", "Bei\u00b7spiel", "an\u00b7zu\u00b7se\u00b7hen", ",", "denkt", "nach", "So\u00b7do\u00b7ma"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVIZU", "$,", "VVFIN", "APPR", "NE"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.50": {"text": "zur\u00fck", "tokens": ["zu\u00b7r\u00fck"], "token_info": ["word"], "pos": ["PTKVZ"], "meter": "-+", "measure": "iambic.single"}, "line.51": {"text": "Auf derselben ihr Verbrechen, auf das harte Un-", "tokens": ["Auf", "der\u00b7sel\u00b7ben", "ihr", "Ver\u00b7bre\u00b7chen", ",", "auf", "das", "har\u00b7te", "Un"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "PPOSAT", "NN", "$,", "APPR", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.52": {"text": "gel\u00fck", "tokens": ["ge\u00b7l\u00fck"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.53": {"text": "Das den Thaten nachgefolgt; so kan man auch", "tokens": ["Das", "den", "Tha\u00b7ten", "nach\u00b7ge\u00b7folgt", ";", "so", "kan", "man", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "VVPP", "$.", "ADV", "VMFIN", "PIS", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.54": {"text": "an ihr lesen,", "tokens": ["an", "ihr", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.55": {"text": "Was sei das Vergeltungsrecht, bei dem allerh\u00f6ch-", "tokens": ["Was", "sei", "das", "Ver\u00b7gel\u00b7tungs\u00b7recht", ",", "bei", "dem", "al\u00b7ler\u00b7h\u00f6ch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "APPR", "ART", "TRUNC"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.56": {"text": "sten Wesen.", "tokens": ["sten", "We\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.57": {"text": "Sodom war ein Lustgefilde, wie ein kleines Para-", "tokens": ["So\u00b7dom", "war", "ein", "Lust\u00b7ge\u00b7fil\u00b7de", ",", "wie", "ein", "klei\u00b7nes", "Pa\u00b7ra"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "TRUNC"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.58": {"text": "dies,", "tokens": ["dies", ","], "token_info": ["word", "punct"], "pos": ["PDS", "$,"], "meter": "-", "measure": "single.down"}, "line.59": {"text": "Wo in einer fetten Gegend, sich nichts als nur", "tokens": ["Wo", "in", "ei\u00b7ner", "fet\u00b7ten", "Ge\u00b7gend", ",", "sich", "nichts", "als", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$,", "PRF", "PIS", "KOKOM", "ADV"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.60": {"text": "Anmuth wies", "tokens": ["An\u00b7muth", "wies"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.61": {"text": "Wo des Himmels Heiterkeit, ihre strahlenreiche", "tokens": ["Wo", "des", "Him\u00b7mels", "Hei\u00b7t