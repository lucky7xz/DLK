{"dta.poem.10505": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast Diamanten und Perlen,", "tokens": ["Du", "hast", "Di\u00b7a\u00b7man\u00b7ten", "und", "Per\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Hast alles, was Menschenbegehr,", "tokens": ["Hast", "al\u00b7les", ",", "was", "Men\u00b7schen\u00b7be\u00b7gehr", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PWS", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und hast die sch\u00f6nsten Augen \u2014", "tokens": ["Und", "hast", "die", "sch\u00f6ns\u00b7ten", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Liebchen, was willst du mehr?", "tokens": ["Mein", "Lieb\u00b7chen", ",", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Auf deine sch\u00f6nen Augen", "tokens": ["Auf", "dei\u00b7ne", "sch\u00f6\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hab' ich ein ganzes Heer", "tokens": ["Hab'", "ich", "ein", "gan\u00b7zes", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von ewigen Liedern gedichtet \u2014", "tokens": ["Von", "e\u00b7wi\u00b7gen", "Lie\u00b7dern", "ge\u00b7dich\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mein Liebchen, was willst du mehr?", "tokens": ["Mein", "Lieb\u00b7chen", ",", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Mit deinen sch\u00f6nen Augen", "tokens": ["Mit", "dei\u00b7nen", "sch\u00f6\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hast du mich gequ\u00e4lt so sehr,", "tokens": ["Hast", "du", "mich", "ge\u00b7qu\u00e4lt", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVPP", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und hast mich zu Grunde gerichtet \u2014", "tokens": ["Und", "hast", "mich", "zu", "Grun\u00b7de", "ge\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mein Liebchen, was willst du mehr?", "tokens": ["Mein", "Lieb\u00b7chen", ",", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}