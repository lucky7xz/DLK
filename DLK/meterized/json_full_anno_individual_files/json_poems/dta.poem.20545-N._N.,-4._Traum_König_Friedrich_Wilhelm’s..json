{"dta.poem.20545": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "4.  \n  Traum K\u00f6nig Friedrich Wilhelm\u2019s.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.99"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "Trauernd, einsam, sich versenkend", "tokens": ["Trau\u00b7ernd", ",", "ein\u00b7sam", ",", "sich", "ver\u00b7sen\u00b7kend"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "$,", "PRF", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Herzens heil\u2019ge R\u00e4ume,", "tokens": ["In", "des", "Her\u00b7zens", "heil'\u00b7ge", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruht\u2019 auf hartem Kriegesfeldbett", "tokens": ["Ruht'", "auf", "har\u00b7tem", "Krie\u00b7ges\u00b7feld\u00b7bett"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6nig Friedrich Wilhelm einst.", "tokens": ["K\u00f6\u00b7nig", "Fried\u00b7rich", "Wil\u00b7helm", "einst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denkt der Unbill Seines Hauses,", "tokens": ["Denkt", "der", "Un\u00b7bill", "Sei\u00b7nes", "Hau\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das die ", "tokens": ["Das", "die"], "token_info": ["word", "word"], "pos": ["PDS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wittelsbacher, ja fast Habsburg", "tokens": ["Wit\u00b7tels\u00b7ba\u00b7cher", ",", "ja", "fast", "Habs\u00b7burg"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uebertraf an Waffenruhm.", "tokens": ["Ue\u00b7bert\u00b7raf", "an", "Waf\u00b7fen\u00b7ruhm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht allein denkt Er der Ahnen,", "tokens": ["Nicht", "al\u00b7lein", "denkt", "Er", "der", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mehr der Tr\u00fcbsal noch der Seinen,", "tokens": ["Mehr", "der", "Tr\u00fcb\u00b7sal", "noch", "der", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "ADV", "ART", "PPOSS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der befleckten Preu\u00dfen-Fahnen;", "tokens": ["Der", "be\u00b7fleck\u00b7ten", "Preu\u00b7\u00dfen\u00b7Fah\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nacht, du h\u00f6rst Sein stilles Weinen!", "tokens": ["Nacht", ",", "du", "h\u00f6rst", "Sein", "stil\u00b7les", "Wei\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Endlich mildert sich Sein Kummer,", "tokens": ["End\u00b7lich", "mil\u00b7dert", "sich", "Sein", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Es umf\u00e4ngt Jhn sanfter Schlummer,", "tokens": ["Es", "um\u00b7f\u00e4ngt", "Jhn", "sanf\u00b7ter", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Jhm im Traum erscheint ", "tokens": ["Jhm", "im", "Traum", "er\u00b7scheint"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Haltend Bayards Kampf-Devise,", "tokens": ["Hal\u00b7tend", "Bay\u00b7ards", "Kampf\u00b7De\u00b7vi\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sprach sie: Mann vom reinsten Adel,", "tokens": ["Sprach", "sie", ":", "Mann", "vom", "reins\u00b7ten", "A\u00b7del", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Halt an Gott Dich, unsern Vater,", "tokens": ["Halt", "an", "Gott", "Dich", ",", "un\u00b7sern", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "NN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bleib Dir selber \u2014 Er sei Rather \u2014", "tokens": ["Bleib", "Dir", "sel\u00b7ber", "Er", "sei", "Ra\u00b7ther"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$(", "PPER", "VAFIN", "NN", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Stets getreu ohn\u2019 Furcht und Tadel.", "tokens": ["Stets", "ge\u00b7treu", "ohn'", "Furcht", "und", "Ta\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hohe Diener hei\u00df\u2019 sie kommen,", "tokens": ["Ho\u00b7he", "Die\u00b7ner", "hei\u00df'", "sie", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jhren Beirath la\u00df Dir frommen;", "tokens": ["Ih\u00b7ren", "Bei\u00b7rath", "la\u00df", "Dir", "from\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6ge, w\u00fcnsche ich, vor allen,", "tokens": ["M\u00f6\u00b7ge", ",", "w\u00fcn\u00b7sche", "ich", ",", "vor", "al\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "$,", "VVFIN", "PPER", "$,", "APPR", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scharnhorst's Vorschlag Dir gefallen!", "tokens": ["Scharn\u00b7hor\u00b7st's", "Vor\u00b7schlag", "Dir", "ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Unsichtbar will ich dann fechten", "tokens": ["Un\u00b7sicht\u00b7bar", "will", "ich", "dann", "fech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Mit f\u00fcr Dich. Es soll den Zelter", "tokens": ["Mit", "f\u00fcr", "Dich", ".", "Es", "soll", "den", "Zel\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "PPER", "$.", "PPER", "VMFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leiten ", "tokens": ["Lei\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Mag herabseh\u2019n, ob nach Rechten", "tokens": ["Mag", "her\u00b7ab\u00b7seh'n", ",", "ob", "nach", "Rech\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "$,", "KOUS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles k\u00e4mpft, ob exerciren", "tokens": ["Al\u00b7les", "k\u00e4mpft", ",", "ob", "ex\u00b7er\u00b7ci\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie gut, gut auch man\u00f6v\u2019riren. \u2014", "tokens": ["Sie", "gut", ",", "gut", "auch", "ma\u00b7n\u00f6\u00b7v'r\u00b7i\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJD", "$,", "ADJD", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch bald; ja der Sieg gl\u00fcckt sicher!", "tokens": ["Doch", "bald", ";", "ja", "der", "Sieg", "gl\u00fcckt", "si\u00b7cher", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "ADV", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Feldmarschall wird unser ", "tokens": ["Feld\u00b7mar\u00b7schall", "wird", "un\u00b7ser"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}