{"dta.poem.920": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "VeRFALL", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Am Abend, wenn die Glocken Frieden l\u00e4uten,", "tokens": ["Am", "A\u00b7bend", ",", "wenn", "die", "Glo\u00b7cken", "Frie\u00b7den", "l\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Folg ich der V\u00f6gel wundervollen Fl\u00fcgen,", "tokens": ["Folg", "ich", "der", "V\u00f6\u00b7gel", "wun\u00b7der\u00b7vol\u00b7len", "Fl\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die lang geschart, gleich frommen Pilgerz\u00fcgen,", "tokens": ["Die", "lang", "ge\u00b7schart", ",", "gleich", "from\u00b7men", "Pil\u00b7ger\u00b7z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entschwinden in den herbstlich klaren Weiten.", "tokens": ["Ent\u00b7schwin\u00b7den", "in", "den", "herbst\u00b7lich", "kla\u00b7ren", "Wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hinwandelnd durch den d\u00e4mmervollen Garten", "tokens": ["Hin\u00b7wan\u00b7delnd", "durch", "den", "d\u00e4m\u00b7mer\u00b7vol\u00b7len", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Tr\u00e4um ich nach ihren helleren Geschicken", "tokens": ["Tr\u00e4um", "ich", "nach", "ih\u00b7ren", "hel\u00b7le\u00b7ren", "Ge\u00b7schi\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und f\u00fchl der Stunden Weiser kaum mehr r\u00fccken.", "tokens": ["Und", "f\u00fchl", "der", "Stun\u00b7den", "Wei\u00b7ser", "kaum", "mehr", "r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So folg ich \u00fcber Wolken ihren Fahrten.", "tokens": ["So", "folg", "ich", "\u00fc\u00b7ber", "Wol\u00b7ken", "ih\u00b7ren", "Fahr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da macht ein Hauch mich von Verfall erzittern.", "tokens": ["Da", "macht", "ein", "Hauch", "mich", "von", "Ver\u00b7fall", "er\u00b7zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Amsel klagt in den entlaubten Zweigen.", "tokens": ["Die", "Am\u00b7sel", "klagt", "in", "den", "ent\u00b7laub\u00b7ten", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es schwankt der rote Wein an rostigen Gittern,", "tokens": ["Es", "schwankt", "der", "ro\u00b7te", "Wein", "an", "ros\u00b7ti\u00b7gen", "Git\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Indes wie blasser Kinder Todesreigen", "tokens": ["In\u00b7des", "wie", "blas\u00b7ser", "Kin\u00b7der", "To\u00b7des\u00b7rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um dunkle Brunnenr\u00e4nder, die verwittern,", "tokens": ["Um", "dunk\u00b7le", "Brun\u00b7nen\u00b7r\u00e4n\u00b7der", ",", "die", "ver\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Wind sich fr\u00f6stelnd blaue Astern neigen.", "tokens": ["Im", "Wind", "sich", "fr\u00f6s\u00b7telnd", "blau\u00b7e", "As\u00b7tern", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}