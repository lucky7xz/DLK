{"dta.poem.8016": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Wirthin T\u00f6chterlein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Es zogen drei Bursche wohl \u00fcber den Rhein,", "tokens": ["Es", "zo\u00b7gen", "drei", "Bur\u00b7sche", "wohl", "\u00fc\u00b7ber", "den", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "ADV", "APPR", "ART", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Bei einer Frau Wirthin, da kehrten sie ein.", "tokens": ["Bei", "ei\u00b7ner", "Frau", "Wirt\u00b7hin", ",", "da", "kehr\u00b7ten", "sie", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "\u201efrau Wirthin! hat sie gut Bier und Wein?", "tokens": ["\u201e", "frau", "Wirt\u00b7hin", "!", "hat", "sie", "gut", "Bier", "und", "Wein", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$.", "VAFIN", "PPER", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo hat sie ihr sch\u00f6nes T\u00f6chterlein?\u201c", "tokens": ["Wo", "hat", "sie", "ihr", "sch\u00f6\u00b7nes", "T\u00f6ch\u00b7ter\u00b7lein", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "\u201emein Bier und Wein ist frisch und klar,", "tokens": ["\u201e", "mein", "Bier", "und", "Wein", "ist", "frisch", "und", "klar", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "KON", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein T\u00f6chterlein liegt auf der Todtenbahr.\u201c", "tokens": ["Mein", "T\u00f6ch\u00b7ter\u00b7lein", "liegt", "auf", "der", "Tod\u00b7ten\u00b7bahr", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und als sie traten zur Kammer hinein,", "tokens": ["Und", "als", "sie", "tra\u00b7ten", "zur", "Kam\u00b7mer", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da lag sie in einem schwarzen Schrein.", "tokens": ["Da", "lag", "sie", "in", "ei\u00b7nem", "schwar\u00b7zen", "Schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Der erste, der schlug den Schleier zur\u00fcck", "tokens": ["Der", "ers\u00b7te", ",", "der", "schlug", "den", "Schlei\u00b7er", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PRELS", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und schaute sie an mit traurigem Blick:", "tokens": ["Und", "schau\u00b7te", "sie", "an", "mit", "trau\u00b7ri\u00b7gem", "Blick", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "\u201each lebtest du noch, du sch\u00f6ne Maid!", "tokens": ["\u201e", "ach", "leb\u00b7test", "du", "noch", ",", "du", "sch\u00f6\u00b7ne", "Maid", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "VVFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ich w\u00fcrde dich lieben von dieser Zeit.\u201c", "tokens": ["Ich", "w\u00fcr\u00b7de", "dich", "lie\u00b7ben", "von", "die\u00b7ser", "Zeit", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Der zweite deckte den Schleier zu,", "tokens": ["Der", "zwei\u00b7te", "deck\u00b7te", "den", "Schlei\u00b7er", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und kehrte sich ab, und weinte dazu:", "tokens": ["Und", "kehr\u00b7te", "sich", "ab", ",", "und", "wein\u00b7te", "da\u00b7zu", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "$,", "KON", "VVFIN", "PAV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "\u201each! da\u00df du liegst auf der Todtenbahr!", "tokens": ["\u201e", "ach", "!", "da\u00df", "du", "liegst", "auf", "der", "Tod\u00b7ten\u00b7bahr", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ich hab\u2019 dich geliebet so manches Jahr.\u201c", "tokens": ["Ich", "hab'", "dich", "ge\u00b7lie\u00b7bet", "so", "man\u00b7ches", "Jahr", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Der dritte hub ihn wieder sogleich,", "tokens": ["Der", "drit\u00b7te", "hub", "ihn", "wie\u00b7der", "sog\u00b7leich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und k\u00fc\u00dfte sie an den Mund so bleich:", "tokens": ["Und", "k\u00fc\u00df\u00b7te", "sie", "an", "den", "Mund", "so", "bleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "\u201edich liebt\u2019 ich immer, dich lieb\u2019 ich noch heut,", "tokens": ["\u201e", "dich", "liebt'", "ich", "im\u00b7mer", ",", "dich", "lieb'", "ich", "noch", "heut", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und werde dich lieben in Ewigkeit.\u201c", "tokens": ["Und", "wer\u00b7de", "dich", "lie\u00b7ben", "in", "E\u00b7wig\u00b7keit", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}