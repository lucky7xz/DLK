{"dta.poem.12744": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihr einbalsamiren.  \n L. R. B.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Xenander fragte mich: woher es kommen m\u00fcsse,", "tokens": ["Xe\u00b7nan\u00b7der", "frag\u00b7te", "mich", ":", "wo\u00b7her", "es", "kom\u00b7men", "m\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$.", "PWAV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df jeder gerne wolt um Amarinden seyn;", "tokens": ["Da\u00df", "je\u00b7der", "ger\u00b7ne", "wolt", "um", "A\u00b7mar\u00b7in\u00b7den", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VMFIN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich sprach: da\u00df ich so viel von einem bader wisse,", "tokens": ["Ich", "sprach", ":", "da\u00df", "ich", "so", "viel", "von", "ei\u00b7nem", "ba\u00b7der", "wis\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie balsamire sich hier da und sonst wo ein.", "tokens": ["Sie", "bal\u00b7sa\u00b7mi\u00b7re", "sich", "hier", "da", "und", "sonst", "wo", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "KON", "ADV", "PWAV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}