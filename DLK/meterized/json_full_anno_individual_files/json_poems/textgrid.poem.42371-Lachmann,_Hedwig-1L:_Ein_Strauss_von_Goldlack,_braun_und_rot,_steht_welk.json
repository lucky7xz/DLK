{"textgrid.poem.42371": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Strauss von Goldlack, braun und rot, steht welk", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Strauss von Goldlack, braun und rot, steht welk", "tokens": ["Ein", "Strauss", "von", "Gold\u00b7lack", ",", "braun", "und", "rot", ",", "steht", "welk"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In meinem Glas \u2013 ich mag ihn nicht entfernen.", "tokens": ["In", "mei\u00b7nem", "Glas", "\u2013", "ich", "mag", "ihn", "nicht", "ent\u00b7fer\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im D\u00e4mmerdunkel, hinter dem Geb\u00e4lk", "tokens": ["Im", "D\u00e4m\u00b7mer\u00b7dun\u00b7kel", ",", "hin\u00b7ter", "dem", "Ge\u00b7b\u00e4lk"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von grauen Wolken, sucht mein Blick nach Sternen.", "tokens": ["Von", "grau\u00b7en", "Wol\u00b7ken", ",", "sucht", "mein", "Blick", "nach", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das zarte Bl\u00e4tterwerk kr\u00fcmmt sich am Strauch,", "tokens": ["Das", "zar\u00b7te", "Bl\u00e4t\u00b7ter\u00b7werk", "kr\u00fcmmt", "sich", "am", "Strauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Winter streift, bevor sein Lauf beendet,", "tokens": ["Der", "Win\u00b7ter", "streift", ",", "be\u00b7vor", "sein", "Lauf", "be\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit einem sp\u00e4ten eiseskalten Hauch", "tokens": ["Mit", "ei\u00b7nem", "sp\u00e4\u00b7ten", "ei\u00b7ses\u00b7kal\u00b7ten", "Hauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Welt, die sich schon fr\u00fchlingsw\u00e4rts gewendet.", "tokens": ["Die", "Welt", ",", "die", "sich", "schon", "fr\u00fch\u00b7lings\u00b7w\u00e4rts", "ge\u00b7wen\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Noch h\u00e4lt mein Traum die alten Bilder fest.", "tokens": ["Noch", "h\u00e4lt", "mein", "Traum", "die", "al\u00b7ten", "Bil\u00b7der", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und doch, und doch! auch ich, ich f\u00fchl's im stillen:", "tokens": ["Und", "doch", ",", "und", "doch", "!", "auch", "ich", ",", "ich", "f\u00fchl's", "im", "stil\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KON", "ADV", "$.", "ADV", "PPER", "$,", "PPER", "VVFIN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn nun der Sommer leuchtet vom Ge\u00e4st,", "tokens": ["Wenn", "nun", "der", "Som\u00b7mer", "leuch\u00b7tet", "vom", "Ge\u00b7\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bin ich gewachsen und erstarkt an Willen.", "tokens": ["Bin", "ich", "ge\u00b7wach\u00b7sen", "und", "er\u00b7starkt", "an", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie jede Kraft, der Erde einverleibt,", "tokens": ["Wie", "je\u00b7de", "Kraft", ",", "der", "Er\u00b7de", "ein\u00b7ver\u00b7leibt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aufs neue wird aus ihrem Schoss geboren \u2013", "tokens": ["Aufs", "neu\u00b7e", "wird", "aus", "ih\u00b7rem", "Schoss", "ge\u00b7bo\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So, Herz, das zu verlieren f\u00fcrchtet, bleibt", "tokens": ["So", ",", "Herz", ",", "das", "zu", "ver\u00b