{"dta.poem.16798": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "157.  GOtt schauet man an sich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ist mein GOtt gestalt? Geh schau dich selber an/", "tokens": ["Wie", "ist", "mein", "Gott", "ge\u00b7stalt", "?", "Geh", "schau", "dich", "sel\u00b7ber", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "VVPP", "$.", "NE", "VVFIN", "PPER", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer sich in GOtt beschaut/ schaut Gott warhafftig an.", "tokens": ["Wer", "sich", "in", "Gott", "be\u00b7schaut", "/", "schaut", "Gott", "war\u00b7haff\u00b7tig", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "NN", "VVPP", "$(", "VVFIN", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}