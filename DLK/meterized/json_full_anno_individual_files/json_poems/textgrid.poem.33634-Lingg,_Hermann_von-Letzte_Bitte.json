{"textgrid.poem.33634": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Letzte Bitte", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immer leiser wird mein Schlummer,", "tokens": ["Im\u00b7mer", "lei\u00b7ser", "wird", "mein", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur wie Schleier liegt mein Kummer", "tokens": ["Nur", "wie", "Schlei\u00b7er", "liegt", "mein", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zitternd \u00fcber mir.", "tokens": ["Zit\u00b7ternd", "\u00fc\u00b7ber", "mir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Oft im Traume h\u00f6r' ich dich", "tokens": ["Oft", "im", "Trau\u00b7me", "h\u00f6r'", "ich", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rufen draus vor meiner T\u00fcr,", "tokens": ["Ru\u00b7fen", "draus", "vor", "mei\u00b7ner", "T\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Niemand wacht und \u00f6ffnet dir,", "tokens": ["Nie\u00b7mand", "wacht", "und", "\u00f6ff\u00b7net", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich erwach' und weine bitterlich.", "tokens": ["Ich", "er\u00b7wach'", "und", "wei\u00b7ne", "bit\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ja, ich werde sterben m\u00fcssen,", "tokens": ["Ja", ",", "ich", "wer\u00b7de", "ster\u00b7ben", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine Andre wirst du k\u00fcssen,", "tokens": ["Ei\u00b7ne", "And\u00b7re", "wirst", "du", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich bleich und kalt,", "tokens": ["Wenn", "ich", "bleich", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Eh die Maienl\u00fcfte wehen,", "tokens": ["Eh", "die", "Mai\u00b7en\u00b7l\u00fcf\u00b7te", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eh die Drossel singt im Wald;", "tokens": ["Eh", "die", "Dros\u00b7sel", "singt", "im", "Wald", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Willst du mich noch einmal sehen,", "tokens": ["Willst", "du", "mich", "noch", "ein\u00b7mal", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Komm, o komme bald!", "tokens": ["Komm", ",", "o", "kom\u00b7me", "bald", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "VVFIN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Immer leiser wird mein Schlummer,", "tokens": ["Im\u00b7mer", "lei\u00b7ser", "wird", "mein", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur wie Schleier liegt mein Kummer", "tokens": ["Nur", "wie", "Schlei\u00b7er", "liegt", "mein", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zitternd \u00fcber mir.", "tokens": ["Zit\u00b7ternd", "\u00fc\u00b7ber", "mir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Oft im Traume h\u00f6r' ich dich", "tokens": ["Oft", "im", "Trau\u00b7me", "h\u00f6r'", "ich", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rufen draus vor meiner T\u00fcr,", "tokens": ["Ru\u00b7fen", "draus", "vor", "mei\u00b7ner", "T\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Niemand wacht und \u00f6ffnet dir,", "to