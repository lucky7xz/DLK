{"textgrid.poem.52441": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "23. Fischerlied", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Fischergewerbe", "tokens": ["Das", "Fi\u00b7scher\u00b7ge\u00b7wer\u00b7be"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Giebt r\u00fcstigen Mut!", "tokens": ["Giebt", "r\u00fcs\u00b7ti\u00b7gen", "Mut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wir haben zum Erbe", "tokens": ["Wir", "ha\u00b7ben", "zum", "Er\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die G\u00fcter der Flut.", "tokens": ["Die", "G\u00fc\u00b7ter", "der", "Flut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wir graben nicht Sch\u00e4tze,", "tokens": ["Wir", "gra\u00b7ben", "nicht", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wir pfl\u00fcgen kein Feld;", "tokens": ["Wir", "pfl\u00fc\u00b7gen", "kein", "Feld", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Wir ernten im Netze,", "tokens": ["Wir", "ern\u00b7ten", "im", "Net\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Wir angeln uns Geld.", "tokens": ["Wir", "an\u00b7geln", "uns", "Geld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Wir heben die Reusen", "tokens": ["Wir", "he\u00b7ben", "die", "Reu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Den Schilfbach entlang,", "tokens": ["Den", "Schilf\u00b7bach", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und ruhn bei den Schleusen,", "tokens": ["Und", "ruhn", "bei", "den", "Schleu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zu sondern den Fang.", "tokens": ["Zu", "son\u00b7dern", "den", "Fang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Goldweiden beschatten", "tokens": ["Gold\u00b7wei\u00b7den", "be\u00b7schat\u00b7ten"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Das moosige Dach;", "tokens": ["Das", "moo\u00b7si\u00b7ge", "Dach", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Wir schlummern auf Matten", "tokens": ["Wir", "schlum\u00b7mern", "auf", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Im k\u00fchlen Gemach.", "tokens": ["Im", "k\u00fch\u00b7len", "Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Mit roten Korallen", "tokens": ["Mit", "ro\u00b7ten", "Ko\u00b7ral\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Prangt Spiegel und Wand,", "tokens": ["Prangt", "Spie\u00b7gel", "und", "Wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Den Estrich der Hallen", "tokens": ["Den", "Est\u00b7rich", "der", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Deckt silberner Sand.", "tokens": ["Deckt", "sil\u00b7ber\u00b7ner", "Sand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Das G\u00e4rtchen daneben", "tokens": ["Das", "G\u00e4rt\u00b7chen", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PAV"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.6": {"text": "Gr\u00fcnt l\u00e4ndlich umz\u00e4unt", "tokens": ["Gr\u00fcnt", "l\u00e4nd\u00b7lich", "um\u00b7z\u00e4unt"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Von kreuzenden St\u00e4ben", "tokens": ["Von", "kreu\u00b7zen\u00b7den", "St\u00e4\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Mit Baste vereint.", "tokens": ["Mit", "Bas\u00b7te", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Im Antlitz der Buben", "tokens": ["Im", "Ant\u00b7litz", "der", "Bu\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Lacht mutiger Sinn,", "tokens": ["Lacht", "mu\u00b7ti\u00b7ger", "Sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sie meiden die Stuben", "tokens": ["Sie", "mei\u00b7den", "die", "Stu\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Bei Tagesbeginn;", "tokens": ["Bei", "Ta\u00b7ges\u00b7be\u00b7ginn", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sie tauchen und schwimmen", "tokens": ["Sie", "tau\u00b7chen", "und", "schwim\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Im eisigen See,", "tokens": ["Im", "ei\u00b7si\u00b7gen", "See", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und barfu\u00df erklimmen", "tokens": ["Und", "bar\u00b7fu\u00df", "er\u00b7klim\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Sie Klippen voll Schnee.", "tokens": ["Sie", "Klip\u00b7pen", "voll", "Schnee", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Die T\u00f6chter erg\u00f6tzen", "tokens": ["Die", "T\u00f6ch\u00b7ter", "er\u00b7g\u00f6t\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sich Abends bei Licht,", "tokens": ["Sich", "A\u00b7bends", "bei", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wenn alles an Netzen", "tokens": ["Wenn", "al\u00b7les", "an", "Net\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und Maschenwerk flicht.", "tokens": ["Und", "Ma\u00b7schen\u00b7werk", "flicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Oft wird mit Gel\u00e4chter", "tokens": ["Oft", "wird", "mit", "Ge\u00b7l\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Durchmustert das Dorf;", "tokens": ["Durch\u00b7mus\u00b7tert", "das", "Dorf", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Die Mutter, als W\u00e4chter,", "tokens": ["Die", "Mut\u00b7ter", ",", "als", "W\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Sch\u00fcrt nickend den Torf.", "tokens": ["Sch\u00fcrt", "ni\u00b7ckend", "den", "Torf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Oft rudern wir ferne", "tokens": ["Oft", "ru\u00b7dern", "wir", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Im wiegenden Kahn,", "tokens": ["Im", "wie\u00b7gen\u00b7den", "Kahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dann blinken die Sterne", "tokens": ["Dann", "blin\u00b7ken", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So freundlich uns an;", "tokens": ["So", "freund\u00b7lich", "uns", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Der Mond aus den H\u00f6hen,", "tokens": ["Der", "Mond", "aus", "den", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der Mond aus dem Bach,", "tokens": ["Der", "Mond", "aus", "dem", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "So schnell wir entfl\u00f6hen,", "tokens": ["So", "schnell", "wir", "ent\u00b7fl\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Sie gleiten uns nach.", "tokens": ["Sie", "glei\u00b7ten", "uns", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Wir trotzen dem Wetter,", "tokens": ["Wir", "trot\u00b7zen", "dem", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Das finster uns droht,", "tokens": ["Das", "fins\u00b7ter", "uns", "droht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wenn sch\u00f6pfende Bretter", "tokens": ["Wenn", "sch\u00f6p\u00b7fen\u00b7de", "Bret\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Kaum hemmen den Tod.", "tokens": ["Kaum", "hem\u00b7men", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wir trotzen auch Wogen", "tokens": ["Wir", "trot\u00b7zen", "auch", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Auf krachendem Schiff,", "tokens": ["Auf", "kra\u00b7chen\u00b7dem", "Schiff", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "In Tiefen gezogen,", "tokens": ["In", "Tie\u00b7fen", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "me