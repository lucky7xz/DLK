{"textgrid.poem.46320": {"metadata": {"author": {"name": "Spee, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[da Jesus an dem Creutze stund]", "genre": "verse", "period": "N.A.", "pub_year": 1613, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da Jesus an dem Creutze stund,", "tokens": ["Da", "Je\u00b7sus", "an", "dem", "Creut\u00b7ze", "stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit blut gantz vberschossen,", "tokens": ["Mit", "blut", "gantz", "vber\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Von haupt bi\u00df zu den f\u00fcssen wund,", "tokens": ["Von", "haupt", "bi\u00df", "zu", "den", "f\u00fcs\u00b7sen", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat mich gar sehr verdrossen,", "tokens": ["Hat", "mich", "gar", "sehr", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df noch die Sonn in vollem brand", "tokens": ["Da\u00df", "noch", "die", "Sonn", "in", "vol\u00b7lem", "brand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit ihren g\u00fclden wagen", "tokens": ["Mit", "ih\u00b7ren", "g\u00fcl\u00b7den", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Thet vberfahren alle land;", "tokens": ["Thet", "vberf\u00b7ah\u00b7ren", "al\u00b7le", "land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Bat Gott, er drein wolt schlagen.", "tokens": ["Bat", "Gott", ",", "er", "drein", "wolt", "schla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Gott, sprach ich, la\u00df eylend doch", "tokens": ["O", "Gott", ",", "sprach", "ich", ",", "la\u00df", "ey\u00b7lend", "doch"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$,", "VVIMP", "VVPP", "KON"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df ro\u00df, vnd wagen st\u00fcrtzen,", "tokens": ["La\u00df", "ro\u00df", ",", "vnd", "wa\u00b7gen", "st\u00fcrt\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "KON", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der freche tag geht vill zu hoch;", "tokens": ["Der", "fre\u00b7che", "tag", "geht", "vill", "zu", "hoch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schnell wollest ihn verk\u00fcrtzen:", "tokens": ["Schnell", "wol\u00b7lest", "ihn", "ver\u00b7k\u00fcrt\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Stell nur, stell ein: all sonnen schein,", "tokens": ["Stell", "nur", ",", "stell", "ein", ":", "all", "son\u00b7nen", "schein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJD", "PTKVZ", "$.", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Das liecht mag ich nit leiden;", "tokens": ["Das", "liecht", "mag", "ich", "nit", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Weill ich nit kan: mehr schawen an", "tokens": ["Weill", "ich", "nit", "kan", ":", "mehr", "scha\u00b7wen", "an"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "$.", "ADV", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein lieb am Creutz verscheiden.", "tokens": ["Mein", "lieb", "am", "Creutz", "ver\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die dunckel nacht: mir ba\u00df behagt,", "tokens": ["Die", "dun\u00b7ckel", "nacht", ":", "mir", "ba\u00df", "be\u00b7hagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wans k\u00e4m den lufft erschwertzen,", "tokens": ["Wans", "k\u00e4m", "den", "lufft", "er\u00b7schwert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd deckt in ruh: mein Jesum zu;", "tokens": ["Vnd", "deckt", "in", "ruh", ":", "mein", "Je\u00b7sum", "zu", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$.", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df niemand seh den schmertzen.", "tokens": ["Da\u00df", "nie\u00b7mand", "seh", "den", "schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ART", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kom nur mit macht: o schwartze nacht:", "tokens": ["Kom", "nur", "mit", "macht", ":", "o", "schwart\u00b7ze", "nacht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "VVFIN", "$.", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Jesum solst bedecken", "tokens": ["Mein", "Je\u00b7sum", "solst", "be\u00b7de\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Er hengt in noth: ringt mit dem tod:", "tokens": ["Er", "hengt", "in", "noth", ":", "ringt", "mit", "dem", "tod", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Es grawset mich vor schrecken.", "tokens": ["Es", "graw\u00b7set", "mich", "vor", "schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach wicklet ein: den liebsten mein,", "tokens": ["Ach", "wick\u00b7let", "ein", ":", "den", "liebs\u00b7ten", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PTKVZ", "$.", "ART", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr finstern\u00fcssen schwere,", "tokens": ["Ihr", "fins\u00b7ter\u00b7n\u00fcs\u00b7sen", "schwe\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich nit seh: sein gro\u00dfes weh", "tokens": ["Da\u00df", "ich", "nit", "seh", ":", "sein", "gro\u00b7\u00dfes", "weh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich kr\u00e4ncket vil zu sehre.", "tokens": ["Mich", "kr\u00e4n\u00b7cket", "vil", "zu", "seh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ade, ade: nit scheinet meh,", "tokens": ["A\u00b7de", ",", "a\u00b7de", ":", "nit", "schei\u00b7net", "meh", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM.la", "$.", "PTKNEG", "VVFIN", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Sonn, Mon, vnd himmel Sterne", "tokens": ["Sonn", ",", "Mon", ",", "vnd", "him\u00b7mel", "Ster\u00b7ne"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NE", "$,", "KON", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bin gar bereit: zu leben alzeit,", "tokens": ["Bin", "gar", "be\u00b7reit", ":", "zu", "le\u00b7ben", "al\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$.", "PTKZU", "VVINF", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Im duncklen also gerne.", "tokens": ["Im", "dunck\u00b7len", "al\u00b7so", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nur trawrigkeit: nur hertzen leid", "tokens": ["Nur", "traw\u00b7rig\u00b7keit", ":", "nur", "hert\u00b7zen", "leid"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "$.", "ADV", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Werd ich hinf\u00fcrter treiben,", "tokens": ["Werd", "ich", "hin\u00b7f\u00fcr\u00b7ter", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dich gar \u00f4 welt, hab abgestelt", "tokens": ["Dich", "gar", "\u00f4", "welt", ",", "hab", "ab\u00b7ge\u00b7stelt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "FM", "FM", "$,", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An dir werd mich nit reiben.", "tokens": ["An", "dir", "werd", "mich", "nit", "rei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Einmahl ich war: in tods gefahr;", "tokens": ["Ein\u00b7mahl", "ich", "war", ":", "in", "tods", "ge\u00b7fahr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "$.", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schier war ich vnter gangen", "tokens": ["Schier", "war", "ich", "vn\u00b7ter", "gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da kam zu steur: der held so theur,", "tokens": ["Da", "kam", "zu", "steur", ":", "der", "held", "so", "theur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKA", "ADJD", "$.", "ART", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcr mich wolt selber hangen.", "tokens": ["F\u00fcr", "mich", "wolt", "sel\u00b7ber", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mit starckem lauff: zum Creutz hinauff", "tokens": ["Mit", "star\u00b7ckem", "lauff", ":", "zum", "Creutz", "hin\u00b7auff"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich that er hoch erschwingen,", "tokens": ["Sich", "that", "er", "hoch", "er\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr mich da starb: mirs heil erwarb:", "tokens": ["F\u00fcr", "mich", "da", "starb", ":", "mirs", "heil", "er\u00b7warb", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "$.", "NE", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure