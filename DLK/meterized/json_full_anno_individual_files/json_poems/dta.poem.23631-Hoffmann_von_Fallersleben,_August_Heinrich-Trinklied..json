{"dta.poem.23631": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, lustig bin ich, das ist wahr!", "tokens": ["Ja", ",", "lus\u00b7tig", "bin", "ich", ",", "das", "ist", "wahr", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VAFIN", "PPER", "$,", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie's L\u00e4mmlein auf der Au.", "tokens": ["Wie's", "L\u00e4mm\u00b7lein", "auf", "der", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die ganze Welt ist Sonnenschein,", "tokens": ["Die", "gan\u00b7ze", "Welt", "ist", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich fange hier den Regen ein", "tokens": ["Ich", "fan\u00b7ge", "hier", "den", "Re\u00b7gen", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und trinke Himmelthau.", "tokens": ["Und", "trin\u00b7ke", "Him\u00b7melt\u00b7hau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Den Stein der Weisen find' ich noch;", "tokens": ["Den", "Stein", "der", "Wei\u00b7sen", "find'", "ich", "noch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Margret, ein Sch\u00f6pplein Wein!", "tokens": ["Marg\u00b7ret", ",", "ein", "Sch\u00f6p\u00b7plein", "Wein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ich mach' aus Wein noch Gold und Geld,", "tokens": ["Ich", "mach'", "aus", "Wein", "noch", "Gold", "und", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Potz Velten! noch die ganze Welt,", "tokens": ["Potz", "Vel\u00b7ten", "!", "noch", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "'s Darf nur kein Kr\u00e4tzer sein!", "tokens": ["'s", "Darf", "nur", "kein", "Kr\u00e4t\u00b7zer", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "He! rei\u00df den Zeiger von der Uhr!", "tokens": ["He", "!", "rei\u00df", "den", "Zei\u00b7ger", "von", "der", "Uhr", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was k\u00fcmmert uns die Zeit?", "tokens": ["Was", "k\u00fcm\u00b7mert", "uns", "die", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lass laufen was nicht bleiben kann!", "tokens": ["Lass", "lau\u00b7fen", "was", "nicht", "blei\u00b7ben", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PWS", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was geht denn mich ein andrer an?", "tokens": ["Was", "geht", "denn", "mich", "ein", "an\u00b7drer", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "PPER", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trink, Bruder, gieb Bescheid!", "tokens": ["Trink", ",", "Bru\u00b7der", ",", "gieb", "Be\u00b7scheid", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVIMP", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr B\u00e4nk' und Tische nehmt's nicht krumm!", "tokens": ["Ihr", "B\u00e4nk'", "und", "Ti\u00b7sche", "nehmt's", "nicht", "krumm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Lied gar bald entflieht.", "tokens": ["Ein", "Lied", "gar", "bald", "ent\u00b7flieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ihr noch gr\u00fcnbelaubet wart,", "tokens": ["Als", "ihr", "noch", "gr\u00fcn\u00b7be\u00b7lau\u00b7bet", "wart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da sangen V\u00f6glein mancher Art", "tokens": ["Da", "san\u00b7gen", "V\u00f6\u00b7glein", "man\u00b7cher", "Art"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Euch auch gar manches Lied.", "tokens": ["Euch", "auch", "gar", "man\u00b7ches", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}