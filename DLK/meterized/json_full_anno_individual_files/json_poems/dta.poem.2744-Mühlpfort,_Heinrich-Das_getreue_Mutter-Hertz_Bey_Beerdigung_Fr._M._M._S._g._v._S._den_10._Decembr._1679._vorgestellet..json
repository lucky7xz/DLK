 "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.98": {"text": "Jhr habt den letzten Ku\u00df von ihrem Mund empfangen/", "tokens": ["Ihr", "habt", "den", "letz\u00b7ten", "Ku\u00df", "von", "ih\u00b7rem", "Mund", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.99": {"text": "Die Augen zugedr\u00fcckt/ so euch so offt erfreut.", "tokens": ["Die", "Au\u00b7gen", "zu\u00b7ge\u00b7dr\u00fcckt", "/", "so", "euch", "so", "offt", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "ADV", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.100": {"text": "Und unter vielem Weh\u2019/ Begierden und Verlangen", "tokens": ["Und", "un\u00b7ter", "vie\u00b7lem", "Weh'", "/", "Be\u00b7gier\u00b7den", "und", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.101": {"text": "Sie nur voran geschickt ins Land der Ewigkeit.", "tokens": ["Sie", "nur", "vo\u00b7ran", "ge\u00b7schickt", "ins", "Land", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVPP", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.102": {"text": "Mausolens Ehgemahl trinck ihres Herren Asche:", "tokens": ["Mau\u00b7so\u00b7lens", "Eh\u00b7ge\u00b7mahl", "trinck", "ih\u00b7res", "Her\u00b7ren", "A\u00b7sche", ":"], "token_info": ["word", "word", "word", "word", "word"