{"dta.poem.2744": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das getreue Mutter-Hertz/  \n Bey Beerdigung Fr. M. M. S. g. v. S.  \n den 10. Decembr. 1679. vorgestellet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hochedle/ jener w\u00fcnscht ihm Fenster in die Hertzen/", "tokens": ["Ho\u00b7ched\u00b7le", "/", "je\u00b7ner", "w\u00fcnscht", "ihm", "Fens\u00b7ter", "in", "die", "Hert\u00b7zen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PDS", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dadurch die Regungen der Geister anzuschau\u2019n:", "tokens": ["Da\u00b7durch", "die", "Re\u00b7gun\u00b7gen", "der", "Geis\u00b7ter", "an\u00b7zu\u00b7schau'n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Hier zengt eu\u2019r Angesicht die innre Seelen-Schmertzen/", "tokens": ["Hier", "zengt", "eu'r", "An\u00b7ge\u00b7sicht", "die", "inn\u00b7re", "See\u00b7len\u00b7Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und selbst der Augen-Brunn mu\u00df nichts als Thr\u00e4nen", "tokens": ["Und", "selbst", "der", "Au\u00b7gen\u00b7Brunn", "mu\u00df", "nichts", "als", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VMFIN", "PIS", "KOKOM", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Warum? ihr solt und must itzt euer Hertz begraben:", "tokens": ["Wa\u00b7rum", "?", "ihr", "solt", "und", "must", "itzt", "eu\u00b7er", "Hertz", "be\u00b7gra\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VMFIN", "KON", "VMFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "(thau'n.", "tokens": ["(", "thau'", "n."], "token_info": ["punct", "word", "abbreviation"], "pos": ["$(", "VVFIN", "NE"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Ists m\u00f6glich/ da\u00df ein Mensch mehr ohne Hertze lebt?", "tokens": ["Ists", "m\u00f6g\u00b7lich", "/", "da\u00df", "ein", "Mensch", "mehr", "oh\u00b7ne", "Hert\u00b7ze", "lebt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "KOUS", "ART", "NN", "ADV", "APPR", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und soll ein Hertz den Sarg in seinem Hertzen haben?", "tokens": ["Und", "soll", "ein", "Hertz", "den", "Sarg", "in", "sei\u00b7nem", "Hert\u00b7zen", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gleichwie ein Seiden-Wurm sein Grab ihm selber webt?", "tokens": ["Gleich\u00b7wie", "ein", "Sei\u00b7den\u00b7Wurm", "sein", "Grab", "ihm", "sel\u00b7ber", "webt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Man schlo\u00df vor diesem ja gekr\u00f6nter H\u00e4upter Hertzen/", "tokens": ["Man", "schlo\u00df", "vor", "die\u00b7sem", "ja", "ge\u00b7kr\u00f6n\u00b7ter", "H\u00e4up\u00b7ter", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PDAT", "ADV", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn sie der letzte Sto\u00df des Todes kalt gemacht/", "tokens": ["Wenn", "sie", "der", "letz\u00b7te", "Sto\u00df", "des", "To\u00b7des", "kalt", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In Gold und Edelstein/ und hat viel tausend Kertzen", "tokens": ["In", "Gold", "und", "E\u00b7del\u00b7stein", "/", "und", "hat", "viel", "tau\u00b7send", "Kert\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$(", "KON", "VAFIN", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zu zieren das Mausol verschwend\u2019risch ausgedacht.", "tokens": ["Zu", "zie\u00b7ren", "das", "Mau\u00b7sol", "ver\u00b7schwen\u00b7d'\u00b7risch", "aus\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.14": {"text": "Viel haben balsamirt die Hertzen wollen halten/", "tokens": ["Viel", "ha\u00b7ben", "bal\u00b7sa\u00b7mirt", "die", "Hert\u00b7zen", "wol\u00b7len", "hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Umb so den Liebes-Dienst unendlich zu verneurn.", "tokens": ["Umb", "so", "den", "Lie\u00b7bes\u00b7Dienst", "un\u00b7end\u00b7lich", "zu", "ver\u00b7neurn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.16": {"text": "Viel/ wenn sie schon gesehn der Adern Quell erkalten/", "tokens": ["Viel", "/", "wenn", "sie", "schon", "ge\u00b7sehn", "der", "A\u00b7dern", "Quell", "er\u00b7kal\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PPER", "ADV", "VVPP", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die wolten doch noch so dem Tod und F\u00e4uln\u00fc\u00df steur\u2019 n.", "tokens": ["Die", "wol\u00b7ten", "doch", "noch", "so", "dem", "Tod", "und", "F\u00e4ul\u00b7n\u00fc\u00df", "steur'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "ADV", "ART", "NN", "KON", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nein/ euer Grabmal trotzt die pr\u00e4chtigen Colossen/", "tokens": ["Nein", "/", "eu\u00b7er", "Grab\u00b7mal", "trotzt", "die", "pr\u00e4ch\u00b7ti\u00b7gen", "Co\u00b7los\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Cairens Seulen-Werck und Memphis Marmelstein/", "tokens": ["Cai\u00b7rens", "Seu\u00b7len\u00b7\u00b7Werck", "und", "Mem\u00b7phis", "Mar\u00b7mel\u00b7stein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NE", "NE", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.20": {"text": "Und was von Bildern mehr Corinthus hat gegossen/", "tokens": ["Und", "was", "von", "Bil\u00b7dern", "mehr", "Co\u00b7rin\u00b7thus", "hat", "ge\u00b7gos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "ADV", "NE", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und das noch alte Rom gehauen Felsen ein.", "tokens": ["Und", "das", "noch", "al\u00b7te", "Rom", "ge\u00b7hau\u00b7en", "Fel\u00b7sen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "ADJA", "NE", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Doch trieb der Ehrgeitz an: Hier ein gehorsam Wille/", "tokens": ["Doch", "trieb", "der", "Ehr\u00b7geitz", "an", ":", "Hier", "ein", "ge\u00b7hor\u00b7sam", "Wil\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$.", "ADV", "ART", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der Kindes-Lieb und Pflicht zu einem Nachbar hat.", "tokens": ["Der", "Kin\u00b7des\u00b7Lieb", "und", "Pflicht", "zu", "ei\u00b7nem", "Nach\u00b7bar", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sie streiten/ wer zu erst die Schuldigkeit erf\u00fclle/", "tokens": ["Sie", "strei\u00b7ten", "/", "wer", "zu", "erst", "die", "Schul\u00b7dig\u00b7keit", "er\u00b7f\u00fcl\u00b7le", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "APPR", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und wer am nechsten ruh\u2019 bey dieser Grabe-Statt.", "tokens": ["Und", "wer", "am", "nechs\u00b7ten", "ruh'", "bey", "die\u00b7ser", "Gra\u00b7be\u00b7Statt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "ADJA", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Was scharrt ihr endlich ein? ein Hertz/ von dem das Hertze/", "tokens": ["Was", "scharrt", "ihr", "end\u00b7lich", "ein", "?", "ein", "Hertz", "/", "von", "dem", "das", "Hert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "ART", "NN", "$(", "APPR", "ART", "PDS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So st\u00fcndlich in euch klopfft/ den ersten Ursprung f\u00fchrt/", "tokens": ["So", "st\u00fcnd\u00b7lich", "in", "euch", "klopfft", "/", "den", "ers\u00b7ten", "Ur\u00b7sprung", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVFIN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die Sonne/ so bi\u00dfher durch ihrer Stralen Kertze", "tokens": ["Die", "Son\u00b7ne", "/", "so", "bi\u00df\u00b7her", "durch", "ih\u00b7rer", "Stra\u00b7len", "Kert\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Euch alle hat gew\u00e4rmt/ beseelet und geziert.", "tokens": ["Euch", "al\u00b7le", "hat", "ge\u00b7w\u00e4rmt", "/", "be\u00b7see\u00b7let", "und", "ge\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "VVPP", "$(", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ist sonst der Menschen Hertz das Wohnhaus ihrer Seele/", "tokens": ["Ist", "sonst", "der", "Men\u00b7schen", "Hertz", "das", "Wohn\u00b7haus", "ih\u00b7rer", "See\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So war der Mutter Hertz auch eures Lebens-Sitz.", "tokens": ["So", "war", "der", "Mut\u00b7ter", "Hertz", "auch", "eu\u00b7res", "Le\u00b7bens\u00b7Sitz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Regt und bewegt es sich erst in des C\u00f6rpers H\u00f6le/", "tokens": ["Regt", "und", "be\u00b7wegt", "es", "sich", "erst", "in", "des", "C\u00f6r\u00b7pers", "H\u00f6\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.33": {"text": "Und weckt die Geister auf/ und theilt aus Blut und Hitz.", "tokens": ["Und", "weckt", "die", "Geis\u00b7ter", "auf", "/", "und", "theilt", "aus", "Blut", "und", "Hitz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "$(", "KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So hat der Mutter Hertz/ worunter ihr gelegen/", "tokens": ["So", "hat", "der", "Mut\u00b7ter", "Hertz", "/", "wo\u00b7run\u00b7ter", "ihr", "ge\u00b7le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$(", "PWAV", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Noch vor dem Tage-Licht wohlth\u00e4tig sich erzeigt/", "tokens": ["Noch", "vor", "dem", "Ta\u00b7ge\u00b7Licht", "wohlt\u00b7h\u00e4\u00b7tig", "sich", "er\u00b7zeigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und denn ohn Unterla\u00df Heil/ Wonne/ Leben/ Seegen/", "tokens": ["Und", "denn", "ohn", "Un\u00b7ter\u00b7la\u00df", "Heil", "/", "Won\u00b7ne", "/", "Le\u00b7ben", "/", "See\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "NN", "$(", "NN", "$(", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und was man w\u00fcnschen mag/ euch mildreich zugeneigt.", "tokens": ["Und", "was", "man", "w\u00fcn\u00b7schen", "mag", "/", "euch", "mil\u00b7dreich", "zu\u00b7gen\u00b7eigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "VVINF", "VMFIN", "$(", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wohnt in dem Hertz ein Feur/ das von des Himmels-H\u00f6hen", "tokens": ["Wohnt", "in", "dem", "Hertz", "ein", "Feur", "/", "das", "von", "des", "Him\u00b7mels\u00b7H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$(", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Prometheus Kunst-Diebstal uns heimlich zu gewandt;", "tokens": ["Pro\u00b7me\u00b7theus", "Kunst\u00b7Dieb\u00b7stal", "uns", "heim\u00b7lich", "zu", "ge\u00b7wandt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "ADJD", "PTKZU", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So war der Mutter Hertz/ solt es zum HErren gehen/", "tokens": ["So", "war", "der", "Mut\u00b7ter", "Hertz", "/", "solt", "es", "zum", "Her\u00b7ren", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$(", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "In gar weit heil\u2019 ger Glut des Glaubens angebrant.", "tokens": ["In", "gar", "weit", "heil'", "ger", "Glut", "des", "Glau\u00b7bens", "an\u00b7ge\u00b7brant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "ADJD", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure"