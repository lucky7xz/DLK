{"dta.poem.21337": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput IX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von C\u00f6llen war ich drei Viertel auf Acht", "tokens": ["Von", "C\u00f6l\u00b7len", "war", "ich", "drei", "Vier\u00b7tel", "auf", "Acht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VAFIN", "PPER", "CARD", "NN", "APPR", "CARD"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Morgens fortgereiset;", "tokens": ["Des", "Mor\u00b7gens", "fort\u00b7ge\u00b7rei\u00b7set", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir kamen nach Hagen schon gegen Drey,", "tokens": ["Wir", "ka\u00b7men", "nach", "Ha\u00b7gen", "schon", "ge\u00b7gen", "Drey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "ADV", "APPR", "CARD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da wird zu Mittag gespeiset.", "tokens": ["Da", "wird", "zu", "Mit\u00b7tag", "ge\u00b7spei\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}