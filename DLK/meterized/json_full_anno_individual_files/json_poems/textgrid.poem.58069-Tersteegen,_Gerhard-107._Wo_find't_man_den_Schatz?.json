{"textgrid.poem.58069": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "107. Wo find't man den Schatz?", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man sucht und findet nicht, doch ist der Schatz so nah;", "tokens": ["Man", "sucht", "und", "fin\u00b7det", "nicht", ",", "doch", "ist", "der", "Schatz", "so", "nah", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "PTKNEG", "$,", "ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was l\u00e4ufst du viel herum? Er ist im Herzen ja;", "tokens": ["Was", "l\u00e4ufst", "du", "viel", "he\u00b7rum", "?", "Er", "ist", "im", "Her\u00b7zen", "ja", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "PPER", "VAFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verkauf nur, was du hast, und geh in dich hinein,", "tokens": ["Ver\u00b7kauf", "nur", ",", "was", "du", "hast", ",", "und", "geh", "in", "dich", "hin\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PWS", "PPER", "VAFIN", "$,", "KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird der beste Schatz, Gott selber, deine sein!", "tokens": ["So", "wird", "der", "bes\u00b7te", "Schatz", ",", "Gott", "sel\u00b7ber", ",", "dei\u00b7ne", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct",