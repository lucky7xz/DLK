{"dta.poem.14799": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das kalte Liebchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Er. ", "tokens": ["Er", "."], "token_info": ["word", "punct"], "pos": ["PPER", "$."], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Sie. Finster ist mein K\u00e4mmerlein.", "tokens": ["Sie", ".", "Fins\u00b7ter", "ist", "mein", "K\u00e4m\u00b7mer\u00b7lein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er. Ach, ich finde doch mein Pl\u00e4tzchen.", "tokens": ["Er", ".", "Ach", ",", "ich", "fin\u00b7de", "doch", "mein", "Pl\u00e4tz\u00b7chen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "ITJ", "$,", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie. Und mein Bett ist eng und klein.", "tokens": ["Sie", ".", "Und", "mein", "Bett", "ist", "eng", "und", "klein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}