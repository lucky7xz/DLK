{"textgrid.poem.58081": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "119. Traue und la\u00df dich f\u00fchren!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du mu\u00dft auf Gott es blindlings wagen,", "tokens": ["Du", "mu\u00dft", "auf", "Gott", "es", "blind\u00b7lings", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht immer forschen, f\u00fcrchten, klagen;", "tokens": ["Nicht", "im\u00b7mer", "for\u00b7schen", ",", "f\u00fcrch\u00b7ten", ",", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df dich ihm ganz im Einfaltssinn!", "tokens": ["La\u00df", "dich", "ihm", "ganz", "im", "Ein\u00b7falts\u00b7sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verlier dich selbst in Gottes H\u00e4nde!", "tokens": ["Ver\u00b7lier", "dich", "selbst", "in", "Got\u00b7tes", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie er dich f\u00fchrt, auch du dich wende", "tokens": ["Wie", "er", "dich", "f\u00fchrt", ",", "auch", "du", "dich", "wen\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF