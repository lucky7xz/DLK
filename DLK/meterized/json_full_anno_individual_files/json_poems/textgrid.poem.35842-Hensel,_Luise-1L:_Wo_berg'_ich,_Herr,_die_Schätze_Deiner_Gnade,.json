{"textgrid.poem.35842": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo berg' ich, Herr, die Sch\u00e4tze Deiner Gnade,", "genre": "verse", "period": "N.A.", "pub_year": 1853, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo berg' ich, Herr, die Sch\u00e4tze Deiner Gnade,", "tokens": ["Wo", "ber\u00b7g'", "ich", ",", "Herr", ",", "die", "Sch\u00e4t\u00b7ze", "Dei\u00b7ner", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Auf da\u00df nicht gier'ge Feinde sie mir rauben?", "tokens": ["Auf", "da\u00df", "nicht", "gier'\u00b7ge", "Fein\u00b7de", "sie", "mir", "rau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PTKNEG", "ADJA", "NN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie schwach und brechlich ist die arme Lade,", "tokens": ["Wie", "schwach", "und", "brech\u00b7lich", "ist", "die", "ar\u00b7me", "La\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Darin sie ruh'n; kann sicher ich sie glauben?", "tokens": ["Da\u00b7rin", "sie", "ruh'n", ";", "kann", "si\u00b7cher", "ich", "sie", "glau\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVINF", "$.", "VMFIN", "ADJD", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Starkbewaffnete wacht vor der Th\u00fcre", "tokens": ["Der", "Stark\u00b7be\u00b7waff\u00b7ne\u00b7te", "wacht", "vor", "der", "Th\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und droht, zu brechen ihre schwachen Riegel,", "tokens": ["Und", "droht", ",", "zu", "bre\u00b7chen", "ih\u00b7re", "schwa\u00b7chen", "Rie\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PTKZU", "VVINF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df er mir Deine G\u00fcter all' entf\u00fchre", "tokens": ["Da\u00df", "er", "mir", "Dei\u00b7ne", "G\u00fc\u00b7ter", "all'", "ent\u00b7f\u00fch\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zusammt der Seele, Deiner Gottheit Spiegel.", "tokens": ["Zu\u00b7sammt", "der", "See\u00b7le", ",", "Dei\u00b7ner", "Got\u00b7theit", "Spie\u00b7gel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und durch die Fenster, mit geschminkten Wangen", "tokens": ["Und", "durch", "die", "Fens\u00b7ter", ",", "mit", "ge\u00b7schmink\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lugt mir ein Weib, und will mir Flitter bieten,", "tokens": ["Lugt", "mir", "ein", "Weib", ",", "und", "will", "