{"textgrid.poem.58743": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So mal dich selbst denn ab, werd mir ins Herz gedr\u00fccket,", "tokens": ["So", "mal", "dich", "selbst", "denn", "ab", ",", "werd", "mir", "ins", "Herz", "ge\u00b7dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "ADV", "PTKVZ", "$,", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du Vorwurf, g\u00f6ttlich sch\u00f6n, der meine Seel entz\u00fccket,", "tokens": ["Du", "Vor\u00b7wurf", ",", "g\u00f6tt\u00b7lich", "sch\u00f6n", ",", "der", "mei\u00b7ne", "Seel", "ent\u00b7z\u00fc\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADJD", "ADJD", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Treib alle Geister draus, die mir verf\u00fchrend sind,", "tokens": ["Treib", "al\u00b7le", "Geis\u00b7ter", "draus", ",", "die", "mir", "ver\u00b7f\u00fch\u00b7rend", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PAV", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit deiner reinen Flamm' der Liebe es entz\u00fcnd!", "tokens": ["Mit", "dei\u00b7ner", "rei\u00b7nen", "Flamm'", "der", "Lie\u00b7be", "es", "ent\u00b7z\u00fcnd", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wann wird's geschehn, mein Gott, da\u00df ich dich hab' alleine", "tokens": ["Wann", "wird's", "ge\u00b7schehn", ",", "mein", "Gott", ",", "da\u00df", "ich", "dich", "hab'", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "VVPP", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sonst kein Vorwurf mehr erscheine?", "tokens": ["Und", "sonst", "kein", "Vor\u00b7wurf", "mehr", "er\u00b7schei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Himmelsbr\u00e4ut'gam, male fein", "tokens": ["Mein", "Him\u00b7mels\u00b7br\u00e4ut'\u00b7gam", ",", "ma\u00b7le", "fein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich selbst durch Gnad' ins Herze ein!", "tokens": ["Dich", "selbst", "durch", "Gnad'", "ins", "Her\u00b7ze", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "APPRART", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich liebe nichts als dich, mein's Glaubens Vorwurf wichtig,", "tokens": ["Ich", "lie\u00b7be", "nichts", "als", "dich", ",", "mein's", "Glau\u00b7bens", "Vor\u00b7wurf", "wich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "