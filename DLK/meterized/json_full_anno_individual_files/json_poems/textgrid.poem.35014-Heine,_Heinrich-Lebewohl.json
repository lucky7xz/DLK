{"textgrid.poem.35014": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lebewohl", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hatte wie ein Pelikan", "tokens": ["Hat\u00b7te", "wie", "ein", "Pe\u00b7li\u00b7kan"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "KOKOM", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich mit eignem Blut getr\u00e4nket,", "tokens": ["Dich", "mit", "eig\u00b7nem", "Blut", "ge\u00b7tr\u00e4n\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du hast mir jetzt zum Dank", "tokens": ["Und", "du", "hast", "mir", "jetzt", "zum", "Dank"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gall' und Wermut eingeschenket.", "tokens": ["Gall'", "und", "Wer\u00b7mut", "ein\u00b7ge\u00b7schen\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "B\u00f6se war es nicht gemeint,", "tokens": ["B\u00f6\u00b7se", "war", "es", "nicht", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und so heiter blieb die Stirne;", "tokens": ["Und", "so", "hei\u00b7ter", "blieb", "die", "Stir\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leider mit Verge\u00dflichkeit", "tokens": ["Lei\u00b7der", "mit", "Ver\u00b7ge\u00df\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Angef\u00fcllt ist dein Gehirne.", "tokens": ["An\u00b7ge\u00b7f\u00fcllt", "ist", "dein", "Ge\u00b7hir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun leb wohl \u2013 du merkst es kaum,", "tokens": ["Nun", "leb", "wohl", "\u2013", "du", "merkst", "es", "kaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$(", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da\u00df ich weinend von dir scheide.", "tokens": ["Da\u00df", "ich", "wei\u00b7nend", "von", "dir", "schei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott erhalte, T\u00f6rin, dir", "tokens": ["Gott", "er\u00b7hal\u00b7te", ",", "T\u00f6\u00b7rin", ",", "dir"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "VVFIN", "$,", "NN", "$,", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Flattersinn und Lebensfreude!", "tokens": ["Flat\u00b7ter\u00b7sinn", "und", "Le\u00b7bens\u00b7freu\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hatte wie ein Pelikan", "tokens": ["Hat\u00b7te", "wie", "ein", "Pe\u00b7li\u00b