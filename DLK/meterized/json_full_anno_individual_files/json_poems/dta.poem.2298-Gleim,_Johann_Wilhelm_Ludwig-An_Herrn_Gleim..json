{"dta.poem.2298": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Gleim.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nimm mich mit, geliebter Damon,", "tokens": ["Nimm", "mich", "mit", ",", "ge\u00b7lieb\u00b7ter", "Da\u00b7mon", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimm mich mit auf deine Fluren.", "tokens": ["Nimm", "mich", "mit", "auf", "dei\u00b7ne", "Flu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df mich dort den iungen Fr\u00fcling,", "tokens": ["La\u00df", "mich", "dort", "den", "i\u00b7un\u00b7gen", "Fr\u00fc\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und den Glanz der Morgenr\u00f6te,", "tokens": ["Und", "den", "Glanz", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Th\u00e4ler voll Violen,", "tokens": ["Und", "die", "Th\u00e4\u00b7ler", "voll", "Vi\u00b7o\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und den Thau auf m\u00fcden Blumen", "tokens": ["Und", "den", "Thau", "auf", "m\u00fc\u00b7den", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die fr\u00fche Venus sehen.", "tokens": ["Und", "die", "fr\u00fc\u00b7he", "Ve\u00b7nus", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schweig! es lispelt schon ein Zefir", "tokens": ["Schweig", "!", "es", "lis\u00b7pelt", "schon", "ein", "Ze\u00b7fir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Ein vergn\u00fcgter Freund des Lenzen.", "tokens": ["Ein", "ver\u00b7gn\u00fcg\u00b7ter", "Freund", "des", "Len\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sieh! er w\u00e4lzt sich auf dem Grase,", "tokens": ["Sieh", "!", "er", "w\u00e4lzt", "sich", "auf", "dem", "Gra\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und im W\u00e4lzen k\u00fc\u00dft er Blumen,", "tokens": ["Und", "im", "W\u00e4l\u00b7zen", "k\u00fc\u00dft", "er", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und die wankende Narzisse,", "tokens": ["Und", "die", "wan\u00b7ken\u00b7de", "Nar\u00b7zis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.13": {"text": "Wird verliebt und k\u00fc\u00dft ihn wieder.", "tokens": ["Wird", "ver\u00b7liebt", "und", "k\u00fc\u00dft", "ihn", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Komm, wir wollen ihn erhaschen,", "tokens": ["Komm", ",", "wir", "wol\u00b7len", "ihn", "er\u00b7ha\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und es soll sein sanftes S\u00e4useln,", "tokens": ["Und", "es", "soll", "sein", "sanf\u00b7tes", "S\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Uns bis in den Busch begleiten,", "tokens": ["Uns", "bis", "in", "den", "Busch", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Wo wir seinen Freund, den Fr\u00fcling,", "tokens": ["Wo", "wir", "sei\u00b7nen", "Freund", ",", "den", "Fr\u00fc\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Unter Linden suchen wollen.", "tokens": ["Un\u00b7ter", "Lin\u00b7den", "su\u00b7chen", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Komm, so bald wir ihn gefunden,", "tokens": ["Komm", ",", "so", "bald", "wir", "ihn", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ADV", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Wollen wir, in seinen Armen,", "tokens": ["Wol\u00b7len", "wir", ",", "in", "sei\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "An dem weichsten Ufer schlummern;", "tokens": ["An", "dem", "weichs\u00b7ten", "U\u00b7fer", "schlum\u00b7mern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Bis uns ein vergn\u00fcgtes M\u00e4dchen,", "tokens": ["Bis", "uns", "ein", "ver\u00b7gn\u00fcg\u00b7tes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Welches unser Schlummer \u00e4rgert,", "tokens": ["Wel\u00b7ches", "un\u00b7ser", "Schlum\u00b7mer", "\u00e4r\u00b7gert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Durch ein Sch\u00e4ferlied erwekket.", "tokens": ["Durch", "ein", "Sch\u00e4\u00b7fer\u00b7lied", "er\u00b7wek\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}