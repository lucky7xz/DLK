{"dta.poem.7305": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Das vierzehnj\u00e4hrige Herz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Er ist so sch\u00f6n! \u2014 sein lichtes Haar", "tokens": ["Er", "ist", "so", "sch\u00f6n", "!", "sein", "lich\u00b7tes", "Haar"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "$(", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Das m\u00f6cht' ich mit Keinem vertauschen,", "tokens": ["Das", "m\u00f6cht'", "ich", "mit", "Kei\u00b7nem", "ver\u00b7tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "APPR", "PIS", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wie seidene F\u00e4den so weich und klar,", "tokens": ["Wie", "sei\u00b7de\u00b7ne", "F\u00e4\u00b7den", "so", "weich", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wenn zarte L\u00f6ckchen sich bauschen;", "tokens": ["Wenn", "zar\u00b7te", "L\u00f6ck\u00b7chen", "sich", "bau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Oft streichl' ich es, dann lacht er traun,", "tokens": ["Oft", "streichl'", "ich", "es", ",", "dann", "lacht", "er", "traun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nennt mich \u201eseine alberne Barbe\u201c;", "tokens": ["Nennt", "mich", "\u201e", "sei\u00b7ne", "al\u00b7ber\u00b7ne", "Bar\u00b7be", "\u201c", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$(", "PPOSAT", "ADJA", "NN", "$(", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Es ist nicht schwarz, nicht blond, nicht braun,", "tokens": ["Es", "ist", "nicht", "schwarz", ",", "nicht", "blond", ",", "nicht", "braun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nun rathet, wie nennt sich die Farbe?", "tokens": ["Nun", "ra\u00b7thet", ",", "wie", "nennt", "sich", "die", "Far\u00b7be", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Und seine Geberde ist k\u00f6niglich,", "tokens": ["Und", "sei\u00b7ne", "Ge\u00b7ber\u00b7de", "ist", "k\u00f6\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Geht majest\u00e4tisch zu Herzen,", "tokens": ["Geht", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "zu", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zuckt er die Braue, dann f\u00fcrcht' ich mich,", "tokens": ["Zuckt", "er", "die", "Brau\u00b7e", ",", "dann", "f\u00fcrcht'", "ich", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und m\u00f6chte auch weinen vor Schmerzen;", "tokens": ["Und", "m\u00f6ch\u00b7te", "auch", "wei\u00b7nen", "vor", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVINF", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und wieder seh' ich sein L\u00e4cheln bl\u00fchn,", "tokens": ["Und", "wie\u00b7der", "seh'", "ich", "sein", "L\u00e4\u00b7cheln", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "So klar wie das reine Gewissen,", "tokens": ["So", "klar", "wie", "das", "rei\u00b7ne", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Da m\u00f6chte ich gleich auf den Schemel knien,", "tokens": ["Da", "m\u00f6ch\u00b7te", "ich", "gleich", "auf", "den", "Sche\u00b7mel", "kni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und die guten H\u00e4nde ihm k\u00fcssen.", "tokens": ["Und", "die", "gu\u00b7ten", "H\u00e4n\u00b7de", "ihm", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Heut' bin ich in aller Fr\u00fche erwacht,", "tokens": ["Heut'", "bin", "ich", "in", "al\u00b7ler", "Fr\u00fc\u00b7he", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Beim ersten Glitzern der Sonnen,", "tokens": ["Beim", "ers\u00b7ten", "Glit\u00b7zern", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und habe mich gleich auf die Sohlen gemacht,", "tokens": ["Und", "ha\u00b7be", "mich", "gleich", "auf", "die", "Soh\u00b7len", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Zum H\u00fcgel dr\u00fcben am Bronnen;", "tokens": ["Zum", "H\u00fc\u00b7gel", "dr\u00fc\u00b7ben", "am", "Bron\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Erdbeeren fand ich, gl\u00fch wie Rubin,", "tokens": ["Erd\u00b7bee\u00b7ren", "fand", "ich", ",", "gl\u00fch", "wie", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJD", "KOKOM", "NE", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schau, wie im Korbe sie lachen!", "tokens": ["Schau", ",", "wie", "im", "Kor\u00b7be", "sie", "la\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Die stell ich ihm nun an das Lager hin,", "tokens": ["Die", "stell", "ich", "ihm", "nun", "an", "das", "La\u00b7ger", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da sieht er sie gleich beim Erwachen.", "tokens": ["Da", "sieht", "er", "sie", "gleich", "beim", "Er\u00b7wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Ich wei\u00df, er denkt mit dem ersten Blick,", "tokens": ["Ich", "wei\u00df", ",", "er", "denkt", "mit", "dem", "ers\u00b7ten", "Blick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u201edas that meine alberne Barbe!\u201c", "tokens": ["\u201e", "das", "that", "mei\u00b7ne", "al\u00b7ber\u00b7ne", "Bar\u00b7be", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und freundlich streicht er das Haar zur\u00fcck", "tokens": ["Und", "freund\u00b7lich", "streicht", "er", "das", "Haar", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von seiner r\u00fchmlichen Narbe,", "tokens": ["Von", "sei\u00b7ner", "r\u00fchm\u00b7li\u00b7chen", "Nar\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ruft mich bei Namen, und zieht mich nah,", "tokens": ["Ruft", "mich", "bei", "Na\u00b7men", ",", "und", "zieht", "mich", "nah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Da\u00df Thr\u00e4nen die Augen mir tr\u00fcben;", "tokens": ["Da\u00df", "Thr\u00e4\u00b7nen", "die", "Au\u00b7gen", "mir", "tr\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Ach, er ist mein herrlicher Vater ja,", "tokens": ["Ach", ",", "er", "ist", "mein", "herr\u00b7li\u00b7cher", "Va\u00b7ter", "ja", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "PTKANT", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Soll ich ihn denn nicht lieben, nicht lieben!", "tokens": ["Soll", "ich", "ihn", "denn", "nicht", "lie\u00b7ben", ",", "nicht", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF", "$."], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}}}}}