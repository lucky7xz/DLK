": "trochaic.tetra"}, "line.2": {"text": "So lang' er nicht will schwitzen;", "tokens": ["So", "lang'", "er", "nicht", "will", "schwit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier wollen wir im Rebenhain", "tokens": ["Hier", "wol\u00b7len", "wir", "im", "Re\u00b7ben\u00b7hain"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bey gro\u00dfen Trauben sitzen:", "tokens": ["Bey", "gro\u00b7\u00dfen", "Trau\u00b7ben", "sit\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Mit Weinlaub unser Haupt bekr\u00f6nt,", "tokens": ["Mit", "Wein\u00b7laub", "un\u00b7ser", "Haupt", "be\u00b7kr\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Thyrsen unsre Lanzen,", "tokens": ["Und", "Thyr\u00b7sen", "uns\u00b7re", "Lan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn hoch der Chor Evoeh t\u00f6nt,", "tokens": ["Wenn", "hoch", "der", "Chor", "E\u00b7voeh", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Um Vater Bacchus tanzen:", "tokens": ["Um", "Va\u00b7ter", "Bac\u00b7chus", "tan\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOU