{"textgrid.poem.52994": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr Bacchus ist der beste Mann", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Bacchus ist der beste Mann", "tokens": ["Herr", "Bac\u00b7chus", "ist", "der", "bes\u00b7te", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu einem Schutzpatrone;", "tokens": ["Zu", "ei\u00b7nem", "Schutz\u00b7pat\u00b7ro\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir nehmen ihn zum Heilgen an:", "tokens": ["Wir", "neh\u00b7men", "ihn", "zum", "Heil\u00b7gen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bringt her die Epheukrone.", "tokens": ["Bringt", "her", "die", "E\u00b7pheu\u00b7kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es mag Herkul, der Griesgram, sich", "tokens": ["Es", "mag", "Her\u00b7kul", ",", "der", "Gries\u00b7gram", ",", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VMFIN", "NN", "$,", "ART", "NN", "$,", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Ungeheuern hauen;", "tokens": ["Mit", "Un\u00b7ge\u00b7heu\u00b7ern", "hau\u00b7en", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier wollen wir uns br\u00fcderlich", "tokens": ["Hier", "wol\u00b7len", "wir", "uns", "br\u00fc\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bey Libers Schlauch erbauen.", "tokens": ["Bey", "Li\u00b7bers", "Schlauch", "er\u00b7bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mag Orpheus vor der H\u00f6llenth\u00fcr,", "tokens": ["Mag", "Or\u00b7pheus", "vor", "der", "H\u00f6l\u00b7lent\u00b7h\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der B\u00e4nkels\u00e4nger, leyern;", "tokens": ["Der", "B\u00e4n\u00b7kel\u00b7s\u00e4n\u00b7ger", ",", "ley\u00b7ern", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier wollen wir ein Fest daf\u00fcr", "tokens": ["Hier", "wol\u00b7len", "wir", "ein", "Fest", "da\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Rebengotte feyern.", "tokens": ["Dem", "Re\u00b7ben\u00b7got\u00b7te", "fey\u00b7ern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Paduaner Anton mag", "tokens": ["Der", "Pa\u00b7du\u00b7a\u00b7ner", "An\u00b7ton", "mag"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit weisen Fischen reden;", "tokens": ["Mit", "wei\u00b7sen", "Fi\u00b7schen", "re\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier wollen wir bey dem Gelag", "tokens": ["Hier", "wol\u00b7len", "wir", "bey", "dem", "Ge\u00b7lag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Im Wein die Grillen t\u00f6dten.", "tokens": ["Im", "Wein", "die", "Gril\u00b7len", "t\u00f6d\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mag unsertwegen hundert Jahr,", "tokens": ["Mag", "un\u00b7sert\u00b7we\u00b7gen", "hun\u00b7dert", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Troste frommer Seelen,", "tokens": ["Zum", "Tros\u00b7te", "from\u00b7mer", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein dicker M\u00f6nch Sanct Januar", "tokens": ["Ein", "di\u00b7cker", "M\u00f6nch", "Sanct", "Ja\u00b7nu\u00b7ar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch eins zu Tode qu\u00e4len:", "tokens": ["Noch", "eins", "zu", "To\u00b7de", "qu\u00e4\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mag ganz Neapel Zeter schreyn,", "tokens": ["Mag", "ganz", "Nea\u00b7pel", "Ze\u00b7ter", "schreyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NE", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So lang' er nicht will schwitzen;", "tokens": ["So", "lang'", "er", "nicht", "will", "schwit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier wollen wir im Rebenhain", "tokens": ["Hier", "wol\u00b7len", "wir", "im", "Re\u00b7ben\u00b7hain"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bey gro\u00dfen Trauben sitzen:", "tokens": ["Bey", "gro\u00b7\u00dfen", "Trau\u00b7ben", "sit\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mit Weinlaub unser Haupt bekr\u00f6nt,", "tokens": ["Mit", "Wein\u00b7laub", "un\u00b7ser", "Haupt", "be\u00b7kr\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Thyrsen unsre Lanzen,", "tokens": ["Und", "Thyr\u00b7sen", "uns\u00b7re", "Lan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn hoch der Chor Evoeh t\u00f6nt,", "tokens": ["Wenn", "hoch", "der", "Chor", "E\u00b7voeh", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Um Vater Bacchus tanzen:", "tokens": ["Um", "Va\u00b7ter", "Bac\u00b7chus", "tan\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Rund um den gro\u00dfen Wundermann", "tokens": ["Rund", "um", "den", "gro\u00b7\u00dfen", "Wun\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und seine Tieger springen;", "tokens": ["Und", "sei\u00b7ne", "Tie\u00b7ger", "sprin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wer den Chor nicht halten kann,", "tokens": ["Und", "wer", "den", "Chor", "nicht", "hal\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch mit Evoeh singen.", "tokens": ["Doch", "mit", "E\u00b7voeh", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Er schuf der Kelter Zaubersaft,", "tokens": ["Er", "schuf", "der", "Kel\u00b7ter", "Zau\u00b7ber\u00b7saft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gab in Purpurreben", "tokens": ["Und", "gab", "in", "Pur\u00b7pur\u00b7re\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Erdens\u00f6hnen G\u00f6tterkraft", "tokens": ["Den", "Er\u00b7den\u00b7s\u00f6h\u00b7nen", "G\u00f6t\u00b7ter\u00b7kraft"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu einem neuen Leben.", "tokens": ["Zu", "ei\u00b7nem", "neu\u00b7en", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Er wandelt durch das Erdenrund", "tokens": ["Er", "wan\u00b7delt", "durch", "das", "Er\u00b7den\u00b7rund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohlth\u00e4tig mit Geschenken,", "tokens": ["Wohlt\u00b7h\u00e4\u00b7tig", "mit", "Ge\u00b7schen\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vom Indusstrande nach Burgund,", "tokens": ["Vom", "In\u00b7dus\u00b7stran\u00b7de", "nach", "Bur\u00b7gund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sterblichen zu tr\u00e4nken.", "tokens": ["Die", "Sterb\u00b7li\u00b7chen", "zu", "tr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Von Cypern bis zum Hoffnungskap,", "tokens": ["Von", "Cy\u00b7pern", "bis", "zum", "Hoff\u00b7nungs\u00b7kap", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Tokay bis zum Rheine", "tokens": ["Von", "To\u00b7kay", "bis", "zum", "Rhei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "APPRART", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Deckt, wo er geht, sein G\u00f6tterstab", "tokens": ["Deckt", ",", "wo", "er", "geht", ",", "sein", "G\u00f6t\u00b7ter\u00b7stab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die H\u00fcgel stracks mit Weine.", "tokens": ["Die", "H\u00fc\u00b7gel", "stracks", "mit", "Wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Er schickt sein gramverscheuchend Gut", "tokens": ["Er", "schickt", "sein", "gram\u00b7ver\u00b7scheu\u00b7chend", "Gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entfernten Nationen,", "tokens": ["Ent\u00b7fern\u00b7ten", "Na\u00b7ti\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die nah am Pol mit kaltem Blut", "tokens": ["Die", "nah", "am", "Pol", "mit", "kal\u00b7tem", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Schoos des Winters wohnen.", "tokens": ["Im", "Schoos", "des", "Win\u00b7ters", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Trinkt, Br\u00fcder, la\u00dft uns Sterblichkeit", "tokens": ["Trinkt", ",", "Br\u00fc\u00b7der", ",", "la\u00dft", "uns", "Sterb\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "VVIMP", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Gruft und Tod vergessen,", "tokens": ["Und", "Gruft", "und", "Tod", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und uns schon jetzt mit Ewigkeit", "tokens": ["Und", "uns", "schon", "jetzt", "mit", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit den G\u00f6ttern messen.", "tokens": ["Und", "mit", "den", "G\u00f6t\u00b7tern", "mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.14": {"line.1": {"text": "Trinkt, Winzer, eure Humpen leer,", "tokens": ["Trinkt", ",", "Win\u00b7zer", ",", "eu\u00b7re", "Hum\u00b7pen", "leer", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fcllet Korb und St\u00e4nder,", "tokens": ["Und", "f\u00fcl\u00b7let", "Korb", "und", "St\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und lehnt, wird euch das Haupt zu schwer,", "tokens": ["Und", "lehnt", ",", "wird", "euch", "das", "Haupt", "zu", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VAFIN", "PPER", "ART", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Euch fest an das Gel\u00e4nder.", "tokens": ["Euch", "fest", "an", "das", "Ge\u00b7l\u00e4n\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.15": {"line.1": {"text": "Evoeh, Bacche, Jacche!", "tokens": ["E\u00b7voeh", ",", "Bac\u00b7che", ",", "Jac\u00b7che", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ly\u00e4ens Nektar winket;", "tokens": ["Ly\u00b7\u00e4\u00b7ens", "Nek\u00b7tar", "win\u00b7ket", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hebt volle Tummler in die H\u00f6h,", "tokens": ["Hebt", "vol\u00b7le", "Tumm\u00b7ler", "in", "die", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jauchzt Libern Dank, und trinket.", "tokens": ["Jauchzt", "Li\u00b7bern", "Dank", ",", "und", "trin\u00b7ket", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Herr Bacch