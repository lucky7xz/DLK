{"textgrid.poem.41603": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nie wird die Zierlichkeit der Sch\u00f6nen aus Vignetten,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nie wird die Zierlichkeit der Sch\u00f6nen aus Vignetten,", "tokens": ["Nie", "wird", "die", "Zier\u00b7lich\u00b7keit", "der", "Sch\u00f6\u00b7nen", "aus", "Vig\u00b7net\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verdorbne Kinder, die ein krank Jahrhundert trug,", "tokens": ["Ver\u00b7dorb\u00b7ne", "Kin\u00b7der", ",", "die", "ein", "krank", "Jahr\u00b7hun\u00b7dert", "trug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die F\u00fc\u00dfe, die verschn\u00fcrt, die Hand mit Kastagnetten", "tokens": ["Die", "F\u00fc\u00b7\u00dfe", ",", "die", "ver\u00b7schn\u00fcrt", ",", "die", "Hand", "mit", "Kas\u00b7tag\u00b7net\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "$,", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Befriedigen ein Herz wie meins mit ihrem Lug.", "tokens": ["Be\u00b7frie\u00b7di\u00b7gen", "ein", "Herz", "wie", "meins", "mit", "ih\u00b7rem", "Lug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KOKOM", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gavarni, der Poet der Bl\u00e4sse, feire seine", "tokens": ["Ga\u00b7var\u00b7ni", ",", "der", "Po\u00b7et", "der", "Bl\u00e4s\u00b7se", ",", "fei\u00b7re", "sei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "ART", "NN", "$,", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gezierten, fl\u00fcsternden Gesch\u00f6pfe vom Spital,", "tokens": ["Ge\u00b7zier\u00b7ten", ",", "fl\u00fcs\u00b7tern\u00b7den", "Ge\u00b7sch\u00f6p\u00b7fe", "vom", "Spi\u00b7tal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch ist in dieser Schar von bleichen Rosen keine,", "tokens": ["Doch", "ist", "in", "die\u00b7ser", "Schar", "von", "blei\u00b7chen", "Ro\u00b7sen", "kei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PDAT", "NN", "APPR", "ADJA", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die je erreichen mag mein rotes Ideal.", "tokens": ["Die", "je", "er\u00b7rei\u00b7chen", "mag", "mein", "ro\u00b7tes", "I\u00b7deal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "VMFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was meinem Herzen ich, dem abgrundtiefen, w\u00e4hle,", "tokens": ["Was", "mei\u00b7nem", "Her\u00b7zen", "ich", ",", "dem", "ab\u00b7grund\u00b7tie\u00b7fen", ",", "w\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word