{"textgrid.poem.43930": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier seze dich, versch\u00e4mtes Kind;", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier seze dich, versch\u00e4mtes Kind;", "tokens": ["Hier", "se\u00b7ze", "dich", ",", "ver\u00b7sch\u00e4m\u00b7tes", "Kind", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier ist gut seyn, hier las uns bleiben,", "tokens": ["Hier", "ist", "gut", "seyn", ",", "hier", "las", "uns", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "VAINF", "$,", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Lind und West gespr\u00e4chig sind", "tokens": ["Wo", "Lind", "und", "West", "ge\u00b7spr\u00e4\u00b7chig", "sind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "KON", "NN", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Fels und Wald den Gram vertreiben;", "tokens": ["Und", "Fels", "und", "Wald", "den", "Gram", "ver\u00b7trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In dieser gr\u00fcnen Einsamkeit,", "tokens": ["In", "die\u00b7ser", "gr\u00fc\u00b7nen", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo Bach und Stein und Bl\u00e4tter rauschen,", "tokens": ["Wo", "Bach", "und", "Stein", "und", "Bl\u00e4t\u00b7ter", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Soll weder List, Gefahr noch Neid", "tokens": ["Soll", "we\u00b7der", "List", ",", "Ge\u00b7fahr", "noch", "Neid"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "KON", "NN", "$,", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den s\u00fc\u00dfen Fr\u00fchlingsscherz belauschen.", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Fr\u00fch\u00b7lings\u00b7scherz", "be\u00b7lau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Sch\u00e4ze deiner keuschen Zucht", "tokens": ["Die", "Sch\u00e4\u00b7ze", "dei\u00b7ner", "keu\u00b7schen", "Zucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und der noch unber\u00fchrten Br\u00fcste", "tokens": ["Und", "der", "noch", "un\u00b7be\u00b7r\u00fchr\u00b7ten", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sind warlich eine seltne Frucht,", "tokens": ["Sind", "war\u00b7lich", "ei\u00b7ne", "selt\u00b7ne", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach der ich innerlich gel\u00fcste.", "tokens": ["Nach", "der", "ich", "in\u00b7ner\u00b7lich", "ge\u00b7l\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erschrick nicht vor der schnellen Hand", "tokens": ["Er\u00b7schrick", "nicht", "vor", "der", "schnel\u00b7len", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und las sie in dem Busen spielen;", "tokens": ["Und", "las", "sie", "in", "dem", "Bu\u00b7sen", "spie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich f\u00fchre dich in einen Stand,", "tokens": ["Ich", "f\u00fch\u00b7re", "dich", "in", "ei\u00b7nen", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Lebens Kern und Marck zu f\u00fchlen.", "tokens": ["Des", "Le\u00b7bens", "Kern", "und", "Marck", "zu", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wohin mein Ku\u00df dein Wange dr\u00fcckt,", "tokens": ["Wo\u00b7hin", "mein", "Ku\u00df", "dein", "Wan\u00b7ge", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da w\u00e4chst der Rosen Glanz und Menge;", "tokens": ["Da", "w\u00e4chst", "der", "Ro\u00b7sen", "Glanz", "und", "Men\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So bald mich nur die Haut entz\u00fcckt,", "tokens": ["So", "bald", "mich", "nur", "die", "Haut", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt Herz und Sehnsucht ins Gedr\u00e4nge,", "tokens": ["Kommt", "Herz", "und", "Sehn\u00b7sucht", "ins", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da wallt, da springt es in der Brust,", "tokens": ["Da", "wallt", ",", "da", "springt", "es", "in", "der", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da will es sich genau verbinden,", "tokens": ["Da", "will", "es", "sich", "ge\u00b7nau", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ach paare doch mit ihm die Lust", "tokens": ["Ach", "paa\u00b7re", "doch", "mit", "ihm", "die", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und las es seine Ruhstatt finden!", "tokens": ["Und", "las", "es", "sei\u00b7ne", "Ruh\u00b7statt", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Vor was err\u00f6thestu, mein Licht?", "tokens": ["Vor", "was", "er\u00b7r\u00f6\u00b7thes\u00b7tu", ",", "mein", "Licht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PWS", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich werde dich nichts B\u00f6ses lehren;", "tokens": ["Ich", "wer\u00b7de", "dich", "nichts", "B\u00f6\u00b7ses", "leh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du kennst das s\u00fc\u00dfe Spiel noch nicht,", "tokens": ["Du", "kennst", "das", "s\u00fc\u00b7\u00dfe", "Spiel", "noch", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Anblick raubt mir Sehn und H\u00f6ren.", "tokens": ["Dein", "An\u00b7blick", "raubt", "mir", "Sehn", "und", "H\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Liebe w\u00fcntscht dich in ihr Reich,", "tokens": ["Die", "Lie\u00b7be", "w\u00fcnt\u00b7scht", "dich", "in", "ihr", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.6": {"text": "Gehorch ihr doch auf mein Erkl\u00e4ren,", "tokens": ["Ge\u00b7horch", "ihr", "doch", "auf", "mein", "Er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie wird sich dir und dies zwar gleich", "tokens": ["Sie", "wird", "sich", "dir", "und", "dies", "zwar", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "PPER", "KON", "PDS", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit aller ihrer Lust gew\u00e4hren.", "tokens": ["Mit", "al\u00b7ler", "ih\u00b7rer", "Lust", "ge\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie ist der Erden h\u00f6chstes Gut,", "tokens": ["Sie", "ist", "der", "Er\u00b7den", "h\u00f6chs\u00b7tes", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie giebt dem Leben erst das Leben;", "tokens": ["Sie", "giebt", "dem", "Le\u00b7ben", "erst", "das", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erforsche nur dein eignes Blut,", "tokens": ["Er\u00b7for\u00b7sche", "nur", "dein", "eig\u00b7nes", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es wird mir hei\u00dfen Beyfall geben.", "tokens": ["Es", "wird", "mir", "hei\u00b7\u00dfen", "Bey\u00b7fall", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich weis, ein unbekandter Zug", "tokens": ["Ich", "weis", ",", "ein", "un\u00b7be\u00b7kand\u00b7ter", "Zug"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erhizt dir Adern, Brust und Wangen;", "tokens": ["Er\u00b7hizt", "dir", "A\u00b7dern", ",", "Brust", "und", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ach, werde doch bey Zeiten klug", "tokens": ["Ach", ",", "wer\u00b7de", "doch", "bey", "Zei\u00b7ten", "klug"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VAFIN", "ADV", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und hintertreib nicht dein Verlangen.", "tokens": ["Und", "hin\u00b7ter\u00b7treib", "nicht", "dein", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Einfalt macht die H\u00f6lle hei\u00df,", "tokens": ["Die", "Ein\u00b7falt", "macht", "die", "H\u00f6l\u00b7le", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vermeid des Aberglaubens Neze,", "tokens": ["Ver\u00b7meid", "des", "A\u00b7berg\u00b7lau\u00b7bens", "Ne\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von welchen die Vernunft nichts weis,", "tokens": ["Von", "wel\u00b7chen", "die", "Ver\u00b7nunft", "nichts", "weis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ART", "NN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist ein blo\u00dfes Weltgeseze.", "tokens": ["Es", "ist", "ein", "blo\u00b7\u00dfes", "Welt\u00b7ge\u00b7se\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Himmel fl\u00f6st den Zunder ein", "tokens": ["Der", "Him\u00b7mel", "fl\u00f6st", "den", "Zun\u00b7der", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und giebt den Saamen treuer Flammen,", "tokens": ["Und", "giebt", "den", "Saa\u00b7men", "treu\u00b7er", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie sollt er denn so th\u00f6richt seyn", "tokens": ["Wie", "sollt", "er", "denn", "so", "th\u00f6\u00b7richt", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und, was er selbst befiehlt, verdammen?", "tokens": ["Und", ",", "was", "er", "selbst", "be\u00b7fiehlt", ",", "ver\u00b7dam\u00b7men", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Beschau die Wercke der Natur,", "tokens": ["Be\u00b7schau", "die", "Wer\u00b7cke", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Betrachte B\u00e4ume, Feld und Thiere", "tokens": ["Be\u00b7trach\u00b7te", "B\u00e4u\u00b7me", ",", "Feld", "und", "Thie\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und lerne, wie der Liebe Spur", "tokens": ["Und", "ler\u00b7ne", ",", "wie", "der", "Lie\u00b7be", "Spur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich \u00fcberall zum Scherzen f\u00fchre!", "tokens": ["Dich", "\u00fc\u00b7be\u00b7rall", "zum", "Scher\u00b7zen", "f\u00fch\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wodurch sind ich und du denn da?", "tokens": ["Wo\u00b7durch", "sind", "ich", "und", "du", "denn", "da", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "KON", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu was bist du nebst mir gebohren?", "tokens": ["Zu", "was", "bist", "du", "nebst", "mir", "ge\u00b7boh\u00b7ren", 