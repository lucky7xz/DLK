{"textgrid.poem.24552": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Als ein guter Freund die Doctor-W\u00fcrde erlangte", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil die Gelehrsamkeit", "tokens": ["Weil", "die", "Ge\u00b7lehr\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Ehre hat zur Tochter angenommen;", "tokens": ["Die", "Eh\u00b7re", "hat", "zur", "Toch\u00b7ter", "an\u00b7ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und der sie allezeit", "tokens": ["Und", "der", "sie", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu seiner Braut bekommen/", "tokens": ["Zu", "sei\u00b7ner", "Braut", "be\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der mit der Mutter es gehalten:", "tokens": ["Der", "mit", "der", "Mut\u00b7ter", "es", "ge\u00b7hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So wird sie dir anitzo anvertraut.", "tokens": ["So", "wird", "sie", "dir", "a\u00b7nit\u00b7zo", "an\u00b7ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Weil nun dein Flei\u00df sich hier belohnet schaut:", "tokens": ["Weil", "nun", "dein", "Flei\u00df", "sich", "hier", "be\u00b7loh\u00b7net", "schaut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PRF", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So ruffet Ungarn schon:", "tokens": ["So", "ruf\u00b7fet", "Un\u00b7garn", "schon", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Komm mein gelehrter Sohn!", "tokens": ["Komm", "mein", "ge\u00b7lehr\u00b7ter", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Komm/ denn du sollst bey mir", "tokens": ["Komm", "/", "denn", "du", "sollst", "bey", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "KON", "PPER", "VMFIN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "In gr\u00f6ssern Gl\u00fccke leben:", "tokens": ["In", "gr\u00f6s\u00b7sern", "Gl\u00fc\u00b7cke", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ich will samt aller Ehre dir", "tokens": ["Ich", "will", "samt", "al\u00b7ler", "Eh\u00b7re", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Noch eine sch\u00f6ne Tochter geben.", "tokens": ["Noch", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Toch\u00b7ter", "ge\u00b7