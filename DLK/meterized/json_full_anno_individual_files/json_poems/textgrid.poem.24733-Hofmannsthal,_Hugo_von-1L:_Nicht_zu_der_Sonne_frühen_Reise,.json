{"textgrid.poem.24733": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht zu der Sonne fr\u00fchen Reise,", "genre": "verse", "period": "N.A.", "pub_year": 1897, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht zu der Sonne fr\u00fchen Reise,", "tokens": ["Nicht", "zu", "der", "Son\u00b7ne", "fr\u00fc\u00b7hen", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht wenn die Abendwolken landen,", "tokens": ["Nicht", "wenn", "die", "Ab\u00b7end\u00b7wol\u00b7ken", "lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Euch Kindern, weder laut noch leise,", "tokens": ["Euch", "Kin\u00b7dern", ",", "we\u00b7der", "laut", "noch", "lei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "KON", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ja, kaum uns selber seis gestanden,", "tokens": ["Ja", ",", "kaum", "uns", "sel\u00b7ber", "seis", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auf welch geheimnisvolle Weise", "tokens": ["Auf", "welch", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7le", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Leben wir den Traum entwanden", "tokens": ["Dem", "Le\u00b7ben", "wir", "den", "Traum", "ent\u00b7wan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und ihn mit Weingewinden leise", "tokens": ["Und", "ihn", "mit", "Wein\u00b7ge\u00b7win\u00b7den", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "An unsres Gartens Brunnen banden.", "tokens": ["An", "uns\u00b7res", "Gar\u00b7tens", "Brun\u00b7nen", "ban\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht zu der Sonne fr\u00fchen Reise,", "tokens": ["Nicht", "zu", "der", "Son\u00b7ne", "fr\u00fc\u00b7hen", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht wenn die Abendwolken landen,", "tokens": ["Nicht", "wenn", "die", "Ab\u00b7end\u00b7wol\u00b7ken", "lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Euch Kindern, weder laut noch leise,", "tokens": ["Euch", "Kin\u00b7dern", ",", "we\u00b7der", "laut", "noch", "lei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "KON", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ja, kaum uns selber seis gestanden,", "tokens": ["Ja", ",", "kaum", "uns", "sel\u00b7ber", "seis", "ge\u00b7stan\u