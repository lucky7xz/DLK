{"dta.poem.210": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er bittet sie zu k\u00fcssen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df deinen Zucker-Mund mich/ Sch\u00f6nste/ doch bek\u00fcssen/", "tokens": ["La\u00df", "dei\u00b7nen", "Zu\u00b7cke\u00b7rMund", "mich", "/", "Sch\u00f6ns\u00b7te", "/", "doch", "be\u00b7k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "$(", "NN", "$(", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und k\u00fchlen ", "tokens": ["Und", "k\u00fch\u00b7len"], "token_info": ["word", "word"], "pos": ["KON", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "La\u00df s\u00fcssen Honig-Thau von deinen Rosen fliessen/", "tokens": ["La\u00df", "s\u00fcs\u00b7sen", "Ho\u00b7nig\u00b7\u00b7T\u00b7hau", "von", "dei\u00b7nen", "Ro\u00b7sen", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "La\u00df den Rubinen-Mund mit s\u00fcssem Moste quillen/", "tokens": ["La\u00df", "den", "Ru\u00b7bi\u00b7nen\u00b7Mund", "mit", "s\u00fcs\u00b7sem", "Mos\u00b7te", "quil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Das mehr von Anmuth hegt als ", "tokens": ["Das", "mehr", "von", "An\u00b7muth", "hegt", "als"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "NN", "VVFIN", "KOKOM"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "La\u00df mich den heissen Durst mit dem Getr\u00e4ncke stillen/", "tokens": ["La\u00df", "mich", "den", "heis\u00b7sen", "Durst", "mit", "dem", "Ge\u00b7tr\u00e4n\u00b7cke", "stil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dem am Geschmacke weicht/ der Palmen weisser Schaum.", "tokens": ["Dem", "am", "Ge\u00b7schma\u00b7cke", "weicht", "/", "der", "Pal\u00b7men", "weis\u00b7ser", "Schaum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$(", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "La\u00df mir den Perlen-Thau von deinen Lippen suchen/", "tokens": ["La\u00df", "mir", "den", "Per\u00b7len\u00b7\u00b7T\u00b7hau", "von", "dei\u00b7nen", "Lip\u00b7pen", "su\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.10": {"text": "Wornach verschmachtend fast ein jedes Hertze l\u00e4chtzt;", "tokens": ["Wor\u00b7nach", "ver\u00b7schmach\u00b7tend", "fast", "ein", "je\u00b7des", "Hert\u00b7ze", "l\u00e4chtzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "ADV", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch der ist Scheltens werth/ ja billig zu verfluchen/", "tokens": ["Doch", "der", "ist", "Schel\u00b7tens", "werth", "/", "ja", "bil\u00b7lig", "zu", "ver\u00b7flu\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "NN", "ADJD", "$(", "ADV", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der bey dem Uberflu\u00df der Quellen durstig \u00e4chtzt.", "tokens": ["Der", "bey", "dem", "U\u00b7ber\u00b7flu\u00df", "der", "Quel\u00b7len", "durs\u00b7tig", "\u00e4chtzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Drum must du ", "tokens": ["Drum", "must", "du"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Von deinen Lippen mir den Honig geben frey/", "tokens": ["Von", "dei\u00b7nen", "Lip\u00b7pen", "mir", "den", "Ho\u00b7nig", "ge\u00b7ben", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ART", "NN", "VVINF", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du must mir unverwehrt den ", "tokens": ["Du", "must", "mir", "un\u00b7ver\u00b7wehrt", "den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Und zeigen/ da\u00df vor Durst dein Mund ein Labsal sey.", "tokens": ["Und", "zei\u00b7gen", "/", 