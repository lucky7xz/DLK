{"dta.poem.14796": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Reitersmann .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Er zielt' mit dem Rohre durch's Gitter,", "tokens": ["Er", "zielt'", "mit", "dem", "Roh\u00b7re", "durch's", "Git\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Auf die schneewei\u00dfe Brust hin;", "tokens": ["Auf", "die", "schnee\u00b7wei\u00b7\u00dfe", "Brust", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "\u201each, wie ist das Sterben so bitter,", "tokens": ["\u201e", "ach", ",", "wie", "ist", "das", "Ster\u00b7ben", "so", "bit\u00b7ter", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Erbarm' dich, weil ich so jung noch bin!\u201c \u2014", "tokens": ["Er\u00b7barm'", "dich", ",", "weil", "ich", "so", "jung", "noch", "bin", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "ADV", "ADJD", "ADV", "VAFIN", "$.", "$(", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}