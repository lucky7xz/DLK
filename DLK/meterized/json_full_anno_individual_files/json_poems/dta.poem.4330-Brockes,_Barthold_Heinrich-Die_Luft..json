{"dta.poem.4330": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Luft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr \u00fcbereilet euch, ihr wehrten B\u00e4ume, nur,", "tokens": ["Ihr", "\u00fc\u00b7be\u00b7rei\u00b7let", "euch", ",", "ihr", "wehr\u00b7ten", "B\u00e4u\u00b7me", ",", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr glaubt, ich schadete zugleich", "tokens": ["Ihr", "glaubt", ",", "ich", "scha\u00b7de\u00b7te", "zu\u00b7gleich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So mir, als euch.", "tokens": ["So", "mir", ",", "als", "euch", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ach nein! Ich folge blo\u00df der Ordnung der Natur;", "tokens": ["Ach", "nein", "!", "Ich", "fol\u00b7ge", "blo\u00df", "der", "Ord\u00b7nung", "der", "Na\u00b7tur", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr wollt der Regel widersteh\u2019n,", "tokens": ["Ihr", "wollt", "der", "Re\u00b7gel", "wi\u00b7der\u00b7steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die unser Urquell uns gesetzt.", "tokens": ["Die", "un\u00b7ser", "Ur\u00b7quell", "uns", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Kreis-Lauf, der so weis\u2019 als sch\u00f6n,", "tokens": ["Der", "Kreis\u00b7Lauf", ",", "der", "so", "weis'", "als", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "W\u00fcrd\u2019 unterbrochen und verletzt.", "tokens": ["W\u00fcrd'", "un\u00b7ter\u00b7bro\u00b7chen", "und", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Lust, euch fr\u00f6hlich anzuseh\u2019n,", "tokens": ["Die", "Lust", ",", "euch", "fr\u00f6h\u00b7lich", "an\u00b7zu\u00b7seh'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "(so doch des Sch\u00f6pfers Absicht war)", "tokens": ["(", "so", "doch", "des", "Sch\u00f6p\u00b7fers", "Ab\u00b7sicht", "war", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ART", "NN", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "W\u00fcrd\u2019 allen Sterblichen vergeh\u2019n,", "tokens": ["W\u00fcrd'", "al\u00b7len", "Sterb\u00b7li\u00b7chen", "ver\u00b7geh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wenn eurer alten Bl\u00e4tter Schaar", "tokens": ["Wenn", "eu\u00b7rer", "al\u00b7ten", "Bl\u00e4t\u00b7ter", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Best\u00e4ndig und unwandelbar", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "und", "un\u00b7wan\u00b7del\u00b7bar"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "In einem gleichen Gr\u00fcnen bliebe,", "tokens": ["In", "ei\u00b7nem", "glei\u00b7chen", "Gr\u00fc\u00b7nen", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und euch der Winter nicht vertriebe;", "tokens": ["Und", "euch", "der", "Win\u00b7ter", "nicht", "ver\u00b7trie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Damit, bey einem frohen Lenzen,", "tokens": ["Da\u00b7mit", ",", "bey", "ei\u00b7nem", "fro\u00b7hen", "Len\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Jhr, ganz vergn\u00fcget, sch\u00f6ner gl\u00e4nzen,", "tokens": ["Ihr", ",", "ganz", "ver\u00b7gn\u00fc\u00b7get", ",", "sch\u00f6\u00b7ner", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "VVFIN", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Erneute Freud\u2019 erregen m\u00f6gtet,", "tokens": ["Er\u00b7neu\u00b7te", "Freud'", "er\u00b7re\u00b7gen", "m\u00f6g\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und gr\u00f6\u00dfre Lust im Wechsel br\u00e4chtet,", "tokens": ["Und", "gr\u00f6\u00df\u00b7re", "Lust", "im", "Wech\u00b7sel", "br\u00e4ch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Dem, dem, auch mitten im Vergn\u00fcgen,", "tokens": ["Dem", ",", "dem", ",", "auch", "mit\u00b7ten", "im", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "$,", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Wofern kein Wechsel da, nichts lieblich scheint,", "tokens": ["Wo\u00b7fern", "kein", "Wech\u00b7sel", "da", ",", "nichts", "lieb\u00b7lich", "scheint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PTKVZ", "$,", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und der nicht m\u00e4chtig gnug, den m\u00e4cht\u2019gen Feind,", "tokens": ["Und", "der", "nicht", "m\u00e4ch\u00b7tig", "gnug", ",", "den", "m\u00e4cht'\u00b7gen", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PTKNEG", "ADJD", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Die alles d\u00e4mpfende Gewohnheit, zu besiegen.", "tokens": ["Die", "al\u00b7les", "d\u00e4mp\u00b7fen\u00b7de", "Ge\u00b7wohn\u00b7heit", ",", "zu", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Drum, liebe Bl\u00e4ttergen, erbleichet,", "tokens": ["Drum", ",", "lie\u00b7be", "Bl\u00e4t\u00b7ter\u00b7gen", ",", "er\u00b7blei\u00b7chet", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Welkt allgemach, vergeht und weichet", "tokens": ["Welkt", "all\u00b7ge\u00b7mach", ",", "ver\u00b7geht", "und", "wei\u00b7chet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Den k\u00fcnft\u2019gen Bl\u00e4ttern aus dem Wege,", "tokens": ["Den", "k\u00fcnft'\u00b7gen", "Bl\u00e4t\u00b7tern", "aus", "dem", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Damit, durch ihre frische Pracht,", "tokens": ["Da\u00b7mit", ",", "durch", "ih\u00b7re", "fri\u00b7sche", "Pracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Der Mensch aus der Gewohnheit Nacht", "tokens": ["Der", "Mensch", "aus", "der", "Ge\u00b7wohn\u00b7heit", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Jm Lenzen recht aufs neu erwacht,", "tokens": ["Jm", "Len\u00b7zen", "recht", "aufs", "neu", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "APPRART", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Den, Der die Welt so sch\u00f6n gemacht,", "tokens": ["Den", ",", "Der", "die", "Welt", "so", "sch\u00f6n", "ge\u00b7macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Mit neuer Andacht preisen m\u00f6ge!", "tokens": ["Mit", "neu\u00b7er", "An\u00b7dacht", "prei\u00b7sen", "m\u00f6\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}