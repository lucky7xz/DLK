{"textgrid.poem.56527": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Meine Seele ist eine Schlange,", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Seele ist eine Schlange,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "ist", "ei\u00b7ne", "Schlan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die ist schon lange tot,", "tokens": ["Die", "ist", "schon", "lan\u00b7ge", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur manchmal in Herbstesmorgen,", "tokens": ["Nur", "manch\u00b7mal", "in", "Herbs\u00b7tes\u00b7mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Entbl\u00e4ttertem Abendrot", "tokens": ["Ent\u00b7bl\u00e4t\u00b7ter\u00b7tem", "A\u00b7ben\u00b7drot"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wachse ich steil aus dem Fenster,", "tokens": ["Wach\u00b7se", "ich", "steil", "aus", "dem", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Wo fallende Sterne sind,", "tokens": ["Wo", "fal\u00b7len\u00b7de", "Ster\u00b7ne", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "\u00dcber den Blumen und Kressen", "tokens": ["\u00dc\u00b7ber", "den", "Blu\u00b7men", "und", "Kres\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Meine Stirne spiegelt", "tokens": ["Mei\u00b7ne", "Stir\u00b7ne", "spie\u00b7gelt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Im st\u00f6hnenden N\u00e4chte-Wind.", "tokens": ["Im", "st\u00f6h\u00b7nen\u00b7den", "N\u00e4ch\u00b7te\u00b7Wind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Meine Seele ist eine Schlange,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "ist", "ei\u00b7ne", "Schlan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die ist schon lange tot,", "tokens": ["Die", "ist", "schon", "lan\u00b7ge", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur manchmal in Herbstesmorgen,", "tokens": ["Nur", "manch\u00b7mal", "in", "Herbs\u00b7tes\u00b7mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Entbl\u00e4ttertem Abendrot", "tokens": ["Ent\u00b7bl\u00e4t\u00b7ter\u00b7tem", "A\u00b7ben\u00b7drot"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wachse ich steil aus dem Fenster,", "tokens"