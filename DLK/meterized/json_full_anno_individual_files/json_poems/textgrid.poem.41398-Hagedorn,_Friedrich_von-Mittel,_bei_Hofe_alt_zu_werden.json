{"textgrid.poem.41398": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Mittel, bei Hofe alt zu werden", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An H\u00f6fen f\u00e4llt es schwer, das Alter zu erreichen,", "tokens": ["An", "H\u00f6\u00b7fen", "f\u00e4llt", "es", "schwer", ",", "das", "Al\u00b7ter", "zu", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das mancher schlechter Greis in niedern H\u00fctten fand.", "tokens": ["Das", "man\u00b7cher", "schlech\u00b7ter", "Greis", "in", "nie\u00b7dern", "H\u00fct\u00b7ten", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dort wird der Gl\u00fccklichste, nach kurzen Gnadenzeichen,", "tokens": ["Dort", "wird", "der", "Gl\u00fcck\u00b7lichs\u00b7te", ",", "nach", "kur\u00b7zen", "Gna\u00b7den\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Titeln wohl versorgt, oft pl\u00f6tzlich weggebannt.", "tokens": ["Mit", "Ti\u00b7teln", "wohl", "ver\u00b7sorgt", ",", "oft", "pl\u00f6tz\u00b7lich", "weg\u00b7ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Alter hatte doch die meisten Lebensjahre", "tokens": ["Ein", "Al\u00b7ter", "hat\u00b7te", "doch", "die", "meis\u00b7ten", "Le\u00b7bens\u00b7jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "An seines F\u00fcrsten Hof ersprie\u00dflich zugebracht,", "tokens": ["An", "sei\u00b7nes", "F\u00fcrs\u00b7ten", "Hof", "er\u00b7sprie\u00df\u00b7lich", "zu\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und seinen ersten Bart und seine grauen Haare", "tokens": ["Und", "sei\u00b7nen", "ers\u00b7ten", "Bart", "und", "sei\u00b7ne", "grau\u00b7en", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zu Zeugen fr\u00fchen Ruhms und langer Gunst gemacht.", "tokens": ["Zu", "Zeu\u00b7gen", "fr\u00fc\u00b7hen", "Ruhms", "und", "lan\u00b7ger", "Gunst", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der ward: wie dieses ihm so meisterlich gelungen,", "tokens": ["Der", "ward", ":", "wie", "die\u00b7ses", "ihm", "so", "meis\u00b7ter\u00b7lich", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$.", "PWAV", "PDAT", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was tausend sonst verfehlt? einst insgeheim befragt.", "tokens": ["Was", "tau\u00b7send", "sonst", "ver\u00b7fehlt", "?", "einst", "ins\u00b7ge\u00b7heim", "be\u00b7fragt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "CARD", "ADV", "VVPP", "$.", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er sprach: Ich habe stets, auch f\u00fcr Beleidigungen,", "tokens": ["Er", "sprach", ":", "Ich", "ha\u00b7be", "stets", ",", "auch", "f\u00fcr", "Be\u00b7lei\u00b7di\u00b7gun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den Feinden meines Gl\u00fccks gelassen Dank gesagt.", "tokens": ["Den", "Fein\u00b7den", "mei\u00b7nes", "Gl\u00fccks", "ge\u00b7las\u00b7sen", "Dank", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "An H\u00f6fen f\u00e4llt es schwer, das Alter zu erreichen,", "tokens": ["An", "H\u00f6\u00b7fen", "f\u00e4llt", "es", "schwer", ",", "das", "Al\u00b7ter", "zu", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das mancher schlechter Greis in niedern H\u00