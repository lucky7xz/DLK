{"dta.poem.3928": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Der dritte  Titul  soll von baar verleiten Sachen,  \n was da zu halten sey, ein kurtz Gesetze machen.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist sonst diesem Volck die K\u00e4lte zwar nicht fremde,", "tokens": ["Es", "ist", "sonst", "die\u00b7sem", "Volck", "die", "K\u00e4l\u00b7te", "zwar", "nicht", "frem\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PDAT", "NN", "ART", "NN", "ADV", "PTKNEG", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df fie auch offtermahls in einem blossen Hemde,", "tokens": ["Da\u00df", "fie", "auch", "off\u00b7ter\u00b7mahls", "in", "ei\u00b7nem", "blos\u00b7sen", "Hem\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So sehr es sonst gefriert, doch auf der Strasse gehn;", "tokens": ["So", "sehr", "es", "sonst", "ge\u00b7friert", ",", "doch", "auf", "der", "Stras\u00b7se", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VVPP", "$,", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch itzt vermochten sie den Frost nicht aus zustehn.", "tokens": ["Doch", "itzt", "ver\u00b7moch\u00b7ten", "sie", "den", "Frost", "nicht", "aus", "zu\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das harte M\u00e4nner-Volck blieb in den L\u00f6chern", "tokens": ["Das", "har\u00b7te", "M\u00e4n\u00b7ner\u00b7Volck", "blieb", "in", "den", "L\u00f6\u00b7chern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "liegen',", "tokens": ["lie\u00b7gen'", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Kein Guckguck konte nicht von der Stelle kriegen.", "tokens": ["Kein", "Guck\u00b7guck", "kon\u00b7te", "nicht", "von", "der", "Stel\u00b7le", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Allein das Weibes-Volck, das stets der Vorwitz", "tokens": ["Al\u00b7lein", "das", "Wei\u00b7bes\u00b7Volck", ",", "das", "stets", "der", "Vor\u00b7witz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "plagt,", "tokens": ["plagt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Hat bey der Morgen-Lufft doch eine\u0303 Streich gewagt.", "tokens": ["Hat", "bey", "der", "Mor\u00b7gen\u00b7Lufft", "doch", "ein\u1ebd", "Streich", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Drey Tage mocht es seyn, seit dem sie sich verkro-", "tokens": ["Drey", "Ta\u00b7ge", "mocht", "es", "seyn", ",", "seit", "dem", "sie", "sich", "ver\u00b7kro"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "VAINF", "$,", "APPR", "PRELS", "PPER", "PRF", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "chen,", "tokens": ["chen", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Seit eine Nachbarin die andre nicht gesprochen,", "tokens": ["Seit", "ei\u00b7ne", "Nach\u00b7ba\u00b7rin", "die", "and\u00b7re", "nicht", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "PIS", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das war vor sie zu lang; so sehr es immer fror,", "tokens": ["Das", "war", "vor", "sie", "zu", "lang", ";", "so", "sehr", "es", "im\u00b7mer", "fror", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "PTKA", "ADJD", "$.", "ADV", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So kamen hier und da die Weibergen empor.", "tokens": ["So", "ka\u00b7men", "hier", "und", "da", "die", "Wei\u00b7ber\u00b7gen", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "KON", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sie fingen s\u00e4mtlich an auf diese Lufft zu schm\u00e4hlen,", "tokens": ["Sie", "fin\u00b7gen", "s\u00e4mt\u00b7lich", "an", "auf", "die\u00b7se", "Lufft", "zu", "schm\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sie k\u00f6nten, sagten sie, einander nicht erzehlen,", "tokens": ["Sie", "k\u00f6n\u00b7ten", ",", "sag\u00b7ten", "sie", ",", "ein\u00b7an\u00b7der", "nicht", "er\u00b7zeh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "PPER", "$,", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was die und jene macht, was sonsten neues w\u00e4r,", "tokens": ["Was", "die", "und", "je\u00b7ne", "macht", ",", "was", "sons\u00b7ten", "neu\u00b7es", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "KON", "PDS", "VVFIN", "$,", "PWS", "ADJA", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und schnatterten etwas, als wie die Endten, her.", "tokens": ["Und", "schnat\u00b7ter\u00b7ten", "et\u00b7was", ",", "als", "wie", "die", "End\u00b7ten", ",", "her", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Was itzo diese sprach, das wuste jene besser,", "tokens": ["Was", "it\u00b7zo", "die\u00b7se", "sprach", ",", "das", "wus\u00b7te", "je\u00b7ne", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PDS", "VVFIN", "$,", "PDS", "VVFIN", "PDS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Bald klang es klar, bald grob, wie rauschendes Ge-", "tokens": ["Bald", "klang", "es", "klar", ",", "bald", "grob", ",", "wie", "rau\u00b7schen\u00b7des", "Ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "ADJD", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "w\u00e4sser,", "tokens": ["w\u00e4s\u00b7ser", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Was einer fehlete, das halff die andre ein,", "tokens": ["Was", "ei\u00b7ner", "feh\u00b7le\u00b7te", ",", "das", "halff", "die", "and\u00b7re", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$,", "PDS", "VVFIN", "ART", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Es war, als solte hier die Juden-Schule seyn.", "tokens": ["Es", "war", ",", "als", "sol\u00b7te", "hier", "die", "Ju\u00b7den\u00b7Schu\u00b7le", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch h\u00f6ret was geschicht; indem sie sich bem\u00fchen,", "tokens": ["Doch", "h\u00f6\u00b7ret", "was", "ge\u00b7schicht", ";", "in\u00b7dem", "sie", "sich", "be\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "VVPP", "$.", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und \u00fcber gro\u00df und klein den Dolch der Zunge ziehen,", "tokens": ["Und", "\u00fc\u00b7ber", "gro\u00df", "und", "klein", "den", "Dolch", "der", "Zun\u00b7ge", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJD", "KON", "ADJD", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So stieg die K\u00e4lte hoch; da sie in bester Ruh,", "tokens": ["So", "stieg", "die", "K\u00e4l\u00b7te", "hoch", ";", "da", "sie", "in", "bes\u00b7ter", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$.", "KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Gefroren allerseits die M\u00e4uler ihnen zu.", "tokens": ["Ge\u00b7fro\u00b7ren", "al\u00b7ler\u00b7seits", "die", "M\u00e4u\u00b7ler", "ih\u00b7nen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die M\u00e4nner mochten hier die Weiber wenig lieben,", "tokens": ["Die", "M\u00e4n\u00b7ner", "moch\u00b7ten", "hier", "die", "Wei\u00b7ber", "we\u00b7nig", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Denn sie bezeugeten hier\u00fcber kein Betr\u00fcben,", "tokens": ["Denn", "sie", "be\u00b7zeu\u00b7ge\u00b7ten", "hier\u00b7\u00fc\u00b7ber", "kein", "Be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Vielmehr traf aller Wunsch hierinnen \u00fcberein,", "tokens": ["Viel\u00b7mehr", "traf", "al\u00b7ler", "Wunsch", "hie\u00b7rin\u00b7nen", "\u00fc\u00b7be\u00b7re\u00b7in", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PAV", "PTKVZ", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.32": {"text": "Die K\u00e4lte m\u00f6chte doch von steter Dauer seyn.", "tokens": ["Die", "K\u00e4l\u00b7te", "m\u00f6ch\u00b7te", "doch", "von", "ste\u00b7ter", "Dau\u00b7er", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}