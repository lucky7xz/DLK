{"textgrid.poem.63849": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Die Fliege", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Menschen lebt, d\u00fcnk' er sich edel auch", "tokens": ["Dem", "Men\u00b7schen", "lebt", ",", "d\u00fcnk'", "er", "sich", "e\u00b7del", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und gut, im Busen ein Vernichtungstrieb,", "tokens": ["Und", "gut", ",", "im", "Bu\u00b7sen", "ein", "Ver\u00b7nich\u00b7tungs\u00b7trieb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie ja der Sch\u00f6pfer, dessen Ebenbild", "tokens": ["Wie", "ja", "der", "Sch\u00f6p\u00b7fer", ",", "des\u00b7sen", "E\u00b7ben\u00b7bild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er sich ber\u00fchmt, was er erschaffen, auch", "tokens": ["Er", "sich", "be\u00b7r\u00fchmt", ",", "was", "er", "er\u00b7schaf\u00b7fen", ",", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "PRF", "ADJD", "$,", "PWS", "PPER", "VVPP", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem Tode weiht.", "tokens": ["Dem", "To\u00b7de", "weiht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Am Fenster stand ich heut", "tokens": ["Am", "Fens\u00b7ter", "stand", "ich", "heut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und blickte m\u00fc\u00dfig auf den See hinaus,", "tokens": ["Und", "blick\u00b7te", "m\u00fc\u00b7\u00dfig", "auf", "den", "See", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der aufgest\u00fcrmt, mit wei\u00dfen K\u00e4mmen wild", "tokens": ["Der", "auf\u00b7ge\u00b7st\u00fcrmt", ",", "mit", "wei\u00b7\u00dfen", "K\u00e4m\u00b7men", "wild"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "VVPP", "$,", "APPR", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die Flut ans Ufer trieb. Im Zimmer doch", "tokens": ["Die", "Flut", "ans", "U\u00b7fer", "trieb", ".", "Im", "Zim\u00b7mer", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$.", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "War's heimlich, denn im Ofen knisternd sang", "tokens": ["Wa\u00b7r's", "heim\u00b7lich", ",", "denn", "im", "O\u00b7fen", "knis\u00b7ternd", "sang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$,", "KON", "APPRART", "NN", "VVPP", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Des \u00d6lbaums gr\u00fcnes Holz. Und wie ich stand,", "tokens": ["Des", "\u00d6l\u00b7baums", "gr\u00fc\u00b7nes", "Holz", ".", "Und", "wie", "ich", "stand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Nichts denkend, sah ich eine Fliege, kaum", "tokens": ["Nichts", "den\u00b7kend", ",", "sah", "ich", "ei\u00b7ne", "Flie\u00b7ge", ",", "kaum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PIS", "VVPP", "$,", "VVFIN", "PPER", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Erwacht zum Leben, die am Fensterglas", "tokens": ["Er\u00b7wacht", "zum", "Le\u00b7ben", ",", "die", "am", "Fens\u00b7ter\u00b7glas"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Behaglich sacht hinaufkroch, wohl gleich mir", "tokens": ["Be\u00b7hag\u00b7lich", "sacht", "hin\u00b7auf\u00b7kroch", ",", "wohl", "gleich", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "$,", "ADV", "ADV", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der W\u00e4rme froh. Und wie zur Sommerszeit,", "tokens": ["Der", "W\u00e4r\u00b7me", "froh", ".", "Und", "wie", "zur", "Som\u00b7mers\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$.", "KON", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Wo nur zu sehr der kleinen N\u00e4scher Schwarm", "tokens": ["Wo", "nur", "zu", "sehr", "der", "klei\u00b7nen", "N\u00e4\u00b7scher", "Schwarm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PTKA", "ADV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Uns l\u00e4stig wird, auch jetzt zerdr\u00fcckt' ich sie", "tokens": ["Uns", "l\u00e4s\u00b7tig", "wird", ",", "auch", "jetzt", "zer\u00b7dr\u00fcckt'", "ich", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "VAFIN", "$,", "ADV", "ADV", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Mit plumpem Finger. Doch sogleich in mir", "tokens": ["Mit", "plum\u00b7pem", "Fin\u00b7ger", ".", "Doch", "sog\u00b7leich", "in", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "KON", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Sprach eine Stimme: O du Grausamer!", "tokens": ["Sprach", "ei\u00b7ne", "Stim\u00b7me", ":", "O", "du", "Grau\u00b7sa\u00b7mer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NE", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Konntst du das kurze bischen Leben ihr", "tokens": ["Konntst", "du", "das", "kur\u00b7ze", "bi\u00b7schen", "Le\u00b7ben", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "ADV", "NN", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Nicht g\u00f6nnen? War die Welt nicht weit genug", "tokens": ["Nicht", "g\u00f6n\u00b7nen", "?", "War", "die", "Welt", "nicht", "weit", "ge\u00b7nug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "$.", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "F\u00fcr dich und sie, und h\u00e4tt' ihr Summen dir", "tokens": ["F\u00fcr", "dich", "und", "sie", ",", "und", "h\u00e4tt'", "ihr", "Sum\u00b7men", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "PPER", "$,", "KON", "VAFIN", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Den Schlaf gest\u00f6rt?", "tokens": ["Den", "Schlaf", "ge\u00b7st\u00f6rt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "So sprach mein bessres Ich.", "tokens": ["So", "sprach", "mein", "bess\u00b7res", "Ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Und von dem Ort der Untat, wo, gestreckt", "tokens": ["Und", "von", "dem", "Ort", "der", "Un\u00b7tat", ",", "wo", ",", "ge\u00b7streckt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,", "PWAV", "$,", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Die zarten F\u00fc\u00dfchen, an der Scheibe hing", "tokens": ["Die", "zar\u00b7ten", "F\u00fc\u00df\u00b7chen", ",", "an", "der", "Schei\u00b7be", "hing"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Die kleine Tote, trat ich rasch zur\u00fcck,", "tokens": ["Die", "klei\u00b7ne", "To\u00b7te", ",", "trat", "ich", "rasch", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Sehr unzufrieden mit mir selbst.", "tokens": ["Sehr", "un\u00b7