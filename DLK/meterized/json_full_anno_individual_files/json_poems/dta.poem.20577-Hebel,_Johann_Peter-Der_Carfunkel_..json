{"dta.poem.20577": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Carfunkel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.99"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "So verzehlt der Aetti, und mit engbr\u00fcstigem", "tokens": ["So", "ver\u00b7zehlt", "der", "A\u00b7et\u00b7ti", ",", "und", "mit", "eng\u00b7br\u00fcs\u00b7ti\u00b7gem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "APPR", "ADJA"], "meter": "--+--+--+-+--", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Othem", "tokens": ["O\u00b7them"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "seit iez d\u2019 Muetter: \u201eBisch bal serig? Mach", "tokens": ["seit", "iez", "d'", "Mu\u00b7et\u00b7ter", ":", "\u201e", "Bisch", "bal", "se\u00b7rig", "?", "Mach"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NE", "NE", "NN", "$.", "$(", "ADJD", "ADV", "ADJD", "$.", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "mer die Meidli", "tokens": ["mer", "die", "Meid\u00b7li"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "\u201enit so z\u2019f\u00f6rche, \u2019s sin doch nummen er-", "tokens": ["\u201e", "nit", "so", "z'\u00b7f\u00f6r\u00b7che", ",", "'s", "sin", "doch", "num\u00b7men", "er"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PTKNEG", "ADV", "ADJA", "$,", "PPER", "VAFIN", "ADV", "ADJA", "TRUNC"], "meter": "+--+--+-+--", "measure": "dactylic.di.plus"}, "line.6": {"text": "dichteti M\u00e4hrli!\u201c \u2014", "tokens": ["dich\u00b7te\u00b7ti", "M\u00e4hr\u00b7li", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["NE", "NE", "$.", "$(", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "\u201ejo, i bi io ferig!\u201c erwiedert der Aetti,", "tokens": ["\u201e", "jo", ",", "i", "bi", "i\u00b7o", "fe\u00b7rig", "!", "\u201c", "er\u00b7wie\u00b7dert", "der", "A\u00b7et\u00b7ti", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "FM", "FM", "FM", "ADJD", "$.", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+---+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "\u201ed\u00f6rt lit er", "tokens": ["\u201e", "d\u00f6rt", "lit", "er"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "ADJD", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "\u201emit sim Ring im Dornegh\u00fcrst, wo d\u2019", "tokens": ["\u201e", "mit", "sim", "Ring", "im", "Dor\u00b7neg\u00b7h\u00fcrst", ",", "wo", "d'"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "APPR", "ADJA", "NN", "APPRART", "NN", "$,", "PWAV", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Trostle nit singe.\u201c", "tokens": ["Trost\u00b7le", "nit", "sin\u00b7ge", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "PTKNEG", "VVFIN", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Aber d\u2019 Marei seit: \u201eO Muetter, wer", "tokens": ["A\u00b7ber", "d'", "Ma\u00b7rei", "seit", ":", "\u201e", "O", "Mu\u00b7et\u00b7ter", ",", "wer"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word"], "pos": ["KON", "NE", "NN", "APPR", "$.", "$(", "NE", "NN", "$,", "PWS"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "wird em denn f\u00f6rche!", "tokens": ["wird", "em", "denn", "f\u00f6r\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "XY", "KON", "ADJA", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "\u201edenksch, i merk nit, was er meint, und", "tokens": ["\u201e", "denksch", ",", "i", "merk", "nit", ",", "was", "er", "meint", ",", "und"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["$(", "ADJD", "$,", "NE", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,", "KON"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.14": {"text": "was er will sage?", "tokens": ["was", "er", "will", "sa\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "\u201ejo, der Vizli Buzli, das isch die b\u00f6si", "tokens": ["\u201e", "jo", ",", "der", "Viz\u00b7li", "Buz\u00b7li", ",", "das", "isch", "die", "b\u00f6\u00b7si"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "NE", "$,", "ART", "NE", "NE", "$,", "PRELS", "ADJD", "ART", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Versuchung.", "tokens": ["Ver\u00b7su\u00b7chung", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "\u201elokt sie nit, und f\u00fchrt sie nit in S\u00fcnden", "tokens": ["\u201e", "lokt", "sie", "nit", ",", "und", "f\u00fchrt", "sie", "nit", "in", "S\u00fcn\u00b7den"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "und Elend,", "tokens": ["und", "E\u00b7lend", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "\u201ewenn e Mensch nit bete mag, und folgt", "tokens": ["\u201e", "wenn", "e", "Mensch", "nit", "be\u00b7te", "mag", ",", "und", "folgt"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "KOUS", "NE", "NN", "PTKNEG", "VVFIN", "VMFIN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.20": {"text": "nit, und schafft n\u00fct!", "tokens": ["nit", ",", "und", "schafft", "n\u00fct", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "+---", "measure": "dactylic.init"}, "line.21": {"text": "\u201eund der lockig Chnab isch gueti Warnig", "tokens": ["\u201e", "und", "der", "lo\u00b7ckig", "Chnab", "isch", "gue\u00b7ti", "War\u00b7nig"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "ART", "FM", "FM", "FM", "FM", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "im Gwisse.", "tokens": ["im", "Gwis\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "\u201eo, i chenn mi Aetti wohl, und sini Gi-", "tokens": ["\u201e", "o", ",", "i", "chenn", "mi", "A\u00b7et\u00b7ti", "wohl", ",", "und", "si\u00b7ni", "Gi"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "FM", "$,", "FM", "FM", "FM", "FM", "ADV", "$,", "KON", "NE", "TRUNC"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "danke!\u201c", "tokens": ["dan\u00b7ke", "!", "\u201c"], "token_info": ["word", "punct", "punct"], "pos": ["VVFIN", "$.", "$("], "meter": "+-", "measure": "trochaic.single"}}}}}