{"textgrid.poem.54235": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sein angenehmes Blat erbrach ich mit Vergn\u00fcgen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sein angenehmes Blat erbrach ich mit Vergn\u00fcgen,", "tokens": ["Sein", "an\u00b7ge\u00b7neh\u00b7mes", "Blat", "er\u00b7brach", "ich", "mit", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich sasse Kummers-voll in meiner Einsamkeit,", "tokens": ["Ich", "sas\u00b7se", "Kum\u00b7mer\u00b7svoll", "in", "mei\u00b7ner", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch muste sichs vergn\u00fcgt nach meiner Sehnsucht f\u00fcgen,", "tokens": ["Doch", "mus\u00b7te", "sichs", "ver\u00b7gn\u00fcgt", "nach", "mei\u00b7ner", "Sehn\u00b7sucht", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df deine Zuschrifft mich von ohngefehr erfreut.", "tokens": ["Da\u00df", "dei\u00b7ne", "Zu\u00b7schrifft", "mich", "von", "ohn\u00b7ge\u00b7fehr", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich zanckte mich mit dir, und zwar in den Gedancken,", "tokens": ["Ich", "zanck\u00b7te", "mich", "mit", "dir", ",", "und", "zwar", "in", "den", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$,", "KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und in dem Augenblick erhielt ich dieses Blat,", "tokens": ["Und", "in", "dem", "Au\u00b7gen\u00b7blick", "er\u00b7hielt", "ich", "die\u00b7ses", "Blat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das, da mein Hofnungs-Schiff so lange schien zu wancken,", "tokens": ["Das", ",", "da", "mein", "Hof\u00b7nungs\u00b7Schiff", "so", "lan\u00b7ge", "schien", "zu", "wan\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mir endlich doch von dir Bericht ertheilet hat.", "tokens": ["Mir", "end\u00b7lich", "doch", "von", "dir", "Be\u00b7richt", "er\u00b7thei\u00b7let", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "PPER", "NN", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Drum st\u00e4rckte sich mein Geist, so bald als er vernommen,", "tokens": ["Drum", "st\u00e4rck\u00b7te", "sich", "mein", "Geist", ",", "so", "bald", "als", "er", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "PPOSAT", "NN", "$,", "ADV", "ADV", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df sich Selander noch zu meinen Freunden zehlt.", "tokens": ["Da\u00df", "sich", "Se\u00b7lan\u00b7der", "noch", "zu", "mei\u00b7nen", "Freun\u00b7den", "zehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Di\u00df hat mir, liebster Freund, den Argwohn gantz benommen,", "tokens": ["Di\u00df", "hat", "mir", ",", "liebs\u00b7ter", "Freund", ",", "den", "Arg\u00b7wohn", "gantz", "be\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der mich die Zeit daher gefoltert und gequ\u00e4lt.", "tokens": ["Der", "mich", "die", "Zeit", "da\u00b7her", "ge\u00b7fol\u00b7tert", "und", "ge\u00b7qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "PAV", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich greiffe recht mit Lust nach den best\u00e4ubten Bogen", "tokens": ["Ich", "greif\u00b7fe", "recht", "mit", "Lust", "nach", "den", "be\u00b7st\u00e4ub\u00b7ten", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und bin, wie du selbst siehst, zum Dichten aufger\u00e4umt,", "tokens": ["Und", "bin", ",", "wie", "du", "selbst", "siehst", ",", "zum", "Dich\u00b7ten", "auf\u00b7ge\u00b7r\u00e4umt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die frohe Post hat mich auf den Parna\u00df gezogen,", "tokens": ["Die", "fro\u00b7he", "Post", "hat", "mich", "auf", "den", "Par\u00b7na\u00df", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Von dem mir bisanher auch nicht einmahl getr\u00e4umt.", "tokens": ["Von", "dem", "mir", "bi\u00b7san\u00b7her", "auch", "nicht", "ein\u00b7mahl", "ge\u00b7tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was k\u00f6nte, frag ich dich, mich z\u00e4rtlicher wohl r\u00fchren?", "tokens": ["Was", "k\u00f6n\u00b7te", ",", "frag", "ich", "dich", ",", "mich", "z\u00e4rt\u00b7li\u00b7cher", "wohl", "r\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "VVIMP", "PPER", "PRF", "$,", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Als da\u00df du noch vor mich ein Angedencken hegst?", "tokens": ["Als", "da\u00df", "du", "noch", "vor", "mich", "ein", "An\u00b7ge\u00b7den\u00b7cken", "hegst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "APPR", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Aus deinen Zeilen kan ich mehr als kl\u00e4rlich sp\u00fchren,", "tokens": ["Aus", "dei\u00b7nen", "Zei\u00b7len", "kan", "ich", "mehr", "als", "kl\u00e4r\u00b7lich", "sp\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "PIAT", "KOKOM", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df du den Nahmen noch von mir in Hertzen tr\u00e4gst.", "tokens": ["Da\u00df", "du", "den", "Nah\u00b7men", "noch", "von", "mir", "in", "Hert\u00b7zen", "tr\u00e4gst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Den hast du, wie du schreibst, recht tieff darein geschrieben,", "tokens": ["Den", "hast", "du", ",", "wie", "du", "schreibst", ",", "recht", "tieff", "da\u00b7rein", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "ADJD", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wo k\u00f6nt ich pr\u00e4chtiger, als da, verwahret seyn?", "tokens": ["Wo", "k\u00f6nt", "ich", "pr\u00e4ch\u00b7ti\u00b7ger", ",", "als", "da", ",", "ver\u00b7wah\u00b7ret", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "$,", "KOUS", "ADV", "$,", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Du schenckst mir deine Gunst, und zwar mit solchen Trieben,", "tokens": ["Du", "schenckst", "mir", "dei\u00b7ne", "Gunst", ",", "und", "zwar", "mit", "sol\u00b7chen", "Trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KON", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die niemand tadeln kan und mehr als Engelrein.", "tokens": ["Die", "nie\u00b7mand", "ta\u00b7deln", "kan", "und", "mehr", "als", "En\u00b7gel\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "KON", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Di\u00df mu\u00df mir in der That was angenehmes heissen,", "tokens": ["Di\u00df", "mu\u00df", "mir", "in", "der", "That", "was", "an\u00b7ge\u00b7neh\u00b7mes", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "APPR", "ART", "NN", "PWS", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "An Freunden fehlt es zwar uns nicht so hier als dar,", "tokens": ["An", "Freun\u00b7den", "fehlt", "es", "zwar", "uns", "nicht", "so", "hier", "als", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "PPER", "PTKNEG", "ADV", "ADV", "KOUS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Doch kan man alle nicht vor \u00e4cht und g\u00fcltig preisen,", "tokens": ["Doch", "kan", "man", "al\u00b7le", "nicht", "vor", "\u00e4cht", "und", "g\u00fcl\u00b7tig", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PIS", "PTKNEG", "APPR", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Denn wahre Freunde seynd, wie mich bed\u00fcncket, rar.", "tokens": ["Denn", "wah\u00b7re", "Freun\u00b7de", "seynd", ",", "wie", "mich", "be\u00b7d\u00fcn\u00b7cket", ",", "rar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die Worte seynd mit Seim und Honig \u00fcberzogen,", "tokens": ["Die", "Wor\u00b7te", "seynd", "mit", "Seim", "und", "Ho\u00b7nig", "\u00fc\u00b7berz\u00b7o\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NE", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Allein das Hertze heist und bleibet stets verg\u00e4llt.", "tokens": ["Al\u00b7lein", "das", "Hert\u00b7ze", "heist", "und", "blei\u00b7bet", "stets", "ver\u00b7g\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "ADJD", "KON", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wie vielmahl hat uns nicht ein falscher Freund betrogen?", "tokens": ["Wie", "viel\u00b7mahl", "hat", "uns", "nicht", "ein", "fal\u00b7scher", "Freund", "be\u00b7tro\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Weil unter tausenden den Strich kaum einer h\u00e4lt.", "tokens": ["Weil", "un\u00b7ter", "tau\u00b7sen\u00b7den", "den", "Strich", "kaum", "ei\u00b7ner", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "ART", "NN", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Drum kanst du k\u00fcnfftighin, Selander, leicht ermessen,", "tokens": ["Drum", "kanst", "du", "k\u00fcnff\u00b7tig\u00b7hin", ",", "Se\u00b7lan\u00b7der", ",", "leicht", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PTKVZ", "$,", "NN", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wie hoch ich deine Gunst und Freundschafft sch\u00e4tzen mu\u00df,", "tokens": ["Wie", "hoch", "ich", "dei\u00b7ne", "Gunst", "und", "Freund\u00b7schafft", "sch\u00e4t\u00b7zen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPOSAT", "NN", "KON", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ich werde selbige so leichte nicht vergessen,", "tokens": ["Ich", "wer\u00b7de", "sel\u00b7bi\u00b7ge", "so", "leich\u00b7te", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "ADV", "ADJA", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Di\u00df ist, was wilst du mehr? von meinen Brieff der Schlu\u00df.", "tokens": ["Di\u00df", "ist", ",", "was", "wilst", "du", "mehr", "?", "von", "mei\u00b7nen", "Brieff", "der", "Schlu\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWS", "VMFIN", "PPER", "ADV", "$.", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sein angenehmes Blat erbrach ich mit Vergn\u00fcgen,", "tokens": ["Sein", "an\u00b7ge\u00b7neh\u00b7mes", "Blat", "er\u00b7brach", "ich", "mit", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich sasse Kummers-voll in meiner Einsamkeit,", "tokens": ["Ich", "sas\u00b7se", "Kum\u00b7mer\u00b7svoll", "in", "mei\u00b7ner", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch muste sichs vergn\u00fcgt nach meiner Sehnsucht f\u00fcgen,", "tokens": ["Doch", "mus\u00b7te", "sichs", "ver\u00b7gn\u00fcgt", "nach", "mei\u00b7ner", "Sehn\u00b7sucht", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df deine Zuschrifft mich von ohngefehr erfreut.", "tokens": ["Da\u00df", "dei\u00b7ne", "Zu\u0