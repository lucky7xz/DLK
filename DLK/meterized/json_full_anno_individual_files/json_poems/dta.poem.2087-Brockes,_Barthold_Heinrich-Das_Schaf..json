{"dta.poem.2087": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Schaf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von allen Thieren in dem Thierreich wird fast kein", "tokens": ["Von", "al\u00b7len", "Thie\u00b7ren", "in", "dem", "Thier\u00b7reich", "wird", "fast", "kein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN", "VAFIN", "ADV", "PIAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "einziges gefunden,", "tokens": ["ein\u00b7zi\u00b7ges", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "In welchem, zu des Menschen Besten, so gar viel n\u00fctz-", "tokens": ["In", "wel\u00b7chem", ",", "zu", "des", "Men\u00b7schen", "Bes\u00b7ten", ",", "so", "gar", "viel", "n\u00fctz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "$,", "APPR", "ART", "NN", "NN", "$,", "ADV", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "liches verbunden,", "tokens": ["li\u00b7ches", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Als in den sanft- und frommen Schafen. Es n\u00fctzt", "tokens": ["Als", "in", "den", "sanft", "und", "from\u00b7men", "Scha\u00b7fen", ".", "Es", "n\u00fctzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "APPR", "ART", "TRUNC", "KON", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "von dem, was an ihm ist,", "tokens": ["von", "dem", ",", "was", "an", "ihm", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein jedes Glied und alle Theile; das Fleisch, die Milch,", "tokens": ["Ein", "je\u00b7des", "Glied", "und", "al\u00b7le", "Thei\u00b7le", ";", "das", "Fleisch", ",", "die", "Milch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "PIAT", "NN", "$.", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "die Haut, die Klauen,", "tokens": ["die", "Haut", ",", "die", "Klau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Die Wolle, die Ged\u00e4rme, Knochen, die H\u00f6rner, ja", "tokens": ["Die", "Wol\u00b7le", ",", "die", "Ge\u00b7d\u00e4r\u00b7me", ",", "Kno\u00b7chen", ",", "die", "H\u00f6r\u00b7ner", ",", "ja"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "NN", "$,", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "sogar der Mist.", "tokens": ["so\u00b7gar", "der", "Mist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Es speist und tr\u00e4nket uns das Schaf, es kleidet uns.", "tokens": ["Es", "speist", "und", "tr\u00e4n\u00b7ket", "uns", "das", "Schaf", ",", "es", "klei\u00b7det", "uns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die L\u00e4nder bauen,", "tokens": ["Die", "L\u00e4n\u00b7der", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Versp\u00fcren durch die\u00df holde Thier, zumal durch seine", "tokens": ["Ver\u00b7sp\u00fc\u00b7ren", "durch", "die\u00df", "hol\u00b7de", "Thier", ",", "zu\u00b7mal", "durch", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "PDS", "ADJA", "NN", "$,", "KOUS", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Fruchtbarkeit,", "tokens": ["Frucht\u00b7bar\u00b7keit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Verschiednen Segen, werden reich, und auf verschiedne", "tokens": ["Ver\u00b7schied\u00b7nen", "Se\u00b7gen", ",", "wer\u00b7den", "reich", ",", "und", "auf", "ver\u00b7schied\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VAFIN", "ADJD", "$,", "KON", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Art erfreut.", "tokens": ["Art", "er\u00b7freut", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Es zeigt die alt\u2019 und neue Zeit, wie mancher Nutz aus", "tokens": ["Es", "zeigt", "die", "alt'", "und", "neu\u00b7e", "Zeit", ",", "wie", "man\u00b7cher", "Nutz", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "KON", "ADJA", "NN", "$,", "PWAV", "PIAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.18": {"text": "Schafen sprie\u00dfe,", "tokens": ["Scha\u00b7fen", "sprie\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Und scheint daher das Spr\u00fcchwort wahr: es hab\u2019 ein", "tokens": ["Und", "scheint", "da\u00b7her", "das", "Spr\u00fcch\u00b7wort", "wahr", ":", "es", "hab'", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "ART", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "ART"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "Sch\u00e4fgen g\u00fcldne F\u00fc\u00dfe.", "tokens": ["Sch\u00e4f\u00b7gen", "g\u00fcld\u00b7ne", "F\u00fc\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "Ja, wenn ich es recht \u00fcberlege, so scheint an diesem", "tokens": ["Ja", ",", "wenn", "ich", "es", "recht", "\u00fc\u00b7berl\u00b7e\u00b7ge", ",", "so", "scheint", "an", "die\u00b7sem"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "PDAT"], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Thier allein", "tokens": ["Thier", "al\u00b7lein"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Sein K\u00f6rperlichs nicht nur zu Nutzen, es scheint sogar", "tokens": ["Sein", "K\u00f6r\u00b7per\u00b7lichs", "nicht", "nur", "zu", "Nut\u00b7zen", ",", "es", "scheint", "so\u00b7gar"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "APPR", "NN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "des Geistes Wesen", "tokens": ["des", "Geis\u00b7tes", "We\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "Zu einem Sinnbild holder Sanftmuth und der Geduld", "tokens": ["Zu", "ei\u00b7nem", "Sinn\u00b7bild", "hol\u00b7der", "Sanft\u00b7muth", "und", "der", "Ge\u00b7duld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "f\u00fcr uns erlesen,", "tokens": ["f\u00fcr", "uns", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.27": {"text": "Und die\u00df Thier ein belehrend Thier, ein Bild der Fr\u00f6m-", "tokens": ["Und", "die\u00df", "Thier", "ein", "be\u00b7leh\u00b7rend", "Thier", ",", "ein", "Bild", "der", "Fr\u00f6m"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDS", "NN", "ART", "ADJD", "NN", "$,", "ART", "NN", "ART", "TRUNC"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.28": {"text": "migkeit, zu seyn.", "tokens": ["mig\u00b7keit", ",", "zu", "seyn", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKZU", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.29": {"text": "Wer etwan meynt, die\u00df sey zu viel, der darf nur Hirten-", "tokens": ["Wer", "et\u00b7wan", "meynt", ",", "die\u00df", "sey", "zu", "viel", ",", "der", "darf", "nur", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "$,", "PDS", "VAFIN", "PTKA", "PIS", "$,", "PRELS", "VMFIN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "lieder lesen;", "tokens": ["lie\u00b7der", "le\u00b7sen", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}