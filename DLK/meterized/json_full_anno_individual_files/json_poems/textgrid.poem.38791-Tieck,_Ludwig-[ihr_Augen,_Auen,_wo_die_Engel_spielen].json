{"textgrid.poem.38791": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[ihr Augen, Auen, wo die Engel spielen]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Augen, Auen, wo die Engel spielen,", "tokens": ["Ihr", "Au\u00b7gen", ",", "Au\u00b7en", ",", "wo", "die", "En\u00b7gel", "spie\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Blicke, Blitze, leuchtend angez\u00fcndet,", "tokens": ["Ihr", "Bli\u00b7cke", ",", "Blit\u00b7ze", ",", "leuch\u00b7tend", "an\u00b7ge\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du Sehn, ein Seegen, welcher kommt und schwindet,", "tokens": ["Du", "Sehn", ",", "ein", "See\u00b7gen", ",", "wel\u00b7cher", "kommt", "und", "schwin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O Strahl, ein Stahl, gez\u00fcckt nach mir zu zielen!", "tokens": ["O", "Strahl", ",", "ein", "Stahl", ",", "ge\u00b7z\u00fcckt", "nach", "mir", "zu", "zie\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "NN", "$,", "VVPP", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "sta