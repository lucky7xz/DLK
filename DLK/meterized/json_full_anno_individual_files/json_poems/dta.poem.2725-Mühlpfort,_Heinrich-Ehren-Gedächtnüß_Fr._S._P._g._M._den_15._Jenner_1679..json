{"dta.poem.2725": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ehren-Ged\u00e4chtn\u00fc\u00df  \n Fr. S.   P.   g. M. den 15. Jenner 1679.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du Tugend-volles Ehren-Weib/", "tokens": ["Du", "Tu\u00b7gen\u00b7dvol\u00b7les", "Eh\u00b7ren\u00b7Weib", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So giebst du endlich deinen Leib/", "tokens": ["So", "giebst", "du", "end\u00b7lich", "dei\u00b7nen", "Leib", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der allgemeinen Mutter wieder/", "tokens": ["Der", "all\u00b7ge\u00b7mei\u00b7nen", "Mut\u00b7ter", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So f\u00fchret dich ein sanffter Tod", "tokens": ["So", "f\u00fch\u00b7ret", "dich", "ein", "sanff\u00b7ter", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus diesem Kercker voller Noth/", "tokens": ["Aus", "die\u00b7sem", "Ker\u00b7cker", "vol\u00b7ler", "Noth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und legt zur Ruh die m\u00fcden Glieder?", "tokens": ["Und", "legt", "zur", "Ruh", "die", "m\u00fc\u00b7den", "Glie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du f\u00fchlst nicht mehr des Alters Schnee/", "tokens": ["Du", "f\u00fchlst", "nicht", "mehr", "des", "Al\u00b7ters", "Schnee", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Sorgen Pein/ der Kranckheit Weh/", "tokens": ["Der", "Sor\u00b7gen", "Pein", "/", "der", "Kran\u00b7ck\u00b7heit", "Weh", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Und bist vollkommen jetzt genesen.", "tokens": ["Und", "bist", "voll\u00b7kom\u00b7men", "jetzt", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Seel\u2019 eilt ihrem Ursprung nach", "tokens": ["Die", "Seel'", "eilt", "ih\u00b7rem", "Ur\u00b7sprung", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und zwingt sich zu dem Sternen Dach/", "tokens": ["Und", "zwingt", "sich", "zu", "dem", "Ster\u00b7nen", "Dach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ob Haut und Bein schon hier verwesen.", "tokens": ["Ob", "Haut", "und", "Bein", "schon", "hier", "ver\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ein Heyde scheut und f\u00fcrcht den Tod/", "tokens": ["Ein", "Hey\u00b7de", "scheut", "und", "f\u00fcrcht", "den", "Tod", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Den Weiser der uns f\u00fchrt zu GOtt", "tokens": ["Den", "Wei\u00b7ser", "der", "uns", "f\u00fchrt", "zu", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Den besten Freund und Rei\u00df-Gesellen.", "tokens": ["Den", "bes\u00b7ten", "Freund", "und", "Rei\u00df\u00b7Ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wenn all\u2019 im sterben von uns fliehn", "tokens": ["Wenn", "all'", "im", "ster\u00b7ben", "von", "uns", "fliehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPRART", "VVFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und ihren Fu\u00df zur\u00fccke ziehn", "tokens": ["Und", "ih\u00b7ren", "Fu\u00df", "zu\u00b7r\u00fc\u00b7cke", "ziehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "So wird er uns die Fahrt bestellen.", "tokens": ["So", "wird", "er", "uns", "die", "Fahrt", "be\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Ein Christ der nimmt den F\u00fchrer an", "tokens": ["Ein", "Christ", "der", "nimmt", "den", "F\u00fch\u00b7rer", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und ist gewi\u00df da\u00df er die Bahn", "tokens": ["Und", "ist", "ge\u00b7wi\u00df", "da\u00df", "er", "die", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Zu jenen Ewigkeiten zeiget.", "tokens": ["Zu", "je\u00b7nen", "E\u00b7wig\u00b7kei\u00b7ten", "zei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Er kan ihm nicht beschwerlich seyn", "tokens": ["Er", "kan", "ihm", "nicht", "be\u00b7schwer\u00b7lich", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Wenn er als Gast sich stellet ein", "tokens": ["Wenn", "er", "als", "Gast", "sich", "stel\u00b7let", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOUS", "NN", "PRF", "VVFIN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und zu ihm durch das Fenster steiget.", "tokens": ["Und", "zu", "ihm", "durch", "das", "Fens\u00b7ter", "stei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Zu dem O seelige Matron", "tokens": ["Zu", "dem", "O", "see\u00b7li\u00b7ge", "Mat\u00b7ron"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.26": {"text": "Wer so in Frieden zieht davon", "tokens": ["Wer", "so", "in", "Frie\u00b7den", "zieht", "da\u00b7von"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Empfindt\u2019 nicht seine Bitterkeiten.", "tokens": ["Emp\u00b7findt'", "nicht", "sei\u00b7ne", "Bit\u00b7ter\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Du h\u00e4ttest l\u00e4ngst daran gedacht", "tokens": ["Du", "h\u00e4t\u00b7test", "l\u00e4ngst", "da\u00b7ran", "ge\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PAV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Wie du zu deiner letzten Nacht", "tokens": ["Wie", "du", "zu", "dei\u00b7ner", "letz\u00b7ten", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "In Andacht m\u00f6chtest dich bereiten.", "tokens": ["In", "An\u00b7dacht", "m\u00f6ch\u00b7test", "dich", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Es schien da keine Sterbens-Qual.", "tokens": ["Es", "schien", "da", "kei\u00b7ne", "Ster\u00b7bens\u00b7Qual", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Gleichwie ein Licht das seinen Strahl", "tokens": ["Gleich\u00b7wie", "ein", "Licht", "das", "sei\u00b7nen", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Nach vielem Brennen mu\u00df verlieren:", "tokens": ["Nach", "vie\u00b7lem", "Bren\u00b7nen", "mu\u00df", "ver\u00b7lie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "So zog auch aus des C\u00f6rpers Hau\u00df/", "tokens": ["So", "zog", "auch", "aus", "des", "C\u00f6r\u00b7pers", "Hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Die Seel\u2019 als wie im Schlaf herau\u00df/", "tokens": ["Die", "Seel'", "als", "wie", "im", "Schlaf", "her\u00b7au\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "KOKOM", "APPRART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Und fieng die Freyheit an zu sp\u00fchren.", "tokens": ["Und", "fi\u00b7eng", "die", "Frey\u00b7heit", "an", "zu", "sp\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.37": {"text": "Dein Beyspiel gab uns zu verstehn.", "tokens": ["Dein", "Bey\u00b7spiel", "gab", "uns", "zu", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Da\u00df wenn wir gar die Welt durchgehn/", "tokens": ["Da\u00df", "wenn", "wir", "gar", "die", "Welt", "durch\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Kein gr\u00f6sser Gut sey zu erlangen.", "tokens": ["Kein", "gr\u00f6s\u00b7ser", "Gut", "sey", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Als wenn wir in der letzten Noth", "tokens": ["Als", "wenn", "wir", "in", "der", "letz\u00b7ten", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Durch einen sanfft und stillen Tod", "tokens": ["Durch", "ei\u00b7nen", "sanfft", "und", "stil\u00b7len", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Den Lohn der Gottesfurcht empfangen.", "tokens": ["Den", "Lohn", "der", "Got\u00b7tes\u00b7furcht", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Die war dir auch von Kindes Bein\u2019", "tokens": ["Die", "war", "dir", "auch", "von", "Kin\u00b7des", "Bein'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "In Seel und Hertz gepflantzet ein.", "tokens": ["In", "Seel", "und", "Hertz", "ge\u00b7pflant\u00b7zet", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Es r\u00fchmt noch Leipzig unvergessen", "tokens": ["Es", "r\u00fchmt", "noch", "Leip\u00b7zig", "un\u00b7ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NE", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Den Vater der zu GOttes Ehr/", "tokens": ["Den", "Va\u00b7ter", "der", "zu", "Got\u00b7tes", "Ehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Hat ausgebreitet seine Lehr/", "tokens": ["Hat", "aus\u00b7ge\u00b7brei\u00b7tet", "sei\u00b7ne", "Lehr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Und in dem Kirchen-Ampt gesessen.", "tokens": ["Und", "in", "dem", "Kir\u00b7chen\u00b7Ampt", "ge\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Gott f\u00fchrt dich aus dem Vaterland", "tokens": ["Gott", "f\u00fchrt", "dich", "aus", "dem", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Und giebet eines Priesters Hand", "tokens": ["Und", "gie\u00b7bet", "ei\u00b7nes", "Pries\u00b7ters", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Dich Priester-Kind in reichem Seegen.", "tokens": ["Dich", "Pries\u00b7ter\u00b7Kind", "in", "rei\u00b7chem", "See\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "So bl\u00fchtest du in vollem Gl\u00fcck.", "tokens": ["So", "bl\u00fch\u00b7test", "du", "in", "vol\u00b7lem", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Ach aber grimmiges Geschick!", "tokens": ["Ach", "a\u00b7ber", "grim\u00b7mi\u00b7ges", "Ge\u00b7schick", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Herr Pollio mu\u00df fr\u00fch sich legen.", "tokens": ["Herr", "Pol\u00b7lio", "mu\u00df", "fr\u00fch", "sich", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VMFIN", "ADJD", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Es trennt des Todes herbes Weh", "tokens": ["Es", "trennt", "des", "To\u00b7des", "her\u00b7bes", "Weh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Das heil\u2019ge Land der keuschen Eh", "tokens": ["Das", "heil'\u00b7ge", "Land", "der", "keu\u00b7schen", "Eh"], "token_info": ["word", "word", "