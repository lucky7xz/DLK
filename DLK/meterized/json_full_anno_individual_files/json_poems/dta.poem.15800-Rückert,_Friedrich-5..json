{"dta.poem.15800": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nach den Umst\u00e4nden sich zu richten, nach der Zeit,", "tokens": ["Nach", "den", "Um\u00b7st\u00e4n\u00b7den", "sich", "zu", "rich\u00b7ten", ",", "nach", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "PTKZU", "VVINF", "$,", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ist zweierlei; hier steht, Kind, der Beweis nicht weit.", "tokens": ["Ist", "zwei\u00b7er\u00b7lei", ";", "hier", "steht", ",", "Kind", ",", "der", "Be\u00b7weis", "nicht", "weit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$.", "ADV", "VVFIN", "$,", "NN", "$,", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Von diesen B\u00e4umen sind die einen buntgelaubt,", "tokens": ["Von", "die\u00b7sen", "B\u00e4u\u00b7men", "sind", "die", "ei\u00b7nen", "bunt\u00b7ge\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die andern v\u00f6llig gr\u00fcn vom Fu\u00dfe bis zum Haupt.", "tokens": ["Die", "an\u00b7dern", "v\u00f6l\u00b7lig", "gr\u00fcn", "vom", "Fu\u00b7\u00dfe", "bis", "zum", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "ADJD", "APPRART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die einen richten sich, weil Herbst ist, nach der Zeit,", "tokens": ["Die", "ei\u00b7nen", "rich\u00b7ten", "sich", ",", "weil", "Herbst", "ist", ",", "nach", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "$,", "KOUS", "NN", "VAFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die andern, weil noch warm, nach der Gelegenheit.", "tokens": ["Die", "an\u00b7dern", ",", "weil", "noch", "warm", ",", "nach", "der", "Ge\u00b7le\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KOUS", "ADV", "ADJD", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}