{"dta.poem.5747": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir k\u00f6nnen C\u00f6rper ohn Gewicht,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "C\u00f6r\u00b7per", "ohn", "Ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ohn Farben, sonder Hitz\u2019 und K\u00e4lt\u2019, und Festigkeit,", "tokens": ["Ohn", "Far\u00b7ben", ",", "son\u00b7der", "Hitz'", "und", "K\u00e4lt'", ",", "und", "Fes\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "NN", "KON", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ohn Regung, ohn Geschmack und ohn Geruch, zwar fassen;", "tokens": ["Ohn", "Re\u00b7gung", ",", "ohn", "Ge\u00b7schmack", "und", "ohn", "Ge\u00b7ruch", ",", "zwar", "fas\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "KON", "APPR", "NN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch will sich die Natur derselben nicht", "tokens": ["Doch", "will", "sich", "die", "Na\u00b7tur", "der\u00b7sel\u00b7ben", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PRF", "ART", "NN", "PDAT", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ohn\u2019 eine Tieffe, L\u00e4ng\u2019 und Breite, fassen lassen.", "tokens": ["Ohn'", "ei\u00b7ne", "Tief\u00b7fe", ",", "L\u00e4ng'", "und", "Brei\u00b7te", ",", "fas\u00b7sen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NE", "KON", "NN", "$,", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man mag sich, was man will, f\u00fcr eine Ausnahm machen;", "tokens": ["Man", "mag", "sich", ",", "was", "man", "will", ",", "f\u00fcr", "ei\u00b7ne", "Aus\u00b7nahm", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "$,", "PRELS", "PIS", "VMFIN", "$,", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So stellen sich doch alle Sachen", "tokens": ["So", "stel\u00b7len", "sich", "doch", "al\u00b7le", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit ihrer Maa\u00df und ihren Gr\u00e4nzen dar:", "tokens": ["Mit", "ih\u00b7rer", "Maa\u00df", "und", "ih\u00b7ren", "Gr\u00e4n\u00b7zen", "dar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Demnach ist dieses klar", "tokens": ["Dem\u00b7nach", "ist", "die\u00b7ses", "klar"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PDAT", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und helle,", "tokens": ["Und", "hel\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJA", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Da\u00df der Materie ihr wahres Wesen", "tokens": ["Da\u00df", "der", "Ma\u00b7te\u00b7rie", "ihr", "wah\u00b7res", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Sey: ", "tokens": ["Sey", ":"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Die man die ", "tokens": ["Die", "man", "die"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIS", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.14": {"text": "Der C\u00f6rper Leib und wahres Seyn.", "tokens": ["Der", "C\u00f6r\u00b7per", "Leib", "und", "wah\u00b7res", "Seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die aber, so man nennet ", "tokens": ["Die", "a\u00b7ber", ",", "so", "man", "nen\u00b7net"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "$,", "ADV", "PIS", "VVFIN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}}}}