{"textgrid.poem.53489": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Wenn erst . . .", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmein Sohn, was h\u00f6r ich nur f\u00fcr Sachen?", "tokens": ["\u00bb", "mein", "Sohn", ",", "was", "h\u00f6r", "ich", "nur", "f\u00fcr", "Sa\u00b7chen", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was schreibt mir Mutter da ins Feld?", "tokens": ["Was", "schreibt", "mir", "Mut\u00b7ter", "da", "ins", "Feld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du willst die Schularbeit nicht machen,", "tokens": ["Du", "willst", "die", "Schul\u00b7ar\u00b7beit", "nicht", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "du brauchst jetzt so viel Taschengeld?", "tokens": ["du", "brauchst", "jetzt", "so", "viel", "Ta\u00b7schen\u00b7geld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du sitzt jetzt manchmal schon beim Weine", "tokens": ["Du", "sitzt", "jetzt", "manch\u00b7mal", "schon", "beim", "Wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "A