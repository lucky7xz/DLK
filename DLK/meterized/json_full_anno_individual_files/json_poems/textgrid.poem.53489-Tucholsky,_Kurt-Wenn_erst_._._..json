des", "tokens": ["Zum", "Bei\u00b7spiel", "Min\u00b7chen", "sp\u00fcrt", "ein", "lin\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gef\u00fchl in ihrem zart Gem\u00fct.", "tokens": ["Ge\u00b7f\u00fchl", "in", "ih\u00b7rem", "zart", "Ge\u00b7m\u00fct", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie steht jetzt im Jahrzehnt des Kindes", "tokens": ["Sie", "steht", "jetzt", "im", "Jahr\u00b7zehnt", "des", "Kin\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "und ist auch h\u00e4ufig drum bem\u00fcht.", "tokens": ["und", "ist", "auch", "h\u00e4u\u00b7fig", "drum", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mama hat die und jene Sorgen,", "tokens": ["Ma\u00b7ma", "hat", "die", "und", "je\u00b7ne", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "KON", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "manch Fellchen ihr von dannen schwomm \u2013", "tokens": ["manch", "Fell\u00b7chen", "ihr", "von", "dan\u00b7nen", "schwomm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "APPR", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "der wuchert, und der will nicht borgen . . .", "tokens": ["der", "wu\u00