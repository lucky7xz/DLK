{"textgrid.poem.67211": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch ist die Zeit des Jahrs zu sehn, und die Gefilde", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch ist die Zeit des Jahrs zu sehn, und die Gefilde", "tokens": ["Noch", "ist", "die", "Zeit", "des", "Jahrs", "zu", "sehn", ",", "und", "die", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Sommers stehn in ihrem Glanz, in ihrer Milde;", "tokens": ["Des", "Som\u00b7mers", "stehn", "in", "ih\u00b7rem", "Glanz", ",", "in", "ih\u00b7rer", "Mil\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Feldes Gr\u00fcn ist pr\u00e4chtig ausgebreitet,", "tokens": ["Des", "Fel\u00b7des", "Gr\u00fcn", "ist", "pr\u00e4ch\u00b7tig", "aus\u00b7ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Allwo der Bach hinab mit Wellen gleitet.", "tokens": ["All\u00b7wo", "der", "Bach", "hin\u00b7ab", "mit", "Wel\u00b7len", "glei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.inve