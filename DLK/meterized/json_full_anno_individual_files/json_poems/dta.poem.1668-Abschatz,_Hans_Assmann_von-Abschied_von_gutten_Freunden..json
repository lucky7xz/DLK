{"dta.poem.1668": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Abschied von gutten Freunden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Was soll die trunckne Feder schreiben? ", "tokens": ["Was", "soll", "die", "trunck\u00b7ne", "Fe\u00b7der", "schrei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was aus des Hertzens Grunde quillt/", "tokens": ["Was", "aus", "des", "Hert\u00b7zens", "Grun\u00b7de", "quillt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Worzu mich Lieb und Treue treiben/", "tokens": ["Wor\u00b7zu", "mich", "Lieb", "und", "Treu\u00b7e", "trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Oder/ Mayn und Rhein best\u00e4ndigst unterhilt.", "tokens": ["Die", "O\u00b7der", "/", "Mayn", "und", "Rhein", "be\u00b7st\u00e4n\u00b7digst", "un\u00b7ter\u00b7hilt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "NE", "KON", "NE", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nimm an di\u00df Denckmahl meiner H\u00e4nde", "tokens": ["Nimm", "an", "di\u00df", "Denck\u00b7mahl", "mei\u00b7ner", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PDS", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du treue Freund- und Br\u00fcderschafft/", "tokens": ["Du", "treu\u00b7e", "Freun\u00b7d", "und", "Br\u00fc\u00b7der\u00b7schafft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "TRUNC", "KON", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der ich zu Diensten mich verpf\u00e4nde", "tokens": ["Der", "ich", "zu", "Diens\u00b7ten", "mich", "ver\u00b7pf\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "PPER", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So lang in diesem Leib ein Lebens-Athem hafft.", "tokens": ["So", "lang", "in", "die\u00b7sem", "Leib", "ein", "Le\u00b7bens\u00b7A\u00b7them", "hafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PDAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Brauch der Welt ist viel zu sagen/", "tokens": ["Der", "Brauch", "der", "Welt", "ist", "viel", "zu", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich schweige was ich leiden mu\u00df:", "tokens": ["Ich", "schwei\u00b7ge", "was", "ich", "lei\u00b7den", "mu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du kanst mein Hertz in deinem fragen/", "tokens": ["Du", "kanst", "mein", "Hertz", "in", "dei\u00b7nem", "fra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ob ich befreyet sey von Schmertzen und Verdru\u00df.", "tokens": ["Ob", "ich", "be\u00b7fre\u00b7yet", "sey", "von", "Schmert\u00b7zen", "und", "Ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VAFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch weil es so der Himmel schicket/", "tokens": ["Doch", "weil", "es", "so", "der", "Him\u00b7mel", "schi\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So m\u00fcssen wir zufrieden seyn;", "tokens": ["So", "m\u00fcs\u00b7sen", "wir", "zu\u00b7frie\u00b7den", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer wei\u00df noch wo es uns gel\u00fccket/", "tokens": ["Wer", "wei\u00df", "noch", "wo", "es", "uns", "ge\u00b7l\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PWAV", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df wir uns hier und dar zusammen finden ein.", "tokens": ["Da\u00df", "wir", "uns", "hier", "und", "dar", "zu\u00b7sam\u00b7men", "fin\u00b7den", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "KON", "PAV", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Indessen leb in Lust und Freuden/", "tokens": ["In\u00b7des\u00b7sen", "leb", "in", "Lust", "und", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du meiner Seelen halbes Theil/", "tokens": ["Du", "mei\u00b7ner", "See\u00b7len", "hal\u00b7bes", "Theil", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auff kurtzer Zeit betr\u00fcbtes Scheiden", "tokens": ["Auff", "kurt\u00b7zer", "Zeit", "be\u00b7tr\u00fcb\u00b7tes", "Schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Folgt mit dem Wieder-sehn erw\u00fcnschtes Gl\u00fcck und Heyl.", "tokens": ["Folgt", "mit", "dem", "Wie\u00b7der\u00b7sehn", "er\u00b7w\u00fcnschtes", "Gl\u00fcck", "und", "Heyl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}}}}}