{"textgrid.poem.63589": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl f\u00fchl' ich, da\u00df der Schmerz gelinder wird,", "tokens": ["Wohl", "f\u00fchl'", "ich", ",", "da\u00df", "der", "Schmerz", "ge\u00b7lin\u00b7der", "wird", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn durch den Dreiklang dieser rauhen Saiten", "tokens": ["Wenn", "durch", "den", "Drei\u00b7klang", "die\u00b7ser", "rau\u00b7hen", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In dumpfer Melodie mein Finger irrt.", "tokens": ["In", "dum\u00b7pfer", "Me\u00b7lo\u00b7die", "mein", "Fin\u00b7ger", "irrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Eint\u00f6nig wie das Lied der Amme gleiten", "tokens": ["Ein\u00b7t\u00f6\u00b7nig", "wie", "das", "Lied", "der", "Am\u00b7me", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "ART", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Die Kl\u00e4nge mir ums Herz und stillen drin", "tokens": ["Die", "Kl\u00e4n\u00b7ge", "mir", "ums", "Herz", "und", "stil\u00b7len", "drin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Klagestimmen, die sich schwer bestreiten.", "tokens": ["Die", "Kla\u00b7ge\u00b7stim\u00b7men", ",", "die", "sich", "schwer", "be\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wenn ich so in Schlaf gesungen bin,", "tokens": ["Und", "wenn", "ich", "so", "in", "Schlaf", "ge\u00b7sun\u00b7gen", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Tritt das geliebte Bild mit hellen Z\u00fcgen,", "tokens": ["Tritt", "das", "ge\u00b7lieb\u00b7te", "Bild", "mit", "hel\u00b7len", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ein Traumgesicht, leibhaftig vor mich hin.", "tokens": ["Ein", "Traum\u00b7ge\u00b7sicht", ",", "leib\u00b7haf\u00b7tig", "vor", "mich", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie gern, wie dankbar la\u00df ich mich betr\u00fcgen", "tokens": ["Wie", "gern", ",", "wie", "dank\u00b7bar", "la\u00df", "ich", "mich", "be\u00b7tr\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "PWAV", "ADJD", "VVIMP", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und schwelg' im Wahne, wieder Hand in Hand", "tokens": ["Und", "schwel\u00b7g'", "im", "Wah\u00b7ne", ",", "wie\u00b7der", "Hand", "in", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "ADV", "NN", "APPR", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Und Mund an Mund und Herz an Herz zu schmiegen.", "tokens": ["Und", "Mund", "an", "Mund", "und", "Herz", "an", "Herz", "zu", "schmie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ich seh' mein Kind, so wie es vor mir stand,", "tokens": ["Ich", "seh'", "mein", "Kind", ",", "so", "wie", "es", "vor", "mir", "stand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "KOKOM", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Horchend, wenn ich ein Liedchen sang und pfiff,", "tokens": ["Hor\u00b7chend", ",", "wenn", "ich", "ein", "Lied\u00b7chen", "sang", "und", "pfiff", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.15": {"text": "Die gro\u00dfen Augen fest auf mich gewandt;", "tokens": ["Die", "gro\u00b7\u00dfen", "Au\u00b7gen", "fest", "auf", "mich", "ge\u00b7wandt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "