{"dta.poem.11339": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der abri\u00df wahrer und lob-w\u00fcrdiger  \n liebe, bey der H. und O. eheverbindung  \n im nahmen eines freundes.  \n  G. S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje mu\u00df die liebe sich nicht offtmahls schelten lassen!", "tokens": ["Wie", "mu\u00df", "die", "lie\u00b7be", "sich", "nicht", "offt\u00b7mahls", "schel\u00b7ten", "las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "VVFIN", "PRF", "PTKNEG", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man sieht sie als den brunn des meisten \u00fcbels an:", "tokens": ["Man", "sieht", "sie", "als", "den", "brunn", "des", "meis\u00b7ten", "\u00fc\u00b7bels", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "KOUS", "ART", "NN", "ART", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df manche vor der zeit erkrancken und verblassen,", "tokens": ["Da\u00df", "man\u00b7che", "vor", "der", "zeit", "er\u00b7kran\u00b7cken", "und", "ver\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VVINF", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das hat gemeiniglich die heisse brunst gethan:", "tokens": ["Das", "hat", "ge\u00b7mei\u00b7nig\u00b7lich", "die", "heis\u00b7se", "brunst", "ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie viele dirnen gehn mit tieffem ach! zu bette,", "tokens": ["Wie", "vie\u00b7le", "dir\u00b7nen", "gehn", "mit", "tief\u00b7fem", "ach", "!", "zu", "bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "PDS", "VVFIN", "APPR", "ADJA", "ITJ", "$.", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil sich ein kalter grei\u00df an ihre seite streckt?", "tokens": ["Weil", "sich", "ein", "kal\u00b7ter", "grei\u00df", "an", "ih\u00b7re", "sei\u00b7te", "streckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie mancher flatter-han\u00df liegt an der schmermuths-kette,", "tokens": ["Wie", "man\u00b7cher", "flat\u00b7ter\u00b7han\u00df", "liegt", "an", "der", "schmer\u00b7muths\u00b7ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil sein verwehntes maul nicht stets was neues leckt?", "tokens": ["Weil", "sein", "ver\u00b7wehn\u00b7tes", "maul", "nicht", "stets", "was", "neu\u00b7es", "leckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADV", "PWS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein ander kratzet sich gewaltig in dem kopffe,", "tokens": ["Ein", "an\u00b7der", "krat\u00b7zet", "sich", "ge\u00b7wal\u00b7tig", "in", "dem", "kopf\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nachdem sein stoltzer schatz sein geld und gut verprahlt:", "tokens": ["Nach\u00b7dem", "sein", "stolt\u00b7zer", "schatz", "sein", "geld", "und", "gut", "ver\u00b7prahlt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und jener kriegt sein weib aus eyfer bey dem zopffe,", "tokens": ["Und", "je\u00b7ner", "kriegt", "sein", "weib", "aus", "ey\u00b7fer", "bey", "dem", "zopf\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil sie ihm seinen ku\u00df nicht theur genung bezahlt.", "tokens": ["Weil", "sie", "ihm", "sei\u00b7nen", "ku\u00df", "nicht", "theur", "ge\u00b7nung", "be\u00b7zahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "NN", "PTKNEG", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dort gehet Corydon, der sich so fr\u00fch verliebet,", "tokens": ["Dort", "ge\u00b7het", "Co\u00b7ry\u00b7don", ",", "der", "sich", "so", "fr\u00fch", "ver\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "PRELS", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und aus der lehr und schul ins hochzeit-bette gieng.", "tokens": ["Und", "aus", "der", "lehr", "und", "schul", "ins", "hoch\u00b7zeit\u00b7bet\u00b7te", "gieng", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "KON", "ADJD", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "N\u00e4chsthin sa\u00df Corniger geb\u00fcckt und h\u00f6chst betr\u00fcbet,", "tokens": ["N\u00e4chst\u00b7hin", "sa\u00df", "Cor\u00b7ni\u00b7ger", "ge\u00b7b\u00fcckt", "und", "h\u00f6chst", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "VVPP", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weil ihm ein langes horn an seiner stirne hieng.", "tokens": ["Weil", "ihm", "ein", "lan\u00b7ges", "horn", "an", "sei\u00b7ner", "stir\u00b7ne", "hieng", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich k\u00f6nte noch sehr viel dergleichen unheil melden;", "tokens": ["Ich", "k\u00f6n\u00b7te", "noch", "sehr", "viel", "derg\u00b7lei\u00b7chen", "un\u00b7heil", "mel\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADV", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Allein das enge blat fa\u00dft dessen menge nicht:", "tokens": ["Al\u00b7lein", "das", "en\u00b7ge", "blat", "fa\u00dft", "des\u00b7sen", "men\u00b7ge", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "PDS", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Man kennet ohnedem die unbegl\u00fcckten helden,", "tokens": ["Man", "ken\u00b7net", "oh\u00b7ne\u00b7dem", "die", "un\u00b7be\u00b7gl\u00fcck\u00b7ten", "hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Venus und ihr sohn so \u00fcbel zugericht.", "tokens": ["Die", "Ve\u00b7nus", "und", "ihr", "sohn", "so", "\u00fc\u00b7bel", "zu\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Kan aber wohl ein mensch dergleichen liebe loben?", "tokens": ["Kan", "a\u00b7ber", "wohl", "ein", "mensch", "derg\u00b7lei\u00b7chen", "lie\u00b7be", "lo\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ART", "NN", "PIS", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wer ihr gehorchen will, ist billich tadelns werth.", "tokens": ["Wer", "ihr", "ge\u00b7hor\u00b7chen", "will", ",", "ist", "bil\u00b7lich", "ta\u00b7delns", "werth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$,", "VAFIN", "ADJD", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Indessen hat sie doch so mancher thor erhoben,", "tokens": ["In\u00b7des\u00b7sen", "hat", "sie", "doch", "so", "man\u00b7cher", "thor", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ob ihn die schmeicheley schon noch so offt gef\u00e4hrt.", "tokens": ["Ob", "ihn", "die", "schmei\u00b7che\u00b7ley", "schon", "noch", "so", "offt", "ge\u00b7f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dergleichen lieb ist blind, und gleichet einem kinde,", "tokens": ["Derg\u00b7lei\u00b7chen", "lieb", "ist", "blind", ",", "und", "glei\u00b7chet", "ei\u00b7nem", "kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VAFIN", "ADJD", "$,", "KON", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Das unverst\u00e4ndig ist, und nichts als spielen kan;", "tokens": ["Das", "un\u00b7ver\u00b7st\u00e4n\u00b7dig", "ist", ",", "und", "nichts", "als", "spie\u00b7len", "kan", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VAFIN", "$,", "KON", "PIS", "KOKOM", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wer blinden leitern solgt, der schifft mit schlechtem winde,", "tokens": ["Wer", "blin\u00b7den", "lei\u00b7tern", "solgt", ",", "der", "schifft", "mit", "schlech\u00b7tem", "win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "VVINF", "VMFIN", "$,", "PRELS", "VVFIN", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Kein kluger nimmt ein kind zu seinem lehrer an.", "tokens": ["Kein", "klu\u00b7ger", "nimmt", "ein", "kind", "zu", "sei\u00b7nem", "leh\u00b7rer", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So ist die liebe nicht, hoch-werthes paar! beschaffen,", "tokens": ["So", "ist", "die", "lie\u00b7be", "nicht", ",", "hoch\u00b7wert\u00b7hes", "paar", "!", "be\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "VVFIN", "PTKNEG", "$,", "ADJA", "NN", "$.", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Zu der dir jeder mund gel\u00fccke w\u00fcnschen mu\u00df.", "tokens": ["Zu", "der", "dir", "je\u00b7der", "mund", "ge\u00b7l\u00fc\u00b7cke", "w\u00fcn\u00b7schen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "PIAT", "NN", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Gesetzt, da\u00df viele sich aus unverstand vergaffen;", "tokens": ["Ge\u00b7setzt", ",", "da\u00df", "vie\u00b7le", "sich", "aus", "un\u00b7ver\u00b7stand", "ver\u00b7gaf\u00b7fen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PIS", "PRF", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Hier sieht und findet man nur einen klugen ku\u00df:", "tokens": ["Hier", "sieht", "und", "fin\u00b7det", "man", "nur", "ei\u00b7nen", "klu\u00b7gen", "ku\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PIS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Hier geht die liebe nicht mit zugebundnen augen:", "tokens": ["Hier", "geht", "die", "lie\u00b7be", "nicht", "mit", "zu\u00b7ge\u00b7bund\u00b7nen", "au\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Hier ist sie nicht ein kind, das unverst\u00e4ndig spielt.", "tokens": ["Hier", "ist", "sie", "nicht", "ein", "kind", ",", "das", "un\u00b7ver\u00b7st\u00e4n\u00b7dig", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Es kan ja die vernunfft leicht aus den fingern saugen:", "tokens": ["Es", "kan", "ja", "die", "ver\u00b7nunfft", "leicht", "aus", "den", "fin\u00b7gern", "sau\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "ADJD", "APPR", "ART", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df eine tumme lieb auf keine wohlfahrt zielt;", "tokens": ["Da\u00df", "ei\u00b7ne", "tum\u00b7me", "lieb", "auf", "kei\u00b7ne", "wohl\u00b7fahrt", "zielt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "APPR", "PIAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Jhr aber, deren lieb auf GOtt und witz gegr\u00fcndet,", "tokens": ["Ihr", "a\u00b7ber", ",", "de\u00b7ren", "lieb", "auf", "Gott", "und", "witz", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELAT", "ADJD", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und also auch mit recht des lobes w\u00fcrdig ist,", "tokens": ["Und", "al\u00b7so", "auch", "mit", "recht", "des", "lo\u00b7bes", "w\u00fcr\u00b7dig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJD", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Seyd sicher, da\u00df ihr hier den quell des gl\u00fcckes findet,", "tokens": ["Seyd", "si\u00b7cher", ",", "da\u00df", "ihr", "hier", "den", "quell", "des", "gl\u00fc\u00b7ckes", "fin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und in so edler eh das paradies erkiest;", "tokens": ["Und", "in", "so", "ed\u00b7ler", "eh", "das", "pa\u00b7ra\u00b7dies", "er\u00b7kiest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADJA", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Denn wahre liebe wird stets diesen ruhm behalten,", "tokens": ["Denn", "wah\u00b7re", "lie\u00b7be", "wird", "stets", "die\u00b7sen", "ruhm", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "VAFIN", "ADV", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Da\u00df sie der rechte port vergn\u00fcgter seelen sey.", "tokens": ["Da\u00df", "sie", "der", "rech\u00b7te", "port", "ver\u00b7gn\u00fcg\u00b7ter", "see\u00b7len", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADJD", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Des H\u00f6chsten gnade l\u00e4\u00dft ihr feuer nicht erkalten,", "tokens": ["Des", "H\u00f6chs\u00b7ten", "gna\u00b7de", "l\u00e4\u00dft", "ihr", "feu\u00b7er", "nicht", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Die tugend macht ihr haus von allem tadel frey.", "tokens": ["Die", "tu\u00b7gend", "macht", "ihr", "haus", "von", "al\u00b7lem", "ta\u00b7del", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "PIS", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Sie \u00fcbernimmt sich nicht; Sie wei\u00df wohl, da\u00df die hitze,", "tokens": ["Sie", "\u00fc\u00b7ber\u00b7nimmt", "sich", "nicht", ";", "Sie", "wei\u00df", "wohl", ",", "da\u00df", "die", "hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "$.", "PPER", "VVFIN", "ADV", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Die keinen z\u00fcgel hat, in das verderben rennt.", "tokens": ["Die", "kei\u00b7nen", "z\u00fc\u00b7gel", "hat", ",", "in", "das", "ver\u00b7der\u00b7ben", "rennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "$,", "APPR", "PDS", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Sie k\u00fc\u00dft nur, was ihr gleicht; Denn gleichheit ist die st\u00fctze,", "tokens": ["Sie", "k\u00fc\u00dft", "nur", ",", "was", "ihr", "gleicht", ";", "Denn", "gleich\u00b7heit", "ist", "die", "st\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "$.", "KON", "ADV", "VAFIN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Darauf sie sicher ruht, und keinen wechsel kennt.", "tokens": ["Da\u00b7rauf", "sie", "si\u00b7cher", "ruht", ",", "und", "kei\u00b7nen", "wech\u00b7sel", "kennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADJD", "VVFIN", "$,", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Sie ist nicht ungetreu; Sie folget dem magnete,", "tokens": ["Sie", "ist", "nicht", "un\u00b7ge\u00b7treu", ";", "Sie", "fol\u00b7get", "dem", "mag\u00b7ne\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$.", "PPER", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.50": {"text": "Und bleibet unverr\u00fcckt bey ihrem pole stehn.", "tokens": ["Und", "blei\u00b7bet", "un\u00b7ver\u00b7r\u00fcckt", "bey", "ih\u00b7rem", "po\u00b7le", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Sie sieht auf keine pracht; Denn stoltz ist ein comete,", "tokens": ["Sie", "sieht", "auf", "kei\u00b7ne", "pracht", ";", "Denn", "stoltz", "ist", "ein", "co\u00b7me\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$.", "KON", "ADJD", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Der denen nur gef\u00e4llt, die bald zu grunde gehn.", "tokens": ["Der", "de\u00b7nen", "nur", "ge\u00b7f\u00e4llt", ",", "die", "bald", "zu", "grun\u00b7de", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRELS", "ADV", "VVPP", "$,", "PRELS", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Sie sieht nicht blos auf geld; St\u00f6\u00dft aber das gel\u00fccke", "tokens": ["Sie", "sieht", "nicht", "blos", "auf", "geld", ";", "St\u00f6\u00dft", "a\u00b7ber", "das", "ge\u00b7l\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "NN", "$.", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Doch nicht mit f\u00fcssen weg. Man lernt erst, eh man freyt,", "tokens": ["Doch", "nicht", "mit", "f\u00fcs\u00b7sen", "weg", ".", "Man", "lernt", "erst", ",", "eh", "man", "freyt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "VVFIN", "PTKVZ", "$.", "PIS", "VVFIN", "ADV", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Wovon man leben kan. Wer klug ist, denckt zur\u00fccke,", "tokens": ["Wo\u00b7von", "man", "le\u00b7ben", "kan", ".", "Wer", "klug", "ist", ",", "denckt", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVINF", "VMFIN", "$.", "PWS", "ADJD", "VAFIN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Und auf das k\u00fcnfftige, damit ihn nichts gereut.", "tokens": ["Und", "auf", "das", "k\u00fcnff\u00b7ti\u00b7ge", ",", "da\u00b7mit", "ihn", "nichts", "ge\u00b7reut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "KOUS", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Die liebe z\u00f6rnet nicht; Sie ist nicht unbed\u00e4chtig;", "tokens": ["Die", "lie\u00b7be", "z\u00f6r\u00b7net", "nicht", ";", "Sie", "ist", "nicht", "un\u00b7be\u00b7d\u00e4ch\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKNEG", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Ein tugendhafftes paar lebt sonder eyfersucht.", "tokens": ["Ein", "tu\u00b7gend\u00b7haff\u00b7tes", "paar", "lebt", "son\u00b7der", "ey\u00b7fer\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Sie \u00fcbereilt sich nicht; Sie wird erst klug und m\u00e4chtig,", "tokens": ["Sie", "\u00fc\u00b7be\u00b7reilt", "sich", "nicht", ";", "Sie", "wird", "erst", "klug", "und", "m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "$.", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Damit das alter nicht die fr\u00fche gluth verflucht.", "tokens": ["Da\u00b7mit", "das", "al\u00b7ter", "nicht", "die", "fr\u00fc\u00b7he", "gluth", "ver\u00b7flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Die liebe geitzet nicht; Sie thut ihr was zu gute,", "tokens": ["Die", "lie\u00b7be", "geit\u00b7zet", "nicht", ";", "Sie", "thut", "ihr", "was", "zu", "gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "PIS", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "H\u00e4ngt aber doch dabey nicht der verschwendung nach.", "tokens": ["H\u00e4ngt", "a\u00b7ber", "doch", "da\u00b7bey", "nicht", "der", "ver\u00b7schwen\u00b7dung", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PAV", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Sie fa\u00dft sich in geduld, und bleibt bey frischem muthe,", "tokens": ["Sie", "fa\u00dft", "sich", "in", "ge\u00b7duld", ",", "und", "bleibt", "bey", "fri\u00b7schem", "mu\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Vertraut dem h\u00f6chsten GOtt, und scheut kein ungemach.", "tokens": ["Ver\u00b7traut", "dem", "h\u00f6chs\u00b7ten", "Gott", ",", "und", "scheut", "kein", "un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Di\u00df ist, geehrtstes paar! der abri\u00df deiner liebe;", "tokens": ["Di\u00df", "ist", ",", "ge\u00b7ehrts\u00b7tes", "paar", "!", "der", "ab\u00b7ri\u00df", "dei\u00b7ner", "lie\u00b7be", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "ADJA", "NN", "$.", "ART", "VVFIN", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Hier ist kein falscher strich, kein gifft der schmeicheley.", "tokens": ["Hier", "ist", "kein", "fal\u00b7scher", "strich", ",", "kein", "gifft", "der", "schmei\u00b7che\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "ADJD", "$,", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Gehorche, wie du thust, nur diesem edlen triebe,", "tokens": ["Ge\u00b7hor\u00b7che", ",", "wie", "du", "thust", ",", "nur", "die\u00b7sem", "ed\u00b7len", "trie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "PDAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Da\u00df deiner liebe ruhm dereinst vollkommen sey!", "tokens": ["Da\u00df", "dei\u00b7ner", "lie\u00b7be", "ruhm", "de\u00b7reinst", "voll\u00b7kom\u00b7men", "sey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Ich wei\u00df, das ende wird den sch\u00f6nen anfang cr\u00f6nen:", "tokens": ["Ich", "wei\u00df", ",", "das", "en\u00b7de", "wird", "den", "sch\u00f6\u00b7nen", "an\u00b7fang", "cr\u00f6\u00b7nen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Der himmel, den ihr ehrt, spricht selbst sein ja dazu!", "tokens": ["Der", "him\u00b7mel", ",", "den", "ihr", "ehrt", ",", "spricht", "selbst", "sein", "ja", "da\u00b7zu", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "PPOSAT", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Die freunde w\u00fcnschen es! La\u00dft neid und mi\u00dfgunst h\u00f6hnen;", "tokens": ["Die", "freun\u00b7de", "w\u00fcn\u00b7schen", "es", "!", "La\u00dft", "neid", "und", "mi\u00df\u00b7gunst", "h\u00f6h\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "$.", "VVIMP", "NN", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Wer so vern\u00fcnfftig liebt, der liebt in sichrer ruh.", "tokens": ["Wer", "so", "ver\u00b7n\u00fcnff\u00b7tig", "liebt", ",", "der", "liebt", "in", "sich\u00b7rer", "ruh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVFIN", "$,", "PRELS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Kurtz: Eure liebe wei\u00df kein kluger mund zu tadeln,", "tokens": ["Kurtz", ":", "Eu\u00b7re", "lie\u00b7be", "wei\u00df", "kein", "klu\u00b7ger", "mund", "zu", "ta\u00b7deln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PPOSAT", "ADJA", "VVFIN", "PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Des H\u00f6chsten segen geht mit euch ins schlaf-gemach:", "tokens": ["Des", "H\u00f6chs\u00b7ten", "se\u00b7gen", "geht", "mit", "euch", "ins", "schlaf\u00b7ge\u00b7mach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "APPR", "PPER", "APPRART", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Der liebe, so verstand und wahre tugend adeln,", "tokens": ["Der", "lie\u00b7be", ",", "so", "ver\u00b7stand", "und", "wah\u00b7re", "tu\u00b7gend", "a\u00b7deln", ","], "token_info": ["word", "word", "punct", "word", "w