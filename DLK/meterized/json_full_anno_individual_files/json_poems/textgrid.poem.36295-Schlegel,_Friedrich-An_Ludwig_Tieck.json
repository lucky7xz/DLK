{"textgrid.poem.36295": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Ludwig Tieck", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl mancher hofft', im neuen Licht zu leben,", "tokens": ["Wohl", "man\u00b7cher", "hofft'", ",", "im", "neu\u00b7en", "Licht", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$,", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es mu\u00df die tr\u00fcbe Nacht ja endlich schweigen;", "tokens": ["Es", "mu\u00df", "die", "tr\u00fc\u00b7be", "Nacht", "ja", "end\u00b7lich", "schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und h\u00e4tte, sich dem Morgenrot zu neigen,", "tokens": ["Und", "h\u00e4t\u00b7te", ",", "sich", "dem", "Mor\u00b7gen\u00b7rot", "zu", "nei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PRF", "ART", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Liebstes freudig opfernd hingegeben.", "tokens": ["Sein", "Liebs\u00b7tes", "freu\u00b7dig", "op\u00b7fernd", "hin\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der wollt' uns k\u00fchn zur alten Kunst erheben,", "tokens": ["Der", "wollt'", "uns", "k\u00fchn", "zur", "al\u00b7ten", "Kunst", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gebildet der des Lebens Massen zeigen,", "tokens": ["Ge\u00b7bil\u00b7det", "der", "des", "Le\u00b7bens", "Mas\u00b7sen", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dir schien des Witzes buntes F\u00fcllhorn eigen;", "tokens": ["Dir", "schien", "des", "Wit\u00b7zes", "bun\u00b7tes", "F\u00fcll\u00b7horn", "ei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch blieb das Beste nur ein liebend Streben.", "tokens": ["Doch", "blieb", "das", "Bes\u00b7te", "nur", "ein", "lie\u00b7bend", "Stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da nahte Genovev' in frommer Sch\u00f6ne;", "tokens": ["Da", "nah\u00b7te", "Ge\u00b7no\u00b7ve\u00b7v'", "in", "from\u00b7mer", "Sch\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wer f\u00fchlt nicht, da\u00df die Poesie gekommen,", "tokens": ["Wer", "f\u00fchlt", "nicht", ",", "da\u00df", "die", "Poe\u00b7sie", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"