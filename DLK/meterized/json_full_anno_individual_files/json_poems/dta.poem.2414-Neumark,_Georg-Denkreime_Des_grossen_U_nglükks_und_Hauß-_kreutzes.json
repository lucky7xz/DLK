{"dta.poem.2414": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Denkreime  \n  Des grossen  U ngl\u00fckks und Hau\u00df-  \n kreutzes", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Mit dlesem st\u00fcrtzet er drey Stokwerkhoch/ der Kleine/", "tokens": ["Mit", "dle\u00b7sem", "st\u00fcrt\u00b7zet", "er", "drey", "Stok\u00b7werk\u00b7hoch", "/", "der", "Klei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "CARD", "NN", "$(", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wei\u00df nicht wie herab/ auff harte Pflastersteine;", "tokens": ["Ich", "wei\u00df", "nicht", "wie", "her\u00b7ab", "/", "auff", "har\u00b7te", "Pflas\u00b7ter\u00b7stei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOKOM", "ADV", "$(", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Hirnfchal; ist entzwey/ das Blut leufft mildiglich", "tokens": ["Die", "Hirn\u00b7fchal", ";", "ist", "ent\u00b7zwey", "/", "das", "Blut", "leufft", "mil\u00b7dig\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "VAFIN", "PTKVZ", "$(", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An dreyen Ohrten au\u00df/ kein Gliedma\u00df reget sich/", "tokens": ["An", "drey\u00b7en", "Ohr\u00b7ten", "au\u00df", "/", "kein", "Glied\u00b7ma\u00df", "re\u00b7get", "sich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PTKVZ", "$(", "PIAT", "NN", "VVFIN", "PRF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er lieget gantz erstummt/ man tr\u00e4gt ihn in die Kammer/", "tokens": ["Er", "lie\u00b7get", "gantz", "er\u00b7stummt", "/", "man", "tr\u00e4gt", "ihn", "in", "die", "Kam\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "$(", "PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zieht ihm die Kleider ab/ man legt ihn voller Jammer", "tokens": ["Zieht", "ihm", "die", "Klei\u00b7der", "ab", "/", "man", "legt", "ihn", "vol\u00b7ler", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$(", "PIS", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und Elend in das Bett\u2019/ er liegt wie eine Leich\u2019/", "tokens": ["Und", "E\u00b7lend", "in", "das", "Bett'", "/", "er", "liegt", "wie", "ei\u00b7ne", "Leich'", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$(", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er siehet nichtes nicht/ das Angesicht ist bleich/", "tokens": ["Er", "sie\u00b7het", "nich\u00b7tes", "nicht", "/", "das", "An\u00b7ge\u00b7sicht", "ist", "bleich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKNEG", "$(", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Leib ist braun und blau/ die H\u00e4nde sind verstauchet/", "tokens": ["Der", "Leib", "ist", "braun", "und", "blau", "/", "die", "H\u00e4n\u00b7de", "sind", "ver\u00b7stau\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$(", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Man merket kaum an ihm  da\u00df er noch etwas hauchet/", "tokens": ["Man", "mer\u00b7ket", "kaum", "an", "ihm", "da\u00df", "er", "noch", "et\u00b7was", "hau\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "PPER", "KOUS", "PPER", "ADV", "PIS", "VVFIN", "$("], "meter": "----+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Man w\u00e4schet ihm de\u0303 Leib mit warmgemachtem Wein\u2019/", "tokens": ["Man", "w\u00e4\u00b7schet", "ihm", "d\u1ebd", "Leib", "mit", "warm\u00b7ge\u00b7mach\u00b7tem", "Wein'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und legt ihm Pfiaster auf Was nur kan dienlich seun", "tokens": ["Und", "legt", "ihm", "Pfi\u00b7as\u00b7ter", "auf", "Was", "nur", "kan", "dien\u00b7lich", "seun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "PWS", "ADV", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Das nimmt man vor die Hand. Es wird durch dieses Fal-", "tokens": ["Das", "nimmt", "man", "vor", "die", "Hand", ".", "Es", "wird", "durch", "die\u00b7ses", "Fa\u00b7l"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIS", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "APPR", "PDAT", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.14": {"text": "len/", "tokens": ["len", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.15": {"text": "Der Vater fo erschrekkt/ als wenn ein Donnerknallen", "tokens": ["Der", "Va\u00b7ter", "fo", "er\u00b7schrekkt", "/", "als", "wenn", "ein", "Don\u00b7ner\u00b7knal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVPP", "$(", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Jhm vor dem Hertzen wer\u2019/ es bricht der kalte Schwei\u00df", "tokens": ["Jhm", "vor", "dem", "Hert\u00b7zen", "wer'", "/", "es", "bricht", "der", "kal\u00b7te", "Schwei\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "VAFIN", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Jhm an der Stirnen aus/ die H\u00e4nde werden Ey\u00df/", "tokens": ["Jhm", "an", "der", "Stir\u00b7nen", "aus", "/", "die", "H\u00e4n\u00b7de", "wer\u00b7den", "Ey\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$(", "ART", "NN", "VAFIN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Er f\u00e4llt in Ohnmacht hin/ er kan kein Glied nicht regen/", "tokens": ["Er", "f\u00e4llt", "in", "Ohn\u00b7macht", "hin", "/", "er", "kan", "kein", "Glied", "nicht", "re\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$(", "PPER", "VMFIN", "PIAT", "NN", "PTKNEG", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da er doch ohne das drey Wochen schon gelegen", "tokens": ["Da", "er", "doch", "oh\u00b7ne", "das", "drey", "Wo\u00b7chen", "schon", "ge\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "CARD", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "An der verfluchten Gicht. Er schweiget lange still/", "tokens": ["An", "der", "ver\u00b7fluch\u00b7ten", "Gicht", ".", "Er", "schwei\u00b7get", "lan\u00b7ge", "still", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "F\u00e4ngt endlich also an: Ach Gott ist denn dein Will/", "tokens": ["F\u00e4ngt", "end\u00b7lich", "al\u00b7so", "an", ":", "Ach", "Gott", "ist", "denn", "dein", "Will", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKVZ", "$.", "ITJ", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Auf mich so wiederlich? Schl\u00e4gt denn mit vollen Fammen", "tokens": ["Auf", "mich", "so", "wie\u00b7der\u00b7lich", "?", "Schl\u00e4gt", "denn", "mit", "vol\u00b7len", "Fam\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "ADJD", "$.", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dein grosses Zorenfeur auf meinen Hal\u00df zusammen?", "tokens": ["Dein", "gros\u00b7ses", "Zo\u00b7ren\u00b7feur", "auf", "mei\u00b7nen", "Hal\u00df", "zu\u00b7sam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Den ersten liebsten Sohn hastu mir weggez\u00fckkt", "tokens": ["Den", "ers\u00b7ten", "liebs\u00b7ten", "Sohn", "has\u00b7tu", "mir", "weg\u00b7ge\u00b7z\u00fckkt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ju einer Schwerennoht; der ander\u2019 ist erdr\u00fckkt", "tokens": ["Ju", "ei\u00b7ner", "Schwe\u00b7ren\u00b7noht", ";", "der", "an\u00b7der'", "ist", "er\u00b7dr\u00fckkt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$.", "ART", "ADJD", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Von seiner Seugerin; Sol auch der dritte sterben", "tokens": ["Von", "sei\u00b7ner", "Seu\u00b7ge\u00b7rin", ";", "Sol", "auch", "der", "drit\u00b7te", "ster\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "VMFIN", "ADV", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und durch den schweren Fall/ so j\u00e4mmerlich verderben", "tokens": ["Und", "durch", "den", "schwe\u00b7ren", "Fall", "/", "so", "j\u00e4m\u00b7mer\u00b7lich", "ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$(", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der nun mein letzter ist? der eintzig und allein", "tokens": ["Der", "nun", "mein", "letz\u00b7ter", "ist", "?", "der", "eint\u00b7zig", "und", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPOSAT", "ADJA", "VAFIN", "$.", "ART", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Mein Trost/ mein Stab/ mein Schild im Alter sote/", "tokens": ["Mein", "Trost", "/", "mein", "Stab", "/", "mein", "Schild", "im", "Al\u00b7ter", "so\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "seyn?", "tokens": ["seyn", "?"], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+", "measure": "single.up"}, "line.31": {"text": "Doch/ mach es wie du wilst/ mein Gott du wirst schon sehen", "tokens": ["Doch", "/", "mach", "es", "wie", "du", "wilst", "/", "mein", "Gott", "du", "wirst", "schon", "se\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$(", "VVFIN", "PPER", "PWAV", "PPER", "VMFIN", "$(", "PPOSAT", "NN", "PPER", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Was mir wird n\u00fctzlich seyn/ Ich la\u00df\u2019 es gern geschehen", "tokens": ["Was", "mir", "wird", "n\u00fctz\u00b7lich", "seyn", "/", "Ich", "la\u00df'", "es", "gern", "ge\u00b7sche\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VAFIN", "ADJD", "VAINF", "$(", "PPER", "VVFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Was ich nicht \u00e4ndern kan/ verleyhe nur Geduld", "tokens": ["Was", "ich", "nicht", "\u00e4n\u00b7dern", "kan", "/", "ver\u00b7ley\u00b7he", "nur", "Ge\u00b7duld"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$(", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Du mein gerechter Gott und sey mir wieder huld.", "tokens": ["Du", "mein", "ge\u00b7rech\u00b7ter", "Gott", "und", "sey", "mir", "wie\u00b7der", "huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "KON", "VAFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Drauff fieng er wieder an alm\u00e4hlich zu beschweimen", "tokens": ["Drauff", "fi\u00b7eng", "er", "wie\u00b7der", "an", "al\u00b7m\u00e4h\u00b7lich", "zu", "be\u00b7schwei\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "ADJD", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.36": {"text": "Und das/ mahl \u00fcber mahl. (Hier war nicht zu verseumen", "tokens": ["Und", "das", "/", "mahl", "\u00fc\u00b7ber", "mahl", ".", "(", "Hier", "war", "nicht", "zu", "ver\u00b7seu\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "$(", "ADV", "APPR", "ADV", "$.", "$(", "ADV", "VAFIN", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Was Sinn und Hertz erquikkt.) Die Mutter steht wie", "tokens": ["Was", "Sinn", "und", "Hertz", "er\u00b7quikkt", ".", ")", "Die", "Mut\u00b7ter", "steht", "wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NN", "KON", "NN", "VVPP", "$.", "$(", "ART", "NN", "VVFIN", "KOKOM"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.38": {"text": "tod/", "tokens": ["tod", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.39": {"text": "Und weinet bitterlich in ihrer Hertzensnoht", "tokens": ["Und", "wei\u00b7net", "bit\u00b7ter\u00b7lich", "in", "ih\u00b7rer", "Hert\u00b7zens\u00b7noht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Sie weis vor grosser Angst sich selber kaum zu finden/", "tokens": ["Sie", "weis", "vor", "gros\u00b7ser", "Angst", "sich", "sel\u00b7ber", "kaum", "zu", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "APPR", "ADJA", "NN", "PRF", "ADV", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Sie klaget j\u00e4mmerlich mit schwachem H\u00e4ndewinden/", "tokens": ["Sie", "kla\u00b7get", "j\u00e4m\u00b7mer\u00b7lich", "mit", "schwa\u00b7chem", "H\u00e4n\u00b7de\u00b7win\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Bald steht sie wie erstarrt/ bald geht sie hin und her", "tokens": ["Bald", "steht", "sie", "wie", "er\u00b7starrt", "/", "