{"dta.poem.5785": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.71", "en:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da nun der Stoff ohn' Ende theilbar ist;", "tokens": ["Da", "nun", "der", "Stoff", "ohn'", "En\u00b7de", "theil\u00b7bar", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So mu\u00df das ", "tokens": ["So", "mu\u00df", "das"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "So unn\u00fctz-als unm\u00f6glich seyn.", "tokens": ["So", "un\u00b7n\u00fctz\u00b7als", "un\u00b7m\u00f6g\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man fasset sonder die\u00df ja die Bewegung wol:", "tokens": ["Man", "fas\u00b7set", "son\u00b7der", "die\u00df", "ja", "die", "Be\u00b7we\u00b7gung", "wol", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "PDS", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es ist und bleibet alles voll,", "tokens": ["Es", "ist", "und", "blei\u00b7bet", "al\u00b7les", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und alles kan sich doch bewegen.", "tokens": ["Und", "al\u00b7les", "kan", "sich", "doch", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Weisen aus Athen,", "tokens": ["Die", "Wei\u00b7sen", "aus", "A\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Woselbst wir von Vernunfft so manches Merckmal sehn,", "tokens": ["Wo\u00b7selbst", "wir", "von", "Ver\u00b7nunfft", "so", "man\u00b7ches", "Merck\u00b7mal", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Erwiesen, da\u00df, (wie wir dieselbe Meinung hegen)", "tokens": ["Er\u00b7wie\u00b7sen", ",", "da\u00df", ",", "(", "wie", "wir", "die\u00b7sel\u00b7be", "Mei\u00b7nung", "he\u00b7gen", ")"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "$,", "$(", "PWAV", "PPER", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Welt erf\u00fcllet sey: sie zeigten, wie so schnelle", "tokens": ["Die", "Welt", "er\u00b7f\u00fcl\u00b7let", "sey", ":", "sie", "zeig\u00b7ten", ",", "wie", "so", "schnel\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VAFIN", "$.", "PPER", "VVFIN", "$,", "PWAV", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein Fisch das Wasser theil\u2019, ohn da\u00df auf einer Stelle", "tokens": ["Ein", "Fisch", "das", "Was\u00b7ser", "theil'", ",", "ohn", "da\u00df", "auf", "ei\u00b7ner", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,", "KOUI", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein leerer Ort entst\u00fcnd\u2019; ihn halte nichts zur\u00fcck,", "tokens": ["Ein", "lee\u00b7rer", "Ort", "ent\u00b7st\u00fcnd'", ";", "ihn", "hal\u00b7te", "nichts", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dieweil die Fluht sich von ihm lencket;", "tokens": ["Die\u00b7weil", "die", "Fluht", "sich", "von", "ihm", "len\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und da\u00df dieselbe Fluht in selbem Augenblick", "tokens": ["Und", "da\u00df", "die\u00b7sel\u00b7be", "Fluht", "in", "sel\u00b7bem", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PDAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sich wieder in den Ort, den er verlassen, sencket.", "tokens": ["Sich", "wie\u00b7der", "in", "den", "Ort", ",", "den", "er", "ver\u00b7las\u00b7sen", ",", "sen\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}