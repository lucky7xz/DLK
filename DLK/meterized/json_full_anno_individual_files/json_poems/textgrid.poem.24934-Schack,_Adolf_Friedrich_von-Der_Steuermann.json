{"textgrid.poem.24934": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Steuermann", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbgen S\u00fcden gesteuert! Auf, zu den Rah'n,", "tokens": ["\u00bb", "gen", "S\u00fc\u00b7den", "ge\u00b7steu\u00b7ert", "!", "Auf", ",", "zu", "den", "Rah'n", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "VVPP", "$.", "APPR", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Matrosen, auf, zu den Reffen!", "tokens": ["Mat\u00b7ro\u00b7sen", ",", "auf", ",", "zu", "den", "Ref\u00b7fen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Brigg treibt der Teufel uns auf die Bahn,", "tokens": ["Die", "Brigg", "treibt", "der", "Teu\u00b7fel", "uns", "auf", "die", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wir konnten's nicht besser treffen!", "tokens": ["Wir", "konn\u00b7ten's", "nicht", "bes\u00b7ser", "tref\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Geschwind sie gekapert! Und ist es gethan,", "tokens": ["Ge\u00b7schwind", "sie", "ge\u00b7ka\u00b7pert", "!", "Und", "ist", "es", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$.", "KON", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "So sei, ich schw\u00f6r' es heilig und teuer,", "tokens": ["So", "sei", ",", "ich", "schw\u00f6r'", "es", "hei\u00b7lig", "und", "teu\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die H\u00e4lfte von dem Erbeuteten euer!\u00ab", "tokens": ["Die", "H\u00e4lf\u00b7te", "von", "dem", "Er\u00b7beu\u00b7te\u00b7ten", "eu\u00b7er", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Schiffsherr ruft's; mit Jubelgeschrei", "tokens": ["Der", "Schiffs\u00b7herr", "ruft's", ";", "mit", "Ju\u00b7bel\u00b7ge\u00b7schrei"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Begr\u00fc\u00dfen sein Wort die Matrosen.", "tokens": ["Be\u00b7gr\u00fc\u00b7\u00dfen", "sein", "Wort", "die", "Mat\u00b7ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "\u00bbwohlauf zur lustigen Kaperei!", "tokens": ["\u00bb", "wohl\u00b7auf", "zur", "lus\u00b7ti\u00b7gen", "Ka\u00b7pe\u00b7rei", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Weh auf der Brigg den Franzosen!\u00ab", "tokens": ["Weh", "auf", "der", "Brigg", "den", "Fran\u00b7zo\u00b7sen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Nur der Steuermann Tom trotzt k\u00fchn und frei:", "tokens": ["Nur", "der", "Steu\u00b7er\u00b7mann", "Tom", "trotzt", "k\u00fchn", "und", "frei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "\u00bbwas? pl\u00fcndern will die verworfene Rotte?", "tokens": ["\u00bb", "was", "?", "pl\u00fcn\u00b7dern", "will", "die", "ver\u00b7wor\u00b7fe\u00b7ne", "Rot\u00b7te", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "$.", "VVINF", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Nie soll das geschehn, beim lebendigen Gotte!\u00ab", "tokens": ["Nie", "soll", "das", "ge\u00b7schehn", ",", "beim", "le\u00b7ben\u00b7di\u00b7gen", "Got\u00b7te", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PDS", "VVINF", "$,", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Die Mannschaft, die es vernahm, erhub", "tokens": ["Die", "Mann\u00b7schaft", ",", "die", "es", "ver\u00b7nahm", ",", "er\u00b7hub"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Lachen: \u00bbHa \u00fcber den Pfaffen!", "tokens": ["Ein", "La\u00b7chen", ":", "\u00bb", "Ha", "\u00fc\u00b7ber", "den", "Pfaf\u00b7fen", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "ITJ", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Geh, Tom, und pred'ge dem Belzebub,", "tokens": ["Geh", ",", "Tom", ",", "und", "pre\u00b7d'\u00b7ge", "dem", "Bel\u00b7ze\u00b7bub", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht k\u00fcmmert dich, was wir schaffen!\u00ab", "tokens": ["Nicht", "k\u00fcm\u00b7mert", "dich", ",", "was", "wir", "schaf\u00b7fen", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVINF", "$.", "$("], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "\u00bbauf!\u00ab \u2013 donnert der Schiffsherr \u2013 \u00bbkein Verschub!", "tokens": ["\u00bb", "auf", "!", "\u00ab", "\u2013", "don\u00b7nert", "der", "Schiffs\u00b7herr", "\u2013", "\u00bb", "kein", "Ver\u00b7schub", "!"], "token_info": ["punct", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "$(", "$(", "VVFIN", "ART", "NN", "$(", "$(", "PIAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Werft \u00fcber Bord mir den Rebellen!\u00ab", "tokens": ["Werft", "\u00fc\u00b7ber", "Bord", "mir", "den", "Re\u00b7bel\u00b7len", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und flugs ihn packen die w\u00fct'gen Gesellen.", "tokens": ["Und", "flugs", "ihn", "pa\u00b7cken", "die", "w\u00fct'\u00b7gen", "Ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Tom st\u00fcrzt, versinkt in das sch\u00e4umende Meer,", "tokens": ["Tom", "st\u00fcrzt", ",", "ver\u00b7sinkt", "in", "das", "sch\u00e4u\u00b7men\u00b7de", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Von den Wirbeln hinabgezogen;", "tokens": ["Von", "den", "Wir\u00b7beln", "hin\u00b7ab\u00b7ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein anderer tritt an den Platz, der leer,", "tokens": ["Ein", "an\u00b7de\u00b7rer", "tritt", "an", "den", "Platz", ",", "der", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und steuert den Kiel durch die Wogen;", "tokens": ["Und", "steu\u00b7ert", "den", "Kiel", "durch", "die", "Wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NE", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Das Schiff fliegt hinter der Beute her;", "tokens": ["Das", "Schiff", "fliegt", "hin\u00b7ter", "der", "Beu\u00b7te", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Doch schon hat die Nacht zu dunkeln begonnen;", "tokens": ["Doch", "schon", "hat", "die", "Nacht", "zu", "dun\u00b7keln", "be\u00b7gon\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Nicht sieht man die Brigg; fast scheint sie entronnen.", "tokens": ["Nicht", "sieht", "man", "die", "Brigg", ";", "fast", "scheint", "sie", "ent\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PIS", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und es d\u00fcstert tief, als ob Todesgraun", "tokens": ["Und", "es", "d\u00fcs\u00b7tert", "tief", ",", "als", "ob", "To\u00b7des\u00b7graun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "$,", "KOKOM", "KOUS", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Rings \u00fcber dem Meere laste;", "tokens": ["Rings", "\u00fc\u00b7ber", "dem", "Mee\u00b7re", "las\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Matrosen stehn bei den Segeln und Taun;", "tokens": ["Die", "Mat\u00b7ro\u00b7sen", "stehn", "bei", "den", "Se\u00b7geln", "und", "Taun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Schiffsherr gebietet vom Maste \u2013", "tokens": ["Der", "Schiffs\u00b7herr", "ge\u00b7bie\u00b7tet", "vom", "Mas\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Da h\u00f6ren sie unten ein Rauschen und schaun,", "tokens": ["Da", "h\u00f6\u00b7ren", "sie", "un\u00b7ten", "ein", "Rau\u00b7schen", "und", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "KON", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Wie Tom, der lange versunken Geglaubte,", "tokens": ["Wie", "Tom", ",", "der", "lan\u00b7ge", "ver\u00b7sun\u00b7ken", "Ge\u00b7glaub\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "PRELS", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Aus der Flut auftaucht mit dem bleichen Haupte.", "tokens": ["Aus", "der", "Flut", "auf\u00b7taucht", "mit", "dem", "blei\u00b7chen", "Haup\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Sieh, wie empor zu des Schiffes Rand", "tokens": ["Sieh", ",", "wie", "em\u00b7por", "zu", "des", "Schif\u00b7fes", "Rand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Langsam der Schreckliche klettert!", "tokens": ["Lang\u00b7sam", "der", "Schreck\u00b7li\u00b7che", "klet\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wie neu er tritt an den alten Stand", "tokens": ["Wie", "neu", "er", "tritt", "an", "den", "al\u00b7ten", "Stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und den Steurer zu Boden schmettert!", "tokens": ["Und", "den", "Steu\u00b7rer", "zu", "Bo\u00b7den", "schmet\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Er wendet das Ruder mit fester Hand,", "tokens": ["Er", "wen\u00b7det", "das", "Ru\u00b7der", "mit", "fes\u00b7ter", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und, bange gekauert an den Borden,", "tokens": ["Und", ",", "ban\u00b7ge", "ge\u00b7kau\u00b7ert", "an", "den", "Bor\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Fl\u00fcstern die Schiffer: \u00bbEr steuert nach Norden.\u00ab", "tokens": ["Fl\u00fcs\u00b7tern", "die", "Schif\u00b7fer", ":", "\u00bb", "Er", "steu\u00b7ert", "nach", "Nor\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Stunden schwinden; dem Grausen zu nahn", "tokens": ["Die", "Stun\u00b7den", "schwin\u00b7den", ";", "dem", "Grau\u00b7sen", "zu", "nahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$.", "ART", "NN", "APPR", "ADJA"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mag keiner vor Schrecken wagen;", "tokens": ["Mag", "kei\u00b7ner", "vor", "Schre\u00b7cken", "wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und als es \u00fcber dem Ocean", "tokens": ["Und", "als", "es", "\u00fc\u00b7ber", "dem", "O\u00b7cean"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aufd\u00e4mmernd begann zu tagen,", "tokens": ["Auf\u00b7d\u00e4m\u00b7mernd", "be\u00b7gann", "zu", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da l\u00e4hmte sie alle das Graun; sie sahn", "tokens": ["Da", "l\u00e4hm\u00b7te", "sie", "al\u00b7le", "das", "Graun", ";", "sie", "sahn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Gebirge von Eise vor sich liegen,", "tokens": ["Ge\u00b7bir\u00b7ge", "von", "Ei\u00b7se", "vor", "sich