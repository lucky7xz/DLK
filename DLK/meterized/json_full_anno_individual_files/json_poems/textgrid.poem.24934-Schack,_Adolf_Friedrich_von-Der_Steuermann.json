d", "bald", ",", "wie", "von", "ei\u00b7nem", "krys\u00b7tal\u00b7le\u00b7nen", "Dom", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Ist oben das Schiff und rings im Kreise", "tokens": ["Ist", "o\u00b7ben", "das", "Schiff", "und", "rings", "im", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "KON", "ADV", "APPRART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Umschlossen von flutendem, berstendem Eise.", "tokens": ["Um\u00b7schlos\u00b7sen", "von", "flu\u00b7ten\u00b7dem", ",", "bers\u00b7ten\u00b7dem", "Ei\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "Die Schollen t\u00fcrmen sich riesengro\u00df", "tokens": ["Die", "Schol\u00b7len", "t\u00fcr\u00b7men", "sich", "rie\u00b7sen\u00b7gro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gleich Alpen zum Himmelsdache", "tokens": ["Gleich", "Al\u00b7pen", "zum", "Him\u00b7mels\u00b7da\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "APPRART", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Und st\u00fcrzen wieder zum Meeresscho\u00df", "tokens": ["Und", "st\u00fcr\u00b7zen", "wie\u00b7der", "zum", "Mee\u00b7res\u00b7scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hin