{"dta.poem.17870": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "113.  Der willige Betrogene.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.85", "af:0.14"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die S\u00fcnd ist voll Betrugs. L\u00e4st du dich sie regiern/", "tokens": ["Die", "S\u00fcnd", "ist", "voll", "Be\u00b7trugs", ".", "L\u00e4st", "du", "dich", "sie", "re\u00b7giern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "NN", "$.", "VVFIN", "PPER", "PRF", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So l\u00e4stu dich mit willn inn schlund der H\u00f6llen f\u00fchrn.", "tokens": ["So", "l\u00e4s\u00b7tu", "dich", "mit", "willn", "inn", "schlund", "der", "H\u00f6l\u00b7len", "f\u00fchrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}