{"dta.poem.23825": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Carolus Magnus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df ist der Grosse Carl/ Pepins des Kleinen Sohn/", "tokens": ["Di\u00df", "ist", "der", "Gros\u00b7se", "Carl", "/", "Pe\u00b7pins", "des", "Klei\u00b7nen", "Sohn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NE", "$(", "NE", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der/ weil ihm ward sein Reich der Francken viel", "tokens": ["Der", "/", "weil", "ihm", "ward", "sein", "Reich", "der", "Fran\u00b7cken", "viel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$(", "KOUS", "PPER", "VAFIN", "PPOSAT", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Teutschen \u00fcberwandt und ihrer G\u00f6tzen Menge/", "tokens": ["Die", "Teut\u00b7schen", "\u00fc\u00b7ber\u00b7wandt", "und", "ih\u00b7rer", "G\u00f6t\u00b7zen", "Men\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch in Italien fand einen neuen Thron/", "tokens": ["Auch", "in", "I\u00b7ta\u00b7li\u00b7en", "fand", "ei\u00b7nen", "neu\u00b7en", "Thron", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da ihm Pabst Leo gab die K\u00e4yserliche Cron.", "tokens": ["Da", "ihm", "Pabst", "Leo", "gab", "die", "K\u00e4y\u00b7ser\u00b7li\u00b7che", "Cron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}