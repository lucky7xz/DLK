{"textgrid.poem.41779": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1. Caritas Pirkheimer", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit seinen T\u00fcrmen, seinen stolzen Warten", "tokens": ["Mit", "sei\u00b7nen", "T\u00fcr\u00b7men", ",", "sei\u00b7nen", "stol\u00b7zen", "War\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Liegt N\u00fcrnberg vor des Wandrers Blicken da,", "tokens": ["Liegt", "N\u00fcrn\u00b7berg", "vor", "des", "Wand\u00b7rers", "Bli\u00b7cken", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der aus dem Forst \u00bbdes Reiches Bienengarten,\u00ab", "tokens": ["Der", "aus", "dem", "Forst", "\u00bb", "des", "Rei\u00b7ches", "Bie\u00b7nen\u00b7gar\u00b7ten", ",", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$(", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich einem Stadtgetrieb' gen\u00fcber sah,", "tokens": ["Sich", "ei\u00b7nem", "Stadt\u00b7ge\u00b7trieb'", "ge\u00b7n\u00fc\u00b7ber", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In dem sich tausend H\u00e4nde emsig regen,", "tokens": ["In", "dem", "sich", "tau\u00b7send", "H\u00e4n\u00b7de", "em\u00b7sig", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "CARD", "NN", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das Gute gut, das Sch\u00f6ne sch\u00f6n zu pflegen.", "tokens": ["Das", "Gu\u00b7te", "gut", ",", "das", "Sch\u00f6\u00b7ne", "sch\u00f6n", "zu", "pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Welch eine Stadt! hier ragen Tempelhallen", "tokens": ["Welch", "ei\u00b7ne", "Stadt", "!", "hier", "ra\u00b7gen", "Tem\u00b7pel\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "$.", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Himmel auf durch allen Erdendunst,", "tokens": ["Zum", "Him\u00b7mel", "auf", "durch", "al\u00b7len", "Er\u00b7den\u00b7dunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bauh\u00fctten dort, drinn Mei\u00dfelschl\u00e4ge schallen", "tokens": ["Bau\u00b7h\u00fct\u00b7ten", "dort", ",", "drinn", "Mei\u00b7\u00dfel\u00b7schl\u00e4\u00b7ge", "schal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "ADV", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum heil'gen Dienst der Kirche und der Kunst;", "tokens": ["Zum", "heil'\u00b7gen", "Dienst", "der", "Kir\u00b7che", "und", "der", "Kunst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und \u00fcberall ein froh gesch\u00e4ftig Leben,", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "ein", "froh", "ge\u00b7sch\u00e4f\u00b7tig", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein heitres Schaffen, r\u00fcstig Vorw\u00e4rtsstreben.", "tokens": ["Ein", "heit\u00b7res", "Schaf\u00b7fen", ",", "r\u00fcs\u00b7tig", "Vor\u00b7w\u00e4rts\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Hoch \u2013 wie die Buchen in den W\u00e4ldern ragen,", "tokens": ["Hoch", "\u2013", "wie", "die", "Bu\u00b7chen", "in", "den", "W\u00e4l\u00b7dern", "ra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hoch \u2013 wie die Dome, die darnach erbaut,", "tokens": ["Hoch", "\u2013", "wie", "die", "Do\u00b7me", ",", "die", "dar\u00b7nach", "er\u00b7baut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "KOKOM", "ART", "NN", "$,", "PRELS", "PAV", "VVPP", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Hoch \u2013 ist das Ziel, dem tausend Herzen schlagen,", "tokens": ["Hoch", "\u2013", "ist", "das", "Ziel", ",", "dem", "tau\u00b7send", "Her\u00b7zen", "schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "VAFIN", "ART", "NN", "$,", "PRELS", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die fr\u00fch schon all die Herrlichkeit geschaut,", "tokens": ["Die", "fr\u00fch", "schon", "all", "die", "Herr\u00b7lich\u00b7keit", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "PIAT", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sich tr\u00e4nken durften an des Wissens Bronnen,", "tokens": ["Sich", "tr\u00e4n\u00b7ken", "durf\u00b7ten", "an", "des", "Wis\u00b7sens", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "VMFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im Strahl der Kunst, der g\u00f6ttlichen sich sonnen!", "tokens": ["Im", "Strahl", "der", "Kunst", ",", "der", "g\u00f6tt\u00b7li\u00b7chen", "sich", "son\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "ART", "ADJA", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Auch Caritas erquickt von solcher Quelle", "tokens": ["Auch", "Ca\u00b7ri\u00b7tas", "er\u00b7quickt", "von", "sol\u00b7cher", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "VVPP", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und von des Bruders Geistesschatz gen\u00e4hrt,", "tokens": ["Und", "von", "des", "Bru\u00b7ders", "Geis\u00b7tes\u00b7schatz", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dr\u00e4ngt es begeistert zu der Weisheit Schwelle,", "tokens": ["Dr\u00e4ngt", "es", "be\u00b7geis\u00b7tert", "zu", "der", "Weis\u00b7heit", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die zu betreten nur dem Mann gew\u00e4hrt \u2013", "tokens": ["Die", "zu", "be\u00b7tre\u00b7ten", "nur", "dem", "Mann", "ge\u00b7w\u00e4hrt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKZU", "VVFIN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Im Kloster darf der Wissenschaft sie leben.", "tokens": ["Im", "Klos\u00b7ter", "darf", "der", "Wis\u00b7sen\u00b7schaft", "sie", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sie ist ein Weib \u2013 und ihres Geistes Bl\u00fcten", "tokens": ["Sie", "ist", "ein", "Weib", "\u2013", "und", "ih\u00b7res", "Geis\u00b7tes", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sieht sie bedroht von rauher Au\u00dfenwelt,", "tokens": ["Sieht", "sie", "be\u00b7droht", "von", "rau\u00b7her", "Au\u00b7\u00dfen\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drum hat im Klara-Kloster, sie zu h\u00fcten,", "tokens": ["Drum", "hat", "im", "Kla\u00b7ra\u00b7Klos\u00b7ter", ",", "sie", "zu", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "APPRART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie sich den Himmelsblumen zugesellt,", "tokens": ["Sie", "sich", "den", "Him\u00b7mels\u00b7blu\u00b7men", "zu\u00b7ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die zwischen Mauern wohlgeborgen stehen,", "tokens": ["Die", "zwi\u00b7schen", "Mau\u00b7ern", "wohl\u00b7ge\u00b7bor\u00b7gen", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nicht mehr die Welt, nur noch den Himmel sehen.", "tokens": ["Nicht", "mehr", "die", "Welt", ",", "nur", "noch", "den", "Him\u00b7mel", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$,", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Den Himmel in dem eng begrenzten Raume,", "tokens": ["Den", "Him\u00b7mel", "in", "dem", "eng", "be\u00b7grenz\u00b7ten", "Rau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie hohe Klostermauern ihn gew\u00e4hrt;", "tokens": ["Wie", "ho\u00b7he", "Klos\u00b7ter\u00b7mau\u00b7ern", "ihn", "ge\u00b7w\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Reichsstadt Treiben weicht dem sanften Traume,", "tokens": ["Der", "Reichs\u00b7stadt", "Trei\u00b7ben", "weicht", "dem", "sanf\u00b7ten", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der hier allein die Seelen wiegt und n\u00e4hrt.", "tokens": ["Der", "hier", "al\u00b7lein", "die", "See\u00b7len", "wiegt", "und", "n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hier Steingew\u00f6lb' statt gr\u00fcner Buchenhallen,", "tokens": ["Hier", "Stein\u00b7ge\u00b7w\u00f6lb'", "statt", "gr\u00fc\u00b7ner", "Bu\u00b7chen\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durch das der Nonnen Chorges\u00e4nge schallen.", "tokens": ["Durch", "das", "der", "Non\u00b7nen", "Chor\u00b7ge\u00b7s\u00e4n\u00b7ge", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und drau\u00dfen Wettersturm und sonnig' Tagen,", "tokens": ["Und", "drau\u00b7\u00dfen", "Wet\u00b7ter\u00b7sturm", "und", "son\u00b7nig'", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der frische Luftzug einer gro\u00dfen Zeit:", "tokens": ["Der", "fri\u00b7sche", "Luft\u00b7zug", "ei\u00b7ner", "gro\u00b7\u00dfen", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sieghaft wird eine Geistesschlacht geschlagen", "tokens": ["Sieg\u00b7haft", "wird", "ei\u00b7ne", "Geis\u00b7tes\u00b7schlacht", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "VVPP"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Und Licht und Freiheit bilden ihr Geleit,", "tokens": ["Und", "Licht", "und", "Frei\u00b7heit", "bil\u00b7den", "ihr", "Ge\u00b7leit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df endlich selbst die festen Klosterschranken", "tokens": ["Da\u00df", "end\u00b7lich", "selbst", "die", "fes\u00b7ten", "Klos\u00b7ter\u00b7schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zerbrochen sind durch leuchtende Gedanken.", "tokens": ["Zer\u00b7bro\u00b7chen", "sind", "durch", "leuch\u00b7ten\u00b7de", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Doch Caritas entsetzt sich vor dem Treiben,", "tokens": ["Doch", "Ca\u00b7ri\u00b7tas", "ent\u00b7setzt", "sich", "vor", "dem", "Trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das selbst bedr\u00e4ngt ihr heiliges Asyl,", "tokens": ["Das", "selbst", "be\u00b7dr\u00e4ngt", "ihr", "hei\u00b7li\u00b7ges", "A\u00b7syl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Fern soll die Neurung dieser Schwelle bleiben,", "tokens": ["Fern", "soll", "die", "Neu\u00b7rung", "die\u00b7ser", "Schwel\u00b7le", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "PDAT", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Fern wie ihr selbst das ganze Weltgew\u00fchl!", "tokens": ["Fern", "wie", "ihr", "selbst", "das", "gan\u00b7ze", "Welt\u00b7ge\u00b7w\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und w\u00fcrden alle Kl\u00f6ster aufgehoben \u2013", "tokens": ["Und", "w\u00fcr\u00b7den", "al\u00b7le", "Kl\u00f6s\u00b7ter", "auf\u00b7ge\u00b7ho\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "\u00bbich weiche nie aus diesen heil'gen Hallen,", "tokens": ["\u00bb", "ich", "wei\u00b7che", "nie", "aus", "die\u00b7sen", "heil'\u00b7gen", "Hal\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", 