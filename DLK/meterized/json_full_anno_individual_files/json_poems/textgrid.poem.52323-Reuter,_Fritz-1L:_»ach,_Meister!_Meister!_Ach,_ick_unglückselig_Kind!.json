word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hei smitt den Spannreim in de Eck", "tokens": ["Hei", "smitt", "den", "Spann\u00b7reim", "in", "de", "Eck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "NE", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Un l\u00f6ppt nah sinen Jungen hen.", "tokens": ["Un", "l\u00f6ppt", "nah", "si\u00b7nen", "Jun\u00b7gen", "hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.8": {"text": "\u00bbherr Gott doch, Jung! Wo is di denn?\u00ab", "tokens": ["\u00bb", "herr", "Gott", "doch", ",", "Jung", "!", "Wo", "is", "di", "denn", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "NN", "ADV", "$,", "NN", "$.", "PWAV", "FM", "FM", "KON", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bbach, Meister! Meister! Kiken S' hir!", "tokens": ["\u00bb", "ach", ",", "Meis\u00b7ter", "!", "Meis\u00b7ter", "!", "Ki\u00b7ken", "S'", "hir", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "$.", "NN", "$.", "NN", "$(", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ick seih de Botter up't Brod nich mihr!\u00ab", "tokens": ["Ick", "seih", "de", "Bot\u00b7ter", "up't", "Brod", "nich", "mihr", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "PTKNEG", "ADV", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "De Meister nimmt dat Botterbrod,", "tokens": ["De", "Meis\u00b7ter", "nimmt", "dat", "Bot\u00b7ter\u00b7brod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Bekikt dat nipp von v\u00f6rn un hin'n.", "tokens": ["Be\u00b7kikt", "dat", "nipp", "von", "v\u00f6rn", "un", "hin'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "\u00bbso slag doch Gott den D\u00fcwel dod!", "tokens": ["\u00bb", "so", "slag", "doch", "Gott", "den", "D\u00fc\u00b7wel", "dod", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ADV", "NN", "ART", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ick s\u00fclwst kann ok kein Botter finn'n.", "tokens": ["Ick", "s\u00fclwst", "kann", "ok", "kein", "Bot\u00b7ter", "finn'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "VMFIN", "ADV", "PIAT", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Na, t\u00e4uw!\u00ab Hei geiht tau de Fru Meistern hen", "tokens": ["Na", ",", "t\u00e4uw", "!", "\u00ab", "Hei", "geiht", "tau", "de", "Fru", "Meis\u00b7tern", "hen"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "NE", "$.", "$(", "FM", "FM", "FM", "FM", "NN", "PPOSAT", "NN"], "meter": "--+-+--+--", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Un seggt tau ehr: \u00bbWat makst du denn?", "tokens": ["Un", "seggt", "tau", "ehr", ":", "\u00bb", "Wat", "makst", "du", "denn", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "$(", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.17": {"text": "Wo is hir Botter up dat Brod?", "tokens": ["Wo", "is", "hir", "Bot\u00b7ter", "up", "dat", "Brod", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "FM", "FM", "NN", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Dor slag doch Gott den D\u00fcwel dod!\u00ab", "tokens": ["Dor", "slag", "doch", "Gott", "den", "D\u00fc\u00b7wel", "dod", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "ADV", "NN", "ART", "NE", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "\u00bbis dat nich gaud f\u00f6r so'n Jungen?", "tokens": ["\u00bb", "is", "dat", "nich", "gaud", "f\u00f6r", "so'n", "Jun\u00b7gen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Ji s\u00fcnd man all so'n Leckertungen;", "tokens": ["Ji", "s\u00fcnd", "man", "all", "so'n", "Le\u00b7cker\u00b7tun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PIS", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Ji m\u00fcggten Hus un Hof vertehren,", "tokens": ["Ji", "m\u00fcgg\u00b7ten", "Hus", "un", "Hof", "ver\u00b7teh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.t