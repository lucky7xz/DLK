": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$.", "FM", "FM", "FM", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Un deiht den Jungen Kes' upsniden.", "tokens": ["Un", "deiht", "den", "Jun\u00b7gen", "Kes'", "ups\u00b7ni\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "De Meister bringt dat Botterbrod herin,", "tokens": ["De", "Meis\u00b7ter", "bringt", "dat", "Bot\u00b7ter\u00b7brod", "he\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Giwwt dat den Jungen hen und fr\u00f6ggt,", "tokens": ["Giwwt", "dat", "den", "Jun\u00b7gen", "hen", "und", "fr\u00f6ggt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Ob sick sin Blindheit nu hadd leggt", "tokens": ["Ob", "sick", "sin", "Blind\u00b7heit", "nu", "hadd", "leggt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Un ob hei wedder seihen k\u00fcnn.", "tokens": ["Un", "ob", "hei", "wed\u00b7der", "sei\u00b7hen", "k\u00fcnn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NE", "KON", "VVINF", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.33": {"text": "\u00bbja, Meister\u00ab, seggt de Jung ganz swipp,", "tokens": ["\u00bb", "ja", ",", "Meis\u00b7ter", "\u00ab", ",", "seggt", "de", "Jung", "ganz", "swipp", "