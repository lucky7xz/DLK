{"textgrid.poem.37668": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Jagdhorn schallt, es blinkt der Wald von Rossen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Jagdhorn schallt, es blinkt der Wald von Rossen,", "tokens": ["Das", "Jagd\u00b7horn", "schallt", ",", "es", "blinkt", "der", "Wald", "von", "Ros\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wer es h\u00f6rt, der ziehet mit im Zuge,", "tokens": ["Und", "wer", "es", "h\u00f6rt", ",", "der", "zie\u00b7het", "mit", "im", "Zu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Bienen folgen so der K\u00f6nigin im Fluge,", "tokens": ["Die", "Bie\u00b7nen", "fol\u00b7gen", "so", "der", "K\u00f6\u00b7ni\u00b7gin", "im", "Flu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So folgen auch der Kunst die Kunstgenossen.", "tokens": ["So", "fol\u00b7gen", "auch", "der", "Kunst", "die", "Kunst\u00b7ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So brausen Saiten aus Akkord zur Fuge,", "tokens": ["So", "brau\u00b7sen", "Sai\u00b7ten", "aus", "Ak\u00b7kord", "zur", "Fu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo kaum noch Lust die stumme Welt erschlossen,", "tokens": ["Wo", "kaum", "noch", "Lust", "die", "stum\u00b7me", "Welt", "er\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es sammeln sich viel tausend bunte Sprossen,", "tokens": ["Es", "sam\u00b7meln", "sich", "viel", "tau\u00b7send", "bun\u00b7te", "Spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Fr\u00fchling schien in bunter V\u00f6gel Zuge.", "tokens": ["Wo", "Fr\u00fch\u00b7ling", "schien", "in", "bun\u00b7ter", "V\u00f6\u00b7gel", "Zu\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Fr\u00fchlingsschein, du Kunst mir fern und nahe,", "tokens": ["O", "Fr\u00fch\u00b7lings\u00b7schein", ",", "du", "Kunst", "mir", "fern", "und", "na\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "NN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Herzen gl\u00fcht es mir, dir unterm Herzen,", "tokens": ["Im", "Her\u00b7zen", "gl\u00fcht", "es", "mir", ",", "dir", "un\u00b7term", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPER", "$,", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In dir ich mich und alle Welt umfahe.", "tokens": ["In", "dir", "ich", "mich", "und", "al\u00b7le", "Welt", "um\u00b7fa\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "PRF", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was du geboren mir in hohen Scherzen,", "tokens": ["Was", "du", "ge\u00b7bo\u00b7ren", "mir", "in", "ho\u00b7hen", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird fremd, wenn ich's in deinem Arm nicht sahe,", "tokens": ["Wird", "fremd", ",", "wenn", "ich's", "in", "dei\u00b7nem", "Arm", "nicht", "sa\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "PIS", "APPR", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da mag ich gern auch fremde Kinder herzen.", "tokens": ["Da", "mag", "ich", "gern", "auch", "frem\u00b7de", "Kin\u00b7der", "her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das Jagdhorn schallt, es blinkt der Wald vo