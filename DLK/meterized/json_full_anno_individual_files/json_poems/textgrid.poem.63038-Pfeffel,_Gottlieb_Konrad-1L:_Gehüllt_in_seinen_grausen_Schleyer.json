ter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Da\u00df sie nur einen Druck noch brauchte,", "tokens": ["Da\u00df", "sie", "nur", "ei\u00b7nen", "Druck", "noch", "brauch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Um todt zu seyn. Was! rief er aus,", "tokens": ["Um", "todt", "zu", "seyn", ".", "Was", "!", "rief", "er", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PTKZU", "VAINF", "$.", "PWS", "$.", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Darf eine schn\u00f6de Fledermaus", "tokens": ["Darf", "ei\u00b7ne", "schn\u00f6\u00b7de", "Fle\u00b7der\u00b7maus"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die Zirkel Archimeds zerst\u00f6ren!", "tokens": ["Die", "Zir\u00b7kel", "Ar\u00b7chi\u00b7meds", "zer\u00b7st\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Stirb, Frevlerin! \u2013 Das M\u00e4uschen schrie:", "tokens": ["Stirb", ",", "Frev\u00b7le\u00b7rin", "!", "\u2013", "Das", "M\u00e4u\u00b7schen", "schrie", ":"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$.", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Gestrenger Herr! la\u00df meine Z\u00e4hren", "tokens": ["Ge\u00b7stren\u00b7ger", "Herr", "!", "la\u00df", "mei\u00b7ne", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VVIMP", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Im Namen der Philosophie", "tokens": ["Im", "Na\u00b7men", "der", "Phi\u00b7lo\u00b7so\u00b7phie"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Dich um Barmherzigkeit beschw\u00f6ren!", "tokens": ["Dich", "um", "Barm\u00b7her\u00b7zig\u00b7keit", "be\u00b7schw\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Auch meinen Geist besch\u00e4ftigt sie,", "tokens": ["Auch", "mei\u00b7nen", "Geist", "be\u00b7sch\u00e4f\u00b7tigt", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Mein Auge sp\u00e4ht den Gang der Sph\u00e4ren:", "tokens": ["Mein", "Au\u00b7ge", "sp\u00e4ht", "den", "Gang", "der", "Sph\u00e4\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Kurz, ehe mich dein Zorn geschreckt,", "tokens": ["Kurz", ",", "e\u00b7he", "mich", "dein", "Zorn", "ge\u00b7schreckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Hab ich im Bild des kleinen B\u00e4ren", "tokens": ["Hab", "ich", "im", "Bild", "des", "klei\u00b7nen", "B\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-