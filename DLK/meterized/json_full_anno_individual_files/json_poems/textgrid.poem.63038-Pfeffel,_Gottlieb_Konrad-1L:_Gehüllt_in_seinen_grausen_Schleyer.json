rn", "ge\u00b7schreckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Hab ich im Bild des kleinen B\u00e4ren", "tokens": ["Hab", "ich", "im", "Bild", "des", "klei\u00b7nen", "B\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Heut einen neuen Stern entdeckt. \u2013", "tokens": ["Heut", "ei\u00b7nen", "neu\u00b7en", "Stern", "ent\u00b7deckt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Ich sehe wohl, wir sind Collegen!", "tokens": ["Ich", "se\u00b7he", "wohl", ",", "wir", "sind", "Col\u00b7le\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Versetzt der Kauz; nun meinetwegen!", "tokens": ["Ver\u00b7setzt", "der", "Kauz", ";", "nun", "mei\u00b7net\u00b7we\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Schon dieser Titel reitzet mich,", "tokens": ["Schon", "die\u00b7ser", "Ti\u00b7tel", "reit\u00b7zet", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Der Fakult\u00e4t zu Ehren, dich", "tokens": ["Der", "Fa\u00b7kul\u00b7t\u00e4t", "zu", "Eh\u00b7ren", ",", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "In meinem Magen zu begraben:", "tokens": ["In", "mei\u00b7nem", "Ma\u00b7gen", "zu", "be\u00b7gra\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Du weist ja, da\u00df zu jeder Zeit", "tokens": ["Du", "weist", "ja", ",", "da\u00df", "zu", "je\u00b7der", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Die Philosophen ungescheut", "tokens": ["Die", "Phi\u00b7lo\u00b7so\u00b7phen", "un\u00b7ge\u00b7scheut"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Einander aufgefressen haben.", "tokens": ["Ein\u00b7an\u00b7der", "auf\u00b7ge\u00b7fres\u00b7sen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}