{"dta.poem.8572": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das kleine J\u00f6hr in mir,", "tokens": ["Das", "klei\u00b7ne", "J\u00f6hr", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "das nach jedem Sonnenstrahl greift und nach jedem Schmetterling,", "tokens": ["das", "nach", "je\u00b7dem", "Son\u00b7nen\u00b7strahl", "greift", "und", "nach", "je\u00b7dem", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PIAT", "NN", "VVFIN", "KON", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "das Vergissmeinnichtaugen hat und das mir vor meinem Tode hoffentlich nicht", "tokens": ["das", "Ver\u00b7giss\u00b7mein\u00b7nich\u00b7tau\u00b7gen", "hat", "und", "das", "mir", "vor", "mei\u00b7nem", "To\u00b7de", "hof\u00b7fent\u00b7lich", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "KON", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "PTKNEG"], "meter": "--+--+-+--+-+-+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "sterben wird,", "tokens": ["ster\u00b7ben", "wird", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VAFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "entz\u00fcckt sich noch immer \u00fcber Ludwig Richter.", "tokens": ["ent\u00b7z\u00fcckt", "sich", "noch", "im\u00b7mer", "\u00fc\u00b7ber", "Lud\u00b7wig", "Rich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "APPR", "NE", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Grosspapa liebt Walter Scott.", "tokens": ["Der", "Gross\u00b7pa\u00b7pa", "liebt", "Wal\u00b7ter", "Scott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Schl\u00e4fchen,", "tokens": ["Mein", "Schl\u00e4f\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "sonntags,", "tokens": ["sonn\u00b7tags", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "wenn es zu Mittag Nelson-Cotteletts,", "tokens": ["wenn", "es", "zu", "Mit\u00b7tag", "Nel\u00b7son\u00b7Cot\u00b7te\u00b7letts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Karpfen in Bier, oder vielleicht gar eine Gans gegeben,", "tokens": ["Karp\u00b7fen", "in", "Bier", ",", "o\u00b7der", "viel\u00b7leicht", "gar", "ei\u00b7ne", "Gans", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "KON", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.5": {"text": "erledige ich auf einem blauen, gr\u00fcngestreiften Biedermannssopha,", "tokens": ["er\u00b7le\u00b7di\u00b7ge", "ich", "auf", "ei\u00b7nem", "blau\u00b7en", ",", "gr\u00fcn\u00b7ge\u00b7streif\u00b7ten", "Bie\u00b7der\u00b7manns\u00b7so\u00b7pha", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "\u00fcber dem an einer gelben Urv\u00e4tertapete ein Stich von Chodowiecki h\u00e4ngt;", "tokens": ["\u00fc\u00b7ber", "dem", "an", "ei\u00b7ner", "gel\u00b7ben", "Ur\u00b7v\u00e4\u00b7ter\u00b7ta\u00b7pe\u00b7te", "ein", "Stich", "von", "Cho\u00b7do\u00b7wi\u00b7e\u00b7cki", "h\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "ART", "ADJA", "NN", "ART", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "+---+-+--+--+--+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.7": {"text": "und auf meinem Vertiko,", "tokens": ["und", "auf", "mei\u00b7nem", "Ver\u00b7ti\u00b7ko", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "zwischen zwei Str\u00e4ussen aus Zittergras,", "tokens": ["zwi\u00b7schen", "zwei", "Str\u00e4us\u00b7sen", "aus", "Zit\u00b7ter\u00b7gras", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "APPR", "NE", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "paradiert eine blanke mit bunten Blumen bemalte Porzellankuh,", "tokens": ["pa\u00b7ra\u00b7diert", "ei\u00b7ne", "blan\u00b7ke", "mit", "bun\u00b7ten", "Blu\u00b7men", "be\u00b7mal\u00b7te", "Por\u00b7zel\u00b7lan\u00b7kuh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "APPR", "ADJA", "NN", "VVFIN", "NE", "$,"], "meter": "+----+--+-+--+-+--+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "die, w\u00e4hrend ich schnarche, gemolken wird.", "tokens": ["die", ",", "w\u00e4h\u00b7rend", "ich", "schnar\u00b7che", ",", "ge\u00b7mol\u00b7ken", "wird", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVPP", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}