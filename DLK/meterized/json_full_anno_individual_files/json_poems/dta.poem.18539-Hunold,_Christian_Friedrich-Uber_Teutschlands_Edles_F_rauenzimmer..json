{"dta.poem.18539": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber Teutschlands  \n Edles  \n  F rauenzimmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Du Wunder-sch\u00f6nes Land/ und Kleinod gantzer Erden!", "tokens": ["Du", "Wun\u00b7der\u00b7sch\u00f6\u00b7nes", "Land", "/", "und", "Klei\u00b7nod", "gant\u00b7zer", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo die Gl\u00fcckseligkeit mit jeden Tag erwacht.", "tokens": ["Wo", "die", "Gl\u00fcck\u00b7se\u00b7lig\u00b7keit", "mit", "je\u00b7den", "Tag", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du ird'sches Paradies/ wo Menschen Engel werden/", "tokens": ["Du", "ird'\u00b7sches", "Pa\u00b7ra\u00b7dies", "/", "wo", "Men\u00b7schen", "En\u00b7gel", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PWAV", "NN", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo Adams s\u00fcsse Kost uns zum Vergn\u00fcgen lacht.", "tokens": ["Wo", "A\u00b7dams", "s\u00fcs\u00b7se", "Kost", "uns", "zum", "Ver\u00b7gn\u00fc\u00b7gen", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADJA", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du Himmel voller Glantz der angenehmsten Sonnen/", "tokens": ["Du", "Him\u00b7mel", "vol\u00b7ler", "Glantz", "der", "an\u00b7ge\u00b7nehms\u00b7ten", "Son\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und deren Sch\u00f6nheits-Strahl in alle L\u00e4nder dringt.", "tokens": ["Und", "de\u00b7ren", "Sch\u00f6n\u00b7heits\u00b7Strahl", "in", "al\u00b7le", "L\u00e4n\u00b7der", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du Meer/ in deren Schaum Saturnus Krafft geronnen/", "tokens": ["Du", "Meer", "/", "in", "de\u00b7ren", "Schaum", "Sa\u00b7tur\u00b7nus", "Krafft", "ge\u00b7ron\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "APPR", "PRELAT", "NN", "NE", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Worinnen Venus offt in einem Jahr entspringt.", "tokens": ["Wo\u00b7rin\u00b7nen", "Ve\u00b7nus", "offt", "in", "ei\u00b7nem", "Jahr", "ent\u00b7springt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Begl\u00fcckter Teutschen Sitz! Du Sammel-Platz der Sch\u00f6nen!", "tokens": ["Be\u00b7gl\u00fcck\u00b7ter", "Teut\u00b7schen", "Sitz", "!", "Du", "Sam\u00b7mel\u00b7Platz", "der", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$.", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wo Himmel und Natur verschwendrisch worden ist.", "tokens": ["Wo", "Him\u00b7mel", "und", "Na\u00b7tur", "ver\u00b7schwend\u00b7risch", "wor\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADJD", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wo sich der St\u00e4dte Pracht mit Myrthen kan bekr\u00f6nen/", "tokens": ["Wo", "sich", "der", "St\u00e4d\u00b7te", "Pracht", "mit", "Myr\u00b7then", "kan", "be\u00b7kr\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "NN", "APPR", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und wo ein Menschen Mund nur G\u00f6tter Lippen k\u00fc\u00dft.", "tokens": ["Und", "wo", "ein", "Men\u00b7schen", "Mund", "nur", "G\u00f6t\u00b7ter", "Lip\u00b7pen", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "ADV", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Des g\u00fcldnen Apfels-Streit/ den Paris beygeleget/", "tokens": ["Des", "g\u00fcld\u00b7nen", "Ap\u00b7fels\u00b7Streit", "/", "den", "Pa\u00b7ris", "bey\u00b7ge\u00b7le\u00b7get", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und dessen Kostbarkeit der Venus zuerkand/", "tokens": ["Und", "des\u00b7sen", "Kost\u00b7bar\u00b7keit", "der", "Ve\u00b7nus", "zu\u00b7er\u00b7kand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wird durch den Ehrgeitz noch bey deiner Zeit erreget/", "tokens": ["Wird", "durch", "den", "Ehr\u00b7geitz", "noch", "bey", "dei\u00b7ner", "Zeit", "er\u00b7re\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und manches nennet sich vor dir das sch\u00f6nste Land.", "tokens": ["Und", "man\u00b7ches", "nen\u00b7net", "sich", "vor", "dir", "das", "sch\u00f6ns\u00b7te", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "---------+-+", "measure": "unknown.measure.di"}, "line.17": {"text": "Du Wunder-Theil der Welt! Paris tritt in die Schrancken/", "tokens": ["Du", "Wun\u00b7der\u00b7T\u00b7heil", "der", "Welt", "!", "Pa\u00b7ris", "tritt", "in", "die", "Schran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$.", "NE", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Venus/ Juno sich und Pallas sonst ersehn/", "tokens": ["Die", "Ve\u00b7nus", "/", "Ju\u00b7no", "sich", "und", "Pal\u00b7las", "sonst", "er\u00b7sehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NE", "PRF", "KON", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und wil sich \u00fcm den Prei\u00df des Apffels mit dir zancken/", "tokens": ["Und", "wil", "sich", "\u00fcm", "den", "Prei\u00df", "des", "Apf\u00b7fels", "mit", "dir", "zan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ADV", "ART", "NN", "ART", "NN", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sein Frauenzimmer soll in ersten Paare gehn.", "tokens": ["Sein", "Frau\u00b7en\u00b7zim\u00b7mer", "soll", "in", "ers\u00b7ten", "Paa\u00b7re", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Nun Engelland wil gar mit lauter Engeln prahlen/", "tokens": ["Nun", "En\u00b7gel\u00b7land", "wil", "gar", "mit", "lau\u00b7ter", "En\u00b7geln", "prah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VMFIN", "ADV", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Franckreich soll neben dir der Palmen Zinsman seyn.", "tokens": ["Fran\u00b7ck\u00b7reich", "soll", "ne\u00b7ben", "dir", "der", "Pal\u00b7men", "Zins\u00b7man", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "APPR", "PPER", "ART", "NN", "NE", "VAINF", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.23": {"text": "Ach Teutschland k\u00f6nte dich ein Paris noch bestrahlen/", "tokens": ["Ach", "Teutschland", "k\u00f6n\u00b7te", "dich", "ein", "Pa\u00b7ris", "noch", "be\u00b7strah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VMFIN", "PRF", "ART", "NE", "ADV", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.24": {"text": "Und stell'te die Vernunfft sich hier zum Richter ein!", "tokens": ["Und", "stell'\u00b7te", "die", "Ver\u00b7nunfft", "sich", "hier", "zum", "Rich\u00b7ter", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So w\u00fcrde deine Pracht und seltne Sch\u00e4tzbarkeiten", "tokens": ["So", "w\u00fcr\u00b7de", "dei\u00b7ne", "Pracht", "und", "selt\u00b7ne", "Sch\u00e4tz\u00b7bar\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sein kluger Augen-Raht noch mehr als vor besehn/", "tokens": ["Sein", "klu\u00b7ger", "Au\u00b7gen\u00b7Raht", "noch", "mehr", "als", "vor", "be\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "PIAT", "KOKOM", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und liese sich sein Blick auff andre L\u00e4nder leiten/", "tokens": ["Und", "lie\u00b7se", "sich", "sein", "Blick", "auff", "and\u00b7re", "L\u00e4n\u00b7der", "lei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So w\u00fcrde Paris Mund auff diesen Spruch bestehn:", "tokens": ["So", "w\u00fcr\u00b7de", "Pa\u00b7ris", "Mund", "auff", "die\u00b7sen", "Spruch", "be\u00b7stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Franckreich ist zwar ein Land mit Klugheit angef\u00fcllet/", "tokens": ["Fran\u00b7ck\u00b7reich", "ist", "zwar", "ein", "Land", "mit", "Klug\u00b7heit", "an\u00b7ge\u00b7f\u00fcl\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.30": {"text": "Und in Galanterie des Frauenzimmers Welt.", "tokens": ["Und", "in", "Ga\u00b7lan\u00b7te\u00b7rie", "des", "Frau\u00b7en\u00b7zim\u00b7mers", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ja wo Geistreicher Schertz gemein wie Wasser quillet.", "tokens": ["Ja", "wo", "Geist\u00b7rei\u00b7cher", "Schertz", "ge\u00b7mein", "wie", "Was\u00b7ser", "quil\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWAV", "NN", "NN", "ADJD", "KOKOM", "NN", "VVFIN", "$."], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.32": {"text": "Wo witziger Verstand die hohe Schule h\u00e4lt.", "tokens": ["Wo", "wit\u00b7zi\u00b7ger", "Ver\u00b7stand", "die", "ho\u00b7he", "Schu\u00b7le", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Allein da\u00df Sch\u00fcler offt die besten Meister werden/", "tokens": ["Al\u00b7lein", "da\u00df", "Sch\u00fc\u00b7ler", "offt", "die", "bes\u00b7ten", "Meis\u00b7ter", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "ADV", "ART", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Kan Teutschland sonder Ruhm mit Sachsen Zeuge seyn.", "tokens": ["Kan", "Teutschland", "son\u00b7der", "Ruhm", "mit", "Sach\u00b7sen", "Zeu\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "KON", "NN", "APPR", "NE", "NN", "VAINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.35": {"text": "Man pflantzet erst geschickt die Liljen frembder Erden/", "tokens": ["Man", "pflant\u00b7zet", "erst", "ge\u00b7schickt", "die", "Lil\u00b7jen", "fremb\u00b7der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "VVPP", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Denn k\u00f6mmt Natur und Kunst verwundrend \u00fcberein.", "tokens": ["Denn", "k\u00f6mmt", "Na\u00b7tur", "und", "Kunst", "ver\u00b7wund\u00b7rend", "\u00fc\u00b7be\u00b7re\u00b7in", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.37": {"text": "Du sch\u00f6ne Linden-Stadt! (so w\u00fcrde Paris sagen.)", "tokens": ["Du", "sch\u00f6\u00b7ne", "Lin\u00b7den\u00b7Stadt", "!", "(", "so", "w\u00fcr\u00b7de", "Pa\u00b7ris", "sa\u00b7gen", ".", ")"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "$(", "ADV", "VAFIN", "NE", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So bald mein Auge dich/ du Kleinod Meissens sieht/", "tokens": ["So", "bald", "mein", "Au\u00b7ge", "dich", "/", "du", "Klei\u00b7nod", "Meis\u00b7sens", "sieht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "PPER", "$(", "PPER", "NN", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Seh' ich die Pallas auch auff ihren Sieges-Wagen", "tokens": ["Seh'", "ich", "die", "Pal\u00b7las", "auch", "auff", "ih\u00b7ren", "Sie\u00b7ges\u00b7Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die mich/ so wie Paris zu ihren Sch\u00e4tzen zieht.", "tokens": ["Die", "mich", "/", "so", "wie", "Pa\u00b7ris", "zu", "ih\u00b7ren", "Sch\u00e4t\u00b7zen", "zieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$(", "ADV", "KOKOM", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die G\u00f6ttin kan allhier die kl\u00fcgsten T\u00f6chter zeigen/", "tokens": ["Die", "G\u00f6t\u00b7tin", "kan", "all\u00b7hier", "die", "kl\u00fcgs\u00b7ten", "T\u00f6ch\u00b7ter", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wo Witz/ Verstand und List sehr 