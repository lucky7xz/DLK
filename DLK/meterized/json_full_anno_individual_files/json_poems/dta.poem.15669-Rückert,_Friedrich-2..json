{"dta.poem.15669": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Begl\u00fcckt ist wer den Weg der S\u00fcnde gar nicht kennt,", "tokens": ["Be\u00b7gl\u00fcckt", "ist", "wer", "den", "Weg", "der", "S\u00fcn\u00b7de", "gar", "nicht", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PWS", "ART", "NN", "ART", "NN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom eignen Trieb gelenkt, den Weg des Guten rennt.", "tokens": ["Vom", "eig\u00b7nen", "Trieb", "ge\u00b7lenkt", ",", "den", "Weg", "des", "Gu\u00b7ten", "rennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch auch begl\u00fcckt, wer kennt den Abweg, ihn zu fliehn,", "tokens": ["Doch", "auch", "be\u00b7gl\u00fcckt", ",", "wer", "kennt", "den", "Ab\u00b7weg", ",", "ihn", "zu", "fliehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "PWS", "VVFIN", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um Andere davon zum Weg zur\u00fcck zu ziehn.", "tokens": ["Um", "An\u00b7de\u00b7re", "da\u00b7von", "zum", "Weg", "zu\u00b7r\u00fcck", "zu", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIS", "PAV", "APPRART", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das ist das schwere Gl\u00fcck des, der f\u00fcr sie geborgen", "tokens": ["Das", "ist", "das", "schwe\u00b7re", "Gl\u00fcck", "des", ",", "der", "f\u00fcr", "sie", "ge\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ART", "$,", "PRELS", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht sein will, sondern auch der Andern Heil besorgen.", "tokens": ["Nicht", "sein", "will", ",", "son\u00b7dern", "auch", "der", "An\u00b7dern", "Heil", "be\u00b7sor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAINF", "VMFIN", "$,", "KON", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}