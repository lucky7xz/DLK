{"dta.poem.12705": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines poeten.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer lieget ein poet, von einer solchen art,", "tokens": ["Hjer", "lie\u00b7get", "ein", "poet", ",", "von", "ei\u00b7ner", "sol\u00b7chen", "art", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie Opitz selber war, der meister deutscher lieder:", "tokens": ["Wie", "O\u00b7pitz", "sel\u00b7ber", "war", ",", "der", "meis\u00b7ter", "deut\u00b7scher", "lie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "VAFIN", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die verse paarten sich wie wohl-formirte glieder,", "tokens": ["Die", "ver\u00b7se", "paar\u00b7ten", "sich", "wie", "wohl\u00b7for\u00b7mir\u00b7te", "glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Frey, ungezwungen, rein, nicht undeutsch oder hart:", "tokens": ["Frey", ",", "un\u00b7ge\u00b7zwun\u00b7gen", ",", "rein", ",", "nicht", "un\u00b7deutsch", "o\u00b7der", "hart", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$,", "PTKNEG", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er hat auch sich und uns zur schande nichts geschrieben;", "tokens": ["Er", "hat", "auch", "sich", "und", "uns", "zur", "schan\u00b7de", "nichts", "ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PRF", "KON", "PPER", "APPRART", "ADJA", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was wunder, da\u00df sein ruhm bey allen ist verblieben?", "tokens": ["Was", "wun\u00b7der", ",", "da\u00df", "sein", "ruhm", "bey", "al\u00b7len", "ist", "ver\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}