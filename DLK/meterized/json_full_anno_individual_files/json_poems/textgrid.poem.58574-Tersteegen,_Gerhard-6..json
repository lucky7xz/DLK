{"textgrid.poem.58574": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bb\u203aGott mit uns!\u2039 ist das Wort, worin ich Frieden finde,", "tokens": ["\u00bb", "\u203a", "Gott", "mit", "uns", "!", "\u2039", "ist", "das", "Wort", ",", "wo\u00b7rin", "ich", "Frie\u00b7den", "fin\u00b7de", ","], "token_info": ["punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "NN", "APPR", "PPER", "$.", "$(", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u203agott mit uns!\u2039 ist der Sieg, worin ich \u00fcberwinde;", "tokens": ["\u203a", "gott", "mit", "uns", "!", "\u2039", "ist", "der", "Sieg", ",", "wo\u00b7rin", "ich", "\u00fc\u00b7berw\u00b7in\u00b7de", ";"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "APPR", "PPER", "$.", "$(", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ei, r\u00fcstet euch mit Macht, Welt, Teufel, S\u00fcnd' und H\u00f6ll',", "tokens": ["Ei", ",", "r\u00fcs\u00b7tet", "euch", "mit", "Macht", ",", "Welt", ",", "Teu\u00b7fel", ",", "S\u00fcnd'", "und", "H\u00f6ll'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schaut in mein Innerstes, hier ist Immanuel!\u00ab", "tokens": ["Schaut", "in", "mein", "In\u00b7ners\u00b7tes", ",", "hier", "ist", "Im\u00b7ma\u00b7nu\u00b7el", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "NE", "$.", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "So spricht der edle Geist, der Gott im Grund besitzet,", "tokens": ["So", "spricht", "der", "ed\u00b7le", "Geist", ",", "der", "Gott", "im", "Grund", "be\u00b7sit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u00bbsein Nahesein gibt Mut, sein Nahesein besch\u00fctzet;", "tokens": ["\u00bb", "sein", "Na\u00b7he\u00b7sein", "gibt", "Mut", ",", "sein", "Na\u00b7he\u00b7sein", "be\u00b7sch\u00fct\u00b7zet", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gott nimmt mich bei der Hand, er geht mit mir allein,", "tokens": ["Gott", "nimmt", "mich", "bei", "der", "Hand", ",", "er", "geht", "mit", "mir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da sagt er mir ins Ohr: ", "tokens": ["Da", "sagt", "er", "mir", "ins", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "