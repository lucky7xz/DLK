{"textgrid.poem.44325": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dir, den in wilde, unwirtbare W\u00fcsten,", "genre": "verse", "period": "N.A.", "pub_year": 1812, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir, den in wilde, unwirtbare W\u00fcsten,", "tokens": ["Dir", ",", "den", "in", "wil\u00b7de", ",", "un\u00b7wirt\u00b7ba\u00b7re", "W\u00fcs\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "wo nie ein Gl\u00fccklicher sich schauen lie\u00df,", "tokens": ["wo", "nie", "ein", "Gl\u00fcck\u00b7li\u00b7cher", "sich", "schau\u00b7en", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "PRF", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "auf Pontus ferne meerumtobte K\u00fcsten", "tokens": ["auf", "Pon\u00b7tus", "fer\u00b7ne", "meer\u00b7um\u00b7tob\u00b7te", "K\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "der Grimm von Romas t\u00fcckschem Herrscher stie\u00df;", "tokens": ["der", "Grimm", "von", "Ro\u00b7mas", "t\u00fcck\u00b7schem", "Herr\u00b7scher", "stie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "NE", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "dir, armer Dulder, weih ich diese Bl\u00e4tter,", "tokens": ["dir", ",", "ar\u00b7mer", "Dul\u00b7der", ",", "weih", "ich", "die\u00b7se", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "KOUS", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "denn gleiches Los beschieden uns die G\u00f6tter.", "tokens": ["denn", "glei\u00b7ches", "Los", "be\u00b7schie\u00b7den", "uns", "die", "G\u00f6t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von Menschen ferne lieg ich hier und weine,", "tokens": ["Von", "Men\u00b7schen", "fer\u00b7ne", "lieg", "ich", "hier", "und", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ungl\u00fccklicher als du, denn mich verbannt", "tokens": ["un\u00b7gl\u00fcck\u00b7li\u00b7cher", "als", "du", ",", "denn", "mich", "ver\u00b7bannt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "PPER", "$,", "KON", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ein Henker, f\u00fcrchterlicher als der deine,", "tokens": ["ein", "Hen\u00b7ker", ",", "f\u00fcrch\u00b7ter\u00b7li\u00b7cher", "als", "der", "dei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KOKOM", "ART", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "des Schicksals allgewaltge Eisenhand.", "tokens": ["des", "Schick\u00b7sals", "all\u00b7ge\u00b7walt\u00b7ge", "Ei\u00b7sen\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "zu Menschen Ohren dringt des Menschen Stimme,", "tokens": ["zu", "Men\u00b7schen", "Oh\u00b7ren", "dringt", "des", "Men\u00b7schen", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "doch taub ist das Geschick in seinem Grimme.", "tokens": ["doch", "taub", "ist", "das", "Ge\u00b7schick", "in", "sei\u00b7nem", "Grim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Weil du zu viel gesehn, zu viel gesprochen,", "tokens": ["Weil", "du", "zu", "viel", "ge\u00b7sehn", ",", "zu", "viel", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "PIS", "VVPP", "$,", "PTKA", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "traf dich des Kaisers harter Richterspruch,", "tokens": ["traf", "dich", "des", "Kai\u00b7sers", "har\u00b7ter", "Rich\u00b7ter\u00b7spruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "doch welch Vergehn wird denn an mir gerochen,", "tokens": ["doch", "welch", "Ver\u00b7gehn", "wird", "denn", "an", "mir", "ge\u00b7ro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "in dessen Herzen Fried und Unschuld schlug,", "tokens": ["in", "des\u00b7sen", "Her\u00b7zen", "Fried", "und", "Un\u00b7schuld", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "ist mirs bestimmt, so martervoll zu leiden,", "tokens": ["ist", "mirs", "be\u00b7stimmt", ",", "so", "mar\u00b7ter\u00b7voll", "zu", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "VVPP", "$,", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "so k\u00f6nnt ich dich um dein Vergehn beneiden.", "tokens": ["so", "k\u00f6nnt", "ich", "dich", "um", "dein", "Ver\u00b7gehn", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "F\u00fcr S\u00fcnden, lieblich im Begehn, zu b\u00fc\u00dfen,", "tokens": ["F\u00fcr", "S\u00fcn\u00b7den", ",", "lieb\u00b7lich", "im", "Be\u00b7gehn", ",", "zu", "b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "APPRART", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "das stumpft der grausenvollsten Strafe Qual,", "tokens": ["das", "stumpft", "der", "grau\u00b7sen\u00b7volls\u00b7ten", "Stra\u00b7fe", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "doch h\u00f6llisch leiden und sich schuldos wissen,", "tokens": ["doch", "h\u00f6l\u00b7lisch", "lei\u00b7den", "und", "sich", "schul\u00b7dos", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "KON", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "das schneidet tief wie dreigeschliffner Stahl;", "tokens": ["das", "schnei\u00b7det", "tief", "wie", "drei\u00b7ge\u00b7schliff\u00b7ner", "Stahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und bei den G\u00f6ttern, die den Meineid r\u00e4chen,", "tokens": ["und", "bei", "den", "G\u00f6t\u00b7tern", ",", "die", "den", "Mei\u00b7neid", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "rein ist mein Herz, ich wei\u00df nichts von Verbrechen!", "tokens": ["rein", "ist", "mein", "Herz", ",", "ich", "wei\u00df", "nichts", "von", "Ver\u00b7bre\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sanft trieb des Lebens Nachen, das Gewissen", "tokens": ["Sanft", "trieb", "des", "Le\u00b7bens", "Na\u00b7chen", ",", "das", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schlief drinnen wie ein neugebornes Kind;", "tokens": ["schlief", "drin\u00b7nen", "wie", "ein", "neu\u00b7ge\u00b7bor\u00b7nes", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "da ward ich pl\u00f6tzlich in die See gerissen,", "tokens": ["da", "ward", "ich", "pl\u00f6tz\u00b7lich", "in", "die", "See", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ein ungl\u00fcckselges Spiel von Meer und Wind;", "tokens": ["ein", "un\u00b7gl\u00fcck\u00b7sel\u00b7ges", "Spiel", "von", "Meer", "und", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "erloschen sind die sichern Leitersterne", "tokens": ["er\u00b7lo\u00b7schen", "sind", "die", "si\u00b7chern", "Lei\u00b7ters\u00b7ter\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "und meine Heimat birgt die Nebelferne.", "tokens": ["und", "mei\u00b7ne", "Hei\u00b7mat", "birgt", "die", "Ne\u00b7bel\u00b7fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die Hoffnung hat das Steuer aufgegeben", "tokens": ["Die", "Hoff\u00b7nung", "hat", "das", "Steu\u00b7er", "auf\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und flieht mit scheuem, windesschnellem Fu\u00df,", "tokens": ["und", "flieht", "mit", "scheu\u00b7em", ",", "win\u00b7des\u00b7schnel\u00b7lem", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "sie, die sonst selbst beim Ausgang aus dem Leben", "tokens": ["sie", ",", "die", "sonst", "selbst", "beim", "Aus\u00b7gang", "aus", "dem", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADV", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "an des Avernus dunkelm Schauerflu\u00df", "tokens": ["an", "des", "A\u00b7ver\u00b7nus", "dun\u00b7kelm", "Schau\u00b7er\u00b7flu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "dem m\u00fcden Waller tr\u00f6stend steht zur Seite,", "tokens": ["dem", "m\u00fc\u00b7den", "Wal\u00b7ler", "tr\u00f6s\u00b7tend", "steht", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "sie selbst versagt mir Armen ihr Geleite.", "tokens": ["sie", "selbst", "ver\u00b7sagt", "mir", "Ar\u00b7men", "ihr", "Ge\u00b7lei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Verzweiflung sitzt an ihrer Statt im Nachen", "tokens": ["Ver\u00b7zwei\u00b7flung", "sitzt", "an", "ih\u00b7rer", "Statt", "im", "Na\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und treibt den Kiel vom Lande weiter fort,", "tokens": ["und", "treibt", "den", "Kiel", "vom", "Lan\u00b7de", "wei\u00b7ter", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NE", "APPRART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "dorthin, wo aus des schwarzen Abgrunds Rachen", "tokens": ["dor\u00b7thin", ",", "wo", "aus", "des", "schwar\u00b7zen", "Ab\u00b7grunds", "Ra\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "der Jammer grinset und der bleiche Mord;", "tokens": ["der", "Jam\u00b7mer", "grin\u00b7set", "und", "der", "blei\u00b7che", "Mord", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und wohin immer meine Blicke schweifen,", "tokens": ["und", "wo\u00b7hin", "im\u00b7mer", "mei\u00b7ne", "Bli\u00b7cke", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "sie k\u00f6nnen nichts als Schreckliches ergreifen.", "tokens": ["sie", "k\u00f6n\u00b7nen", "nichts", "als", "Schreck\u00b7li\u00b7ches", "er\u00b7grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Nur einen Hafen l\u00e4\u00dft sie mich erschauen,", "tokens": ["Nur", "ei\u00b7nen", "Ha\u00b7fen", "l\u00e4\u00dft", "sie", "mich", "er\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "an dessen Mund in unerforschter Nacht", "tokens": ["an", "des\u00b7sen", "Mund", "in", "un\u00b7er\u00b7forschter", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "der Ewigkeit furchtbare Nebel grauen,", "tokens": ["der", "E\u00b7wig\u00b7keit", "furcht\u00b7ba\u00b7re", "Ne\u00b7bel", "grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "die bleiche Furcht mit scheuem Zagen wacht,", "tokens": ["die", "blei\u00b7che", "Furcht", "mit", "scheu\u00b7em", "Za\u00b7gen", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "die jedem, der sich nahet ihren Toren,", "tokens": ["die", "je\u00b7dem", ",", "der", "sich", "na\u00b7het", "ih\u00b7ren", "To\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PRELS", "PRF", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "d