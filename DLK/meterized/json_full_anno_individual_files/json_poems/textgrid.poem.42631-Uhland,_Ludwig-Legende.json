{"textgrid.poem.42631": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Legende", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist 'ne Kirche wohlbekannt,", "tokens": ["Es", "ist", "'ne", "Kir\u00b7che", "wohl\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sankt Michael vom Berg genannt;", "tokens": ["Sankt", "Mic\u00b7hael", "vom", "Berg", "ge\u00b7nannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPRART", "NN", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Am Ende vom Normannenlande", "tokens": ["Am", "En\u00b7de", "vom", "Nor\u00b7man\u00b7nen\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Auf eines hohen Felsen Rande,", "tokens": ["Auf", "ei\u00b7nes", "ho\u00b7hen", "Fel\u00b7sen", "Ran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Umschlossen \u00fcberall vom Meer,", "tokens": ["Um\u00b7schlos\u00b7sen", "\u00fc\u00b7be\u00b7rall", "vom", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur da\u00df von ", "tokens": ["Nur", "da\u00df", "von"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOUS", "APPR"], "meter": "+--", "measure": "dactylic.init"}, "line.7": {"text": "Sowie die Flut zur\u00fccketrat,", "tokens": ["So\u00b7wie", "die", "Flut", "zu\u00b7r\u00fc\u00b7cke\u00b7trat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich \u00f6ffnet ein gebahnter Pfad.", "tokens": ["Sich", "\u00f6ff\u00b7net", "ein", "ge\u00b7bahn\u00b7ter", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Es kommt die Flut zweimal im Tage", "tokens": ["Es", "kommt", "die", "Flut", "zwei\u00b7mal", "im", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mit schnell- und starkem Wellenschlage,", "tokens": ["Mit", "schnell", "und", "star\u00b7kem", "Wel\u00b7len\u00b7schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df mancher zu derselben Frist", "tokens": ["Da\u00df", "man\u00b7cher", "zu", "der\u00b7sel\u00b7ben", "Frist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mit gro\u00dfer Not entronnen ist.", "tokens": ["Mit", "gro\u00b7\u00dfer", "Not", "ent\u00b7ron\u00b7nen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Viel Waller zu der Kirche kommen", "tokens": ["Viel", "Wal\u00b7ler", "zu", "der", "Kir\u00b7che", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Zu ihres ew'gen Erbes Frommen.", "tokens": ["Zu", "ih\u00b7res", "ew'\u00b7gen", "Er\u00b7bes", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Einmal an einem hohen Feste", "tokens": ["Ein\u00b7mal", "an", "ei\u00b7nem", "ho\u00b7hen", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Beeilten sich die frommen G\u00e4ste,", "tokens": ["Be\u00b7eil\u00b7ten", "sich", "die", "from\u00b7men", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Zur heil'gen Messe hinzuwallen;", "tokens": ["Zur", "heil'\u00b7gen", "Mes\u00b7se", "hin\u00b7zu\u00b7wal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Doch hat die Flut sie \u00fcberfallen.", "tokens": ["Doch", "hat", "die", "Flut", "sie", "\u00fc\u00b7berf\u00b7al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Sie flohen auf des Pfades Enge", "tokens": ["Sie", "flo\u00b7hen", "auf", "des", "Pfa\u00b7des", "En\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Mit Hast und m\u00e4chtigem Gedr\u00e4nge.", "tokens": ["Mit", "Hast", "und", "m\u00e4ch\u00b7ti\u00b7gem", "Ge\u00b7dr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Nur einer armen Schwangern war", "tokens": ["Nur", "ei\u00b7ner", "ar\u00b7men", "Schwan\u00b7gern", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Die Kraft geschwunden ganz und gar,", "tokens": ["Die", "Kraft", "ge\u00b7schwun\u00b7den", "ganz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Gehemmt ihr Lauf von herben Schmerzen,", "tokens": ["Ge\u00b7hemmt", "ihr", "Lauf", "von", "her\u00b7ben", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Die sich ihr regten unterm Herzen.", "tokens": ["Die", "sich", "ihr", "reg\u00b7ten", "un\u00b7term", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Sie ward gesto\u00dfen von der Menge", "tokens": ["Sie", "ward", "ge\u00b7sto\u00b7\u00dfen", "von", "der", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Und fiel zu Boden im Gedr\u00e4nge.", "tokens": ["Und", "fiel", "zu", "Bo\u00b7den", "im", "Ge\u00b7dr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "So blieb sie liegen, unbeachtet,", "tokens": ["So", "blieb", "sie", "lie\u00b7gen", ",", "un\u00b7be\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Weil jeder sich zu retten trachtet.", "tokens": ["Weil", "je\u00b7der", "sich", "zu", "ret\u00b7ten", "trach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Die andern waren all entronnen", "tokens": ["Die", "an\u00b7dern", "wa\u00b7ren", "all", "ent\u00b7ron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VAFIN", "PIAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Und hatten schon den Berg gewonnen,", "tokens": ["Und", "hat\u00b7ten", "schon", "den", "Berg", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Doch wie sie nach der Frau hinsahen,", "tokens": ["Doch", "wie", "sie", "nach", "der", "Frau", "hin\u00b7sa\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "So t\u00e4t sich schon die Flut ihr nahen;", "tokens": ["So", "t\u00e4t", "sich", "schon", "die", "Flut", "ihr", "na\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "NN", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Wohl jede H\u00fclfe war zu sp\u00e4t,", "tokens": ["Wohl", "je\u00b7de", "H\u00fcl\u00b7fe", "war", "zu", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Drum wandten sie sich zum Gebet.", "tokens": ["Drum", "wand\u00b7ten", "sie", "sich", "zum", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.35": {"text": "Auch jene, die, dem Tode nah,", "tokens": ["Auch", "je\u00b7ne", ",", "die", ",", "dem", "To\u00b7de", "nah", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "$,", "PRELS", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Nicht Menschenh\u00fclfe m\u00f6glich sah,", "tokens": ["Nicht", "Men\u00b7schen\u00b7h\u00fcl\u00b7fe", "m\u00f6g\u00b7lich", "sah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Sie hat zu Jesus und Marien", "tokens": ["Sie", "hat", "zu", "Je\u00b7sus", "und", "Ma\u00b7ri\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NE", "KON", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.38": {"text": "Und zum Erzengel laut geschrieen.", "tokens": ["Und", "zum", "Er\u00b7zen\u00b7gel", "laut", "ge\u00b7schri\u00b7een", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.39": {"text": "Die Pilger haben's nicht vernommen,", "tokens": ["Die", "Pil\u00b7ger", "ha\u00b7ben's", "nicht", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Zum Himmel ist der Ruf gekommen.", "tokens": ["Zum", "Him\u00b7mel", "ist", "der", "Ruf", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Die s\u00fc\u00dfe Gottesmutter oben", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Got\u00b7tes\u00b7mut\u00b7ter", "o\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Hat sich von ihrem Thron erhoben.", "tokens": ["Hat", "sich", "von", "ih\u00b7rem", "Thron", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Die heil'ge Herrin voll Erbarmen", "tokens": ["Die", "heil'\u00b7ge", "Her\u00b7rin", "voll", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Wirft einen Schleier hin der Armen,", "tokens": ["Wirft", "ei\u00b7nen", "Schlei\u00b7er", "hin", "der", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Die unter solcher Decke Schutz", "tokens": ["Die", "un\u00b7ter", "sol\u00b7cher", "De\u00b7cke", "Schutz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Bewahrt ist vor der Wellen Trutz;", "tokens": ["Be\u00b7wahrt", "ist", "vor", "der", "Wel\u00b7len", "Trutz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Denn mitten in der Wasser Braus", "tokens": ["Denn", "mit\u00b7ten", "in", "der", "Was\u00b7ser", "Braus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Ist ihr gebaut ein trocknes Haus.", "tokens": ["Ist", "ihr", "ge\u00b7baut", "ein", "trock\u00b7nes", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Die Ebbezeit nicht ferne war,", "tokens": ["Die", "Eb\u00b7be\u00b7zeit", "nicht", "fer\u00b7ne", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Nun stund am Strand die ganze Schar.", "tokens": ["Nun", "stund", "am", "Strand", "die", "gan\u00b7ze", "Schar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Die Frau man l\u00e4ngst verloren gab;", "tokens": ["Die", "Frau", "man", "l\u00e4ngst", "ver\u00b7lo\u00b7ren", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Da wich die Flut vom Land hinab,", "tokens": ["Da", "wich", "die", "Flut", "vom", "Land", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Und trat aus all der Wellen Grund", "tokens": ["Und", "trat", "aus", "all", "der", "Wel\u00b7len", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Die Frau, ganz freudig und gesund,", "tokens": ["Die", "Frau", ",", "ganz", "freu\u00b7dig", "und", "ge\u00b7sund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Und in den Armen hielt sie lind", "tokens": ["Und", "in", "den", "Ar\u00b7men", "hielt", "sie", "lind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Ein lieblich neugeboren Kind.", "tokens": ["Ein", "lieb\u00b7lich", "neu\u00b7ge\u00b7bo\u00b7ren", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "Da t\u00e4ten Geistliche und Laien", "tokens": ["Da", "t\u00e4\u00b7ten", "Geist\u00b7li\u00b7che", "und", "Lai\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", 