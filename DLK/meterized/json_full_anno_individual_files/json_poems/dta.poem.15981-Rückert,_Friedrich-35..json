{"dta.poem.15981": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich seh' auf dieser Stuf', auf der ich bin gestellt,", "tokens": ["Ich", "seh'", "auf", "die\u00b7ser", "Stuf'", ",", "auf", "der", "ich", "bin", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$,", "APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nichts, wenn mein Blick sich hebt, viel, wenn er abwerts f\u00e4llt.", "tokens": ["Nichts", ",", "wenn", "mein", "Blick", "sich", "hebt", ",", "viel", ",", "wenn", "er", "ab\u00b7werts", "f\u00e4llt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$,", "ADV", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Tief seh' ich unter mir, und tiefer stets hinunter,", "tokens": ["Tief", "seh'", "ich", "un\u00b7ter", "mir", ",", "und", "tie\u00b7fer", "stets", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPER", "$,", "KON", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein reges Lebensheer, ein Wimmeln ewig munter.", "tokens": ["Ein", "re\u00b7ges", "Le\u00b7bens\u00b7heer", ",", "ein", "Wim\u00b7meln", "e\u00b7wig", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch wenn ich blick' empor, so seh' ich nichts als Licht;", "tokens": ["Doch", "wenn", "ich", "blick'", "em\u00b7por", ",", "so", "seh'", "ich", "nichts", "als", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Reicht, die hinunter reicht, die Leiter aufwerts nicht?", "tokens": ["Reicht", ",", "die", "hin\u00b7un\u00b7ter", "reicht", ",", "die", "Lei\u00b7ter", "auf\u00b7werts", "nicht", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "ART", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wol reicht sie auch hinauf, wol werden zwischen mir", "tokens": ["Wol", "reicht", "sie", "auch", "hin\u00b7auf", ",", "wol", "wer\u00b7den", "zwi\u00b7schen", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "ADV", "VAFIN", "APPR", "PPER"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Viel h\u00f6hre Wesen stehn und, H\u00f6chstes, zwischen dir.", "tokens": ["Viel", "h\u00f6h\u00b7re", "We\u00b7sen", "stehn", "und", ",", "H\u00f6chs\u00b7tes", ",", "zwi\u00b7schen", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "KON", "$,", "ADJA", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Allein ich seh' sie nicht, von deinem Licht geblendet,", "tokens": ["Al\u00b7lein", "ich", "seh'", "sie", "nicht", ",", "von", "dei\u00b7nem", "Licht", "ge\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das seine Kraft mir nur zum Niederblicken sendet.", "tokens": ["Das", "sei\u00b7ne", "Kraft", "mir", "nur", "zum", "Nie\u00b7der\u00b7bli\u00b7cken", "sen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "In tausend Bildern seh' ich hier dein Bild gewoben,", "tokens": ["In", "tau\u00b7send", "Bil\u00b7dern", "seh'", "ich", "hier", "dein", "Bild", "ge\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das tr\u00f6stet mich, da\u00df ich dich selbst nicht sehn kann droben.", "tokens": ["Das", "tr\u00f6s\u00b7tet", "mich", ",", "da\u00df", "ich", "dich", "selbst", "nicht", "sehn", "kann", "dro\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "PTKNEG", "VVINF", "VMFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}