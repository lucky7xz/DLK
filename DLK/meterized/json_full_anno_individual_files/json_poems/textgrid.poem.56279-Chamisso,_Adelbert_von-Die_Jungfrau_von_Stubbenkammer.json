{"textgrid.poem.56279": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Die Jungfrau von Stubbenkammer", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich trank in schnellen Z\u00fcgen", "tokens": ["Ich", "trank", "in", "schnel\u00b7len", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Leben und den Tod", "tokens": ["Das", "Le\u00b7ben", "und", "den", "Tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Beim K\u00f6nigsstuhl auf R\u00fcgen", "tokens": ["Beim", "K\u00f6\u00b7nigs\u00b7stuhl", "auf", "R\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am Strand im Morgenrot.", "tokens": ["Am", "Strand", "im", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich kam am fr\u00fchen Tage", "tokens": ["Ich", "kam", "am", "fr\u00fc\u00b7hen", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nachsinnend einsam her,", "tokens": ["Nach\u00b7sin\u00b7nend", "ein\u00b7sam", "her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und lauscht dem Wellenschlage,", "tokens": ["Und", "lauscht", "dem", "Wel\u00b7len\u00b7schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schaute \u00fcbers Meer.", "tokens": ["Und", "schau\u00b7te", "\u00fc\u00b7bers", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie schweifend aus der Weite", "tokens": ["Wie", "schwei\u00b7fend", "aus", "der", "Wei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Blick sich wieder neigt,", "tokens": ["Mein", "Blick", "sich", "wie\u00b7der", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da hat sich mir zur Seite", "tokens": ["Da", "hat", "sich", "mir", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Feenweib gezeigt.", "tokens": ["Ein", "Feen\u00b7weib", "ge\u00b7zeigt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "An Sch\u00f6nheit sondergleichen,", "tokens": ["An", "Sch\u00f6n\u00b7heit", "son\u00b7derg\u00b7lei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie nimmer Augen sahn,", "tokens": ["Wie", "nim\u00b7mer", "Au\u00b7gen", "sahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit goldner Kron und reichen", "tokens": ["Mit", "gold\u00b7ner", "Kron", "und", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gew\u00e4ndern angetan.", "tokens": ["Ge\u00b7w\u00e4n\u00b7dern", "an\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie kniet' auf Felsensteinen,", "tokens": ["Sie", "kniet'", "auf", "Fel\u00b7sen\u00b7stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umbrandet von der Flut,", "tokens": ["Um\u00b7bran\u00b7det", "von", "der", "Flut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wusch, mit vielem Weinen,", "tokens": ["Und", "wusch", ",", "mit", "vie\u00b7lem", "Wei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Tuch befleckt mit Blut.", "tokens": ["Ein", "Tuch", "be\u00b7fleckt", "mit", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Umsonst war ihr Beginnen,", "tokens": ["Um\u00b7sonst", "war", "ihr", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie wusch und wusch mit Flei\u00df,", "tokens": ["Sie", "wusch", "und", "wusch", "mit", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der b\u00f6se Fleck im Linnen", "tokens": ["Der", "b\u00f6\u00b7se", "Fleck", "im", "Lin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erschien doch nimmer wei\u00df.", "tokens": ["Er\u00b7schien", "doch", "nim\u00b7mer", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da sah sie unter Tr\u00e4nen", "tokens": ["Da", "sah", "sie", "un\u00b7ter", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich an, und bittend fast;", "tokens": ["Mich", "an", ",", "und", "bit\u00b7tend", "fast", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KON", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da hat ein hei\u00dfes Sehnen", "tokens": ["Da", "hat", "ein", "hei\u00b7\u00dfes", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich namenlos erfa\u00dft.", "tokens": ["Mich", "na\u00b7men\u00b7los", "er\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbgegr\u00fc\u00dfet mir, du blendend,", "tokens": ["\u00bb", "ge\u00b7gr\u00fc\u00b7\u00dfet", "mir", ",", "du", "blen\u00b7dend", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du wundersames Bild! \u2013 \u2013\u00ab", "tokens": ["Du", "wun\u00b7der\u00b7sa\u00b7mes", "Bild", "!", "\u2013", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "$(", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie aber, ab sich wendend,", "tokens": ["Sie", "a\u00b7ber", ",", "ab", "sich", "wen\u00b7dend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sprach schluchzend aber mild:", "tokens": ["Sprach", "schluch\u00b7zend", "a\u00b7ber", "mild", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbich weine tr\u00fcb und tr\u00fcber", "tokens": ["\u00bb", "ich", "wei\u00b7ne", "tr\u00fcb", "und", "tr\u00fc\u00b7ber"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ADJD", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Augen mir und blind;", "tokens": ["Die", "Au\u00b7gen", "mir", "und", "blind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gar viele ziehn vor\u00fcber,", "tokens": ["Gar", "vie\u00b7le", "ziehn", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nicht ", "tokens": ["Und", "nicht"], "token_info": ["word", "word"], "pos": ["KON", "PTKNEG"], "meter": "-+", "measure": "iambic.single"}}, "stanza.10": {"line.1": {"text": "Nach langem, bangem Hoffen", "tokens": ["Nach", "lan\u00b7gem", ",", "ban\u00b7gem", "Hof\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erreichst auch du den Ort \u2013", "tokens": ["Er\u00b7reichst", "auch", "du", "den", "Ort", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O h\u00e4ttest du getroffen", "tokens": ["O", "h\u00e4t\u00b7test", "du", "ge\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Gru\u00df das rechte Wort!", "tokens": ["Zum", "Gru\u00df", "das", "rech\u00b7te", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "H\u00e4ttst du ", "tokens": ["H\u00e4ttst", "du"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Ich war erl\u00f6st und dein,", "tokens": ["Ich", "war", "er\u00b7l\u00f6st", "und", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Hoffnung ist gebrochen,", "tokens": ["Die", "Hoff\u00b7nung", "ist", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es mu\u00df geschieden sein!\u00ab \u2013", "tokens": ["Es", "mu\u00df", "ge\u00b7schie\u00b7den", "sein", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Da stand sie auf zu gehen,", "tokens": ["Da", "stand", "sie", "auf", "zu", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Tuch in ihrer Hand,", "tokens": ["Das", "Tuch", "in", "ih\u00b7rer", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, wo die Pfeiler stehen,", "tokens": ["Und", ",", "wo", "die", "Pfei\u00b7ler", "ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Versank sie und verschwand.", "tokens": ["Ver\u00b7sank", "sie", "und", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ich trank in schnellen Z\u00fcgen", "tokens": ["Ich", "trank", "in", "schnel\u00b7len", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Leben und den Tod", "tokens": ["Das", "Le\u00b7ben", "und", "den", "Tod"], "token_info": ["word", "word", "word", "wor