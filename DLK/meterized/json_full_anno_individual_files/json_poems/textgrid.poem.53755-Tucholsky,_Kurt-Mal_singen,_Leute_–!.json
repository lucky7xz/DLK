{"textgrid.poem.53755": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Mal singen, Leute \u2013!", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Seemann schifft ins Meer hinaus,", "tokens": ["Der", "See\u00b7mann", "schifft", "ins", "Meer", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ihm ist so leicht zu Sinn.", "tokens": ["ihm", "ist", "so", "leicht", "zu", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Marie weint sich die Augen aus \u2013", "tokens": ["Ma\u00b7rie", "weint", "sich", "die", "Au\u00b7gen", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "er segelt rasch dahin.", "tokens": ["er", "se\u00b7gelt", "rasch", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er sitzt in der Komb\u00fcse", "tokens": ["Er", "sitzt", "in", "der", "Kom\u00b7b\u00fc\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "und stochert im Gem\u00fcse.", "tokens": ["und", "sto\u00b7chert", "im", "Ge\u00b7m\u00fc\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "und denkt sich: Wenns Marie nicht ist, na, dann ists eine Negerin . . .", "tokens": ["und", "denkt", "sich", ":", "Wenns", "Ma\u00b7rie", "nicht", "ist", ",", "na", ",", "dann", "ists", "ei\u00b7ne", "Ne\u00b7ge\u00b7rin", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "PRF", "$.", "KOUS", "NE", "PTKNEG", "VAFIN", "$,", "ITJ", "$,", "ADV", "VAFIN", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+--+-++-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "Der hat", "tokens": ["Der", "hat"], "token_info": ["word", "word"], "pos": ["PDS", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "in jeder Stadt 'ne Braut \u2013!", "tokens": ["in", "je\u00b7der", "Stadt", "'ne", "Braut", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Die erste f\u00fcr die Seele,", "tokens": ["Die", "ers\u00b7te", "f\u00fcr", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "die zweite f\u00fcrs Gem\u00fct;", "tokens": ["die", "zwei\u00b7te", "f\u00fcrs", "Ge\u00b7m\u00fct", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "die dritte wegen Hoppeldibopp \u2013", "tokens": ["die", "drit\u00b7te", "we\u00b7gen", "Hop\u00b7pel\u00b7di\u00b7bopp", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "NE", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "auf Nacht, wenns keiner sieht!", "tokens": ["auf", "Nacht", ",", "wenns", "kei\u00b7ner", "sieht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Freund, da\u00df du geheirat hast,", "tokens": ["Mein", "Freund", ",", "da\u00df", "du", "ge\u00b7hei\u00b7rat", "hast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das will mir gar nicht ein.", "tokens": ["das", "will", "mir", "gar", "nicht", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein St\u00fcck Malheur ist eine Last!", "tokens": ["Dein", "St\u00fcck", "Mal\u00b7heur", "ist", "ei\u00b7ne", "Last", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Komm, wirf sie in den Rhein!", "tokens": ["Komm", ",", "wirf", "sie", "in", "den", "Rhein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVIMP", "PPER", "APPR", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er sagt: \u00bbIch w\u00fcnscht, ich kann es!", "tokens": ["Er", "sagt", ":", "\u00bb", "Ich", "w\u00fcnscht", ",", "ich", "kann", "es", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wem sagst du das, Johannes!", "tokens": ["Wem", "sagst", "du", "das", ",", "Jo\u00b7han\u00b7nes", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PDS", "$,", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich denk so oft, wenn die Alte schnarcht: Ach, w\u00e4r ich jetzt allein!\u00ab", "tokens": ["Ich", "denk", "so", "oft", ",", "wenn", "die", "Al\u00b7te", "schnarcht", ":", "Ach", ",", "w\u00e4r", "ich", "jetzt", "al\u00b7lein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$.", "ITJ", "$,", "VAFIN", "PPER", "ADV", "ADV", "$.", "$("], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Ich h\u00e4tt", "tokens": ["Ich", "h\u00e4tt"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "in jeder Stadt 'ne Braut \u2013!", "tokens": ["in", "je\u00b7der", "Stadt", "'ne", "Braut", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Die erste f\u00fcr die Seele,", "tokens": ["Die", "ers\u00b7te", "f\u00fcr", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "die zweite f\u00fcrs Gem\u00fct;", "tokens": ["die", "zwei\u00b7te", "f\u00fcrs", "Ge\u00b7m\u00fct", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "die dritte f\u00fcr das Hoppeldibopp \u2013", "tokens": ["die", "drit\u00b7te", "f\u00fcr", "das", "Hop\u00b7pel\u00b7di\u00b7bopp", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "auf Nacht, wenns keiner sieht!", "tokens": ["auf", "Nacht", ",", "wenns", "kei\u00b7ner", "sieht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mensch, unser Gustav Stresemann", "tokens": ["Mensch", ",", "un\u00b7ser", "Gus\u00b7tav", "Stre\u00b7se\u00b7mann"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das ist wohl ein Filou!", "tokens": ["das", "ist", "wohl", "ein", "Fi\u00b7lou", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er meiert sich bei jedem an", "tokens": ["Er", "mei\u00b7ert", "sich", "bei", "je\u00b7dem", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIS", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und singt was Sch\u00f6ns dazu.", "tokens": ["und", "singt", "was", "Sch\u00f6ns", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er steht am Wasserglase", "tokens": ["Er", "steht", "am", "Was\u00b7ser\u00b7gla\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "und redet durch die Nase,", "tokens": ["und", "re\u00b7det", "durch", "die", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "mal rechts durchs Loch, mal links durchs Loch \u2013 der Junge ist atout!", "tokens": ["mal", "rechts", "durchs", "Loch", ",", "mal", "links", "durchs", "Loch", "\u2013", "der", "Jun\u00b7ge", "ist", "at\u00b7out", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,", "ADV", "ADV", "APPRART", "NN", "$(", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Der hat", "tokens": ["Der", "hat"], "token_info": ["word", "word"], "pos": ["PDS", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "in jeder Stadt 'ne Braut \u2013!", "tokens": ["in", "je\u00b7der", "Stadt", "'ne", "Braut", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Die erste f\u00fcr die Seele,", "tokens": ["Die", "ers\u00b7te", "f\u00fcr", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "die zweite f\u00fcrs Gem\u00fct;", "tokens": ["die", "zwei\u00b7te", "f\u00fcrs", "Ge\u00b7m\u00fct", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "die dritte f\u00fcr das Hoppeldibopp \u2013", "tokens": ["die", "drit\u00b7te", "f\u00fcr", "das", "Hop\u00b7pel\u00b7di\u00b7bopp", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "auf Nacht, wenns keiner sieht!", "tokens": ["auf", "Nacht", ",", "wenns", "kei\u00b7ner", "sieht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Das macht der Reiz seines Angesichts!", "tokens": ["Das", "macht", "der", "Reiz", "sei\u00b7nes", "An\u00b7ge\u00b7sichts", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Und die eine wei\u00df von der andern nichts,", "tokens": ["Und", "die", "ei\u00b7ne", "wei\u00df", "von", "der", "an\u00b7dern", "nichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "VVFIN", "APPR", "ART", "ADJA", "PIS", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "da\u00df er ihr Programm geklaut!", "tokens": ["da\u00df", "er", "ihr", "Pro\u00b7gramm", "ge\u00b7klaut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "In jeder Stadt", "tokens": ["In", "je\u00b7der", "Stadt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "in jeder Stadt", "tokens": ["in", "je\u00b7der", "Stadt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "in jeder Stadt 'ne Braut 