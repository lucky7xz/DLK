{"dta.poem.402": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als  Syris  weinete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Schatz hier sind der Perlen viel", "tokens": ["Mein", "Schatz", "hier", "sind", "der", "Per\u00b7len", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mehr als die Muschel fassen will.", "tokens": ["Mehr", "als", "die", "Mu\u00b7schel", "fas\u00b7sen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum h\u00f6re auf die Sch\u00e4tze zu verschwenden/", "tokens": ["Drum", "h\u00f6\u00b7re", "auf", "die", "Sch\u00e4t\u00b7ze", "zu", "ver\u00b7schwen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die mehr als allzukostbahr sind/", "tokens": ["Die", "mehr", "als", "all\u00b7zu\u00b7kost\u00b7bahr", "sind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "KOKOM", "ADV", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nur allein", "tokens": ["Und", "nur", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "In deinen Augen seyn.", "tokens": ["In", "dei\u00b7nen", "Au\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die keiner nicht an weit-entfernten Enden", "tokens": ["Die", "kei\u00b7ner", "nicht", "an", "weit\u00b7ent\u00b7fern\u00b7ten", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Was achte ich der Perlen Pracht", "tokens": ["Was", "ach\u00b7te", "ich", "der", "Per\u00b7len", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Morgenland gemacht/", "tokens": ["Die", "Mor\u00b7gen\u00b7land", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Auch die uns Teutschland zinst", "tokens": ["Auch", "die", "uns", "Teutschland", "zinst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "PPER", "NE", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Jm ", "tokens": ["Jm"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Drum h\u00f6r doch auf mein Schatz/", "tokens": ["Drum", "h\u00f6r", "doch", "auf", "mein", "Schatz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "La\u00df ", "tokens": ["La\u00df"], "token_info": ["word"], "pos": ["VVIMP"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Gib ", "tokens": ["Gib"], "token_info": ["word"], "pos": ["VVIMP"], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Aurora mu\u00df allein", "tokens": ["Au\u00b7ro\u00b7ra", "mu\u00df", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["NE", "VMFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Jm Thr\u00e4nen-Thau erscheinen.", "tokens": ["Jm", "Thr\u00e4\u00b7nen\u00b7\u00b7T\u00b7hau", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}