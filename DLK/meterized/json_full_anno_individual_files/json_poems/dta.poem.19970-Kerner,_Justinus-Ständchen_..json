{"dta.poem.19970": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "St\u00e4ndchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kam vor Liebchens Fensterlein,", "tokens": ["Ich", "kam", "vor", "Lieb\u00b7chens", "Fens\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Th\u00e4t viele Stunden stehen,", "tokens": ["Th\u00e4t", "vie\u00b7le", "Stun\u00b7den", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ob nicht im milden Abendschein", "tokens": ["Ob", "nicht", "im", "mil\u00b7den", "A\u00b7bend\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Liebe w\u00e4r' zu sehen.", "tokens": ["Die", "Lie\u00b7be", "w\u00e4r'", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was f\u00fchlt die\u00df Herz? So Lust als Weh,", "tokens": ["Was", "f\u00fchlt", "die\u00df", "Herz", "?", "So", "Lust", "als", "Weh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDS", "NN", "$.", "ADV", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie k\u00f6mmt! o s\u00fc\u00dfes Bangen!", "tokens": ["Sie", "k\u00f6mmt", "!", "o", "s\u00fc\u00b7\u00dfes", "Ban\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah wohl zitternd in die H\u00f6h, \u2014", "tokens": ["Ich", "sah", "wohl", "zit\u00b7ternd", "in", "die", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "APPR", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da kam der Mond gegangen.", "tokens": ["Da", "kam", "der", "Mond", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch jezt, doch jezt, was f\u00fchlt die\u00df Herz?", "tokens": ["Doch", "jezt", ",", "doch", "jezt", ",", "was", "f\u00fchlt", "die\u00df", "Herz", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "ADV", "$,", "PWS", "VVFIN", "PDS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gewi\u00df! sie ist nicht ferne!", "tokens": ["Ge\u00b7wi\u00df", "!", "sie", "ist", "nicht", "fer\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah wohl zitternd himmelw\u00e4rts \u2014", "tokens": ["Ich", "sah", "wohl", "zit\u00b7ternd", "him\u00b7mel\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "ADV", "$("], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Da stunden tausend Sterne.", "tokens": ["Da", "stun\u00b7den", "tau\u00b7send", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann dr\u00fcben an dem Fensterlein", "tokens": ["Dann", "dr\u00fc\u00b7ben", "an", "dem", "Fens\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich mir ihr Bildni\u00df zeigte;", "tokens": ["Sich", "mir", "ihr", "Bild\u00b7ni\u00df", "zeig\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es war des Himmels Wiederschein,", "tokens": ["Es", "war", "des", "Him\u00b7mels", "Wie\u00b7der\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was sich herunterneigte.", "tokens": ["Was", "sich", "her\u00b7un\u00b7ter\u00b7neig\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}