{"dta.poem.22376": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "179. Es sol ein Einigs werden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Ach ja! w\u00e4r\u2019 ich im Du/ und Du im ich ein Ein!", "tokens": ["Ach", "ja", "!", "w\u00e4r'", "ich", "im", "Du", "/", "und", "Du", "im", "ich", "ein", "Ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "VAFIN", "PPER", "APPRART", "PPER", "$(", "KON", "PPER", "APPRART", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So m\u00f6chte Tausendmahl der Himmel Himmel sein.", "tokens": ["So", "m\u00f6ch\u00b7te", "Tau\u00b7send\u00b7mahl", "der", "Him\u00b7mel", "Him\u00b7mel", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}