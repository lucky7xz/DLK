{"textgrid.poem.66267": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Das Bild der Religion", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sprich! wer bist du? \u2013 des gro\u00dfen Vaters", "tokens": ["O", "sprich", "!", "wer", "bist", "du", "?", "\u2013", "des", "gro\u00b7\u00dfen", "Va\u00b7ters"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NE", "ADJD", "$.", "PWS", "VAFIN", "PPER", "$.", "$(", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wahre Tochter, die Religion! \u2013", "tokens": ["Wah\u00b7re", "Toch\u00b7ter", ",", "die", "Re\u00b7li\u00b7gi\u00b7on", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "$.", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Warum ein zerrissenes Bettlergewand? \u2013", "tokens": ["Wa\u00b7rum", "ein", "zer\u00b7ris\u00b7se\u00b7nes", "Bett\u00b7ler\u00b7ge\u00b7wand", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$.", "$("], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Erde G\u00fcter veracht' ich! \u2013", "tokens": ["Der", "Er\u00b7de", "G\u00fc\u00b7ter", "ver\u00b7acht'", "ich", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und dieses Buch, das deine Blicke verschlingen?", "tokens": ["Und", "die\u00b7ses", "Buch", ",", "das", "dei\u00b7ne", "Bli\u00b7cke", "ver\u00b7schlin\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ist meines Vaters heiliges Gesetz! \u2013", "tokens": ["Ist", "mei\u00b7nes", "Va\u00b7ters", "hei\u00b7li\u00b7ges", "Ge\u00b7setz", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Warum den keuschen Busen unverh\u00fcllt? \u2013", "tokens": ["Wa\u00b7rum", "den", "keu\u00b7schen", "Bu\u00b7sen", "un\u00b7ver\u00b7h\u00fcllt", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der offnen Einfalt Freundin liebt es so! \u2013", "tokens": ["Der", "off\u00b7nen", "Ein\u00b7falt", "Freun\u00b7din", "liebt", "es", "so", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Warum auf ein Kreuz dich lehnend? \u2013 Das Kreuz", "tokens": ["Wa\u00b7rum", "auf", "ein", "Kreuz", "dich", "leh\u00b7nend", "?", "\u2013", "Das", "Kreuz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "VVPP", "$.", "$(", "ART", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Schafft mir die gew\u00fcnschteste Ruhe. \u2013", "tokens": ["Schafft", "mir", "die", "ge\u00b7w\u00fcnschtes\u00b7te", "Ru\u00b7he", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Warum gefl\u00fcgelt? \u2013 Die Kinder des Staubs", "tokens": ["Wa\u00b7rum", "ge\u00b7fl\u00fc\u00b7gelt", "?", "\u2013", "Die", "Kin\u00b7der", "des", "Staubs"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "$.", "$(", "ART", "NN", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.12": {"text": "Flug \u00fcber die Sterne zu lehren! \u2013", "tokens": ["Flug", "\u00fc\u00b7ber", "die", "Ster\u00b7ne", "zu", "leh\u00b7ren", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.13": {"text": "Warum so strahlend? \u2013 Die t\u00e4uschende Nacht", "tokens": ["Wa\u00b7rum", "so", "strah\u00b7lend", "?", "\u2013", "Die", "t\u00e4u\u00b7schen\u00b7de", "Nacht"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "$.", "$(", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Der Menschenseelen zu bannen! \u2013", "tokens": ["Der", "Men\u00b7schen\u00b7see\u00b7len", "zu", "ban\u00b7nen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Warum ein Zaum in deiner Linken? \u2013 Damit", "tokens": ["Wa\u00b7rum", "ein", "Zaum", "in", "dei\u00b7ner", "Lin\u00b7ken", "?", "\u2013", "Da\u00b7mit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "PPOSAT", "NN", "$.", "$(", "PAV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Des Herzens St\u00f6