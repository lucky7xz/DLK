{"textgrid.poem.35127": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Das heilige Land", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehst du die Berge kahl sich legen", "tokens": ["Siehst", "du", "die", "Ber\u00b7ge", "kahl", "sich", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fernhin, so weit das Auge reicht?", "tokens": ["Fern\u00b7hin", ",", "so", "weit", "das", "Au\u00b7ge", "reicht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Schreien ists um Thau und Regen,", "tokens": ["Ein", "Schrei\u00b7en", "ists", "um", "Thau", "und", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Gott, der Herr, erh\u00f6rts vielleicht.", "tokens": ["Und", "Gott", ",", "der", "Herr", ",", "er\u00b7h\u00f6rts", "viel\u00b7leicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So liegt vor seinem Angesichte", "tokens": ["So", "liegt", "vor", "sei\u00b7nem", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Orient in 