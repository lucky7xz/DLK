{"dta.poem.2955": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mir ist im M\u00fc\u00dfiggang ein Monat hingegangen,", "tokens": ["Mir", "ist", "im", "M\u00fc\u00b7\u00dfig\u00b7gang", "ein", "Mo\u00b7nat", "hin\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit neuer Arbeit sei ein neuer angefangen.", "tokens": ["Mit", "neu\u00b7er", "Ar\u00b7beit", "sei", "ein", "neu\u00b7er", "an\u00b7ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "September war ein Glanz an Himmel und Gefild;", "tokens": ["Sep\u00b7tem\u00b7ber", "war", "ein", "Glanz", "an", "Him\u00b7mel", "und", "Ge\u00b7fild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Oktober st\u00fcrme nun! dich macht die Arbeit mild.", "tokens": ["Ok\u00b7to\u00b7ber", "st\u00fcr\u00b7me", "nun", "!", "dich", "macht", "die", "Ar\u00b7beit", "mild", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}