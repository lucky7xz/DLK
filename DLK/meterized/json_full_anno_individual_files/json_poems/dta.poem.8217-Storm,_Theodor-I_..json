{"dta.poem.8217": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "I .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Liegt todt und \u00f6d' ein niedrig Haus;", "tokens": ["Liegt", "todt", "und", "\u00f6d'", "ein", "nied\u00b7rig", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Epheu steigt das Dach hinan,", "tokens": ["Der", "E\u00b7pheu", "steigt", "das", "Dach", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Waldv\u00f6glein stiegen ein und aus.", "tokens": ["Wald\u00b7v\u00f6\u00b7glein", "stie\u00b7gen", "ein", "und", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Und drin am blanken Eichentisch", "tokens": ["Und", "drin", "am", "blan\u00b7ken", "Ei\u00b7chen\u00b7tisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verzaubert schl\u00e4ft ein M\u00e4gdelein;", "tokens": ["Ver\u00b7zau\u00b7bert", "schl\u00e4ft", "ein", "M\u00e4g\u00b7del\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wangen bl\u00fchn ihr rosenfrisch,", "tokens": ["Die", "Wan\u00b7gen", "bl\u00fchn", "ihr", "ro\u00b7sen\u00b7frisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf den Locken wallt ihr der Sonnenschein.", "tokens": ["Auf", "den", "Lo\u00b7cken", "wallt", "ihr", "der", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Die B\u00e4ume rauschen im Waldesdicht,", "tokens": ["Die", "B\u00e4u\u00b7me", "rau\u00b7schen", "im", "Wal\u00b7des\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Eint\u00f6nig f\u00e4llt der Quelle Schaum;", "tokens": ["Ein\u00b7t\u00f6\u00b7nig", "f\u00e4llt", "der", "Quel\u00b7le", "Schaum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Es lullt sie ein, es l\u00e4\u00dft sie nicht,", "tokens": ["Es", "lullt", "sie", "ein", ",", "es", "l\u00e4\u00dft", "sie", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sinket tief von Traum zu Traum.", "tokens": ["Sie", "sin\u00b7ket", "tief", "von", "Traum", "zu", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur wenn im Arm die Zitter klingt,", "tokens": ["Nur", "wenn", "im", "Arm", "die", "Zit\u00b7ter", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da hell der Wind vor\u00fcberzieht,", "tokens": ["Da", "hell", "der", "Wind", "vor\u00b7\u00fc\u00b7berz\u00b7ieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn gar zu laut die Drossel singt,", "tokens": ["Wenn", "gar", "zu", "laut", "die", "Dros\u00b7sel", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKA", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zuckt manchesmal ihr Augenlied.", "tokens": ["Zuckt", "man\u00b7ches\u00b7mal", "ihr", "Au\u00b7gen\u00b7lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dann wirft sie das blonde K\u00f6pfchen herum,", "tokens": ["Dann", "wirft", "sie", "das", "blon\u00b7de", "K\u00f6pf\u00b7chen", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df am Hals das g\u00fcldene Kettlein klingt;", "tokens": ["Da\u00df", "am", "Hals", "das", "g\u00fcl\u00b7de\u00b7ne", "Ket\u00b7tlein", "klingt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auf fliegen die V\u00f6gel, der Wald ist stumm,", "tokens": ["Auf", "flie\u00b7gen", "die", "V\u00f6\u00b7gel", ",", "der", "Wald", "ist", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und zur\u00fcck in den Schlummer das M\u00e4gdlein sinkt.", "tokens": ["Und", "zu\u00b7r\u00fcck", "in", "den", "Schlum\u00b7mer", "das", "M\u00e4gd\u00b7lein", "sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}}}}