{"dta.poem.10124": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "GoTTES Gr\u00f6sse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch lange nicht ein Punct, und w\u00e4r' er noch so klein,", "tokens": ["Noch", "lan\u00b7ge", "nicht", "ein", "Punct", ",", "und", "w\u00e4r'", "er", "noch", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ART", "NN", "$,", "KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ja bildetest du dir von ihm ein Theilchen ein,", "tokens": ["Ja", "bil\u00b7de\u00b7test", "du", "dir", "von", "ihm", "ein", "Theil\u00b7chen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "PPER", "APPR", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das weder in den Sinn, noch in Gedaucken f\u00e4llt,", "tokens": ["Das", "we\u00b7der", "in", "den", "Sinn", ",", "noch", "in", "Ge\u00b7dau\u00b7cken", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KON", "APPR", "ART", "NN", "$,", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das auch ein Mathematicus", "tokens": ["Das", "auch", "ein", "Ma\u00b7the\u00b7ma\u00b7ti\u00b7cus"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr unzertheilbar halten mu\u00df;", "tokens": ["F\u00fcr", "un\u00b7zer\u00b7theil\u00b7bar", "hal\u00b7ten", "mu\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist, gegen GOTT, die gantze Welt.", "tokens": ["Ist", ",", "ge\u00b7gen", "GoTT", ",", "die", "gant\u00b7ze", "Welt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}