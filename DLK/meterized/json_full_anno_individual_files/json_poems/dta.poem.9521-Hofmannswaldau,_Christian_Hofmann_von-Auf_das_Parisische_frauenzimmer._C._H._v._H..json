{"dta.poem.9521": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf das Parisische frauenzimmer.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Paris verweigert mir fast einen ku\u00df zu schencken.", "tokens": ["Pa\u00b7ris", "ver\u00b7wei\u00b7gert", "mir", "fast", "ei\u00b7nen", "ku\u00df", "zu", "schen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein auge wil sich hier auf meine seite lencken/", "tokens": ["Kein", "au\u00b7ge", "wil", "sich", "hier", "auf", "mei\u00b7ne", "sei\u00b7te", "len\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das liebliche geschlecht/ so reich an flammen ist/", "tokens": ["Das", "lieb\u00b7li\u00b7che", "ge\u00b7schlecht", "/", "so", "reich", "an", "flam\u00b7men", "ist", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVPP", "$(", "ADV", "ADJD", "APPR", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat mich zu einem zweck des hasses ausserkist.", "tokens": ["Hat", "mich", "zu", "ei\u00b7nem", "zweck", "des", "has\u00b7ses", "aus\u00b7ser\u00b7kist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es denckt die sch\u00f6ne stadt/ da\u00df farbe/ haut und haare", "tokens": ["Es", "denckt", "die", "sch\u00f6\u00b7ne", "stadt", "/", "da\u00df", "far\u00b7be", "/", "haut", "und", "haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$(", "KOUS", "NN", "$(", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bey mir zu wenig sind zu handeln sch\u00f6ne waare/", "tokens": ["Bey", "mir", "zu", "we\u00b7nig", "sind", "zu", "han\u00b7deln", "sch\u00f6\u00b7ne", "waa\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKA", "PIS", "VAFIN", "PTKZU", "VVINF", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und zwingt/ da\u00df meine faust wirft diese worte hin:", "tokens": ["Und", "zwingt", "/", "da\u00df", "mei\u00b7ne", "faust", "wirft", "die\u00b7se", "wor\u00b7te", "hin", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "VVFIN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Paris verachtet mich/ weil ich nicht Paris bin.", "tokens": ["Pa\u00b7ris", "ver\u00b7ach\u00b7tet", "mich", "/", "weil", "ich", "nicht", "Pa\u00b7ris", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PTKNEG", "NE", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}