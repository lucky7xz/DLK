{"dta.poem.8453": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Hexenlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling siegt,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "siegt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und spendet uns Blumen zum Kranze!", "tokens": ["Und", "spen\u00b7det", "uns", "Blu\u00b7men", "zum", "Kran\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Bald huschen wir", "tokens": ["Bald", "hu\u00b7schen", "wir"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Leis' aus der Th\u00fcr,", "tokens": ["Leis'", "aus", "der", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und fliegen zum pr\u00e4chtigen Tanze!", "tokens": ["Und", "flie\u00b7gen", "zum", "pr\u00e4ch\u00b7ti\u00b7gen", "Tan\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Ein schwarzer Bock,", "tokens": ["Ein", "schwar\u00b7zer", "Bock", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ein Besenstock,", "tokens": ["Ein", "Be\u00b7sen\u00b7stock", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Ofengabel, der Wocken,", "tokens": ["Die", "O\u00b7fen\u00b7ga\u00b7bel", ",", "der", "Wo\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Reisst uns geschwind,", "tokens": ["Reisst", "uns", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wie Bliz und Wind,", "tokens": ["Wie", "Bliz", "und", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Durch sausende L\u00fcfte zum Brocken!", "tokens": ["Durch", "sau\u00b7sen\u00b7de", "L\u00fcf\u00b7te", "zum", "Bro\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Um Belzebub", "tokens": ["Um", "Bel\u00b7ze\u00b7bub"], "token_info": ["word", "word"], "pos": ["KOUI", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Tanzt unser Trupp,", "tokens": ["Tanzt", "un\u00b7ser", "Trupp", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und k\u00fcsst ihm die krallichten H\u00e4nde!", "tokens": ["Und", "k\u00fcsst", "ihm", "die", "kral\u00b7lich\u00b7ten", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ein Geisterschwarm", "tokens": ["Ein", "Geis\u00b7ter\u00b7schwarm"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Fasst uns beim Arm,", "tokens": ["Fasst", "uns", "beim", "Arm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und schwinget im Tanzen die Br\u00e4nde!", "tokens": ["Und", "schwin\u00b7get", "im", "Tan\u00b7zen", "die", "Br\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Und Belzebub", "tokens": ["Und", "Bel\u00b7ze\u00b7bub"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Verheisst dem Trupp", "tokens": ["Ver\u00b7heisst", "dem", "Trupp"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Tanzenden Gaben auf Gaben:", "tokens": ["Der", "Tan\u00b7zen\u00b7den", "Ga\u00b7ben", "auf", "Ga\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sie sollen sch\u00f6n", "tokens": ["Sie", "sol\u00b7len", "sch\u00f6n"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In Seide gehn,", "tokens": ["In", "Sei\u00b7de", "gehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und T\u00f6pfe voll Goldes sich graben!", "tokens": ["Und", "T\u00f6p\u00b7fe", "voll", "Gol\u00b7des", "sich", "gra\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "NN", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Ein Feuerdrach'", "tokens": ["Ein", "Feu\u00b7er\u00b7drach'"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Umfliegt das Dach,", "tokens": ["Um\u00b7fliegt", "das", "Dach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und bringet uns Butter und Eier!", "tokens": ["Und", "brin\u00b7get", "uns", "But\u00b7ter", "und", "Ei\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Nachbarn sehn", "tokens": ["Die", "Nach\u00b7barn", "sehn"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die Funken wehn,", "tokens": ["Die", "Fun\u00b7ken", "wehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und schlagen ein Kreuz vor dem Feuer!", "tokens": ["Und", "schla\u00b7gen", "ein", "Kreuz", "vor", "dem", "Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Die Schwalbe fliegt,", "tokens": ["Die", "Schwal\u00b7be", "fliegt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Fr\u00fchling siegt,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "siegt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und Blumen entbl\u00fchn um die Wette!", "tokens": ["Und", "Blu\u00b7men", "ent\u00b7bl\u00fchn", "um", "die", "Wet\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Bald huschen wir", "tokens": ["Bald", "hu\u00b7schen", "wir"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Leis' aus der Th\u00fcr,", "tokens": ["Leis'", "aus", "der", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und lassen die M\u00e4nner im Bette!", "tokens": ["Und", "las\u00b7sen", "die", "M\u00e4n\u00b7ner", "im", "Bet\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}