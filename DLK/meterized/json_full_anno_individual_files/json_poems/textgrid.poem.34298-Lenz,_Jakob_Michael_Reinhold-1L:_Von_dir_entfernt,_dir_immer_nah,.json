{"textgrid.poem.34298": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von dir entfernt, dir immer nah,", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von dir entfernt, dir immer nah,", "tokens": ["Von", "dir", "ent\u00b7fernt", ",", "dir", "im\u00b7mer", "nah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O du mein Leben, Seraphine,", "tokens": ["O", "du", "mein", "Le\u00b7ben", ",", "Se\u00b7ra\u00b7phi\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist das ein Traum, was mir geschah?", "tokens": ["Ist", "das", "ein", "Traum", ",", "was", "mir", "ge\u00b7schah", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich tr\u00f6stet, da\u00df ichs nicht verdiene.", "tokens": ["Mich", "tr\u00f6s\u00b7tet", ",", "da\u00df", "ichs", "nicht", "ver\u00b7die\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nein selbst dein Zorn versch\u00f6nert dich", "tokens": ["Nein", "selbst", "dein", "Zorn", "ver\u00b7sch\u00f6\u00b7nert", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ist das h\u00f6chste Gut f\u00fcr mich.", "tokens": ["Und", "ist", "das", "h\u00f6chs\u00b7te", "Gut", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In dieser Einsamkeit, des kurzen Lebens m\u00fcde", "tokens": ["In", "die\u00b7ser", "Ein\u00b7sam\u00b7keit", ",", "des", "kur\u00b7zen", "Le\u00b7bens", "m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das ich doch nicht verlieren kann,", "tokens": ["Das", "ich", "doch", "nicht", "ver\u00b7lie\u00b7ren", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da schenkst nur du, mein Gl\u00fcck! dem bangen Herzen Friede", "tokens": ["Da", "schenkst", "nur", "du", ",", "mein", "Gl\u00fcck", "!", "dem", "ban\u00b7gen", "Her\u00b7zen", "Frie\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$,", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das dich auf ewig liebgewann.", "tokens": ["Das", "dich", "auf", "e\u00b7wig", "lieb\u00b7ge\u00b7wann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wie, wer verbietet mirs? wer kann es mir verbieten?", "tokens": ["Wie", ",", "wer", "ver\u00b7bie\u00b7tet", "mirs", "?", "wer", "kann", "es", "mir", "ver\u00b7bie\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWS", "VVFIN", "NE", "$.", "PWS", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ist das ein Laster, G\u00f6tterbild!", "tokens": ["Ist", "das", "ein", "Las\u00b7ter", ",", "G\u00f6t\u00b7ter\u00b7bild", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Von dir ger\u00fchrt zu seyn? wer kann sein Herz beh\u00fcten", "tokens": ["Von", "dir", "ge\u00b7r\u00fchrt", "zu", "seyn", "?", "wer", "kann", "sein", "Herz", "be\u00b7h\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "PTKZU", "VAINF", "$.", "PWS", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn selbst der Himmel nicht solch eine Neigung schilt.", "tokens": ["Wenn", "selbst", "der", "Him\u00b7mel", "nicht", "solch", "ei\u00b7ne", "Nei\u00b7gung", "schilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKNEG", "PIAT", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nein G\u00f6ttliche! solch eine Lieb ist Pflicht,", "tokens": ["Nein", "G\u00f6tt\u00b7li\u00b7che", "!", "solch", "ei\u00b7ne", "Lieb", "ist", "Pflicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "$.", "PIAT", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "F\u00fcr die will ich mein Blut verstr\u00f6men,", "tokens": ["F\u00fcr", "die", "will", "ich", "mein", "Blut", "ver\u00b7str\u00f6\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VMFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "Man kann mir zwar das Leben nehmen,", "tokens": ["Man", "kann", "mir", "zwar", "das", "Le\u00b7ben", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Doch meine Liebe ewig nicht.", "tokens": ["Doch", "mei\u00b7ne", "Lie\u00b7be", "e\u00b7wig", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ich kenne dich nicht erst von heute,", "tokens": ["Ich", "ken\u00b7ne", "dich", "nicht", "erst", "von", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Ich kenne dich von jeder sch\u00f6nen Seite.", "tokens": ["Ich", "ken\u00b7ne", "dich", "von", "je\u00b7der", "sch\u00f6\u00b7nen", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Ich bete, denk ich noch daran,", "tokens": ["Ich", "be\u00b7te", ",", "denk", "ich", "noch", "da\u00b7ran", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Dank, Sehnsucht, Tr\u00e4nen in den Blicken", "tokens": ["Dank", ",", "Sehn\u00b7sucht", ",", "Tr\u00e4\u00b7nen", "in", "den", "Bli\u00b7cken"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Den, der dich schuf, mit heiligem Entz\u00fccken", "tokens": ["Den", ",", "der", "dich", "schuf", ",", "mit", "hei\u00b7li\u00b7gem", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und dich, sein sch\u00f6n Gesch\u00f6pfe, an.", "tokens": ["Und", "dich", ",", "sein", "sch\u00f6n", "Ge\u00b7sch\u00f6p\u00b7fe", ",", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "ADJD", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Ach wieviel Gl\u00fcck ist selbst in diesen Tr\u00e4nen,", "tokens": ["Ach", "wie\u00b7viel", "Gl\u00fcck", "ist", "selbst", "in", "die\u00b7sen", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "NN", "VAFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Nach wem kann sich mein Herz sonst sehnen", "tokens": ["Nach", "wem", "kann", "sich", "mein", "Herz", "sonst", "seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWS", "VMFIN", "PRF", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.27": {"text": "Als nur nach dir und stets nach dir", "tokens": ["Als", "nur", "nach", "dir", "und", "stets", "nach", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPER", "KON", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Und dies \u2013 nur dies \u2013 verbeut man mir?", "tokens": ["Und", "dies", "\u2013", "nur", "dies", "\u2013", "ver\u00b7beut", "man", "mir", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$(", "ADV", "PDS", "$(", "VVFIN", "PIS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Dis reine Feuer macht ein Bube, sich zu r\u00e4chen,", "tokens": ["Dis", "rei\u00b7ne", "Feu\u00b7er", "macht", "ein", "Bu\u00b7be", ",", "sich", "zu", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "VVFIN", "ART", "NN", "$,", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.30": {"text": "Mir zu dem schw\u00e4rzesten Verbrechen?", "tokens": ["Mir", "zu", "dem", "schw\u00e4r\u00b7zes\u00b7ten", "Ver\u00b7bre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.31": {"text": "Und du mit ihm? Du die Gerechtigkeit", "tokens": ["Und", "du", "mit", "ihm", "?", "Du", "die", "Ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPER", "$.", "PPER", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.32": {"text": "Die G\u00fcte selbst? War es Verwegenheit", "tokens": ["Die", "G\u00fc\u00b7te", "selbst", "?", "War", "es", "Ver\u00b7we\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$.", "VAFIN", "PPER", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.33": {"text": "Dich anzusehn? Gott! ist es eine S\u00fcnde", "tokens": ["Dich", "an\u00b7zu\u00b7sehn", "?", "Gott", "!", "ist", "es", "ei\u00b7ne", "S\u00fcn\u00b7de"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVIZU", "$.", "NN", "$.", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Wenn ich in dir den Himmel finde", "tokens": ["Wenn", "ich", "in", "dir", "den", "Him\u00b7mel", "fin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Mit aller seiner Seeligkeit?", "tokens": ["Mit", "al\u00b7ler", "sei\u00b7ner", "See\u00b7lig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Schiltst du ein Kind, das dir die H\u00e4nde k\u00fc\u00dft,", "tokens": ["Schiltst", "du", "ein", "Kind", ",", "das", "dir", "die", "H\u00e4n\u00b7de", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Daf\u00fcr, da\u00df du ihm freundlich bist?", "tokens": ["Da\u00b7f\u00fcr", ",", "da\u00df", "du", "ihm", "freund\u00b7lich", "bist", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "PPER", "ADJD", "VAFIN", 