{"dta.poem.7586": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts bessers kann der Mensch hienieden thun, als treten", "tokens": ["Nichts", "bes\u00b7sers", "kann", "der", "Mensch", "hien\u00b7ie\u00b7den", "thun", ",", "als", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "ADV", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,", "KOUS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus sich und aus der Welt und auf zum Himmel beten.", "tokens": ["Aus", "sich", "und", "aus", "der", "Welt", "und", "auf", "zum", "Him\u00b7mel", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "KON", "APPR", "ART", "NN", "KON", "APPR", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es sollen ein Gebet die Worte nicht allein,", "tokens": ["Es", "sol\u00b7len", "ein", "Ge\u00b7bet", "die", "Wor\u00b7te", "nicht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es sollen ein Gebet auch die Gedanken seyn.", "tokens": ["Es", "sol\u00b7len", "ein", "Ge\u00b7bet", "auch", "die", "Ge\u00b7dan\u00b7ken", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es sollen ein Gebet die Werke werden auch,", "tokens": ["Es", "sol\u00b7len", "ein", "Ge\u00b7bet", "die", "Wer\u00b7ke", "wer\u00b7den", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit das Leben rein aufgeh' in einen Hauch.", "tokens": ["Da\u00b7mit", "das", "Le\u00b7ben", "rein", "auf\u00b7geh'", "in", "ei\u00b7nen", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}