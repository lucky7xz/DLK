{"textgrid.poem.57251": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: O du, der Traube Sohn, der im Golde blinkt,", "genre": "verse", "period": "N.A.", "pub_year": 1753, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du, der Traube Sohn, der im Golde blinkt,", "tokens": ["O", "du", ",", "der", "Trau\u00b7be", "Sohn", ",", "der", "im", "Gol\u00b7de", "blinkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Freund, sonst Niemand, lad' in die K\u00fchlung ein.", "tokens": ["Den", "Freund", ",", "sonst", "Nie\u00b7mand", ",", "lad'", "in", "die", "K\u00fch\u00b7lung", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PIS", "$,", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wir drey sind unser werth, und jener", "tokens": ["Wir", "drey", "sind", "un\u00b7ser", "werth", ",", "und", "je\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "CARD", "VAFIN", "PPOSAT", "ADJD", "$,", "KON", "PDS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Deutscheren Zeit, da du, edler Alter,", "tokens": ["Deut\u00b7sche\u00b7ren", "Zeit", ",", "da", "du", ",", "ed\u00b7ler", "Al\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Noch ungekeltert, aber schon feuriger", "tokens": ["Noch", "un\u00b7ge\u00b7kel\u00b7tert", ",", "a\u00b7ber", "schon", "feu\u00b7ri\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADV", "ADJA"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem Rheine zuhingst, der dich mit auferzog,", "tokens": ["Dem", "Rhei\u00b7ne", "zu\u00b7hingst", ",", "der", "dich", "mit", "auf\u00b7er\u00b7zog", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "PRELS", "PRF", "APPR", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und deiner heissen Berge F\u00fcsse", "tokens": ["Und", "dei\u00b7ner", "heis\u00b7sen", "Ber\u00b7ge", "F\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sorgsam mit gr\u00fcnlicher Woge k\u00fchlte.", "tokens": ["Sorg\u00b7sam", "mit", "gr\u00fcn\u00b7li\u00b7cher", "Wo\u00b7ge", "k\u00fchl\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Jetzt, da dein R\u00fccken bald ein Jahrhundert tr\u00e4gt,", "tokens": ["Jetzt", ",", "da", "dein", "R\u00fc\u00b7cken", "bald", "ein", "Jahr\u00b7hun\u00b7dert", "tr\u00e4gt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verdienest du es, dass man den hohen Geist", "tokens": ["Ver\u00b7die\u00b7nest", "du", "es", ",", "dass", "man", "den", "ho\u00b7hen", "Geist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$,", "KOUS", "PIS", "ART", "ADJA", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "In dir verstehen lern', und Kato's", "tokens": ["In", "dir", "ver\u00b7ste\u00b7hen", "lern'", ",", "und", "Ka\u00b7to's"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "VVINF", "VVFIN", "$,", "KON", "NE"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ernstere Tugend von dir entgl\u00fche.", "tokens": ["Erns\u00b7te\u00b7re", "Tu\u00b7gend", "von", "dir", "ent\u00b7gl\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Der Schule Lehrer kennet des Thiers um ihn,", "tokens": ["Der", "Schu\u00b7le", "Leh\u00b7rer", "ken\u00b7net", "des", "Thiers", "um", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kennt aller Pflanzen Seele. Der Dichter weiss", "tokens": ["Kennt", "al\u00b7ler", "Pflan\u00b7zen", "See\u00b7le", ".", "Der", "Dich\u00b7ter", "weiss"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So viel nicht; aber seiner Rose", "tokens": ["So", "viel", "nicht", ";", "a\u00b7ber", "sei\u00b7ner", "Ro\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKNEG", "$.", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weibliche Seele, des Weines st\u00e4rkre,", "tokens": ["Weib\u00b7li\u00b7che", "See\u00b7le", ",", "des", "Wei\u00b7nes", "st\u00e4r\u00b7kre", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Den jene kr\u00e4nzt, der fl\u00f6tenden Nachtigall", "tokens": ["Den", "je\u00b7ne", "kr\u00e4nzt", ",", "der", "fl\u00f6\u00b7ten\u00b7den", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PDS", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erfindungsvolle Seele, die seinen Wein", "tokens": ["Er\u00b7fin\u00b7dungs\u00b7vol\u00b7le", "See\u00b7le", ",", "die", "sei\u00b7nen", "Wein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit ihm besingt, die kennt er besser,", "tokens": ["Mit", "ihm", "be\u00b7singt", ",", "die", "kennt", "er", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als der Erweis, der von Folgen triefet.", "tokens": ["Als", "der", "Er\u00b7weis", ",", "der", "von", "Fol\u00b7gen", "trie\u00b7fet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Rheinwein, von ihnen hast du die edelste,", "tokens": ["Rhein\u00b7wein", ",", "von", "ih\u00b7nen", "hast", "du", "die", "e\u00b7dels\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPER", "VAFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und bist es w\u00fcrdig, dass du des Deutschen Geist", "tokens": ["Und", "bist", "es", "w\u00fcr\u00b7dig", ",", "dass", "du", "des", "Deut\u00b7schen", "Geist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nachahmst! bist gl\u00fchend, nicht aufflammend,", "tokens": ["Nach\u00b7ahmst", "!", "bist", "gl\u00fc\u00b7hend", ",", "nicht", "auf\u00b7flam\u00b7mend", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "VAFIN", "ADJD", "$,", "PTKNEG", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Taumellos, stark, und von leichtem Schaum leer.", "tokens": ["Tau\u00b7mel\u00b7los", ",", "stark", ",", "und", "von", "leich\u00b7tem", "Schaum", "leer", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "KON", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Du duftest Balsam, wie mit der Abendluft", "tokens": ["Du", "duf\u00b7test", "Bal\u00b7sam", ",", "wie", "mit", "der", "A\u00b7ben\u00b7dluft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der W\u00fcrze Blume von dem Gestade dampft,", "tokens": ["Der", "W\u00fcr\u00b7ze", "Blu\u00b7me", "von", "dem", "Ge\u00b7sta\u00b7de", "dampft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dass selbst der Kr\u00e4mer die Ger\u00fcche", "tokens": ["Dass", "selbst", "der", "Kr\u00e4\u00b7mer", "die", "Ge\u00b7r\u00fc\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Athmender trinkt, und nur gleitend fortschift.", "tokens": ["Ath\u00b7men\u00b7der", "trinkt", ",", "und", "nur", "glei\u00b7tend", "fort\u00b7schift", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KON", "ADV", "ADJD", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.8": {"line.1": {"text": "Freund, lass die Hall' uns schliessen; der Lebensduft", "tokens": ["Freund", ",", "lass", "die", "Hall'", "uns", "schlies\u00b7sen", ";", "der", "Le\u00b7bens\u00b7duft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "PPER", "VVINF", "$.", "ART", "NN"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verstr\u00f6met sonst, und etwa ein kluger Mann", "tokens": ["Ver\u00b7str\u00f6\u00b7met", "sonst", ",", "und", "et\u00b7wa", "ein", "klu\u00b7ger", "Mann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "M\u00f6cht' uns besuchen, breit sich setzen,", "tokens": ["M\u00f6cht'", "uns", "be\u00b7su\u00b7chen", ",", "breit", "sich", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und von der Weisheit wohl gar mit sprechen.", "tokens": ["Und", "von", "der", "Weis\u00b7heit", "wohl", "gar", "mit", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADV", "APPR", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Nun sind wir sicher. Engere Wissenschaft,", "tokens": ["Nun", "sind", "wir", "si\u00b7cher", ".", "En\u00b7ge\u00b7re", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den hellen Einfall, lehr uns des Alten Geist!", "tokens": ["Den", "hel\u00b7len", "Ein\u00b7fall", ",", "lehr", "uns", "des", "Al\u00b7ten", "Geist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Sorgen soll er nicht vertreiben!", "tokens": ["Die", "Sor\u00b7gen", "soll", "er", "nicht", "ver\u00b7trei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hast du geweinte, geliebte Sorgen,", "tokens": ["Hast", "du", "ge\u00b7wein\u00b7te", ",", "ge\u00b7lieb\u00b7te", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Lass mich mit dir sie sorgen. Ich weine mit,", "tokens": ["Lass", "mich", "mit", "dir", "sie", "sor\u00b7gen", ".", "Ich", "wei\u00b7ne", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "PPER", "NN", "$.", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+----+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wenn dir ein Freund starb. Nenn ihn. So starb er mir!", "tokens": ["Wenn", "dir", "ein", "Freund", "starb", ".", "Nenn", "ihn", ".", "So", "starb", "er", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$.", "VVFIN", "PPER", "$.", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das sprach er noch! nun kam das letzte,", "tokens": ["Das", "sprach", "er", "noch", "!", "nun", "kam", "das", "letz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$.", "ADV", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Letzte Verstummen! nun lag er todt da!", "tokens": ["Letz\u00b7te", "Ver\u00b7stum\u00b7men", "!", "nun", "lag", "er", "todt", "da", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.11": {"line.1": {"text": "Von allem Kummer, welcher des Sterblichen", "tokens": ["Von", "al\u00b7lem", "Kum\u00b7mer", ",", "wel\u00b7cher", "des", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIS", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kurzsichtig Leben nervenlos niederwirft,", "tokens": ["Kurz\u00b7sich\u00b7tig", "Le\u00b7ben", "ner\u00b7ven\u00b7los", "nie\u00b7der\u00b7wirft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADJD", "VVFIN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "W\u00e4rst du, des Freundes Tod! der tr\u00fcbste;", "tokens": ["W\u00e4rst", "du", ",", "des", "Freun\u00b7des", "Tod", "!", "der", "tr\u00fcbs\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ART", "NN", "NN", "$.", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00e4r sie nicht auch die Geliebte sterblich!", "tokens": ["W\u00e4r", "sie", "nicht", "auch", "die", "Ge\u00b7lieb\u00b7te", "sterb\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Doch wenn dich, J\u00fcngling, andere Sorg entflamt,", "tokens": ["Doch", "wenn", "dich", ",", "J\u00fcng\u00b7ling", ",", "an\u00b7de\u00b7re", "Sorg", "ent\u00b7flamt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "NN", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und dirs zu heiss wird, dass du der Barden Gang", "tokens": ["Und", "dirs", "zu", "heiss", "wird", ",", "dass", "du", "der", "Bar\u00b7den", "Gang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "APPR", "NE", "VAFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Haine noch nicht gingst, dein Name", "tokens": ["Im", "Hai\u00b7ne", "noch", "nicht", "gingst", ",", "dein", "Na\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "ADV", "PTKNEG", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Noch unerh\u00f6ht mit der grossen Fluth fleusst;", "tokens": ["Noch", "un\u00b7er\u00b7h\u00f6ht", "mit", "der", "gros\u00b7sen", "Fluth", "fleusst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "So red'! In Weisheit wandelt sich Ehrbegier,", "tokens": ["So", "red'", "!", "In", "Weis\u00b7heit", "wan\u00b7delt", "sich", "Ehr\u00b7be\u00b7gier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "APPR", "NN", "VVFIN", "PRF", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "W\u00e4hlt jene. Thorheit ist es, ein kleines Ziel", "tokens": ["W\u00e4hlt", "je\u00b7ne", ".", "Thor\u00b7heit", "ist", "es", ",", "ein", "klei\u00b7nes", "Ziel"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PDS", "$.", "NN", "VAFIN", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das w\u00fcrdigen, zum Ziel zu machen,", "tokens": ["Das", "w\u00fcr\u00b7di\u00b7gen", ",", "zum", "Ziel", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach der unsterblichen Schelle laufen!", "tokens": ["Nach", "der", "uns\u00b7terb\u00b7li\u00b7chen", "Schel\u00b7le", "lau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.14": {"line.1": {"text": "Noch viel Verdienst ist \u00fcbrig. Auf, hab es nur;", "tokens": ["Noch", "viel", "Ver\u00b7dienst", "ist", "\u00fcb\u00b7rig", ".", "Auf", ",", "hab", "es", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "ADJD", "$.", "APPR", "$,", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Welt wirds kennen. Aber das edelste", "tokens": ["Die", "Welt", "wirds", "ken\u00b7nen", ".", "A\u00b7ber", "das", "e\u00b7dels\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$.", "KON", "ART", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ist Tugend! Meisterwerke werden", "tokens": ["Ist", "Tu\u00b7gend", "!", "Meis\u00b7ter\u00b7wer\u00b7ke", "wer\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VAFIN", "NN", "$.", "NN", "VAINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sicher unsterblich; die Tugend selten!", "tokens": ["Si\u00b7cher", "uns\u00b7terb\u00b7lich", ";", "die", "Tu\u00b7gend", "sel\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "ART", "NN", "ADJD", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.15": {"line.1": {"text": "Allein sie soll auch Lohn der Unsterblichkeit", "tokens": ["Al\u00b7lein", "sie", "soll", "auch", "Lohn", "der", "U\u00b7nsterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VMFIN", "ADV", "NN", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Entbehren k\u00f6nnen. Athme nun auf, und trink.", "tokens": ["Ent\u00b7beh\u00b7ren", "k\u00f6n\u00b7nen", ".", "Ath\u00b7me", "nun", "auf", ",", "und", "trink", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVINF", "VMINF", "$.", "NN", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wir reden viel noch, eh des Aufgangs", "tokens": ["Wir", "re\u00b7den", "viel", "noch", ",", "eh", "des", "Auf\u00b7gangs"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00fchlungen wehen, von grosseu M\u00e4nnern.", "tokens": ["K\u00fch\u00b7lun\u00b7gen", "we\u00b7hen", ",", "von", "gros\u00b7seu", "M\u00e4n\u00b7nern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.16": {"line.1": {"text": "O du, der Traube Sohn, der im Golde blinkt,", "tokens": ["O", "du", ",", "der", "Trau\u00b7be", "Sohn", ",", "der", "im", "Gol\u00b7de", "blinkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Freund, sonst Niemand, lad' in die K\u00fchlung ein.", "tokens": ["Den", "Freund", ",", "sonst", "Nie\u00b7mand", ",", "lad'", "in", "die", "K\u00fch\u00b7lung", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PIS", "$,", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wir drey sind unser werth, und jener", "tokens": ["Wir", "drey", "sind", "un\u00b7ser", "werth", ",", "und", "je\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "CARD", "VAFIN", "PPOSAT", "ADJD", "$,", "KON", "PDS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Deutscheren Zeit, da du, edler Alter,", "tokens": ["Deut\u00b7sche\u00b7ren", "Zeit", ",", "da", "du", ",", "ed\u00b7ler", "Al\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.17": {"line.1": {"text": "Noch ungekeltert, aber schon feuriger", "tokens": ["Noch", "un\u00b7ge\u00b7kel\u00b7tert", ",", "a\u00b7ber", "schon", "feu\u00b7ri\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADV", "ADJA"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem Rheine zuhingst, der dich mit auferzog,", "tokens": ["Dem", "Rhei\u00b7ne", "zu\u00b7hingst", ",", "der", "dich", "mit", "auf\u00b7er\u00b7zog", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "PRELS", "PRF", "APPR", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und deiner heissen Berge F\u00fcsse", "tokens": ["Und", "dei\u00b7ner", "heis\u00b7sen", "Ber\u00b7ge", "F\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sorgsam mit gr\u00fcnlicher Woge k\u00fchlte.", "tokens": ["Sorg\u00b7sam", "mit", "gr\u00fcn\u00b7li\u00b7cher", "Wo\u00b7ge", "k\u00fchl\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.18": {"line.1": {"text": "Jetzt, da dein R\u00fccken bald ein Jahrhundert tr\u00e4gt,", "tokens": ["Jetzt", ",", "da", "dein", "R\u00fc\u00b7cken", "bald", "ein", "Jahr\u00b7hun\u00b7dert", "tr\u00e4gt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verdienest du es, dass man den hohen Geist", "tokens": ["Ver\u00b7die\u00b7nest", "du", "es", ",", "dass", "man", "den", "ho\u00b7hen", "Geist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$,", "KOUS", "PIS", "ART", "ADJA", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "In dir verstehen lern', und Kato's", "tokens": ["In", "dir", "ver\u00b7ste\u00b7hen", "lern'", ",", "und", "Ka\u00b7to's"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "VVINF", "VVFIN", "$,", "KON", "NE"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ernstere Tugend von dir entgl\u00fche.", "tokens": ["Erns\u00b7te\u00b7re", "Tu\u00b7gend", "von", "dir", "ent\u00b7gl\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.19": {"line.1": {"text": "Der Schule Lehrer kennet des Thiers um ihn,", "tokens": ["Der", "Schu\u00b7le", "Leh\u00b7rer", "ken\u00b7net", "des", "Thiers", "um", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kennt aller Pflanzen Seele. Der Dichter weiss", "tokens": ["Kennt", "al\u00b7ler", "Pflan\u00b7zen", "See\u00b7le", ".", "Der", "Dich\u00b7ter", "weiss"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So viel nicht; aber seiner Rose", "tokens": ["So", "viel", "nicht", ";", "a\u00b7ber", "sei\u00b7ner", "Ro\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKNEG", "$.", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weibliche Seele, des Weines st\u00e4rkre,", "tokens": ["Weib\u00b7li\u00b7che", "See\u00b7le", ",", "des", "Wei\u00b7nes", "st\u00e4r\u00b7kre", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.20": {"line.1": {"text": "Den jene kr\u00e4nzt, der fl\u00f6tenden Nachtigall", "tokens": ["Den", "je\u00b7ne", "kr\u00e4nzt", ",", "der", "fl\u00f6\u00b7ten\u00b7den", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PDS", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erfindungsvolle Seele, die seinen Wein", "tokens": ["Er\u00b7fin\u00b7dungs\u00b7vol\u00b7le", "See\u00b7le", ",", "die", "sei\u00b7nen", "Wein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit ihm besingt, die kennt er besser,", "tokens": ["Mit", "ihm", "be\u00b7singt", ",", "die", "kennt", "er", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als der Erweis, der von Folgen triefet.", "tokens": ["Als", "der", "Er\u00b7weis", ",", "der", "von", "Fol\u00b7gen", "trie\u00b7fet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.21": {"line.1": {"text": "Rheinwein, von ihnen hast du die edelste,", "tokens": ["Rhein\u00b7wein", ",", "von", "ih\u00b7nen", "hast", "du", "die", "e\u00b7dels\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPER", "VAFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und bist es w\u00fcrdig, dass du des Deutschen Geist", "tokens": ["Und", "bist", "es", "w\u00fcr\u00b7dig", ",", "dass", "du", "des", "Deut\u00b7schen", "Geist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nachahmst! bist gl\u00fchend, nicht aufflammend,", "tokens": ["Nach\u00b7ahmst", "!", "bist", "gl\u00fc\u00b7hend", ",", "nicht", "auf\u00b7flam\u00b7mend", ","], "token_info": ["word", "punct", "word", "word", "punct