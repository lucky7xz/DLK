te", "tokens": ["Die", "Welt", "wirds", "ken\u00b7nen", ".", "A\u00b7ber", "das", "e\u00b7dels\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$.", "KON", "ART", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ist Tugend! Meisterwerke werden", "tokens": ["Ist", "Tu\u00b7gend", "!", "Meis\u00b7ter\u00b7wer\u00b7ke", "wer\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VAFIN", "NN", "$.", "NN", "VAINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sicher unsterblich; die Tugend selten!", "tokens": ["Si\u00b7cher", "uns\u00b7terb\u00b7lich", ";", "die", "Tu\u00b7gend", "sel\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "ART", "NN", "ADJD", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.30": {"line.1": {"text": "Allein sie soll auch Lohn der Unsterblichkeit", "tokens": ["Al\u00b7lein", "sie", "soll", "auch", "Lohn", "der", "U\u00b7nsterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VMFIN", "ADV", "NN", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Entbehren k\u00f6nnen. Athme nun auf, und trink.", "tokens": ["Ent\u00b7beh\u00b7ren", "k\u00f6n\u00b7nen", ".", "Ath\u00b7me", "nun", "auf", ",", "und", "trink", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVINF", "VMINF", "$.", "NN", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wir reden viel noch, eh des Aufgangs", "tokens": ["Wir", "re\u00b7den", "viel", "noch", ",", "eh", "des", "Auf\u00b7gangs"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": 