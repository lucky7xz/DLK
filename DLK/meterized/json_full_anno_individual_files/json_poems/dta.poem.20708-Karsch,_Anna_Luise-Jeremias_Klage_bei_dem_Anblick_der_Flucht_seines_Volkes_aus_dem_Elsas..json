{"dta.poem.20708": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Jeremias Klage  \n bei  \n dem Anblick der Flucht seines Volkes  \n aus dem Elsas.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jeremias, der vor Zeiten                 ", "tokens": ["Je\u00b7re\u00b7mi\u00b7as", ",", "der", "vor", "Zei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Chald\u00e4er Kriegesgrausamkeiten", "tokens": ["Der", "Chal\u00b7d\u00e4\u00b7er", "Krie\u00b7ges\u00b7grau\u00b7sam\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und den Jammer Zions sang \u2014", "tokens": ["Und", "den", "Jam\u00b7mer", "Zi\u00b7ons", "sang"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeremias in dem Himmel", "tokens": ["Je\u00b7re\u00b7mi\u00b7as", "in", "dem", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mischte Klagen unterm Harfenklang,", "tokens": ["Mischte", "Kla\u00b7gen", "un\u00b7term", "Har\u00b7fen\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da vom Elsa\u00df ein Get\u00fcmmel", "tokens": ["Da", "vom", "El\u00b7sa\u00df", "ein", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ein Angstgeschrei empor", "tokens": ["Und", "ein", "Angst\u00b7ge\u00b7schrei", "em\u00b7por"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Drang bis in das Seraphchor,", "tokens": ["Drang", "bis", "in", "das", "Se\u00b7raph\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Da des Aufruhrs f\u00fcrchterliche Rotte", "tokens": ["Da", "des", "Auf\u00b7ruhrs", "f\u00fcrch\u00b7ter\u00b7li\u00b7che", "Rot\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Seinem Volke schrecklich war.", "tokens": ["Sei\u00b7nem", "Vol\u00b7ke", "schreck\u00b7lich", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Nackend, unter bittrem Spotte", "tokens": ["Na\u00b7ckend", ",", "un\u00b7ter", "bitt\u00b7rem", "Spot\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Trieb die wuthbeflammte Schaar", "tokens": ["Trieb", "die", "wuth\u00b7be\u00b7flamm\u00b7te", "Schaar"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Das beraubte Volk von hinnen,", "tokens": ["Das", "be\u00b7raub\u00b7te", "Volk", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und es konnte durch die Flucht", "tokens": ["Und", "es", "konn\u00b7te", "durch", "die", "Flucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Lebensrettung kaum gewinnen", "tokens": ["Le\u00b7bens\u00b7ret\u00b7tung", "kaum", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "F\u00fcr den Grimm der R\u00e4ubersucht.", "tokens": ["F\u00fcr", "den", "Grimm", "der", "R\u00e4u\u00b7ber\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201egott, du bist die Liebe selber,", "tokens": ["\u201e", "gott", ",", "du", "bist", "die", "Lie\u00b7be", "sel\u00b7ber", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PPER", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rief der Klages\u00e4nger aus", "tokens": ["Rief", "der", "Kla\u00b7ge\u00b7s\u00e4n\u00b7ger", "aus"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201ego\u00df dies Volk sich goldne K\u00e4lber?", "tokens": ["\u201e", "go\u00df", "dies", "Volk", "sich", "gold\u00b7ne", "K\u00e4l\u00b7ber", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PDS", "NN", "PRF", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201elief es in ein G\u00f6tzenhaus?", "tokens": ["\u201e", "lief", "es", "in", "ein", "G\u00f6t\u00b7zen\u00b7haus", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u201etrat es dein Gesetz mit F\u00fc\u00dfen?", "tokens": ["\u201e", "trat", "es", "dein", "Ge\u00b7setz", "mit", "F\u00fc\u00b7\u00dfen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u201ebaute sichs Alt\u00e4re dort,", "tokens": ["\u201e", "bau\u00b7te", "sichs", "Al\u00b7t\u00e4\u00b7re", "dort", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "CARD", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u201eein Trankopfer auszugie\u00dfen?", "tokens": ["\u201e", "ein", "Tran\u00b7kop\u00b7fer", "aus\u00b7zu\u00b7gie\u00b7\u00dfen", "?"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVIZU", "$."], "meter": "+++-+-+-", "measure": "unknown.measure.penta"}, "line.8": {"text": "\u201esprachs zu Holz und Stein: Du Hort", "tokens": ["\u201e", "sprachs", "zu", "Holz", "und", "Stein", ":", "Du", "Hort"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "APPR", "NN", "KON", "NN", "$.", "PPER", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "\u201emeines Heils! Hilf mir, und neige", "tokens": ["\u201e", "mei\u00b7nes", "Heils", "!", "Hilf", "mir", ",", "und", "nei\u00b7ge"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "$.", "NE", "PPER", "$,", "KON", "ADJA"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "\u201edoch dein Ohr zu meinem Flehn!\u201e", "tokens": ["\u201e", "doch", "dein", "Ohr", "zu", "mei\u00b7nem", "Flehn", "!", "\u201e"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "\u201egott, warst du des Frevels Zeuge?", "tokens": ["\u201e", "gott", ",", "warst", "du", "des", "Fre\u00b7vels", "Zeu\u00b7ge", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "VAFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "\u201ehat dein Auge dies gesehn?", "tokens": ["\u201e", "hat", "dein", "Au\u00b7ge", "dies", "ge\u00b7sehn", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPOSAT", "NN", "PDS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "\u201eund hast du im Zorn befohlen,", "tokens": ["\u201e", "und", "hast", "du", "im", "Zorn", "be\u00b7foh\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "\u201eda\u00df wie ein geschenchtes Reh,", "tokens": ["\u201e", "da\u00df", "wie", "ein", "ge\u00b7schench\u00b7tes", "Reh", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "\u201eohne Athemholen", "tokens": ["\u201e", "oh\u00b7ne", "A\u00b7them\u00b7ho\u00b7len"], "token_info": ["punct", "word", "word"], "pos": ["$(", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "\u201edieses Volk mit Ach und Weh", "tokens": ["\u201e", "die\u00b7ses", "Volk", "mit", "Ach", "und", "Weh"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PDAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "\u201efliehen soll an der Welt Ende? \u2014", "tokens": ["\u201e", "flie\u00b7hen", "soll", "an", "der", "Welt", "En\u00b7de", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVINF", "VMFIN", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.18": {"text": "\u201egott, Erbarmer! siehe drein;", "tokens": ["\u201e", "gott", ",", "Er\u00b7bar\u00b7mer", "!", "sie\u00b7he", "drein", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "$.", "VVIMP", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "\u201emache, da\u00df der Zorn sich wende!", "tokens": ["\u201e", "ma\u00b7che", ",", "da\u00df", "der", "Zorn", "sich", "wen\u00b7de", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "KOUS", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "\u201ela\u00df wie vormals m\u00e4chtig seyn", "tokens": ["\u201e", "la\u00df", "wie", "vor\u00b7mals", "m\u00e4ch\u00b7tig", "seyn"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "KOKOM", "ADV", "ADJD", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "\u201edeine Liebe, dein Erbarmen!", "tokens": ["\u201e", "dei\u00b7ne", "Lie\u00b7be", ",", "dein", "Er\u00b7bar\u00b7men", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "\u201esiehe, siehe da, die Armen,", "tokens": ["\u201e", "sie\u00b7he", ",", "sie\u00b7he", "da", ",", "die", "Ar\u00b7men", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "VVIMP", "ADV", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "\u201eweib und Greis und Kinder fliehn", "tokens": ["\u201e", "weib", "und", "Greis", "und", "Kin\u00b7der", "fliehn"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "KON", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "\u201evoller Todesfurcht und Schrecken:", "tokens": ["\u201e", "vol\u00b7ler", "To\u00b7des\u00b7furcht", "und", "Schre\u00b7cken", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "\u201ewer, o wer wird sie bedecken?", "tokens": ["\u201e", "wer", ",", "o", "wer", "wird", "sie", "be\u00b7de\u00b7cken", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "$,", "FM", "PWS", "VAFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "\u201ewelche H\u00fclfshand wird sie ziehn", "tokens": ["\u201e", "wel\u00b7che", "H\u00fclfs\u00b7hand", "wird", "sie", "ziehn"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAT", "NN", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "\u201evon dem Rande des Verderbens?", "tokens": ["\u201e", "von", "dem", "Ran\u00b7de", "des", "Ver\u00b7der\u00b7bens", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "\u201ewelches Zoar nimmt sie auf", "tokens": ["\u201e", "wel\u00b7ches", "Zoar", "nimmt", "sie", "auf"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAT", "NN", "VVFIN", "PPER", "APPR"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.29": {"text": "\u201ein der Angst des Hungersterbens?", "tokens": ["\u201e", "in", "der", "Angst", "des", "Hun\u00b7gers\u00b7ter\u00b7bens", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "\u201eund wo endet sich ihr Lauf? \u2014", "tokens": ["\u201e", "und", "wo", "en\u00b7det", "sich", "ihr", "Lauf", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PWAV", "VVFIN", "PRF", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "So sprach der Prophet, und sehet", "tokens": ["So", "sprach", "der", "Pro\u00b7phet", ",", "und", "se\u00b7het"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Erbarmung w\u00fcrkte schon", "tokens": ["Die", "Er\u00b7bar\u00b7mung", "w\u00fcrk\u00b7te", "schon"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4chtig eh er ausgeflehet.", "tokens": ["M\u00e4ch\u00b7tig", "eh", "er", "aus\u00b7ge\u00b7fle\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zitternd war das Volk entflohn,", "tokens": ["Zit\u00b7ternd", "war", "das", "Volk", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bebend kam es an die Thore", "tokens": ["Be\u00b7bend", "kam", "es", "an", "die", "Tho\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Basels, einer Schweizerstadt,                 ", "tokens": ["Ba\u00b7sels", ",", "ei\u00b7ner", "Schwei\u00b7zer\u00b7stadt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die sonst mit verschlo\u00dfnem Ohre", "tokens": ["Die", "sonst", "mit", "ver\u00b7schlo\u00df\u00b7nem", "Oh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sich hinweggewendet hat,", "tokens": ["Sich", "hin\u00b7weg\u00b7ge\u00b7wen\u00b7det", "hat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn sich ein Israelite", "tokens": ["Wenn", "sich", "ein", "Is\u00b7ra\u00b7e\u00b7li\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nur von fernher merken lie\u00df,", "tokens": ["Nur", "von", "fern\u00b7her", "mer\u00b7ken", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Da\u00df er dort um Eingang bi