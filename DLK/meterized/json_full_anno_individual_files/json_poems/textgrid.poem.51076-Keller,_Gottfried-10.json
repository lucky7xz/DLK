{"textgrid.poem.51076": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "10", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, h\u00e4tt ich ein verla\u00dfnes Liebchen nun,", "tokens": ["Ja", ",", "h\u00e4tt", "ich", "ein", "ver\u00b7la\u00df\u00b7nes", "Lieb\u00b7chen", "nun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VAFIN", "PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das vor dem Morgenrot zu klagen k\u00e4me,", "tokens": ["Das", "vor", "dem", "Mor\u00b7gen\u00b7rot", "zu", "kla\u00b7gen", "k\u00e4\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf meinem frischen Pf\u00fchle auszuruhn,", "tokens": ["Auf", "mei\u00b7nem", "fri\u00b7schen", "Pf\u00fch\u00b7le", "aus\u00b7zu\u00b7ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und meinen Ruf mit s\u00fc\u00dfem Graun vern\u00e4hme!", "tokens": ["Und", "mei\u00b7nen", "Ruf", "mit", "s\u00fc\u00b7\u00dfem", "Graun", "ver\u00b7n\u00e4h\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Warum hab ich der ", "tokens": ["Wa\u00b7rum", "hab", "ich", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ART"], "meter": "+-+-+", 