": ["Mag", "sie", "es", "\u00fc\u00b7ber\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mag ich vergessen sein!", "tokens": ["Mag", "ich", "ver\u00b7ges\u00b7sen", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dringt, was ich f\u00fcr sie flehe,", "tokens": ["Dringt", ",", "was", "ich", "f\u00fcr", "sie", "fle\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Nur zu der ew'gen H\u00f6he,", "tokens": ["Nur", "zu", "der", "ew'\u00b7gen", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nur in den treuen Himmel ein.", "tokens": ["Nur", "in", "den", "treu\u00b7en", "Him\u00b7mel", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "La\u00df ihr die Wangen bl\u00fchen,", "tokens": ["La\u00df", "ihr", "die", "Wan\u00b7gen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die dunkeln Augen gl\u00fchen,", "tokens": ["Die", "dun\u00b7keln", "Au\u00b7gen", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O du der Liebe Hort!", "tokens": ["O", "du", "der", "Lie\u00b7be", "Hort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch wen sie soll entz\u00fcnden", "tokens": ["Doch", "wen", "sie", "soll", "ent\u00b7z\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In seines Herzens Gr\u00fcnden,", "tokens": ["In", "sei\u00b7nes", "Her\u00b7zens", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem schenk' auch ihre Huld sofort.", "tokens": ["Dem", "schenk'", "auch", "ih\u00b7re", "Huld", "so\u00b7fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "F\u00fcr mich nicht will ich bitten,", "tokens": ["F\u00fcr", "mich", "nicht", "will", "ich", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKNEG", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich habe mir erstritten", "tokens": ["Ich", "ha\u00b7be", "mir", "er\u00b7strit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein fest und ruhig Herz!", "tokens": ["Ein", "fest", "und", "ru\u00b7hig", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nur, kann sie nichts erwiedern,", "tokens": ["Nur", ",", "kann", "sie", "nichts", "er\u00b7wie\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So spar' auch andern Br\u00fcdern", "tokens": ["So", "spa\u00b7r'", "auch", "an\u00b7dern", "Br\u00fc\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Unangeh\u00f6rter Liebe Schmerz.", "tokens": ["Un\u00b7an\u00b7ge\u00b7h\u00f6r\u00b7ter", "Lie\u00b7be", "Schmerz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Doch willst du Lieb' und Leben", "tokens": ["Doch", "willst", "du", "Lieb'", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem keuschen Busen geben,", "tokens": ["Dem", "keu\u00b7schen", "Bu\u00b7sen", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So thu's in diesem Jahr;", "tokens": ["So", "thu's", "in", "die\u00b7sem", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich will es fr\u00f6hlich schauen,", "tokens": ["Ich", "will", "es", "fr\u00f6h\u00b7lich", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df schweben aus dem Blauen", "tokens": ["La\u00df", "schwe\u00b7ben", "aus", "dem", "Blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Brautkranz ihr ins dunkle Haar.", "tokens": ["Den", "Braut\u00b7kranz", "ihr", "ins", "dunk\u00b7le", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}