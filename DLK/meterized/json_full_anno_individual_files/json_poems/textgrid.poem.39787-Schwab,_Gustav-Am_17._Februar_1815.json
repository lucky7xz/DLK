{"textgrid.poem.39787": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Am 17. Februar 1815", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So feir' ich denn mit Thr\u00e4nen,", "tokens": ["So", "feir'", "ich", "denn", "mit", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Einsamen Leiert\u00f6nen", "tokens": ["Ein\u00b7sa\u00b7men", "Lei\u00b7er\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Den freudenreichen Tag!", "tokens": ["Den", "freu\u00b7den\u00b7rei\u00b7chen", "Tag", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie m\u00f6cht' ich gr\u00fc\u00dfend k\u00fcssen,", "tokens": ["Sie", "m\u00f6cht'", "ich", "gr\u00fc\u00b7\u00dfend", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und kann, ach, doch nicht wissen,", "tokens": ["Und", "kann", ",", "ach", ",", "doch", "nicht", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "ITJ", "$,", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie es zu ihr gelangen mag.", "tokens": ["Wie", "es", "zu", "ihr", "ge\u00b7lan\u00b7gen", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Verbundnen treuen Seelen", "tokens": ["Ver\u00b7bund\u00b7nen", "treu\u00b7en", "See\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kann's nie an Boten fehlen", "tokens": ["Kann's", "nie", "an", "Bo\u00b7ten", "feh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu ew'gem Wechselgru\u00df,", "tokens": ["Zu", "ew'\u00b7gem", "Wech\u00b7sel\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da giebt es keine Ferne,", "tokens": ["Da", "giebt", "es", "kei\u00b7ne", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie schauen in die Sterne,", "tokens": ["Sie", "schau\u00b7en", "in", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da regnet Brief herab und Ku\u00df.", "tokens": ["Da", "reg\u00b7net", "Brief", "her\u00b7ab", "und", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du lichte, n\u00e4cht'ge Bl\u00e4ue,", "tokens": ["Du", "lich\u00b7te", ",", "n\u00e4cht'\u00b7ge", "Bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du nimmst den Schwur der Treue", "tokens": ["Du", "nimmst", "den", "Schwur", "der", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von meinen Lippen auf;", "tokens": ["Von", "mei\u00b7nen", "Lip\u00b7pen", "auf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er s\u00e4uselt wohl hin\u00fcber,", "tokens": ["Er", "s\u00e4u\u00b7selt", "wohl", "hin\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er weht an ihr vor\u00fcber,", "tokens": ["Er", "weht", "an", "ihr", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie aber achtet nicht darauf.", "tokens": ["Sie", "a\u00b7ber", "ach\u00b7tet", "nicht", "da\u00b7rauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich will sie nicht beth\u00f6ren,", "tokens": ["Ich", "will", "sie", "nicht", "be\u00b7th\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mag sie es \u00fcberh\u00f6ren,", "tokens": ["Mag", "sie", "es", "\u00fc\u00b7ber\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mag ich vergessen sein!", "tokens": ["Mag", "ich", "ver\u00b7ges\u00b7sen", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dringt, was ich f\u00fcr sie flehe,", "tokens": ["Dringt", ",", "was", "ich", "f\u00fcr", "sie", "fle\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Nur zu der ew'gen H\u00f6he,", "tokens": ["Nur", "zu", "der", "ew'\u00b7gen", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nur in den treuen Himmel ein.", "tokens": ["Nur", "in", "den", "treu\u00b7en", "Him\u00b7mel", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "La\u00df ihr die Wangen bl\u00fchen,", "tokens": ["La\u00df", "ihr", "die", "Wan\u00b7gen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die dunkeln Augen gl\u00fchen,", "tokens": ["Die", "dun\u00b7keln", "Au\u00b7gen", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O du der Liebe Hort!", "tokens": ["O", "du", "der", "Lie\u00b7be", "Hort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch wen sie soll entz\u00fcnden", "tokens": ["Doch", "wen", "sie", "soll", "ent\u00b7z\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In seines Herzens Gr\u00fcnden,", "tokens": ["In", "sei\u00b7nes", "Her\u00b7zens", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem schenk' auch ihre Huld sofort.", "tokens": ["Dem", "schenk'", "auch", "ih\u00b7re", "Huld", "so\u00b7fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "F\u00fcr mich nicht will ich bitten,", "tokens": ["F\u00fcr", "mich", "nicht", "will", "ich", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKNEG", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich habe mir erstritten", "tokens": ["Ich", "ha\u00b7be", "mir", "er\u00b7strit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein fest und ruhig Herz!", "tokens": ["Ein", "fest", "und", "ru\u00b7hig", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nur, kann sie nichts erwiedern,", "tokens": ["Nur", ",", "kann", "sie", "nichts", "er\u00b7wie\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So spar' auch andern Br\u00fcdern", "tokens": ["So", "spa\u00b7r'", "auch", "an\u00b7dern", "Br\u00fc\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Unangeh\u00f6rter Liebe Schmerz.", "tokens": ["Un\u00b7an\u00b7ge\u00b7h\u00f6r\u00b7ter", "Lie\u00b7be", "Schmerz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch willst du Lieb' und Leben", "tokens": ["Doch", "willst", "du", "Lieb'", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem keuschen Busen geben,", "tokens": ["Dem", "keu\u00b7schen", "Bu\u00b7sen", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So thu's in diesem Jahr;", "tokens": ["So", "thu's", "in", "die\u00b7sem", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich will es fr\u00f6hlich schauen,", "tokens": ["Ich", "will", "es", "fr\u00f6h\u00b7lich", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df schweben aus dem Blauen", "tokens": ["La\u00df", "schwe\u00b7ben", "aus", "dem", "Blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Brautkranz ihr ins dunkle Haar.", "tokens": ["Den", "Braut\u00b7kranz", "ihr", "ins", "dunk\u00b7le", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "So feir' ich denn mit Thr\u00e4nen,", "tokens": ["So", "feir'", "ich", "denn", "mit", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Einsamen Leiert\u00f6nen", "tokens": ["Ein\u00b7sa\u00b7men", "Lei\u00b7er\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Den freudenreichen Tag!", "tokens": ["Den", "freu\u00b7den\u00b7rei\u00b7chen", "Tag", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie m\u00f6cht' ich gr\u00fc\u00dfend k\u00fcssen,", "tokens": ["Sie", "m\u00f6cht'", "ich", "gr\u00fc\u00b7\u00dfend", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und kann, ach, doch nicht wissen,", "tokens": ["Und", "kann", ",", "ach", ",", "doch", "nicht", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "ITJ", "$,", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie es zu ihr gelangen mag.", "tokens": ["