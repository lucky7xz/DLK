{"textgrid.poem.35136": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Ernste Weisung", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft euch ein ernstes Wort der Liebe sagen,", "tokens": ["La\u00dft", "euch", "ein", "erns\u00b7tes", "Wort", "der", "Lie\u00b7be", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und grabt es tief in eure Herzen ein:", "tokens": ["Und", "grabt", "es", "tief", "in", "eu\u00b7re", "Her\u00b7zen", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Starke hat den Schwachen hier zu tragen,", "tokens": ["Der", "Star\u00b7ke", "hat", "den", "Schwa\u00b7chen", "hier", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dieser soll ihm daf\u00fcr dankbar sein.", "tokens": ["Und", "die\u00b7ser", "soll", "ihm", "da\u00b7f\u00fcr", "dank\u00b7bar", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PPER", "PAV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es ist das Beider Pflicht, vom Herrn geboten,", "tokens": ["Es", "ist", "das", "Bei\u00b7der", "Pflicht", ",", "vom", "Herrn", "ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wer sie nicht erf\u00fcllt, hat einst und dann", "tokens": ["Und", "wer", "sie", "nicht", "er\u00b7f\u00fcllt", ",", "hat", "einst", "und", "dann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "VVPP", "$,", "VAFIN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als seelisch Todter bei den seelisch Todten", "tokens": ["Als", "see\u00b7lisch", "Tod\u00b7ter", "bei", "den", "see\u00b7lisch", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "NN", "APPR", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weit mehr zu tragen, als er tragen kann.", "tokens": ["Weit", "mehr", "zu", "tra\u00b7gen", ",", "als", "er", "tra\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wer sich weigert, hier den Dank zu zollen,", "tokens": ["Und", "wer", "sich", "wei\u00b7gert", ",", "hier", "den", "Dank", "zu", "zol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "VVFIN", "$,", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ihn die H\u00fclfe liebevoll umarmt,", "tokens": ["Wenn", "ihn", "die", "H\u00fcl\u00b7fe", "lie\u00b7be\u00b7voll", "um\u00b7armt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der wird einst gerne, gerne danken wollen,", "tokens": ["Der", "wird", "einst", "ger\u00b7ne", ",", "ger\u00b7ne", "dan\u00b7ken", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "$,", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch Niemand finden, der sich sein erbarmt.", "tokens": ["Doch", "Nie\u00b7mand", "fin\u00b7den", ",", "der", "sich", "sein", "er\u00b7barmt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "$,", "PRELS", "PRF", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "La\u00dft euch ein ernstes Wort der Liebe sagen,", "tokens": ["La\u00dft", "euch", "ein", "erns\u00b7tes", "Wort", "der", "Lie\u00b7be", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und grabt es tief in eure Herzen ein:", "tokens": ["Und", "grabt", "es", "tief", "in", "eu\u00b7re", "Her\u00b7zen", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Starke hat den Schwachen hier zu tragen,", "tokens": ["Der", "Star\u00b7ke", "hat", "den", "Schwa\u00b7chen", "hier", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", 