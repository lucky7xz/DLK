{"dta.poem.3641": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Erschreckliche Geschichte vom H\u00fcnchen und  \n vom H\u00e4nchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4nchen ist zum Born gelaufen,", "tokens": ["H\u00e4n\u00b7chen", "ist", "zum", "Born", "ge\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Born du sollst mir Wasser geben,", "tokens": ["Born", "du", "sollst", "mir", "Was\u00b7ser", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00fcnchen liegt an jenem Berg,", "tokens": ["H\u00fcn\u00b7chen", "liegt", "an", "je\u00b7nem", "Berg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und schluckt an einem Nu\u00dfkern;", "tokens": ["Und", "schluckt", "an", "ei\u00b7nem", "Nu\u00df\u00b7kern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und da hat der Born gesprochen:", "tokens": ["Und", "da", "hat", "der", "Born", "ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Erst sollst du zur Braut hinspringen,", "tokens": ["Erst", "sollst", "du", "zur", "Braut", "hin\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und mir klare Seide bringen,", "tokens": ["Und", "mir", "kla\u00b7re", "Sei\u00b7de", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00e4nchen ist zur Braut gesprungen,", "tokens": ["H\u00e4n\u00b7chen", "ist", "zur", "Braut", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Braut du sollst mir Seide geben,", "tokens": ["Braut", "du", "sollst", "mir", "Sei\u00b7de", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Seide soll ich Brunnen bringen,", "tokens": ["Sei\u00b7de", "soll", "ich", "Brun\u00b7nen", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Brunnen soll mir Wasser geben,", "tokens": ["Brun\u00b7nen", "soll", "mir", "Was\u00b7ser", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wasser soll ich H\u00fcnchen bringen,", "tokens": ["Was\u00b7ser", "soll", "ich", "H\u00fcn\u00b7chen", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "H\u00fcnchen liegt an jenem Berg,", "tokens": ["H\u00fcn\u00b7chen", "liegt", "an", "je\u00b7nem", "Berg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Und schluckt an einem Nu\u00dfkern.", "tokens": ["Und", "schluckt", "an", "ei\u00b7nem", "Nu\u00df\u00b7kern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Und da hat die Braut gesprochen:", "tokens": ["Und", "da", "hat", "die", "Braut", "ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.16": {"text": "Sollst mir erst mein Kr\u00e4nzlein langen,", "tokens": ["Sollst", "mir", "erst", "mein", "Kr\u00e4nz\u00b7lein", "lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Blieb mir in den Weiden hangen;", "tokens": ["Blieb", "mir", "in", "den", "Wei\u00b7den", "han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "H\u00e4nchen ist zur Weide flogen,", "tokens": ["H\u00e4n\u00b7chen", "ist", "zur", "Wei\u00b7de", "flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}