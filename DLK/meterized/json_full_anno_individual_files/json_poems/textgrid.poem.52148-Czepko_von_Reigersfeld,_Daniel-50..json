{"textgrid.poem.52148": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur aus, o Buch, das Dorff vom Mertzen das ist Graus,", "tokens": ["Nur", "aus", ",", "o", "Buch", ",", "das", "Dorff", "vom", "Mert\u00b7zen", "das", "ist", "Graus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "FM", "NN", "$,", "ART", "NN", "APPRART", "NN", "PDS", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der sch\u00f6ne Meyer Hoff zu Kletschkau w\u00fcste worden,", "tokens": ["Der", "sch\u00f6\u00b7ne", "Me\u00b7yer", "Hoff", "zu", "Kletschkau", "w\u00fcs\u00b7te", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "APPR", "NE", "VVFIN", "VAPP", "$,"], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Das Striegen Vorwerck fort: Das Vierdt' in diesem Orden", "tokens": ["Das", "Strie\u00b7gen", "Vor\u00b7werck", "fort", ":", "Das", "Vierdt'", "in", "die\u00b7sem", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PTKVZ", "$.", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wil auch mein Berg Gut seyn, darumb, o Buch, nur aus.", "tokens": ["Wil", "auch", "mein", "Berg", "Gut", "seyn", ",", "da\u00b7rumb", ",", "o", "Buch", ",", "nur", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "ADJD", "VAINF", "$,", "PAV", "$,", "FM", "NN", "$,", "ADV", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Die G\u00fcter nehmen ab, die Schatzung aber zu,", "tokens": ["Die", "G\u00fc\u00b7ter", "neh\u00b7men", "ab", ",", "die", "Schat\u00b7zung", "a\u00b7ber", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Haus wird leer an Geld u. voll an Krieges Knechten,", "tokens": ["Das", "Haus", "wird", "leer", "an", "Geld", "u.", "voll", "an", "Krie\u00b7ges", "Knech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "APPR", "ADJD", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Kehr ich mich hundertmal zur Lincken oder Rechten,", "tokens": ["Kehr", "ich", "mich", "hun\u00b7dert\u00b7mal", "zur", "Lin\u00b7cken", "o\u00b7der", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So hab' ich Tag und Nacht bey keinem keine Ruh.", "tokens": ["So", "hab'", "ich", "Tag", "und", "Nacht", "bey", "kei\u00b7nem", "kei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "APPR", "PIAT", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich wil hier unterm 