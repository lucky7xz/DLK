{"dta.poem.5651": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In allen, was man sp\u00fcrt, im Frost und in der Hitze,", "tokens": ["In", "al\u00b7len", ",", "was", "man", "sp\u00fcrt", ",", "im", "Frost", "und", "in", "der", "Hit\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PRELS", "PIS", "VVFIN", "$,", "APPRART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jm Schmecken, im Geruch, im Stich von einer Spitze,", "tokens": ["Jm", "Schme\u00b7cken", ",", "im", "Ge\u00b7ruch", ",", "im", "Stich", "von", "ei\u00b7ner", "Spit\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jm Ton und in der Farb\u2019, erkennt er blos allein", "tokens": ["Jm", "Ton", "und", "in", "der", "Fa\u00b7rb'", ",", "er\u00b7kennt", "er", "blos", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Ein F\u00fchlen, anders nichts. Da\u00df wir empfindlich seyn,", "tokens": ["Ein", "F\u00fch\u00b7len", ",", "an\u00b7ders", "nichts", ".", "Da\u00df", "wir", "emp\u00b7find\u00b7lich", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PIS", "$.", "KOUS", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Macht ein geheimer Grund. Will man nun weiter gehen", "tokens": ["Macht", "ein", "ge\u00b7hei\u00b7mer", "Grund", ".", "Will", "man", "nun", "wei\u00b7ter", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "VMFIN", "PIS", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und, wie ein C\u00f6rper uns den Eindruck macht, verstehen;", "tokens": ["Und", ",", "wie", "ein", "C\u00f6r\u00b7per", "uns", "den", "Ein\u00b7druck", "macht", ",", "ver\u00b7ste\u00b7hen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So ist solch eine Wissenschafft", "tokens": ["So", "ist", "solch", "ei\u00b7ne", "Wis\u00b7sen\u00b7schafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Allein der Geister Werck und des Verstandes Krafft.", "tokens": ["Al\u00b7lein", "der", "Geis\u00b7ter", "Werck", "und", "des", "Ver\u00b7stan\u00b7des", "Krafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}