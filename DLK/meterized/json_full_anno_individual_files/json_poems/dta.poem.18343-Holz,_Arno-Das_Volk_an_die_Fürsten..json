{"dta.poem.18343": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Das Volk an die F\u00fcrsten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Denkt, o denkt an die Befreiungskriege!", "tokens": ["Denkt", ",", "o", "denkt", "an", "die", "Be\u00b7frei\u00b7ungs\u00b7krie\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und auch heut noch mu\u00df ich, wie befohlen,", "tokens": ["Und", "auch", "heut", "noch", "mu\u00df", "ich", ",", "wie", "be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "VMFIN", "PPER", "$,", "PWAV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die Kastanien aus dem Feuer holen.", "tokens": ["Die", "Kas\u00b7ta\u00b7ni\u00b7en", "aus", "dem", "Feu\u00b7er", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Einmal auch schon hab ich, selbst verschuldet,", "tokens": ["Ein\u00b7mal", "auch", "schon", "hab", "ich", ",", "selbst", "ver\u00b7schul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VAFIN", "PPER", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Euren k\u00f6niglichen Dank erduldet:", "tokens": ["Eu\u00b7ren", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Dank", "er\u00b7dul\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Erst mir l\u00e4chelnd ins Gesicht geheuchelt,", "tokens": ["Erst", "mir", "l\u00e4\u00b7chelnd", "ins", "Ge\u00b7sicht", "ge\u00b7heu\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dann mich hinterr\u00fccks ins Knie gemeuchelt!", "tokens": ["Dann", "mich", "hin\u00b7ter\u00b7r\u00fccks", "ins", "Knie", "ge\u00b7meu\u00b7chelt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Glaubt mir, auch die Liebe wei\u00df zu hassen;", "tokens": ["Glaubt", "mir", ",", "auch", "die", "Lie\u00b7be", "wei\u00df", "zu", "has\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eure Sonnen werden nicht verblassen!", "tokens": ["Eu\u00b7re", "Son\u00b7nen", "wer\u00b7den", "nicht", "ver\u00b7blas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sink ich heute auch verblutend nieder:", "tokens": ["Sink", "ich", "heu\u00b7te", "auch", "ver\u00b7blu\u00b7tend", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "VVPP", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u201ebei Philippi sehen wir uns wieder!\u201c", "tokens": ["\u201e", "bei", "Phil\u00b7ip\u00b7pi", "se\u00b7hen", "wir", "uns", "wie\u00b7der", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NE", "VVFIN", "PPER", "PRF", "ADV", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}