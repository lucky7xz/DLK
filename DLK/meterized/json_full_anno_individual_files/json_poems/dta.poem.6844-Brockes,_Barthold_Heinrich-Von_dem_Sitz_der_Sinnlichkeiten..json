{"dta.poem.6844": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Beyspiel zeiget uns, da\u00df dies die Wahrheit sey:", "tokens": ["Ein", "Bey\u00b7spiel", "zei\u00b7get", "uns", ",", "da\u00df", "dies", "die", "Wahr\u00b7heit", "sey", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PDS", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn sich in jeden Aug\u2019 ein Gegenwurf formiret,", "tokens": ["Wenn", "sich", "in", "je\u00b7den", "Aug'", "ein", "Ge\u00b7gen\u00b7wurf", "for\u00b7mi\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie kommt es denn, da\u00df man nur eine Schilderey", "tokens": ["Wie", "kommt", "es", "denn", ",", "da\u00df", "man", "nur", "ei\u00b7ne", "Schil\u00b7de\u00b7rey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PIS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von selben Gegenwurff in unserm Hirn versp\u00fcret?", "tokens": ["Von", "sel\u00b7ben", "Ge\u00b7gen\u00b7wurff", "in", "un\u00b7serm", "Hirn", "ver\u00b7sp\u00fc\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn man nun diesen Bau, der wol recht wunderbar,", "tokens": ["Wenn", "man", "nun", "die\u00b7sen", "Bau", ",", "der", "wol", "recht", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PDAT", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von innen nahe sieht; wird die Erfahrung klar.", "tokens": ["Von", "in\u00b7nen", "na\u00b7he", "sieht", ";", "wird", "die", "Er\u00b7fah\u00b7rung", "klar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "VVFIN", "$.", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jm Haupte sieht man Fern-und Spiegel-Gl\u00e4ser sitzen,", "tokens": ["Jm", "Haup\u00b7te", "sieht", "man", "Fern\u00b7\u00b7und", "Spie\u00b7gel\u00b7Gl\u00e4\u00b7ser", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durch unsre Augen dringt des Lichtes schnelles Blitzen.", "tokens": ["Durch", "uns\u00b7re", "Au\u00b7gen", "dringt", "des", "Lich\u00b7tes", "schnel\u00b7les", "Blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ins Auges Apffel wird erblickt,", "tokens": ["Ins", "Au\u00b7ges", "Apf\u00b7fel", "wird", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wie sich in jeglichem ein kleines Bildni\u00df dr\u00fcckt.", "tokens": ["Wie", "sich", "in", "jeg\u00b7li\u00b7chem", "ein", "klei\u00b7nes", "Bild\u00b7ni\u00df", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "PIAT", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Allein hier mu\u00df man nicht verbleiben,", "tokens": ["Al\u00b7lein", "hier", "mu\u00df", "man", "nicht", "ver\u00b7blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Indem der Eindruck noch viel tieffer geht.", "tokens": ["In\u00b7dem", "der", "Ein\u00b7druck", "noch", "viel", "tief\u00b7fer", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Lasst uns bis in den Grund des Hirns das Dencken treiben,", "tokens": ["Lasst", "uns", "bis", "in", "den", "Grund", "des", "Hirns", "das", "Den\u00b7cken", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wo des Gesichtes Nervgen steht.", "tokens": ["Wo", "des", "Ge\u00b7sich\u00b7tes", "Nerv\u00b7gen", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da, wo aus beyden Augen sich", "tokens": ["Da", ",", "wo", "aus", "bey\u00b7den", "Au\u00b7gen", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "PIAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Verbindungs-F\u00e4den schnell begegnen und ber\u00fchren,", "tokens": ["Ver\u00b7bin\u00b7dungs\u00b7F\u00e4\u00b7den", "schnell", "be\u00b7geg\u00b7nen", "und", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wird man vom Vorwurff sichtbarlich", "tokens": ["Wird", "man", "vom", "Vor\u00b7wurff", "sicht\u00b7bar\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "APPRART", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Nur eine Schilderey versp\u00fcren.", "tokens": ["Nur", "ei\u00b7ne", "Schil\u00b7de\u00b7rey", "ver\u00b7sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Also vereinen sich die Ohren-Nerven auch.", "tokens": ["Al\u00b7so", "ver\u00b7ei\u00b7nen", "sich", "die", "Oh\u00b7ren\u00b7Ner\u00b7ven", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der Zungen und der Nas\u2019 und die, so zum Gebrauch", "tokens": ["Der", "Zun\u00b7gen", "und", "der", "Nas'", "und", "die", ",", "so", "zum", "Ge\u00b7brauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "KON", "ART", "$,", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Des F\u00fchlens zugericht\u2019t und unsern Leib durchgehen,", "tokens": ["Des", "F\u00fch\u00b7lens", "zu\u00b7ge\u00b7richt't", "und", "un\u00b7sern", "Leib", "durch\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die k\u00f6nnen wir f\u00fcr sich auch also w\u00fcrcken sehen.", "tokens": ["Die", "k\u00f6n\u00b7nen", "wir", "f\u00fcr", "sich", "auch", "al\u00b7so", "w\u00fcr\u00b7cken", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "APPR", "PRF", "ADV", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}