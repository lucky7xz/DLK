{"dta.poem.4176": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Anmuht des Geh\u00f6rs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem ich j\u00fcngst zur Fr\u00fchlings-Zeit", "tokens": ["Nach\u00b7dem", "ich", "j\u00fcngst", "zur", "Fr\u00fch\u00b7lings\u00b7Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An der Gesch\u00f6pfe Herrlichkeit,", "tokens": ["An", "der", "Ge\u00b7sch\u00f6p\u00b7fe", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die itzo wunderw\u00fcrdig, sch\u00f6n,", "tokens": ["Die", "it\u00b7zo", "wun\u00b7der\u00b7w\u00fcr\u00b7dig", ",", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich matt, jedoch nicht satt, gesehn;", "tokens": ["Mich", "matt", ",", "je\u00b7doch", "nicht", "satt", ",", "ge\u00b7sehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "ADV", "PTKNEG", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Setzt\u2019 ich mich, inniglich erfreut,", "tokens": ["Setzt'", "ich", "mich", ",", "in\u00b7nig\u00b7lich", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ins Gras, im k\u00fchlen Schatten, nieder,", "tokens": ["Ins", "Gras", ",", "im", "k\u00fch\u00b7len", "Schat\u00b7ten", ",", "nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und schlo\u00df mit Flei\u00df die Augenlieder,", "tokens": ["Und", "schlo\u00df", "mit", "Flei\u00df", "die", "Au\u00b7gen\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Damit die Seele durchs Geh\u00f6r", "tokens": ["Da\u00b7mit", "die", "See\u00b7le", "durchs", "Ge\u00b7h\u00f6r"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mehr f\u00e4hig und geschickter w\u00e4r\u2019,", "tokens": ["Mehr", "f\u00e4\u00b7hig", "und", "ge\u00b7schick\u00b7ter", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Zu unsrer Lust, und GOtt zu Ehren,", "tokens": ["Zu", "uns\u00b7rer", "Lust", ",", "und", "Gott", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Noch sch\u00e4rfer, was man h\u00f6rt, zu h\u00f6ren.", "tokens": ["Noch", "sch\u00e4r\u00b7fer", ",", "was", "man", "h\u00f6rt", ",", "zu", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PRELS", "PIS", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Da denn der L\u00fcfte sanft Bewegen,", "tokens": ["Da", "denn", "der", "L\u00fcf\u00b7te", "sanft", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wodurch sich Zweig\u2019 und Bl\u00e4tter regen,", "tokens": ["Wo\u00b7durch", "sich", "Zweig'", "und", "Bl\u00e4t\u00b7ter", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "KON", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Da ich des Windes Blasen sp\u00fcrte,", "tokens": ["Da", "ich", "des", "Win\u00b7des", "Bla\u00b7sen", "sp\u00fcr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Zuerst durchs Ohr den Geist mir r\u00fchrte.", "tokens": ["Zu\u00b7erst", "durchs", "Ohr", "den", "Geist", "mir", "r\u00fchr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}