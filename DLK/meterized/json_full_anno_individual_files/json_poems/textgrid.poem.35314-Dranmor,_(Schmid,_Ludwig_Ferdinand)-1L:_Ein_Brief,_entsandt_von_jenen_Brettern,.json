": ["KON", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Zu mir, du Holde, Teure,", "tokens": ["Zu", "mir", ",", "du", "Hol\u00b7de", ",", "Teu\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nach dieser gr\u00fcnen Einsamkeit.", "tokens": ["Nach", "die\u00b7ser", "gr\u00fc\u00b7nen", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Will dir dereinst nicht mehr gefallen,", "tokens": ["Will", "dir", "de\u00b7reinst", "nicht", "mehr", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was jetzt dein junges Herz erfreut,", "tokens": ["Was", "jetzt", "dein", "jun\u00b7ges", "Herz", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann la\u00df die Schmeichler und Vasallen", "tokens": ["Dann", "la\u00df", "die", "Schmeich\u00b7ler", "und", "Va\u00b7sal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ART", "NN", "KON", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und alles, was die Sonne scheut;", "tokens": ["Und", "al\u00b7les", ",", "was", "die", "Son\u00b7ne", "scheut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-