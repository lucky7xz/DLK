{"textgrid.poem.35314": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Brief, entsandt von jenen Brettern,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Brief, entsandt von jenen Brettern,", "tokens": ["Ein", "Brief", ",", "ent\u00b7sandt", "von", "je\u00b7nen", "Bret\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo deine Kunst mich einst entz\u00fcckt?", "tokens": ["Wo", "dei\u00b7ne", "Kunst", "mich", "einst", "ent\u00b7z\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Freundin! Die bekannten Lettern,", "tokens": ["O", "Freun\u00b7din", "!", "Die", "be\u00b7kann\u00b7ten", "Let\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie haben die mich neu begl\u00fcckt!", "tokens": ["Wie", "ha\u00b7ben", "die", "mich", "neu", "be\u00b7gl\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun winken mir so s\u00fc\u00dfe Bilder,", "tokens": ["Nun", "win\u00b7ken", "mir", "so", "s\u00fc\u00b7\u00dfe", "Bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nun scheint die Zukunft wieder milder", "tokens": ["Nun", "scheint", "die", "Zu\u00b7kunft", "wie\u00b7der", "mil\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und nahe die Vergangenheit;", "tokens": ["Und", "na\u00b7he", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Stern, der mich einst geblendet,", "tokens": ["Stern", ",", "der", "mich", "einst", "ge\u00b7blen\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Dein Licht ist nicht verschwendet", "tokens": ["Dein", "Licht", "ist", "nicht", "ver\u00b7schwen\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "In dieser gr\u00fcnen Einsamkeit.", "tokens": ["In", "die\u00b7ser", "gr\u00fc\u00b7nen", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da mir die Jugend t\u00e4glich schwindet,", "tokens": ["Da", "mir", "die", "Ju\u00b7gend", "t\u00e4g\u00b7lich", "schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und da mein Herz, zu sp\u00e4t belehrt,", "tokens": ["Und", "da", "mein", "Herz", ",", "zu", "sp\u00e4t", "be\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "$,", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ha\u00df und Verachtung nur empfindet", "tokens": ["Ha\u00df", "und", "Ver\u00b7ach\u00b7tung", "nur", "emp\u00b7fin\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADV", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "F\u00fcr vieles, was ich sonst gelehrt;", "tokens": ["F\u00fcr", "vie\u00b7les", ",", "was", "ich", "sonst", "ge\u00b7lehrt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da seine Seufzer l\u00e4ngst verklungen,", "tokens": ["Da", "sei\u00b7ne", "Seuf\u00b7zer", "l\u00e4ngst", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ach! nur von Z\u00e4rtlichkeit durchdrungen", "tokens": ["Ach", "!", "nur", "von", "Z\u00e4rt\u00b7lich\u00b7keit", "durch\u00b7drun\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr dich, so wird es jederzeit", "tokens": ["F\u00fcr", "dich", ",", "so", "wird", "es", "je\u00b7der\u00b7zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zwar schmerzlich dich vermissen,", "tokens": ["Zwar", "schmerz\u00b7lich", "dich", "ver\u00b7mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Doch dich zu ehren wissen", "tokens": ["Doch", "dich", "zu", "eh\u00b7ren", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKZU", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "In dieser gr\u00fcnen Einsamkeit.", "tokens": ["In", "die\u00b7ser", "gr\u00fc\u00b7nen", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df, du bist nicht zu bedauern", "tokens": ["Ich", "wei\u00df", ",", "du", "bist", "nicht", "zu", "be\u00b7dau\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dort in dem glanzerf\u00fcllten Saal;", "tokens": ["Dort", "in", "dem", "glan\u00b7zer\u00b7f\u00fcll\u00b7ten", "Saal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dich l\u00e4\u00dft das Schicksal nicht verbauern", "tokens": ["Dich", "l\u00e4\u00dft", "das", "Schick\u00b7sal", "nicht", "ver\u00b7bau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In einem stillen Palmenthal;", "tokens": ["In", "ei\u00b7nem", "stil\u00b7len", "Pal\u00b7ment\u00b7hal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch dr\u00fccken dich die seidnen Kleider,", "tokens": ["Doch", "dr\u00fc\u00b7cken", "dich", "die", "seid\u00b7nen", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dann g\u00f6nne mir verbl\u00fcffte Neider,", "tokens": ["Dann", "g\u00f6n\u00b7ne", "mir", "ver\u00b7bl\u00fcff\u00b7te", "Nei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann denk' an mich und fliehe weit", "tokens": ["Dann", "denk'", "an", "mich", "und", "flie\u00b7he", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und \u00fcbers Weltmeer steure", "tokens": ["Und", "\u00fc\u00b7bers", "Welt\u00b7meer", "steu\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Zu mir, du Holde, Teure,", "tokens": ["Zu", "mir", ",", "du", "Hol\u00b7de", ",", "Teu\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nach dieser gr\u00fcnen Einsamkeit.", "tokens": ["Nach", "die\u00b7ser", "gr\u00fc\u00b7nen", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Will dir dereinst nicht mehr gefallen,", "tokens": ["Will", "dir", "de\u00b7reinst", "nicht", "mehr", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was jetzt dein junges Herz erfreut,", "tokens": ["Was", "jetzt", "dein", "jun\u00b7ges", "Herz", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann la\u00df die Schmeichler und Vasallen", "tokens": ["Dann", "la\u00df", "die", "Schmeich\u00b7ler", "und", "Va\u00b7sal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ART", "NN", "KON", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und alles, was die Sonne scheut;", "tokens": ["Und", "al\u00b7les", ",", "was", "die", "Son\u00b7ne", "scheut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann la\u00df, wohin dein Freund verschlagen,", "tokens": ["Dann", "la\u00df", ",", "wo\u00b7hin", "dein", "Freund", "ver\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dich die beschwingten F\u00fc\u00dfchen tragen \u2013", "tokens": ["Dich", "die", "be\u00b7schwing\u00b7ten", "F\u00fc\u00df\u00b7chen", "tra\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sieh! Seine H\u00fctte steht bereit", "tokens": ["Sieh", "!", "Sei\u00b7ne", "H\u00fct\u00b7te", "steht", "be\u00b7reit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcr dich zu jeder Stunde.", "tokens": ["F\u00fcr", "dich", "zu", "je\u00b7der", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Geliebte, komm! Gesunde", "tokens": ["Ge\u00b7lieb\u00b7te", ",", "komm", "!", "Ge\u00b7sun\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "$.", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "In dieser gr\u00fcnen Einsamkeit.", "tokens": ["In", "die\u00b7ser", "gr\u00fc\u00b7nen", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein Brief, entsandt von jenen Brettern,", "tokens": ["Ein", "Brief", ",", "ent\u00b7sandt", "von", "je\u00b7nen", "Bret\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo deine Kunst mich einst entz\u00fcckt?", "tokens": ["Wo", "dei\u00b7ne", "Kunst", "mich", "einst", "ent\u00b7z\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Freundin! Die bekannten Lettern,", "tokens": ["O", "Freun\u00b7din", "!", "Die", "be\u00b7kann\u00b7ten", "Let\u00b7tern", ","], "token_info": ["word"