{"dta.poem.21095": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Harmonie der Sph\u00e4ren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Horch, wie orgelt, wie braust die &#196;olsharfe der               ", "tokens": ["Horch", ",", "wie", "or\u00b7gelt", ",", "wie", "braust", "die", "&#196;", "ols\u00b7har\u00b7fe", "der"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADJD", "$,", "PWAV", "VVFIN", "ART", "ADJA", "NN", "ART"], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sch\u00f6pfung!", "tokens": ["Sch\u00f6p\u00b7fung", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Droben und drunten und rings t\u00f6net ihr", "tokens": ["Dro\u00b7ben", "und", "drun\u00b7ten", "und", "rings", "t\u00f6\u00b7net", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "VVFIN", "KON", "ADV", "VVFIN", "PPER"], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "bebendes Gold.", "tokens": ["be\u00b7ben\u00b7des", "Gold", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Helios Flammengeschoss, Selenens silberner Bo-", "tokens": ["He\u00b7li\u00b7os", "Flam\u00b7men\u00b7ge\u00b7schoss", ",", "Se\u00b7le\u00b7nens", "sil\u00b7ber\u00b7ner", "Bo"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "ADV", "ADJA", "TRUNC"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.6": {"text": "gen,", "tokens": ["gen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Hesperus Strahlengespann klirren im sph\u00e4-", "tokens": ["Hes\u00b7pe\u00b7rus", "Strah\u00b7len\u00b7ge\u00b7spann", "klir\u00b7ren", "im", "sph\u00e4"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "APPRART", "TRUNC"], "meter": "+--+---+--+", "measure": "dactylic.di.plus"}, "line.8": {"text": "rischen Tanz.", "tokens": ["ri\u00b7schen", "Tanz", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Heilige ", "tokens": ["Hei\u00b7li\u00b7ge"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+--", "measure": "dactylic.init"}, "line.10": {"text": "Reigen;", "tokens": ["Rei\u00b7gen", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Singend steiget, es sinkt singend der himm-", "tokens": ["Sin\u00b7gend", "stei\u00b7get", ",", "es", "sinkt", "sin\u00b7gend", "der", "him\u00b7m"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "ART", "TRUNC"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "lische ", "tokens": ["li\u00b7sche"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}}}}}