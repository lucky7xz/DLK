{"dta.poem.19208": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was gl\u00e4nzt, da\u00df du es siehst, ist gleichsam im Verbrennen;", "tokens": ["Was", "gl\u00e4nzt", ",", "da\u00df", "du", "es", "siehst", ",", "ist", "gleich\u00b7sam", "im", "Ver\u00b7bren\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Farben werden sich davon wie Funken trennen.", "tokens": ["Die", "Far\u00b7ben", "wer\u00b7den", "sich", "da\u00b7von", "wie", "Fun\u00b7ken", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "PAV", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was schallt, da\u00df du es h\u00f6rst, ist nah dran zu zerspringen;", "tokens": ["Was", "schallt", ",", "da\u00df", "du", "es", "h\u00f6rst", ",", "ist", "nah", "dran", "zu", "zer\u00b7sprin\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur durch Ersch\u00fctterung vermag's dich anzuklingen.", "tokens": ["Nur", "durch", "Er\u00b7sch\u00fct\u00b7te\u00b7rung", "ver\u00b7mag's", "dich", "an\u00b7zu\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was duftet, da\u00df du's riechst, und was du schmeckest gar,", "tokens": ["Was", "duf\u00b7tet", ",", "da\u00df", "du's", "riechst", ",", "und", "was", "du", "schme\u00b7ckest", "gar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$,", "KON", "PWS", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In diesem nimmst du leicht der Theil' Aufl\u00f6sung wahr.", "tokens": ["In", "die\u00b7sem", "nimmst", "du", "leicht", "der", "Theil'", "Auf\u00b7l\u00f6\u00b7sung", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "ADJD", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und das was dich ber\u00fchrt, da\u00df es dein Finger sp\u00fcrt,", "tokens": ["Und", "das", "was", "dich", "be\u00b7r\u00fchrt", ",", "da\u00df", "es", "dein", "Fin\u00b7ger", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PRELS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist seinem Untergang entgegen so gef\u00fchrt,", "tokens": ["Ist", "sei\u00b7nem", "Un\u00b7ter\u00b7gang", "ent\u00b7ge\u00b7gen", "so", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wenn alles auch so leicht nicht der Zerst\u00f6rung Staub", "tokens": ["Wenn", "al\u00b7les", "auch", "so", "leicht", "nicht", "der", "Zer\u00b7st\u00f6\u00b7rung", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ADJD", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird durch Ber\u00fchrung, wie des Silfen Fl\u00fcgelstaub.", "tokens": ["Wird", "durch", "Be\u00b7r\u00fch\u00b7rung", ",", "wie", "des", "Sil\u00b7fen", "Fl\u00fc\u00b7gel\u00b7staub", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "$,", "PWAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Dem Sinne kann die Welt nicht anders kund sich geben", "tokens": ["Dem", "Sin\u00b7ne", "kann", "die", "Welt", "nicht", "an\u00b7ders", "kund", "sich", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "PTKNEG", "ADV", "PTKVZ", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als nur im Uebergang zum Tode von dem Leben.", "tokens": ["Als", "nur", "im", "Ue\u00b7ber\u00b7gang", "zum", "To\u00b7de", "von", "dem", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}