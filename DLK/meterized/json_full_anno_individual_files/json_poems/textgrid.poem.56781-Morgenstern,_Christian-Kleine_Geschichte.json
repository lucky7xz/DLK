{"textgrid.poem.56781": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Kleine Geschichte", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Litt einst ein F\u00e4hnlein gro\u00dfe Not,", "tokens": ["Litt", "einst", "ein", "F\u00e4hn\u00b7lein", "gro\u00b7\u00dfe", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "halb war es gelb, halb war es rot,", "tokens": ["halb", "war", "es", "gelb", ",", "halb", "war", "es", "rot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD", "$,", "ADJD", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und wollte gern zusammen", "tokens": ["und", "woll\u00b7te", "gern", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "zu einer lichten Flammen.", "tokens": ["zu", "ei\u00b7ner", "lich\u00b7ten", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es zog sich, wand sich, wellte sich,", "tokens": ["Es", "zog", "sich", ",", "wand", "sich", ",", "well\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "VVFIN", "PRF", "$,", "PRELS", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "es knitterte, es schnellte sich, \u2013", "tokens": ["es", "knit\u0