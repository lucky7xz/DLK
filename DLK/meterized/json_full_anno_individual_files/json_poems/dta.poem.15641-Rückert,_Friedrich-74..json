{"dta.poem.15641": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Solang' ist nicht die Zeit auf ihre H\u00f6h gebracht,", "tokens": ["So\u00b7lang'", "ist", "nicht", "die", "Zeit", "auf", "ih\u00b7re", "H\u00f6h", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als nicht zusammentrifft die Einsicht mit der Macht.", "tokens": ["Als", "nicht", "zu\u00b7sam\u00b7men\u00b7trifft", "die", "Ein\u00b7sicht", "mit", "der", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Trifft einst die Macht der Zeit und ihr Begriff zusammen,", "tokens": ["Trifft", "einst", "die", "Macht", "der", "Zeit", "und", "ihr", "Be\u00b7griff", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "KON", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus diesem Bunde wird ein neues Weltheil stammen.", "tokens": ["Aus", "die\u00b7sem", "Bun\u00b7de", "wird", "ein", "neu\u00b7es", "Welt\u00b7heil", "stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-