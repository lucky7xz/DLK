{"dta.poem.23669": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Breslauer Schillerfest 10. Nov. 1839.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe einst die Philister leben lassen,", "tokens": ["Ich", "ha\u00b7be", "einst", "die", "Phi\u00b7lis\u00b7ter", "le\u00b7ben", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aber ich m\u00fcsste jetzt das Leben hassen", "tokens": ["A\u00b7ber", "ich", "m\u00fcss\u00b7te", "jetzt", "das", "Le\u00b7ben", "has\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ART", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und die Sonn und den Regen, die die Reben n\u00e4hren", "tokens": ["Und", "die", "Sonn", "und", "den", "Re\u00b7gen", ",", "die", "die", "Re\u00b7ben", "n\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und uns das Sch\u00f6nre zum Leben gew\u00e4hren \u2014", "tokens": ["Und", "uns", "das", "Sch\u00f6n\u00b7re", "zum", "Le\u00b7ben", "ge\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sollt' ich mich zu solchen Dingen zwingen", "tokens": ["Sollt'", "ich", "mich", "zu", "sol\u00b7chen", "Din\u00b7gen", "zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und ein Lob den Philisterlingen bringen.", "tokens": ["Und", "ein", "Lob", "den", "Phi\u00b7lis\u00b7ter\u00b7lin\u00b7gen", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ich will nicht beehren mit einem Tropfen die Tr\u00f6pfe", "tokens": ["Ich", "will", "nicht", "be\u00b7eh\u00b7ren", "mit", "ei\u00b7nem", "Trop\u00b7fen", "die", "Tr\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und werf' ihnen lieber den Pfropfen an die K\u00f6pfe.", "tokens": ["Und", "wer\u00b7f'", "ih\u00b7nen", "lie\u00b7ber", "den", "Pfrop\u00b7fen", "an", "die", "K\u00f6p\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Doch will ich heute herauf beschw\u00f6ren", "tokens": ["Doch", "will", "ich", "heu\u00b7te", "her\u00b7auf", "be\u00b7schw\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Was unter Schillers Denkmal liegt wie im Grabe,", "tokens": ["Was", "un\u00b7ter", "Schil\u00b7lers", "Denk\u00b7mal", "liegt", "wie", "im", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NE", "NE", "VVFIN", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Ich will es zu meiner eigenen Schande h\u00f6ren,", "tokens": ["Ich", "will", "es", "zu", "mei\u00b7ner", "ei\u00b7ge\u00b7nen", "Schan\u00b7de", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Wie ich damals die Philister bedichtet habe:", "tokens": ["Wie", "ich", "da\u00b7mals", "die", "Phi\u00b7lis\u00b7ter", "be\u00b7dich\u00b7tet", "ha\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "\u201ees leben die Philister,", "tokens": ["\u201e", "es", "le\u00b7ben", "die", "Phi\u00b7lis\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Ihre Gevattern und ihre Geschwister!", "tokens": ["Ih\u00b7re", "Ge\u00b7vat\u00b7tern", "und", "ih\u00b7re", "Ge\u00b7schwis\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.15": {"text": "Die Poetenverachter,", "tokens": ["Die", "Poe\u00b7ten\u00b7ver\u00b7ach\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Monetenbetrachter,", "tokens": ["Mo\u00b7ne\u00b7ten\u00b7be\u00b7trach\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Die Luchser, die Muckser,", "tokens": ["Die", "Luc\u00b7hser", ",", "die", "Muck\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Die Pfennigfuchser,", "tokens": ["Die", "Pfen\u00b7nig\u00b7fuch\u00b7ser", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Die Mucker und Achselzucker,", "tokens": ["Die", "Mu\u00b7cker", "und", "Ach\u00b7sel\u00b7zu\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Die Agio- und Taxenkucker,", "tokens": ["Die", "A\u00b7gio", "und", "Ta\u00b7xen\u00b7ku\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Die Linsenleser", "tokens": ["Die", "Lin\u00b7sen\u00b7le\u00b7ser"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Und Zinsenz\u00e4hler,", "tokens": ["Und", "Zin\u00b7sen\u00b7z\u00e4h\u00b7ler", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Die Couponsschneider", "tokens": ["Die", "Cou\u00b7pons\u00b7schnei\u00b7der"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Und Hungerleider,", "tokens": ["Und", "Hun\u00b7ger\u00b7lei\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "Die, wo andre vor Freude weinen,", "tokens": ["Die", ",", "wo", "and\u00b7re", "vor", "Freu\u00b7de", "wei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PIS", "APPR", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Gleich mit dem Regenschirm erscheinen;", "tokens": ["Gleich", "mit", "dem", "Re\u00b7gen\u00b7schirm", "er\u00b7schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Und wo die Freude droht einzuschlagen,", "tokens": ["Und", "wo", "die", "Freu\u00b7de", "droht", "ein\u00b7zu\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Den Blitzableiter in der Tasche tragen;", "tokens": ["Den", "Blitz\u00b7ab\u00b7lei\u00b7ter", "in", "der", "Ta\u00b7sche", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Die den Teufel scheuen", "tokens": ["Die", "den", "Teu\u00b7fel", "scheu\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.30": {"text": "Und sich wie Teufel freuen;", "tokens": ["Und", "sich", "wie", "Teu\u00b7fel", "freu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Die nicht mehr mit dem Zopfe prangen", "tokens": ["Die", "nicht", "mehr", "mit", "dem", "Zop\u00b7fe", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Und doch an dem Zopfe hangen;", "tokens": ["Und", "doch", "an", "dem", "Zop\u00b7fe", "han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Die Pantoffelgedr\u00fcckten,", "tokens": ["Die", "Pan\u00b7tof\u00b7fel\u00b7ge\u00b7dr\u00fcck\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Kartoffelentz\u00fcckten,", "tokens": ["Kar\u00b7tof\u00b7fe\u00b7lent\u00b7z\u00fcck\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.35": {"text": "Wasser-Verprasser,", "tokens": ["Was\u00b7ser\u00b7Ver\u00b7pras\u00b7ser", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.36": {"text": "Die sich mit der Schlinge der M\u00e4\u00dfigkeit schn\u00fcren,", "tokens": ["Die", "sich", "mit", "der", "Schlin\u00b7ge", "der", "M\u00e4\u00b7\u00dfig\u00b7keit", "schn\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.37": {"text": "Und doch die Klinge der Gefr\u00e4\u00dfigkeit f\u00fchren;", "tokens": ["Und", "doch", "die", "Klin\u00b7ge", "der", "Ge\u00b7fr\u00e4\u00b7\u00dfig\u00b7keit", "f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Die in lauter Formen und Normen sich bewegen,", "tokens": ["Die", "in", "lau\u00b7ter", "For\u00b7men", "und", "Nor\u00b7men", "sich", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "KON", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.39": {"text": "In lauter Schmiegen und Biegen sich regen;", "tokens": ["In", "lau\u00b7ter", "Schmie\u00b7gen", "und", "Bie\u00b7gen", "sich", "re\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "PRF", "ADJA", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.40": {"text": "Die auf dem Stuhle des Schlendrians sitzen,", "tokens": ["Die", "auf", "dem", "Stuh\u00b7le", "des", "Schlen\u00b7dri\u00b7ans", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.41": {"text": "Und in der Schule des Bocksbeutels schwitzen.", "tokens": ["Und", "in", "der", "Schu\u00b7le", "des", "Bocks\u00b7beu\u00b7tels", "schwit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Es leben die Philister,", "tokens": ["Es", "le\u00b7ben", "die", "Phi\u00b7lis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihre Gevattern und ihre Geschwister!", "tokens": ["Ih\u00b7re", "Ge\u00b7vat\u00b7tern", "und", "ih\u00b7re", "Ge\u00b7schwis\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Denn \u2014", "tokens": ["Denn"], "token_info": ["word", "punct"], "pos": ["KON", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Wenn", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Die Philister nicht mehr leben,", "tokens": ["Die", "Phi\u00b7lis\u00b7ter", "nicht", "mehr", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So wird es auch keine Poeten mehr geben!\u201c", "tokens": ["So", "wird", "es", "auch", "kei\u00b7ne", "Po\u00b7et\u00b7en", "mehr", "ge\u00b7ben", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIAT", "NN", "ADV", "VVINF", "$.", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Nun aber seh' ich, wie die Philister hecken,", "tokens": ["Nun", "a\u00b7ber", "seh'", "ich", ",", "wie", "die", "Phi\u00b7lis\u00b7ter", "he\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Wie sie die Lande mit Schauder und Schrecken bedecken.", "tokens": ["Wie", "sie", "die", "Lan\u00b7de", "mit", "Schau\u00b7der", "und", "Schre\u00b7cken", "be\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Geld und Brot, und Brot und Geld!", "tokens": ["Geld", "und", "Brot", ",", "und", "Brot", "und", "Geld", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "So schreit die Welt;", "tokens": ["So", "schreit", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Das ist die einzige Mannigfaltigkeit", "tokens": ["Das", "ist", "die", "ein\u00b7zi\u00b7ge", "Man\u00b7nig\u00b7fal\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "In dem langweiligen Liede unsrer Zeit.", "tokens": ["In", "dem", "lang\u00b7wei\u00b7li\u00b7gen", "Lie\u00b7de", "uns\u00b7rer", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-++--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Brot ist das einzige Universelle", "tokens": ["Brot", "ist", "das", "ein\u00b7zi\u00b7ge", "U\u00b7niv\u00b7er\u00b7sel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Unserer Universit\u00e4ten \u2014", "tokens": ["Un\u00b7se\u00b7rer", "U\u00b7niv\u00b7er\u00b7si\u00b7t\u00e4\u00b7ten"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Das reimt sich nicht ist aber doch wahr,", "tokens": ["Das", "reimt", "sich", "nicht", "ist", "a\u00b7ber", "doch", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "PTKNEG", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "Und wer's nicht glaubt, dem wird's mit der Zeit noch klar.", "tokens": ["Und", "wer's", "nicht", "glaubt", ",", "dem", "wird's", "mit", "der", "Zeit", "noch", "klar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "VVFIN", "$,", "PRELS", "PIS", "APPR", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Auf Brot gerichtet ist der Knabe", "tokens": ["Auf", "Brot", "ge\u00b7rich\u00b7tet", "ist", "der", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und verfolgt das Brot wie ein Rabe,", "tokens": ["Und", "ver\u00b7folgt", "das", "Brot", "wie", "ein", "Ra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Brot ist des J\u00fcnglings Preisaufgabe,", "tokens": ["Brot", "ist", "des", "J\u00fcng\u00b7lings", "Preis\u00b7auf\u00b7ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und der Mann studirt es bis zum Grabe;", "tokens": ["Und", "der", "Mann", "stu\u00b7dirt", "es", "bis", "zum", "Gra\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.21": {"text": "Und alle jagen, haschen, streben, ringen,", "tokens": ["Und", "al\u00b7le", "ja\u00b7gen", ",", "ha\u00b7schen", ",", "stre\u00b7ben", ",", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Wollen es zum Brote, zum Leben bringen.", "tokens": ["Wol\u00b7len", "es", "zum", "Bro\u00b7te", ",", "zum", "Le\u00b7ben", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.23": {"text": "Und was ist Geld?", "tokens": ["Und", "was", "ist", "Geld", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "Ach, leider, ach es gilt \u2014", "tokens": ["Ach", ",", "lei\u00b7der", ",", "ach", "es", "gilt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "$,", "XY", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Das ist ein treues Bild", "tokens": ["Das", "ist", "ein", "treu\u00b7es", "Bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Von der Philisterwelt.", "tokens": ["Von", "der", "Phi\u00b7lis\u00b7ter\u00b7welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Wir wollen unsre Schwerter und Schilde r\u00fchren", "tokens": ["Wir", "wol\u00b7len", "uns\u00b7re", "Schwer\u00b7ter", "und", "Schil\u00b7de", "r\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Und ein anderes Bild im Schilde f\u00fchren.", "tokens": ["Und", "ein", "an\u00b7de\u00b7res", "Bild", "im", "Schil\u00b7de", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.29": {"text": "Wir wollen Schiller als Reichspanier tragen", "tokens": ["Wir", "wol\u00b7len", "Schil\u00b7ler", "als", "Reich\u00b7spa\u00b7nier", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NE", "KOUS", "NN", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Und mit Schillern die Philister schlagen.", "tokens": ["Und", "mit", "Schil\u00b7lern", "die", "Phi\u00b7lis\u00b7ter", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.31": {"text": "Man sollte eigentlich mit dem Esels-Kinnbacken", "tokens": ["Man", "soll\u00b7te", "ei\u00b7gent\u00b7lich", "mit", "dem", "E\u00b7sels\u00b7Kinn\u00b7ba\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+--", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Wie Simson weiland auf sie hinhacken", "tokens": ["Wie", "Sim\u00b7son", "wei\u