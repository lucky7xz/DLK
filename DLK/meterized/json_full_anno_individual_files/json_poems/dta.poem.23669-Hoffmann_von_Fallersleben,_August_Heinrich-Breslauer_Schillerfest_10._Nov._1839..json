xt": "Und wollen lieber bei Schiller bleiben.", "tokens": ["Und", "wol\u00b7len", "lie\u00b7ber", "bei", "Schil\u00b7ler", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "NE", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.39": {"text": "Wir, die wir die Poesie ins Leben trugen,", "tokens": ["Wir", ",", "die", "wir", "die", "Poe\u00b7sie", "ins", "Le\u00b7ben", "tru\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.40": {"text": "Und uns f\u00fcr Ideen zankten und schlugen,", "tokens": ["Und", "uns", "f\u00fcr", "I\u00b7deen", "zank\u00b7ten", "und", "schlu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.41": {"text": "Mit unsrer Begeisterung ausgepfiffen,", "tokens": ["Mit", "uns\u00b7rer", "Be\u00b7geis\u00b7te\u00b7rung", "aus\u00b7ge\u00b7pfif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.42": {"text": "Wir, von des Lebens Ernst ergriffen,", "tokens": ["Wir", ",", "von", "des", "Le\u00b7bens", "Ernst", "er\u00b7grif\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Von seinem Leid und seiner Kl\u00e4glichkeit,", "tokens": ["Von", "sei\u00b7nem", "Leid", "und", "sei\u00b7ner", "Kl\u00e4g\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Von Hass und Neid und mancher Unertr\u00e4glichkeit,", "tokens": ["Von", "Hass", "und", "Neid", "und", "man\u00b7cher", "Un\u00b7er\u00b7tr\u00e4g\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Da\u00df das Philistervolk wider Willen tobe und rase,", "tokens": ["Da\u00df", "das", "Phi\u00b7lis\u00b7ter\u00b7volk", "wi\u00b7der", "Wil\u00b7len", "to\u00b7be", "und", "ra\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.46": {"text": "Und mit uns singe im lustigsten Triller:", "tokens": ["Und", "mit", "uns", "sin\u00b7ge", "im", "lus\u00b7tigs\u00b7ten", "Tril\u00b7ler", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.47": {"text": "Hoch lebe! hoch, hoch Schiller!", "tokens": ["Hoch", "le\u00b7be", "!", "hoch", ",", "hoch", "Schil\u00b7ler", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$.", "ADJD", "$,", "ADJD", "NE", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}