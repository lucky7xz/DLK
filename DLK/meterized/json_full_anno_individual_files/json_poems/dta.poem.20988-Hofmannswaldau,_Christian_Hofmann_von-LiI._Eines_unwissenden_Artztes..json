{"dta.poem.20988": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LiI.  \n Eines unwissenden Artztes.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Des Todes Lietenant hat sich hieher gesellt/", "tokens": ["Des", "To\u00b7des", "Lie\u00b7ten\u00b7ant", "hat", "sich", "hie\u00b7her", "ge\u00b7sellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PRF", "PAV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach dem sein ", "tokens": ["Nach", "dem", "sein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Mich wundert da\u00df der Tod nicht seiner hat ver-", "tokens": ["Mich", "wun\u00b7dert", "da\u00df", "der", "Tod", "nicht", "sei\u00b7ner", "hat", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "ART", "NN", "PTKNEG", "PPOSAT", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "schonet/", "tokens": ["scho\u00b7net", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Und ihm den treuen Dienst/ so er gethan/ belohnet.", "tokens": ["Und", "ihm", "den", "treu\u00b7en", "Dienst", "/", "so", "er", "ge\u00b7than", "/", "be\u00b7loh\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "$(", "ADV", "PPER", "VVPP", "$(", "VVFIN", "$."], "meter