{"textgrid.poem.35885": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Die heiligen Martyrer", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, wer sind diese, die den Himmel st\u00fcrmen,", "tokens": ["Herr", ",", "wer", "sind", "die\u00b7se", ",", "die", "den", "Him\u00b7mel", "st\u00fcr\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VAFIN", "PDS", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ihn an sich rei\u00dfend mit des Glaubens Kraft?", "tokens": ["Ihn", "an", "sich", "rei\u00b7\u00dfend", "mit", "des", "Glau\u00b7bens", "Kraft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PRF", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df nicht Dein Zorn ihr neue S\u00fcndfluth schafft.", "tokens": ["Da\u00df", "nicht", "Dein", "Zorn", "ihr", "neu\u00b7e", "S\u00fcnd\u00b7fluth", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wer sind sie, die das Meer, den Felsen zwingen,", "tokens": ["Wer", "sind", "sie", ",", "die", "das", "Meer", ",", "den", "Fel\u00b7sen", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2"