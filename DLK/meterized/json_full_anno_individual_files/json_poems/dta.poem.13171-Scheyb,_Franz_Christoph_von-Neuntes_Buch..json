{"dta.poem.13171": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "An ihrer Seite kam noch eine Majest\u00e4t:", "tokens": ["An", "ih\u00b7rer", "Sei\u00b7te", "kam", "noch", "ei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "390Thalia sagte mir: Es sey Elisabeth", "tokens": ["sag\u00b7te", "mir", ":", "Es", "sey", "E\u00b7lis\u00b7a\u00b7beth"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VAFIN", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Die grosse Kayserinn, Gemahlinn ", "tokens": ["Die", "gros\u00b7se", "Kay\u00b7se\u00b7rinn", ",", "Ge\u00b7mah\u00b7linn"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Woher ", "tokens": ["Wo\u00b7her"], "token_info": ["word"], "pos": ["PWAV"], "meter": "--", "measure": "unknown.measure.zero"}, "line.5": {"text": "Gro\u00df-Mutter dieses Kinds, der Herrscherinnen Prei\u00df", "tokens": ["Gro\u00df\u00b7Mut\u00b7ter", "die\u00b7ses", "Kinds", ",", "der", "Herr\u00b7sche\u00b7rin\u00b7nen", "Prei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PDAT", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Seit dem der Erde Rund von Kayserinnen wei\u00df.", "tokens": ["Seit", "dem", "der", "Er\u00b7de", "Rund", "von", "Kay\u00b7ser\u00b7in\u00b7nen", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "395Man las' auf ihrer Stirn ein z\u00e4rtliches Vergn\u00fcgen,", "tokens": ["las'", "auf", "ih\u00b7rer", "Stirn", "ein", "z\u00e4rt\u00b7li\u00b7ches", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Wann sie das theure Pfand, den Enckel sahe ligen.", "tokens": ["Wann", "sie", "das", "theu\u00b7re", "Pfand", ",", "den", "En\u00b7ckel", "sa\u00b7he", "li\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man sah ", "tokens": ["Man", "sah"], "token_info": ["word", "word"], "pos": ["PIS", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "In ihrem Angesicht und Hergang abgemahlt.", "tokens": ["In", "ih\u00b7rem", "An\u00b7ge\u00b7sicht", "und", "Her\u00b7gang", "ab\u00b7ge\u00b7mahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was Wunder, sprachen wir, wann sie von ihr erzogen,", "tokens": ["Was", "Wun\u00b7der", ",", "spra\u00b7chen", "wir", ",", "wann", "sie", "von", "ihr", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "VVFIN", "PPER", "$,", "PWAV", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "400Da\u00df sie nichts als Verstand und Tugend eingesogen!", "tokens": ["sie", "nichts", "als", "Ver\u00b7stand", "und", "Tu\u00b7gend", "ein\u00b7ge\u00b7so\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "KOKOM", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Genug: wer sie nur sah, der bildte sich schon ein,", "tokens": ["Ge\u00b7nug", ":", "wer", "sie", "nur", "sah", ",", "der", "bild\u00b7te", "sich", "schon", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "PPER", "ADV", "VVFIN", "$,", "PRELS", "VVFIN", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df diese K\u00f6niginn m\u00fc\u00dft\u2019 ihre Tochter seyn.", "tokens": ["Da\u00df", "die\u00b7se", "K\u00f6\u00b7ni\u00b7ginn", "m\u00fc\u00dft'", "ih\u00b7re", "Toch\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Frucht folgt ihrem Baum; von Eichen kommen Eichen;", "tokens": ["Die", "Frucht", "folgt", "ih\u00b7rem", "Baum", ";", "von", "Ei\u00b7chen", "kom\u00b7men", "Ei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$.", "APPR", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Adler zeugen nichts, was nicht kann Adlern gleichen.", "tokens": ["Die", "Ad\u00b7ler", "zeu\u00b7gen", "nichts", ",", "was", "nicht", "kann", "Ad\u00b7lern", "glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "$,", "PRELS", "PTKNEG", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}