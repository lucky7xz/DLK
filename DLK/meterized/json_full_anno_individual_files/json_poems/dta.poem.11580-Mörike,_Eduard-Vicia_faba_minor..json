{"dta.poem.11580": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Vicia faba minor.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Fort mit diesem Geruch, dem zauberhaften! Er mahnt", "tokens": ["Fort", "mit", "die\u00b7sem", "Ge\u00b7ruch", ",", "dem", "zau\u00b7ber\u00b7haf\u00b7ten", "!", "Er", "mahnt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "PDAT", "NN", "$,", "ART", "ADJA", "$.", "PPER", "VVFIN"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "An die Haare, die mir einst alle Sinne bestrickt.", "tokens": ["An", "die", "Haa\u00b7re", ",", "die", "mir", "einst", "al\u00b7le", "Sin\u00b7ne", "be\u00b7strickt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Weg mit dieser Bl\u00fcthe, der schwarz und wei\u00dfen! Sie", "tokens": ["Weg", "mit", "die\u00b7ser", "Bl\u00fc\u00b7the", ",", "der", "schwarz", "und", "wei\u00b7\u00dfen", "!", "Sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "PDAT", "NN", "$,", "PRELS", "ADJD", "KON", "VVINF", "$.", "PPER"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Da\u00df die Verf\u00fchrerin, ach! schwer mit dem Tode ge-", "tokens": ["Da\u00df", "die", "Ver\u00b7f\u00fch\u00b7re\u00b7rin", ",", "ach", "!", "schwer", "mit", "dem", "To\u00b7de", "ge"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "ITJ", "$.", "ADJD", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}