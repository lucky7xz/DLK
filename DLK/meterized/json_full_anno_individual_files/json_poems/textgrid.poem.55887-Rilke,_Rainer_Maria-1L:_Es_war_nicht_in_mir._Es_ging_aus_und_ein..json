{"textgrid.poem.55887": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war nicht in mir. Es ging aus und ein.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war nicht in mir. Es ging aus und ein.", "tokens": ["Es", "war", "nicht", "in", "mir", ".", "Es", "ging", "aus", "und", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "PPER", "$.", "PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da wollt ich es halten. Da hielt es der Wein.", "tokens": ["Da", "wollt", "ich", "es", "hal\u00b7ten", ".", "Da", "hielt", "es", "der", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "(ich wei\u00df nicht mehr was es war.)", "tokens": ["(", "ich", "wei\u00df", "nicht", "mehr", "was", "es", "war", ".", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "ADV", "PWS", "PPER", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann hielt er mir jenes und hielt mir dies", "tokens": ["Dann", "hielt", "er", "mir", "je\u00b7nes", "und", "hielt", "mir", "dies"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PDAT", "KON", "VVFIN", "PPER", "PDS"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "bis ich mich ganz auf ihn verlie\u00df.", "tokens": ["bis", "ich", "mich", "ganz", "auf", "ihn", "ver\u00b7lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Ich Narr.", "tokens": ["Ich", "Narr", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NE", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Jetzt bin ich in seinem Spiel und er streut", "tokens": ["Jetzt", "bin", "ich", "in", "sei\u00b7nem", "Spiel", "und", "er", "streut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "KON", "PPER", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "mich ver\u00e4chtlich herum und verliert mich noch heut", "tokens": ["mich", "ver\u00b7\u00e4cht\u00b7lich", "he\u00b7rum", "und", "ver\u00b7liert", "mich", "noch", "heut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "PTKVZ", "KON", "VVFIN", "PPER", "ADV", "ADV"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "an dieses Vieh, an den Tod.", "tokens": ["an", "die\u00b7ses", "Vieh", ",", "an", "den", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wenn der mich, schmutzige Karte, gewinnt,", "tokens": ["Wenn", "der", "mich", ",", "schmut\u00b7zi\u00b7ge", "Kar\u00b7te", ",", "ge\u00b7winnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relax