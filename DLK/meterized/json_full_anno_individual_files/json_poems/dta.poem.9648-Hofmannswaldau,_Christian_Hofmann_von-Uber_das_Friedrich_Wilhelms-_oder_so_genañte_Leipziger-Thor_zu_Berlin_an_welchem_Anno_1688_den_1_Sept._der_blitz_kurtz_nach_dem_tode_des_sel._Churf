{"dta.poem.9648": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uber das Friedrich Wilhelms- oder so genan\u0303te  \n Leipziger-Thor zu Berlin/ an welchem Anno 1688  \n den 1 Sept. der blitz kurtz nach dem tode des sel. Churf\u00fcrstens/  \n von der ihm zu ehren gesetzten \u00fcberschrifft/ diese  \n worte ber\u00fchret hatte:  \n  FRIDERICVS, ELECTOR, FELIX.  \n  Friedrich/ Churf\u00fcrst/ gl\u00fcckseelig.  \n \u2020 \u2020 \u2020", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der blitz ber\u00fchrte j\u00fcngst die eine von den pforten/", "tokens": ["Der", "blitz", "be\u00b7r\u00fchr\u00b7te", "j\u00fcngst", "die", "ei\u00b7ne", "von", "den", "pfor\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ART", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und traff die \u00fcberschrifft am Friedrich Wilhelms-thor.", "tokens": ["Und", "traff", "die", "\u00fc\u00b7bersc\u00b7hrifft", "am", "Fried\u00b7rich", "Wil\u00b7helms\u00b7thor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "VVFIN", "APPRART", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der vorwitz ist besorgt/ ob den gestreifften worten;", "tokens": ["Der", "vor\u00b7witz", "ist", "be\u00b7sorgt", "/", "ob", "den", "ge\u00b7streiff\u00b7ten", "wor\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was aber stellt man sich f\u00fcr fremde deutung vor?", "tokens": ["Was", "a\u00b7ber", "stellt", "man", "sich", "f\u00fcr", "frem\u00b7de", "deu\u00b7tung", "vor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PIS", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der held/ dem di\u00df geweiht/ ist aus der welt gewichen/", "tokens": ["Der", "held", "/", "dem", "di\u00df", "ge\u00b7weiht", "/", "ist", "aus", "der", "welt", "ge\u00b7wi\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "ART", "PDS", "VVPP", "$(", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So ", "tokens": ["So"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Und hat/ zu unserm trost/ hingegen unterstrichen/", "tokens": ["Und", "hat", "/", "zu", "un\u00b7serm", "trost", "/", "hin\u00b7ge\u00b7gen", "un\u00b7ter\u00b7stri\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$(", "APPR", "PPOSAT", "NN", "$(", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was uns von ihm verbleibt. Welch h\u00f6chst erw\u00fcnschte wahl!", "tokens": ["Was", "uns", "von", "ihm", "ver\u00b7bleibt", ".", "Welch", "h\u00f6chst", "er\u00b7w\u00fcnschte", "wahl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVFIN", "$.", "PIAT", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Sein \u2020 Friedrich/ Chur und gl\u00fcck ist uns zur\u00fcck geblieben;", "tokens": ["Sein", "\u2020", "Fried\u00b7rich", "/", "Chur", "und", "gl\u00fcck", "ist", "uns", "zu\u00b7r\u00fcck", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$(", "NE", "$(", "NN", "KON", "ADJD", "VAFIN", "PPER", "PTKVZ", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "Wer glaubt dem himmel nicht/ der dieses unterschrieben?", "tokens": ["Wer", "glaubt", "dem", "him\u00b7mel", "nicht", "/", "der", "die\u00b7ses", "un\u00b7ter\u00b7schrie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKNEG", "$(", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}