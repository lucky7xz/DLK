{"textgrid.poem.64195": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weit \u00fcber die Heide bl\u00e4st der Wind", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weit \u00fcber die Heide bl\u00e4st der Wind", "tokens": ["Weit", "\u00fc\u00b7ber", "die", "Hei\u00b7de", "bl\u00e4st", "der", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und es nicken die Halme, so viel ihrer sind,", "tokens": ["Und", "es", "ni\u00b7cken", "die", "Hal\u00b7me", ",", "so", "viel", "ih\u00b7rer", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "ADV", "ADV", "PPOSAT", "VAFIN", "$,"], "meter": "--+--+--++-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und die grauen Wolken jagen geschwind: \u2013", "tokens": ["Und", "die", "grau\u00b7en", "Wol\u00b7ken", "ja\u00b7gen", "ge\u00b7schwind", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADJD", "$.", "$("], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Da kommt es gewandert, das Heidekind.", "tokens": ["Da", "kommt", "es", "ge\u00b7wan\u00b7dert", ",", "das", "Hei\u00b7de\u00b7kind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ihr rotes Gelock um den Nacken ihr fliegt,", "tokens": ["Ihr", "ro\u00b7tes", "Ge\u00b7lock", "um", "den", "Na\u00b7cken",