{"textgrid.poem.48369": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Lord Maxwells Lebewohl", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbleb wohl, leb wohl, liebe Mutter mein,", "tokens": ["\u00bb", "leb", "wohl", ",", "leb", "wohl", ",", "lie\u00b7be", "Mut\u00b7ter", "mein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "$,", "VVFIN", "ADV", "$,", "ADJA", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und leb wohl, meines Vaters Haus,", "tokens": ["Und", "leb", "wohl", ",", "mei\u00b7nes", "Va\u00b7ters", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lebt wohl, es soll geschieden sein,", "tokens": ["Lebt", "wohl", ",", "es", "soll", "ge\u00b7schie\u00b7den", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich mu\u00df in die See hinaus;", "tokens": ["Ich", "mu\u00df", "in", "die", "See", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Leb wohl, du Garten im Sonnenschein,", "tokens": ["Leb", "wohl", ",", "du", "Gar\u00b7ten", "im", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Drin die Maiengl\u00f6ckchen stehn,", "tokens": ["Drin", "die", "Mai\u00b7en\u00b7gl\u00f6\u00b7ck\u00b7chen", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Und vor allem leb wohl, liebe Lady mein,", "tokens": ["Und", "vor", "al\u00b7lem", "leb", "wohl", ",", "lie\u00b7be", "La\u00b7dy", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "ADV", "$,", "VVFIN", "NE", "PPOSAT", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Ich mu\u00df von dannen gehn.", "tokens": ["Ich", "mu\u00df", "von", "dan\u00b7nen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Lord Johnston erschlug ich am Wege hier,", "tokens": ["Lord", "John\u00b7ston", "er\u00b7schlug", "ich", "am", "We\u00b7ge", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es war eine dunkle Nacht,", "tokens": ["Es", "war", "ei\u00b7ne", "dunk\u00b7le", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Lord Johnston erschlug meinen Vater mir,", "tokens": ["Lord", "John\u00b7ston", "er\u00b7schlug", "mei\u00b7nen", "Va\u00b7ter", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und so hab ich's quitt gemacht;", "tokens": ["Und", "so", "hab", "ich's", "quitt", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drei Jahre harrt' ich bei Nacht, bei Tag,", "tokens": ["Drei", "Jah\u00b7re", "harrt'", "ich", "bei", "Nacht", ",", "bei", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.6": {"text": "Meinen Vater ger\u00e4cht zu sehn,", "tokens": ["Mei\u00b7nen", "Va\u00b7ter", "ge\u00b7r\u00e4cht", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Ich hab' nicht Reu, was kommen mag,", "tokens": ["Ich", "hab'", "nicht", "Reu", ",", "was", "kom\u00b7men", "mag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NE", "$,", "PWS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aber von dir mu\u00df ich gehn.", "tokens": ["A\u00b7ber", "von", "dir", "mu\u00df", "ich", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und h\u00e4tt' ich Reu, ich d\u00e4cht' an den Tag,", "tokens": ["Und", "h\u00e4tt'", "ich", "Reu", ",", "ich", "d\u00e4cht'", "an", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NE", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der wie gestern vor mir steht,", "tokens": ["Der", "wie", "ge\u00b7stern", "vor", "mir", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wo mein Vater auf seinen Knieen lag", "tokens": ["Wo", "mein", "Va\u00b7ter", "auf", "sei\u00b7nen", "Kni\u00b7e\u00b7en", "lag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und die Johnstons um Gnade gefleht;", "tokens": ["Und", "die", "John\u00b7stons", "um", "Gna\u00b7de", "ge\u00b7fleht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "APPR", "NN", "VVPP", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Sie hieben ihm ab die flehende Hand,", "tokens": ["Sie", "hie\u00b7ben", "ihm", "ab", "die", "fle\u00b7hen\u00b7de", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Geschehn ist, was geschehn,", "tokens": ["Ge\u00b7schehn", "ist", ",", "was", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nun mu\u00df ich lassen Lieb' und Land", "tokens": ["Nun", "mu\u00df", "ich", "las\u00b7sen", "Lieb'", "und", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und, lieb' Lady, von dir gehn.", "tokens": ["Und", ",", "lieb'", "La\u00b7dy", ",", "von", "dir", "gehn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "NE", "$,", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Leb wohl, Carnarven, mein Fels, mein Schlo\u00df,", "tokens": ["Leb", "wohl", ",", "Car\u00b7nar\u00b7ven", ",", "mein", "Fels", ",", "mein", "Schlo\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "NE", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Leb wohl auf manches Jahr,", "tokens": ["Leb", "wohl", "auf", "man\u00b7ches", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Leb wohl, du Wald, du stiller Geno\u00df,", "tokens": ["Leb", "wohl", ",", "du", "Wald", ",", "du", "stil\u00b7ler", "Ge\u00b7no\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Darinnen ich gl\u00fccklich war,", "tokens": ["Da\u00b7rin\u00b7nen", "ich", "gl\u00fcck\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Leb wohl, Lochmabens Birkenhain,", "tokens": ["Leb", "wohl", ",", "Loch\u00b7ma\u00b7bens", "Bir\u00b7ken\u00b7hain", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und du Platz, wo die Tannen stehn,", "tokens": ["Und", "du", "Platz", ",", "wo", "die", "Tan\u00b7nen", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und vor allem leb wohl, lieb' Lady mein,", "tokens": ["Und", "vor", "al\u00b7lem", "leb", "wohl", ",", "lieb'", "La\u00b7dy", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "ADV", "$,", "VVFIN", "NE", "PPOSAT", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Denn ich mu\u00df von dir gehn.\u00ab", "tokens": ["Denn", "ich", "mu\u00df", "von", "dir", "gehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie hielt ihn an ihr Herz gepre\u00dft:", "tokens": ["Sie", "hielt", "ihn", "an", "ihr", "Herz", "ge\u00b7pre\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbbleib hier und bleibe mir!", "tokens": ["\u00bb", "bleib", "hier", "und", "blei\u00b7be", "mir", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Meines Bruders Schlo\u00df ist stark und fest", "tokens": ["Mei\u00b7nes", "Bru\u00b7ders", "Schlo\u00df", "ist", "stark", "und", "fest"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und doppelt fest mit dir;", "tokens": ["Und", "dop\u00b7pelt", "fest", "mit", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Hamiltons und die Douglas beid',", "tokens": ["Die", "Ha\u00b7mil\u00b7tons", "und", "die", "Doug\u00b7las", "beid'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie werden zu uns stehn \u2013\u00ab", "tokens": ["Sie", "wer\u00b7den", "zu", "uns", "stehn", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVINF", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbes bricht mein Herz in Weh und Leid,", "tokens": ["\u00bb", "es", "bricht", "mein", "Herz", "in", "Weh", "und", "Leid", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aber von dir mu\u00df ich gehn.\u00ab", "tokens": ["A\u00b7ber", "von", "dir", "mu\u00df", "ich", "gehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "PPER", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Er nahm einen Ring, an dem Ringe hing", "tokens": ["Er", "nahm", "ei\u00b7nen", "Ring", ",", "an", "dem", "Rin\u00b7ge", "hing"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein Kreuz von rotem Stein:", "tokens": ["Ein", "Kreuz", "von", "ro\u00b7tem", "Stein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbnimm hin den Ring und trage den Ring", "tokens": ["\u00bb", "nimm", "hin", "den", "Ring", "und", "tra\u00b7ge", "den", "Ring"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "ADV", "ART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und vor allem gedenke mein,", "tokens": ["Und", "vor", "al\u00b7lem", "ge\u00b7den\u00b7ke", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "PPOSAT", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Denn ach, verg\u00e4\u00dfest du mich je,", "tokens": ["Denn", "ach", ",", "ver\u00b7g\u00e4\u00b7\u00dfest", "du", "mich", "je", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um nach andrem Gl\u00fcck zu sehn,", "tokens": ["Um", "nach", "an\u00b7drem", "Gl\u00fcck", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "R\u00fcck fl\u00f6g' ich \u00fcber die sch\u00e4umende See,", "tokens": ["R\u00fcck", "fl\u00f6g'", "ich", "\u00fc\u00b7ber", "die", "sch\u00e4u\u00b7men\u00b7de", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "Und um alles w\u00e4r' es geschehn.\u00ab", "tokens": ["Und", "um", "al\u00b7les", "w\u00e4r'", "es", "ge\u00b7schehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "PIS", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Der Tag war grau, das Deck war klar,", "tokens": ["Der", "Tag", "war", "grau", ",", "das", "Deck", "war", "klar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lord Maxwell ging zu Schiff,", "tokens": ["Lord", "Max\u00b7well", "ging", "zu", "Schiff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Wind in allen Segeln war,", "tokens": ["Der", "Wind", "in", "al\u00b7len", "Se\u00b7geln", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bootsmannspfeife pfiff;", "tokens": ["Die", "Boots\u00b7manns\u00b7pfei\u00b7fe", "pfiff", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Streifen schwand das Ufer jetzt,", "tokens": ["Ein", "Strei\u00b7fen", "schwand", "das", "U\u00b7fer", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbade!\u00ab Die See ging hohl,", "tokens": ["\u00bb", "a\u00b7de", "!", "\u00ab", "Die", "See", "ging", "hohl", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$.", "$(", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und Wind und Wogen verschlangen zuletzt", "tokens": ["Und", "Wind", "und", "Wo\u00b7gen", "ver\u00b7schlan\u00b7gen", "zu\u00b7letzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Lord Maxwells Lebewohl.", "tokens": ["Lord", "Max\u00b7wells", "Le\u00b7be\u00b7wohl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbleb wohl, leb wohl, liebe Mutter mein,", "tokens": ["\u00bb", "leb", "wohl", ",", "leb", "wohl", ",", "lie\u00b7be", "Mut\u00b7ter", "mein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "$,", "VVFIN", "ADV", "$,", "ADJA", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und leb wohl, meines Vaters Haus,", "tokens": ["Und", "leb", "wohl", ",", "mei\u00b7nes", "Va\u00b7ters", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lebt wohl, es soll geschieden sein,", "tokens": ["Lebt", "wohl", ",", "es", "soll", "ge\u00b7schie\u00b7den", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich mu\u00df in die See hinaus;", "tokens": ["Ich", "mu\u00df", "in", "die", "See", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Leb wohl, du Garten im Sonnenschein,", "tokens": ["Leb", "wohl", ",", "du", "Gar\u00b7ten", "im", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Drin die Maiengl\u00f6ckchen stehn,", "tokens": ["Drin", "die", "Mai\u00b7en\u00b7gl\u00f6\u00b7ck\u00b7chen", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Und vor allem leb wohl, liebe Lady mein,", "tokens": ["Und", "vor", "al\u00b7lem", "leb", "wohl", ",", "lie\u00b7be", "La\u00b7dy", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "ADV", "$,", "VVFIN", "NE", "PPOSAT", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Ich mu\u00df von dannen gehn.", "tokens": ["Ich", "mu\u00df", "von", "dan\u00b7nen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Lord Johnston erschlug ich am Wege hier,", "tokens": ["Lord", "John\u00b7ston", "er\u00b7schlug", "ich", "am", "We\u00b7ge", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es war eine dunkle Nacht,", "tokens": ["Es", "war", "ei\u00b7ne", "dunk\u00b7le", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Lord Johnston erschlug meinen Vater mir,", "tokens": ["Lord", "John\u00b7ston", "er\u00b7schlug", "mei\u00b7nen", "Va\u00b7ter", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und so hab ich's quitt gemacht;", "tokens": ["Und", "so", "hab", "ich's", "quitt", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drei Jahre harrt' ich bei Nacht, bei Tag,", "tokens": ["Drei", "Jah\u00b7re", "harrt'", "ich", "bei", "Nacht", ",", "bei", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.6": {"text": "Meinen Vater ger\u00e4cht zu sehn,", "tokens": ["Mei\u00b7nen", "Va\u00b7ter", "ge\u00b7r\u00e4cht", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Ich hab' nicht Reu, was kommen mag,", "tokens": ["Ich", "hab'", "nicht", "Reu", ",", "was", "kom\u00b7men", "mag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NE", "$,", "PWS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aber von dir mu\u00df ich gehn.", "tokens": ["A\u00b7ber", "von", "dir", "mu\u00df", "ich", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und h\u00e4tt' ich Reu, ich d\u00e4cht' an den Tag,", "tokens": ["Und", "h\u00e4tt'", "ich", "Reu", ",", "ich", "d\u00e4cht'", "an", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NE", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der wie gestern vor mir steht,", "tokens": ["Der", "wie", "ge\u00b7stern", "vor", "mir", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wo mein Vater auf seinen Knieen lag", "tokens": ["Wo", "mein", "Va\u00b7ter", "auf", "sei\u00b7nen", "Kni\u00b7e\u00b7en", "lag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und die Johnstons um Gnade gefleht;", "tokens": ["Und", "die", "John\u00b7stons", "um", "Gna\u00b7de", "ge\u00b7fleht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "APPR", "NN", "VVPP", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Sie hieben ihm ab die flehende Hand,", "tokens": ["Sie", "hie\u00b7ben", "ihm", "ab", "die", "fle\u00b7hen\u00b7de", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Geschehn ist, was geschehn,", "tokens": ["Ge\u00b7schehn", "ist", ",", "was", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nun mu\u00df ich lassen Lieb' und Land", "tokens": ["Nun", "mu\u00df", "ich", "las\u00b7sen", "Lieb'", "und", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und, lieb' Lady, von dir gehn.", "tokens": ["Und", ",", "lieb'", "La\u00b7dy", ",", "von", "dir", "gehn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "NE", "$,", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Leb wohl, Carnarven, mein Fels, mein Schlo\u00df,", "tokens": ["Leb", "wohl", ",", "Car\u00b7nar\u00b7ven", ",", "mein", "Fels", ",", "mein", "Schlo\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "NE", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Leb wohl auf manches Jahr,", "tokens": ["Leb", "wohl", "auf", "man\u00b7ches", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Leb wohl, du Wald, du stiller Geno\u00df,", "tokens": ["Leb", "wohl", ",", "du", "Wald", ",", "du", "stil\u00b7ler", "Ge\u00b7no\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Darinnen ich gl\u00fccklich war,", "tokens": ["Da\u00b7rin\u00b7nen", "ich", "gl\u00fcck\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Leb wohl, Lochmabens Birkenhain,", "tokens": ["Leb", "wohl", ",", "Loch\u00b7ma\u00b7bens", "Bir\u00b7ken\u00b7hain", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und du Platz, wo die Tannen stehn,", "tokens": ["Und", "du", "Platz", ",", "wo", "die", "Tan\u00b7nen", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und vor allem leb wohl, lieb' Lady mein,", "tokens": ["Und", "vor", "al\u00b7lem", "leb", "wohl", ",", "lieb'", "La\u00b7dy", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "ADV", "$,", "VVFIN", "NE", "PPOSAT", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Denn ich mu\u00df von dir gehn.\u00ab", "tokens": ["Denn", "ich", "mu\u00df", "von", "dir", "gehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Sie hielt ihn an ihr Herz gepre\u00dft:", "tokens": ["Sie", "hielt", "ihn", "an", "ihr", "Herz", "ge\u00b7pre\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbbleib hier und bleibe mir!", "tokens": ["\u00bb", "bleib", "hier", "und", "blei\u00b7be", "mir", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Meines Bruders Schlo\u00df ist stark und fest", "tokens": ["Mei\u00b7nes", "Bru\u00b7ders", "Schlo\u00df", "ist", "stark", "und", "fest"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und doppelt fest mit dir;", "tokens": ["Und", "dop\u00b7pelt", "fest", "mit", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Hamiltons und die Douglas beid',", "tokens": ["Die", "Ha\u00b7mil\u00b7tons", "und", "die", "Doug\u00b7las", "beid'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie werden zu uns stehn \u2013\u00ab", "tokens": ["Sie", "wer\u00b7den", "zu", "uns", "stehn", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVINF", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbes bricht mein Herz in Weh und Leid,", "tokens": ["\u00bb", "es", "bricht", "mein", "Herz", "in", "Weh", "und", "Leid", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aber von dir mu\u00df ich gehn.\u00ab", "tokens": ["A\u00b7ber", "von", "dir", "mu\u00df", "ich", "gehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "PPER", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Er nahm einen Ring, an dem Ringe hing", "tokens": ["Er", "nahm", "ei\u00b7nen", "Ring", ",", "an", "dem", "Rin\u00b7ge", "hing"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein Kreuz von rotem Stein:", "tokens": ["Ein", "Kreuz", "von", "ro\u00b7tem", "Stein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbnimm hin den Ring und trage den Ring", "tokens": ["\u00bb", "nimm", "hin", "den", "Ring", "und", "tra\u00b7ge", "den", "Ring"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "ADV", "ART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und vor allem gedenke mein,", "tokens"