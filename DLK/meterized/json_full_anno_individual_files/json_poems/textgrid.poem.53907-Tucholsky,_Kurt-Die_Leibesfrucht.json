{"textgrid.poem.53907": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Leibesfrucht", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist so schwer, du bist so bla\u00df \u2013", "tokens": ["Du", "bist", "so", "schwer", ",", "du", "bist", "so", "bla\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "was hast du, Mutter?", "tokens": ["was", "hast", "du", ",", "Mut\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Du willst etwas und wei\u00dft nicht was \u2013", "tokens": ["Du", "willst", "et\u00b7was", "und", "wei\u00dft", "nicht", "was", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "KON", "VVFIN", "PTKNEG", "PWS", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "was hast du, Mutter?", "tokens": ["was", "hast", "du", ",", "Mut\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "\u00bbich trag in meinem Leibe ein Kind;", "tokens": ["\u00bb", "ich", "trag", "in", "mei\u00b7nem", "Lei\u00b7be", "ein", "Kind", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "ich wei\u00df, wie seine Geschwister sind:", "tokens": ["ich", "wei\u00df", ",", "wie", "sei\u00b7ne", "Ge\u00b7schwis\u00b7ter", "sind", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "ohne Stiefel, ohne Wolle, ohne Milch, ohne Butter \u2013", "tokens": ["oh\u00b7ne", "Stie\u00b7fel", ",", "oh\u00b7ne", "Wol\u00b7le", ",", "oh\u00b7ne", "Milch", ",", "oh\u00b7ne", "But\u00b7ter", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,", "KOUI", "NN", "$,", "KOUI", "NN", "$("], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "ich bin eine Mutter! Ich will keine Mutter mehr sein!", "tokens": ["ich", "bin", "ei\u00b7ne", "Mut\u00b7ter", "!", "Ich", "will", "kei\u00b7ne", "Mut\u00b7ter", "mehr", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PPER", "VMFIN", "PIAT", "NN", "ADV", "VAINF", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.9": {"text": "La\u00df mich schrein \u2013!", "tokens": ["La\u00df", "mich", "schrein", "\u2013", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "$(", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "La\u00df mich schrein \u2013!\u00ab", "tokens": ["La\u00df", "mich", "schrein", "\u2013", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "$(", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Es darf und darf mir nicht zur Welt!", "tokens": ["Es", "darf", "und", "darf", "mir", "nicht", "zur", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbfrau, was wollen Sie?\u00ab", "tokens": ["\u00bb", "frau", ",", "was", "wol\u00b7len", "Sie", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "PWS", "VMFIN", "PPER", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mein Mann ohne Stellung \u2013 wir haben kein Geld!", "tokens": ["Mein", "Mann", "oh\u00b7ne", "Stel\u00b7lung", "\u2013", "wir", "ha\u00b7ben", "kein", "Geld", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$(", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "\u00bbfrau, was wollen Sie?\u00ab", "tokens": ["\u00bb", "frau", ",", "was", "wol\u00b7len", "Sie", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "PWS", "VMFIN", "PPER", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ich will nicht, da\u00df man f\u00fcr eine Nacht", "tokens": ["Ich", "will", "nicht", ",", "da\u00df", "man", "f\u00fcr", "ei\u00b7ne", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "$,", "KOUS", "PIS", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "mich und die Kinder ungl\u00fccklich macht!", "tokens": ["mich", "und", "die", "Kin\u00b7der", "un\u00b7gl\u00fcck\u00b7lich", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Dieselben Rechte will ich wie die Reichen,", "tokens": ["Die\u00b7sel\u00b7ben", "Rech\u00b7te", "will", "ich", "wie", "die", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VMFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "die ungestraft zum Abtreiber schleichen \u2013", "tokens": ["die", "un\u00b7ge\u00b7straft", "zum", "Ab\u00b7trei\u00b7ber", "schlei\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Warum will mich denn keiner befrein?", "tokens": ["Wa\u00b7rum", "will", "mich", "denn", "kei\u00b7ner", "be\u00b7fr\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "La\u00dft mich schrein \u2013!", "tokens": ["La\u00dft", "mich", "schrein", "\u2013", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "$(", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "La\u00dft mich schrein \u2013!", "tokens": ["La\u00dft", "mich", "schrein", "\u2013", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "$(", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Mit Schreien ist da nichts getan \u2013", "tokens": ["Mit", "Schrei\u00b7en", "ist", "da", "nichts", "ge\u00b7tan", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADV", "PIS", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wacht auf, ihr Frauen!", "tokens": ["Wacht", "auf", ",", "ihr", "Frau\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Nieder mit kirchlichem Gr\u00f6\u00dfenwahn!", "tokens": ["Nie\u00b7der", "mit", "kirch\u00b7li\u00b7chem", "Gr\u00f6\u00b7\u00dfen\u00b7wahn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wacht auf, ihr Frauen!", "tokens": ["Wacht", "auf", ",", "ihr", "Frau\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Ihr kr\u00fcmmt euch vor Schmerzen, und in euer Ohr", "tokens": ["Ihr", "kr\u00fcmmt", "euch", "vor", "Schmer\u00b7zen", ",", "und", "in", "eu\u00b7er", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "t\u00f6nt heulend der Untemehmerchor:", "tokens": ["t\u00f6nt", "heu\u00b7lend", "der", "Un\u00b7te\u00b7meh\u00b7mer\u00b7chor", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "\u00bbtrag es aus! Trag es aus!", "tokens": ["\u00bb", "trag", "es", "aus", "!", "Trag", "es", "aus", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PTKVZ", "$.", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Trag es aus im Sturmgebraus!", "tokens": ["Trag", "es", "aus", "im", "Sturm\u00b7ge\u00b7braus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn der Staat bleibt bestehn,", "tokens": ["Wenn", "der", "Staat", "bleibt", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "k\u00f6nnt ihr alle zugrunde gehn!", "tokens": ["k\u00f6nnt", "ihr", "al\u00b7le", "zu\u00b7grun\u00b7de", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "Ihr habt nichts zu fressen?", "tokens": ["Ihr", "habt", "nichts", "zu", "fres\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Wir brauchen die Kinder f\u00fcr Dortmund und Essen,", "tokens": ["Wir", "brau\u00b7chen", "die", "Kin\u00b7der", "f\u00fcr", "Dort\u00b7mund", "und", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "f\u00fcr die Reichswehr und f\u00fcr die B\u00fcros \u2013", "tokens": ["f\u00fcr", "die", "Reichs\u00b7wehr", "und", "f\u00fcr", "die", "B\u00fc\u00b7ros", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "und wenn ihr krepiert, dann sind wir euch los!\u00ab", "tokens": ["und", "wenn", "ihr", "kre\u00b7piert", ",", "dann", "sind", "wir", "euch", "los", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Aus Jodoform und blutigem Leinen", "tokens": ["Aus", "Jo\u00b7do\u00b7form", "und", "blu\u00b7ti\u00b7gem", "Lei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "ADJA", "NN"], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "kommt winselnd eines Kindes Weinen.", "tokens": ["kommt", "win\u00b7selnd", "ei\u00b7nes", "Kin\u00b7des", "Wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es wartet an dem kleinen Bett", "tokens": ["Es", "war\u00b7tet", "an", "dem", "klei\u00b7nen", "Bett"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "bereits ein m\u00e4chtiges Quartett:", "tokens": ["be\u00b7reits", "ein", "m\u00e4ch\u00b7ti\u00b7ges", "Quar\u00b7tett", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fabrik. Finanzamt. Schwindsucht. Kirchenzucht.", "tokens": ["Fab\u00b7rik", ".", "Fi\u00b7nanz\u00b7amt", ".", "Schwind\u00b7sucht", ".", "Kir\u00b7chen\u00b7zucht", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Das ist das Schicksal einer deutschen Leibesfrucht.", "tokens": ["Das", "ist", "das", "Schick\u00b7sal", "ei\u00b7ner", "deut\u00b7schen", "Lei\u00b7bes\u00b7frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du bist so schwer, du bist so bla\u00df \u2013", "tokens": ["Du", "bist", "so", "schwer", ",", "du", "bist", "so", "bla\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "was hast du, Mutter?", "tokens": ["was", "hast", "du", ",", "Mut\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Du willst etwas und wei\u00dft nicht was \u2013", "tokens": ["Du", "willst", "et\u00b7was", "und", "wei\u00dft", "nicht", "was", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "KON", "VVFIN", "PTKNEG", "PWS", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "was hast du, Mutter?", "tokens": ["was", "hast", "du", ",", "Mut\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "\u00bbich trag in meinem Leibe ein Kind;", "tokens": ["\u00bb", "ich", "trag", "in", "mei\u00b7nem", "Lei\u00b7be", "ein", "Kind", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "ich wei\u00df, wie seine Geschwister sind:", "tokens": ["ich", "wei\u00df", ",", "wie", "sei\u00b7ne", "Ge\u00b7schwis\u00b7ter", "sind", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "ohne Stiefel, ohne Wolle, ohne Milch, ohne Butter \u2013", "tokens": ["oh\u00b7ne", "Stie\u00b7fel", ",", "oh\u00b7ne", "Wol\u00b7le", ",", "oh\u00b7ne", "Milch", ",", "oh\u00b7ne", "But\u00b7ter", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,", "KOUI", "NN", "$,", "KOUI", "NN", "$("], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "ich bin eine Mutter! Ich will keine Mutter mehr sein!", "tokens": ["ich", "bin", "ei\u00b7ne", "Mut\u00b7ter", "!", "Ich", "will", "kei\u00b7ne", "Mut\u00b7ter", "mehr", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PPER", "VMFIN", "PIAT", "NN", "ADV", "VAINF", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.9": {"text": "La\u00df mich schrein \u2013!", "tokens": ["La\u00df", "mich", "schrein", "\u2013", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "$(", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "La\u00df mich schrein \u2013!\u00ab", "tokens": ["La\u00df", "mich", "schrein", "\u2013", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "$(", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Es darf und