{"dta.poem.8766": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Christnacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.71", "en:0.28"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Fromme Gluth entfache", "tokens": ["From\u00b7me", "Gluth", "ent\u00b7fa\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Jedes Herz gelind,", "tokens": ["Je\u00b7des", "Herz", "ge\u00b7lind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Eilt nach jenem Dache,", "tokens": ["Eilt", "nach", "je\u00b7nem", "Da\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Betet an das Kind!", "tokens": ["Be\u00b7tet", "an", "das", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Jener hei\u00dferflehte", "tokens": ["Je\u00b7ner", "hei\u00b7\u00dfer\u00b7fleh\u00b7te"], "token_info": ["word", "word"], "pos": ["PDAT", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Hort der Menschen lebt,", "tokens": ["Hort", "der", "Men\u00b7schen", "lebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der euch im Gebete", "tokens": ["Der", "euch", "im", "Ge\u00b7be\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Lange vorgeschwebt.", "tokens": ["Lan\u00b7ge", "vor\u00b7ge\u00b7schwebt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Traun! Die Macht des B\u00f6sen", "tokens": ["Traun", "!", "Die", "Macht", "des", "B\u00f6\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sinkt nun fort und fort,", "tokens": ["Sinkt", "nun", "fort", "und", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Jener wird erl\u00f6sen", "tokens": ["Je\u00b7ner", "wird", "er\u00b7l\u00f6\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "VAFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Durch das Eine Wort.", "tokens": ["Durch", "das", "Ei\u00b7ne", "Wort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}