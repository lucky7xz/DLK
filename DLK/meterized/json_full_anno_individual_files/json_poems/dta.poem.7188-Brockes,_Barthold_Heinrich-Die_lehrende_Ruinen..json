{"dta.poem.7188": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die lehrende Ruinen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch ein fataler Fall, welch schreckliches Exempel!", "tokens": ["Welch", "ein", "fa\u00b7ta\u00b7ler", "Fall", ",", "welch", "schreck\u00b7li\u00b7ches", "Ex\u00b7em\u00b7pel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$,", "PWAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der k\u00f6stlich-pr\u00e4chtige Sophien-Tempel,", "tokens": ["Der", "k\u00f6st\u00b7lich\u00b7pr\u00e4ch\u00b7ti\u00b7ge", "So\u00b7phien\u00b7Tem\u00b7pel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein Wunder-Werck der neuen Zeit,", "tokens": ["Ein", "Wun\u00b7der\u00b7\u00b7Werck", "der", "neu\u00b7en", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Heiligkeit, an Pracht, an Kunst und Kostbarkeit,", "tokens": ["An", "Hei\u00b7lig\u00b7keit", ",", "an", "Pracht", ",", "an", "Kunst", "und", "Kost\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist Mahometh geweyht.", "tokens": ["Ist", "Ma\u00b7ho\u00b7me\u00b7th", "ge\u00b7weyht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Von T\u00fcrckschen Seuffzern dampfft und d\u00fcnstet der Altar,", "tokens": ["Von", "T\u00fcrck\u00b7schen", "Seuff\u00b7zern", "dampfft", "und", "d\u00fcns\u00b7tet", "der", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der ehemahls den Christen heilig war.", "tokens": ["Der", "e\u00b7he\u00b7mahls", "den", "Chris\u00b7ten", "hei\u00b7lig", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Erschrecklich-strenge Macht der R\u00e4uberischen Zeit!", "tokens": ["Er\u00b7schreck\u00b7lich\u00b7stren\u00b7ge", "Macht", "der", "R\u00e4u\u00b7be\u00b7ri\u00b7schen", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was l\u00e4sset uns dein Grimm f\u00fcr wilde Proben sehen!", "tokens": ["Was", "l\u00e4s\u00b7set", "uns", "dein", "Grimm", "f\u00fcr", "wil\u00b7de", "Pro\u00b7ben", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NE", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von welcher grausen Kostbarkeit", "tokens": ["Von", "wel\u00b7cher", "grau\u00b7sen", "Kost\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sind deine gr\u00e4\u00dfliche Tropheen!", "tokens": ["Sind", "dei\u00b7ne", "gr\u00e4\u00df\u00b7li\u00b7che", "Tro\u00b7pheen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sie hauchen w\u00fcrcklich noch aus Schutt und Graus", "tokens": ["Sie", "hau\u00b7chen", "w\u00fcrck\u00b7lich", "noch", "aus", "Schutt", "und", "Graus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "So Majest\u00e4t als Ehrfurcht aus.", "tokens": ["So", "Ma\u00b7jes\u00b7t\u00e4t", "als", "Ehr\u00b7furcht", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Von ihrem Uber-Rest pr\u00e4gt ein bemoster Stein", "tokens": ["Von", "ih\u00b7rem", "U\u00b7ber\u00b7Rest", "pr\u00e4gt", "ein", "be\u00b7mos\u00b7ter", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Zugleich Erstaunen, Gram, Verwund\u2019rung, Mitleid, ein.", "tokens": ["Zu\u00b7gleich", "Er\u00b7stau\u00b7nen", ",", "Gram", ",", "Ver\u00b7wun\u00b7d'\u00b7rung", ",", "Mit\u00b7leid", ",", "ein", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Ja eine Lehre selbst find\u2019 ich daran geschrieben,", "tokens": ["Ja", "ei\u00b7ne", "Leh\u00b7re", "selbst", "find'", "ich", "da\u00b7ran", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "ADV", "VVFIN", "PPER", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die auch im Schutt und Graus selbst unversehrt geblieben,", "tokens": ["Die", "auch", "im", "Schutt", "und", "Graus", "selbst", "un\u00b7ver\u00b7sehrt", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "KON", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ja die so gar in ihrem Sturtz und Fall", "tokens": ["Ja", "die", "so", "gar", "in", "ih\u00b7rem", "Sturtz", "und", "Fall"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ART", "ADV", "ADV", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Recht ausgedr\u00fcckt, und \u00fcberall", "tokens": ["Recht", "aus\u00b7ge\u00b7dr\u00fcckt", ",", "und", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$,", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Erst deutlich vorgestellt,", "tokens": ["Erst", "deut\u00b7lich", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Die jedem, der es liest, gleich in die Augen f\u00e4llt:", "tokens": ["Die", "je\u00b7dem", ",", "der", "es", "liest", ",", "gleich", "in", "die", "Au\u00b7gen", "f\u00e4llt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der Menschen Werck ist, wie er selber, nichtig,", "tokens": ["Der", "Men\u00b7schen", "Werck", "ist", ",", "wie", "er", "sel\u00b7ber", ",", "nich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$,", "PWAV", "PPER", "ADV", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Verg\u00e4nglich, wandelbar und fl\u00fcchtig.", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich", ",", "wan\u00b7del\u00b7bar", "und", "fl\u00fcch\u00b7tig", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}