{"dta.poem.8430": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n rechte Gebrauch des Lebens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Wie Pfeile Gottes! Jeder Sekundenschlag", "tokens": ["Wie", "Pfei\u00b7le", "Got\u00b7tes", "!", "Je\u00b7der", "Se\u00b7kun\u00b7den\u00b7schlag"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "NN", "NN", "$.", "PIAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Reisst uns dem Sterbebette n\u00e4her,", "tokens": ["Reisst", "uns", "dem", "Ster\u00b7be\u00b7bet\u00b7te", "n\u00e4\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "N\u00e4her dem eisernen Todesschlafe!", "tokens": ["N\u00e4\u00b7her", "dem", "ei\u00b7ser\u00b7nen", "To\u00b7des\u00b7schla\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Dir bl\u00fcht kein Fr\u00fchling, wann du gestorben bist;", "tokens": ["Dir", "bl\u00fcht", "kein", "Fr\u00fch\u00b7ling", ",", "wann", "du", "ge\u00b7stor\u00b7ben", "bist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dir weht kein Schatten, t\u00f6net kein Becherklang;", "tokens": ["Dir", "weht", "kein", "Schat\u00b7ten", ",", "t\u00f6\u00b7net", "kein", "Be\u00b7cher\u00b7klang", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dir lacht kein s\u00fcsses M\u00e4dchenl\u00e4cheln,", "tokens": ["Dir", "lacht", "kein", "s\u00fcs\u00b7ses", "M\u00e4d\u00b7chen\u00b7l\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Str\u00f6met kein Scherz von des Freundes Lippe!", "tokens": ["Str\u00f6\u00b7met", "kein", "Scherz", "von", "des", "Freun\u00b7des", "Lip\u00b7pe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Noch rauscht der schwarze Fl\u00fcgel des Todes nicht!", "tokens": ["Noch", "rauscht", "der", "schwar\u00b7ze", "Fl\u00fc\u00b7gel", "des", "To\u00b7des", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Drum hasch die Freuden, eh sie der Sturm verweht,", "tokens": ["Drum", "hasch", "die", "Freu\u00b7den", ",", "eh", "sie", "der", "Sturm", "ver\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Gott, wie Sonnenschein und Regen,", "tokens": ["Die", "Gott", ",", "wie", "Son\u00b7nen\u00b7schein", "und", "Re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus der vergeudenden Urne sch\u00fcttet!", "tokens": ["Aus", "der", "ver\u00b7geu\u00b7den\u00b7den", "Ur\u00b7ne", "sch\u00fct\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein froher Abend, welchen der heitre Scherz", "tokens": ["Ein", "fro\u00b7her", "A\u00b7bend", ",", "wel\u00b7chen", "der", "heit\u00b7re", "Scherz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PWAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Der Freundschaft fl\u00fcgelt, oder das Deckelglas;", "tokens": ["Der", "Freund\u00b7schaft", "fl\u00fc\u00b7gelt", ",", "o\u00b7der", "das", "De\u00b7ckel\u00b7glas", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein Kuss auf deines M\u00e4dchens Wangen,", "tokens": ["Ein", "Kuss", "auf", "dei\u00b7nes", "M\u00e4d\u00b7chens", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Oder auf ihren gehobnen Busen;", "tokens": ["O\u00b7der", "auf", "ih\u00b7ren", "ge\u00b7hob\u00b7nen", "Bu\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Ein Gang im Gr\u00fcnen, wann du, o Nachtigall,", "tokens": ["Ein", "Gang", "im", "Gr\u00fc\u00b7nen", ",", "wann", "du", ",", "o", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "PWAV", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dein s\u00fcsses Mailied durch die Gestr\u00e4uche t\u00f6nst,", "tokens": ["Dein", "s\u00fcs\u00b7ses", "Mai\u00b7lied", "durch", "die", "Ge\u00b7str\u00e4u\u00b7che", "t\u00f6nst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "W\u00e4gt jeden Kranz des Nachruhms nieder,", "tokens": ["W\u00e4gt", "je\u00b7den", "Kranz", "des", "Nach\u00b7ruhms", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den sich der Held und der Weise wanden!", "tokens": ["Den", "sich", "der", "Held", "und", "der", "Wei\u00b7se", "wan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Der Kuss, den mir die bl\u00fchende Tochter giebt,", "tokens": ["Der", "Kuss", ",", "den", "mir", "die", "bl\u00fc\u00b7hen\u00b7de", "Toch\u00b7ter", "giebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ist s\u00fcsser, als die K\u00fcsse der Enkelin,", "tokens": ["Ist", "s\u00fcs\u00b7ser", ",", "als", "die", "K\u00fcs\u00b7se", "der", "En\u00b7ke\u00b7lin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die sie dem kalten H\u00fcgel opfert,", "tokens": ["Die", "sie", "dem", "kal\u00b7ten", "H\u00fc\u00b7gel", "op\u00b7fert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo ich den eisernen Schlummer schlafe.", "tokens": ["Wo", "ich", "den", "ei\u00b7ser\u00b7nen", "Schlum\u00b7mer", "schla\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}