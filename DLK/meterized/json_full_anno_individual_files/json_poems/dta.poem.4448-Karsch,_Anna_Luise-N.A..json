{"dta.poem.4448": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "So w\u00e4r auch ich verwildert; aber Deine,", "tokens": ["So", "w\u00e4r", "auch", "ich", "ver\u00b7wil\u00b7dert", ";", "a\u00b7ber", "Dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "VVPP", "$.", "KON", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von einem Gott gelenkte, rechte Freundes Hand,", "tokens": ["Von", "ei\u00b7nem", "Gott", "ge\u00b7lenk\u00b7te", ",", "rech\u00b7te", "Freun\u00b7des", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zog mich zum grossen Sitz des K\u00f6niges, der seine", "tokens": ["Zog", "mich", "zum", "gros\u00b7sen", "Sitz", "des", "K\u00f6\u00b7ni\u00b7ges", ",", "der", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gecr\u00f6nte Schl\u00e4fe gr\u00fcn umwand.", "tokens": ["Ge\u00b7cr\u00f6n\u00b7te", "Schl\u00e4\u00b7fe", "gr\u00fcn", "um\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}