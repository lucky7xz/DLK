{"dta.poem.9667": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff einen tadler.  \n * * v. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jenes maul in deinen augen ist zu gro\u00df und di\u00df zu klein/", "tokens": ["Je\u00b7nes", "maul", "in", "dei\u00b7nen", "au\u00b7gen", "ist", "zu", "gro\u00df", "und", "di\u00df", "zu", "klein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "KON", "PDS", "PTKA", "ADJD", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Diese hat zu grobe h\u00e4nde/ jene solten weisser seyn;", "tokens": ["Die\u00b7se", "hat", "zu", "gro\u00b7be", "h\u00e4n\u00b7de", "/", "je\u00b7ne", "sol\u00b7ten", "weis\u00b7ser", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "NN", "$(", "PDS", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ja fast alles/ was nicht sch\u00f6n/ kanstu wohl an andern kennen/", "tokens": ["Ja", "fast", "al\u00b7les", "/", "was", "nicht", "sch\u00f6n", "/", "kans\u00b7tu", "wohl", "an", "an\u00b7dern", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "PIS", "$(", "PWS", "PTKNEG", "ADJD", "$(", "VMFIN", "ADV", "APPR", "PIS", "VVINF", "$("], "meter": "-+---+-+-+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Nur dich selber kennst du nicht/ weil dich alle/ tadler/ nennen.", "tokens": ["Nur", "dich", "sel\u00b7b