-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Leib, ein Herz, eine einzige Demokratie:", "tokens": ["Leib", ",", "ein", "Herz", ",", "ei\u00b7ne", "ein\u00b7zi\u00b7ge", "De\u00b7mo\u00b7kra\u00b7tie", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "\u00bballons, enfants de la patrie \u2013!\u00ab", "tokens": ["\u00bb", "al\u00b7lons", ",", "en\u00b7fants", "de", "la", "pa\u00b7trie", "\u2013", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PIS", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$(", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hunderttausend Willen sind in meinem \u2013", "tokens": ["Hun\u00b7dert\u00b7tau\u00b7send", "Wil\u00b7len", "sind", "in", "mei\u00b7nem", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "APPR", "PPOSAT", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "aber ich bin mehr als die Hunderttausend.", "tokens": ["a\u00b7ber", "ich", "bin", "mehr", "als", "die", "Hun\u00b7dert\u00b7tau\u00b7send", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "KOKOM", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Tausend Gesichter habe ich und habe doch kein Gesicht.", "tokens": ["Tau\u00b7send", "Ge\u00b7sich\u00b7ter", "ha\u00b7be", "ich", "und", "ha\u00b7be", "doch", "kein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "KON", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "+--+-+-+-+-