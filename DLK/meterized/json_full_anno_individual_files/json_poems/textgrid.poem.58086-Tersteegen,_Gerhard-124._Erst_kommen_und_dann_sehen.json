{"textgrid.poem.58086": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "124. Erst kommen und dann sehen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott wohnet au\u00dfer Ort und Zeit,", "tokens": ["Gott", "woh\u00b7net", "au\u00b7\u00dfer", "Ort", "und", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Kreatur und Sinnen weit,", "tokens": ["Von", "Kre\u00b7a\u00b7tur", "und", "Sin\u00b7nen", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Still in sich selbst, in sanftem Frieden;", "tokens": ["Still", "in", "sich", "selbst", ",", "in", "sanf\u00b7tem", "Frie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "ADV", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Willst du ihn schauen, frommer Christ,", "tokens": ["Willst", "du", "ihn", "schau\u00b7en", ",", "from\u00b7mer", "Christ", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So mu\u00dft du kommen, wo er ist,", "tokens": ["So", "mu\u00dft", "du", "kom\u00b7men", ",", "wo", "er", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und werden auch so abgeschieden.", "tokens": ["Und", "wer\u00b7den", "auch", "so", "ab\u00b7ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gott wohnet au\u00dfer Ort und Zeit,", "tokens": ["Gott", "woh\u00b7net", "au\u00b7\u00dfer", "Ort", "und", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Kreatur und Sinnen weit,", "tokens": ["Von", "Kre\u00b7a\u00b7tur", "und", "Sin\u00b7nen", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Still in sich selbst, in sanftem Frieden;", "tokens": ["Still", "in", "sich", "selbst", ",", "in", "sanf\u00b7tem", "Frie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "ADV", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Willst du ihn schauen, frommer Christ,", "tokens": ["Willst", "du", "ihn", "schau\u00b7en", ",", "from\u00b7mer", "Christ", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So mu\u00dft du kommen, wo er ist,", "tokens": ["So", "mu\u00dft", "du", "kom\u00b7men", ",", "wo", "er", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measu