{"dta.poem.613": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Eben dasselbige Hochzeit-Fest wurde im  \n Nahmen eines andern beehret mit folgenden", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wer sagt/ da\u00df Cypripor mit seinen Liebes-Kertzen", "tokens": ["Wer", "sagt", "/", "da\u00df", "Cyp\u00b7ri\u00b7por", "mit", "sei\u00b7nen", "Lie\u00b7bes\u00b7Kert\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "NE", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur rauhen Winters-Zeit kein Hertz entz\u00fcnden kan/", "tokens": ["Zur", "rau\u00b7hen", "Win\u00b7ter\u00b7sZeit", "kein", "Hertz", "ent\u00b7z\u00fcn\u00b7den", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PIAT", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der trifft das Wider-Spiel bey diesem Eh-Paar an/", "tokens": ["Der", "trifft", "das", "Wi\u00b7der\u00b7Spiel", "bey", "die\u00b7sem", "Eh\u00b7Paar", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es steht im vollem Feur/ und die verliebten Hertzen", "tokens": ["Es", "steht", "im", "vol\u00b7lem", "Feur", "/", "und", "die", "ver\u00b7lieb\u00b7ten", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$(", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sieht man vor Liebes-Glut in reinen Flammen schertzen/", "tokens": ["Sieht", "man", "vor", "Lie\u00b7bes\u00b7Glut", "in", "rei\u00b7nen", "Flam\u00b7men", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wozu der Himmel selbst den Zunder hergethan/", "tokens": ["Wo\u00b7zu", "der", "Him\u00b7mel", "selbst", "den", "Zun\u00b7der", "her\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Anmuth f\u00fchret es auf einer Nelcken Bahn/", "tokens": ["Die", "An\u00b7muth", "f\u00fch\u00b7ret", "es", "auf", "ei\u00b7ner", "Nel\u00b7cken", "Bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und die Vergn\u00fcgung rufft/ es weichen alle Schmertzen.", "tokens": ["Und", "die", "Ver\u00b7gn\u00fc\u00b7gung", "rufft", "/", "es", "wei\u00b7chen", "al\u00b7le", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$(", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So brenn denn immerfort jung- und begl\u00fccktes Paar/", "tokens": ["So", "brenn", "denn", "im\u00b7mer\u00b7fort", "jung", "und", "be\u00b7gl\u00fcck\u00b7tes", "Paar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "TRUNC", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df aus den Gluthen mag manch junger ", "tokens": ["Da\u00df", "aus", "den", "Glu\u00b7then", "mag", "manch", "jun\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "VMFIN", "PIAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der seinen Ursprung her aus deiner Brunst genommen.", "tokens": ["Der", "sei\u00b7nen", "Ur\u00b7sprung", "her", "aus", "dei\u00b7ner", "Brunst", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APZR", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Leb\u2019 immerfort begl\u00fcckt ohn Ungl\u00fcck und Gefahr.", "tokens": ["Leb'", "im\u00b7mer\u00b7fort", "be\u00b7gl\u00fcckt", "ohn", "Un\u00b7gl\u00fcck", "und", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Damit du dermahleins magst gantz vergn\u00fcget sehen", "tokens": ["Da\u00b7mit", "du", "der\u00b7mah\u00b7leins", "magst", "gantz", "ver\u00b7gn\u00fc\u00b7get", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDS", "VMFIN", "ADV", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Fr\u00fcchte deiner Lust/ in gleichen Flammen stehen.", "tokens": ["Die", "Fr\u00fcch\u00b7te", "dei\u00b7ner", "Lust", "/", "in", "glei\u00b7chen", "Flam\u00b7men", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}