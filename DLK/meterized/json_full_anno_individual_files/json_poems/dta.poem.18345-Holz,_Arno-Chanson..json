{"dta.poem.18345": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Chanson.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Der alten Welt ins Genick,", "tokens": ["Der", "al\u00b7ten", "Welt", "ins", "Ge\u00b7nick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Noch immer schmort ihr kein Huhn im Topf,", "tokens": ["Noch", "im\u00b7mer", "schmort", "ihr", "kein", "Huhn", "im", "Topf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Drum: ", "tokens": ["Drum", ":"], "token_info": ["word", "punct"], "pos": ["PAV", "$."], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Drum: ", "tokens": ["Drum", ":"], "token_info": ["word", "punct"], "pos": ["PAV", "$."], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Das Herz schl\u00e4gt uns im Bauch,", "tokens": ["Das", "Herz", "schl\u00e4gt", "uns", "im", "Bauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Knutenthum haben wir dick, dick, dick,", "tokens": ["Das", "Knu\u00b7ten\u00b7thum", "ha\u00b7ben", "wir", "dick", ",", "dick", ",", "dick", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-----+-+", "measure": "dactylic.init"}, "line.4": {"text": "Und Kartoffel und H\u00e4ring auch!", "tokens": ["Und", "Kar\u00b7tof\u00b7fel", "und", "H\u00e4\u00b7ring", "auch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}