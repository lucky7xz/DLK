{"dta.poem.23569": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Dunkelmannstracht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unsre Freuden, unsre Leiden", "tokens": ["Uns\u00b7re", "Freu\u00b7den", ",", "uns\u00b7re", "Lei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wollen wir in Schwarz nur kleiden;", "tokens": ["Wol\u00b7len", "wir", "in", "Schwarz", "nur", "klei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwarz ist Anstand \u00fcberall", "tokens": ["Schwarz", "ist", "An\u00b7stand", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bei dem Grab und auf dem Ball.", "tokens": ["Bei", "dem", "Grab", "und", "auf", "dem", "Ball", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Tragt die Nacht nicht am Gewande,", "tokens": ["Tragt", "die", "Nacht", "nicht", "am", "Ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jagt sie lieber aus dem Lande!", "tokens": ["Jagt", "sie", "lie\u00b7ber", "aus", "dem", "Lan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Finsterni\u00df und Traurigkeit", "tokens": ["Fins\u00b7ter\u00b7ni\u00df", "und", "Trau\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Herrscht genug in unsrer Zeit.", "tokens": ["Herrscht", "ge\u00b7nug", "in", "uns\u00b7rer", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nach dem Sprichwort unsrer Alten", "tokens": ["Nach", "dem", "Sprich\u00b7wort", "uns\u00b7rer", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sollet ihr auf Farbe halten.", "tokens": ["Sol\u00b7let", "ihr", "auf", "Far\u00b7be", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kleidet euch in Sonnenschein!", "tokens": ["Klei\u00b7det", "euch", "in", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nacht stellt sich von selber ein.", "tokens": ["Nacht", "stellt", "sich", "von", "sel\u00b7ber", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ADV", "PTKVZ", "$."], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}}}}}