{"dta.poem.19494": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Neujahrsglocken  .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "In den L\u00fcften schwellendes Gedr\u00f6hne,", "tokens": ["In", "den", "L\u00fcf\u00b7ten", "schwel\u00b7len\u00b7des", "Ge\u00b7dr\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Leicht wie Halme beugt der Wind die T\u00f6ne.", "tokens": ["Leicht", "wie", "Hal\u00b7me", "beugt", "der", "Wind", "die", "T\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Leis verhallen die zum ersten riefen,", "tokens": ["Leis", "ver\u00b7hal\u00b7len", "die", "zum", "ers\u00b7ten", "rie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Neu Gel\u00e4ute hebt sich aus den Tiefen.", "tokens": ["Neu", "Ge\u00b7l\u00e4u\u00b7te", "hebt", "sich", "aus", "den", "Tie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Gro\u00dfe Heere, nicht ein einzler Rufer!", "tokens": ["Gro\u00b7\u00dfe", "Hee\u00b7re", ",", "nicht", "ein", "einz\u00b7ler", "Ru\u00b7fer", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wohllaut fluthet ohne Strand und Ufer.", "tokens": ["Wohl\u00b7laut", "flut\u00b7het", "oh\u00b7ne", "Strand", "und", "U\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}