{"dta.poem.1235": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Pruna manu pron\u00e2 pariter prunasque  \n dedisti.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Pflaumen hast du mit der Hand/ Flam\u0303en aber auch gegeben;", "tokens": ["Pflau\u00b7men", "hast", "du", "mit", "der", "Hand", "/", "Flam\u0303en", "a\u00b7ber", "auch", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$(", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Diese dringen uns ins Hertz/ jene f\u00fcllen unsern Mund.", "tokens": ["Die\u00b7se", "drin\u00b7gen", "uns", "ins", "Hertz", "/", "je\u00b7ne", "f\u00fcl\u00b7len", "un\u00b7sern", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "$(", "PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Pflaumen hat der Baum gebracht/ Flam\u0303 und Brand von Aug", "tokens": ["Pflau\u00b7men", "hat", "der", "Baum", "ge\u00b7bracht", "/", "Flam\u0303", "und", "Brand", "von", "Aug"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "VVPP", "$(", "NN", "KON", "NN", "APPR", "NN"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "entstund/", "tokens": ["ent\u00b7stund", "/"], "token_info": ["word", "punct"], "pos": ["KON", "$("], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Jene streifft der Reiff zwar ab/ diese Glutt wird ewig leben.", "tokens": ["Je\u00b7ne", "streifft", "der", "Reiff", "zwar", "ab", "/", "die\u00b7se", "Glutt", "wird", "e\u00b7wig", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$(", "PDAT", "NN", "VAFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}