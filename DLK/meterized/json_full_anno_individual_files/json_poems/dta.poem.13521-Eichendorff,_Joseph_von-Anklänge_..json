{"dta.poem.13521": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ankl\u00e4nge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "V\u00f6glein in den sonn'gen Tagen!", "tokens": ["V\u00f6\u00b7glein", "in", "den", "sonn'\u00b7gen", "Ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00fcfte blau, die mich verf\u00fchren!", "tokens": ["L\u00fcf\u00b7te", "blau", ",", "die", "mich", "ver\u00b7f\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt' ich bunte Fl\u00fcgel r\u00fchren,", "tokens": ["K\u00f6nnt'", "ich", "bun\u00b7te", "Fl\u00fc\u00b7gel", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber Berg und Wald sie schlagen!", "tokens": ["Ue\u00b7ber", "Berg", "und", "Wald", "sie", "schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}