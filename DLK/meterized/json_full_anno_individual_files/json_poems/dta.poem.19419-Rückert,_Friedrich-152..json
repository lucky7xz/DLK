{"dta.poem.19419": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "152.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch ist nicht so schlimm als seine Thaten zeigen,", "tokens": ["Der", "Mensch", "ist", "nicht", "so", "schlimm", "als", "sei\u00b7ne", "Tha\u00b7ten", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "KOKOM", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn seine Thaten sind zum kleinsten Theil ihm eigen.", "tokens": ["Denn", "sei\u00b7ne", "Tha\u00b7ten", "sind", "zum", "kleins\u00b7ten", "Theil", "ihm", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPRART", "ADJA", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nimmst du die Zuthat weg von Zufall, Unverstand,", "tokens": ["Nimmst", "du", "die", "Zut\u00b7hat", "weg", "von", "Zu\u00b7fall", ",", "Un\u00b7ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nachl\u00e4ssigkeit; was bleibt als That der freien Hand?", "tokens": ["Nach\u00b7l\u00e4s\u00b7sig\u00b7keit", ";", "was", "bleibt", "als", "That", "der", "frei\u00b7en", "Hand", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VVFIN", "KOUS", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nichts B\u00f6ses \u00fcberhaupt thut er villeicht aus Trieb", "tokens": ["Nichts", "B\u00f6\u00b7ses", "\u00fc\u00b7ber\u00b7haupt", "thut", "er", "vil\u00b7leicht", "aus", "Trieb"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "NN", "ADV", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum B\u00f6sen, sondern weil zu thun nichts andres blieb.", "tokens": ["Zum", "B\u00f6\u00b7sen", ",", "son\u00b7dern", "weil", "zu", "thun", "nichts", "and\u00b7res", "blieb", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "KOUS", "PTKZU", "VVINF", "PIS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "La\u00dft ihn das Gute thun, gebt ihm zum Guten Raum;", "tokens": ["La\u00dft", "ihn", "das", "Gu\u00b7te", "thun", ",", "gebt", "ihm", "zum", "Gu\u00b7ten", "Raum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$,", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und B\u00f6ses dann zu thun f\u00e4llt ihm nicht ein im Traum.", "tokens": ["Und", "B\u00f6\u00b7ses", "dann", "zu", "thun", "f\u00e4llt", "ihm", "nicht", "ein", "im", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "PPER", "PTKNEG", "ART", "APPRART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}