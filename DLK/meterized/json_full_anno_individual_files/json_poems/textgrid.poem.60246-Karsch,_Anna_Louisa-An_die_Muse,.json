{"textgrid.poem.60246": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "An die Muse,", "genre": "verse", "period": "N.A.", "pub_year": 1763, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die du das Feld des Krieges \u00fcberflogest,", "tokens": ["Die", "du", "das", "Feld", "des", "Krie\u00b7ges", "\u00fc\u00b7berf\u00b7lo\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Schwefelduft und Kugelregen drangst,", "tokens": ["Durch", "Schwe\u00b7fel\u00b7duft", "und", "Ku\u00b7gel\u00b7re\u00b7gen", "drangst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Sieger, und mit ihm durch Ehrenpforten zogest,", "tokens": ["Zum", "Sie\u00b7ger", ",", "und", "mit", "ihm", "durch", "Eh\u00b7ren\u00b7pfor\u00b7ten", "zo\u00b7gest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und seines Einzugs Jubel sangst,", "tokens": ["Und", "sei\u00b7nes", "Ein\u00b7zugs", "Ju\u00b7bel", "sangst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Muse! singe nun auch k\u00fchn den stolzen Abend,", "tokens": ["O", "Mu\u00b7se", "!", "sin\u00b7ge", "nun", "auch", "k\u00fchn", "den", "stol\u00b7zen", "A\u00b7bend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "ADV", "ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der von des Sturmwinds Fl\u00fcgeln lo\u00df,", "tokens": ["Der", "von", "des", "Sturm\u00b7winds", "Fl\u00fc\u00b7geln", "lo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Sternenmantel um sich habend,", "tokens": ["Den", "Ster\u00b7nen\u00b7man\u00b7tel", "um", "sich", "ha\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Herabsah, auf ", "tokens": ["Her\u00b7ab\u00b7sah", ",", "auf"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und seinen Frieden ewig nannte,", "tokens": ["Und", "sei\u00b7nen", "Frie\u00b7den", "e\u00b7wig", "nann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und, von der Kunst beflammt, den Sph\u00e4ren \u00e4hnlich brannte,", "tokens": ["Und", ",", "von", "der", "Kunst", "be\u00b7flammt", ",", "den", "Sph\u00e4\u00b7ren", "\u00e4hn\u00b7lich", "brann\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ganz \u00e4hnlich jenem Pomp der pr\u00e4chtigen Natur.", "tokens": ["Ganz", "\u00e4hn\u00b7lich", "je\u00b7nem", "Pomp", "der", "pr\u00e4ch\u00b7ti\u00b7gen", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PDAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hast du sie nachgez\u00e4hlt, die hundert tausend Flammen,", "tokens": ["Hast", "du", "sie", "nach\u00b7ge\u00b7z\u00e4hlt", ",", "die", "hun\u00b7dert", "tau\u00b7send", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVPP", "$,", "PRELS", "CARD", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch deren Glanz der Sieger fuhr?", "tokens": ["Durch", "de\u00b7ren", "Glanz", "der", "Sie\u00b7ger", "fuhr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sanftl\u00e4chelnd, wie sein Gott, wenn, auf der Weizen-Flur", "tokens": ["Sanft\u00b7l\u00e4\u00b7chelnd", ",", "wie", "sein", "Gott", ",", "wenn", ",", "auf", "der", "Wei\u00b7zen\u00b7Flur"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "PWAV", "PPOSAT", "NN", "$,", "KOUS", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von tausend Schnittern froh zusammen", "tokens": ["Von", "tau\u00b7send", "Schnit\u00b7tern", "froh", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Stimmen mischen sich in ein harmonisch Lied,", "tokens": ["Die", "Stim\u00b7men", "mi\u00b7schen", "sich", "in", "ein", "har\u00b7mo\u00b7nisch", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und jeder Busen dankbar gl\u00fcht!", "tokens": ["Und", "je\u00b7der", "Bu\u00b7sen", "dank\u00b7bar", "gl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und jeder Blick emporgehoben,", "tokens": ["Und", "je\u00b7der", "Blick", "em\u00b7por\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Erndtegeber w\u00fcnscht zu sehen und zu loben,", "tokens": ["Den", "Ernd\u00b7te\u00b7ge\u00b7ber", "w\u00fcnscht", "zu", "se\u00b7hen", "und", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und seiner G\u00fcte Bild weit ausgebreitet sieht:", "tokens": ["Und", "sei\u00b7ner", "G\u00fc\u00b7te", "Bild", "weit", "aus\u00b7ge\u00b7brei\u00b7tet", "sieht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "ADJD", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So ausgebreitet, also m\u00e4chtig fortgerissen,", "tokens": ["So", "aus\u00b7ge\u00b7brei\u00b7tet", ",", "al\u00b7so", "m\u00e4ch\u00b7tig", "fort\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drang Freude sich von Brust zu Brust;", "tokens": ["Drang", "Freu\u00b7de", "sich", "von", "Brust", "zu", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PRF", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es staunte, trunken von des Patrioten Lust,", "tokens": ["Es", "staun\u00b7te", ",", "trun\u00b7ken", "von", "des", "Pat\u00b7ri\u00b7o\u00b7ten", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Auge, wenn den Hercul der besch\u00fctzten Preussen,", "tokens": ["Das", "Au\u00b7ge", ",", "wenn", "den", "Her\u00b7cul", "der", "be\u00b7sch\u00fctz\u00b7ten", "Preus\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Amphitrions Sohn mi\u00dfg\u00fcnstig vor sich sah,", "tokens": ["Des", "Am\u00b7phi\u00b7tri\u00b7ons", "Sohn", "mi\u00df\u00b7g\u00fcns\u00b7tig", "vor", "sich", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wild, trozend, stand das Bild des Stierbek\u00e4mpfers da,", "tokens": ["Wild", ",", "tro\u00b7zend", ",", "stand", "das", "Bild", "des", "Stier\u00b7be\u00b7k\u00e4mp\u00b7fers", "da", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVPP", "$,", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und schwang, mit Riesenarm die knotenreiche Keule;", "tokens": ["Und", "schwang", ",", "mit", "Rie\u00b7se\u00b7narm", "die", "kno\u00b7ten\u00b7rei\u00b7che", "Keu\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du glaubtest, da\u00df der Schlag geschah,", "tokens": ["Du", "glaub\u00b7test", ",", "da\u00df", "der", "Schlag", "ge\u00b7schah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und bebtest vor dem Zungenpfeile", "tokens": ["Und", "beb\u00b7test", "vor", "dem", "Zun\u00b7gen\u00b7pfei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Des siebenk\u00f6pfigen, gekr\u00fcmmten, schlangengleich", "tokens": ["Des", "sie\u00b7ben\u00b7k\u00f6p\u00b7fi\u00b7gen", ",", "ge\u00b7kr\u00fcmm\u00b7ten", ",", "schlan\u00b7gen\u00b7gleich"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "VVPP", "$,", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Geformten Thieres, das ihm drohte,", "tokens": ["Ge\u00b7form\u00b7ten", "Thie\u00b7res", ",", "das", "ihm", "droh\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und gr\u00f6\u00dfer wuchs nach jedem Streich;", "tokens": ["Und", "gr\u00f6\u00b7\u00dfer", "wuchs", "nach", "je\u00b7dem", "Streich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "O! dich erschreckte selbst die todte", "tokens": ["O", "!", "dich", "er\u00b7schreck\u00b7te", "selbst", "die", "tod\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Giftlose Hyder, an dem Fu\u00df", "tokens": ["Gift\u00b7lo\u00b7se", "Hy\u00b7der", ",", "an", "dem", "Fu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Des Halbgotts, der sich tief verbeugen", "tokens": ["Des", "Halb\u00b7gotts", ",", "der", "sich", "tief", "ver\u00b7beu\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Vor ", "tokens": ["Vor"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Wenn Malerei und Dichtkunst schweigen;", "tokens": ["Wenn", "Ma\u00b7le\u00b7rei", "und", "Dicht\u00b7kunst", "schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So redet von dem Vater mehr, als von dem Held,", "tokens": ["So", "re\u00b7det", "von", "dem", "Va\u00b7ter", "mehr", ",", "als", "von", "dem", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADV", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein Landvolk, das ein \u00f6des Feld", "tokens": ["Sein", "Land\u00b7volk", ",", "das", "ein", "\u00f6\u00b7des", "Feld"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun wieder tief in Furchen ziehet;", "tokens": ["Nun", "wie\u00b7der", "tief", "in", "Fur\u00b7chen", "zie\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Aus Vorrathsh\u00e4usern Korn empf\u00e4ngt;", "tokens": ["Aus", "Vor\u00b7raths\u00b7h\u00e4u\u00b7sern", "Korn", "emp\u00b7f\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und H\u00fctten, die der Feind versengt,", "tokens": ["Und", "H\u00fct\u00b7ten", ",", "die", "der", "Feind", "ver\u00b7sengt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Aus ihrer Asche steigen siehet.", "tokens": ["Aus", "ih\u00b7rer", "A\u00b7sche", "stei\u00b7gen", "sie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihm t\u00f6net Lob der Mildigkeit", "tokens": ["Ihm", "t\u00f6\u00b7net", "Lob", "der", "Mil\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Erhabner, aus der Kinder Munde,", "tokens": ["Er\u00b7hab\u00b7ner", ",", "aus", "der", "Kin\u00b7der", "Mun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Als Siegsges\u00e4nge, nach dem Streit,", "tokens": ["Als", "Siegs\u00b7ge\u00b7s\u00e4n\u00b7ge", ",", "nach", "dem", "Streit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der jedes Lorbeerblatt erkauft mit einer Wunde", "tokens": ["Der", "je\u00b7des", "Lor\u00b7beer\u00b7blatt", "er\u00b7kauft", "mit", "ei\u00b7ner", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Des K\u00f6niglichen Herzens hat.", "tokens": ["Des", "K\u00f6\u00b7nig\u00b7li\u00b7chen", "Her\u00b7zens", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O Muse! h\u00f6rst du nicht das arme Volk der Stadt?", "tokens": ["O", "Mu\u00b7se", "!", "h\u00f6rst", "du", "nicht", "das", "ar\u00b7me", "Volk", "der", "Stadt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es jauchzt, und tanzt