{"dta.poem.17547": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "168.  Der S\u00fcnder thut nichts gut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch speise wen du wilt/ zeuch tausend Armen an;", "tokens": ["Mensch", "spei\u00b7se", "wen", "du", "wilt", "/", "zeuch", "tau\u00b7send", "Ar\u00b7men", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PWS", "PPER", "VMFIN", "$(", "VVIMP", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo du ein S\u00fcnder bist/ du hast nicht wol gethan.", "tokens": ["Wo", "du", "ein", "S\u00fcn\u00b7der", "bist", "/", "du", "hast", "nicht", "wol", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VAFIN", "$(", "PPER", "VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}