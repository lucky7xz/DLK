{"textgrid.poem.36856": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Danklied", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist's, dem Ruhm und Ehre geb\u00fchret;", "tokens": ["Du", "bist's", ",", "dem", "Ruhm", "und", "Eh\u00b7re", "ge\u00b7b\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Ruhm und Ehre bring ich dir.", "tokens": ["Und", "Ruhm", "und", "Eh\u00b7re", "bring", "ich", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du, Herr, hast stets mein Schicksal regieret,", "tokens": ["Du", ",", "Herr", ",", "hast", "stets", "mein", "Schick\u00b7sal", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und deine Hand war \u00fcber mir.", "tokens": ["Und", "dei\u00b7ne", "Hand", "war", "\u00fc\u00b7ber", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn Not zu meiner H\u00fctte sich nahte:", "tokens": ["Wenn", "Not", "zu", "mei\u00b7ner", "H\u00fct\u00b7te", "sich", "nah\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So h\u00f6rte Gott, der Herr, mein Flehn,", "tokens": ["So", "h\u00f6r\u00b7te", "Gott", ",", "der", "Herr", ",", "mein", "Flehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lie\u00df, nach seinem gn\u00e4digen Rate,", "tokens": ["Und", "lie\u00df", ",", "nach", "sei\u00b7nem", "gn\u00e4\u00b7di\u00b7gen", "Ra\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mich nicht in meiner Not vergehn.", "tokens": ["Mich", "nicht", "in", "mei\u00b7ner", "Not", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich sank in Schmerz und Krankheit danieder,", "tokens": ["Ich", "sank", "in", "Schmerz", "und", "Krank\u00b7heit", "da\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "PAV", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und rief: O Herr, errette mich!", "tokens": ["Und", "rief", ":", "O", "Herr", ",", "er\u00b7ret\u00b7te", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da half mir Gott, der M\u00e4chtige, wieder,", "tokens": ["Da", "half", "mir", "Gott", ",", "der", "M\u00e4ch\u00b7ti\u00b7ge", ",", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "ART", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und mein Gebein erfreute sich.", "tokens": ["Und", "mein", "Ge\u00b7bein", "er\u00b7freu\u00b7te", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn mich der Ha\u00df des Feindes betr\u00fcbte:", "tokens": ["Wenn", "mich", "der", "Ha\u00df", "des", "Fein\u00b7des", "be\u00b7tr\u00fcb\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Klagt ich Gott kindlich meinen Schmerz.", "tokens": ["Klagt", "ich", "Gott", "kind\u00b7lich", "mei\u00b7nen", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er half, da\u00df ich nicht Rache ver\u00fcbte,", "tokens": ["Er", "half", ",", "da\u00df", "ich", "nicht", "Ra\u00b7che", "ver\u00b7\u00fcb\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und st\u00e4rkte durch Geduld mein Herz.", "tokens": ["Und", "st\u00e4rk\u00b7te", "durch", "Ge\u00b7duld", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn ich, verirrt vom richtigen Pfade,", "tokens": ["Wenn", "ich", ",", "ver\u00b7irrt", "vom", "rich\u00b7ti\u00b7gen", "Pfa\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit S\u00fcnde mich umfangen sah:", "tokens": ["Mit", "S\u00fcn\u00b7de", "mich", "um\u00b7fan\u00b7gen", "sah", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Rief ich zu ihm, dem Vater der Gnade;", "tokens": ["Rief", "ich", "zu", "ihm", ",", "dem", "Va\u00b7ter", "der", "Gna\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und seine Gnade war mir nah.", "tokens": ["Und", "sei\u00b7ne", "Gna\u00b7de", "war", "mir", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Um Trost war meiner Seele so bange;", "tokens": ["Um", "Trost", "war", "mei\u00b7ner", "See\u00b7le", "so", "ban\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Denn Gott verbarg sein Angesicht.", "tokens": ["Denn", "Gott", "ver\u00b7barg", "sein", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich rief zu ihm: Ach Herr, wie so lange?", "tokens": ["Ich", "rief", "zu", "ihm", ":", "Ach", "Herr", ",", "wie", "so", "lan\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$.", "ITJ", "NN", "$,", "PWAV", "ADV", "ADV", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Gott verlie\u00df den Schwachen nicht.", "tokens": ["Und", "Gott", "ver\u00b7lie\u00df", "den", "Schwa\u00b7chen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er half, und wird mich ferner erl\u00f6sen,", "tokens": ["Er", "half", ",", "und", "wird", "mich", "fer\u00b7ner", "er\u00b7l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er hilft; der Herr ist fromm und gut.", "tokens": ["Er", "hilft", ";", "der", "Herr", "ist", "fromm", "und", "gut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hilft aus der Versuchung zum B\u00f6sen,", "tokens": ["Er", "hilft", "aus", "der", "Ver\u00b7su\u00b7chung", "zum", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und gibt mir zu der Tugend Mut.", "tokens": ["Und", "gibt", "mir", "zu", "der", "Tu\u00b7gend", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dir dank ich f\u00fcr die Pr\u00fcfung der Leiden,", "tokens": ["Dir", "dank", "ich", "f\u00fcr", "die", "Pr\u00fc\u00b7fung", "der", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+----+--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Die du mir liebreich zugeschickt.", "tokens": ["Die", "du", "mir", "lieb\u00b7reich", "zu\u00b7ge\u00b7schickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir dank ich f\u00fcr die h\u00e4ufigern Freuden,", "tokens": ["Dir", "dank", "ich", "f\u00fcr", "die", "h\u00e4u\u00b7fi\u00b7gern", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Womit mich deine Hand begl\u00fcckt.", "tokens": ["Wo\u00b7mit", "mich", "dei\u00b7ne", "Hand", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Dir dank ich f\u00fcr die G\u00fcter der Erden,", "tokens": ["Dir", "dank", "ich", "f\u00fcr", "die", "G\u00fc\u00b7ter", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcr die Geschenke deiner Treu.", "tokens": ["F\u00fcr", "die", "Ge\u00b7schen\u00b7ke", "dei\u00b7ner", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir dank ich; denn du hie\u00dfest sie werden,", "tokens": ["Dir", "dank", "ich", ";", "denn", "du", "hie\u00b7\u00dfest", "sie", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "KON", "PPER", "VVFIN", "PPER", "VAINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und deine G\u00fct ist t\u00e4glich neu.", "tokens": ["Und", "dei\u00b7ne", "G\u00fct", "ist", "t\u00e4g\u00b7lich", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Dir dank ich f\u00fcr das Wunder der G\u00fcte:", "tokens": ["Dir", "dank", "ich", "f\u00fcr", "das", "Wun\u00b7der", "der", "G\u00fc\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Selbst deinen Sohn gabst du f\u00fcr mich.", "tokens": ["Selbst", "dei\u00b7nen", "Sohn", "gabst", "du", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von ganzer Seel und ganzem Gem\u00fcte,", "tokens": ["Von", "gan\u00b7zer", "Seel", "und", "gan\u00b7zem", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von allen Kr\u00e4ften preis ich dich.", "tokens": ["Von", "al\u00b7len", "Kr\u00e4f\u00b7ten", "preis", "ich", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Erhebt ihn ewig, g\u00f6ttliche Werke!", "tokens": ["Er\u00b7hebt", "ihn", "e\u00b7wig", ",", "g\u00f6tt\u00b7li\u00b7che", "Wer\u00b7ke", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Erd ist voll der Huld des Herrn.", "tokens": ["Die", "Erd", "ist", "voll", "der", "Huld", "des", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein, sein ist Ruhm und Weisheit und St\u00e4rke;", "tokens": ["Sein", ",", "sein", "ist", "Ruhm", "und", "Weis\u00b7heit", "und", "St\u00e4r\u00b7ke", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "VAINF", "VAFIN", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er hilft und er errettet gern.", "tokens": ["Er", "hilft", "und", "er", "er\u00b7ret\u00b7tet", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Er hilft. Des Abends w\u00e4hret die Klage,", "tokens": ["Er", "hilft", ".", "Des", "A\u00b7bends", "w\u00e4h\u00b7ret", "die", "Kla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Morgens die Zufriedenheit.", "tokens": ["Des", "Mor\u00b7gens", "die", "Zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach einer Pr\u00fcfung weniger Tage", "tokens": ["Nach", "ei\u00b7ner", "Pr\u00fc\u00b7fung", "we\u00b7ni\u00b7ger", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erhebt er uns zur Seligkeit.", "tokens": ["Er\u00b7hebt", "er", "uns", "zur", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Vergi\u00df nicht deines Gottes, o Seele!", "tokens": ["Ver\u00b7gi\u00df", "nicht", "dei\u00b7nes", "Got\u00b7tes", ",", "o", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "PPOSAT", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vergi\u00df nicht, was er dir getan.", "tokens": ["Ver