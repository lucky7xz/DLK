{"textgrid.poem.43391": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: O singet nicht so kleine Lieder", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O singet nicht so kleine Lieder", "tokens": ["O", "sin\u00b7get", "nicht", "so", "klei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von eines Volkes Weh und Ach!", "tokens": ["Von", "ei\u00b7nes", "Vol\u00b7kes", "Weh", "und", "Ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die hallen von den H\u00f6fen wieder", "tokens": ["Die", "hal\u00b7len", "von", "den", "H\u00f6\u00b7fen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr euch nur Leid und Ungemach.", "tokens": ["F\u00fcr", "euch", "nur", "Leid", "und", "Un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schreibt Oden, gro\u00dfe, ellenlange", "tokens": ["Schreibt", "O\u00b7den", ",", "gro\u00b7\u00dfe", ",", "el\u00b7len\u00b7lan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "NE", "$,", "ADJA", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von F\u00fcrsten-Glanz und Herrlichkeit", "tokens": ["Von", "F\u0