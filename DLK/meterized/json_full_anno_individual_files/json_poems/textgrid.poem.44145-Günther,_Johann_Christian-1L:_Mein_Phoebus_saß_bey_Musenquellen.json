{"textgrid.poem.44145": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Phoebus sa\u00df bey Musenquellen", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Phoebus sa\u00df bey Musenquellen", "tokens": ["Mein", "Phoe\u00b7bus", "sa\u00df", "bey", "Mu\u00b7sen\u00b7quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und leyerte die Finger lahm;", "tokens": ["Und", "ley\u00b7er\u00b7te", "die", "Fin\u00b7ger", "lahm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch weil Herr Sp(eeres) Wort nicht kam,", "tokens": ["Doch", "weil", "Herr", "Sp", "(", "e\u00b7e\u00b7res", ")", "Wort", "nicht", "kam", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "NE", "$(", "NE", "$(", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wollt er fast vor Durst geschwellen.", "tokens": ["So", "wollt", "er", "fast", "vor", "Durst", "ge\u00b7schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Reim gerieth, der Finger stund,", "tokens": ["Kein", "Reim", "ge\u00b7rieth", ",", "der", "Fin\u00b7ger", "stund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und um den halbverdorrten Mund", "tokens": ["Und", "um", "den", "halb\u00b7ver\u00b7dorr\u00b7ten", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Fing schon das Leder an zu springen.", "tokens": ["Fing", "schon", "das", "Le\u00b7der", "an", "zu", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Herr R(asper) schickt den vollen Krug,", "tokens": ["Herr", "R", "(", "as\u00b7per", ")", "schickt", "den", "vol\u00b7len", "Krug", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "NE", "$(", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Nun, sprach er, kan ich wieder singen", "tokens": ["Nun", ",", "sprach", "er", ",", "kan", "ich", "wie\u00b7der", "sin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und habe Lust und Geist genug.", "tokens": ["Und", "ha\u00b7be", "Lust", "und", "Geist", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Da kommen Verse fast mit Haufen", "tokens": ["Da", "kom\u00b7men", "Ver\u00b7se", "fast", "mit", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wie die nach Heinersdorf gelaufen,", "tokens": ["Wie", "die", "nach", "Hei\u00b7ners\u00b7dorf", "ge\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ja, Hand und Feder will nicht ruhn,", "tokens": ["Ja", ",", "Hand", "und", "Fe\u00b7der", "will", "nicht", "ruhn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "KON", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die Zeilen gro\u00df und voll zu tragen,", "tokens": ["Die", "Zei\u00b7len", "gro\u00df", "und", "voll", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Drum kan ich jezt mit Warheit sagen:", "tokens": ["Drum", "kan", "ich", "jezt", "mit", "War\u00b7heit", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wie kan das Bier doch oft so gro\u00dfe Dinge thun!", "tokens": ["Wie", "kan", "das", "Bier", "doch", "oft", "so", "gro\u00b7\u00dfe", "Din\u00b7ge", "thun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ADV", "ADV", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein Phoebus sa\u00df bey Musenquellen", "tokens": ["Mein", "Phoe\u00b7bus", "sa\u00df", "bey", "Mu\u00b7sen\u00b7quel\u00b7