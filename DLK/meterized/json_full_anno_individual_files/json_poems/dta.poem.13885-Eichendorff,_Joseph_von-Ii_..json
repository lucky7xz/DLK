{"dta.poem.13885": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, Trinken frisch und Singen", "tokens": ["Ja", ",", "Trin\u00b7ken", "frisch", "und", "Sin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das bricht durch alles Weh,", "tokens": ["Das", "bricht", "durch", "al\u00b7les", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das sind zwei gute Schwingen,", "tokens": ["Das", "sind", "zwei", "gu\u00b7te", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gemeine Welt, ade!", "tokens": ["Ge\u00b7mei\u00b7ne", "Welt", ",", "a\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}}}}