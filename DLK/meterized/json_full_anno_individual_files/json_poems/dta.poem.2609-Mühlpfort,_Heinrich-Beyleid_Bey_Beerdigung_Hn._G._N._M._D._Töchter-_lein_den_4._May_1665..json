{"dta.poem.2609": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Beyleid  \n  Bey Beerdigung Hn. G. N.   M. D.   T\u00f6chter-  \n lein den 4. May 1665.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Best\u00fcrtzte/ wo mein Reim nach einer Leiche schmecket/", "tokens": ["Be\u00b7st\u00fcrtz\u00b7te", "/", "wo", "mein", "Reim", "nach", "ei\u00b7ner", "Lei\u00b7che", "schme\u00b7cket", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PWAV", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wisst bey Sterbenden hab\u2019 ich ihn auch gemacht/", "tokens": ["So", "wisst", "bey", "Ster\u00b7ben\u00b7den", "hab'", "ich", "ihn", "auch", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo weder Liebligkeit noch Zucker drinnen stecket/", "tokens": ["Wo", "we\u00b7der", "Lieb\u00b7lig\u00b7keit", "noch", "Zu\u00b7cker", "drin\u00b7nen", "ste\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KON", "NN", "ADV", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So dencket da\u00df der Tod Kunst/ Witz und Zier verlacht.", "tokens": ["So", "den\u00b7cket", "da\u00df", "der", "Tod", "Kunst", "/", "Witz", "und", "Zier", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOUS", "ART", "NN", "NN", "$(", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Euch ist ein liebes Kind/ und mir ein Freund verblichen/", "tokens": ["Euch", "ist", "ein", "lie\u00b7bes", "Kind", "/", "und", "mir", "ein", "Freund", "ver\u00b7bli\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "KON", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und beyde fallen hin im angenehmen May;", "tokens": ["Und", "bey\u00b7de", "fal\u00b7len", "hin", "im", "an\u00b7ge\u00b7neh\u00b7men", "May", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es sind die Gratien von meiner Brust gewichen/", "tokens": ["Es", "sind", "die", "Gra\u00b7ti\u00b7en", "von", "mei\u00b7ner", "Brust", "ge\u00b7wi\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Statt eines s\u00fcssen Klangs erth\u00f6nt ein Leich-Geschrey.", "tokens": ["Statt", "ei\u00b7nes", "s\u00fcs\u00b7sen", "Klangs", "er\u00b7th\u00f6nt", "ein", "Leich\u00b7Ge\u00b7schrey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Kirchhoffs-Blumen sind nicht nur die grauen Haare/", "tokens": ["Die", "Kirch\u00b7hoffs\u00b7Blu\u00b7men", "sind", "nicht", "nur", "die", "grau\u00b7en", "Haa\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Weil bey den Kindern offt der Tod in Wiegen liegt/", "tokens": ["Weil", "bey", "den", "Kin\u00b7dern", "offt", "der", "Tod", "in", "Wie\u00b7gen", "liegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das Grab verzehret auch der Jugend Rosen-Jahre/", "tokens": ["Das", "Grab", "ver\u00b7zeh\u00b7ret", "auch", "der", "Ju\u00b7gend", "Ro\u00b7sen\u00b7Jah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Man wei\u00df von keinem Held der \u00fcbern Tod gesiegt.", "tokens": ["Man", "wei\u00df", "von", "kei\u00b7nem", "Held", "der", "\u00fc\u00b7bern", "Tod", "ge\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PIAT", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn schon die Stunde schl\u00e4gt/ kan Esculap nicht rathen/", "tokens": ["Wenn", "schon", "die", "Stun\u00b7de", "schl\u00e4gt", "/", "kan", "E\u00b7scu\u00b7lap", "nicht", "ra\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$(", "VMFIN", "NE", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Und gantz Arabiens sein Balsam gilt nicht viel.", "tokens": ["Und", "gantz", "A\u00b7ra\u00b7biens", "sein", "Bal\u00b7sam", "gilt", "nicht", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "PPOSAT", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.15": {"text": "Der Syrer theures Oel und Indiens Mu\u00dfkaten", "tokens": ["Der", "Sy\u00b7rer", "theu\u00b7res", "O\u00b7el", "und", "In\u00b7di\u00b7ens", "Mu\u00df\u00b7ka\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NE", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Sambt allem Bezoar verl\u00e4ngern nicht das Ziel.", "tokens": ["Sambt", "al\u00b7lem", "Be\u00b7zoar", "ver\u00b7l\u00e4n\u00b7gern", "nicht", "das", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Ich wei\u00df/ Apollens Sohn/ wie sehr er sich bem\u00fchet", "tokens": ["Ich", "wei\u00df", "/", "A\u00b7pol\u00b7lens", "Sohn", "/", "wie", "sehr", "er", "sich", "be\u00b7m\u00fc\u00b7het"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "NE", "NN", "$(", "KOKOM", "ADV", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sein allerliebstes Kind dem sterben zu entziehn:", "tokens": ["Sein", "al\u00b7ler\u00b7liebs\u00b7tes", "Kind", "dem", "ster\u00b7ben", "zu", "ent\u00b7ziehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Alleine diese Ros\u2019 und Lilg\u2019 ist nur verbl\u00fchet/", "tokens": ["Al\u00b7lei\u00b7ne", "die\u00b7se", "Ros'", "und", "Lil\u00b7g'", "ist", "nur", "ver\u00b7bl\u00fc\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "KON", "NN", "VAFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Ein Wollust-reicher May der Eltern gehet hin.", "tokens": ["Ein", "Wol\u00b7lust\u00b7rei\u00b7cher", "May", "der", "El\u00b7tern", "ge\u00b7het", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es scheinet der Natur Gesetze fast zu wieder/", "tokens": ["Es", "schei\u00b7net", "der", "Na\u00b7tur", "Ge\u00b7set\u00b7ze", "fast", "zu", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "ADV", "PTKA", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und stellt mit der Vernunfft schier einen Wett-Streit an/", "tokens": ["Und", "stellt", "mit", "der", "Ver\u00b7nunfft", "schier", "ei\u00b7nen", "Wet\u00b7tStreit", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADJD", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Den Kindern dr\u00fccken zu die matten Augen-Lieder/", "tokens": ["Den", "Kin\u00b7dern", "dr\u00fc\u00b7cken", "zu", "die", "mat\u00b7ten", "Au\u00b7gen\u00b7Lie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da man doch sonsten w\u00fcnscht/ da\u00df sie uns di\u00df gethan.", "tokens": ["Da", "man", "doch", "sons\u00b7ten", "w\u00fcnscht", "/", "da\u00df", "sie", "uns", "di\u00df", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "VVFIN", "$(", "KOUS", "PPER", "PPER", "PDS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch unterstehet sich ein Mensch mit GOtt zu rechten?", "tokens": ["Doch", "un\u00b7ter\u00b7ste\u00b7het", "sich", "ein", "Mensch", "mit", "Gott", "zu", "rech\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Betagt der Erden-Klo\u00df den Sch\u00f6pffer f\u00fcr Gericht?", "tokens": ["Be\u00b7tagt", "der", "Er\u00b7den\u00b7Klo\u00df", "den", "Sch\u00f6pf\u00b7fer", "f\u00fcr", "Ge\u00b7richt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ach nein! ein spr\u00f6der Thon der kan nicht wieder fechten/", "tokens": ["Ach", "nein", "!", "ein", "spr\u00f6\u00b7der", "Thon", "der", "kan", "nicht", "wie\u00b7der", "fech\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$.", "ART", "ADJA", "NN", "ART", "VMFIN", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dem Allgewaltigen h\u00e4lt niemand das Gewicht.", "tokens": ["Dem", "All\u00b7ge\u00b7wal\u00b7ti\u00b7gen", "h\u00e4lt", "nie\u00b7mand", "das", "Ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Es ist dem sch\u00f6nen Kind so \u00fcbel nicht geschehen/", "tokens": ["Es", "ist", "dem", "sch\u00f6\u00b7nen", "Kind", "so", "\u00fc\u00b7bel", "nicht", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wie saur es Fleisch und Blut auch immer deucht zu seyn/", "tokens": ["Wie", "saur", "es", "Fleisch", "und", "Blut", "auch", "im\u00b7mer", "deucht", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "NN", "KON", "NN", "ADV", "ADV", "ADJD", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Eltern kr\u00e4nckt es zwar/ da\u00df sie die Lust nicht sehen/", "tokens": ["Die", "El\u00b7tern", "kr\u00e4nckt", "es", "zwar", "/", "da\u00df", "sie", "die", "Lust", "nicht", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So stets den Fr\u00fchling gab und einen Sonnen-Schein.", "tokens": ["So", "stets", "den", "Fr\u00fch\u00b7ling", "gab", "und", "ei\u00b7nen", "Son\u00b7nen\u00b7Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der zarten Glieder Schmuck ist freylich jetzt versehret/", "tokens": ["Der", "zar\u00b7ten", "Glie\u00b7der", "Schmuck", "ist", "frey\u00b7lich", "jetzt", "ver\u00b7seh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der Wangen Nelcke bleich/ der Lippen Scharlach bla\u00df.", "tokens": ["Der", "Wan\u00b7gen", "Nel\u00b7cke", "bleich", "/", "der", "Lip\u00b7pen", "Schar\u00b7lach", "bla\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "$(", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der Aeuglein Sternen-Glut hat Finsterni\u00df verst\u00f6ret/", "tokens": ["Der", "A\u00b7e\u00b7u\u00b7glein", "Ster\u00b7nen\u00b7Glut", "hat", "Fins\u00b7ter\u00b7ni\u00df", "ver\u00b7st\u00f6\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.36": {"text": "Und diese Tods-Gestalt macht eure Augen na\u00df.", "tokens": ["Und", "die\u00b7se", "Tods\u00b7Ge\u00b7stalt", "macht", "eu\u00b7re", "Au\u00b7gen", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Es lacht euch nicht mehr an/ gibt weiter keine K\u00fcsse/", "tokens": ["Es", "lacht", "euch", "nicht", "mehr", "an", "/", "gibt", "wei\u00b7ter", "kei\u00b7ne", "K\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$(", "VVFIN", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und redet/ was den Geist von Grund erquicken kan.", "tokens": ["Und", "re\u00b7det", "/", "was", "den", "Geist", "von", "Grund", "er\u00b7qui\u00b7cken", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PWS", "ART", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ach ja ich geb es zu/ da\u00df herbe Seelen-Risse/", "tokens": ["Ach", "ja", "ich", "geb", "es", "zu", "/", "da\u00df", "her\u00b7be", "See\u00b7len\u00b7Ris\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PPER", "VVFIN", "PPER", "PTKZU", "$(", "KOUS", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Als Wecker strenger Noth/ euch hefftig greiffen an.", "tokens": ["Als", "We\u00b7cker", "stren\u00b7ger", "Noth", "/", "euch", "heff\u00b7tig", "greif\u00b7fen", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "$(", "PPER", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Hingegen wann ihr wohl des H\u00f6chsten Schlu\u00df erweget/", "tokens": ["Hin\u00b7ge\u00b7gen", "wann", "ihr", "wohl", "des", "H\u00f