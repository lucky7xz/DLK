{"textgrid.poem.34331": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach du um die die Blumen sich", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach du um die die Blumen sich", "tokens": ["Ach", "du", "um", "die", "die", "Blu\u00b7men", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PPER", "APPR", "PRELS", "ART", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verliebt aus ihren Knospen dr\u00e4ngen,", "tokens": ["Ver\u00b7liebt", "aus", "ih\u00b7ren", "Knos\u00b7pen", "dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und mit der frohen Luft um dich", "tokens": ["Und", "mit", "der", "fro\u00b7hen", "Luft", "um", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entz\u00fcckt auch ihren Weihrauch mengen,", "tokens": ["Ent\u00b7z\u00fcckt", "auch", "ih\u00b7ren", "Weih\u00b7rauch", "men\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Um die jetzt Flur und Garten lacht,", "tokens": ["Um", "die", "jetzt", "Flur", "und", "Gar\u00b7ten", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil sie dein Auge bl\u00fchen macht;", "tokens": [