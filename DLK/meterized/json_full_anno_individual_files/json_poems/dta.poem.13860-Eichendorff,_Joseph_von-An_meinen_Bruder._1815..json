{"dta.poem.13860": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An meinen Bruder.  \n 1815.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und hat nun ausgerungen", "tokens": ["Und", "hat", "nun", "aus\u00b7ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Deutschland siegeswund:", "tokens": ["Mein", "Deutschland", "sie\u00b7ges\u00b7wund", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Was damals Lieb' gesungen,", "tokens": ["Was", "da\u00b7mals", "Lieb'", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was Schwerdter Dir geklungen,", "tokens": ["Was", "Schwerd\u00b7ter", "Dir", "ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Klingt fort im Herzensgrund.", "tokens": ["Klingt", "fort", "im", "Her\u00b7zens\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}