{"textgrid.poem.66776": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Psalm", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe die Tiefen des Elends geschaut,", "tokens": ["Ich", "ha\u00b7be", "die", "Tie\u00b7fen", "des", "E\u00b7lends", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und es hat mir in Tiefen der Seele gegraut.", "tokens": ["Und", "es", "hat", "mir", "in", "Tie\u00b7fen", "der", "See\u00b7le", "ge\u00b7graut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Ich sah lebendiger Toten Skelett", "tokens": ["Ich", "sah", "le\u00b7ben\u00b7di\u00b7ger", "To\u00b7ten", "Ske\u00b7lett"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und stand an der Buhlen entweihtem Bett.", "tokens": ["Und", "stand", "an", "der", "Buh\u00b7len", "ent\u00b7weih\u00b7tem", "Bett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Die stolze Vermessenheit sah ich im Schwang", "tokens": ["Die", "stol\u00b7ze", "Ver\u00b7mes\u00b7sen\u00b7heit", "sah", "ich", "im", "Schwang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und lauschte der Reichen bet\u00f6rtem Sang.", "tokens": ["Und", "lauschte", "der", "Rei\u00b7chen", "be\u00b7t\u00f6r\u00b7tem", "Sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die Seelen sah ich verkauft und feil,", "tokens": ["Die", "See\u00b7len", "sah", "ich", "ver\u00b7kauft", "und", "feil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Nach Gold und Ehren und Wollust geil.", "tokens": ["Nach", "Gold", "und", "Eh\u00b7ren", "und", "Wol\u00b7lust", "geil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Der Knechte traf ich ein zahllos Heer", "tokens": ["Der", "Knech\u00b7te", "traf", "ich", "ein", "zahl\u00b7los", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJD", "NN"], "meter": "-+-+----+", "measure": "unknown.measure.tri"}, "line.10": {"text": "Und fand der L\u00fcgner und Heuchler noch mehr.", "tokens": ["Und", "fand", "der", "L\u00fcg\u00b7ner", "und", "Heuch\u00b7ler", "noch", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "NN", "ADV", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Im Bethaus sah ich vor Gott sie knien", "tokens": ["Im", "Be\u00b7thaus", "sah", "ich", "vor", "Gott", "sie", "kni\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Und sah, wie sie heimlich den Heiland bespien,", "tokens": ["Und", "sah", ",", "wie", "sie", "heim\u00b7lich", "den", "Hei\u00b7land", "be\u00b7spi\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.13": {"text": "Und lachten verborgen und trieben Hohn", "tokens": ["Und", "lach\u00b7ten", "ver\u00b7bor\u00b7gen", "und", "trie\u00b7ben", "Hohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVPP", "KON", "VVFIN", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Und krochen h\u00fcndisch zu Kreuz und Thron.", "tokens": ["Und", "kro\u00b7chen", "h\u00fcn\u00b7disch", "zu", "Kreuz", "und", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Und ich sah, was mir h\u00f6llisch die Sinne gepackt,", "tokens": ["Und", "ich", "sah", ",", "was", "mir", "h\u00f6l\u00b7lisch", "die", "Sin\u00b7ne", "ge\u00b7packt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWS", "PPER", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.16": {"text": "Sie die Wahrheit peitschen \u2013 notz\u00fcchtigen nackt!", "tokens": ["Sie", "die", "Wahr\u00b7heit", "peit\u00b7schen", "\u2013", "not\u00b7z\u00fcch\u00b7ti\u00b7gen", "nackt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$(", "ADV", "ADJD", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Und zu Boden sank ich und rang und rang", "tokens": ["Und", "zu", "Bo\u00b7den", "sank", "ich", "und", "rang", "und", "rang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "KON", "VVFIN", "KON", "VVFIN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Und lag todm\u00fcde und lebensbang.", "tokens": ["Und", "lag", "tod\u00b7m\u00fc\u00b7de", "und", "le\u00b7bens\u00b7bang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Meine Seele war w\u00fcst und mein Geist war Nacht,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "war", "w\u00fcst", "und", "mein", "Geist", "war", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Da flammte ein Strahl, nun bin ich erwacht,", "tokens": ["Da", "flamm\u00b7te", "ein", "Strahl", ",", "nun", "bin", "ich", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Und ich schreie empor voll br\u00fcnstiger Glut:", "tokens": ["Und", "ich", "schrei\u00b7e", "em\u00b7por", "voll", "br\u00fcns\u00b7ti\u00b7ger", "Glut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.22": {"text": "Du Geist der Welten! Verleih uns Mut,", "tokens": ["Du", "Geist", "der", "Wel\u00b7ten", "!", "Ver\u00b7leih", "uns", "Mut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$.", "VVIMP", "PPER", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Da\u00df das Zagen zergeht und der Zweifel zerbricht,", "tokens": ["Da\u00df", "das", "Za\u00b7gen", "zer\u00b7geht", "und", "der", "Zwei\u00b7fel", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.24": {"text": "Zu sehnen und suchen das ewige Licht,", "tokens": ["Zu", "seh\u00b7nen", "und", "su\u00b7chen", "das", "e\u00b7wi\u00b7ge", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.25": {"text": "In harrender Treu, in Gedanken und Tat,", "tokens": ["In", "har\u00b7ren\u00b7der", "Treu", ",", "in", "Ge\u00b7dan\u00b7ken", "und", "Tat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.26": {"text": "Wann der Abend sinkt, wann der Morgen naht,", "tokens": ["Wann", "der", "A\u00b7bend", "sinkt", ",", "wann", "der", "Mor\u00b7gen", "naht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Mit der Liebe R\u00fcstung im brennenden Kampf,", "tokens": ["Mit", "der", "Lie\u00b7be", "R\u00fcs\u00b7tung", "im", "bren\u00b7nen\u00b7den", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Schildleuchtende Helden im Nebeldampf,", "tokens": ["Schild\u00b7leuch\u00b7ten\u00b7de", "Hel\u00b7den", "im", "Ne\u00b7bel\u00b7dampf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.29": {"text": "Mit des Mitleids Helm, mit der Wahrheit Speer,", "tokens": ["Mit", "des", "Mit\u00b7leids", "Helm", ",", "mit", "der", "Wahr\u00b7heit", "Speer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Zahllos sich mehrend ein siegreich Heer,", "tokens": ["Zahl\u00b7los", "sich", "meh\u00b7rend", "ein", "sieg\u00b7reich", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "ADJD", "ART", "ADJD", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.31": {"text": "Zu l\u00f6sen das Leid und die Welt zu befrein \u2013", "tokens": ["Zu", "l\u00f6\u00b7sen", "das", "Leid", "und", "die", "Welt", "zu", "be\u00b7fr\u00b7ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "KON", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.32": {"text": "O seli