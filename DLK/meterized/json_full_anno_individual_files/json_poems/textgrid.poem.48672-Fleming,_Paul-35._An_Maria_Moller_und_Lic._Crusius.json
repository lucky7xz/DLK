Ra\u00b7te", "zu", ",", "was", "Rat", "kan", "ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWS", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles fehlt, was Trost verspricht,", "tokens": ["Al\u00b7les", "fehlt", ",", "was", "Trost", "ver\u00b7spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWS", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "r\u00e4t Eins Amaryllen nicht:", "tokens": ["r\u00e4t", "Eins", "A\u00b7ma\u00b7ryl\u00b7len", "nicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "so wird sie nicht wieder leben.", "tokens": ["so", "wird", "sie", "nicht", "wie\u00b7der", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Amaryllis, unser Preis,", "tokens": ["A\u00b7ma\u00b7ryl\u00b7lis", ",", "un\u00b7ser", "Preis", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "wei\u00df, was ihr zu raten wei\u00df.", "tokens": ["wei\u00df", ",", "was", "ihr", "zu", "ra\u00b7ten", "wei\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ist es jemals Zeit gewesen,", "tokens": ["Ist", "es", "je\u00b7mals", "Zeit", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "w