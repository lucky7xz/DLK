{"dta.poem.11617": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Ii.  \n  Nixe Binsefu\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Des Wassermanns sein T\u00f6chterlein", "tokens": ["Des", "Was\u00b7ser\u00b7manns", "sein", "T\u00f6ch\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tanzt auf dem Eis im Vollmondschein,", "tokens": ["Tanzt", "auf", "dem", "Eis", "im", "Voll\u00b7mond\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sie singt und lachet sonder Scheu", "tokens": ["Sie", "singt", "und", "la\u00b7chet", "son\u00b7der", "Scheu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl an des Fischers Haus vorbei.", "tokens": ["Wohl", "an", "des", "Fi\u00b7schers", "Haus", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201eich bin die Jungfer Binsefu\u00df,", "tokens": ["\u201e", "ich", "bin", "die", "Jung\u00b7fer", "Bin\u00b7se\u00b7fu\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und meine Fisch' wohl h\u00fcten mu\u00df,", "tokens": ["Und", "mei\u00b7ne", "Fisch'", "wohl", "h\u00fc\u00b7ten", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Meine Fisch' die sind im Kasten,", "tokens": ["Mei\u00b7ne", "Fisch'", "die", "sind", "im", "Kas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie haben kalte Fasten;", "tokens": ["Sie", "ha\u00b7ben", "kal\u00b7te", "Fas\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von B\u00f6hmer-Glas mein Kasten ist,", "tokens": ["Von", "B\u00f6h\u00b7mer\u00b7Glas", "mein", "Kas\u00b7ten", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da z\u00e4hl' ich sie zu jeder Frist.", "tokens": ["Da", "z\u00e4hl'", "ich", "sie", "zu", "je\u00b7der", "Frist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gelt, Fischer-Matz? gelt, alter Tropf,", "tokens": ["Gelt", ",", "Fi\u00b7scher\u00b7Matz", "?", "gelt", ",", "al\u00b7ter", "Tropf", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir will der Winter nicht in Kopf?", "tokens": ["Dir", "will", "der", "Win\u00b7ter", "nicht", "in", "Kopf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Komm' mir mit deinen Netzen!", "tokens": ["Komm'", "mir", "mit", "dei\u00b7nen", "Net\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die will ich sch\u00f6n zerfetzen!", "tokens": ["Die", "will", "ich", "sch\u00f6n", "zer\u00b7fet\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dein M\u00e4gdlein zwar ist fromm und gut,", "tokens": ["Dein", "M\u00e4gd\u00b7lein", "zwar", "ist", "fromm", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Schatz ein braves J\u00e4gerblut.", "tokens": ["Ihr", "Schatz", "ein", "bra\u00b7ves", "J\u00e4\u00b7ger\u00b7blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Drum h\u00e4ng' ich ihr, zum Hochzeitstrau\u00df,", "tokens": ["Drum", "h\u00e4ng'", "ich", "ihr", ",", "zum", "Hoch\u00b7zeit\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein schilfen Kr\u00e4nzlein vor das Haus,", "tokens": ["Ein", "schil\u00b7fen", "Kr\u00e4nz\u00b7lein", "vor", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und einen Hecht, von Silber schwer,", "tokens": ["Und", "ei\u00b7nen", "Hecht", ",", "von", "Sil\u00b7ber", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er stammt von K\u00f6nig Artus her,", "tokens": ["Er", "stammt", "von", "K\u00f6\u00b7nig", "Ar\u00b7tus", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Zwergen-Goldschmieds-Meisterst\u00fcck,", "tokens": ["Ein", "Zwer\u00b7gen\u00b7Gold\u00b7schmieds\u00b7Meis\u00b7ter\u00b7st\u00fcck", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer's hat, dem bringt es eitel Gl\u00fcck:", "tokens": ["Wer's", "hat", ",", "dem", "bringt", "es", "ei\u00b7tel", "Gl\u00fcck", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er l\u00e4\u00dft sich schuppen Jahr f\u00fcr Jahr,", "tokens": ["Er", "l\u00e4\u00dft", "sich", "schup\u00b7pen", "Jahr", "f\u00fcr", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da sind's f\u00fcnf hundert Gr\u00f6schlein baar.", "tokens": ["Da", "sin\u00b7d's", "f\u00fcnf", "hun\u00b7dert", "Gr\u00f6schlein", "baar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "CARD", "CARD", "NN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Ade, mein Kind! Ade f\u00fcr heut!", "tokens": ["A\u00b7de", ",", "mein", "Kind", "!", "A\u00b7de", "f\u00fcr", "heut", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "NN", "APPR", "ADV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Morgenhahn im Dorfe schreit.\u201c", "tokens": ["Der", "Mor\u00b7gen\u00b7hahn", "im", "Dor\u00b7fe", "schreit", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}