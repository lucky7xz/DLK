{"dta.poem.12044": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "64.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu welchem willst du dich von beiden Ch\u00f6ren wenden?", "tokens": ["Zu", "wel\u00b7chem", "willst", "du", "dich", "von", "bei\u00b7den", "Ch\u00f6\u00b7ren", "wen\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "VMFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du hast die freie Wahl dich so und so zu blenden.", "tokens": ["Du", "hast", "die", "frei\u00b7e", "Wahl", "dich", "so", "und", "so", "zu", "blen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PPER", "ADV", "KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn du den einen glaubst, so geht die Zeit bergunter;", "tokens": ["Wenn", "du", "den", "ei\u00b7nen", "glaubst", ",", "so", "geht", "die", "Zeit", "ber\u00b7gun\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PIS", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du den andern traust, so klimmt sie aufwerts munter.", "tokens": ["Wenn", "du", "den", "an\u00b7dern", "traust", ",", "so", "klimmt", "sie", "auf\u00b7werts", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ist sie villeicht das Rad, von dem sich niederneigt", "tokens": ["Ist", "sie", "vil\u00b7leicht", "das", "Rad", ",", "von", "dem", "sich", "nie\u00b7der\u00b7neigt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,", "APPR", "PRELS", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Vordere, derweil das Hintre wieder steigt?", "tokens": ["Das", "Vor\u00b7de\u00b7re", ",", "der\u00b7weil", "das", "Hin\u00b7tre", "wie\u00b7der", "steigt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Vordern klagen, da\u00df zum Untergang sichs lenke,", "tokens": ["Die", "Vor\u00b7dern", "kla\u00b7gen", ",", "da\u00df", "zum", "Un\u00b7ter\u00b7gang", "sichs", "len\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "KOUS", "APPRART", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Hintern jubeln, da\u00df es sich zum Aufgang schwenke.", "tokens": ["Die", "Hin\u00b7tern", "ju\u00b7beln", ",", "da\u00df", "es", "sich", "zum", "Auf\u00b7gang", "schwen\u00b7ke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Es steigt und f\u00e4llt zugleich; ob es im Ganzen falle,", "tokens": ["Es", "steigt", "und", "f\u00e4llt", "zu\u00b7gleich", ";", "ob", "es", "im", "Gan\u00b7zen", "fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "$.", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob steige, wei\u00df die Kraft, durch deren Sto\u00df es walle!", "tokens": ["Ob", "stei\u00b7ge", ",", "wei\u00df", "die", "Kraft", ",", "durch", "de\u00b7ren", "Sto\u00df", "es", "wal\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "$,", "VVFIN", "ART", "NN", "$,", "APPR", "PRELAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}