$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als vern\u00e4hme seine T\u00f6ne sie,", "tokens": ["Als", "ver\u00b7n\u00e4h\u00b7me", "sei\u00b7ne", "T\u00f6\u00b7ne", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPOSAT", "NN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Sterne seine Br\u00fcder nannte", "tokens": ["Und", "die", "Ster\u00b7ne", "sei\u00b7ne", "Br\u00fc\u00b7der", "nann\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und den Fr\u00fchling Gottes Melodie,", "tokens": ["Und", "den", "Fr\u00fch\u00b7ling", "Got\u00b7tes", "Me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Da im Hauche, der den Hain bewegte,", "tokens": ["Da", "im", "Hau\u00b7che", ",", "der", "den", "Hain", "be\u00b7weg\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Noch dein Geist, dein Geist der Freude sich", "tokens": ["Noch", "dein", "Geist", ",", "dein", "Geist", "der", "Freu\u00b7de", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ART", "NN", "PRF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "In des Herzens stiller Welle regte,", "tokens": ["In", "des", "Her\u00b7zens", "stil\u00b7ler", "Wel\u00b7le", "reg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "