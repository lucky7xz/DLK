{"textgrid.poem.67092": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Natur", "genre": "verse", "period": "N.A.", "pub_year": 1795, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da ich noch um deinen Schleier spielte,", "tokens": ["Da", "ich", "noch", "um", "dei\u00b7nen", "Schlei\u00b7er", "spiel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Noch an dir, wie eine Bl\u00fcte, hing,", "tokens": ["Noch", "an", "dir", ",", "wie", "ei\u00b7ne", "Bl\u00fc\u00b7te", ",", "hing", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Noch dein Herz in jedem Laute f\u00fchlte,", "tokens": ["Noch", "dein", "Herz", "in", "je\u00b7dem", "Lau\u00b7te", "f\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der mein z\u00e4rtlichbebend Herz umfing,", "tokens": ["Der", "mein", "z\u00e4rt\u00b7lich\u00b7be\u00b7bend", "Herz", "um\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Da ich noch mit Glauben und mit Sehnen", "tokens": ["Da", "ich", "noch", "mit", "Glau\u00b7ben", "und", "mit", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "KON", "APPR", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Reich, wie du, vor deinem Bilde stand,", "tokens": ["Reich", ",", "wie", "du", ",", "vor", "dei\u00b7nem", "Bil\u00b7de", "stand", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Eine Stelle noch f\u00fcr meine Tr\u00e4nen,", "tokens": ["Ei\u00b7ne", "Stel\u00b7le", "noch", "f\u00fcr", "mei\u00b7ne", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Eine Welt f\u00fcr meine Liebe fand,", "tokens": ["Ei\u00b7ne", "Welt", "f\u00fcr", "mei\u00b7ne", "Lie\u00b7be", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Da zur Sonne noch mein Herz sich wandte,", "tokens": ["Da", "zur", "Son\u00b7ne", "noch", "mein", "Herz", "sich", "wand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als vern\u00e4hme seine T\u00f6ne sie,", "tokens": ["Als", "ver\u00b7n\u00e4h\u00b7me", "sei\u00b7ne", "T\u00f6\u00b7ne", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPOSAT", "NN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Sterne seine Br\u00fcder nannte", "tokens": ["Und", "die", "Ster\u00b7ne", "sei\u00b7ne", "Br\u00fc\u00b7der", "nann\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und den Fr\u00fchling Gottes Melodie,", "tokens": ["Und", "den", "Fr\u00fch\u00b7ling", "Got\u00b7tes", "Me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Da im Hauche, der den Hain bewegte,", "tokens": ["Da", "im", "Hau\u00b7che", ",", "der", "den", "Hain", "be\u00b7weg\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Noch dein Geist, dein Geist der Freude sich", "tokens": ["Noch", "dein", "Geist", ",", "dein", "Geist", "der", "Freu\u00b7de", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ART", "NN", "PRF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "In des Herzens stiller Welle regte,", "tokens": ["In", "des", "Her\u00b7zens", "stil\u00b7ler", "Wel\u00b7le", "reg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Da umfingen goldne Tage mich.", "tokens": ["Da", "um\u00b7fin\u00b7gen", "gold\u00b7ne", "Ta\u00b7ge", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "PPER", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Wenn im Tale, wo der Quell mich k\u00fchlte,", "tokens": ["Wenn", "im", "Ta\u00b7le", ",", "wo", "der", "Quell", "mich", "k\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo der jugendlichen Str\u00e4uche Gr\u00fcn", "tokens": ["Wo", "der", "ju\u00b7gend\u00b7li\u00b7chen", "Str\u00e4u\u00b7che", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Um die stillen Felsenw\u00e4nde spielte", "tokens": ["Um", "die", "stil\u00b7len", "Fel\u00b7sen\u00b7w\u00e4n\u00b7de", "spiel\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und der Aether durch die Zweige schien,", "tokens": ["Und", "der", "A\u00b7e\u00b7ther", "durch", "die", "Zwei\u00b7ge", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn ich da, von Bl\u00fcten \u00fcbergossen,", "tokens": ["Wenn", "ich", "da", ",", "von", "Bl\u00fc\u00b7ten", "\u00fc\u00b7ber\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Still und trunken ihren Othem trank", "tokens": ["Still", "und", "trun\u00b7ken", "ih\u00b7ren", "O\u00b7them", "trank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und zu mir, von Licht und Glanz umflossen,", "tokens": ["Und", "zu", "mir", ",", "von", "Licht", "und", "Glanz", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Aus den H\u00f6hn die goldne Wolke sank \u2013", "tokens": ["Aus", "den", "H\u00f6hn", "die", "gold\u00b7ne", "Wol\u00b7ke", "sank", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn ich fern auf nackter Heide wallte,", "tokens": ["Wenn", "ich", "fern", "auf", "nack\u00b7ter", "Hei\u00b7de", "wall\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo aus d\u00e4mmernder Gekl\u00fcfte Scho\u00df", "tokens": ["Wo", "aus", "d\u00e4m\u00b7mern\u00b7der", "Ge\u00b7kl\u00fcf\u00b7te", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Der Titanensang der Str\u00f6me schallte", "tokens": ["Der", "Ti\u00b7ta\u00b7nen\u00b7sang", "der", "Str\u00f6\u00b7me", "schall\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Nacht der Wolken mich umschlo\u00df,", "tokens": ["Und", "die", "Nacht", "der", "Wol\u00b7ken", "mich", "um\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn der Sturm mit seinen Wetterwogen", "tokens": ["Wenn", "der", "Sturm", "mit", "sei\u00b7nen", "Wet\u00b7ter\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mir vor\u00fcber durch die Berge fuhr", "tokens": ["Mir", "vor\u00b7\u00fc\u00b7ber", "durch", "die", "Ber\u00b7ge", "fuhr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und des Himmels Flammen mich umflogen,", "tokens": ["Und", "des", "Him\u00b7mels", "Flam\u00b7men", "mich", "um\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Da erschienst du, Seele der Natur!", "tokens": ["Da", "er\u00b7schienst", "du", ",", "See\u00b7le", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Oft verlor ich da mit trunknen Tr\u00e4nen", "tokens": ["Oft", "ver\u00b7lor", "ich", "da", "mit", "trunk\u00b7nen", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Liebend, wie nach langer Irre sich", "tokens": ["Lie\u00b7bend", ",", "wie", "nach", "lan\u00b7ger", "Ir\u00b7re", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "APPR", "ADJA", "NN", "PRF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "In den Ozean die Str\u00f6me sehnen,", "tokens": ["In", "den", "O\u00b7ze\u00b7an", "die", "Str\u00f6\u00b7me", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sch\u00f6ne Welt! in deiner F\u00fclle mich;", "tokens": ["Sch\u00f6\u00b7ne", "Welt", "!", "in", "dei\u00b7ner", "F\u00fcl\u00b7le", "mich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "APPR", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ach! da st\u00fcrzt ich mit den Wesen allen", "tokens": ["Ach", "!", "da", "st\u00fcrzt", "ich", "mit", "den", "We\u00b7sen", "al\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PIAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Freudig aus der Einsamkeit der Zeit,", "tokens": ["Freu\u00b7dig", "aus", "der", "Ein\u00b7sam\u00b7keit", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wie ein Pilger in des Vaters Hallen,", "tokens": ["Wie", "ein", "Pil\u00b7ger", "in", "des", "Va\u00b7ters", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "In die Arme der Unendlichkeit. \u2013", "tokens": ["In", "die", "Ar\u00b7me", "der", "Un\u00b7end\u00b7lich\u00b7keit", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Seid gesegnet, goldne Kindertr\u00e4ume,", "tokens": ["Seid", "ge\u00b7seg\u00b7net", ",", "gold\u00b7ne", "Kin\u00b7der\u00b7tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihr verbargt des Lebens Armut mir,", "tokens": ["Ihr", "ver\u00b7bargt", "des", "Le\u00b7bens", "Ar\u00b7mut", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ihr erzogt des Herzens gute Keime,", "tokens": ["Ihr", "er\u00b7zogt", "des", "Her\u00b7zens", "gu\u00b7te", "Kei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Was ich nie erringe, schenktet ihr!", "tokens": ["Was", "ich", "nie", "er\u00b7rin\u00b7ge", ",", "schenk\u00b7tet", "ihr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "O Natur! an deiner Sch\u00f6nheit Lichte,", "tokens": ["O", "Na\u00b7tur", "!", "an", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "Lich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ohne M\u00fch und Zwang entfalteten", "tokens": ["Oh\u00b7ne", "M\u00fch", "und", "Zwang", "ent\u00b7fal\u00b7te\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Sich der Liebe k\u00f6nigliche Fr\u00fcchte,", "tokens": ["Sich", "der", "Lie\u00b7be", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wie die Ernten in Arkadien.", "tokens": ["Wie", "die", "Ern\u00b7ten", "in", "Ar\u00b7ka\u00b7di\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NE", "$."], "meter": "--+--++-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Tot ist nun, die mich erzog und stillte,", "tokens": ["Tot", "ist", "nun", ",", "die", "mich", "er\u00b7zog", "und", "still\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Tot ist nun die jugendliche Welt,", "tokens": ["Tot", "ist", "nun", "die", "ju\u00b7gend\u00b7li\u00b7che", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Diese Brust, die einst ein Himmel f\u00fcllte,", "tokens": ["Die\u00b7se", "Brust", ",", "die", "einst", "ein", "Him\u00b7mel", "f\u00fcll\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN