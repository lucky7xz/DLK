", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Da\u00df so ferne dir die Heimat liegt,", "tokens": ["Da\u00df", "so", "fer\u00b7ne", "dir", "die", "Hei\u00b7mat", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJA", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Armes Herz, du wirst sie nie erfragen,", "tokens": ["Ar\u00b7mes", "Herz", ",", "du", "wirst", "sie", "nie", "er\u00b7fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wenn dir nicht ein Traum von ihr gen\u00fcgt.", "tokens": ["Wenn", "dir", "nicht", "ein", "Traum", "von", "ihr", "ge\u00b7n\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}