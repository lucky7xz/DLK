{"dta.poem.2215": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist nicht der Mensch fast l\u00e4cherlich, der Menschen zu", "tokens": ["Ist", "nicht", "der", "Mensch", "fast", "l\u00e4\u00b7cher\u00b7lich", ",", "der", "Men\u00b7schen", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ADV", "ADJD", "$,", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "verbessern trachtet,", "tokens": ["ver\u00b7bes\u00b7sern", "trach\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Da keiner weder an den Thomson noch an die Pamela", "tokens": ["Da", "kei\u00b7ner", "we\u00b7der", "an", "den", "Thom\u00b7son", "noch", "an", "die", "Pa\u00b7me\u00b7la"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "KON", "APPR", "ART", "NN", "ADV", "APPR", "ART", "NE"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "sich kehrt?", "tokens": ["sich", "kehrt", "?"], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Wofern man diese nicht gebrauchet, und sich, aus ih-", "tokens": ["Wo\u00b7fern", "man", "die\u00b7se", "nicht", "ge\u00b7brau\u00b7chet", ",", "und", "sich", ",", "aus", "ih"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIS", "PDS", "PTKNEG", "VVPP", "$,", "KON", "PRF", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "nen, nicht belehrt;", "tokens": ["nen", ",", "nicht", "be\u00b7lehrt", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PTKNEG", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wird, in der Pamela, die Tugend, im Thomson, die", "tokens": ["Wird", ",", "in", "der", "Pa\u00b7me\u00b7la", ",", "die", "Tu\u00b7gend", ",", "im", "Thom\u00b7son", ",", "die"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "$,", "APPR", "ART", "NE", "$,", "ART", "NN", "$,", "APPRART", "NN", "$,", "PRELS"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Natur verachtet.", "tokens": ["Na\u00b7tur", "ver\u00b7ach\u00b7tet", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}