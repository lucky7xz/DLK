{"dta.poem.13132": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Inzwischen sahen wir dort eine Staats-Matron", "tokens": ["I\u00b7nzwi\u00b7schen", "sa\u00b7hen", "wir", "dort", "ei\u00b7ne", "Staats\u00b7Ma\u00b7tron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Von ihrer Stelle gehn. Sie wandte sich zum Thron,", "tokens": ["Von", "ih\u00b7rer", "Stel\u00b7le", "gehn", ".", "Sie", "wand\u00b7te", "sich", "zum", "Thron", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$.", "PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "15Und truge vor, da\u00df man von ihr vernehmen m\u00f6chte,", "tokens": ["tru\u00b7ge", "vor", ",", "da\u00df", "man", "von", "ihr", "ver\u00b7neh\u00b7men", "m\u00f6ch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KOUS", "PIS", "APPR", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Was sie von ihrem Amt dem Rath zu sagen d\u00e4chte,", "tokens": ["Was", "sie", "von", "ih\u00b7rem", "Amt", "dem", "Rath", "zu", "sa\u00b7gen", "d\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eich bitt\u2019 euch um Geduld! vernehmt nur eine Frag!", "tokens": ["\u201e", "ich", "bitt'", "euch", "um", "Ge\u00b7duld", "!", "ver\u00b7nehmt", "nur", "ei\u00b7ne", "Frag", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPR", "NN", "$.", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eerweget auch \u201e, sprach sie, was meine Kunst vermag!", "tokens": ["\u201e", "er\u00b7we\u00b7get", "auch", "\u201e", ",", "sprach", "sie", ",", "was", "mei\u00b7ne", "Kunst", "ver\u00b7mag", "!"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$(", "$,", "VVFIN", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "\u201ewollt ihr nicht durch den Streit ", "tokens": ["\u201e", "wollt", "ihr", "nicht", "durch", "den", "Streit"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VMFIN", "PPER", "PTKNEG", "APPR", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "20\u201dMir scheint, da\u00df alle nur von dieser Absicht sprachen.", "tokens": ["\"", "Mir", "scheint", ",", "da\u00df", "al\u00b7le", "nur", "von", "die\u00b7ser", "Ab\u00b7sicht", "spra\u00b7chen", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "KOUS", "PIS", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ezugleich bestrebt ihr euch um euern eignen Ruhm;", "tokens": ["\u201e", "zu\u00b7gleich", "be\u00b7strebt", "ihr", "euch", "um", "eu\u00b7ern", "eig\u00b7nen", "Ruhm", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eso melden meine Kunst und Kiel sich auch darum.", "tokens": ["\u201e", "so", "mel\u00b7den", "mei\u00b7ne", "Kunst", "und", "Kiel", "sich", "auch", "da\u00b7rum", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPOSAT", "NN", "KON", "NE", "PRF", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}