"text": "M\u00e4del, 's ist Winter, die B\u00e4che sind Eis;", "tokens": ["M\u00e4\u00b7del", ",", "'s", "ist", "Win\u00b7ter", ",", "die", "B\u00e4\u00b7che", "sind", "Eis", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "NN", "$,", "ART", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "D\u00e4cher der l\u00e4ndlichen H\u00fctten sind wei\u00df.", "tokens": ["D\u00e4\u00b7cher", "der", "l\u00e4nd\u00b7li\u00b7chen", "H\u00fct\u00b7ten", "sind", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VAFIN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Grau und ehrw\u00fcrdig, im silbernen Flor,", "tokens": ["Grau", "und", "ehr\u00b7w\u00fcr\u00b7dig", ",", "im", "sil\u00b7ber\u00b7nen", "Flor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Streckt sich der stattliche Kirchthurm empor.", "tokens": ["Streckt", "sich", "der", "statt\u00b7li\u00b7che", "Kirch\u00b7thurm", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "M\u00e4del, 's ist Winter. Mach's St\u00fcblein fein warm;", "tokens": ["M\u00e4\u00b7del", ",", "'s", "ist", "Win\u00b7ter", ".", "Mach's", "St\u00fcb\u00b7lein", "fein", "warm", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "NN", "$.", "NE", "NN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measur