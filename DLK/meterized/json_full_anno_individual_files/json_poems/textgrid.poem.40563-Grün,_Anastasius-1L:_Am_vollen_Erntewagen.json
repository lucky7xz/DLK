, "pos": ["PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wei\u00df Gott, es schmeckt ihm nicht recht.", "tokens": ["Wei\u00df", "Gott", ",", "es", "schmeckt", "ihm", "nicht", "recht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Am vollen Erntewagen", "tokens": ["Am", "vol\u00b7len", "Ern\u00b7te\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Froh wallte der Bauer einher,", "tokens": ["Froh", "wall\u00b7te", "der", "Bau\u00b7er", "ein\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Erntekr\u00e4nze sie lagen", "tokens": ["Die", "Ern\u00b7te\u00b7kr\u00e4n\u00b7ze", "sie", "la\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Auf garbenbeladenem Wagen,", "tokens": ["Auf", "gar\u00b7ben\u00b7be\u00b7la\u00b7de\u00b7nem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die R\u00f6\u00dflein zogen gar schwer.", "tokens": ["Die", "R\u00f6\u00df\u00b7lein", "zo\u00b7gen", "gar", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "Ein Adler flog an den Wagen:", "tokens": ["Ein", "Ad\u00b7ler", "flog", "an", "den",