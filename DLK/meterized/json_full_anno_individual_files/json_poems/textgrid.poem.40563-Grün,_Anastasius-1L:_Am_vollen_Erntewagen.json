{"textgrid.poem.40563": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am vollen Erntewagen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am vollen Erntewagen", "tokens": ["Am", "vol\u00b7len", "Ern\u00b7te\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Froh wallte der Bauer einher,", "tokens": ["Froh", "wall\u00b7te", "der", "Bau\u00b7er", "ein\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Erntekr\u00e4nze sie lagen", "tokens": ["Die", "Ern\u00b7te\u00b7kr\u00e4n\u00b7ze", "sie", "la\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Auf garbenbeladenem Wagen,", "tokens": ["Auf", "gar\u00b7ben\u00b7be\u00b7la\u00b7de\u00b7nem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die R\u00f6\u00dflein zogen gar schwer.", "tokens": ["Die", "R\u00f6\u00df\u00b7lein", "zo\u00b7gen", "gar", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ein Adler flog an den Wagen:", "tokens": ["Ein", "Ad\u00b7ler", "flog", "an", "den", "Wa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00bbmein B\u00e4uerlein, halt, ich bin's!", "tokens": ["\u00bb", "mein", "B\u00e4u\u00b7er\u00b7lein", ",", "halt", ",", "ich", "bin's", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "$,", "PPER", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df F\u00fcchse dein Huhn nicht nagen,", "tokens": ["Da\u00df", "F\u00fcch\u00b7se", "dein", "Huhn", "nicht", "na\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verbarg ichs in meinem Magen;", "tokens": ["Ver\u00b7barg", "ichs", "in", "mei\u00b7nem", "Ma\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Lad' ab mir den Schutzherrnzins!\u00ab", "tokens": ["Lad'", "ab", "mir", "den", "Schutz\u00b7her\u00b7rn\u00b7zins", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PTKVZ", "PPER", "ART", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Ein Falke flog in den R\u00e4umen:", "tokens": ["Ein", "Fal\u00b7ke", "flog", "in", "den", "R\u00e4u\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00bbmein B\u00e4uerlein, halt, ich bin's!", "tokens": ["\u00bb", "mein", "B\u00e4u\u00b7er\u00b7lein", ",", "halt", ",", "ich", "bin's", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "$,", "PPER", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich lasse dein Saatfeld keimen,", "tokens": ["Ich", "las\u00b7se", "dein", "Saat\u00b7feld", "kei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie Sonn' und Hagel es reimen;", "tokens": ["Wie", "Sonn'", "und", "Ha\u00b7gel", "es", "rei\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Lad' ab mir den Bodenzins!\u00ab", "tokens": ["Lad'", "ab", "mir", "den", "Bo\u00b7den\u00b7zins", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PTKVZ", "PPER", "ART", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Geh\u00fcpft kam auch ein Rabe:", "tokens": ["Ge\u00b7h\u00fcpft", "kam", "auch", "ein", "Ra\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbmein B\u00e4uerlein, halt, ich bin's!", "tokens": ["\u00bb", "mein", "B\u00e4u\u00b7er\u00b7lein", ",", "halt", ",", "ich", "bin's", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "$,", "PPER", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df ich, der einst dich begrabe,", "tokens": ["Da\u00df", "ich", ",", "der", "einst", "dich", "be\u00b7gra\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu \u00fcberleben dich habe,", "tokens": ["Zu", "\u00fc\u00b7berl\u00b7e\u00b7ben", "dich", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "VAFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Lad' ab mir den Sterbezins!\u00ab", "tokens": ["Lad'", "ab", "mir", "den", "Ster\u00b7be\u00b7zins", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PTKVZ", "PPER", "ART", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Zur Scheuer rollte der Wagen,", "tokens": ["Zur", "Scheu\u00b7er", "roll\u00b7te", "der", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die R\u00f6\u00dflein zogen nicht schwer;", "tokens": ["Die", "R\u00f6\u00df\u00b7lein", "zo\u00b7gen", "nicht", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Erntekr\u00e4nze nur lagen", "tokens": ["Die", "Ern\u00b7te\u00b7kr\u00e4n\u00b7ze", "nur", "la\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und soviel Garben im Wagen,", "tokens": ["Und", "so\u00b7viel", "Gar\u00b7ben", "im", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da\u00df Einer drauf schlafe, nicht mehr!", "tokens": ["Da\u00df", "Ei\u00b7ner", "drauf", "schla\u00b7fe", ",", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PAV", "VVFIN", "$,", "PTKNEG", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Der Bauer betet gen oben:", "tokens": ["Der", "Bau\u00b7er", "be\u00b7tet", "gen", "o\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00bbes soll, hilf Herre des Alls!", "tokens": ["\u00bb", "es", "soll", ",", "hilf", "Her\u00b7re", "des", "Alls", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "$,", "VVIMP", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Adler mein Blei noch erproben,", "tokens": ["Der", "Ad\u00b7ler", "mein", "Blei", "noch", "er\u00b7pro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der Falk' in den Schlingen mir toben,", "tokens": ["Der", "Fal\u00b7k'", "in", "den", "Schlin\u00b7gen", "mir", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Umdreh' ich dem Raben den Hals!\u00ab", "tokens": ["Um\u00b7dreh'", "ich", "dem", "Ra\u00b7ben", "den", "Hals", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Hui! sank er aufs Stroh, ein M\u00fcder,", "tokens": ["Hui", "!", "sank", "er", "aufs", "Stroh", ",", "ein", "M\u00fc\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM", "$.", "VVFIN", "PPER", "APPRART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und an ein Schnarchen ging's!", "tokens": ["Und", "an", "ein", "Schnar\u00b7chen", "ging's", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da schwebten vom Himmel hernieder", "tokens": ["Da", "schweb\u00b7ten", "vom", "Him\u00b7mel", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zwei T\u00e4ublein im Silbergefieder,", "tokens": ["Zwei", "T\u00e4ub\u00b7lein", "im", "Sil\u00b7ber\u00b7ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Eins rechts zu ihm, eins links.", "tokens": ["Eins", "rechts", "zu", "ihm", ",", "eins", "links", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "$,", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie f\u00e4cheln ihm mit den Schwingen", "tokens": ["Sie", "f\u00e4\u00b7cheln", "ihm", "mit", "den", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den Schwei\u00df vom Stirnenrund,", "tokens": ["Den", "Schwei\u00df", "vom", "Stir\u00b7nen\u00b7rund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die goldenen Schn\u00e4blein klingen.", "tokens": ["Die", "gol\u00b7de\u00b7nen", "Schn\u00e4b\u00b7lein", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was sie ins Ohr ihm wohl singen?", "tokens": ["Was", "sie", "ins", "Ohr", "ihm", "wohl", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "S\u00fc\u00df l\u00e4chelt und lispelt sein Mund.", "tokens": ["S\u00fc\u00df", "l\u00e4\u00b7chelt", "und", "lis\u00b7pelt", "sein", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Das mocht' ihn gar tr\u00f6stlich umschmiegen,", "tokens": ["Das", "mocht'", "ihn", "gar", "tr\u00f6st\u00b7lich", "um\u00b7schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Das mochte gar Friedliches sein,", "tokens": ["Das", "moch\u00b7te", "gar", "Fried\u00b7li\u00b7ches", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJA", "VAINF", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Er l\u00e4\u00dft ja den Adler noch fliegen,", "tokens": ["Er", "l\u00e4\u00dft", "ja", "den", "Ad\u