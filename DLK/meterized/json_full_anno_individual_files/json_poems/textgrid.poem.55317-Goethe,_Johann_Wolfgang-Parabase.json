{"textgrid.poem.55317": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Parabase", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freudig war vor vielen Jahren", "tokens": ["Freu\u00b7dig", "war", "vor", "vie\u00b7len", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eifrig so der Geist bestrebt,", "tokens": ["Eif\u00b7rig", "so", "der", "Geist", "be\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu erforschen, zu erfahren,", "tokens": ["Zu", "er\u00b7for\u00b7schen", ",", "zu", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie Natur im Schaffen lebt.", "tokens": ["Wie", "Na\u00b7tur", "im", "Schaf\u00b7fen", "lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und es ist das ewig Eine,", "tokens": ["Und", "es", "ist", "das", "e\u00b7wig", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJD", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das sich vielfach offenbart;", "tokens": ["Das", "sich", "viel\u00b7fach", "of\u00b7fen\u00b7bart", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Klein das Gro\u00dfe, gro\u00df das Kleine,", "tokens": ["Klein", "das", "Gro\u00b7\u00dfe", ",", "gro\u00df", "das", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "$,", "ADJD", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles nach der eignen Art.", "tokens": ["Al\u00b7les", "nach", "der", "eig\u00b7nen", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Immer wechselnd, fest sich haltend,", "tokens": ["Im\u00b7mer", "wech\u00b7selnd", ",", "fest", "sich", "hal\u00b7tend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nah und fern und fern und nah;", "tokens": ["Nah", "und", "fern", "und", "fern", "und", "nah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So gestaltend, umgestaltend \u2013", "tokens": ["So", "ge\u00b7stal\u00b7tend", ",", "um\u00b7ge\u00b7stal\u00b7tend", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zum Erstaunen bin ich da.", "tokens": ["Zum", "Er\u00b7stau\u00b7nen", "bin", "ich", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Freudig war vor viele