{"textgrid.poem.63629": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir m\u00fcssen es nur ja der Welt nicht sagen,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "es", "nur", "ja", "der", "Welt", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sie zu arm, dies Kleinod zu ersetzen.", "tokens": ["Da\u00df", "sie", "zu", "arm", ",", "dies", "Klei\u00b7nod", "zu", "er\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "$,", "PDS", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie zuckt die Achseln nur zu unsern Klagen:", "tokens": ["Sie", "zuckt", "die", "Ach\u00b7seln", "nur", "zu", "un\u00b7sern", "Kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbwas man verloren, darf man \u00fcbersch\u00e4tzen!\u00ab \u2013", "tokens": ["\u00bb", "was", "man", "ver\u00b7lo\u00b7ren", ",", "darf", "man", "\u00fc\u00b7bersc\u00b7h\u00e4t\u00b7zen", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PWS", "PIS", "VVPP", "$,", "VMFIN", "PIS", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": 