{"dta.poem.340": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als seine Sch\u00f6ne starb.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Engel stirbt und ich soll leben", "tokens": ["Mein", "En\u00b7gel", "stirbt", "und", "ich", "soll", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nein! dieses geht nicht an", "tokens": ["Nein", "!", "die\u00b7ses", "geht", "nicht", "an"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PDS", "VVFIN", "PTKNEG", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich mu\u00df mit ihr den Geist aufgeben/", "tokens": ["Ich", "mu\u00df", "mit", "ihr", "den", "Geist", "auf\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich kan nicht mehr am Leben seyn.", "tokens": ["Ich", "kan", "nicht", "mehr", "am", "Le\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie! da\u00df ich doch noch sprechen kan!", "tokens": ["Wie", "!", "da\u00df", "ich", "doch", "noch", "spre\u00b7chen", "kan", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gew\u00fcnschter Tod kehr bey mir ein.", "tokens": ["Ge\u00b7w\u00fcnschter", "Tod", "kehr", "bey", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Atropos komm rei\u00df den Faden", "tokens": ["A\u00b7tro\u00b7pos", "komm", "rei\u00df", "den", "Fa\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meines Lebens nur entzwey/", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "nur", "ent\u00b7zwey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00f6dte die betr\u00fcbten Glieder/", "tokens": ["T\u00f6d\u00b7te", "die", "be\u00b7tr\u00fcb\u00b7ten", "Glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gib mir meine Sch\u00f6ne wieder/", "tokens": ["Gib", "mir", "mei\u00b7ne", "Sch\u00f6\u00b7ne", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oder da\u00df ich bey ihr sey", "tokens": ["O\u00b7der", "da\u00df", "ich", "bey", "ihr", "sey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meines Lebens nur entzwey.", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "nur", "ent\u00b7zwey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf! auf! mein Geist", "tokens": ["Auf", "!", "auf", "!", "mein", "Geist"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "$.", "PTKVZ", "$.", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Es ist nun Zeit zu scheiden.", "tokens": ["Es", "ist", "nun", "Zeit", "zu", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Jammer reist", "tokens": ["Der", "Jam\u00b7mer", "reist"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Den Lebens-Drat entzwey/", "tokens": ["Den", "Le\u00b7bens\u00b7Drat", "ent\u00b7zwey", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jhr Sinnen seyd nun frey", "tokens": ["Ihr", "Sin\u00b7nen", "seyd", "nun", "frey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und k\u00f6nnt die Erden meiden", "tokens": ["Und", "k\u00f6nnt", "die", "Er\u00b7den", "mei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}