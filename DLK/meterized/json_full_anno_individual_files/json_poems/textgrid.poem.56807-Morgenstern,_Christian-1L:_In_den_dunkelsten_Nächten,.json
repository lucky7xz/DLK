{"textgrid.poem.56807": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: In den dunkelsten N\u00e4chten,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In den dunkelsten N\u00e4chten,", "tokens": ["In", "den", "dun\u00b7kels\u00b7ten", "N\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "wo nur die Eule noch jagt,", "tokens": ["wo", "nur", "die", "Eu\u00b7le", "noch", "jagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "zieht durch des einsamsten Waldes", "tokens": ["zieht", "durch", "des", "ein\u00b7sams\u00b7ten", "Wal\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "finstersten Teil", "tokens": ["fins\u00b7ters\u00b7ten", "Teil"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "ein gespenstischer Stier ...", "tokens": ["ein", "ge\u00b7spens\u00b7ti\u00b7scher", "Stier", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Sie nur", "tokens": ["Sie", "nur"], "token_info": ["word", "word"], "pos": ["PPER", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "kennt seinen Namen", "tokens": ["kennt", "sei\u00b7nen", "Na\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach