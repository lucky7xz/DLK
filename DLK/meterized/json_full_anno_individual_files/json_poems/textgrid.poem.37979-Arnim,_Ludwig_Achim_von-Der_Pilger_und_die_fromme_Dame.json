{"textgrid.poem.37979": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Pilger und die fromme Dame", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es reist ein Pilgersmann nach Morgenland hinaus,", "tokens": ["Es", "reist", "ein", "Pil\u00b7gers\u00b7mann", "nach", "Mor\u00b7gen\u00b7land", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "APZR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er kam vor eines Edelmannes Haus,", "tokens": ["Er", "kam", "vor", "ei\u00b7nes", "E\u00b7del\u00b7man\u00b7nes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kam vor sein Haus, vor seine Th\u00fcr,", "tokens": ["Kam", "vor", "sein", "Haus", ",", "vor", "sei\u00b7ne", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Trat eine sch\u00f6ne Dam herf\u00fcr.", "tokens": ["Trat", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Dam", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er sprach sie an um eine gute Gab,", "tokens": ["Er", "sprach", "sie", "an", "um", "ei\u00b7ne", "gu\u00b7te", "Gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "P