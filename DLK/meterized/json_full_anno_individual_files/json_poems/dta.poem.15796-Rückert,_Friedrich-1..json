{"dta.poem.15796": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lob Ihm, mit dessen H\u00fclf' auch das ist abgethan!", "tokens": ["Lob", "Ihm", ",", "mit", "des\u00b7sen", "H\u00fcl\u00b7f'", "auch", "das", "ist", "ab\u00b7ge\u00b7than", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPR", "PRELAT", "NN", "ADV", "PDS", "VAFIN", "VVPP", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sein Buch der Weisheit hat vollendet der Brahman.", "tokens": ["Sein", "Buch", "der", "Weis\u00b7heit", "hat", "voll\u00b7en\u00b7det", "der", "Brah\u00b7man", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VAFIN", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Nur diese Schnitzelchen hab' ich noch aufzuheben,", "tokens": ["Nur", "die\u00b7se", "Schnit\u00b7zel\u00b7chen", "hab'", "ich", "noch", "auf\u00b7zu\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VAFIN", "PPER", "ADV", "VVIZU", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und eines fehlt nun euch und mir: das Buch zu leben!", "tokens": ["Und", "ei\u00b7nes", "fehlt", "nun", "euch", "und", "mir", ":", "das", "Buch", "zu", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "PPER", "KON", "PPER", "$.", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}