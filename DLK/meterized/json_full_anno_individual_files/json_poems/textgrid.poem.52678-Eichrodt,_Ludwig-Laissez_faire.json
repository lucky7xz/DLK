d", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "FM", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "PTKZU", "VVINF", "ART", "NN", "$,", "KON", "PTKZU", "VVINF", "ADJA", "NN", "$,"], "meter": "--+--+---+-+--+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Zu beh\u00fcten im Sturm all die Bl\u00fcthen des Geists? und wohlan sei die Gegenwart Richter!", "tokens": ["Zu", "be\u00b7h\u00fc\u00b7ten", "im", "Sturm", "all", "die", "Bl\u00fc\u00b7then", "des", "Geists", "?", "und", "wo\u00b7hlan", "sei", "die", "Ge\u00b7gen\u00b7wart", "Rich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPRART", "NN", "PIAT", "ART", "NN", "ART", "NN", "$.", "KON", "ADV", "VAFIN", "ART", "NN", "NN", "$."], "meter": "--+--+--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Sind der Herzen doch viel, die zu schlagen gewohnt \u2013 nun es fordert sein Recht auch der Dichter.", "tokens": ["Sind", "der", "Her\u00b7zen", "doch", "viel", ",", "die", "zu", "schla\u00b7gen", "ge\u00b7wohnt", "\u2013", "nun", "es", "for\u00b7dert", "sein", "Recht", "auch", "der", "Dich\u00b7ter."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADV", "$,", "PRELS", "PTKZU", "VVINF", "VVPP", "$(", "ADV", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ART", "NN"], "meter": "--+--+--+--+--+-+-+-", "measure": "anapaest.tetra.plus"}}, "stanza.4": {"line.1": {"text": "O so gebt mir ein Lied, ein gefl\u00fcgeltes Lied der Begeisterung, Lieder der Liebe \u2013", "tokens": ["O", "so", "gebt", "mir", "ein", "Lied", ",", "ein", "ge\u00b7fl\u00fc\u00b7gel\u00b7tes", "Lied", "der", "Be\u00b7g