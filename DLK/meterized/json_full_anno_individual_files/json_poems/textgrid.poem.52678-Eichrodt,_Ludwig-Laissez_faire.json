{"textgrid.poem.52678": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Laissez faire", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O so gebt mir ein Lied, ein gefl\u00fcgeltes Lied der Begeisterung, Lieder der Liebe \u2013", "tokens": ["O", "so", "gebt", "mir", "ein", "Lied", ",", "ein", "ge\u00b7fl\u00fc\u00b7gel\u00b7tes", "Lied", "der", "Be\u00b7geis\u00b7te\u00b7rung", ",", "Lie\u00b7der", "der", "Lie\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "$("], "meter": "+-+--+--+--+--+--+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Ihr bewegenden M\u00e4chte der menschlichen Brust, ihr Gedanken und herrlichen Triebe!", "tokens": ["Ihr", "be\u00b7we\u00b7gen\u00b7den", "M\u00e4ch\u00b7te", "der", "menschli\u00b7chen", "Brust", ",", "ihr", "Ge\u00b7dan\u00b7ken", "und", "herr\u00b7li\u00b7chen", "Trie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+--+--+-+--+--+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Aus der gl\u00fchenden Seel ach! entquelle der Strom, aus der vollen Seele die Dichtung,", "tokens": ["Aus", "der", "gl\u00fc\u00b7hen\u00b7den", "Seel", "ach", "!", "ent\u00b7quel\u00b7le", "der", "Strom", ",", "aus", "der", "vol\u00b7len", "See\u00b7le", "die", "Dich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ITJ", "$.", "VVFIN", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+--+--+--+--+-+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Und sie rausche dahin ungez\u00fcgelten Muths, nur erf\u00fcllend des Sch\u00f6nen Verpflichtung.", "tokens": ["Und", "sie", "rau\u00b7sche", "da\u00b7hin", "un\u00b7ge\u00b7z\u00fc\u00b7gel\u00b7ten", "Muths", ",", "nur", "er\u00b7f\u00fcl\u00b7lend", "des", "Sch\u00f6\u00b7nen", "Ver\u00b7pflich\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PAV", "ADJA", "NN", "$,", "ADV", "VVPP", "ART", "NN", "NN", "$."], "meter": "--+-+-+-+--+--+--+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Denn es jagt ein Gef\u00fchl wohl ein hei\u00dfes Gef\u00fchl des Verlangens durch all meine Glieder,", "tokens": ["Denn", "es", "jagt", "ein", "Ge\u00b7f\u00fchl", "wohl", "ein", "hei\u00b7\u00dfes", "Ge\u00b7f\u00fchl", "des", "Ver\u00b7lan\u00b7gens", "durch", "all", "mei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "--+--+--+--+--+-+-+-+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Zu erquicken den Sinn und zu baden das Hertz in dem g\u00f6ttlichen Ozean wieder.", "tokens": ["Zu", "er\u00b7qui\u00b7cken", "den", "Sinn", "und", "zu", "ba\u00b7den", "das", "Hertz", "in", "dem", "g\u00f6tt\u00b7li\u00b7chen", "O\u00b7ze\u00b7an", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "KON", "PTKZU", "VVINF", "ART", "NN", "APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+--+--+--+--+--+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.7": {"text": "Zwar ich wei\u00df wohl, es steht mit abwinkender Hand da die Zeit und bel\u00e4chelt die Gluten,", "tokens": ["Zwar", "ich", "wei\u00df", "wohl", ",", "es", "steht", "mit", "ab\u00b7win\u00b7ken\u00b7der", "Hand", "da", "die", "Zeit", "und", "be\u00b7l\u00e4\u00b7chelt", "die", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "ADV", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-++--+--+--+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "Und sie schwenket mit Macht ihre Fahne mir zu, nur in dieser zu siegen, zu bluten;", "tokens": ["Und", "sie", "schwen\u00b7ket", "mit", "Macht", "ih\u00b7re", "Fah\u00b7ne", "mir", "zu", ",", "nur", "in", "die\u00b7ser", "zu", "sie\u00b7gen", ",", "zu", "blu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "PPER", "PTKVZ", "$,", "ADV", "APPR", "PDAT", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "--+--+--+-+-+-+--+--+-", "measure": "anapaest.tri.plus"}, "line.9": {"text": "F\u00fcr die Wissenschaft nur und politische That wird ein Lorbeer noch f\u00fcrder gedeihen:", "tokens": ["F\u00fcr", "die", "Wis\u00b7sen\u00b7schaft", "nur", "und", "po\u00b7li\u00b7ti\u00b7sche", "That", "wird", "ein", "Lor\u00b7beer", "noch", "f\u00fcr\u00b7der", "ge\u00b7dei\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"],