{"textgrid.poem.44536": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[ihr seid gar wackre Pfl\u00fcger]", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr seid gar wackre Pfl\u00fcger", "tokens": ["Ihr", "seid", "gar", "wack\u00b7re", "Pfl\u00fc\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit immer regem Mut,", "tokens": ["Mit", "im\u00b7mer", "re\u00b7gem", "Mut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr w\u00e4hlt den besten Samen,", "tokens": ["Ihr", "w\u00e4hlt", "den", "bes\u00b7ten", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und euer Feld ist gut.", "tokens": ["Und", "eu\u00b7er", "Feld", "ist", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nur tut in euerm Eifer", "tokens": ["Nur", "tut", "in", "eu\u00b7erm", "Ei\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr nimmer euch genug,", "tokens": ["Ihr", "nim\u00b7mer", "euch", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kaum sprossen die gr\u00fcnen Saaten,", "tokens": ["Kaum", "spros\u00b7sen", "die", "gr\u00fc\u00b7nen", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Geht neu dar\u00fcber der Pflug,", "tokens": ["Geht", "neu", "da\u00b7r\u00fc\u00b7ber", "der", "Pflug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PAV", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Und seht ihr ein H\u00e4lmchen Unkraut,", "tokens": ["Und", "seht", "ihr", "ein", "H\u00e4lm\u00b7chen", "Un\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So tretet ihr in die Saat,", "tokens": ["So", "tre\u00b7tet", "ihr", "in", "die", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der eine Distel gej\u00e4tet,", "tokens": ["Der", "ei\u00b7ne", "Dis\u00b7tel", "ge\u00b7j\u00e4\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Daf\u00fcr zehn Halme zertrat.", "tokens": ["Da\u00b7f\u00fcr", "zehn", "Hal\u00b7me", "zer\u00b7trat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "CARD", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Man ackert doch nur, da\u00df man ernte,", "tokens": ["Man", "ac\u00b7kert", "doch", "nur", ",", "da\u00df", "man", "ern\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "w