{"textgrid.poem.36452": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2. Der L\u00f6we, der Tiger und der Wandersmann", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als \u00d6sterreich und Sachsen sich verband,", "tokens": ["Als", "\u00d6s\u00b7ter\u00b7reich", "und", "Sach\u00b7sen", "sich", "ver\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und dein geliebtes Vaterland", "tokens": ["Und", "dein", "ge\u00b7lieb\u00b7tes", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verschlingen wollte, Prinz!", "tokens": ["Ver\u00b7schlin\u00b7gen", "woll\u00b7te", ",", "Prinz", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und unter sich schon jegliche Provinz", "tokens": ["Und", "un\u00b7ter", "sich", "schon", "jeg\u00b7li\u00b7che", "Pro\u00b7vinz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRF", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Geteilet hatte, da entwich", "tokens": ["Ge\u00b7tei\u00b7let", "hat\u00b7te", ",", "da", "ent\u00b7wich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "KOUS", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von uns der Vater Friederich", "tokens": ["Von", "uns", "der", "Va\u00b7ter", "Frie\u00b7de\u00b7rich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit seinem Heer, that einen Flug", "tokens": ["Mit", "sei\u00b7nem", "Heer", ",", "that", "ei\u00b7nen", "Flug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf unsern Feind, und sah, und schlug,", "tokens": ["Auf", "un\u00b7sern", "Feind", ",", "und", "sah", ",", "und", "schlug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und war des Feindes Sieger!", "tokens": ["Und", "war", "des", "Fein\u00b7des", "Sie\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und als ich da", "tokens": ["Und", "als", "ich", "da"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Den Held ins Vaterland zur\u00fccke kommen sah,", "tokens": ["Den", "Held", "ins", "Va\u00b7ter\u00b7land", "zu\u00b7r\u00fc\u00b7cke", "kom\u00b7men", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da schon erz\u00e4hlt' ich, Prinz! die Fabel von dem Tiger.", "tokens": ["Da", "schon", "er\u00b7z\u00e4hlt'", "ich", ",", "Prinz", "!", "die", "Fa\u00b7bel", "von", "dem", "Ti\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "NN", "$.", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Tiger schrecklich anzusehn,", "tokens": ["Ein", "Ti\u00b7ger", "schreck\u00b7lich", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Obgleich von au\u00dfen sch\u00f6n,", "tokens": ["Ob\u00b7gleich", "von", "au\u00b7\u00dfen", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fiel einen armen Wandersmann,", "tokens": ["Fiel", "ei\u00b7nen", "ar\u00b7men", "Wan\u00b7ders\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der vor sich hin, bei stillem Gang,", "tokens": ["Der", "vor", "sich", "hin", ",", "bei", "stil\u00b7lem", "Gang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Morgenlied dem Sch\u00f6pfer sang,", "tokens": ["Ein", "Mor\u00b7gen\u00b7lied", "dem", "Sch\u00f6p\u00b7fer", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit ausgestreckten Klauen an", "tokens": ["Mit", "aus\u00b7ge\u00b7streck\u00b7ten", "Klau\u00b7en", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihn zu zerrei\u00dfen \u2013 Was geschieht?", "tokens": ["Ihn", "zu", "zer\u00b7rei\u00b7\u00dfen", "\u2013", "Was", "ge\u00b7schieht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$(", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein alter L\u00f6we sieht", "tokens": ["Ein", "al\u00b7ter", "L\u00f6\u00b7we", "sieht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Heldenthat, aus seiner nahen H\u00f6hle;", "tokens": ["Die", "Hel\u00b7den\u00b7that", ",", "aus", "sei\u00b7ner", "na\u00b7hen", "H\u00f6h\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fliegt, angespornt von seiner gro\u00dfen Seele,", "tokens": ["Fliegt", ",", "an\u00b7ge\u00b7spornt", "von", "sei\u00b7ner", "gro\u00b7\u00dfen", "See\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hervor aus ihr, springt auf den Tiger,", "tokens": ["Her\u00b7vor", "aus", "ihr", ",", "springt", "auf", "den", "Ti\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00e4lt ihn \u2013 \u2013 Rund um erschallt,", "tokens": ["H\u00e4lt", "ihn", "\u2013", "\u2013", "Rund", "um", "er\u00b7schallt", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER"