{"textgrid.poem.59333": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Frau Venus", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was weckst du, Fr\u00fchling, mich von neuem wieder?", "tokens": ["Was", "weckst", "du", ",", "Fr\u00fch\u00b7ling", ",", "mich", "von", "neu\u00b7em", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$,", "PRF", "APPR", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df all die alten W\u00fcnsche auferstehen,", "tokens": ["Da\u00df", "all", "die", "al\u00b7ten", "W\u00fcn\u00b7sche", "auf\u00b7er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geht \u00fcbers Land ein wunderbares Wehen;", "tokens": ["Geht", "\u00fc\u00b7bers", "Land", "ein", "wun\u00b7der\u00b7ba\u00b7res", "We\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das schauert mir so lieblich durch die Glieder.", "tokens": ["Das", "schau\u00b7ert", "mir", "so", "lieb\u00b7lich", "durch", "die", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die sch\u00f6ne Mutter gr\u00fc\u00dfen tausend Lieder,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Mut\u00b7ter", "gr\u00fc\u00b7\u00dfen", "tau\u00b7send", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die, wieder jung, im Brautkranz s\u00fc\u00df zu sehen;", "tokens": ["Die", ",", "wie\u00b7der", "jung", ",", "im", "Braut\u00b7kranz", "s\u00fc\u00df", "zu", "se\u00b7hen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ADJD", "$,", "APPRART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Wald will sprechen, rauschend Str\u00f6me gehen,", "tokens": ["Der", "Wald", "will", "spre\u00b7chen", ",", "rau\u00b7schend", "Str\u00f6\u00b7me", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Najaden tauchen singend auf und nieder.", "tokens": ["Na\u00b7ja\u00b7den", "tau\u00b7chen", "sin\u00b7gend", "auf", "und", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Rose seh ich gehn aus gr\u00fcner Klause", "tokens": ["Die", "Ro\u00b7se", "seh", "ich", "gehn", "aus", "gr\u00fc\u00b7ner", "Klau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und, wie so buhlerisch die L\u00fcfte f\u00e4cheln,", "tokens": ["Und", ",", "wie", "so", "buh\u00b7le\u00b7risch", "die", "L\u00fcf\u00b7te", "f\u00e4\u00b7cheln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ADV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Err\u00f6tend in die laue Flut sich dehnen.", "tokens": ["Er\u00b7r\u00f6\u00b7tend", "in", "die", "lau\u00b7e", "Flut", "sich", "deh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So mich auch ruft ihr aus dem stillen Hause \u2013", "tokens": ["So", "mich", "auch", "ruft", "ihr", "aus", "dem", "stil\u00b7len", "Hau\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schmerzlich nun mu\u00df ich im Fr\u00fchling l\u00e4cheln,", "tokens": ["Und", "schmerz\u00b7lich", "nun", "mu\u00df", "ich", "im", "Fr\u00fch\u00b7ling", "l\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VMFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Versinkend zwischen Duft und Klang vor Sehnen.", "tokens": ["Ver\u00b7sin\u00b7kend", "zwi\u00b7schen", "Duft", "und", "Klang", "vor", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was weckst du, Fr\u00fchling, mich von neuem wieder?", "tokens": ["Was", "weckst", "du", ",", "Fr\u00fch\u00b7l