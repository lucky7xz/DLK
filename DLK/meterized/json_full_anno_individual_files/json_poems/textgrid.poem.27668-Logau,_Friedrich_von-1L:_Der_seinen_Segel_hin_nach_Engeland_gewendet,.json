{"textgrid.poem.27668": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der seinen Segel hin nach Engeland gewendet,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der seinen Segel hin nach Engeland gewendet,", "tokens": ["Der", "sei\u00b7nen", "Se\u00b7gel", "hin", "nach", "En\u00b7ge\u00b7land", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist manchmal durch den Wind in Holland angel\u00e4ndet,", "tokens": ["Ist", "manch\u00b7mal", "durch", "den", "Wind", "in", "Hol\u00b7land", "an\u00b7ge\u00b7l\u00e4n\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und der durchs enge Meer zu lauffen ihm getrauet,", "tokens": ["Und", "der", "durchs", "en\u00b7ge", "Meer", "zu", "lauf\u00b7fen", "ihm", "ge\u00b7trau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "ADJA", "NN", "PTKZU", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat unverhofft sein Schiff in offner See geschauet.", "tokens": ["Hat", "un\u00b7ver\u00b7hofft", "sein", "Schiff", "in", "off\u00b7ner", "See", "ge\u00b7schau\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer manchmals eine Nu\u00df f\u00fcr gut hat anger\u00fchret,", "tokens": ["Wer", "manch\u00b7mals", "ei\u00b7ne", "Nu\u00df", "f\u00fcr", "gut", "hat", "an\u00b7ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "APPR", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat drinnen einen Wurm und dran ein Loch versp\u00fcret.", "tokens": ["Hat", "drin\u00b7nen", "ei\u00b7nen", "Wurm", "und", "dran", "ein", "Loch", "ver\u00b7sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "KON", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ob Jungefrau zwar nicht und Jungfrau hoch entschieden,", "tokens": ["Ob", "Jun\u00b7ge\u00b7frau", "zwar", "nicht", "und", "Jung\u00b7frau", "hoch", "ent\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PTKNEG", "KON", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist dem doch wol, der nam die, die das E vermieden.", "tokens": ["Ist", "dem", "doch", "wol", ",", "der", "nam", "die", ",", "die", "das", "E", "ver\u00b7mie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "ADV", "$,", "PRELS", "VVFIN", "ART", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der seinen Segel hin nach Engeland gewendet,", "tokens": ["Der", "sei\u00b7nen", "Se\u00b7gel", "hin", "nach", "En\