{"textgrid.poem.33504": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Lied der Sehnsucht des Kuno Kohn", "genre": "verse", "period": "N.A.", "pub_year": 1911, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Falten des Meeres platzen wie Peitschen auf meiner Haut.", "tokens": ["Die", "Fal\u00b7ten", "des", "Mee\u00b7res", "plat\u00b7zen", "wie", "Peit\u00b7schen", "auf", "mei\u00b7ner", "Haut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "KOKOM", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und die Sterne des Meeres rei\u00dfen mich auf.", "tokens": ["Und", "die", "Ster\u00b7ne", "des", "Mee\u00b7res", "rei\u00b7\u00dfen", "mich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Von schreienden Wunden ist der Abend des Meeres Einsamen.", "tokens": ["Von", "schrei\u00b7en\u00b7den", "Wun\u00b7den", "ist", "der", "A\u00b7bend", "des", "Mee\u00b7res", "Ein\u00b7sa\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+--+-+--", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Aber die Liebenden finden den guten vertr\u00e4umten Tod ...", "tokens": ["A\u00b7ber", "die", "Lie\u00b7ben\u00b7den", "fin\u00b7den", "den", "gu\u00b7ten", "ver\u00b7tr\u00e4um\u00b7ten", "Tod", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "+--+--+--+--+-+", "measure": "diphilius"}}, "stanza.3": {"line.1": {"text": "Sei bald da, Schmerz\u00e4ugige, das Meer tut so weh.", "tokens": ["Sei", "bald", "da", ",", "Schmer\u00b7z\u00e4u\u00b7gi\u00b7ge", ",", "das", "Meer", "tut", "so", "weh", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$,", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sei bald da, Liebleidende, das Meer erschl\u00e4gt mich so.", "tokens": ["Sei", "bald", "da", ",", "Lieb\u00b7lei\u00b7den\u00b7de", ",", "das", "Meer", "er\u00b7schl\u00e4gt", "mich", "so", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$,", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Deine H\u00e4nde sind k\u00fchle Heilige. H\u00fcll mich mit ihnen, das Meer brennt auf mir.", "tokens": ["Dei\u00b7ne", "H\u00e4n\u00b7de", "sind", "k\u00fch\u00b7le", "Hei\u00b7li\u00b7ge", ".", "H\u00fcll", "mich", "mit", "ih\u00b7nen", ",", "das", "Meer", "brennt", "auf", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$.", "NN", "PRF", "APPR", "PPER", "$,", "ART", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "--+--+-+--+--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Hilf doch! Hilf doch! ... Deck mich. Rette mich. Heil mich, Freundin.", "tokens": ["Hilf", "doch", "!", "Hilf", "doch", "!", "...", "Deck", "mich", ".", "Ret\u00b7te", "mich", ".", "Heil", "mich", ",", "Freun\u00b7din", "."], "token_