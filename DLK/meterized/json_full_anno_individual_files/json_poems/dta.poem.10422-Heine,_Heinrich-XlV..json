{"dta.poem.10422": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XlV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Und wenn ich dich geliebet hab',", "tokens": ["Und", "wenn", "ich", "dich", "ge\u00b7lie\u00b7bet", "hab'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis in meiner Todesstunde,", "tokens": ["Bis", "in", "mei\u00b7ner", "To\u00b7dess\u00b7tun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So nehm' ich mit in's ew'ge Grab", "tokens": ["So", "nehm'", "ich", "mit", "in's", "ew'\u00b7ge", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die gro\u00dfe Liebeswunde.", "tokens": ["Die", "gro\u00b7\u00dfe", "Lie\u00b7bes\u00b7wun\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}