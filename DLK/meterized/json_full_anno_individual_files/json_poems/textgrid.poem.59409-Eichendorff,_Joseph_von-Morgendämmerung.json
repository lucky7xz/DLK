{"textgrid.poem.59409": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Morgend\u00e4mmerung", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein still Erwarten in den B\u00e4umen,", "tokens": ["Es", "ist", "ein", "still", "Er\u00b7war\u00b7ten", "in", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Nachtigallen in den B\u00fcschen schlagen", "tokens": ["Die", "Nach\u00b7ti\u00b7gal\u00b7len", "in", "den", "B\u00fc\u00b7schen", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In irren Klagen, k\u00f6nnen's doch nicht sagen,", "tokens": ["In", "ir\u00b7ren", "Kla\u00b7gen", ",", "k\u00f6n\u00b7nen's", "doch", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Schmerzen all und Wonne, halb in Tr\u00e4umen.", "tokens": ["Die", "Schmer\u00b7zen", "all", "und", "Won\u00b7ne", ",", "halb", "in", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "KON", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Lerche auch will nicht die Zeit vers\u00e4umen,", "tokens": ["Die", "Ler\u00b7che", "auch", "will", "nicht", "die", "Zeit", "ver\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da solches Schallen bringt die Luft getragen,", "tokens": ["Da", "sol\u00b7ches", "Schal\u00b7len", "bringt", "die", "Luft", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schwingt sich vom Tal, eh's noch beginnt zu tagen,", "tokens": ["Schwingt", "sich", "vom", "Tal", ",", "eh's", "noch", "be\u00b7ginnt", "zu", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$,", "KOUS", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Im ersten Strahl die Fl\u00fcgel sich zu s\u00e4umen.", "tokens": ["Im", "ers\u00b7ten", "Strahl", "die", "Fl\u00fc\u00b7gel", "sich", "zu", "s\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich aber stand schon lange in dem Garten", "tokens": ["Ich", "a\u00b7ber", "stand", "schon", "lan\u00b7ge", "in", "dem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und bin ins stille Feld hinausgegangen,", "tokens": ["Und", "bin", "ins", "stil\u00b7le", "Feld", "hin\u00b7aus\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo leis die \u00c4hren an zu wogen fingen.", "tokens": ["Wo", "leis", "die", "\u00c4h\u00b7ren", "an", "zu", "wo\u00b7gen", "fin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O fromme V\u00f6glein, ihr und ich, wir warten", "tokens": ["O", "from\u00b7me", "V\u00f6\u00b7glein", ",", "ihr", "und", "ich", ",", "wir", "war\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PPER", "KON", "PPER", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aufs frohe Licht, da ist uns vor Verlangen", "tokens": ["Aufs", "fro\u00b7he", "Licht", ",", "da", "ist", "uns", "vor", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADV", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bei stiller Nacht erwacht so sehnend Singen.", "tokens": ["Bei", "stil\u00b7ler", "Nacht", "er\u00b7wacht", "so", "seh\u00b7nend", "Sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es ist ein still Erwarten in den B\u00e4umen,", "tokens": ["Es", "ist", "ein", "still", "Er\u00b7war\u00b7ten", "in", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word",