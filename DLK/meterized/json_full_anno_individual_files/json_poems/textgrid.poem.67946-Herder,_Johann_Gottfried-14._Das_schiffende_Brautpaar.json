{"textgrid.poem.67946": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "14. Das schiffende Brautpaar", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch in weissem Schaume flogen", "tokens": ["Hoch", "in", "weis\u00b7sem", "Schau\u00b7me", "flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vier Barbarische Galeren,", "tokens": ["Vier", "Bar\u00b7ba\u00b7ri\u00b7sche", "Ga\u00b7le\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Machten schnelle Jagd auf Eine", "tokens": ["Mach\u00b7ten", "schnel\u00b7le", "Jagd", "auf", "Ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kleine Spanische Gallione.", "tokens": ["Klei\u00b7ne", "Spa\u00b7ni\u00b7sche", "Gal\u00b7li\u00b7o\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "In der ein begl\u00fccktes Brautpaar", "tokens": ["In", "der", "ein", "be\u00b7gl\u00fcck\u00b7tes", "Braut\u00b7paar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freudig durch die Wellen schiffte:", "tokens": ["Freu\u00b7dig", "durch", "die", "Wel\u00b7len", "schiff\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er ein Edler von Mallorca,", "tokens": ["Er", "ein", "Ed\u00b7ler", "von", "Mal\u00b7lor\u00b7ca", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "NE", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sie die Sch\u00f6nste Valenciana.", "tokens": ["Sie", "die", "Sch\u00f6ns\u00b7te", "Va\u00b7len\u00b7ci\u00b7a\u00b7na", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Hold beg\u00fcnstigt von der Liebe,", "tokens": ["Hold", "be\u00b7g\u00fcns\u00b7tigt", "von", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sehnen sie sich nach Mallorca,", "tokens": ["Seh\u00b7nen", "sie", "sich", "nach", "Mal\u00b7lor\u00b7ca", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NE", "$,"], "meter": "+--+-+++", "measure": "iambic.penta.invert"}, "line.3": {"text": "Da ihr Freudenfest zu feiren,", "tokens": ["Da", "ihr", "Freu\u00b7den\u00b7fest", "zu", "fei\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da zu sehn der Liebe Heimath.", "tokens": ["Da", "zu", "sehn", "der", "Lie\u00b7be", "Hei\u00b7math", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und je mehr bei stillem Ruder", "tokens": ["Und", "je", "mehr", "bei", "stil\u00b7lem", "Ru\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sanfter sich die Wellen neigen,", "tokens": ["Sanf\u00b7ter", "sich", "die", "Wel\u00b7len", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer schmeichelnder die Winde", "tokens": ["Im\u00b7mer", "schmei\u00b7cheln\u00b7der", "die", "Win\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rauschten in der Liebe Segel.", "tokens": ["Rauschten", "in", "der", "Lie\u00b7be", "Se\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Sehen schnell sie sich umgeben", "tokens": ["Se\u00b7hen", "schnell", "sie", "sich", "um\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "PRF", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der tiefsten Meeresenge;", "tokens": ["In", "der", "tiefs\u00b7ten", "Mee\u00b7re\u00b7sen\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Schnell von allen Seiten kommen", "tokens": ["Schnell", "von", "al\u00b7len", "Sei\u00b7ten", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf sie stolze Feindesmasten,", "tokens": ["Auf", "sie", "stol\u00b7ze", "Fein\u00b7des\u00b7mas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Die die Raubessucht befl\u00fcgelt,", "tokens": ["Die", "die", "Rau\u00b7bes\u00b7sucht", "be\u00b7fl\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie fl\u00fcgelt kaltes Schrecken.", "tokens": ["Wie", "sie", "fl\u00fc\u00b7gelt", "kal\u00b7tes", "Schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zarte Silberperlen weinend,", "tokens": ["Zar\u00b7te", "Sil\u00b7ber\u00b7per\u00b7len", "wei\u00b7nend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flehet so die arme Dame:", "tokens": ["Fle\u00b7het", "so", "die", "ar\u00b7me", "Da\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbholdes, liebes, frisches L\u00fcftchen,", "tokens": ["\u00bb", "hol\u00b7des", ",", "lie\u00b7bes", ",", "fri\u00b7sches", "L\u00fcft\u00b7chen", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Warest du der Flora Liebling;", "tokens": ["Wa\u00b7rest", "du", "der", "Flo\u00b7ra", "Lieb\u00b7ling", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denk an deine ersten K\u00fcsse", "tokens": ["Denk", "an", "dei\u00b7ne", "ers\u00b7ten", "K\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und errette unsre Liebe.", "tokens": ["Und", "er\u00b7ret\u00b7te", "uns\u00b7re", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Du der mit der G\u00f6tter Allmacht,", "tokens": ["Du", "der", "mit", "der", "G\u00f6t\u00b7ter", "All\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wenn du auf ein Schif ergrimmest,", "tokens": ["Wenn", "du", "auf", "ein", "Schif", "er\u00b7grim\u00b7mest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schleuderst es auf Sand des Meeres", "tokens": ["Schleu\u00b7derst", "es", "auf", "Sand", "des", "Mee\u00b7res"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als obs hundert Felsen w\u00e4ren;", "tokens": ["Als", "obs", "hun\u00b7dert", "Fel\u00b7sen", "w\u00e4\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "CARD", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und der mit der G\u00f6tter Linde,", "tokens": ["Und", "der", "mit", "der", "G\u00f6t\u00b7ter", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn dir gute Menschen flehen,", "tokens": ["Wenn", "dir", "gu\u00b7te", "Men\u00b7schen", "fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine [arme] Meerestr\u00fcmmer", "tokens": ["Ei\u00b7ne", "ar\u00b7me", "Mee\u00b7rest\u00b7r\u00fcm\u00b7mer"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["ART", "$(", "ADJA", "$(", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Kanst aus K\u00f6nigsflotten retten;", "tokens": ["Kanst", "aus", "K\u00f6\u00b7nigs\u00b7flot\u00b7ten", "ret\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Rette unser liebend Segel", "tokens": ["Ret\u00b7te", "un\u00b7ser", "lie\u00b7bend", "Se\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus den H\u00e4nden jener R\u00e4uber,", "tokens": ["Aus", "den", "H\u00e4n\u00b7den", "je\u00b7ner", "R\u00e4u\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie du aus der Geyer Klauen", "tokens": ["Wie", "du", "aus", "der", "Ge\u00b7yer", "Klau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "tr