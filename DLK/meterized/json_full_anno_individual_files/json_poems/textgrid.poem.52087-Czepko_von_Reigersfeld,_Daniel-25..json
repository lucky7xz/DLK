{"textgrid.poem.52087": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du f\u00fchrst, als wie man sieht zwar einen schweren Orden,", "tokens": ["Du", "f\u00fchrst", ",", "als", "wie", "man", "sieht", "zwar", "ei\u00b7nen", "schwe\u00b7ren", "Or\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PWAV", "PIS", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil du dir nicht einmahl begehrest auffzustehn,", "tokens": ["Weil", "du", "dir", "nicht", "ein\u00b7mahl", "be\u00b7geh\u00b7rest", "auff\u00b7zu\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "ADV", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch bist du mehr besucht, als die wir t\u00e4glich gehn,", "tokens": ["Doch", "bist", "du", "mehr", "be\u00b7sucht", ",", "als", "die", "wir", "t\u00e4g\u00b7lich", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$,", "KOUS", "ART", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor unsern Augen sehn, o sch\u00f6ne Nymphe, worden.", "tokens": ["Vor", "un\u00b7sern", "Au\u00b7gen", "sehn", ",", "o", "sch\u00f6\u00b7ne", "Nym\u00b7phe", ",", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,", "FM", "ADJA", "NN", "$,", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wann man die Brunnen deckt, so pflegt uns mehr zu d\u00fcrsten,", "tokens": ["Wann", "man", "die", "Brun\u00b7nen", "deckt", ",", "so", "pflegt", "uns", "mehr", "zu", "d\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Steh' auff, es sta\u00fcbt zu sehr, wo Bett und Federn seyn,", "tokens": ["Steh'", "auff", ",", "es", "sta\u00fcbt", "zu", "sehr", ",", "wo", "Bett", "und", "Fe\u00b7dern", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "ADV", "$,", "PWAV", "NN", "KON", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da hat ein gutes Kleid mit ihnen nichts gemein,", "tokens": ["Da", "hat", "ein", "gu\u00b7tes", "Kleid", "mit", "ih\u00b7nen", "nichts", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPER", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dann welche zu dir gehn, die sollen immer b\u00fcrsten.", "tokens": ["Dann", "wel\u00b7che", "zu", "dir", "gehn", ",", "die", "sol\u00b7len", "im\u00b7mer", "b\u00fcrs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRELS", "APPR", "PPER", "VVINF", "$,", "PRELS", "VMFIN", "ADV", "VVFIN", "$."], "m