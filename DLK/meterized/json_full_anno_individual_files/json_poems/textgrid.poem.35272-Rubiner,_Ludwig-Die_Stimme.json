{"textgrid.poem.35272": {"metadata": {"author": {"name": "Rubiner, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Stimme", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Mund, der nun spricht, hinschwingend in durchsichtigen St\u00f6\u00dfen \u00fcber die gew\u00f6lbten Meere.", "tokens": ["O", "Mund", ",", "der", "nun", "spricht", ",", "hin\u00b7schwin\u00b7gend", "in", "durch\u00b7sich\u00b7ti\u00b7gen", "St\u00f6\u00b7\u00dfen", "\u00fc\u00b7ber", "die", "ge\u00b7w\u00f6lb\u00b7ten", "Mee\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "VVPP", "APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "O Mund, rundes schallendes Tor, Hall und Lust, Volkschoral, da\u00df der Saal mitschwang.", "tokens": ["O", "Mund", ",", "run\u00b7des", "schal\u00b7len\u00b7des", "Tor", ",", "Hall", "und", "Lust", ",", "Volk\u00b7scho\u00b7ral", ",", "da\u00df", "der", "Saal", "mit\u00b7schwang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "ADJA", "NN", "$,", "NE", "KON", "NN", "$,", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--++-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "O bitterer N\u00e4hm\u00e4dchenmund, der nach Gerechtigkeit klagt und schrill Groschen und Wiegpfunde z\u00e4hlt.", "tokens": ["O", "bit\u00b7te\u00b7rer", "N\u00e4h\u00b7m\u00e4d\u00b7chen\u00b7mund", ",", "der", "nach", "Ge\u00b7rech\u00b7tig\u00b7keit", "klagt", "und", "schrill