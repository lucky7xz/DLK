{"dta.poem.7932": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Tells Platte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Hier ist das Felsenriff, drauf Tell aus der Barke gesprungen;", "tokens": ["Hier", "ist", "das", "Fel\u00b7sen\u00b7riff", ",", "drauf", "Tell", "aus", "der", "Bar\u00b7ke", "ge\u00b7sprun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PAV", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sieh! ein ewiges Mal hebet dem K\u00fchnen sich hier.", "tokens": ["Sieh", "!", "ein", "e\u00b7wi\u00b7ges", "Mal", "he\u00b7bet", "dem", "K\u00fch\u00b7nen", "sich", "hier", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PRF", "ADV", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Nicht die Kapelle dort, wo sie j\u00e4hrliche Messen ihm singen!", "tokens": ["Nicht", "die", "Ka\u00b7pel\u00b7le", "dort", ",", "wo", "sie", "j\u00e4hr\u00b7li\u00b7che", "Mes\u00b7sen", "ihm", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADV", "$,", "PWAV", "PPER", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Nein! des Mannes Gestalt, siehst du, wie herrlich sie steht?", "tokens": ["Nein", "!", "des", "Man\u00b7nes", "Ge\u00b7stalt", ",", "siehst", "du", ",", "wie", "herr\u00b7lich", "sie", "steht", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "NN", "NN", "$,", "VVFIN", "PPER", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Schon mit dem einen Fu\u00dfe betrat er die heilige Erde,", "tokens": ["Schon", "mit", "dem", "ei\u00b7nen", "Fu\u00b7\u00dfe", "be\u00b7trat", "er", "die", "hei\u00b7li\u00b7ge", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "---+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "St\u00f6\u00dft mit dem andern hinaus weit das verzweifelnde Schiff.", "tokens": ["St\u00f6\u00dft", "mit", "dem", "an\u00b7dern", "hin\u00b7aus", "weit", "das", "ver\u00b7zwei\u00b7feln\u00b7de", "Schiff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "APZR", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Nicht aus Stein ist das Bild, noch von Erz, nicht Arbeit der", "tokens": ["Nicht", "aus", "Stein", "ist", "das", "Bild", ",", "noch", "von", "Erz", ",", "nicht", "Ar\u00b7beit", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "VAFIN", "ART", "NN", "$,", "ADV", "APPR", "NN", "$,", "PTKNEG", "NN", "ART"], "meter": "--+--+--+-+--", "measure": "anapaest.tri.plus"}, "line.8": {"text": "H\u00e4nde,", "tokens": ["H\u00e4n\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Nur dem geistigen Blick Freier erscheinet es klar;", "tokens": ["Nur", "dem", "geis\u00b7ti\u00b7gen", "Blick", "Frei\u00b7er", "er\u00b7schei\u00b7net", "es", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Und je wilder der Sturm, je h\u00f6her brauset die Brandung,", "tokens": ["Und", "je", "wil\u00b7der", "der", "Sturm", ",", "je", "h\u00f6\u00b7her", "brau\u00b7set", "die", "Bran\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "ART", "NN", "$,", "ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+---+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Um so m\u00e4chtiger nur hebt sich die Heldengestalt.", "tokens": ["Um", "so", "m\u00e4ch\u00b7ti\u00b7ger", "nur", "hebt", "sich", "die", "Hel\u00b7den\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJA", "ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}