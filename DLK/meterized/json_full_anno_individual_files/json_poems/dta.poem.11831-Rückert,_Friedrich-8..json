{"dta.poem.11831": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dich lehrt ein Sprichwort: Nie trink aus zersprungnem Glase!", "tokens": ["Dich", "lehrt", "ein", "Sprich\u00b7wort", ":", "Nie", "trink", "aus", "zer\u00b7sprung\u00b7nem", "Gla\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein junges Leben welkt sonst hin gleich m\u00fcrbem Grase.", "tokens": ["Dein", "jun\u00b7ges", "Le\u00b7ben", "welkt", "sonst", "hin", "gleich", "m\u00fcr\u00b7bem", "Gra\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich aber lehre dich: Nicht deinen Leib zerr\u00fctten", "tokens": ["Ich", "a\u00b7ber", "leh\u00b7re", "dich", ":", "Nicht", "dei\u00b7nen", "Leib", "zer\u00b7r\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$.", "PTKNEG", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird das zerr\u00fcttete, doch deine Lust versch\u00fctten.", "tokens": ["Wird", "das", "zer\u00b7r\u00fct\u00b7te\u00b7te", ",", "doch", "dei\u00b7ne", "Lust", "ver\u00b7sch\u00fct\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn wenn beim frohen Fest du willst mit ihm anklingen,", "tokens": ["Denn", "wenn", "beim", "fro\u00b7hen", "Fest", "du", "willst", "mit", "ihm", "an\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "ADJA", "NN", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird es klappen und dir in der Hand zerspringen.", "tokens": ["So", "wird", "es", "klap\u00b7pen", "und", "dir", "in", "der", "Hand", "zer\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "KON", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "So gie\u00df auch dein Vertraun, dein Lieben rein und jung,", "tokens": ["So", "gie\u00df", "auch", "dein", "Ver\u00b7traun", ",", "dein", "Lie\u00b7ben", "rein", "und", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nie in ein schadhaft Herz, das Ri\u00df hat oder Sprung.", "tokens": ["Nie", "in", "ein", "schad\u00b7haft", "Herz", ",", "das", "Ri\u00df", "hat", "o\u00b7der", "Sprung", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJD", "NN", "$,", "ART", "NN", "VAFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du hoffst es werde rein mit dir zusammenklingen,", "tokens": ["Du", "hoffst", "es", "wer\u00b7de", "rein", "mit", "dir", "zu\u00b7sam\u00b7men\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da wirds den Herzenswein versch\u00fctten und zerspringen.", "tokens": ["Da", "wirds", "den", "Her\u00b7zens\u00b7wein", "ver\u00b7sch\u00fct\u00b7ten", "und", "zer\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}