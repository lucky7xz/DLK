{"dta.poem.2915": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "114.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der schlimmste Neider ist, der das sich l\u00e4\u00dft verdrie\u00dfen,", "tokens": ["Der", "schlimms\u00b7te", "Nei\u00b7der", "ist", ",", "der", "das", "sich", "l\u00e4\u00dft", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PRELS", "ART", "PRF", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn, was er nicht mehr kann, nun andere genie\u00dfen.", "tokens": ["Wenn", ",", "was", "er", "nicht", "mehr", "kann", ",", "nun", "an\u00b7de\u00b7re", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "PPER", "PTKNEG", "ADV", "VMFIN", "$,", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kann einen hungrigen der satte wol beneiden,", "tokens": ["Kann", "ei\u00b7nen", "hung\u00b7ri\u00b7gen", "der", "sat\u00b7te", "wol", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und m\u00f6chte lieber selbst noch einmal Hunger leiden?", "tokens": ["Und", "m\u00f6ch\u00b7te", "lie\u00b7ber", "selbst", "noch", "ein\u00b7mal", "Hun\u00b7ger", "lei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Begierde \u2014 schlimm genung, da\u00df sie Befriedigung", "tokens": ["Be\u00b7gier\u00b7de", "schlimm", "ge\u00b7nung", ",", "da\u00df", "sie", "Be\u00b7frie\u00b7di\u00b7gung"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "ADJD", "ADV", "$,", "KOUS", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begehrte, da sie war am Leben frisch und jung;", "tokens": ["Be\u00b7gehr\u00b7te", ",", "da", "sie", "war", "am", "Le\u00b7ben", "frisch", "und", "jung", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VAFIN", "APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nun sie gestorben ist, so sei sie auch begraben!", "tokens": ["Nun", "sie", "ge\u00b7stor\u00b7ben", "ist", ",", "so", "sei", "sie", "auch", "be\u00b7gra\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir freun uns, da\u00df wir Ruh, die Unruh andre haben.", "tokens": ["Wir", "freun", "uns", ",", "da\u00df", "wir", "Ruh", ",", "die", "Un\u00b7ruh", "and\u00b7re", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "NN", "$,", "ART", "NN", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}