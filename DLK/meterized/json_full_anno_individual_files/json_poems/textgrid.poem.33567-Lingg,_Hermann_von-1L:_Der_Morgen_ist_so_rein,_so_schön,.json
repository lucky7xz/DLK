{"textgrid.poem.33567": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Morgen ist so rein, so sch\u00f6n,", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Morgen ist so rein, so sch\u00f6n,", "tokens": ["Der", "Mor\u00b7gen", "ist", "so", "rein", ",", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es wogt in den Wellen der brausende F\u00f6hn.", "tokens": ["Es", "wogt", "in", "den", "Wel\u00b7len", "der", "brau\u00b7sen\u00b7de", "F\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich seh' einen Stern, er sinkt in die Flut,", "tokens": ["Ich", "seh'", "ei\u00b7nen", "Stern", ",", "er", "sinkt", "in", "die", "Flut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Stern und ich, wir kennen uns gut.", "tokens": ["Der", "Stern", "und", "ich", ",", "wir", "ken\u00b7nen", "uns", "gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "O h\u00e4tte mir stets geleuchtet sein Glanz,", "tokens": ["O", "h\u00e4t\u00b7te", "mir", 