{"textgrid.poem.56237": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Gern und gerner", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Gang war schwer, der Tag war rauh,", "tokens": ["Der", "Gang", "war", "schwer", ",", "der", "Tag", "war", "rauh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kalt weht' es und st\u00fcrmisch aus Norden;", "tokens": ["Kalt", "weht'", "es", "und", "st\u00fcr\u00b7misch", "aus", "Nor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "ADJD", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es trieft mein Haar vom Abendtau,", "tokens": ["Es", "trieft", "mein", "Haar", "vom", "A\u00b7bend\u00b7tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fast w\u00e4r ich m\u00fcde geworden.", "tokens": ["Fast", "w\u00e4r", "ich", "m\u00fc\u00b7de", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VAPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "La\u00df blinken den roten, den s\u00fc\u00dfen Wein:", "tokens": ["La\u00df", "blin\u00b7ken", "den", "ro\u00b7ten", ",", "den", "s\u00fc\u00b7\u00dfen", "Wein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART"