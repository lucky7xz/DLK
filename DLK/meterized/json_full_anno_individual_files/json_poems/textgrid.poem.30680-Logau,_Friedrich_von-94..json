{"textgrid.poem.30680": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "94.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man solt uns H\u00fclffe thun. Da nam man ein Gebie\u00df,", "tokens": ["Man", "solt", "uns", "H\u00fclf\u00b7fe", "thun", ".", "Da", "nam", "man", "ein", "Ge\u00b7bie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "NN", "VVINF", "$.", "ADV", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das man in unser Maul, uns zu beschreiten, stie\u00df.", "tokens": ["Das", "man", "in", "un\u00b7ser", "Maul", ",", "uns", "zu", "be\u00b7schrei\u00b7ten", ",", "stie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PIS", "APPR", "PPOSAT", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man riet uns hin und her; man lie\u00df uns keine Ruh", "tokens": ["Man", "riet", "uns", "hin", "und", "her", ";", "man", "lie\u00df", "uns", "kei\u00b7ne", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$.", "PIS", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sagte, da\u00df man uns riet unsrer Wolfahrt zu.", "tokens": ["Und", "sag\u00b7te", ",", "da\u00df", "man", "uns", "riet", "uns\u00b7rer", "Wol\u00b7fahrt", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Wolfahrt, die es war, die war also bewand,", "tokens": ["Die", "Wol\u00b7fahrt", ",", "die", "es", "war", ",", "die", "war", "al\u00b7so", "be\u00b7wand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,", "PRELS", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df, eh man sie gef\u00fchlt, man uns zu Lager rand.", "tokens": ["Da\u00df", ",", "eh", "man", "sie", "ge\u00b7f\u00fchlt", ",", "man", "uns", "zu", "La\u00b7ger", "rand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PIS", "PPER", "VVPP", "$,", "PIS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Man solt uns H\u00fclffe thun. Da nam man ein Gebie\u00df,", "tokens": ["Man", "solt", "uns", "H\u00fclf\u00b7fe", "thun", ".", "Da", "nam", "man", "ein", "Ge\u00b7bie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "NN", "VVINF", "$.", "ADV", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das man in unser Maul, uns zu beschreiten, stie\u00df.", "tokens": ["Das", "man", "in", "un\u00b7ser", "Maul", ",", "uns", "zu", "be\u00b7schrei\u00b7ten", ",", "stie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PIS", "APPR", "PPOSAT", "NN", "$,", "PPER", "PTKZU", "VVINF