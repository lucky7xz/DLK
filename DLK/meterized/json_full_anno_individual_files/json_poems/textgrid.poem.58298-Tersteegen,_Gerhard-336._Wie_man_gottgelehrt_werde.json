{"textgrid.poem.58298": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "336. Wie man gottgelehrt werde", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel Forschen der Vernunft erreicht nicht Gottes Licht,", "tokens": ["Viel", "For\u00b7schen", "der", "Ver\u00b7nunft", "er\u00b7reicht", "nicht", "Got\u00b7tes", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVPP", "PTKNEG", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Solang du S\u00fcnde tust, kennst du die Wahrheit nicht;", "tokens": ["So\u00b7lang", "du", "S\u00fcn\u00b7de", "tust", ",", "kennst", "du", "die", "Wahr\u00b7heit", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mach nur dein Herze leer und sanft und gottgelassen,", "tokens": ["Mach", "nur", "dein", "Her\u00b7ze", "leer", "und", "sanft", "und", "gott\u00b7ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "VVFIN", "ADJD", "KON", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So kannst du leichtlich Gott und seine Wahrheit fassen!", "tokens": ["So", "kannst", "du", "leicht\u00b7lich", "Gott", "und", "sei\u00b7ne", "Wahr\u00b7heit", "fas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "NN", "KON", "PPOSAT