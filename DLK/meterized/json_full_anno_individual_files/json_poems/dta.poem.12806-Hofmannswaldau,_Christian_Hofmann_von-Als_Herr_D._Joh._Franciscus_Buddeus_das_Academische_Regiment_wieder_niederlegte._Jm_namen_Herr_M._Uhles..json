{"dta.poem.12806": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als  \n Herr  D.  Joh. Franciscus Buddeus  \n das Academische Regiment wieder  \n niederlegte.  \n Jm namen Herr  M.  Uhles.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wem es an weisheit fehlt,", "tokens": ["Wem", "es", "an", "weis\u00b7heit", "fehlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der hat sich sonder streit", "tokens": ["Der", "hat", "sich", "son\u00b7der", "streit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit der gerechtigkeit,", "tokens": ["Mit", "der", "ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit gottesfurcht und sanfftmuth nie verm\u00e4hlt.", "tokens": ["Mit", "got\u00b7tes\u00b7furcht", "und", "sanfft\u00b7muth", "nie", "ver\u00b7m\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}