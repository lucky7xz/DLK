{"dta.poem.16635": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "296.  Die N\u00e4chsten GOttes gespielen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gott\u2019 ist nicht alles nah: die Jungfraw und das Kind/", "tokens": ["Gott'", "ist", "nicht", "al\u00b7les", "nah", ":", "die", "Jung\u00b7fraw", "und", "das", "Kind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKNEG", "PIS", "ADJD", "$.", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die zwey die sinds allein die Gottsgespielen sind.", "tokens": ["Die", "zwey", "die", "sinds", "al\u00b7lein", "die", "Gotts\u00b7ge\u00b7spie\u00b7len", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ART", "NN", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}