{"dta.poem.1020": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Die jagd hat sich verzogen               ", "tokens": ["Die", "jagd", "hat", "sich", "ver\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du bleibst mit tr&#228;gem bogen &#8212;               ", "tokens": ["Du", "bleibst", "mit", "tr", "&#228;", "gem", "bo\u00b7gen", "&#8212;"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "XML_entity"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$(", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Blutspuren unter tannen               ", "tokens": ["Blut\u00b7spu\u00b7ren", "un\u00b7ter", "tan\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Horch welch ein laut! von wannen?             ", "tokens": ["Horch", "welch", "ein", "laut", "!", "von", "wan\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PWAT", "ART", "ADJD", "$.", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das ist kein l&#228;rm der r&#252;den               ", "tokens": ["Das", "ist", "kein", "l", "&#228;", "rm", "der", "r", "&#252;", "den"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "$(", "FM.la", "FM.la", "FM.la", "$(", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kein schrei der fl&#252;chtig-m&#252;den               ", "tokens": ["Kein", "schrei", "der", "fl", "&#252;", "chtig\u00b7\u00b7m", "&#252;", "den"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "XML_entity", "word"], "pos": ["PIAT", "NN", "ART", "FM", "FM", "FM", "$(", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du lauschst am grund beklommen               ", "tokens": ["Du", "lauschst", "am", "grund", "be\u00b7klom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sollst du entgegenkommen?             ", "tokens": ["Sollst", "du", "ent\u00b7ge\u00b7gen\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nur still! schon dringt er n&#228;her               ", "tokens": ["Nur", "still", "!", "schon", "dringt", "er", "n", "&#228;", "her"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["ADV", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "XY", "$(", "APZR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Dir schien verirrter sp&#228;her               ", "tokens": ["Dir", "schien", "ver\u00b7irr\u00b7ter", "sp", "&#228;", "her"], "token_info": ["word", "word", "word", "word", "XML_entity", "word"], "pos": ["PPER", "VVFIN", "ADJD", "VVFIN", "$(", "APZR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im widerschall der hiefe               ", "tokens": ["Im", "wi\u00b7der\u00b7schall", "der", "hie\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dass jene stimme riefe.             ", "tokens": ["Dass", "je\u00b7ne", "stim\u00b7me", "rie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}