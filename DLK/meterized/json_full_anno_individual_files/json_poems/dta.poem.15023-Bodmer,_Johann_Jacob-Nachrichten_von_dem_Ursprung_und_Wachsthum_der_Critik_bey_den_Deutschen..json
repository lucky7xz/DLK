{"dta.poem.15023": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Nachrichten von dem Ursprung und  \n Wachsthum der Critik bey den  \n Deutschen.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1741", "urn": "urn:nbn:de:kobv:b4-200905198526", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir k\u00f6nnen freylich nicht vorbey, mein Vaterland,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "frey\u00b7lich", "nicht", "vor\u00b7bey", ",", "mein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und m\u00fcssen nur gestehn, der Himmel sey entbrandt", "tokens": ["Und", "m\u00fcs\u00b7sen", "nur", "ge\u00b7stehn", ",", "der", "Him\u00b7mel", "sey", "ent\u00b7brandt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "VVPP", "$,", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus Eifer gegen uns, nachdem so schwere Plagen", "tokens": ["Aus", "Ei\u00b7fer", "ge\u00b7gen", "uns", ",", "nach\u00b7dem", "so", "schwe\u00b7re", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "PPER", "$,", "KOUS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von allen Ecken her bey dir zusammenschlagen.", "tokens": ["Von", "al\u00b7len", "E\u00b7cken", "her", "bey", "dir", "zu\u00b7sam\u00b7men\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APZR", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u2012 \u2012 \u2012 \u2012 \u2012 \u2012 \u2012 \u2012 \u2012", "tokens": ["\u2012", "\u2012", "\u2012", "\u2012", "\u2012", "\u2012", "\u2012", "\u2012", "\u2012"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "\u2012 \u2012 \u2012 Doch gleichwohl ist ein Gott,", "tokens": ["\u2012", "\u2012", "\u2012", "Doch", "gleich\u00b7wohl", "ist", "ein", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "$(", "KON", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Der seiner Gnaden Licht auch mitten in der Noth", "tokens": ["Der", "sei\u00b7ner", "Gna\u00b7den", "Licht", "auch", "mit\u00b7ten", "in", "der", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "NN", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Des tr\u00fcben Wetters zeigt.", "tokens": ["Des", "tr\u00fc\u00b7ben", "Wet\u00b7ters", "zeigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "\u2012 \u2012 \u2012 \u2012 \u2012 \u2012 \u2012 \u2012", "tokens": ["\u2012", "\u2012", "\u2012", "\u2012", "\u2012", "\u2012", "\u2012", "\u2012"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wo Caurus um den Korb des hohen Mastes pfeift,", "tokens": ["Wo", "Cau\u00b7rus", "um", "den", "Korb", "des", "ho\u00b7hen", "Mas\u00b7tes", "pfeift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wo die ergrimmte See mit ganzen Wellen streift,", "tokens": ["Wo", "die", "er\u00b7grimm\u00b7te", "See", "mit", "gan\u00b7zen", "Wel\u00b7len", "streift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und jagt das Schiff grundab. Doch der wird Hand anlegen,", "tokens": ["Und", "jagt", "das", "Schiff", "grund\u00b7ab", ".", "Doch", "der", "wird", "Hand", "an\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "$.", "KON", "PDS", "VAFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der lieb hat, wenn er z\u00f6rnt.", "tokens": ["Der", "lieb", "hat", ",", "wenn", "er", "z\u00f6rnt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}