{"textgrid.poem.52618": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Beim Wein", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn laute Becher klingen", "tokens": ["Wenn", "lau\u00b7te", "Be\u00b7cher", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und golden gr\u00fc\u00dft der Wein,", "tokens": ["Und", "gol\u00b7den", "gr\u00fc\u00dft", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wollen wir auch singen", "tokens": ["So", "wol\u00b7len", "wir", "auch", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und guter Dinge sein,", "tokens": ["Und", "gu\u00b7ter", "Din\u00b7ge", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So wollen wir, so sollen", "tokens": ["So", "wol\u00b7len", "wir", ",", "so", "sol\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "$,", "ADV", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wir bis der Tag erwacht,", "tokens": ["Wir", "bis", "der", "Tag", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Durchjubeln und durchtollen", "tokens": ["Durch\u00b7ju\u00b7beln", "und", "durc