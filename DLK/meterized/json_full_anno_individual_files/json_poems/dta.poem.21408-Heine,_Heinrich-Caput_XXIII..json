{"dta.poem.21408": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie trat zu mir heran und sprach:", "tokens": ["Sie", "trat", "zu", "mir", "he\u00b7ran", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ewillkommen an der Elbe,", "tokens": ["\u201e", "will\u00b7kom\u00b7men", "an", "der", "El\u00b7be", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nach dreyzehnj\u00e4hr\u2019ger Abwesenheit \u2013", "tokens": ["Nach", "drey\u00b7zehn\u00b7j\u00e4hr'\u00b7ger", "Ab\u00b7we\u00b7sen\u00b7heit", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ich sehe du bist noch derselbe!", "tokens": ["Ich", "se\u00b7he", "du", "bist", "noch", "der\u00b7sel\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "ADV", "PDAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201edu suchst die sch\u00f6nen Seelen vielleicht,", "tokens": ["\u201e", "du", "suchst", "die", "sch\u00f6\u00b7nen", "See\u00b7len", "viel\u00b7leicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die dir so oft begegen\u2019t", "tokens": ["Die", "dir", "so", "oft", "be\u00b7ge\u00b7gen't"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und mit dir geschw\u00e4rmt die Nacht hindurch,", "tokens": ["Und", "mit", "dir", "ge\u00b7schw\u00e4rmt", "die", "Nacht", "hin\u00b7durch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVPP", "ART", "NN", "PAV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "In dieser sch\u00f6nen Gegend.", "tokens": ["In", "die\u00b7ser", "sch\u00f6\u00b7nen", "Ge\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201edas Leben verschlang sie, das Ungeth\u00fcm,", "tokens": ["\u201e", "das", "Le\u00b7ben", "ver\u00b7schlang", "sie", ",", "das", "Un\u00b7ge\u00b7th\u00fcm", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die hundertk\u00f6pfige Hyder\u2019;", "tokens": ["Die", "hun\u00b7dert\u00b7k\u00f6p\u00b7fi\u00b7ge", "Hyder'", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Du findest nicht die alte Zeit", "tokens": ["Du", "fin\u00b7dest", "nicht", "die", "al\u00b7te", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und die Zeitgen\u00f6ssinnen wieder!", "tokens": ["Und", "die", "Zeit\u00b7ge\u00b7n\u00f6s\u00b7sin\u00b7nen", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "\u201edu findest die holden Blumen nicht mehr,", "tokens": ["\u201e", "du", "fin\u00b7dest", "die", "hol\u00b7den", "Blu\u00b7men", "nicht", "mehr", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die das junge Herz verg\u00f6ttert;", "tokens": ["Die", "das", "jun\u00b7ge", "Herz", "ver\u00b7g\u00f6t\u00b7tert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier bl\u00fchten sie \u2013 jetzt sind sie verwelkt,", "tokens": ["Hier", "bl\u00fch\u00b7ten", "sie", "\u2013", "jetzt", "sind", "sie", "ver\u00b7welkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und der Sturm hat sie entbl\u00e4ttert.", "tokens": ["Und", "der", "Sturm", "hat", "sie", "ent\u00b7bl\u00e4t\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}}}}