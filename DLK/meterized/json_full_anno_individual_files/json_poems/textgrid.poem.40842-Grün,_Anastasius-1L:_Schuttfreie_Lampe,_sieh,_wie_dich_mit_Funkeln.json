{"textgrid.poem.40842": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schuttfreie Lampe, sieh, wie dich mit Funkeln", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schuttfreie Lampe, sieh, wie dich mit Funkeln", "tokens": ["Schutt\u00b7frei\u00b7e", "Lam\u00b7pe", ",", "sieh", ",", "wie", "dich", "mit", "Fun\u00b7keln"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,", "PWAV", "PPER", "APPR", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Des Lichtes, deines Vaters Augen gr\u00fc\u00dfen,", "tokens": ["Des", "Lich\u00b7tes", ",", "dei\u00b7nes", "Va\u00b7ters", "Au\u00b7gen", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seit dich aus tausendj\u00e4hr'gem Kerkerdunkeln", "tokens": ["Seit", "dich", "aus", "tau\u00b7send\u00b7j\u00e4hr'\u00b7gem", "Ker\u00b7ker\u00b7dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Schaufel seiner Feindin Nacht entrissen!", "tokens": ["Die", "Schau\u00b7fel", "sei\u00b7ner", "Fein\u00b7din", "Nacht", "ent\u00b7ris\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Erf\u00fcllt hast du den Lichtberuf, den edeln,", "tokens": ["Er\u00b7f\u00fcllt", "hast", "du", "den", "Licht\u00b7be\u00b7ruf", ",", "den", "e\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch k\u00fcndet's deiner M\u00fcndung Kohlenfarbe;", "tokens": ["Noch", "k\u00fcn\u00b7det's", "dei\u00b7ner", "M\u00fcn\u00b7dung", "Koh\u00b7len\u00b7far\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie steht dir gut, wie bleichen Kriegersch\u00e4deln", "tokens": ["Sie", "steht", "dir", "gut", ",", "wie", "blei\u00b7chen", "Krie\u00b7ger\u00b7sch\u00e4\u00b7deln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des alten Schlachtfelds tiefe Ehrennarbe.", "tokens": ["Des", "al\u00b7ten", "Schlacht\u00b7felds", "tie\u00b7fe", "Eh\u00b7ren\u00b7nar\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ob einst dein Licht am Bett der Liebe blinkte?", "tokens": ["Ob", "einst", "dein", "Licht", "am", "Bett", "der", "Lie\u00b7be", "blink\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da warst du in der N\u00e4chte Ozeane", "tokens": ["Da", "warst", "du", "in", "der", "N\u00e4ch\u00b7te", "O\u00b7zea\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein Schifflein, dem vom Borde f\u00e4chelnd winkte", "tokens": ["Ein", "Schif\u00b7flein", ",", "dem", "vom", "Bor\u00b7de", "f\u00e4\u00b7chelnd", "wink\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Liebeshafen deine Flamm' als Fahne.", "tokens": ["Zum", "Lie\u00b7bes\u00b7ha\u00b7fen", "dei\u00b7ne", "Flamm'", "als", "Fah\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ob einst dein Strahlenschrein vielleicht geschimmert", "tokens": ["Ob", "einst", "dein", "Strah\u00b7len\u00b7schrein", "viel\u00b7leicht", "ge\u00b7schim\u00b7mert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als Ph\u00f6buswagen durch die Nacht des Weisen,", "tokens": ["Als", "Ph\u00f6\u00b7bus\u00b7wa\u00b7gen", "durch", "die", "Nacht", "des", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "De\u00df Herz, von Menschenelend tief bek\u00fcmmert,", "tokens": ["De\u00df", "Herz", ",", "von", "Men\u00b7sche\u00b7ne\u00b7lend", "tief", "be\u00b7k\u00fcm\u00b7mert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nachforscht des Gl\u00fcckes lichten, sel'gen Gleisen?", "tokens": ["Nach\u00b7forscht", "des", "Gl\u00fc\u00b7ckes", "lich\u00b7ten", ",", "sel'\u00b7gen", "Glei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}}, "stanza.5": {"line.1": {"text": "Da warst das Fr\u00fchroth du, an dessen W\u00e4rme", "tokens": ["Da", "warst", "das", "Fr\u00fch\u00b7roth", "du", ",", "an", "des\u00b7sen", "W\u00e4r\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Geistes Rosen bl\u00fchend ausgegangen,", "tokens": ["Des", "Geis\u00b7tes", "Ro\u00b7sen", "bl\u00fc\u00b7hend", "aus\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um dessen Strahlenkern, wie Lerchenschw\u00e4rme,", "tokens": ["Um", "des\u00b7sen", "Strah\u00b7len\u00b7kern", ",", "wie", "Ler\u00b7chen\u00b7schw\u00e4r\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gedanken ihre jungen Fl\u00fcgel schwangen.", "tokens": ["Ge\u00b7dan\u00b7ken", "ih\u00b7re", "jun\u00b7gen", "Fl\u00fc\u00b7gel", "schwan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die Rosen werden Kr\u00e4nze, die auf Erden", "tokens": ["Die", "Ro\u00b7sen", "wer\u00b7den", "Kr\u00e4n\u00b7ze", ",", "die", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der alten G\u00f6tter Tempel reich umschlingen;", "tokens": ["Der", "al\u00b7ten", "G\u00f6t\u00b7ter", "Tem\u00b7pel", "reich", "um\u00b7schlin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Lerchen aber, Fl\u00fcgelbarden, werden", "tokens": ["Die", "Ler\u00b7chen", "a\u00b7ber", ",", "Fl\u00fc\u00b7gel\u00b7bar\u00b7den", ",", "wer\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "ADV", "$,", "NN", "$,", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der alten G\u00f6tter Preis am Himmel singen", "tokens": ["Der", "al\u00b7ten", "G\u00f6t\u00b7ter", "Preis", "am", "Him\u00b7mel", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "So sann und nickt' einst ein am Tisch von Steine", "tokens": ["So", "sann", "und", "nickt'", "einst", "ein", "am", "Tisch", "von", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ADV", "ART", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Weisen Haupt, als wenn's noch pr\u00fcfen werde,", "tokens": ["Des", "Wei\u00b7sen", "Haupt", ",", "als", "wenn's", "noch", "pr\u00fc\u00b7fen", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KOUS", "PIS", "ADV", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob selbst es nun, ob jener k\u00e4lter scheine?", "tokens": ["Ob", "selbst", "es", "nun", ",", "ob", "je\u00b7ner", "k\u00e4l\u00b7ter", "schei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADV", "$,", "KOUS", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch rollt, des alten Elends voll, die Erde!", "tokens": ["Noch", "rollt", ",", "des", "al\u00b7ten", "E\u00b7lends", "voll", ",", "die", "Er\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ART", "ADJA", "NN", "ADJD", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ein Andrer kam; und wieder, Lampe, zittert", "tokens": ["Ein", "A\u00b7ndrer", "kam", ";", "und", "wie\u00b7der", ",", "Lam\u00b7pe", ",", "zit\u00b7tert"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "ADV", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Strahlenschrein am Tische eines Weisen,", "tokens": ["Dein", "Strah\u00b7len\u00b7schrein", "am", "Ti\u00b7sche", "ei\u00b7nes", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "De\u00df Herz, vom Menschenelend tief ersch\u00fcttert,", "tokens": ["De\u00df", "Herz", ",", "vom", "Men\u00b7sche\u00b7ne\u00b7lend", "tief", "er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nachforscht des Gl\u00fcckes lichten, sel'gen Gleisen.", "tokens": ["Nach\u00b7forscht", "des", "Gl\u00fc\u00b7ckes", "lich\u00b7ten", ",", "sel'\u00b7gen", "Glei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}}, "stanza.9": {"line.1": {"text": "Da warst du eines Scheiterhaufens Lohe,", "tokens": ["Da", "warst", "du", "ei\u00b7nes", "Schei\u00b7ter\u00b7hau\u00b7fens", "Lo\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drein warf die alten, heitren G\u00f6tter alle,", "tokens": ["Drein", "warf", "die", "al\u00b7ten", ",", "hei\u00b7tren", "G\u00f6t\u00b7ter", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie d\u00fcrres Reisig, der Zerst\u00f6rungsfrohe,", "tokens": ["Wie", "d\u00fcr\u00b7res", "Rei\u00b7sig", ",", "der", "Zer\u00b7st\u00f6\u00b7rungs\u00b7fro\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ganz in Staub und Asch' ihr Glanz zerfalle!", "tokens": ["Da\u00df", "ganz", "in", "Staub", "und", "Asch'", "ihr", "Glanz", "zer\u00b7fal\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "KON", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Und l\u00e4chelnd schaut' ins Prasseln er der Flamme,", "tokens": ["Und", "l\u00e4\u00b7chelnd", "schaut'", "ins", "Pras\u00b7seln", "er", "der", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis einst er selbst am grausen Opferheerde", "tokens": ["Bis", "einst", "er", "selbst", "am", "grau\u00b7sen", "Op\u00b7fer\u00b7heer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hinglitt, wie d\u00fcrres Reis vom Lebensstamme!", "tokens": ["Hing\u00b7litt", ",", "wie", "d\u00fcr\u00b7res", "Reis", "vom", "Le\u00b7bens\u00b7stam\u00b7me", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fortrollt, des alten Elends voll, die Erde!", "tokens": ["Fort\u00b7rollt", ",", "des", "al\u00b7ten", "E\u00b7lends", "voll", ",", "die", "Er\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "ADJD", "$,", "ART", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.11": {"line.1": {"text": "Ein Andrer kam; und wieder, Lampe, schimmert", "tokens": ["Ein", "A\u00b7ndrer", "kam", ";", "und", "wie\u00b7der", ",", "Lam\u00b7pe", ",", "schim\u00b7mert"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "ADV", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hehr dein Gedankenpharus einem Weisen,", "tokens": ["Hehr", "dein", "Ge\u00b7dan\u00b7ken\u00b7pha\u00b7rus", "ei\u00b7nem", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "De\u00df Herz, vom Menschenelend tief bek\u00fcmmert,", "tokens": ["De\u00df", "Herz", ",", "vom", "Men\u00b7sche\u00b7ne\u00b7lend", "tief", "be\u00b7k\u00fcm\u00b7mert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nachforscht des Gl\u00fcckes lichten, sel'gen Gleisen.", "tokens": ["Nach\u00b7forscht", "des", "Gl\u00fc\u00b7ckes", "lich\u00b7ten", ",", "sel'\u00b7gen", "Glei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}}, "stanza.12": {"line.1": {"text": "Da wardst die Glorie du, von der umfangen", "tokens": ["Da", "wardst", "die", "Glo\u00b7rie", "du", ",", "von", "der", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Glanzvoll vor ihm das Christuskreuz jetzt ragte,", "tokens": ["Glanz\u00b7voll", "vor", "ihm", "das", "Chris\u00b7tus\u00b7kreuz", "jetzt", "rag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ART", "NN"