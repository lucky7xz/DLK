n Z\u00fcgen", "tokens": ["De\u00df", "Geist", "auf", "des", "Pa\u00b7py\u00b7rus", "wel\u00b7ken", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nachschleicht der Ahnen fernen, lichten Gleisen.", "tokens": ["Nach\u00b7schleicht", "der", "Ah\u00b7nen", "fer\u00b7nen", ",", "lich\u00b7ten", "Glei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.31": {"line.1": {"text": "Ein Lenz, zweitausend Jahr' im Grab vergessen,", "tokens": ["Ein", "Lenz", ",", "zweit\u00b7au\u00b7send", "Jahr'", "im", "Grab", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "CARD", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ries'ger Rosenph\u00f6nix leuchtend, schreitet", "tokens": ["Als", "ries'\u00b7ger", "Ro\u00b7sen\u00b7ph\u00f6\u00b7nix", "leuch\u00b7tend", ",", "schrei\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ADJA", "NN", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus des Papyrus Kohlen ihm, \u2013 indessen", "tokens": ["Aus", "des", "Pa\u00b7py\u00b7rus", "Koh\u00b7len", "ihm", ",", "\u2013", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["APPR", "ART", "NN", "NN", "PPER", "$,", "$(", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein eigner Lenz vor'm Thor vor\u00fcbergleitet!", "tokens": ["Sein", "eig\u00b7ner", "Lenz", "vor'm", "Thor", "vor\u00b7\u00fc\u00b7berg\u00b7lei\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.32": {"line.1": {"text": "Mann, f\u00fcll' mit Oel die Lampe, da\u00df sie heiter", "tokens": ["Mann", ",", "f\u00fcll'", "mit", "O\u00b7el", "die", "Lam\u00b7pe", ",", "da\u00df", "sie", "hei\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPR", "NN", "ART", "NN", "$,", "KOUS", "PPER", "ADJD"], "meter": "+--++-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Zum Tempeldienst des Lichts entz\u00fcndet werde,", "tokens": ["Zum", "Tem\u00b7pel\u00b7dienst", "des", "Lichts", "ent\u00b7z\u00fcn\u00b7det", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sinne du das alte R\u00e4thsel weiter!", "tokens": ["Und", "sin\u00b7ne", "du", "das", "al\u00b7te", "R\u00e4th\u00b7sel", "wei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch rollt, des alten Elends voll, die Erde.", "tokens": ["Noch", "rollt", ",", "des", "al\u00b7ten", "E\u00b7lends", "voll", ",", "die", "Er\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ART", "ADJA", "NN", "ADJD", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}