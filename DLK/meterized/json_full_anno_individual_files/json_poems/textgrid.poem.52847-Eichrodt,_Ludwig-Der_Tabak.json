{"textgrid.poem.52847": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Tabak", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G\u00e4b's keine Kartoffeln und g\u00e4b's kein Brod,", "tokens": ["G\u00e4b's", "kei\u00b7ne", "Kar\u00b7tof\u00b7feln", "und", "g\u00e4b's", "kein", "Brod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "KON", "NE", "PIAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie wollt' der gemeine Mann leben?", "tokens": ["Wie", "wollt'", "der", "ge\u00b7mei\u00b7ne", "Mann", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So h\u00f6ren wir jammern in Zeiten der Noth,", "tokens": ["So", "h\u00f6\u00b7ren", "wir", "jam\u00b7mern", "in", "Zei\u00b7ten", "der", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Als k\u00f6nnt' es nichts Besseres geben.", "tokens": ["Als", "k\u00f6nnt'", "es", "nichts", "Bes\u00b7se\u00b7res", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PIS", "PIS", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Kartoffeln und Brod,", "tokens": ["Kar\u00b7tof\u00b7feln", "und", "Brod", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Thu