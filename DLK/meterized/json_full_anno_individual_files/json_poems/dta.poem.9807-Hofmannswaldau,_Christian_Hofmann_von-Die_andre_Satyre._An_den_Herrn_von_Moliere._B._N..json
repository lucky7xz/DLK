{"dta.poem.9807": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die andre Satyre.  \n An den Herrn von Moliere.  \n B. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ber\u00fchmt und seltner geist/ der wegen seiner gaben", "tokens": ["Be\u00b7r\u00fchmt", "und", "selt\u00b7ner", "geist", "/", "der", "we\u00b7gen", "sei\u00b7ner", "ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJA", "NN", "$(", "ART", "APPR", "PPOSAT", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht wei\u00df/ was ihrer viel f\u00fcr m\u00fch im dichten haben.", "tokens": ["Nicht", "wei\u00df", "/", "was", "ih\u00b7rer", "viel", "f\u00fcr", "m\u00fch", "im", "dich\u00b7ten", "ha\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$(", "PWS", "PPER", "ADV", "APPR", "ADJD", "APPRART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00fcr dem Apollo selbst mu\u00df seinen schatz ausstreu\u2019n/", "tokens": ["F\u00fcr", "dem", "A\u00b7pol\u00b7lo", "selbst", "mu\u00df", "sei\u00b7nen", "schatz", "a\u00b7usstreu'n", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und der gar wohl versteht/ was gute verse seyn.", "tokens": ["Und", "der", "gar", "wohl", "ver\u00b7steht", "/", "was", "gu\u00b7te", "ver\u00b7se", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADV", "VVFIN", "$(", "PWS", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erfahrner held in dem/ was witz und kunst ergr\u00fcndet/", "tokens": ["Er\u00b7fahr\u00b7ner", "held", "in", "dem", "/", "was", "witz", "und", "kunst", "er\u00b7gr\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "$(", "PWS", "NN", "KON", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Moliere/ sage doch/ wie man die reimen findet.", "tokens": ["Mo\u00b7lie\u00b7re", "/", "sa\u00b7ge", "doch", "/", "wie", "man", "die", "rei\u00b7men", "fin\u00b7det", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "ADV", "$(", "PWAV", "PIS", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man schw\u00fcre/ wenn du wilst/ so lieffen sie dir nach;", "tokens": ["Man", "schw\u00fc\u00b7re", "/", "wenn", "du", "wilst", "/", "so", "lief\u00b7fen", "sie", "dir", "nach", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "KOUS", "PPER", "VMFIN", "$(", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So gar flie\u00dft ieder ver\u00df dir sonder ungemach.", "tokens": ["So", "gar", "flie\u00dft", "ie\u00b7der", "ver\u00df", "dir", "son\u00b7der", "un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du darffst nicht allererst viel in gedancken tr\u00e4umen/", "tokens": ["Du", "darffst", "nicht", "al\u00b7le\u00b7rerst", "viel", "in", "ge\u00b7dan\u00b7cken", "tr\u00e4u\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denn was dein mund nur spricht/ das sind schon lauter reimen:", "tokens": ["Denn", "was", "dein", "mund", "nur", "spricht", "/", "das", "sind", "schon", "lau\u00b7ter", "rei\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "ADV", "VVFIN", "$(", "PDS", "VAFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich aber/ den der wahn und eine blinde macht", "tokens": ["Ich", "a\u00b7ber", "/", "den", "der", "wahn", "und", "ei\u00b7ne", "blin\u00b7de", "macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$(", "ART", "ART", "NN", "KON", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zur straffe/ wie es scheint/ aus reimen hat gebracht/", "tokens": ["Zur", "straf\u00b7fe", "/", "wie", "es", "scheint", "/", "aus", "rei\u00b7men", "hat", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "$(", "PWAV", "PPER", "VVFIN", "$(", "APPR", "VVINF", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bekenne/ da\u00df ich mich oft nur umsonst erhitze.", "tokens": ["Be\u00b7ken\u00b7ne", "/", "da\u00df", "ich", "mich", "oft", "nur", "um\u00b7sonst", "er\u00b7hit\u00b7ze", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "PRF", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich suche mehr/ als du; ich sinne/ denck und schwitze/", "tokens": ["Ich", "su\u00b7che", "mehr", "/", "als", "du", ";", "ich", "sin\u00b7ne", "/", "denck", "und", "schwit\u00b7ze", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KOUS", "PPER", "$.", "PPER", "VVFIN", "$(", "VVIMP", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und spare weder fr\u00fch noch abends meinen flei\u00df;", "tokens": ["Und", "spa\u00b7re", "we\u00b7der", "fr\u00fch", "noch", "a\u00b7bends", "mei\u00b7nen", "flei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "ADJD", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Doch sagt das hertze schwartz/ so spricht die dinte wei\u00df.", "tokens": ["Doch", "sagt", "das", "hert\u00b7ze", "schwartz", "/", "so", "spricht", "die", "din\u00b7te", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$(", "ADV", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Red\u2019 in gedancken ich von einer hof-figure/", "tokens": ["Red'", "in", "ge\u00b7dan\u00b7cken", "ich", "von", "ei\u00b7ner", "hof\u00b7fi\u00b7gu\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So reimt mein feder-kiel darauff den abt von Pure.", "tokens": ["So", "reimt", "mein", "fe\u00b7der\u00b7kiel", "dar\u00b7auff", "den", "abt", "von", "Pu\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PAV", "ART", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Fehlt mir ein grosser mann und dichter in dem schlu\u00df/", "tokens": ["Fehlt", "mir", "ein", "gros\u00b7ser", "mann", "und", "dich\u00b7ter", "in", "dem", "schlu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "KON", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So spricht der reim Kainant/ an statt Virgilius.", "tokens": ["So", "spricht", "der", "reim", "Kai\u00b7nant", "/", "an", "statt", "Vir\u00b7gi\u00b7lius", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "APPR", "APPR", "NE", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Mit einem wort\u2019: ich mag mich hin und her bewegen/", "tokens": ["Mit", "ei\u00b7nem", "wort'", ":", "ich", "mag", "mich", "hin", "und", "her", "be\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "PPER", "VMFIN", "PPER", "PTKVZ", "KON", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So laufft mir dennoch stets das widerspiel entgegen.", "tokens": ["So", "laufft", "mir", "den\u00b7noch", "stets", "das", "wi\u00b7der\u00b7spiel", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Weil ich mit aller m\u00fch nun nichts ersinnen kan/", "tokens": ["Weil", "ich", "mit", "al\u00b7ler", "m\u00fch", "nun", "nichts", "er\u00b7sin\u00b7nen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "ADJD", "ADV", "PIS", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So denck\u2019 ich weiter nicht f\u00fcr schmertz und eifer dran/", "tokens": ["So", "denck", "ich", "wei\u00b7ter", "nicht", "f\u00fcr", "schmertz", "und", "ei\u00b7fer", "dran", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "PTKNEG", "APPR", "ADJD", "KON", "ADJD", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Verfluche mit verdru\u00df die geister/ die mich treiben/", "tokens": ["Ver\u00b7flu\u00b7che", "mit", "ver\u00b7dru\u00df", "die", "geis\u00b7ter", "/", "die", "mich", "trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "$(", "PRELS", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und schwere tausendmahl/ nicht mehr ein wort zu schreiben.", "tokens": ["Und", "schwe\u00b7re", "tau\u00b7send\u00b7mahl", "/", "nicht", "mehr", "ein", "wort", "zu", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$(", "PTKNEG", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Doch wenn ich lange zeit den Musen-gott verflucht/", "tokens": ["Doch", "wenn", "ich", "lan\u00b7ge", "zeit", "den", "Mu\u00b7sen\u00b7gott", "ver\u00b7flucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So find ich offt den reim/ wo ich ihn nicht gesucht.", "tokens": ["So", "find", "ich", "offt", "den", "reim", "/", "wo", "ich", "ihn", "nicht", "ge\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$(", "PWAV", "PPER", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Alsbald durchdringt die glut vom neuen meine glieder:", "tokens": ["Als\u00b7bald", "durch\u00b7dringt", "die", "glut", "vom", "neu\u00b7en", "mei\u00b7ne", "glie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ich nehme/ doch mit zwang/ papier und dinte wieder/", "tokens": ["Ich", "neh\u00b7me", "/", "doch", "mit", "zwang", "/", "pa\u00b7pier", "und", "din\u00b7te", "wie\u00b7der", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADV", "APPR", "VVFIN", "$(", "NE", "KON", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Vergesse meinen eyd/ und warte sonder ziel/", "tokens": ["Ver\u00b7ges\u00b7se", "mei\u00b7nen", "eyd", "/", "und", "war\u00b7te", "son\u00b7der", "ziel", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Bi\u00df wieder nach und nach ein ver\u00dfchen kommen will.", "tokens": ["Bi\u00df", "wie\u00b7der", "nach", "und", "nach", "ein", "ver\u00df\u00b7chen", "kom\u00b7men", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "KON", "APPR", "ART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ach w\u00e4re doch mein geist nur nicht so undescheiden/", "tokens": ["Ach", "w\u00e4\u00b7re", "doch", "mein", "geist", "nur", "nicht", "so", "un\u00b7de\u00b7schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "PTKNEG", "ADV", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und k\u00f6nte wenigstens ein hartes beywort leiden;", "tokens": ["Und", "k\u00f6n\u00b7te", "we\u00b7nigs\u00b7tens", "ein", "har\u00b7tes", "bey\u00b7wort", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So w\u00e4r ich auch vielleicht wie andre w\u00f6rter-reich;", "tokens": ["So", "w\u00e4r", "ich", "auch", "viel\u00b7leicht", "wie", "and\u00b7re", "w\u00f6r\u00b7ter\u00b7reich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "KOKOM", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Denn alles g\u00fclte mir alsdenn im reimen gleich.", "tokens": ["Denn", "al\u00b7les", "g\u00fcl\u00b7te", "mir", "als\u00b7denn", "im", "rei\u00b7men", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADV", "APPRART", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Rennt ich die Rosilis/ der erden lust und wonne/", "tokens": ["Rennt", "ich", "die", "Ro\u00b7si\u00b7lis", "/", "der", "er\u00b7den", "lust", "und", "won\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NE", "$(", "ART", "NN", "VVFIN", "KON", "PWAV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So setzt\u2019 ich gleich darauff: sch\u00f6n/ wie die liebe sonne.", "tokens": ["So", "setzt'", "ich", "gleich", "dar\u00b7auff", ":", "sch\u00f6n", "/", "wie", "die", "lie\u00b7be", "son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PAV", "$.", "ADJD", "$(", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Erh\u00fcb\u2019 ich aber gar in versen einen held/", "tokens": ["Er\u00b7h\u00fcb'", "ich", "a\u00b7ber", "gar", "in", "ver\u00b7sen", "ei\u00b7nen", "held", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So spr\u00e4ch ich augenblicks/ das wunder dieser welt.", "tokens": ["So", "spr\u00e4ch", "ich", "au\u00b7gen\u00b7blicks", "/", "das", "wun\u00b7der", "die\u00b7ser", "welt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und also d\u00fcrfft ich nur von lauter wunder-dingen/", "tokens": ["Und", "al\u00b7so", "d\u00fcrfft", "ich", "nur", "von", "lau\u00b7ter", "wun\u00b7der\u00b7din\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Von himmel/ stern und licht und seltner sch\u00f6nheit singen:", "tokens": ["Von", "him\u00b7mel", "/", "stern", "und", "licht", "und", "selt\u00b7ner", "sch\u00f6n\u00b7heit", "sin\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "VVINF", "KON", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und wenn ich nur fein offt so stoltze w\u00f6rter-pracht", "tokens": ["Und", "wenn", "ich", "nur", "fein", "offt", "so", "stolt\u00b7ze", "w\u00f6r\u00b7ter\u00b7pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJD", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "H\u00e4tt\u2019 ohne m\u00fch und kunst im dichten angebracht;", "tokens": ["H\u00e4tt'", "oh\u00b7ne", "m\u00fch", "und", "kunst", "im", "dich\u00b7ten", "an\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "KON", "ADV", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ja/ wann ich noch dazu der syllben thon verletzte/", "tokens": ["Ja", "/", "wann", "ich", "noch", "da\u00b7zu", "der", "syll\u00b7ben", "thon", "ver\u00b7letz\u00b7te", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PWAV", "PPER", "ADV", "PAV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und bald ein w\u00f6rtgen hier/ das andre dahin setzte/", "tokens": ["Und", "bald", "ein", "w\u00f6rt\u00b7gen", "hier", "/", "das", "and\u00b7re", "da\u00b7hin", "setz\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADV", "$(", "ART", "PIS", "PAV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "So k\u00f6nten/ tr\u00e4ff es gleich auch nur in st\u00fccken ein/", "tokens": ["So", "k\u00f6n\u00b7ten", "/", "tr\u00e4ff", "es", "gleich", "auch", "nur", "in", "st\u00fc\u00b7cken", "ein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$(", "VVFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Doch meine verse leicht Malherbens \u00e4hnlich seyn.", "tokens": ["Doch", "mei\u00b7ne", "ver\u00b7se", "leicht", "Mal\u00b7her\u00b7bens", "\u00e4hn\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "So aber will mein geist sich leider! nicht bequemen/", "tokens": ["So", "a\u00b7ber", "will", "mein", "geist", "sich", "lei\u00b7der", "!", "nicht", "be\u00b7que\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPOSAT", "NN", "PRF", "ADV", "$.", "PTKNEG", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Er mag zum schlusse nichts/ als was sich schicket/ nehmen/", "tokens": ["Er", "mag", "zum", "schlus\u00b7se", "nichts", "/", "als", "was", "sich", "schi\u00b7cket", "/", "neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "VVFIN", "PIS", "$(", "KOUS", "PRELS", "PRF", "VVFIN", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Und kan unm\u00f6glich sehn/ da\u00df meine redens-art", "tokens": ["Und", "kan", "un\u00b7m\u00f6g\u00b7lich", "sehn", "/", "da\u00df", "mei\u00b7ne", "re\u00b7den\u00b7sart"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADJD", "VVINF", "$(", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Sich mit der zeile blo\u00df des reimes wegen paart.", "tokens": ["Sich", "mit", "der", "zei\u00b7le", "blo\u00df", "des", "rei\u00b7mes", "we\u00b7gen", "paart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADV", "ART", "ADJA", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Wenn er vier worte sagt/ l\u00e4st er nur eines bleiben;", "tokens": ["Wenn", "er", "vier", "wor\u00b7te", "sagt", "/", "l\u00e4st", "er", "nur", "ei\u00b7nes", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "CARD", "NN", "VVFIN", "$(", "VVFIN", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.54": {"text": "So da\u00df ich offt mein werck mu\u00df zwantzig mahl umschreiben.", "tokens": ["So", "da\u00df", "ich", "offt", "mein", "werck", "mu\u00df", "zwant\u00b7zig", "mahl", "um\u00b7schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VMFIN", "CARD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Verflucht sey doch der mann/ der blo\u00df aus unbedacht", "tokens": ["Ver\u00b7flucht", "sey", "doch", "der", "mann", "/", "der", "blo\u00df", "aus", "un\u00b7be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ADV", "ART", "NN", "$(", "ART", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Die ersten regeln hat im reimen auffgebracht:", "tokens": ["Die", "ers\u00b7ten", "re\u00b7geln", "hat", "im", "rei\u00b7men", "auff\u00b7ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Der seiner reden krafft in zahlen eingeschr\u00e4ncket/", "tokens": ["Der", "sei\u00b7ner", "re\u00b7den", "krafft", "in", "zah\u00b7len", "ein\u00b7ge\u00b7schr\u00e4n\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Und sie nebst der vernunfft in solche noth versencket!", "tokens": ["Und", "sie", "nebst", "der", "ver\u00b7nunfft", "in", "sol\u00b7che", "noth", "ver\u00b7sen\u00b7cket", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.59": {"text": "W\u00e4r dieses handwerck nicht/ was h\u00e4tt\u2019 ich f\u00fcr gewinn?", "tokens": ["W\u00e4r", "die\u00b7ses", "hand\u00b7werck", "nicht", "/", "was", "h\u00e4tt'", "ich", "f\u00fcr", "ge\u00b7winn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "PTKNEG", "$(", "PWS", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Die tage lieffen mir voll s\u00fcsser stunden hin:", "tokens": ["Die", "ta\u00b7ge", "lief\u00b7fen", "mir", "voll", "s\u00fcs\u00b7ser", "stun\u00b7den", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Ich d\u00fcrffte nichts mehr thun/ als singen/ trincken/ lachen/", "tokens": ["Ich", "d\u00fcrff\u00b7te", "nichts", "mehr", "thun", "/", "als", "sin\u00b7gen", "/", "trin\u00b7cken", "/", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADV", "VVINF", "$(", "KOKOM", "VVFIN", "$(", "VVINF", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Und wie ein thumherr mich nach willen lustig machen.", "tokens": ["Und", "wie", "ein", "thum\u00b7herr", "mich", "nach", "wil\u00b7len", "lus\u00b7tig", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PRF", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Ich k\u00f6nte ruhig seyn/ bey zeiten schlaffen gehn/", "tokens": ["Ich", "k\u00f6n\u00b7te", "ru\u00b7hig", "seyn", "/", "bey", "zei\u00b7ten", "schlaf\u00b7fen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF", "$(", "APPR", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Bey tage m\u00fc\u00dfig seyn und ohne sorgen stehn.", "tokens": ["Bey", "ta\u00b7ge", "m\u00fc\u00b7\u00dfig", "seyn", "und", "oh\u00b7ne", "sor\u00b7gen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VAINF", "KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Und weil mein hertz ohndem zum grame sich nicht schicket/", "tokens": ["Und", "weil", "mein", "hertz", "ohn\u00b7dem", "zum", "gra\u00b7me", "sich", "nicht", "schi\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PAV", "APPRART", "ADJA", "PRF", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Ein feind der mi\u00dfgunst ist/ die ehrsucht niederdr\u00fccket/", "tokens": ["Ein", "feind", "der", "mi\u00df\u00b7gunst", "ist", "/", "die", "ehr\u00b7sucht", "nie\u00b7der\u00b7dr\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Die stoltze gegenwart der grossen herren scheut/", "tokens": ["Die", "stolt\u00b7ze", "ge\u00b7gen\u00b7wart", "der", "gros\u00b7sen", "her\u00b7ren", "scheut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Und der Fortuna nicht im Louvre weyrauch streut.", "tokens": ["Und", "der", "For\u00b7tu\u00b7na", "nicht", "im", "Louv\u00b7re", "wey\u00b7rauch", "streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Wie gl\u00fccklich w\u00e4r ich doch/ wenn/ meine ruh zu st\u00f6ren/", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "w\u00e4r", "ich", "doch", "/", "wenn", "/", "mei\u00b7ne", "ruh", "zu", "st\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "$(", "KOUS", "$(", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Nur das verh\u00e4ngni\u00df mich nicht h\u00e4tte reimen lehren.", "tokens": ["Nur", "das", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "mich", "nicht", "h\u00e4t\u00b7te", "rei\u00b7men", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "PTKNEG", "VAFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Allein seit dem der wahn/ den diese pest gebiehrt/", "tokens": ["Al\u00b7lein", "seit", "dem", "der", "wahn", "/", "den", "die\u00b7se", "pest", "ge\u00b7biehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ART", "NN", "$(", "ART", "PDS", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Durch seinen nabel mir die sinnen hat ger\u00fchrt/", "tokens": ["Durch", "sei\u00b7nen", "na\u00b7bel", "mir", "die", "sin\u00b7nen", "hat", "ge\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Und ein verdammter geist/ blo\u00df seinen spott zu treiben/", "tokens": ["Und", "ein", "ver\u00b7damm\u00b7ter", "geist", "/", "blo\u00df", "sei\u00b7nen", "spott", "zu", "trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$(", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Mich auf den schlu\u00df gebracht/ recht wohl und rein zu schreiben;", "tokens": ["Mich", "auf", "den", "schlu\u00df", "ge\u00b7bracht", "/", "recht", "wohl", "und", "rein", "zu", "schrei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$(", "ADV", "ADV", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "So sitz ich tag f\u00fcr tag bey einem wercke still/", "tokens": ["So", "sitz", "ich", "tag", "f\u00fcr", "tag", "bey", "ei\u00b7nem", "wer\u00b7cke", "still", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "NN", "APPR", "NN", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Ver\u00e4ndre di\u00df und das/ was sich nicht reimen will:", "tokens": ["Ver\u00b7\u00e4nd\u00b7re", "di\u00df", "und", "das", "/", "was", "sich", "nicht", "rei\u00b7men", "will", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "KON", "ART", "$(", "PWS", "PRF", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Flick\u2019 an und streiche weg/ und heb offt an zu fluchen/", "tokens": ["Flick'", "an", "und", "strei\u00b7che", "weg", "/", "und", "heb", "offt", "an", "zu", "flu\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "VVFIN", "PTKVZ", "$(", "KON", "VVFIN", "ADV", "PTKVZ", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Da\u00df mich die Musen nicht wie Pelletieren suchen.", "tokens": ["Da\u00df", "mich", "die", "Mu\u00b7sen", "nicht", "wie", "Pel\u00b7le\u00b7tie\u00b7ren", "su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Begl\u00fcckter Scuderi! du schwitzest nicht wie wir/", "tokens": ["Be\u00b7gl\u00fcck\u00b7ter", "Scu\u00b7de\u00b7ri", "!", "du", "schwit\u00b7zest", "nicht", "wie", "wir", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "PPER", "VVFIN", "PTKNEG", "KOKOM", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Und bringest monatlich ein neues werck herf\u00fcr.", "tokens": ["Und", "brin\u00b7gest", "mo\u00b7nat\u00b7lich", "ein", "neu\u00b7es", "werck", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.81": {"text": "Zwar deine schrifften sind nichts als gemeine lieder/", "tokens": ["Zwar", "dei\u00b7ne", "schriff\u00b7ten", "sind", "nichts", "als", "ge\u00b7mei\u00b7ne", "lie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "VAFIN", "PIS", "KOKOM", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.82": {"text": "Ohn arbeit/ ohne kunst/ und der vernunfft zuwider:", "tokens": ["Ohn", "ar\u00b7beit", "/", "oh\u00b7ne", "kunst", "/", "und", "der", "ver\u00b7nunfft", "zu\u00b7wi\u00b7der", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "APPR", "NN", "$(", "KON", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.83": {"text": "Allein sie treffen doch/ was man auch sagen kan/", "tokens": ["Al\u00b7lein", "sie", "tref\u00b7fen", "doch", "/", "was", "man", "auch", "sa\u00b7gen", "kan", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "$(", "PWS", "PIS", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.84": {"text": "Viel narren zum verkauff und auch zum lesen an.", "tokens": ["Viel", "nar\u00b7ren", "zum", "ver\u00b7kauff", "und", "auch", "zum", "le\u00b7sen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "KON", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.85": {"text": "Und endlich wenn der reim am ende richtig klinget/", "tokens": ["Und", "end\u00b7lich", "wenn", "der", "reim", "am", "en\u00b7de", "rich\u00b7tig", "klin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "ART", "NN", "APPRART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.86": {"text": "Was ist es denn nun mehr/ ob der verstand sich zwinget?", "tokens": ["Was", "ist", "es", "denn", "nun", "mehr", "/", "ob", "der", "ver\u00b7stand", "sich", "zwin\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "ADV", "$(", "KOUS", "ART", "VVFIN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.87": {"text": "Der ist in warheit wohl rechtschaffen arm und blind/", "tokens": ["Der", "ist", "in", "war\u00b7heit", "wohl", "recht\u00b7schaf\u00b7fen", "arm", "und", "blind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "ADV", "ADJD", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.88": {"text": "Der seinen freyen geist an kunst und regeln bind\u2019t.", "tokens": ["Der", "sei\u00b7nen", "frey\u00b7en", "geist", "an", "kunst", "und", "re\u00b7geln", "bind'", "t."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "NN", "KON", "XY", "XY", "XY"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.89": {"text": "Ein narr hat tausendmahl mehr lust in seinem dichten/", "tokens": ["Ein", "narr", "hat", "tau\u00b7send\u00b7mahl", "mehr", "lust", "in", "sei\u00b7nem", "dich\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.90": {"text": "Er darff sich/ wenn er reimt/ nach keinen w\u00f6rtern richten/", "tokens": ["Er", "darff", "sich", "/", "wenn", "er", "reimt", "/", "nach", "kei\u00b7nen", "w\u00f6r\u00b7tern", "rich\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "$(", "KOUS", "PPER", "VVFIN", "$(", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.91": {"text": "Liebt alles/ was er macht/ und bildet selbst ihm ein/", "tokens": ["Liebt", "al\u00b7les", "/", "was", "er", "macht", "/", "und", "bil\u00b7det", "selbst", "ihm", "ein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$(", "PWS", "PPER", "VVFIN", "$(", "KON", "VVFIN", "ADV", "PPER", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.92": {"text": "Da\u00df er und seine schrifft die gr\u00f6sten wunder seyn.", "tokens": ["Da\u00df", "er", "und", "sei\u00b7ne", "schrifft", "die", "gr\u00f6s\u00b7ten", "wun\u00b7der", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PPOSAT", "VVFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.93": {"text": "Allein ein hoher geist sucht nur umsonst auff erden/", "tokens": ["Al\u00b7lein", "ein", "ho\u00b7her", "geist", "sucht", "nur", "um\u00b7sonst", "auff", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.94": {"text": "In dieser schweren kunst vollkommen klug zu werden.", "tokens": ["In", "die\u00b7ser", "schwe\u00b7ren", "kunst", "voll\u00b7kom\u00b7men", "klug", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADJD", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.95": {"text": "Er ist stets mi\u00dfvergn\u00fcgt ob dem/ was er verricht/", "tokens": ["Er", "ist", "stets", "mi\u00df\u00b7ver\u00b7gn\u00fcgt", "ob", "dem", "/", "was", "er", "ver\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "KOUS", "ART", "$(", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.96": {"text": "Gef\u00e4llet aller welt/ nur blo\u00df ihm selber nicht;", "tokens": ["Ge\u00b7f\u00e4l\u00b7let", "al\u00b7ler", "welt", "/", "nur", "blo\u00df", "ihm", "sel\u00b7ber", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$(", "ADV", "ADV", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.97": {"text": "Und da ein ieder mensch ihn preisen mu\u00df und lieben/", "tokens": ["Und", "da", "ein", "ie\u00b7der", "mensch", "ihn", "prei\u00b7sen", "mu\u00df", "und", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "PIAT", "NN", "PPER", "VVFIN", "VMFIN", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.98": {"text": "W\u00fcnscht er zu seiner ruh: er h\u00e4tte nichts geschrieben.", "tokens": ["W\u00fcnscht", "er", "zu", "sei\u00b7ner", "ruh", ":", "er", "h\u00e4t\u00b7te", "nichts", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.99": {"text": "Drum bitt ich nochmahls dich/ du f\u00fcrst der dichterey/", "tokens": ["Drum", "bitt", "ich", "noch\u00b7mahls", "dich", "/", "du", "f\u00fcrst", "der", "dich\u00b7te\u00b7rey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PPER", "$(", "PPER", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.100": {"text": "Moliere/ bringe mir die kunst zu reimen bey:", "tokens": ["Mo\u00b7lie\u00b7re", "/", "brin\u00b7ge", "mir", "die", "kunst", "zu", "rei\u00b7men", "bey", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.101": {"text": "Ist aber dieses dir unm\u00f6glich/ mir zu zeigen/", "tokens": ["Ist", "a\u00b7ber", "die\u00b7ses", "dir", "un\u00b7m\u00f6g\u00b7lich", "/", "mir", "zu", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDAT", "PPER", "ADJD", "$(", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.102": {"text": "So lehre mich die kunst im reimen gar zu schweigen.", "tokens": ["So", "leh\u00b7re", "mich", "die", "kunst", "im", "rei\u00b7men", "gar", "zu", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPRART", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}