{"textgrid.poem.43895": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr Bogen voller g\u00fcldnen Pfeile,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Bogen voller g\u00fcldnen Pfeile,", "tokens": ["Ihr", "Bo\u00b7gen", "vol\u00b7ler", "g\u00fcld\u00b7nen", "Pfei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr schwarzen Augen voller Glut,", "tokens": ["Ihr", "schwar\u00b7zen", "Au\u00b7gen", "vol\u00b7ler", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erlaubt mir, da\u00df ich mich verweile,", "tokens": ["Er\u00b7laubt", "mir", ",", "da\u00df", "ich", "mich", "ver\u00b7wei\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fchrt den Ku\u00df in Nerv und Blut,", "tokens": ["Und", "f\u00fchrt", "den", "Ku\u00df", "in", "Nerv", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Damit er Lenchens Herze lehre,", "tokens": ["Da\u00b7mit", "er", "Len\u00b7chens", "Her\u00b7ze", "leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie nah ich ihm schon angeh\u00f6re.", "tokens": ["Wie", "nah", "ich", "ihm", "schon", "an\u00b7ge\u00b7h\u00f6\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich schmeck auf euch, ihr warmen Lider,", "tokens": ["Ich", "schmeck", "auf", "euch", ",", "ihr", "war\u00b7men", "Li\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Frucht, so dort in Eden stund;", "tokens": ["Die", "Frucht", ",", "so", "dort", "in", "E\u00b7den", "stund", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr w\u00e4lzt euch br\u00fcnstig hin und wieder", "tokens": ["Ihr", "w\u00e4lzt", "euch", "br\u00fcns\u00b7tig", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und streift den aufgelegten Mund", "tokens": ["Und", "streift", "den", "auf\u00b7ge\u00b7leg\u00b7ten", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wist mit euren weichen Sachen", "tokens": ["Und", "wist", "mit", "eu\u00b7ren", "wei\u00b7chen", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Lippen Spielwerck nachzumachen.", "tokens": ["Der", "Lip\u00b7pen", "Spiel\u00b7werck", "nach\u00b7zu\u00b7ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Venus hat viel treue Seelen,", "tokens": ["Die", "Ve\u00b7nus", "hat", "viel", "treu\u00b7e", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Zehnte kennt die Wollust nicht;", "tokens": ["Der", "Zehn\u00b7te", "kennt", "die", "Wol\u00b7lust", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Kind, wir wollen sie verheelen,", "tokens": ["Mein", "Kind", ",", "wir", "wol\u00b7len", "sie", "ver\u00b7hee\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn ein andrer Rosen bricht,", "tokens": ["Und", "wenn", "ein", "an\u00b7drer", "Ro\u00b7sen", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So k\u00fc\u00df ich deine Sonnenlichter", "tokens": ["So", "k\u00fc\u00df", "ich", "dei\u00b7ne", "Son\u00b7nen\u00b7lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mercke keinen Splitterrichter.", "tokens": ["Und", "mer\u00b7cke", "kei\u00b7nen", "Split\u00b7ter\u00b7rich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So zwinckert unter meiner Zunge,", "tokens": ["So", "zwin\u00b7ckert", "un\u00b7ter", "mei\u00b7ner", "Zun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So, sch\u00f6nen Augen, k\u00fczelt sie;", "tokens": ["So", ",", "sch\u00f6\u00b7nen", "Au\u00b7gen", ",", "k\u00fc\u00b7zelt", "sie", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So geht die Regung halb zu Sprunge,", "tokens": ["So", "geht", "die", "Re\u00b7gung", "halb", "zu", "Sprun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So kostet's mich nur halbe M\u00fch,", "tokens": ["So", "kos\u00b7tet's", "mich", "nur", "hal\u00b7be", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu sehn, zu f\u00fchlen und zu glauben:", "tokens": ["Zu", "sehn", ",", "zu", "f\u00fch\u00b7len", "und", "zu", "glau\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr k\u00f6nt die Freyheit zwiefach rauben.", "tokens": ["Ihr", "k\u00f6nt", "die", "Frey\u00b7heit", "zwie\u00b7fach", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch f\u00fcrchtet euch vor keinen Bi\u00dfen", "tokens": ["Doch", "f\u00fcrch\u00b7tet", "euch", "vor", "kei\u00b7nen", "Bi\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und glaubt nur, da\u00df ihr sichrer seyd,", "tokens": ["Und", "glaubt", "nur", ",", "da\u00df", "ihr", "sich\u00b7rer", "seyd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PRF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als wenn mein geil- und starckes K\u00fc\u00dfen", "tokens": ["Als", "wenn", "mein", "geil", "und", "star\u00b7ckes", "K\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPOSAT", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Mund mit Narden \u00fcberstreut;", "tokens": ["Den", "Mund", "mit", "Nar\u00b7den", "\u00fc\u00b7bers\u00b7treut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich will euch dr\u00fccken und nicht schonen,", "tokens": ["Ich", "will", "euch", "dr\u00fc\u00b7cken", "und", "nicht", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "KON", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr m\u00fcst mir nur die Lust verlohnen.", "tokens": ["Ihr", "m\u00fcst", "mir", "nur", "die", "Lust", "ver\u00b7loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihr m\u00fcst euch nehmlich abw\u00e4rts lencken,", "tokens": ["Ihr", "m\u00fcst", "euch", "nehm\u00b7lich", "ab\u00b7w\u00e4rts", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Nebenbuhler pr\u00e4chtig gehn;", "tokens": ["Wenn", "Ne\u00b7ben\u00b7buh\u00b7ler", "pr\u00e4ch\u00b7tig", "gehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will Lenchen einen Blick verschencken,", "tokens": ["Will", "Len\u00b7chen", "ei\u00b7nen", "Blick", "ver\u00b7schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So sollt ihr mir zu Diensten stehn.", "tokens": ["So", "sollt", "ihr", "mir", "zu", "Diens\u00b7ten", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verschliest euch Fremden, die ihr dienen,", "tokens": ["Ver\u00b7schliest", "euch", "Frem\u00b7den", ",", "die", "ihr", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und \u00f6fnet euch vor meinen Mienen.", "tokens": ["Und", "\u00f6f\u00b7net", "euch", "vor", "mei\u00b7nen", "Mie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Bekommt sie ein Versuchungsschreiben,", "tokens": ["Be\u00b7kommt", "sie", "ein", "Ver\u00b7su\u00b7chungs\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dem viel s\u00fc\u00dfe Worte sind,", "tokens": ["In", "dem", "viel", "s\u00fc\u00b7\u00dfe", "Wor\u00b7te", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So last den hellen Vorwiz bleiben", "tokens": ["So", "last", "den", "hel\u00b7len", "Vor\u00b7wiz", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und stellt euch wie mein Amor blind;", "tokens": ["Und", "stellt", "euch", "wie", "mein", "A\u00b7mor", "blind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOKOM", "PPOSAT", "NE", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hingegen, will sie meines lesen,", "tokens": ["Hin\u00b7ge\u00b7gen", ",", "will", "sie", "mei\u00b7nes", "le\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VMFIN", "PPER", "PPOSAT", "VVINF", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "So thut, als w\u00e4ret ihr genesen.", "tokens": ["So", "thut", ",", "als", "w\u00e4\u00b7ret", "ihr", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-----", "measure": "unknown.measure.di"}}, "stanza.8": {"line.1": {"text": "Und darum mach ich euch die Freude", "tokens": ["Und", "da\u00b7rum", "mach", "ich", "euch", "die", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und darum k\u00fc\u00df ich euch so scharf,", "tokens": ["Und", "da\u00b7rum", "k\u00fc\u00df", "ich", "euch", "so", "scharf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jezt dies, jezt das, jezt alle beide,", "tokens": ["Jezt", "dies", ",", "jezt", "das", ",", "jezt", "al\u00b7le", "bei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "$,", "ADV", "PDS", "$,", "ADV", "PIAT", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Damit nicht eines z\u00fcrnen darf", "tokens": ["Da\u00b7mit", "nicht", "ei\u00b7nes", "z\u00fcr\u00b7nen", "darf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PTKNEG", "PIS", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und, wenn ich mit dem rechten spiele,", "tokens": ["Und", ",", "wenn", "ich", "mit", "dem", "rech\u00b7ten", "spie\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das linck' aus Rach aufs andre schiele.", "tokens": ["Das", "linck", "aus", "Rach", "aufs", "and\u00b7re", "schie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ihr Bogen voller g\u00fcldnen Pfeile,", "tokens": ["Ihr", "Bo\u00b7gen", "vol\u00b7ler", "g\u00fcld\u00b7nen", "Pfei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr schwarzen Augen voller Glut,", "tokens": ["Ihr", "schwar\u00b7zen", "Au\u00b7gen", "vol\u00b7ler", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erlaubt mir, da\u00df ich mich verweile,", "tokens": ["Er\u00b7laubt", "mir", ",", "da\u00df", "ich", "mich", "ver\u00b7wei\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fchrt den Ku\u00df in Nerv und Blut,", "tokens": ["Und", "f\u00fchrt", "den", "Ku\u00df", "in", "Nerv", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Damit er Lenchens Herze lehre,", "tokens": ["Da\u00b7mit", "er", "Len\u00b7chens", "Her\u00b7ze", "leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie nah ich ihm schon angeh\u00f6re.", "tokens": ["Wie", "nah", "ich", "ihm", "schon", "an\u00b7ge\u00b7h\u00f6\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ich schmeck auf euch, ihr warmen Lider,", "tokens": ["Ich", "schmeck", "auf", "euch", ",", "ihr", "war\u00b7men", "Li\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Frucht, so dort in Eden stund;", "tokens": ["Die", "Frucht", ",", "so", "dort", "in", "E\u00b7den", "stund", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr w\u00e4lzt euch br\u00fcnstig hin und wieder", "tokens": ["Ihr", "w\u00e4lzt", "euch", "br\u00fcns\u00b7tig", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und streift den aufgelegten Mund", "tokens": ["Und", "streift", "den", "auf\u00b7ge\u00b7leg\u00b7ten", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wist mit euren weichen Sachen", "tokens": ["Und", "wist", "mit", "eu\u00b7ren", "wei\u00b7chen", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Lippen Spielwerck nachzumachen.", "tokens": ["Der", "Lip\u00b7pen", "Spiel\u00b7werck", "nach\u00b7zu\u00b7ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Die Venus hat viel treue Seelen,", "tokens": ["Die", "Ve\u00b7nus", "hat", "viel", "treu\u00b7e", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Zehnte kennt die Wollust nicht;", "tokens": ["Der", "Zehn\u00b7te", "kennt", "die", "Wol\u00b7lust", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Kind, wir wollen sie verheelen,", "tokens": ["Mein", "Kind", ",", "wir", "wol\u00b7len", "sie", "ver\u00b7hee\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn ein andrer Rosen bricht,", "tokens": ["Und", "wenn", "ein", "an\u00b7drer", "Ro\u00b7sen", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So k\u00fc\u00df ich deine Sonnenlichter", "tokens": ["So", "k\u00fc\u00df", "ich", "dei\u00b7ne", "Son\u00b7nen\u00b7lich\u