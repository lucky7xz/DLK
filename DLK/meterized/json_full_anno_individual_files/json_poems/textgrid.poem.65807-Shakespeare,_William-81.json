{"textgrid.poem.65807": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "81", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Entweder leb' ich, dir die Grabschrift zu ersinnen,", "tokens": ["Ent\u00b7we\u00b7der", "leb'", "ich", ",", "dir", "die", "Grab\u00b7schrift", "zu", "er\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Oder du dauerst noch, wenn Moder mich verzehrt.", "tokens": ["O\u00b7der", "du", "dau\u00b7erst", "noch", ",", "wenn", "Mo\u00b7der", "mich", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "$,", "KOUS", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Angedenken rafft kein Tod von hinnen,", "tokens": ["Dein", "An\u00b7ge\u00b7den\u00b7ken", "rafft", "kein", "Tod", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn auch von mir kein Lebender mehr h\u00f6rt.", "tokens": ["Wenn", "auch", "von", "mir", "kein", "Le\u00b7ben\u00b7der", "mehr", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPER", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Fortan unsterblich wird dein Name leben,", "tokens": ["For\u00b7tan", "uns\u00b7terb\u00b7lich", "wird", "dein", "Na\u00b7me", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Wenn mich auf ewig Staub der Welt verbarg.", "tokens": ["Wenn", "mich", "auf", "e\u00b7wig", "Staub", "der", "Welt", "ver\u00b7barg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mir kann die Erd' ein schlechtes Grab nur geben;", "tokens": ["Mir", "kann", "die", "Erd'", "ein", "schlech\u00b7tes", "Grab", "nur", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du ruhst in Menschenaugen eingesargt.", "tokens": ["Du", "ruhst", "in", "Men\u00b7schen\u00b7au\u00b7gen", "ein\u00b7ge\u00b7sargt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Mein Freundesvers wird sein dein Monument,", "tokens": ["Mein", "Freun\u00b7des\u00b7vers", "wird", "sein", "dein", "Mo\u00b7nu\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df dich noch ungeborne Augen lesen", "tokens": ["Da\u00df", "dich", "noch", "un\u00b7ge\u00b7bor\u00b7ne", "Au\u00b7gen", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und kommender Geschlechter Mund dich nennt,", "tokens": ["Und", "kom\u00b7men\u00b7der", "Ge\u00b7schlech\u00b7ter", "Mund", "dich", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wenn alle Atmer dieser Welt verwesen.", "tokens": ["Wenn", "al\u00b7le", "At\u00b7mer", "die\u00b7ser", "Welt", "ver\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "So h\u00e4lt dich da, wo Odem nie versiegt,", "tokens": ["So", "h\u00e4lt", "dich", "da", ",", "wo", "O\u00b7dem", "nie", "ver\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Auf Menschenlippen atmend mein Gedicht.", "tokens": ["Auf", "Men\u00b7schen\u00b7lip\u00b7pen", "at\u00b7mend", "mein", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Entweder leb' ich, dir die Grabschrift zu ersinnen,", "tokens": ["Ent\u00b7we\u00b7der", "leb'", "ich", ",", "dir", "die", "Grab\u00b7schrift", "zu", "er\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Oder du