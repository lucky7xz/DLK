{"dta.poem.20496": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der versto\u00dfne liebhaber.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was vor ein strahl will meine seele f\u00e4llen/", "tokens": ["Was", "vor", "ein", "strahl", "will", "mei\u00b7ne", "see\u00b7le", "f\u00e4l\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Will sich der tag in d\u00fcstre nacht verstellen?", "tokens": ["Will", "sich", "der", "tag", "in", "d\u00fcst\u00b7re", "nacht", "ver\u00b7stel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und soll mein sonnenschein", "tokens": ["Und", "soll", "mein", "son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mir pl\u00f6tzlichen verschlossen seyn?", "tokens": ["Mir", "pl\u00f6tz\u00b7li\u00b7chen", "ver\u00b7schlos\u00b7sen", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Will nun der lufft erblastes rasen", "tokens": ["Will", "nun", "der", "lufft", "er\u00b7blas\u00b7tes", "ra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In meine schwache segel blasen/", "tokens": ["In", "mei\u00b7ne", "schwa\u00b7che", "se\u00b7gel", "bla\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hat alles sich auff meinen fall vereint?", "tokens": ["Hat", "al\u00b7les", "sich", "auff", "mei\u00b7nen", "fall", "ver\u00b7eint", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der himmel ist ja selbst zu st\u00fcrtzen mich vermeynt/", "tokens": ["Der", "him\u00b7mel", "ist", "ja", "selbst", "zu", "st\u00fcrt\u00b7zen", "mich", "ver\u00b7meynt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Weil mir mi\u00dfgunst/ verleumdung/ neid/", "tokens": ["Weil", "mir", "mi\u00df\u00b7gunst", "/", "ver\u00b7leum\u00b7dung", "/", "neid", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$(", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Solch jammer und solch elend zubereit.", "tokens": ["Solch", "jam\u00b7mer", "und", "solch", "e\u00b7lend", "zu\u00b7be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kan iemand wohl ein schlimmer fallbret legen/", "tokens": ["Kan", "ie\u00b7mand", "wohl", "ein", "schlim\u00b7mer", "fall\u00b7bret", "le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ART", "ADJD", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die arglist selbst wird nichts verfluchters hegen/", "tokens": ["Die", "arg\u00b7list", "selbst", "wird", "nichts", "ver\u00b7fluch\u00b7ters", "he\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PIS", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als di\u00df/ was mir geschehn/", "tokens": ["Als", "di\u00df", "/", "was", "mir", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$(", "PWS", "PPER", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Will man ein lebend beyspiel sehn/", "tokens": ["Will", "man", "ein", "le\u00b7bend", "bey\u00b7spiel", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "ADJD", "PIS", "VVINF", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Wie das/ was fast am gipffel schwebet/", "tokens": ["Wie", "das", "/", "was", "fast", "am", "gipf\u00b7fel", "schwe\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "$(", "PWS", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch schlechten wind ger\u00fchrt/ erbebet/", "tokens": ["Durch", "schlech\u00b7ten", "wind", "ge\u00b7r\u00fchrt", "/", "er\u00b7be\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df sich ein strenger fall auch sternen zugesellt/", "tokens": ["Da\u00df", "sich", "ein", "stren\u00b7ger", "fall", "auch", "ster\u00b7nen", "zu\u00b7ge\u00b7sellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "ADV", "VVINF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ein erh\u00f6hter berg in tieffe th\u00e4ler f\u00e4llt/", "tokens": ["Und", "ein", "er\u00b7h\u00f6h\u00b7ter", "berg", "in", "tief\u00b7fe", "th\u00e4\u00b7ler", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df list auch l\u00f6wen st\u00fcrtzen kan/", "tokens": ["Da\u00df", "list", "auch", "l\u00f6\u00b7wen", "st\u00fcrt\u00b7zen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVINF", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der sehe mich und meinen kummer an.", "tokens": ["Der", "se\u00b7he", "mich", "und", "mei\u00b7nen", "kum\u00b7mer", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich/ leider! bin empfindlicher ger\u00fchret/", "tokens": ["Ich", "/", "lei\u00b7der", "!", "bin", "emp\u00b7find\u00b7li\u00b7cher", "ge\u00b7r\u00fch\u00b7ret", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "ADV", "$.", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Indem man mir Celindens gunst entf\u00fchret/", "tokens": ["In\u00b7dem", "man", "mir", "Ce\u00b7lin\u00b7dens", "gunst", "ent\u00b7f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die einig mich vergn\u00fcgt/", "tokens": ["Die", "ei\u00b7nig", "mich", "ver\u00b7gn\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und mir mein gantzes hertz besiegt;", "tokens": ["Und", "mir", "mein", "gant\u00b7zes", "hertz", "be\u00b7siegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Celinden/ der mein gantzes leben/", "tokens": ["Ce\u00b7lin\u00b7den", "/", "der", "mein", "gant\u00b7zes", "le\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu steten diensten bleibt ergeben/", "tokens": ["Zu", "ste\u00b7ten", "diens\u00b7ten", "bleibt", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ach da\u00df ein falscher wahn und schlechter argwohn sta", "tokens": ["Ach", "da\u00df", "ein", "fal\u00b7scher", "wahn", "und", "schlech\u00b7ter", "arg\u00b7wohn", "sta"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "KOUS", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vor der bewehrten treu bey ihr gefunden hat/", "tokens": ["Vor", "der", "be\u00b7wehr\u00b7ten", "treu", "bey", "ihr", "ge\u00b7fun\u00b7den", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJD", "APPR", "PPOSAT", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der sie so gegen mich verstellt/", "tokens": ["Der", "sie", "so", "ge\u00b7gen", "mich", "ver\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df sie mich straff- und ungl\u00fccks-w\u00fcrdig h\u00e4lt.", "tokens": ["Da\u00df", "sie", "mich", "straff", "und", "un\u00b7gl\u00fccks\u00b7w\u00fcr\u00b7dig", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "TRUNC", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die wangen/ wo die reiche anmuth spielte/", "tokens": ["Die", "wan\u00b7gen", "/", "wo", "die", "rei\u00b7che", "an\u00b7muth", "spiel\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo sich die glut an weissen rosen k\u00fchlte/", "tokens": ["Wo", "sich", "die", "glut", "an", "weis\u00b7sen", "ro\u00b7sen", "k\u00fchl\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bestrahlt ein strenger plitz/", "tokens": ["Be\u00b7strahlt", "ein", "stren\u00b7ger", "plitz", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der holde mund/ der sch\u00f6nheit sitz", "tokens": ["Der", "hol\u00b7de", "mund", "/", "der", "sch\u00f6n\u00b7heit", "sitz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "L\u00e4st rauhe donner-worte knallen/", "tokens": ["L\u00e4st", "rau\u00b7he", "don\u00b7ner\u00b7wor\u00b7te", "knal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich h\u00f6re schon mein urtheil fallen/", "tokens": ["Ich", "h\u00f6\u00b7re", "schon", "mein", "ur\u00b7theil", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Florettens wanckelmuth hat h\u00f6ll und tod verdient/", "tokens": ["Flo\u00b7ret\u00b7tens", "wan\u00b7ckel\u00b7muth", "hat", "h\u00f6ll", "und", "tod", "ver\u00b7dient", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ADJD", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil er Celinden sich zu lieben hat erk\u00fchnt/", "tokens": ["Weil", "er", "Ce\u00b7lin\u00b7den", "sich", "zu", "lie\u00b7ben", "hat", "er\u00b7k\u00fchnt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PRF", "PTKZU", "VVINF", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihr zornig auge zeiget frey/", "tokens": ["Ihr", "zor\u00b7nig", "au\u00b7ge", "zei\u00b7get", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df di\u00df ihr schlu\u00df und steter wille sey.", "tokens": ["Da\u00df", "di\u00df", "ihr", "schlu\u00df", "und", "ste\u00b7ter", "wil\u00b7le", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PPOSAT", "NN", "KON", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So spielt die welt/ so mu\u00df die unschuld leiden/", "tokens": ["So", "spielt", "die", "welt", "/", "so", "mu\u00df", "die", "un\u00b7schuld", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "ADV", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn l\u00fcgen sich in fremde farben kleiden/", "tokens": ["Wenn", "l\u00fc\u00b7gen", "sich", "in", "frem\u00b7de", "far\u00b7ben", "klei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ha\u00df und neid bem\u00fcht", "tokens": ["Wenn", "ha\u00df", "und", "neid", "be\u00b7m\u00fcht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu f\u00e4llen/ was in zierde bl\u00fcht/", "tokens": ["Zu", "f\u00e4l\u00b7len", "/", "was", "in", "zier\u00b7de", "bl\u00fcht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$(", "P