{"textgrid.poem.33659": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Fr\u00fchling, holder fahrender Sch\u00fcler,", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Fr\u00fchling, holder fahrender Sch\u00fcler,", "tokens": ["O", "Fr\u00fch\u00b7ling", ",", "hol\u00b7der", "fah\u00b7ren\u00b7der", "Sch\u00fc\u00b7ler", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo zogst du hin? Die Linden bl\u00fchn,", "tokens": ["Wo", "zogst", "du", "hin", "?", "Die", "Lin\u00b7den", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NE", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die N\u00e4chte werden stiller, schw\u00fcler,", "tokens": ["Die", "N\u00e4ch\u00b7te", "wer\u00b7den", "stil\u00b7ler", ",", "schw\u00fc\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dichter schwillt das dunkle Gr\u00fcn.", "tokens": ["Und", "dich\u00b7ter", "schwillt", "das", "dunk\u00b7le", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch ach! die sch\u00f6nen Stunden fehlen,", "tokens": ["Doch", "ach", "!", "die", "sch\u00f6\u00b7nen", "Stun\u00b7den", "feh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word"