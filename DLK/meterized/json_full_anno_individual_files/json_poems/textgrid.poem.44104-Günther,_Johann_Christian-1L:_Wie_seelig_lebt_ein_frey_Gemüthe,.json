{"textgrid.poem.44104": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie seelig lebt ein frey Gem\u00fcthe,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie seelig lebt ein frey Gem\u00fcthe,", "tokens": ["Wie", "see\u00b7lig", "lebt", "ein", "frey", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das weder List noch Rachgier hegt", "tokens": ["Das", "we\u00b7der", "List", "noch", "Rach\u00b7gier", "hegt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "KON", "NN", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und das des weisen Sch\u00f6pfers G\u00fcte", "tokens": ["Und", "das", "des", "wei\u00b7sen", "Sch\u00f6p\u00b7fers", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch in der gr\u00f6sten Noth erwegt,", "tokens": ["Auch", "in", "der", "gr\u00f6s\u00b7ten", "Noth", "er\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja, das in allem, was es \u00fcbt,", "tokens": ["Ja", ",", "das", "in", "al\u00b7lem", ",", "was", "es", "\u00fcbt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "APPR", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gerechtigkeit und Warheit liebt.", "tokens": ["Ge\u00b7rech\u00b7tig\u00b7keit", "und", "War\u00b7heit", "liebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sein kluger Sinn vertr\u00e4gt die Thoren", "tokens": ["Sein", "klu\u00b7ger", "Sinn", "ver\u00b7tr\u00e4gt", "die", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sieht sie blos mit Mitleid an;", "tokens": ["Und", "sieht", "sie", "blos", "mit", "Mit\u00b7leid", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er sch\u00e4zt die Stunde vor verloren,", "tokens": ["Er", "sch\u00e4zt", "die", "Stun\u00b7de", "vor", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Worin er keinem Guts gethan;", "tokens": ["Wo\u00b7rin", "er", "kei\u00b7nem", "Guts", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er be\u00dfert eigner Schwachheit Schuld", "tokens": ["Er", "be\u00b7\u00dfert", "eig\u00b7ner", "Schwach\u00b7heit", "Schuld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und hat mit fremder gern Gedult.", "tokens": ["Und", "hat", "mit", "frem\u00b7der", "gern", "Ge\u00b7dult", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Fehler sind sein Tugendzunder", "tokens": ["Die", "Feh\u00b7ler", "sind", "sein", "Tu\u00b7gend\u00b7zun\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lehren ihn bescheiden seyn;", "tokens": ["Und", "leh\u00b7ren", "ihn", "be\u00b7schei\u00b7den", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er sieht mit Lust des H\u00f6chsten Wunder", "tokens": ["Er", "sieht", "mit", "Lust", "des", "H\u00f6chs\u00b7ten", "Wun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nimmt verborgne Wei\u00dfheit ein;", "tokens": ["Und", "nimmt", "ver\u00b7borg\u00b7ne", "Wei\u00df\u00b7heit", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So heftig auch der Creuzdorn sticht,", "tokens": ["So", "hef\u00b7tig", "auch", "der", "Creuz\u00b7dorn", "sticht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Flucht er doch dem Verh\u00e4ngn\u00fc\u00df nicht.", "tokens": ["Flucht", "er", "doch", "dem", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Kein Undanck macht sein Wohlthun m\u00fcde,", "tokens": ["Kein", "Un\u00b7danck", "macht", "sein", "Wohl\u00b7thun", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Strafwort seinen Zorn erhizt;", "tokens": ["Kein", "Straf\u00b7wort", "sei\u00b7nen", "Zorn", "er\u00b7hizt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er sucht mit eignem Schaden Friede,", "tokens": ["Er", "sucht", "mit", "eig\u00b7nem", "Scha\u00b7den", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So oft nur dieser andern n\u00fczt.", "tokens": ["So", "oft", "nur", "die\u00b7ser", "an\u00b7dern", "n\u00fczt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PDAT", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er scheut und reizet keinen Feind", "tokens": ["Er", "scheut", "und", "rei\u00b7zet", "kei\u00b7nen", "Feind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und redet allzeit, wie er's meint.", "tokens": ["Und", "re\u00b7det", "all\u00b7zeit", ",", "wie", "er's", "meint", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er strebt nicht mit Gewalt nach Dingen,", "tokens": ["Er", "strebt", "nicht", "mit", "Ge\u00b7walt", "nach", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die \u00fcber sein Verm\u00f6gen gehn;", "tokens": ["Die", "\u00fc\u00b7ber", "sein", "Ver\u00b7m\u00f6\u00b7gen", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er sucht sich nirgends hoch zu schwingen", "tokens": ["Er", "sucht", "sich", "nir\u00b7gends", "hoch", "zu", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als da, wo fromme Seelen stehn,", "tokens": ["Als", "da", ",", "wo", "from\u00b7me", "See\u00b7len", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Seelen, derer Beyspiel zeigt,", "tokens": ["Die", "See\u00b7len", ",", "de\u00b7rer", "Bey\u00b7spiel", "zeigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie hoch Verstand in Demuth steigt.", "tokens": ["Wie", "hoch", "Ver\u00b7stand", "in", "De\u00b7muth", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein solches Herz mag ohne Wafen", "tokens": ["Ein", "sol\u00b7ches", "Herz", "mag", "oh\u00b7ne", "Wa\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch See und W\u00fcste sicher ziehn,", "tokens": ["Durch", "See", "und", "W\u00fcs\u00b7te", "si\u00b7cher", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es darf, wenn b\u00f6se M\u00e4uler klafen,", "tokens": ["Es", "darf", ",", "wenn", "b\u00f6\u00b7se", "M\u00e4u\u00b7ler", "kla\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht schamroth in den Winckel fliehn;", "tokens": ["Nicht", "scham\u00b7roth", "in", "den", "Win\u00b7ckel", "fliehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Wandel widerlegt den Neid", "tokens": ["Sein", "Wan\u00b7del", "wi\u00b7der\u00b7legt", "den", "Neid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ist sein sch\u00f6nstes Ehrenkleid.", "tokens": ["Und", "ist", "sein", "sch\u00f6ns\u00b7tes", "Eh\u00b7ren\u00b7kleid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wovor sich andre f\u00fcrchten m\u00fc\u00dfen,", "tokens": ["Wo\u00b7vor", "sich", "and\u00b7re", "f\u00fcrch\u00b7ten", "m\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIS", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gespenster, Tr\u00e4ume, H\u00f6ll und Tod,", "tokens": ["Ge\u00b7spens\u00b7ter", ",", "Tr\u00e4u\u00b7me", ",", "H\u00f6ll", "und", "Tod", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das tritt sein weiser Geist mit F\u00fc\u00dfen,", "tokens": ["Das", "tritt", "sein", "wei\u00b7ser", "Geist", "mit", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn ihm noch die S\u00fcnde droht,", "tokens": ["Und", "wenn", "ihm", "noch", "die", "S\u00fcn\u00b7de", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So eilt es Salems H\u00fcgeln zu", "tokens": ["So", "eilt", "es", "Sa\u00b7lems", "H\u00fc\u00b7geln", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und holt sich dort Gewi\u00dfensruh.", "tokens": ["Und", "holt", "sich", "dort", "Ge\u00b7wi\u00b7\u00dfen\u00b7sruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und h\u00f6rt es endlich auch die Stunde,", "tokens": ["Und", "h\u00f6rt", "es", "end\u00b7lich", "auch", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die mit dem Leben Abend macht,", "tokens": ["Die", "mit", "dem", "Le\u00b7ben", "A\u00b7bend", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So giebt es mit getrostem Munde", "tokens": ["So", "giebt", "es", "mit", "ge\u00b7tros\u00b7tem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Eitelkeiten gute Nacht;", "tokens": ["Den", "Ei\u00b7tel\u00b7kei\u00b7ten", "gu\u00b7te", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So bleibt es seelig dort und hier.", "tokens": ["So", "bleibt", "es", "see\u00b7lig", "dort", "und", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott, so ein Herz bescheere mir.", "tokens": ["Gott", ",", "so", "ein", "Herz", "be\u00b7schee\u00b7re", "mir", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.9": {"line.1": {"text": "Wie seelig lebt ein frey Gem\u00fcthe,", "tokens": ["Wie", "see\u00b7lig", "lebt", "ein", "frey", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das weder List noch Rachgier hegt", "tokens": ["Das", "we\u00b7der", "List", "noch", "Rach\u00b7gier", "hegt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "KON", "NN", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und das des weisen Sch\u00f6pfers G\u00fcte", "tokens": ["Und", "das", "des", "wei\u00b7sen", "Sch\u00f6p\u00b7fers", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch in der gr\u00f6sten Noth erwegt,", "tokens": ["Auch", "in", "der", "gr\u00f6s\u00b7ten", "Noth", "er\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja, das in allem, was es \u00fcbt,", "tokens": ["Ja", ",", "das", "in", "al\u00b7lem", ",", "was", "es", "\u00fcbt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "APPR", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gerechtigkeit und Warheit liebt.", "tokens": ["Ge\u00b7rech\u00b7tig\u00b7keit", "und", "War\u00b7heit", "liebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"tex