heus", "von", "den", "Thie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "APPR", "ART", "NN"], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Dem Menschen dies und das erstahl.", "tokens": ["Dem", "Men\u00b7schen", "dies", "und", "das", "er\u00b7stahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDS", "KON", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er schuf nach sch\u00f6nen G\u00f6tterbildern,", "tokens": ["Er", "schuf", "nach", "sch\u00f6\u00b7nen", "G\u00f6t\u00b7ter\u00b7bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Vorsicht Kunst darin zu schildern,", "tokens": ["Der", "Vor\u00b7sicht", "Kunst", "da\u00b7rin", "zu", "schil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Menschen sich ein Ideal.", "tokens": ["Im", "Men\u00b7schen", "sich", "ein", "I\u00b7deal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbim Haupte soll die Pallas thronen;", "tokens": ["\u00bb", "im", "Haup\u00b7te", "soll", "die", "Pal\u00b7las", "thro\u00b7nen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier,\u00ab sprach er, \u00bbsoll die Weisheit wohnen", "tokens": ["Hier", ",", "\u00ab", "sprach", "er", ",", "\u00bb"