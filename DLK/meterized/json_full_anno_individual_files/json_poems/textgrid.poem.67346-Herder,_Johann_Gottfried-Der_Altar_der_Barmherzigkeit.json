{"textgrid.poem.67346": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Altar der Barmherzigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sage will uns irre f\u00fchren,", "tokens": ["Die", "Sa\u00b7ge", "will", "uns", "ir\u00b7re", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df einst Prometheus von den Thieren", "tokens": ["Da\u00df", "einst", "Pro\u00b7me\u00b7theus", "von", "den", "Thie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "APPR", "ART", "NN"], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Dem Menschen dies und das erstahl.", "tokens": ["Dem", "Men\u00b7schen", "dies", "und", "das", "er\u00b7stahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDS", "KON", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er schuf nach sch\u00f6nen G\u00f6tterbildern,", "tokens": ["Er", "schuf", "nach", "sch\u00f6\u00b7nen", "G\u00f6t\u00b7ter\u00b7bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Vorsicht Kunst darin zu schildern,", "tokens": ["Der", "Vor\u00b7sicht", "Kunst", "da\u00b7rin", "zu", "schil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Menschen sich ein Ideal.", "tokens": ["Im", "Men\u00b7schen", "sich", "ein", "I\u00b7deal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbim Haupte soll die Pallas thronen;", "tokens": ["\u00bb", "im", "Haup\u00b7te", "soll", "die", "Pal\u00b7las", "thro\u00b7nen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier,\u00ab sprach er, \u00bbsoll die Weisheit wohnen", "tokens": ["Hier", ",", "\u00ab", "sprach", "er", ",", "\u00bb", "soll", "die", "Weis\u00b7heit", "woh\u00b7nen"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und zeig' im Blicke den Verstand.", "tokens": ["Und", "zeig'", "im", "Bli\u00b7cke", "den", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Stirn sei Tempel der Gedanken;", "tokens": ["Die", "Stirn", "sei", "Tem\u00b7pel", "der", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hier werd' erfunden, was in Schranken", "tokens": ["Hier", "werd'", "er\u00b7fun\u00b7den", ",", "was", "in", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Menschenstirn ein Mensch erfand.", "tokens": ["Der", "Men\u00b7schens\u00b7tirn", "ein", "Mensch", "er\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Aurora soll auf seinen Wangen,", "tokens": ["Au\u00b7ro\u00b7ra", "soll", "auf", "sei\u00b7nen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf seinen Lippen Suada hangen;", "tokens": ["Auf", "sei\u00b7nen", "Lip\u00b7pen", "Sua\u00b7da", "han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Zephyr f\u00e4chle frischen Duft", "tokens": ["Der", "Ze\u00b7phyr", "f\u00e4ch\u00b7le", "fri\u00b7schen", "Duft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit unbemerkbar leichtem Fl\u00fcgel", "tokens": ["Mit", "un\u00b7be\u00b7merk\u00b7bar", "leich\u00b7tem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zu diesem sch\u00f6ngew\u00f6lbten H\u00fcgel;", "tokens": ["Zu", "die\u00b7sem", "sch\u00f6n\u00b7ge\u00b7w\u00f6lb\u00b7ten", "H\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier athme, Mensch, der Gottheit Luft!", "tokens": ["Hier", "ath\u00b7me", ",", "Mensch", ",", "der", "Got\u00b7theit", "Luft", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich will, da\u00df diese Geistesh\u00f6he", "tokens": ["Ich", "will", ",", "da\u00df", "die\u00b7se", "Geis\u00b7tes\u00b7h\u00f6\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gebietend auf dem Thurmbau stehe,", "tokens": ["Ge\u00b7bie\u00b7tend", "auf", "dem", "Thurm\u00b7bau", "ste\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der \u00fcber Thiere sich erhebt.", "tokens": ["Der", "\u00fc\u00b7ber", "Thie\u00b7re", "sich", "er\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In dieser Brust soll St\u00e4rke thronen,", "tokens": ["In", "die\u00b7ser", "Brust", "soll", "St\u00e4r\u00b7ke", "thro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auf diesem Busen Liebe wohnen,", "tokens": ["Auf", "die\u00b7sem", "Bu\u00b7sen", "Lie\u00b7be", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Empfindend, was im Menschen lebt.", "tokens": ["Emp\u00b7fin\u00b7dend", ",", "was", "im", "Men\u00b7schen", "lebt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sein Arm soll Geisteskr\u00e4fte regen,", "tokens": ["Sein", "Arm", "soll", "Geis\u00b7tes\u00b7kr\u00e4f\u00b7te", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die schlanken H\u00e4nde sollen w\u00e4gen", "tokens": ["Die", "schlan\u00b7ken", "H\u00e4n\u00b7de", "sol\u00b7len", "w\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wirken mit Behendigkeit.", "tokens": ["Und", "wir\u00b7ken", "mit", "Be\u00b7hen\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Sein Schenkel steh', und seinen R\u00fccken", "tokens": ["Sein", "Schen\u00b7kel", "steh'", ",", "und", "sei\u00b7nen", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Soll keines Atlas' Last erdr\u00fccken;", "tokens": ["Soll", "kei\u00b7nes", "At\u00b7las'", "Last", "er\u00b7dr\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "NE", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Fu\u00dfe geb' ich Schnelligkeit.", "tokens": ["Dem", "Fu\u00b7\u00dfe", "geb'", "ich", "Schnel\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und inw\u00e4rts diesem Heiligthume", "tokens": ["Und", "in\u00b7w\u00e4rts", "die\u00b7sem", "Hei\u00b7lig\u00b7thu\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stell' ich mir selbst zum ew'gen Ruhme", "tokens": ["Stell'", "ich", "mir", "selbst", "zum", "ew'\u00b7gen", "Ruh\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der F\u00fchlbarkeiten Wunder dar.", "tokens": ["Der", "F\u00fchl\u00b7bar\u00b7kei\u00b7ten", "Wun\u00b7der", "dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier soll mit tausend Leidenschaften", "tokens": ["Hier", "soll", "mit", "tau\u00b7send", "Lei\u00b7den\u00b7schaf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erbarmen, Zorn und Sehnsucht haften,", "tokens": ["Er\u00b7bar\u00b7men", ",", "Zorn", "und", "Sehn\u00b7sucht", "haf\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier sei des Mitgef\u00fchls Altar.\u00ab", "tokens": ["Hier", "sei", "des", "Mit\u00b7ge\u00b7f\u00fchls", "Al\u00b7tar", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er schuf das Herz. \u00bbAus m\u00e4cht'ger Quelle", "tokens": ["Er", "schuf", "das", "Herz", ".", "\u00bb", "Aus", "m\u00e4cht'\u00b7ger", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit nie versiegter, reger Welle", "tokens": ["Mit", "nie", "ver\u00b7sieg\u00b7ter", ",", "re\u00b7ger", "Wel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "ADJD", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Str\u00f6m' hier des Lebens Ueberflu\u00df.", "tokens": ["Str\u00f6m'", "hier", "des", "Le\u00b7bens", "Ue\u00b7berf\u00b7lu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In engen, schlaugewundnen Schranken", "tokens": ["In", "en\u00b7gen", ",", "schlau\u00b7ge\u00b7wund\u00b7nen", "Schran\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Str\u00f6m' er dem Haupte zu Gedanken", "tokens": ["Str\u00f6m'", "er", "dem", "Haup\u00b7te", "zu", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und allen Gliedern Wohlgenu\u00df.\u00ab", "tokens": ["Und", "al\u00b7len", "Glie\u00b7dern", "Wohl\u00b7ge\u00b7nu\u00df", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"tex