{"textgrid.poem.33398": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "An die Rosenn\u00e4therin Schwester M.v.B.", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gute Menschen, die sich innig lieben,", "tokens": ["Gu\u00b7te", "Men\u00b7schen", ",", "die", "sich", "in\u00b7nig", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und in br\u00fcderlicher Eintracht \u00fcben,", "tokens": ["Und", "in", "br\u00fc\u00b7der\u00b7li\u00b7cher", "Ein\u00b7tracht", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Senden dieses Angedenken dir", "tokens": ["Sen\u00b7den", "die\u00b7ses", "An\u00b7ge\u00b7den\u00b7ken", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PDAT", "NN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Rosen n\u00e4htest du f\u00fcr deine Br\u00fcder", "tokens": ["Ro\u00b7sen", "n\u00e4h\u00b7test", "du", "f\u00fcr", "dei\u00b7ne", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Rosen geben sie zum Dank dir wieder,", "tokens": ["Ro\u00b7sen", "ge\u00b7ben", "sie", "zum", "Dank", "dir", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ehre, M\u00e4dchen, diese Dankbegier!", "tokens": ["Eh\u00b7re", ",", "M\u00e4d\u00b7chen", ",", "die\u00b7se", "Dank\u00b7be\u00b7gier", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PDAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Willst du, da\u00df die Unschuld deiner Wange", "tokens": ["Willst", "du", ",", "da\u00df", "die", "Un\u00b7schuld", "dei\u00b7ner", "Wan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stets so sch\u00f6n, wie diese Rosen prange,", "tokens": ["Stets", "so", "sch\u00f6n", ",", "wie", "die\u00b7se", "Ro\u00b7sen", "pran\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "PWAV", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "So vergesse niemals der Natur!", "tokens": ["So", "ver\u00b7ges\u00b7se", "nie\u00b7mals", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Jeder Reiz, der ihre T\u00f6chter schm\u00fccket,", "tokens": ["Je\u00b7der", "Reiz", ",", "der", "ih\u00b7re", "T\u00f6ch\u00b7ter", "schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und des Mannes Auge nicht ber\u00fccket,", "tokens": ["Und", "des", "Man\u00b7nes", "Au\u00b7ge", "nicht", "be\u00b7r\u00fc\u00b7cket", ","], "token_info": ["word", "w