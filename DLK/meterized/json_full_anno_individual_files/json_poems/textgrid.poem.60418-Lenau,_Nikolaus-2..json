{"textgrid.poem.60418": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich trinke hier allein,", "tokens": ["Ich", "trin\u00b7ke", "hier", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von Freund und Feinden ferne,", "tokens": ["Von", "Freund", "und", "Fein\u00b7den", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In stiller Nacht den Wein", "tokens": ["In", "stil\u00b7ler", "Nacht", "den", "Wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und meide selbst die Sterne:", "tokens": ["Und", "mei\u00b7de", "selbst", "die", "Ster\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da f\u00e4hrt man gerne mit", "tokens": ["Da", "f\u00e4hrt", "man", "ger\u00b7ne", "mit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In Blicken und Gedanken", "tokens": ["In", "Bli\u00b7cken", "und", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und k\u00f6nnt auf solchem Ritt", "tokens": ["Und", "k\u00f6nnt", "auf", "sol\u00b7chem", "Ritt"], "token_