{"textgrid.poem.53653": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Das Firmenschild", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist wo ein frisches Schweineschlachten", "tokens": ["Ist", "wo", "ein", "fri\u00b7sches", "Schwei\u00b7ne\u00b7schlach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und hat man also Wurst im Haus,", "tokens": ["und", "hat", "man", "al\u00b7so", "Wurst", "im", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ADV", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "dann stellt der Schl\u00e4chter zum Nachachten", "tokens": ["dann", "stellt", "der", "Schl\u00e4ch\u00b7ter", "zum", "Nac\u00b7hach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "den Stuhl mit seiner Sch\u00fcrze raus.", "tokens": ["den", "Stuhl", "mit", "sei\u00b7ner", "Sch\u00fcr\u00b7ze", "raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bedienen wo in einer Stampe", "tokens": ["Be\u00b7die\u00b7nen", "wo", "in", "ei\u00b7ner", "Stam\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "statt Kellner M\u00e4dchen, voll und hei\u00df:", "tokens": ["statt", "Kell\u00b7ner", "M\u00e4d\