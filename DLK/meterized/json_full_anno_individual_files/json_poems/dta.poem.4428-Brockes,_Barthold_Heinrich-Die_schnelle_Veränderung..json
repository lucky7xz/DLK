{"dta.poem.4428": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die schnelle Ver\u00e4nderung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach GOTT! unendlichs ewigs All!", "tokens": ["Ach", "GoTT", "!", "un\u00b7end\u00b7lichs", "e\u00b7wigs", "All", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Selbstst\u00e4ndig-selige Vollkommenheit,", "tokens": ["Selbst\u00b7st\u00e4n\u00b7dig\u00b7se\u00b7li\u00b7ge", "Voll\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gib, da\u00df, so lang\u2019 ich hier mein Auge r\u00fchre,", "tokens": ["Gib", ",", "da\u00df", ",", "so", "lang'", "ich", "hier", "mein", "Au\u00b7ge", "r\u00fch\u00b7re", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "$,", "ADV", "ADV", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich Dich, verh\u00fcllt in ird\u2019scher Herrlichkeit,", "tokens": ["Ich", "Dich", ",", "ver\u00b7h\u00fcllt", "in", "ird'\u00b7scher", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit Andachts-voller Lust versp\u00fcre,", "tokens": ["Mit", "An\u00b7dachts\u00b7vol\u00b7ler", "Lust", "ver\u00b7sp\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis da\u00df dereinsten dort in den gestirnten H\u00f6hen", "tokens": ["Bis", "da\u00df", "de\u00b7reins\u00b7ten", "dort", "in", "den", "ge\u00b7stirn\u00b7ten", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PIS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich Deine Majest\u00e4t mag ungehindert sehen,", "tokens": ["Ich", "Dei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", "mag", "un\u00b7ge\u00b7hin\u00b7dert", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und blo\u00df an Dir in ewig-sel\u2019gen Freuden", "tokens": ["Und", "blo\u00df", "an", "Dir", "in", "e\u00b7wig\u00b7sel'\u00b7gen", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ganz ungest\u00f6r\u2019t so Sel\u2019 als Augen weiden!", "tokens": ["Ganz", "un\u00b7ge\u00b7st\u00f6r't", "so", "Sel'", "als", "Au\u00b7gen", "wei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "NE", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}