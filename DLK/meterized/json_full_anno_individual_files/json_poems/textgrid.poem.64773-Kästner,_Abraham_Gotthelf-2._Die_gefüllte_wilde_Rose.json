{"textgrid.poem.64773": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "2. Die gef\u00fcllte wilde Rose", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Felde, wo noch frey vom k\u00fcnstelnden Bem\u00fchen", "tokens": ["Im", "Fel\u00b7de", ",", "wo", "noch", "frey", "vom", "k\u00fcns\u00b7teln\u00b7den", "Be\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "ADV", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die reizende Natur entz\u00fcckt,", "tokens": ["Die", "rei\u00b7zen\u00b7de", "Na\u00b7tur", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sah man sich einen Busch in hundert Aesten ziehen,", "tokens": ["Sah", "man", "sich", "ei\u00b7nen", "Busch", "in", "hun\u00b7dert", "A\u00b7es\u00b7ten", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ART", "NN", "APPR", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Von tausend Rosen ausgeschm\u00fcckt:", "tokens": ["Von", "tau\u00b7send", "Ro\u00b7sen", "aus\u00b7ge\u00b7schm\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcnf Bl\u00e4tter, welche sich an Farb' und Sch\u00f6nheit gleichen,", "tokens": ["F\u00fcnf", "Bl\u00e4t\u00b7ter", ",", "wel\u00b7che", "sich", "an", "Fa\u00b7rb'", "und", "Sch\u00f6n\u00b7heit", "glei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "KON", "NN", "ADJA", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Bekr\u00f6nen jener Blume Haupt;", "tokens": ["Be\u00b7kr\u00f6\u00b7nen", "je\u00b7ner", "Blu\u00b7me", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch einer Blume nur ist gr\u00f6\u00dfrer Schmuck erlaubt,", "tokens": ["Doch", "ei\u00b7ner", "Blu\u00b7me", "nur", "ist", "gr\u00f6\u00df\u00b7rer", "Schmuck", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VAFIN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df ihr die andern alle weichen.", "tokens": ["Da\u00df", "ihr", "die", "an\u00b7dern", "al\u00b7le", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Zum Vorzug, der ihr eigen ist,", "tokens": ["Zum", "Vor\u00b7zug", ",", "der", "ihr", "ei\u00b7gen", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Kann sie allein, in wiederholten Kreisen,", "tokens": ["Kann", "sie", "al\u00b7lein", ",", "in", "wie\u00b7der\u00b7hol\u00b7ten", "Krei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Da einer stets den andern in sich schlie\u00dft,", "tokens": ["Da", "ei\u00b7ner", "stets", "den", "an\u00b7dern", "in", "sich", "schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "ART", "ADJA", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "F\u00fcnf Bl\u00e4tter jedesmal, doch oft vervielfacht weisen.", "tokens": ["F\u00fcnf", "Bl\u00e4t\u00b7ter", "je\u00b7des\u00b7mal", ",", "doch", "oft", "ver\u00b7viel\u00b7facht", "wei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "$,", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie fand ein Blumenfreund, er nahm sie mit Vergn\u00fcgen;", "tokens": ["Sie", "fand", "ein", "Blu\u00b7men\u00b7freund", ",", "er", "nahm", "sie", "mit", "Ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die andern w\u00fcrdigt er nicht einmal anzusehn:", "tokens": ["Die", "an\u00b7dern", "w\u00fcr\u00b7digt", "er", "nicht", "ein\u00b7mal", "an\u00b7zu\u00b7sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "PTKNEG", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie ist dadurch der Rose Stolz gestiegen!", "tokens": ["Wie", "ist", "da\u00b7durch", "der", "Ro\u00b7se", "Stolz", "ge\u00b7stie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PAV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Wie fing sie an, die Schwestern zu verschm\u00e4hn!", "tokens": ["Wie", "fing", "sie", "an", ",", "die", "Schwes\u00b7tern", "zu", "ver\u00b7schm\u00e4hn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Doch ihren hohen Sinn zu schw\u00e4chen,", "tokens": ["Doch", "ih\u00b7ren", "ho\u00b7hen", "Sinn", "zu", "schw\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Hat ihr der, der sie nahm, des Vorzugs Grund erkl\u00e4rt:", "tokens": ["Hat", "ihr", "der", ",", "der", "sie", "nahm", ",", "des", "Vor\u00b7zugs", "Grund", "er\u00b7kl\u00e4rt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u00bbim Garten w\u00fcrde man unz\u00e4hlig bessre brechen,", "tokens": ["\u00bb", "im", "Gar\u00b7ten", "w\u00fcr\u00b7de", "man", "un\u00b7z\u00e4h\u00b7lig", "bess\u00b7re", "bre\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "VAFIN", "PIS", "ADJD", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Am wilden Rosenstrauch bist du bewundernswerth.\u00ab", "tokens": ["Am", "wil\u00b7den", "Ro\u00b7sen\u00b7st