{"textgrid.poem.40490": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Winter steigt, ein Riesenschwan, hernieder,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Winter steigt, ein Riesenschwan, hernieder,", "tokens": ["Der", "Win\u00b7ter", "steigt", ",", "ein", "Rie\u00b7sen\u00b7schwan", ",", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die weite Welt bedeckt sein Schneegefieder.", "tokens": ["Die", "wei\u00b7te", "Welt", "be\u00b7deckt", "sein", "Schnee\u00b7ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er singt kein Lied, so sterbensmatt er liegt,", "tokens": ["Er", "singt", "kein", "Lied", ",", "so", "ster\u00b7bens\u00b7matt", "er", "liegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und br\u00fctend auf die todte Saat sich schmiegt;", "tokens": ["Und", "br\u00fc\u00b7tend", "auf", "die", "tod\u00b7te", "Saat", "sich", "schmiegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der junge Lenz doch schl\u00e4ft in seinem Schoo\u00df,", "tokens": ["Der", "jun\u00b7ge", "Lenz", "doch", "schl\u00e4ft", "in", "sei\u00b7nem", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und saugt an seiner kalten Brust sich gro\u00df,", "tokens": ["Und", "saugt", "an", "sei\u00b7ner", "kal\u00b7ten", "Brust", "sich", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und bl\u00fcht in tausend Blumen wohl herauf,", "tokens": ["Und", "bl\u00fcht", "in", "tau\u00b7send", "Blu\u00b7men", "wohl", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "CARD", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und jubelt einst in tausend Liedern auf.", "tokens": ["Und", "ju\u00b7belt", "einst", "in", "tau\u00b7send", "Lie\u00b7dern", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So steigt, ein bleicher Schwan, der Tod hernieder,", "tokens": ["So", "steigt", ",", "ein", "blei\u00b7cher", "Schwan", ",", "der", "Tod", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ART", "ADJA", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Senkt auf die Saat der Gr\u00e4ber sein Gefieder,", "tokens": ["Senkt", "auf", "die", "Saat", "der", "Gr\u00e4\u00b7ber", "sein", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und breitet weithin \u00fcber stilles Land,", "tokens": ["Und", "brei\u00b7tet", "weit\u00b7hin", "\u00fc\u00b7ber", "stil\u00b7les", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Selbst still und stumm, das starre Eisgewand;", "tokens": ["Selbst", "still", "und", "stumm", ",", "das", "star\u00b7re", "Eis\u00b7ge\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Manch frischen H\u00fcgel, manch verweht Gebein,", "tokens": ["Manch", "fri\u00b7schen", "H\u00fc\u00b7gel", ",", "manch", "ver\u00b7weht", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PIAT", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wohl theure Saaten, h\u00fcllt sein Busen ein;", "tokens": ["Wohl", "theu\u00b7re", "Saa\u00b7ten", ",", "h\u00fcllt", "sein", "Bu\u00b7sen", "ein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wir aber stehn dabei und harren still,", "tokens": ["Wir", "a\u00b7ber", "stehn", "da\u00b7bei", "und", "har\u00b7ren", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct