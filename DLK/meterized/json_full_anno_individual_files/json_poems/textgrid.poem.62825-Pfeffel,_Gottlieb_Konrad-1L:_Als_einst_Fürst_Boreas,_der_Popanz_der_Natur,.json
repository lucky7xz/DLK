{"textgrid.poem.62825": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als einst F\u00fcrst Boreas, der Popanz der Natur,", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als einst F\u00fcrst Boreas, der Popanz der Natur,", "tokens": ["Als", "einst", "F\u00fcrst", "Bo\u00b7re\u00b7as", ",", "der", "Po\u00b7panz", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "NE", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf einen Staatsbesuch zum Vetter Pluto fuhr,", "tokens": ["Auf", "ei\u00b7nen", "Staats\u00b7be\u00b7such", "zum", "Vet\u00b7ter", "Plu\u00b7to", "fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verdarb sein freyer Hauch Alekten", "tokens": ["Ver\u00b7darb", "sein", "frey\u00b7er", "Hauch", "A\u00b7lek\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von ungefehr die hydrische Frisur.", "tokens": ["Von", "un\u00b7ge\u00b7fehr", "die", "hyd\u00b7ri\u00b7sche", "Fri\u00b7sur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das war ein L\u00e4rm! Die falben Schlangen bleckten", "tokens": ["Das", "war", "ein", "L\u00e4rm", "!", "Die", "fal\u00b7ben", "Schlan\u00b7gen", "bleck\u00b7ten"], "token_info": ["word", "word", "word", "