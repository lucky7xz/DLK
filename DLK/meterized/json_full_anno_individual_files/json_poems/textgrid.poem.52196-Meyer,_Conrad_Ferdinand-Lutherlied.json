{"textgrid.poem.52196": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Lutherlied", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Knabe wandert \u00fcber Land", "tokens": ["Ein", "Kna\u00b7be", "wan\u00b7dert", "\u00fc\u00b7ber", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einem schlichten Volksgewand,", "tokens": ["In", "ei\u00b7nem", "schlich\u00b7ten", "Volks\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gew\u00f6lke quillt am Himmel auf,", "tokens": ["Ge\u00b7w\u00f6l\u00b7ke", "quillt", "am", "Him\u00b7mel", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er blickt empor, er eilt den Lauf,", "tokens": ["Er", "blickt", "em\u00b7por", ",", "er", "eilt", "den", "Lauf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Stracks f\u00e4hrt ein Blitz mit j\u00e4hem Licht", "tokens": ["Stracks", "f\u00e4hrt", "ein", "Blitz", "mit", "j\u00e4\u00b7hem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und raucht an seiner Ferse dicht \u2013", "tokens": ["Und", "raucht", "an", "sei\u00b7ner", "Fer\u00b7se", "dicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So ward getauft an jenem Tag", "tokens": ["So", "ward", "ge\u00b7tauft", "an", "je\u00b7nem", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Bergmanns Sohn vom Wetterschlag.", "tokens": ["Des", "Berg\u00b7manns", "Sohn", "vom", "Wet\u00b7ter\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schmal ist der Klosterzelle Raum,", "tokens": ["Schmal", "ist", "der", "Klos\u00b7ter\u00b7zel\u00b7le", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drin lebt ein J\u00fcngling dumpfen Traum,", "tokens": ["Drin", "lebt", "ein", "J\u00fcng\u00b7ling", "dum\u00b7pfen", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er flei\u00dfigt sich der M\u00f6ncherei,", "tokens": ["Er", "flei\u00b7\u00dfigt", "sich", "der", "M\u00f6n\u00b7che\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er durch Werke selig sei,", "tokens": ["Da\u00df", "er", "durch", "Wer\u00b7ke", "se\u00b7lig", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADJD", "VAFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ein V\u00f6glein blickt zu ihm ins Grab,", "tokens": ["Ein", "V\u00f6\u00b7glein", "blickt", "zu", "ihm", "ins", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbluthere\u00ab, singt's, \u00bbwirf ab, wirf ab!", "tokens": ["\u00bb", "lu\u00b7the\u00b7re", "\u00ab", ",", "singt's", ",", "\u00bb", "wirf", "ab", ",", "wirf", "ab", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "$(", "$,", "VVFIN", "$,", "$(", "VVIMP", "PTKVZ", "$,", "VVIMP", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Ich flattre durch die lichte Welt,", "tokens": ["Ich", "flatt\u00b7re", "durch", "die", "lich\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Derweil mich Gottes Gnade h\u00e4lt.\u00ab", "tokens": ["Der\u00b7weil", "mich", "Got\u00b7tes", "Gna\u00b7de", "h\u00e4lt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "NN", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In Augsburg war's, da\u00df der Legat", "tokens": ["In", "Augs\u00b7burg", "wa\u00b7r's", ",", "da\u00df", "der", "Le\u00b7gat"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "VAFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein M\u00f6nchlein auf die Stube bat,", "tokens": ["Ein", "M\u00f6nch\u00b7lein", "auf", "die", "Stu\u00b7be", "bat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er war ein grundgelehrtes Haus,", "tokens": ["Er", "war", "ein", "grund\u00b7ge\u00b7lehr\u00b7tes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch kannt er nicht die Geister aus,", "tokens": ["Doch", "kannt", "er", "nicht", "die", "Geis\u00b7ter", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des M\u00f6nchleins Augen brannten tief,", "tokens": ["Des", "M\u00f6nch\u00b7leins", "Au\u00b7gen", "brann\u00b7ten", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df er: \u00bbEs ist der D\u00e4mon!\u00ab rief \u2013", "tokens": ["Da\u00df", "er", ":", "\u00bb", "Es", "ist", "der", "D\u00e4\u00b7mon", "!", "\u00ab", "rief", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$.", "$(", "PPER", "VAFIN", "ART", "NN", "$.", "$(", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du bebst vor diesem scharfen Strahl?", "tokens": ["Du", "bebst", "vor", "die\u00b7sem", "schar\u00b7fen", "Strahl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So blickt die Wahrheit, Kardinal!", "tokens": ["So", "blickt", "die", "Wahr\u00b7heit", ",", "Kar\u00b7di\u00b7nal", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jetzt tritt am Wittenberger Tor", "tokens": ["Jetzt", "tritt", "am", "Wit\u00b7ten\u00b7ber\u00b7ger", "Tor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein M\u00f6nch aus allem Volk hervor:", "tokens": ["Ein", "M\u00f6nch", "aus", "al\u00b7lem", "Volk", "her\u00b7vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Flamme steigt auf seinen Wink,", "tokens": ["Die", "Flam\u00b7me", "steigt", "auf", "sei\u00b7nen", "Wink", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bulle schmei\u00dft hinein er flink,", "tokens": ["Die", "Bul\u00b7le", "schmei\u00dft", "hin\u00b7ein", "er", "flink", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Paulus schlenkert' in den Brand", "tokens": ["Wie", "Pau\u00b7lus", "schlen\u00b7kert'", "in", "den", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Den Wurm, der ihm den Arm umwand,", "tokens": ["Den", "Wurm", ",", "der", "ihm", "den", "Arm", "um\u00b7wand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und \u00fcber Deutschland einen Schein", "tokens": ["Und", "\u00fc\u00b7ber", "Deutschland", "ei\u00b7nen", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Wie Nordlicht wirft das Feuerlein.", "tokens": ["Wie", "Nord\u00b7licht", "wirft", "das", "Feu\u00b7er\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "In Worms sprach Martin Luther frank", "tokens": ["In", "Worms", "sprach", "Mar\u00b7tin", "Lu\u00b7ther", "frank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "NE", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Kaiser und zur F\u00fcrstenbank:", "tokens": ["Zum", "Kai\u00b7ser", "und", "zur", "F\u00fcrs\u00b7ten\u00b7bank", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbsuch, Menschenherz, wo du dich labst!", "tokens": ["\u00bb", "such", ",", "Men\u00b7schen\u00b7herz", ",", "wo", "du", "dich", "labst", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das lehrt dich nicht Konzil noch Papst!", "tokens": ["Das", "lehrt", "dich", "nicht", "Kon\u00b7zil", "noch", "Papst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Quelle str\u00f6mt an tiefrem Ort:", "tokens": ["Die", "Quel\u00b7le", "str\u00f6mt", "an", "tief\u00b7rem", "Ort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der lautre Born, das reine Wort", "tokens": ["Der", "laut\u00b7re", "Born", ",", "das", "rei\u00b7ne", "Wort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Stillt unsrer Seelen Heilsbegier \u2013", "tokens": ["Stillt", "uns\u00b7rer", "See\u00b7len", "Heils\u00b7be\u00b7gier", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hier steh ich und Gott helfe mir!\u00ab", "tokens": ["Hier", "steh", "ich", "und", "Gott", "hel\u00b7fe", "mir", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Herr Kaiser Karl, du warst zu fein,", "tokens": ["Herr", "Kai\u00b7ser", "Karl", ",", "du", "warst", "zu", "fein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NE", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Luther fandest du gemein \u2013", "tokens": ["Den", "Lu\u00b7ther", "fan\u00b7dest", "du", "ge\u00b7mein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gemein wie Lieb und Zorn und Pflicht,", "tokens": ["Ge\u00b7mein", "wie", "Lieb", "und", "Zorn", "und", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie unsrer Kinder Angesicht,", "tokens": ["Wie", "uns\u00b7rer", "Kin\u00b7der", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Hof und Heim, wie Salz und Brot,", "tokens": ["Wie", "Hof", "und", "Heim", ",", "wie", "Salz", "und", "Brot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$,", "PWAV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie die Geburt und wie der Tod \u2013", "tokens": ["Wie", "die", "Ge\u00b7burt", "und", "wie", "der", "Tod", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "PWAV", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er atmet tief in unsrer Brust,", "tokens": ["Er", "at\u00b7met", "tief", "in", "uns\u00b7rer", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und du begrubst dich in Sankt Just.", "tokens": ["Und", "du", "be\u00b7grubst", "dich", "in", "Sankt", "Just", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbein feste Burg\u00ab \u2013 im Lande steht,", "tokens": ["\u00bb", "ein", "fes\u00b7te", "Burg", "\u00ab", "\u2013", "im", "Lan\u00b7de", "steht", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$(", "$(", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drin wacht der Luther fr\u00fch und sp\u00e4t,", "tokens": ["Drin", "wacht", "der", "Lu\u00b7ther", "fr\u00fch", "und", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis redlich er, und Spruch um Spruch,", "tokens": ["Bis", "red\u00b7lich", "er", ",", "und", "Spruch", "um", "Spruch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PPER", "$,", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verdeutscht das liebe Bibelbuch.", "tokens": ["Ver\u00b7deutscht", "das", "lie\u00b7be", "Bi\u00b7bel\u00b7buch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Herr Doktor, sprecht! Wo nahmt Ihr her", "tokens": ["Herr", "Dok\u00b7tor", ",", "sprecht", "!", "Wo", "nahmt", "Ihr", "her"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "VVFIN", "$.", "PWAV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text":