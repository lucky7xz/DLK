{"textgrid.poem.34560": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ist's nicht im Grunde wesenloser Tand,", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist's nicht im Grunde wesenloser Tand,", "tokens": ["Ist's", "nicht", "im", "Grun\u00b7de", "we\u00b7sen\u00b7lo\u00b7ser", "Tand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "was ich in Reimen aneinander f\u00fcge?", "tokens": ["was", "ich", "in", "Rei\u00b7men", "an\u00b7ein\u00b7an\u00b7der", "f\u00fc\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist's nicht im Grunde eine bunte L\u00fcge,", "tokens": ["Ist's", "nicht", "im", "Grun\u00b7de", "ei\u00b7ne", "bun\u00b7te", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "was ich in m\u00fcssig heitrem Spiel erfand?", "tokens": ["was", "ich", "in", "m\u00fcs\u00b7sig", "heit\u00b7rem", "Spiel", "er\u00b7fand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Scheint dir mein Reimgeb\u00e4ude imposant?", "tokens": ["Scheint", "dir", "mein", "Reim\u00b7ge\u00b7b\u00e4u\u00b7de", "im\u00b7po\u00b7sant", "?"], "token_info": ["word", "word", "word", "word