{"textgrid.poem.39963": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "4. Wie die K\u00f6nige fuhren", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch war die Reise noch so fern,", "tokens": ["Doch", "war", "die", "Rei\u00b7se", "noch", "so", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So ging die Fahrt doch wunderleicht,", "tokens": ["So", "ging", "die", "Fahrt", "doch", "wun\u00b7der\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor jedem wandelt hin der Stern,", "tokens": ["Vor", "je\u00b7dem", "wan\u00b7delt", "hin", "der", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Sterne, Mond und Sonne bleicht.", "tokens": ["Der", "Ster\u00b7ne", ",", "Mond", "und", "Son\u00b7ne", "bleicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Hunger kam, kein Schlaf auf sie,", "tokens": ["Kein", "Hun\u00b7ger", "kam", ",", "kein", "Schlaf", "auf", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PIAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es war ein ew'ger, gleicher Tag,", "tokens": ["Es", "war", "ein", "ew'\u00b7ger", ",", "glei\u00b7cher", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nach keinem Futter schnaubt ihr Vieh,", "tokens": ["Nach", "kei\u00b7nem", "Fut\u00b7ter", "schnaubt", "ihr", "Vieh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es gehn die Hufe Schlag auf Schlag.", "tokens": ["Es", "gehn", "die", "Hu\u00b7fe", "Schlag", "auf", "Schlag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In keiner Herberg h\u00e4lt der Zug,", "tokens": ["In", "kei\u00b7ner", "Her\u00b7berg", "h\u00e4lt", "der", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn lockt nicht Lust, ihn hemmt nicht Qual,", "tokens": ["Ihn", "lockt", "nicht", "Lust", ",", "ihn", "hemmt", "nicht", "Qual", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Stepp' und Fruchtfeld geht's im Flug", "tokens": ["Durch", "Stepp'", "und", "Frucht\u00b7feld", "geht's", "im", "Flug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "