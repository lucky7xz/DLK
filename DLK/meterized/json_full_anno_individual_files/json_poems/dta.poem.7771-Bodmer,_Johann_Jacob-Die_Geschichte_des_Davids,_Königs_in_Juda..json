{"dta.poem.7771": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschichte des Davids,  \n K\u00f6nigs in Juda.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Saul der zu Gibea sich fand mit seinen Helden,", "tokens": ["Saul", "der", "zu", "Gi\u00b7bea", "sich", "fand", "mit", "sei\u00b7nen", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "APPR", "NE", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Als die von Socho ihm die\u00df liessen hin vermelden,", "tokens": ["Als", "die", "von", "So\u00b7cho", "ihm", "die\u00df", "lies\u00b7sen", "hin", "ver\u00b7mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "NE", "PPER", "PDS", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "War stets in seiner Burg, und lie\u00df sich wenig sehn,", "tokens": ["War", "stets", "in", "sei\u00b7ner", "Burg", ",", "und", "lie\u00df", "sich", "we\u00b7nig", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil die Ahinoam dies rieth, um zu entgehn,", "tokens": ["Weil", "die", "A\u00b7hi\u00b7no\u00b7am", "dies", "rieth", ",", "um", "zu", "ent\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PDS", "VVFIN", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Da\u00df nicht gantz Jsrael des K\u00f6nigs Trauer-Wesen", "tokens": ["Da\u00df", "nicht", "gantz", "Js\u00b7rael", "des", "K\u00f6\u00b7nigs", "Trau\u00b7e\u00b7rWe\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADV", "NE", "ART", "NN", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Sobald erfahren m\u00f6cht, bis da\u00df er erst genesen,", "tokens": ["So\u00b7bald", "er\u00b7fah\u00b7ren", "m\u00f6cht", ",", "bis", "da\u00df", "er", "erst", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$,", "KON", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie ihre Hoffnung war; und Jonathan sein Sohn,", "tokens": ["Wie", "ih\u00b7re", "Hoff\u00b7nung", "war", ";", "und", "Jo\u00b7na\u00b7than", "sein", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VAFIN", "$.", "KON", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Helden edle Zier und der Soldaten Kron,", "tokens": ["Der", "Hel\u00b7den", "ed\u00b7le", "Zier", "und", "der", "Sol\u00b7da\u00b7ten", "Kron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Verh\u00fctete mit Flei\u00df, da\u00df seines Vaters Zagen", "tokens": ["Ver\u00b7h\u00fc\u00b7te\u00b7te", "mit", "Flei\u00df", ",", "da\u00df", "sei\u00b7nes", "Va\u00b7ters", "Za\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "$,", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nicht w\u00fcrd von einem Stam\u0303 zum andern umgetragen.", "tokens": ["Nicht", "w\u00fcrd", "von", "ei\u00b7nem", "Stam\u0303", "zum", "an\u00b7dern", "um\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "APPR", "ART", "NN", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Ursach war ihm kund, und mehr dann allzu leyd,", "tokens": ["Die", "Ur\u00b7sach", "war", "ihm", "kund", ",", "und", "mehr", "dann", "all\u00b7zu", "leyd", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKVZ", "$,", "KON", "ADV", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und deren zu entgehn war es nun nicht mehr Zeit.", "tokens": ["Und", "de\u00b7ren", "zu", "ent\u00b7gehn", "war", "es", "nun", "nicht", "mehr", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PTKZU", "VVINF", "VAFIN", "PPER", "ADV", "PTKNEG", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die S\u00fcnde war gescheh\u2019n, und der, so Gott verlassen,", "tokens": ["Die", "S\u00fcn\u00b7de", "war", "ge\u00b7scheh'n", ",", "und", "der", ",", "so", "Gott", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "KON", "ART", "$,", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und sein Geboth vers\u00e4umt, ward wieder gleichermassen", "tokens": ["Und", "sein", "Ge\u00b7both", "ver\u00b7s\u00e4umt", ",", "ward", "wie\u00b7der", "glei\u00b7cher\u00b7mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ohn Gottes Geist und Trost alleine hingestellt;", "tokens": ["Ohn", "Got\u00b7tes", "Geist", "und", "Trost", "al\u00b7lei\u00b7ne", "hin\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Gnaden-Th\u00fcr war zu, das Urtheil schon gef\u00e4llt,", "tokens": ["Die", "Gna\u00b7den\u00b7T\u00b7h\u00fcr", "war", "zu", ",", "das", "Ur\u00b7theil", "schon", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da\u00df Saul sein K\u00f6nigreich solt werden hingerissen,", "tokens": ["Da\u00df", "Saul", "sein", "K\u00f6\u00b7nig\u00b7reich", "solt", "wer\u00b7den", "hin\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "VMFIN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wie Samuel ihm lie\u00df von Gottes wegen wissen,", "tokens": ["Wie", "Sa\u00b7muel", "ihm", "lie\u00df", "von", "Got\u00b7tes", "we\u00b7gen", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PPER", "VVFIN", "APPR", "NN", "APPR", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.19": {"text": "Und da\u00df sein N\u00e4chster solt, der besser, als wie er,", "tokens": ["Und", "da\u00df", "sein", "N\u00e4chs\u00b7ter", "solt", ",", "der", "bes\u00b7ser", ",", "als", "wie", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VMFIN", "$,", "PRELS", "ADJD", "$,", "KOUS", "PWAV", "PPER", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Besteigen seinen Thron und seine K\u00f6nigs-Ehr.", "tokens": ["Be\u00b7stei\u00b7gen", "sei\u00b7nen", "Thron", "und", "sei\u00b7ne", "K\u00f6\u00b7nigs\u00b7Ehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wann alles dieses Saul in seinem Sinn erwoge,", "tokens": ["Wann", "al\u00b7les", "die\u00b7ses", "Saul", "in", "sei\u00b7nem", "Sinn", "er\u00b7wo\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und den gedrohten Fall sich zu Gem\u00fcthe zoge,", "tokens": ["Und", "den", "ge\u00b7droh\u00b7ten", "Fall", "sich", "zu", "Ge\u00b7m\u00fc\u00b7the", "zo\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Kam ihm ein Gr\u00e4men an, ein Zagen, eine Quaal,", "tokens": ["Kam", "ihm", "ein", "Gr\u00e4\u00b7men", "an", ",", "ein", "Za\u00b7gen", ",", "ei\u00b7ne", "Qua\u00b7al", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die ihm durch Marck und Bein, durchdrunge \u00fcberall.", "tokens": ["Die", "ihm", "durch", "Marck", "und", "Bein", ",", "durch\u00b7drun\u00b7ge", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Er war aus Gottes Gnad, drum kont kein Trost ihn laben.", "tokens": ["Er", "war", "aus", "Got\u00b7tes", "Gnad", ",", "drum", "kont", "kein", "Trost", "ihn", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "NN", "$,", "PAV", "VMFIN", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der nicht hat Gottes Geist, was wolt der \u00fcbrig haben?", "tokens": ["Der", "nicht", "hat", "Got\u00b7tes", "Geist", ",", "was", "wolt", "der", "\u00fcb\u00b7rig", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VAFIN", "NN", "NN", "$,", "PWS", "VMFIN", "ART", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wann ihm der Satan setzt mit seinen Pfeilen zu,", "tokens": ["Wann", "ihm", "der", "Sa\u00b7tan", "setzt", "mit", "sei\u00b7nen", "Pfei\u00b7len", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wo ist dann wohl ein Orth, der ihme giebet Ruh?", "tokens": ["Wo", "ist", "dann", "wohl", "ein", "Orth", ",", "der", "ih\u00b7me", "gie\u00b7bet", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Doch konte der allein, auf den der Geist des Herren", "tokens": ["Doch", "kon\u00b7te", "der", "al\u00b7lein", ",", "auf", "den", "der", "Geist", "des", "Her\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "ADV", "$,", "APPR", "ART", "ART", "NN", "ART", "NN"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.30": {"text": "Gerathen war, mit Krafft, des K\u00f6nigs Trauren kehren,", "tokens": ["Ge\u00b7ra\u00b7then", "war", ",", "mit", "Krafft", ",", "des", "K\u00f6\u00b7nigs", "Trau\u00b7ren", "keh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "APPR", "NN", "$,", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der sch\u00f6ne Hirten-Knab, Jsai j\u00fcngster Sohn,", "tokens": ["Der", "sch\u00f6\u00b7ne", "Hir\u00b7ten\u00b7Knab", ",", "Jsai", "j\u00fcngs\u00b7ter", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Der David, des Geschlechts und seines Hauses Kron.", "tokens": ["Der", "Da\u00b7vid", ",", "des", "Ge\u00b7schlechts", "und", "sei\u00b7nes", "Hau\u00b7ses", "Kron", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ART", "NN", "KON", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Den hatte man nach Hof zum K\u00f6nig lassen kommen,", "tokens": ["Den", "hat\u00b7te", "man", "nach", "Hof", "zum", "K\u00f6\u00b7nig", "las\u00b7sen", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PIS", "APPR", "NE", "APPRART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wie man an ihm zuerst die Hertzensangst vernommen,", "tokens": ["Wie", "man", "an", "ihm", "zu\u00b7erst", "die", "Hert\u00b7zen\u00b7sangst", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.35": {"text": "Und weil sonst keine Cur den K\u00f6nig heilen kunnt,", "tokens": ["Und", "weil", "sonst", "kei\u00b7ne", "Cur", "den", "K\u00f6\u00b7nig", "hei\u00b7len", "kunnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PIAT", "NN", "ART", "NN", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So solt es David thun, der die Musik verstuhnd,", "tokens": ["So", "solt", "es", "Da\u00b7vid", "thun", ",", "der", "die", "Mu\u00b7sik", "ver\u00b7stuhnd", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NE", "VVINF", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und Stimm und Harffenklang so k\u00fcnstlich wu\u00dft zu f\u00fchren,", "tokens": ["Und", "Stimm", "und", "Harf\u00b7fen\u00b7klang", "so", "k\u00fcnst\u00b7lich", "wu\u00dft", "zu", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "ADJD", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Da\u00df ihre Lieblichkeit konnt auch die Geister r\u00fchren.", "tokens": ["Da\u00df", "ih\u00b7re", "Lieb\u00b7lich\u00b7keit", "konnt", "auch", "die", "Geis\u00b7ter", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der b\u00f6se Geist entweicht, wann Davids Harff erklingt,", "tokens": ["Der", "b\u00f6\u00b7se", "Geist", "ent\u00b7weicht", ",", "wann", "Da\u00b7vids", "Harff", "er\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PWAV", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Weil dessen Reinlichkeit ihm einen Schrecken bringt.", "tokens": ["Weil", "des\u00b7sen", "Rein\u00b7lich\u00b7keit", "ihm", "ei\u00b7nen", "Schre\u00b7cken", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELAT", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Er meidet David drum, weil da aus dessen Lende