{"dta.poem.3125": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "170.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Woher nimmst du den Muth, von neuem vorzutragen,", "tokens": ["Wo\u00b7her", "nimmst", "du", "den", "Muth", ",", "von", "neu\u00b7em", "vor\u00b7zu\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$,", "APPR", "ADJA", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was l\u00e4ngst schon besser ward gesagt in alten Tagen? \u2014", "tokens": ["Was", "l\u00e4ngst", "schon", "bes\u00b7ser", "ward", "ge\u00b7sagt", "in", "al\u00b7ten", "Ta\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Weil alles Alte neu und immer neu mu\u00df werden,", "tokens": ["Weil", "al\u00b7les", "Al\u00b7te", "neu", "und", "im\u00b7mer", "neu", "mu\u00df", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "ADJD", "KON", "ADV", "ADJD", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So tr\u00e4gt die Dichtung auch stets ihrer Zeit Geberden.", "tokens": ["So", "tr\u00e4gt", "die", "Dich\u00b7tung", "auch", "stets", "ih\u00b7rer", "Zeit", "Ge\u00b7ber\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verwandeln mu\u00df sie sich, beharren kann sie nimmer;", "tokens": ["Ver\u00b7wan\u00b7deln", "mu\u00df", "sie", "sich", ",", "be\u00b7har\u00b7ren", "kann", "sie", "nim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "$,", "VVINF", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht besser wird sie stets, zuweilen wird sie schlimmer.", "tokens": ["Nicht", "bes\u00b7ser", "wird", "sie", "stets", ",", "zu\u00b7wei\u00b7len", "wird", "sie", "schlim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein angestammtes Recht hat jedes Zeitgeschlecht,", "tokens": ["Ein", "an\u00b7ge\u00b7stamm\u00b7tes", "Recht", "hat", "je\u00b7des", "Zeit\u00b7ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Zeiten Weisheit sich zu machen mundgerecht.", "tokens": ["Der", "Zei\u00b7ten", "Weis\u00b7heit", "sich", "zu", "ma\u00b7chen", "mund\u00b7ge\u00b7recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "PTKZU", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und jeder hat dies Recht f\u00fcr sich auch und sein Haus;", "tokens": ["Und", "je\u00b7der", "hat", "dies", "Recht", "f\u00fcr", "sich", "auch", "und", "sein", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PDS", "NN", "APPR", "PRF", "ADV", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und er macht es nicht schlecht, wenn er damit kommt aus.", "tokens": ["Und", "er", "macht", "es", "nicht", "schlecht", ",", "wenn", "er", "da\u00b7mit", "kommt", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "ADJD", "$,", "KOUS", "PPER", "PAV", "VVFIN", "PTKVZ", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Nur hat er nicht das Recht, es andern aufzudringen,", "tokens": ["Nur", "hat", "er", "nicht", "das", "Recht", ",", "es", "an\u00b7dern", "auf\u00b7zu\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "$,", "PPER", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein eigen Hausgem\u00e4cht auch auf den Markt zu bringen.", "tokens": ["Sein", "ei\u00b7gen", "Haus\u00b7ge\u00b7m\u00e4cht", "auch", "auf", "den", "Markt", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Bring' ich das Meine doch, so bild' ich wol mir ein,", "tokens": ["Bring'", "ich", "das", "Mei\u00b7ne", "doch", ",", "so", "bild'", "ich", "wol", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "PPOSAT", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es sei f\u00fcr andre noch, und nicht f\u00fcr mich allein.", "tokens": ["Es", "sei", "f\u00fcr", "and\u00b7re", "noch", ",", "und", "nicht", "f\u00fcr", "mich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "ADV", "$,", "KON", "PTKNEG", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}