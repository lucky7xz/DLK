{"dta.poem.20435": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Wettstreit der tulpen/ nelcke/ rose/  \n lilie/ cypre\u00df/ narci\u00df und  \n hyacinth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der rosen lieblich blut/ der liljen silber-schein/", "tokens": ["Der", "ro\u00b7sen", "lieb\u00b7lich", "blut", "/", "der", "lil\u00b7jen", "sil\u00b7ber\u00b7schein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der nelcken bunte zier w\u00e4chst nicht auff meinen zweigen/", "tokens": ["Der", "nel\u00b7cken", "bun\u00b7te", "zier", "w\u00e4chst", "nicht", "auff", "mei\u00b7nen", "zwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich kan sonst weiter nichts als gr\u00fcne bleichheit zeigen/", "tokens": ["Ich", "kan", "sonst", "wei\u00b7ter", "nichts", "als", "gr\u00fc\u00b7ne", "bleich\u00b7heit", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PIS", "KOKOM", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und zu dem leichen schmuck nur eintzig dienlich seyn.", "tokens": ["Und", "zu", "dem", "lei\u00b7chen", "schmuck", "nur", "eint\u00b7zig", "dien\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADV", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch wenn des winters frost reist saam und buntwerck ein/", "tokens": ["Doch", "wenn", "des", "win\u00b7ters", "frost", "reist", "saam", "und", "bunt\u00b7werck", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "ADJD", "KON", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So sieht ein wundernd aug aus meinen stengel steigen/", "tokens": ["So", "sieht", "ein", "wun\u00b7dernd", "aug", "aus", "mei\u00b7nen", "sten\u00b7gel", "stei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Verneute lebens-krafft. Mir bleibt solch wachsthum eigen/", "tokens": ["Ver\u00b7neu\u00b7te", "le\u00b7bens\u00b7krafft", ".", "Mir", "bleibt", "solch", "wach\u00b7sthum", "ei\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PPER", "VVFIN", "PIAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das sonst bey reiff und schnee den blumen ungemein.", "tokens": ["Das", "sonst", "bey", "reiff", "und", "schnee", "den", "blu\u00b7men", "un\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "ADJD", "KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Aus zunder reiner lieb entsprang mein erstes kraut/", "tokens": ["Aus", "zun\u00b7der", "rei\u00b7ner", "lieb", "ent\u00b7sprang", "mein", "ers\u00b7tes", "kraut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und eine staud umschlo\u00df den sch\u00f6nen Cyparissen/", "tokens": ["Und", "ei\u00b7ne", "staud", "um\u00b7schlo\u00df", "den", "sch\u00f6\u00b7nen", "Cy\u00b7pa\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Drauff lie\u00df mein buhler thau und endlich thr\u00e4nen fliessen", "tokens": ["Drauff", "lie\u00df", "mein", "buh\u00b7ler", "thau", "und", "end\u00b7lich", "thr\u00e4\u00b7nen", "flies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "ADJA", "NN", "KON", "ADV", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bi\u00df da\u00df er solchen strauch in solcher bl", "tokens": ["Bi\u00df", "da\u00df", "er", "sol\u00b7chen", "strauch", "in", "sol\u00b7cher", "bl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PIAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und da\u00df ich eingedenck des ursprungs ewig bliebe/", "tokens": ["Und", "da\u00df", "ich", "ein\u00b7ge\u00b7denck", "des", "ur\u00b7sprungs", "e\u00b7wig", "blie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So trag ich einig nur zu gr\u00e4bern meine liebe.", "tokens": ["So", "trag", "ich", "ei\u00b7nig", "nur", "zu", "gr\u00e4\u00b7bern", "mei\u00b7ne", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "PTKZU", "VVINF", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}