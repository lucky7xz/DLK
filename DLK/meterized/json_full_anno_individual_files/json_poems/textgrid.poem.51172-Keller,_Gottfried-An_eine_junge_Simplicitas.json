{"textgrid.poem.51172": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An eine junge Simplicitas", "genre": "verse", "period": "N.A.", "pub_year": 1880, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00e4mig versagst du den Blick dem \u00fcbel beleumdeten Ketzer,", "tokens": ["Sch\u00e4\u00b7mig", "ver\u00b7sagst", "du", "den", "Blick", "dem", "\u00fc\u00b7bel", "be\u00b7leum\u00b7de\u00b7ten", "Ket\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Spendest zur Seite gewandt deinen verk\u00fcmmerten Knicks!", "tokens": ["Spen\u00b7dest", "zur", "Sei\u00b7te", "ge\u00b7wandt", "dei\u00b7nen", "ver\u00b7k\u00fcm\u00b7mer\u00b7ten", "Knicks", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "VVPP", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Schwebe nur zierlich von hinnen: als M\u00fctterchen seh ich dich humpeln,", "tokens": ["Schwe\u00b7be", "nur", "zier\u00b7lich", "von", "hin\u00b7nen", ":", "als", "M\u00fct\u00b7ter\u00b7chen", "seh", "ich", "dich", "hum\u00b7peln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "ADV", "$.", "KOUS", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.4": {"text": "Welches zu Hussens Gericht steuert sein schwelendes Scheit!", "tokens": ["Wel\u00b7ches", "zu", "Hus\u00b7sens", "Ge\u00b7richt", "steu\u00b7ert", "sein", "schwe\u00b7len\u00b7des", "Scheit", "!"], "token_info"