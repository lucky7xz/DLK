{"dta.poem.9836": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der verzweifflende liebhaber.  \n  Flectere si nequeo superos, Acheronta  \n movebo.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erz\u00fcrnter himmel/ brich mit deinem blitz herein!", "tokens": ["Er\u00b7z\u00fcrn\u00b7ter", "him\u00b7mel", "/", "brich", "mit", "dei\u00b7nem", "blitz", "her\u00b7ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "ADJD", "APPR", "PPOSAT", "VVIMP", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr berge \u00f6ffnet euch und eure dunckle gr\u00fcffte/", "tokens": ["Ihr", "ber\u00b7ge", "\u00f6ff\u00b7net", "euch", "und", "eu\u00b7re", "dunck\u00b7le", "gr\u00fcff\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PPER", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gebt her von schwefel-dunst/ und von verdammtem giffte/", "tokens": ["Gebt", "her", "von", "schwe\u00b7fel\u00b7dunst", "/", "und", "von", "ver\u00b7damm\u00b7tem", "giff\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "NE", "$(", "KON", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mischt diamanten-staub und schlangen-geiffer drein/", "tokens": ["Mischt", "di\u00b7a\u00b7man\u00b7ten\u00b7staub", "und", "schlan\u00b7gen\u00b7geif\u00b7fer", "drein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ADJD", "PTKVZ", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Thut drachen-blut hinzu zur k\u00fcrtzung meiner pein;", "tokens": ["Thut", "dra\u00b7chen\u00b7blut", "hin\u00b7zu", "zur", "k\u00fcrt\u00b7zung", "mei\u00b7ner", "pein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Streut pech und flammen au\u00df/ ihr donner-schwangern l\u00fcffte;", "tokens": ["Streut", "pech", "und", "flam\u00b7men", "au\u00df", "/", "ihr", "don\u00b7ner\u00b7schwan\u00b7gern", "l\u00fcff\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "PTKVZ", "$(", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Reicht stahl und elsen her/ ihr harte felsen-kl\u00fcffte;", "tokens": ["Reicht", "stahl", "und", "el\u00b7sen", "her", "/", "ihr", "har\u00b7te", "fel\u00b7sen\u00b7kl\u00fcff\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "ADJD", "PTKVZ", "$(", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich kan/ doch was? ich will nicht mehr am leben seyn.", "tokens": ["Ich", "kan", "/", "doch", "was", "?", "ich", "will", "nicht", "mehr", "am", "le\u00b7ben", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "KON", "PWS", "$.", "PPER", "VMFIN", "PTKNEG", "ADV", "PTKA", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Kommt/ f\u00f6rdert meinen tod/ zerst\u00fcckt die schwachen glieder/", "tokens": ["Kommt", "/", "f\u00f6r\u00b7dert", "mei\u00b7nen", "tod", "/", "zer\u00b7st\u00fcckt", "die", "schwa\u00b7chen", "glie\u00b7der", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "PPOSAT", "NN", "$(", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Entgeistert fleisch und blut/ schlagt in den staub mich nieder/", "tokens": ["Ent\u00b7geis\u00b7tert", "fleisch", "und", "blut", "/", "schlagt", "in", "den", "staub", "mich", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "NN", "$(", "VVFIN", "APPR", "ART", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Helfft meinem leben ab/ das mit verzweiflung ringt/", "tokens": ["Helfft", "mei\u00b7nem", "le\u00b7ben", "ab", "/", "das", "mit", "ver\u00b7zwei\u00b7flung", "ringt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "VVFIN", "PTKVZ", "$(", "PDS", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "D\u00e4mpfft meine liebes-glut/ die mich zum sterben zwingt;", "tokens": ["D\u00e4mpfft", "mei\u00b7ne", "lie\u00b7bes\u00b7glut", "/", "die", "mich", "zum", "ster\u00b7ben", "zwingt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$(", "PRELS", "PRF", "APPRART", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Trit/ h\u00f6lle/ selbst hervor mit deinen lichten flammen;", "tokens": ["Trit", "/", "h\u00f6l\u00b7le", "/", "selbst", "her\u00b7vor", "mit", "dei\u00b7nen", "lich\u00b7ten", "flam\u00b7men", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "$(", "ADV", "PTKVZ", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Will erd und himmel nicht/ wirs", "tokens": ["Will", "erd", "und", "him\u00b7mel", "nicht", "/", "wirs"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "NN", "KON", "NN", "PTKNEG", "$(", "XY"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}