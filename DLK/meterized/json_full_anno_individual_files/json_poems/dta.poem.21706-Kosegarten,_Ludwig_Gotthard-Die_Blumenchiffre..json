{"dta.poem.21706": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die Blumenchiffre.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Eine Eugenia sah ich, verm&#228;hlt dem edlen Pla-\ntanus.", "tokens": ["Ei\u00b7ne", "Eu\u00b7ge\u00b7nia", "sah", "ich", ",", "verm", "&#228;", "hlt", "dem", "ed\u00b7len", "Pla", "ta\u00b7nus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "$,", "ADJA", "$(", "VVFIN", "ART", "ADJA", "NN", "NE", "$."], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Froh des vertraulichen Schirms, bl\u00fcheten Blu-", "tokens": ["Froh", "des", "ver\u00b7trau\u00b7li\u00b7chen", "Schirms", ",", "bl\u00fc\u00b7he\u00b7ten", "Blu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,", "ADJA", "TRUNC"], "meter": "+--+--++--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Jegliche anders gef\u00e4rbt, und jegliche anders ge-", "tokens": ["Jeg\u00b7li\u00b7che", "an\u00b7ders", "ge\u00b7f\u00e4rbt", ",", "und", "jeg\u00b7li\u00b7che", "an\u00b7ders", "ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "VVPP", "$,", "KON", "PIS", "ADV", "TRUNC"], "meter": "+--+--+-+--+--", "measure": "dactylic.di.plus"}, "line.4": {"text": "Jegliche anders begabt von der Natur und dem", "tokens": ["Jeg\u00b7li\u00b7che", "an\u00b7ders", "be\u00b7gabt", "von", "der", "Na\u00b7tur", "und", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "ADJD", "APPR", "ART", "NN", "KON", "ART"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Gott.", "tokens": ["Gott", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Schwermuthduftend entgegen der strahlenden Son-", "tokens": ["Schwer\u00b7muth\u00b7duf\u00b7tend", "ent\u00b7ge\u00b7gen", "der", "strah\u00b7len\u00b7den", "Son"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "TRUNC"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Wendend den ahnenden Blick, schosste der", "tokens": ["Wen\u00b7dend", "den", "ah\u00b7nen\u00b7den", "Blick", ",", "schoss\u00b7te", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "VVFIN", "ART"], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Heliotrop.", "tokens": ["He\u00b7li\u00b7o\u00b7trop", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Blendender bl\u00fcht' und brannte zugleich die sch\u00f6ne", "tokens": ["Blen\u00b7den\u00b7der", "bl\u00fcht'", "und", "brann\u00b7te", "zu\u00b7gleich", "die", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "ADV", "ART", "ADJA"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.10": {"text": "Ixora.", "tokens": ["I\u00b7xo\u00b7ra", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.11": {"text": "Stilleren Reizes zun\u00e4chst senktest du bl\u00f6de den", "tokens": ["Stil\u00b7le\u00b7ren", "Rei\u00b7zes", "zu\u00b7n\u00e4chst", "senk\u00b7test", "du", "bl\u00f6\u00b7de", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ADV", "VVFIN", "PPER", "ADJA", "ART"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Blick,", "tokens": ["Blick", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Holde Mimosa. Es hing der gedankenhauchende", "tokens": ["Hol\u00b7de", "Mi\u00b7mo\u00b7sa", ".", "Es", "hing", "der", "ge\u00b7dan\u00b7ken\u00b7hau\u00b7chen\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA"], "meter": "+--+--+--+-+--", "measure": "dactylic.tri.plus"}, "line.14": {"text": "Diptam", "tokens": ["Dip\u00b7tam"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Schweigsam das sinnige Haupt. G\u00f6ttern und", "tokens": ["Schweig\u00b7sam", "das", "sin\u00b7ni\u00b7ge", "Haupt", ".", "G\u00f6t\u00b7tern", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN", "$.", "NN", "KON"], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.16": {"text": "Menschen geliebt,", "tokens": ["Men\u00b7schen", "ge\u00b7liebt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Funkelt' im Schmelz des Rasens die tausendbl\u00e4ttri-", "tokens": ["Fun\u00b7kelt'", "im", "Schmelz", "des", "Ra\u00b7sens", "die", "tau\u00b7send\u00b7bl\u00e4t\u00b7tri"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "ART", "TRUNC"], "meter": "+--+-+--+---", "measure": "iambic.tetra.invert"}, "line.18": {"text": "Ein Schneegl\u00f6ckchen entspross keimend dem", "tokens": ["Ein", "Schnee\u00b7gl\u00f6ck\u00b7chen", "ent\u00b7spross", "kei\u00b7mend", "dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "gr\u00fcnenden Grund.", "tokens": ["gr\u00fc\u00b7nen\u00b7den", "Grund", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Schlanke Eugenia, dir, gest\u00fctzt auf deinem Platanus,", "tokens": ["Schlan\u00b7ke", "Eu\u00b7ge\u00b7nia", ",", "dir", ",", "ge\u00b7st\u00fctzt", "auf", "dei\u00b7nem", "Pla\u00b7ta\u00b7nus", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPER", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Danket der schimmernde Strauss reinen harmo-", "tokens": ["Dan\u00b7ket", "der", "schim\u00b7mern\u00b7de", "Strauss", "rei\u00b7nen", "har\u00b7mo"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJA", "TRUNC"], "meter": "+--+--++-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Streb' empor in freudiger Kraft, getr\u00e4nkt von Au-", "tokens": ["Streb'", "em\u00b7por", "in", "freu\u00b7di\u00b7ger", "Kraft", ",", "ge\u00b7tr\u00e4nkt", "von", "Au"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "APPR", "ADJA", "NN", "$,", "VVPP", "APPR", "TRUNC"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Duftigsten Thr\u00e4nen, von G\u00e4's kr\u00e4ftigstem Mar-", "tokens": ["Duf\u00b7tigs\u00b7ten", "Thr\u00e4\u00b7nen", ",", "von", "G\u00e4's", "kr\u00e4f\u00b7tigs\u00b7tem", "Ma\u00b7r"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "NE", "ADJA", "TRUNC"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Spat lass sinken, Geliebte, die liebes\u00e4uselnden", "tokens": ["Spat", "lass", "sin\u00b7ken", ",", "Ge\u00b7lieb\u00b7te", ",", "die", "lie\u00b7be\u00b7s\u00e4u\u00b7seln\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "NN", "$,", "ART", "ADJA"], "meter": "+-+--+--+-+--", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Bl\u00e4tter.", "tokens": ["Bl\u00e4t\u00b7ter", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Spat einst welkend, verstreu welkend den s\u00fcs-", "tokens": ["Spat", "einst", "wel\u00b7kend", ",", "ver\u00b7streu", "wel\u00b7kend", "den", "s\u00fcs"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVPP", "$,", "ADJD", "VVPP", "ART", "TRUNC"], "meter": "+-+--+---+", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Sch\u00f6n und bedeutend verwallt der Blum' unschul-", "tokens": ["Sch\u00f6n", "und", "be\u00b7deu\u00b7tend", "ver\u00b7wallt", "der", "Blum'", "un\u00b7schul"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "VVFIN", "ART", "NN", "TRUNC"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Friedlich durchbl\u00fcht sie des Seyns freundlich", "tokens": ["Fried\u00b7lich", "durch\u00b7bl\u00fcht", "sie", "des", "Seyns", "freund\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "ADJD"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "beschlossenen Kreis,", "tokens": ["be\u00b7schlos\u00b7se\u00b7nen", "Kreis", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Giebt nach verstr\u00f6mtem Duft und verstreutem Saa-", "tokens": ["Giebt", "nach", "ver\u00b7str\u00f6m\u00b7tem", "Duft", "und", "ver\u00b7streu\u00b7tem", "Saa"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "KON", "ADJA", "TRUNC"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.12": {"text": "Farb' und Gestalt, den Stoff ruhig der Tellus", "tokens": ["Fa\u00b7rb'", "und", "Ge\u00b7stalt", ",", "den", "Stoff", "ru\u00b7hig", "der", "Tel\u00b7lus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "ART", "NN", "ADJD", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "zur\u00fcck.", "tokens": ["zu\u00b7r\u00fcck", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}}}}}