{"dta.poem.8785": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Vi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Buntfarbig sonnt sich die Phal\u00e4ne nicht;", "tokens": ["Bunt\u00b7far\u00b7big", "sonnt", "sich", "die", "Pha\u00b7l\u00e4\u00b7ne", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Schwan befurcht mit stolzem Hals den See,", "tokens": ["Der", "Schwan", "be\u00b7furcht", "mit", "stol\u00b7zem", "Hals", "den", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch hoch im Aether hausen Schw\u00e4ne nicht;", "tokens": ["Doch", "hoch", "im", "A\u00b7e\u00b7ther", "hau\u00b7sen", "Schw\u00e4\u00b7ne", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "NN", "PTKNEG", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Rieselquelle murmelt angenehm,", "tokens": ["Die", "Rie\u00b7sel\u00b7quel\u00b7le", "mur\u00b7melt", "an\u00b7ge\u00b7nehm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch Schiffe tr\u00e4gt sie nicht und K\u00e4hne nicht;", "tokens": ["Doch", "Schif\u00b7fe", "tr\u00e4gt", "sie", "nicht", "und", "K\u00e4h\u00b7ne", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKNEG", "KON", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "An Dauer weicht die Rose dem Rubin,", "tokens": ["An", "Dau\u00b7er", "weicht", "die", "Ro\u00b7se", "dem", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ART", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ihn aber schm\u00fcckt des Thaues Thr\u00e4ne nicht;", "tokens": ["Ihn", "a\u00b7ber", "schm\u00fcckt", "des", "Thau\u00b7es", "Thr\u00e4\u00b7ne", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Was suchst du mehr, als was du bist, zu seyn,", "tokens": ["Was", "suchst", "du", "mehr", ",", "als", "was", "du", "bist", ",", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PRELS", "PPER", "VAFIN", "$,", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein andres je zu werden, w\u00e4hne nicht!", "tokens": ["Ein", "and\u00b7res", "je", "zu", "wer\u00b7den", ",", "w\u00e4h\u00b7ne", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "PTKZU", "VAINF", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}