{"textgrid.poem.31641": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "13. Gott ist ein rechter Richter", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob schon der Gerechte leidet / und der Gottlo\u00df Vnbill \u00fcbt;", "tokens": ["Ob", "schon", "der", "Ge\u00b7rech\u00b7te", "lei\u00b7det", "/", "und", "der", "Gott\u00b7lo\u00df", "Vn\u00b7bill", "\u00fcbt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$(", "KON", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "wann schon B\u00f6\u00dfe sing-und springen / und die Frommen sind betr\u00fcbt:", "tokens": ["wann", "schon", "B\u00f6\u00b7\u00dfe", "sing\u00b7\u00b7und", "sprin\u00b7gen", "/", "und", "die", "From\u00b7men", "sind", "be\u00b7tr\u00fcbt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "ADJD", "VVINF", "$(", "KON", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "bleibt doch gleichwol Gott gerecht / der es allzeit pflegt zu schicken /", "tokens": ["bleibt", "doch", "gleich\u00b7wol", "Gott", "ge\u00b7recht", "/", "der", "es", "all\u00b7zeit", "pflegt", "zu", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "NN", "ADJD", "$(", "PRELS", "PPER", "ADV", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "da\u00df der B\u00f6sen T\u00fcck und Trutz / Fromme mu\u00df z