{"dta.poem.6925": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein, spricht mancher hier:", "tokens": ["Al\u00b7lein", ",", "spricht", "man\u00b7cher", "hier", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So hat ja dann die Welt, nebst der Materie", "tokens": ["So", "hat", "ja", "dann", "die", "Welt", ",", "nebst", "der", "Ma\u00b7te\u00b7rie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nichts w\u00fcrckliches in ihr,", "tokens": ["Nichts", "w\u00fcrck\u00b7li\u00b7ches", "in", "ihr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der SCHOEPFFER \u00e4fft mich nur, was ich bewundere,", "tokens": ["Der", "ScHOE\u00b7PF\u00b7FER", "\u00e4fft", "mich", "nur", ",", "was", "ich", "be\u00b7wun\u00b7de\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Ist blo\u00df ein Hauffen Staub. Es ist die Welt gemacht", "tokens": ["Ist", "blo\u00df", "ein", "Hauf\u00b7fen", "Staub", ".", "Es", "ist", "die", "Welt", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Aus nichts als tr\u00fcgerisch- und eitlen Phantaseyen.", "tokens": ["Aus", "nichts", "als", "tr\u00fc\u00b7ge\u00b7risch", "und", "eit\u00b7len", "Phan\u00b7ta\u00b7se\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KOKOM", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}