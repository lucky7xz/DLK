{"dta.poem.7996": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der blinde K\u00f6nig .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Was steht der nord\u2019schen Fechter Schaar", "tokens": ["Was", "steht", "der", "nord'\u00b7schen", "Fech\u00b7ter", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hoch auf des Meeres Bord?", "tokens": ["Hoch", "auf", "des", "Mee\u00b7res", "Bord", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was will in seinem grauen Haar", "tokens": ["Was", "will", "in", "sei\u00b7nem", "grau\u00b7en", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der blinde K\u00f6nig dort?", "tokens": ["Der", "blin\u00b7de", "K\u00f6\u00b7nig", "dort", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er ruft, in bittrem Harme", "tokens": ["Er", "ruft", ",", "in", "bitt\u00b7rem", "Har\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf seinen Stab gelehnt,", "tokens": ["Auf", "sei\u00b7nen", "Stab", "ge\u00b7lehnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df \u00fcber\u2019m Meeresarme", "tokens": ["Da\u00df", "\u00fc\u00b7ber'm", "Mee\u00b7res\u00b7ar\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Eiland wiedert\u00f6nt:", "tokens": ["Das", "Ei\u00b7land", "wie\u00b7der\u00b7t\u00f6nt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201egib, R\u00e4uber, aus dem Felsverlie\u00df", "tokens": ["\u201e", "gib", ",", "R\u00e4u\u00b7ber", ",", "aus", "dem", "Fels\u00b7ver\u00b7lie\u00df"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVIMP", "$,", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Tochter mir zur\u00fcck!", "tokens": ["Die", "Toch\u00b7ter", "mir", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Harfenspiel, ihr Lied, so s\u00fc\u00df,", "tokens": ["Ihr", "Har\u00b7fen\u00b7spiel", ",", "ihr", "Lied", ",", "so", "s\u00fc\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "War meines Alters Gl\u00fcck.", "tokens": ["War", "mei\u00b7nes", "Al\u00b7ters", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vom Tanz auf gr\u00fcnem Strande", "tokens": ["Vom", "Tanz", "auf", "gr\u00fc\u00b7nem", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hast du sie weggeraubt,", "tokens": ["Hast", "du", "sie", "weg\u00b7ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dir ist es ewig Schande,", "tokens": ["Dir", "ist", "es", "e\u00b7wig", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mir beugt\u2019s das graue Haupt.\u201c", "tokens": ["Mir", "beugt's", "das", "grau\u00b7e", "Haupt", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da tritt aus seiner Kluft hervor", "tokens": ["Da", "tritt", "aus", "sei\u00b7ner", "Kluft", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der R\u00e4uber, gro\u00df und wild,", "tokens": ["Der", "R\u00e4u\u00b7ber", ",", "gro\u00df", "und", "wild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er schwingt sein H\u00fcnenschwerdt empor", "tokens": ["Er", "schwingt", "sein", "H\u00fc\u00b7nen\u00b7schwerdt", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schl\u00e4gt an seinen Schild:", "tokens": ["Und", "schl\u00e4gt", "an", "sei\u00b7nen", "Schild", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201edu hast ja viele W\u00e4chter,", "tokens": ["\u201e", "du", "hast", "ja", "vie\u00b7le", "W\u00e4ch\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Warum denn litten\u2019s die?", "tokens": ["Wa\u00b7rum", "denn", "lit\u00b7ten's", "die", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.7": {"text": "Dir dient so mancher Fechter,", "tokens": ["Dir", "dient", "so", "man\u00b7cher", "Fech\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und keiner k\u00e4mpft um Sie?\u201c", "tokens": ["Und", "kei\u00b7ner", "k\u00e4mpft", "um", "Sie", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Noch stehn die Fechter alle stumm,", "tokens": ["Noch", "stehn", "die", "Fech\u00b7ter", "al\u00b7le", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIAT", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tritt keiner aus dem Reihn,", "tokens": ["Tritt", "kei\u00b7ner", "aus", "dem", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der blinde K\u00f6nig kehrt sich um:", "tokens": ["Der", "blin\u00b7de", "K\u00f6\u00b7nig", "kehrt", "sich", "um", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ebin ich denn ganz allein?\u201c", "tokens": ["\u201e", "bin", "ich", "denn", "ganz", "al\u00b7lein", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "PPER", "ADV", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da fa\u00dft des Vaters Rechte", "tokens": ["Da", "fa\u00dft", "des", "Va\u00b7ters", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sein junger Sohn so warm:", "tokens": ["Sein", "jun\u00b7ger", "Sohn", "so", "warm", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u201everg\u00f6nn mir\u2019s, da\u00df ich fechte!", "tokens": ["\u201e", "ver\u00b7g\u00f6nn", "mir's", ",", "da\u00df", "ich", "fech\u00b7te", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NE", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wohl f\u00fchl\u2019 ich Kraft im Arm.\u201c", "tokens": ["Wohl", "f\u00fchl'", "ich", "Kraft", "im", "Arm", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u201eo Sohn! der Feind ist riesenstark,", "tokens": ["\u201e", "o", "Sohn", "!", "der", "Feind", "ist", "rie\u00b7sen\u00b7stark", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihm hielt noch Keiner Stand.", "tokens": ["Ihm", "hielt", "noch", "Kei\u00b7ner", "Stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und doch! in dir ist edles Mark,", "tokens": ["Und", "doch", "!", "in", "dir", "ist", "ed\u00b7les", "Mark", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "APPR", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich f\u00fchl\u2019s am Druck der Hand.", "tokens": ["Ich", "f\u00fchl's", "am", "Druck", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nimm hier die alte Klinge!", "tokens": ["Nimm", "hier", "die", "al\u00b7te", "Klin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie ist der Skalden Preis.", "tokens": ["Sie", "ist", "der", "Skal\u00b7den", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und f\u00e4llst du, so verschlinge", "tokens": ["Und", "f\u00e4llst", "du", ",", "so", "ver\u00b7schlin\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Flut mich armen Greis!\u201c", "tokens": ["Die", "Flut", "mich", "ar\u00b7men", "Greis", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und horch! es sch\u00e4umet und es rauscht", "tokens": ["Und", "horch", "!", "es", "sch\u00e4u\u00b7met", "und", "es", "rauscht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "PPER", "VVFIN", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Nachen \u00fcber\u2019s Meer.", "tokens": ["Der", "Na\u00b7chen", "\u00fc\u00b7ber's", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der blinde K\u00f6nig steht und lauscht,", "tokens": ["Der", "blin\u00b7de", "K\u00f6\u00b7nig", "steht", "und", "lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Alles schweigt umher;", "tokens": ["Und", "Al\u00b7les", "schweigt", "um\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bis dr\u00fcben sich erhoben", "tokens": ["Bis", "dr\u00fc\u00b7ben", "sich", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Schild\u2019 und Schwerdter Schall,", "tokens": ["Der", "Schild'", "und", "Schwerd\u00b7ter", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Kampfgeschrei und Toben,", "tokens": ["Und", "Kampf\u00b7ge\u00b7schrei", "und", "To\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und dumpfer Wiederhall.", "tokens": ["Und", "dum\u00b7pfer", "Wie\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da ruft der Greis so freudig bang:", "tokens": ["Da", "ruft", "der", "Greis", "so", "freu\u00b7dig", "bang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201esagt an, was ihr erschaut!", "tokens": ["\u201e", "sagt", "an", ",", "was", "ihr", "er\u00b7schaut", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Schwerdt, ich kenn\u2019s am guten Klang,", "tokens": ["Mein", "Schwerdt", ",", "ich", "kenn's", "am", "gu\u00b7ten", "Klang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es gab so scharfen Laut.\u201c", "tokens": ["Es", "gab", "so", "schar\u00b7fen", "Laut", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "APPR", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201eder R\u00e4uber ist gefallen,", "tokens": ["\u201e", "der", "R\u00e4u\u00b7ber", "ist", "ge\u00b7fal\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er hat den blut\u2019gen Lohn.", "tokens": ["Er", "hat", "den", "blut'\u00b7gen", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Heil dir, du Held vor allen,", "tokens": ["Heil", "dir", ",", "du", "Held", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "NN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du starker K\u00f6nigssohn!\u201c", "tokens": ["Du", "star\u00b7ker", "K\u00f6\u00b7nigs\u00b7sohn", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und wieder wird es still umher,", "tokens": ["Und", "wie\u00b7der", "wird", "es", "still", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der K\u00f6nig steht und lauscht:", "tokens": ["Der", "K\u00f6\u00b7nig", "steht", "und", "lauscht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201ewas h\u00f6r\u2019 ich kommen \u00fcber\u2019s Meer?", "tokens": ["\u201e", "was", "h\u00f6r'", "ich"