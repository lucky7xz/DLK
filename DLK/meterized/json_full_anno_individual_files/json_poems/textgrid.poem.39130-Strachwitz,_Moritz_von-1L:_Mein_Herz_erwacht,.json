{"textgrid.poem.39130": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Herz erwacht,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz erwacht,", "tokens": ["Mein", "Herz", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Es schl\u00e4gt mit Macht,", "tokens": ["Es", "schl\u00e4gt", "mit", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mein Arm ist fest und sehnig;", "tokens": ["Mein", "Arm", "ist", "fest", "und", "seh\u00b7nig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Liebesfehde, die Harfenschlacht,", "tokens": ["Die", "Lie\u00b7bes\u00b7feh\u00b7de", ",", "die", "Har\u00b7fen\u00b7schlacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sie sind mir viel zu wenig;", "tokens": ["Sie", "sind", "mir", "viel", "zu", "we\u00b7nig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich habe gezecht", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7zecht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Im Bechergefecht,", "tokens": ["Im", "Be\u00b7cher\u00b7ge\u00b7fecht", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Mag nicht mehr schw\u00e4rmen und zechen;", "tokens": ["Mag", "nicht", "mehr", "schw\u00e4r\u00b7men", "und", "ze\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADV", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Und wenn ihr Flaschenh\u00e4lse zerbrecht,", "tokens": ["Und", "wenn", "ihr", "Fla\u00b7schen\u00b7h\u00e4l\u00b7se", "zer\u00b7brecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "M\u00f6cht' ich andere H\u00e4lse brechen.", "tokens": ["M\u00f6cht'", "ich", "an\u00b7de\u00b7re", "H\u00e4l\u00b7se", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Aus des M\u00e4dchens Scho\u00df,", "tokens": ["Aus", "des", "M\u00e4d\u00b7chens", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.2": {"text": "Da ringt euch los", "tokens": ["Da", "ringt", "euch", "los"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und zerdr\u00fccket die letzte Tr\u00e4ne;", "tokens": ["Und", "zer\u00b7dr\u00fc\u00b7cket", "die", "letz\u00b7te", "Tr\u00e4\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "lin