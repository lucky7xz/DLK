{"dta.poem.2098": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Biber.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo eins von allen andern Thieren den Menschen was", "tokens": ["Wo", "eins", "von", "al\u00b7len", "an\u00b7dern", "Thie\u00b7ren", "den", "Men\u00b7schen", "was"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "APPR", "PIAT", "ADJA", "NN", "ART", "NN", "PWS"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Betr\u00e4chtlichs zeiget,", "tokens": ["Be\u00b7tr\u00e4cht\u00b7lichs", "zei\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "So ist es die\u00df besondre Thier; indem, was man an ihm", "tokens": ["So", "ist", "es", "die\u00df", "be\u00b7sond\u00b7re", "Thier", ";", "in\u00b7dem", ",", "was", "man", "an", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "ADJA", "NN", "$.", "KOUS", "$,", "PRELS", "PIS", "APPR", "PPER"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "erblickt,", "tokens": ["er\u00b7blickt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Fast alles das, was Thierisch hei\u00dft, in seinem Bauen", "tokens": ["Fast", "al\u00b7les", "das", ",", "was", "Thie\u00b7risch", "hei\u00dft", ",", "in", "sei\u00b7nem", "Bau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIS", "PDS", "$,", "PWS", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u00fcbersteiget,", "tokens": ["\u00fc\u00b7bers\u00b7tei\u00b7get", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Da solches nicht allein von ihm bewundernsw\u00fcrdig zuge-", "tokens": ["Da", "sol\u00b7ches", "nicht", "al\u00b7lein", "von", "ihm", "be\u00b7wun\u00b7derns\u00b7w\u00fcr\u00b7dig", "zu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PTKNEG", "ADV", "APPR", "PPER", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "schickt,", "tokens": ["schickt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Da es die gr\u00f6\u00dften B\u00e4ume f\u00e4llt, das Holz in richt\u2019ge", "tokens": ["Da", "es", "die", "gr\u00f6\u00df\u00b7ten", "B\u00e4u\u00b7me", "f\u00e4llt", ",", "das", "Holz", "in", "richt'\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,", "ART", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "St\u00fccke theilet,", "tokens": ["St\u00fc\u00b7cke", "thei\u00b7let", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Sie auf die breite Schw\u00e4nze legt, mit ihnen nach der", "tokens": ["Sie", "auf", "die", "brei\u00b7te", "Schw\u00e4n\u00b7ze", "legt", ",", "mit", "ih\u00b7nen", "nach", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,", "APPR", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wohnung eilet,", "tokens": ["Woh\u00b7nung", "ei\u00b7let", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Sie k\u00fcnstlich, regelm\u00e4\u00dfig f\u00fcgt; ja, da\u00df die Flut sie", "tokens": ["Sie", "k\u00fcnst\u00b7lich", ",", "re\u00b7gel\u00b7m\u00e4\u00b7\u00dfig", "f\u00fcgt", ";", "ja", ",", "da\u00df", "die", "Flut", "sie"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "$,", "ADJD", "VVFIN", "$.", "PTKANT", "$,", "KOUS", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "nicht verschwemmet,", "tokens": ["nicht", "ver\u00b7schwem\u00b7met", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Mit gro\u00dfer Vorsicht, M\u00fch und Flei\u00df oft einen ganzen", "tokens": ["Mit", "gro\u00b7\u00dfer", "Vor\u00b7sicht", ",", "M\u00fch", "und", "Flei\u00df", "oft", "ei\u00b7nen", "gan\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Flu\u00df verd\u00e4mmet.", "tokens": ["Flu\u00df", "ver\u00b7d\u00e4m\u00b7met", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Sie theilen ihre Wohnung selbst in unterschiedne Stock-", "tokens": ["Sie", "thei\u00b7len", "ih\u00b7re", "Woh\u00b7nung", "selbst", "in", "un\u00b7ter\u00b7schied\u00b7ne", "Stock"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.18": {"text": "werk ein,", "tokens": ["werk", "ein", ","], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Damit sie in dem obersten, bey feuchten Zeiten, sicher", "tokens": ["Da\u00b7mit", "sie", "in", "dem", "o\u00b7bers\u00b7ten", ",", "bey", "feuch\u00b7ten", "Zei\u00b7ten", ",", "si\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "$,", "APPR", "ADJA", "NN", "$,", "ADJA"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "seyn.", "tokens": ["seyn", "."], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Verwunderlich ist die Geduld, da, wenn sie sich, ge-", "tokens": ["Ver\u00b7wun\u00b7der\u00b7lich", "ist", "die", "Ge\u00b7duld", ",", "da", ",", "wenn", "sie", "sich", ",", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "KOUS", "$,", "KOUS", "PPER", "PRF", "$,", "TRUNC"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.22": {"text": "scheucht, verstecken,", "tokens": ["scheucht", ",", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.23": {"text": "Sie sich in zwey bis dreyen Tagen nicht wieder aus der", "tokens": ["Sie", "sich", "in", "zwey", "bis", "drey\u00b7en", "Ta\u00b7gen", "nicht", "wie\u00b7der", "aus", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PRF", "APPR", "CARD", "APPR", "CARD", "NN", "PTKNEG", "ADV", "APPR", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Flut entdecken.", "tokens": ["Flut", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.25": {"text": "Die Wilden, welche diesen Vorthel denselbigen nun ab-", "tokens": ["Die", "Wil\u00b7den", ",", "wel\u00b7che", "die\u00b7sen", "Vor\u00b7thel", "den\u00b7sel\u00b7bi\u00b7gen", "nun", "ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PDAT", "NN", "VVFIN", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.26": {"text": "gemerkt,", "tokens": ["ge\u00b7merkt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.27": {"text": "Sind durch die\u00df Beyspiel der Geduld, in einer Art Ge-", "tokens": ["Sind", "durch", "die\u00df", "Bey\u00b7spiel", "der", "Ge\u00b7duld", ",", "in", "ei\u00b7ner", "Art", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PDS", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "duld gest\u00e4rkt,", "tokens": ["duld", "ge\u00b7st\u00e4rkt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "Da sie sich f\u00fcr geschimpfet halten, von Bibern sich be-", "tokens": ["Da", "sie", "sich", "f\u00fcr", "ge\u00b7schimp\u00b7fet", "hal\u00b7ten", ",", "von", "Bi\u00b7bern", "sich", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "VVPP", "VVINF", "$,", "APPR", "NN", "PRF", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "siegt zu sehn:", "tokens": ["siegt", "zu", "sehn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}