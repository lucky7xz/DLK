{"dta.poem.21646": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Riesenpr\u00fcgelei.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Er stand vor einer Schmiede tief im Walde", "tokens": ["Er", "stand", "vor", "ei\u00b7ner", "Schmie\u00b7de", "tief", "im", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schaute durch die ru\u00df\u2019ge Fensterh\u00f6hle,", "tokens": ["Und", "schau\u00b7te", "durch", "die", "ru\u00df'\u00b7ge", "Fens\u00b7ter\u00b7h\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo drinnen riesige Gestalten regsam", "tokens": ["Wo", "drin\u00b7nen", "rie\u00b7si\u00b7ge", "Ge\u00b7stal\u00b7ten", "reg\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit langen Haken sch\u00fcrten wilde Gluthen,", "tokens": ["Mit", "lan\u00b7gen", "Ha\u00b7ken", "sch\u00fcr\u00b7ten", "wil\u00b7de", "Glu\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wo fauchend in die Flamme fuhr der Sturmwind,", "tokens": ["Wo", "fau\u00b7chend", "in", "die", "Flam\u00b7me", "fuhr", "der", "Sturm\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der aus dem Blasebalg gewaltig schnob,", "tokens": ["Der", "aus", "dem", "Bla\u00b7se\u00b7balg", "ge\u00b7wal\u00b7tig", "schnob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wo gl\u00fchend Eisen schleppend trug ein Riese", "tokens": ["Wo", "gl\u00fc\u00b7hend", "Ei\u00b7sen", "schlep\u00b7pend", "trug", "ein", "Rie\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "NN", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und auf den Ambos legte und der Andre", "tokens": ["Und", "auf", "den", "Am\u00b7bos", "leg\u00b7te", "und", "der", "And\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Den wucht\u2019gen Hammer in die H\u00f6he schwang", "tokens": ["Den", "wucht'\u00b7gen", "Ham\u00b7mer", "in", "die", "H\u00f6\u00b7he", "schwang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und seine Sehnen kraftvoll dehnend reckte", "tokens": ["Und", "sei\u00b7ne", "Seh\u00b7nen", "kraft\u00b7voll", "deh\u00b7nend", "reck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und klingend, schmetternd auf das spr\u00fchende Eisen", "tokens": ["Und", "klin\u00b7gend", ",", "schmet\u00b7ternd", "auf", "das", "spr\u00fc\u00b7hen\u00b7de", "Ei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "$,", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Den Hammer warf, inde\u00df der Flamme Gl\u00fchen", "tokens": ["Den", "Ham\u00b7mer", "warf", ",", "in\u00b7de\u00df", "der", "Flam\u00b7me", "Gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Des Mannes Antlitz purpurroth umschien.", "tokens": ["Des", "Man\u00b7nes", "Ant\u00b7litz", "pur\u00b7pur\u00b7roth", "um\u00b7schien", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Da staunt er ob der kr\u00e4ftigen Gesellen", "tokens": ["Da", "staunt", "er", "ob", "der", "kr\u00e4f\u00b7ti\u00b7gen", "Ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und m\u00e4chtig fa\u00dfte ihn der Anblick an,", "tokens": ["Und", "m\u00e4ch\u00b7tig", "fa\u00df\u00b7te", "ihn", "der", "An\u00b7blick", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Der riesenhafte und vulkanisch-sch\u00f6ne.", "tokens": ["Der", "rie\u00b7sen\u00b7haf\u00b7te", "und", "vul\u00b7ka\u00b7nischsch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Da trat ein schwarzer Kerl her aus der Schmiede", "tokens": ["Da", "trat", "ein", "schwar\u00b7zer", "Kerl", "her", "aus", "der", "Schmie\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu ihm, der fuhr ihn h\u00f6hnisch lachend an:", "tokens": ["Zu", "ihm", ",", "der", "fuhr", "ihn", "h\u00f6h\u00b7nisch", "la\u00b7chend", "an", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u201ewas gaffst du hier, du Milchgesicht, gewaschnes?!", "tokens": ["\u201e", "was", "gaffst", "du", "hier", ",", "du", "Milch\u00b7ge\u00b7sicht", ",", "ge\u00b7waschnes", "?!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "$,", "PPER", "NN", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kannst du den Hammer schwingen? Was? Und nieder", "tokens": ["Kannst", "du", "den", "Ham\u00b7mer", "schwin\u00b7gen", "?", "Was", "?", "Und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$.", "PWS", "$.", "KON", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Ambos schmettern, da\u00df der Boden raucht?", "tokens": ["Den", "Am\u00b7bos", "schmet\u00b7tern", ",", "da\u00df", "der", "Bo\u00b7den", "raucht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Heb dich hinweg, sonst haucht der Blasebalg", "tokens": ["Heb", "dich", "hin\u00b7weg", ",", "sonst", "haucht", "der", "Bla\u00b7se\u00b7balg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dich wie ein St\u00fcckchen Wolle aus dem Wege!\u201c", "tokens": ["Dich", "wie", "ein", "St\u00fcck\u00b7chen", "Wol\u00b7le", "aus", "dem", "We\u00b7ge", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "KOKOM", "ART", "NN", "NE", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und sprach\u2019s und packte zu und warf ihn nieder", "tokens": ["Und", "sprach's", "und", "pack\u00b7te", "zu", "und", "warf", "ihn", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und pr\u00fcgelt ihn nach Leibeskr\u00e4ften durch.", "tokens": ["Und", "pr\u00fc\u00b7gelt", "ihn", "nach", "Lei\u00b7bes\u00b7kr\u00e4f\u00b7ten", "durch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Es traten die Gesellen in das Thor hin", "tokens": ["Es", "tra\u00b7ten", "die", "Ge\u00b7sel\u00b7len", "in", "das", "Thor", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und lachten vor Vergn\u00fcgen, da\u00df die Sterne", "tokens": ["Und", "lach\u00b7ten", "vor", "Ver\u00b7gn\u00fc\u00b7gen", ",", "da\u00df", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Vom Dr\u00f6hnen der Ersch\u00fctt\u2019rung zitternd bebten", "tokens": ["Vom", "Dr\u00f6h\u00b7nen", "der", "Er\u00b7sch\u00fctt'\u00b7rung", "zit\u00b7ternd", "beb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und fast vor Schreck herabgefallen w\u00e4ren.", "tokens": ["Und", "fast", "vor", "Schreck", "her\u00b7ab\u00b7ge\u00b7fal\u00b7len", "w\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sternschnuppen fielen wie die Pr\u00fcgel nieder,", "tokens": ["Stern\u00b7schnup\u00b7pen", "fie\u00b7len", "wie", "die", "Pr\u00fc\u00b7gel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOKOM", "ART", "NN", "PTKVZ", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.15": {"text": "Der Mond verzog sein breites Angesicht", "tokens": ["Der", "Mond", "ver\u00b7zog", "sein", "brei\u00b7tes", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und lachte, da\u00df die heitren Thr\u00e4nen nieder", "tokens": ["Und", "lach\u00b7te", ",", "da\u00df", "die", "hei\u00b7tren", "Thr\u00e4\u00b7nen", "nie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "In wei\u00dfem Schimmer auf die B\u00e4ume flossen", "tokens": ["In", "wei\u00b7\u00dfem", "Schim\u00b7mer", "auf", "die", "B\u00e4u\u00b7me", "flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und in den Teich in hellen Tropfen fielen.", "tokens": ["Und", "in", "den", "Teich", "in", "hel\u00b7len", "Trop\u00b7fen", "fie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Es l\u00e4chelte der Teich sogar in Wellen", "tokens": ["Es", "l\u00e4\u00b7chel\u00b7te", "der", "Teich", "so\u00b7gar", "in", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und sah den Mond verst\u00e4ndni\u00dfinnig an", "tokens": ["Und", "sah", "den", "Mond", "ver\u00b7st\u00e4nd\u00b7ni\u00b7\u00dfin\u00b7nig", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Und schluckte vor Vergn\u00fcgen all\u2019 die Thr\u00e4nen,", "tokens": ["Und", "schluck\u00b7te", "vor", "Ver\u00b7gn\u00fc\u00b7gen", "all'", "die", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Die von dem Mond in wei\u00dfen Lichtern fielen.", "tokens": ["Die", "von", "dem", "Mond", "in", "wei\u00b7\u00dfen", "Lich\u00b7tern", "fie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Und Alles lachte auf der weiten Erde,", "tokens": ["Und", "Al\u00b7les", "lach\u00b7te", "auf", "der", "wei\u00b7ten", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Bis die Gesellen pr\u00fcgelsatt ihn lie\u00dfen;", "tokens": ["Bis", "die", "Ge\u00b7sel\u00b7len", "pr\u00fc\u00b7gel\u00b7satt", "ihn", "lie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Sie schritten wieder in die hei\u00dfe Schmiede", "tokens": ["Sie", "schrit\u00b7ten", "wie\u00b7der", "in", "die", "hei\u00b7\u00dfe", "Schmie\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Und sch\u00fcrten lachend neue Gluthen auf.", "tokens": ["Und", "sch\u00fcr\u00b7ten", "la\u00b7chend", "neu\u00b7e", "Glu\u00b7then", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"t