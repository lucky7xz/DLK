{"dta.poem.20894": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Und da\u00df er mich macht alles Zweiffels frey/", "tokens": ["Und", "da\u00df", "er", "mich", "macht", "al\u00b7les", "Zweif\u00b7fels", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVFIN", "PIAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Warum der Erd ist Kugel Art gegeben/", "tokens": ["Wa\u00b7rum", "der", "Erd", "ist", "Ku\u00b7gel", "Art", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ob denn di\u00df die beste Stelle sey/", "tokens": ["Und", "ob", "denn", "di\u00df", "die", "bes\u00b7te", "Stel\u00b7le", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PDS", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df sie also mu\u00df in den L\u00fcfften schweben.", "tokens": ["Da\u00df", "sie", "al\u00b7so", "mu\u00df", "in", "den", "L\u00fcff\u00b7ten", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-++-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ich w\u00fcnschte wol/ da\u00df er mir kund gethan/", "tokens": ["Ich", "w\u00fcnschte", "wol", "/", "da\u00df", "er", "mir", "kund", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "KOUS", "PPER", "PPER", "PTKVZ", "VVPP", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Warum der Mond itzt scheinet/ itzt vergehet/", "tokens": ["Wa\u00b7rum", "der", "Mond", "itzt", "schei\u00b7net", "/", "itzt", "ver\u00b7ge\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$(", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Warum der Tag auch hier sich zeigen kan;", "tokens": ["Wa\u00b7rum", "der", "Tag", "auch", "hier", "sich", "zei\u00b7gen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADV", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wie das Gel\u00fcck noch unterm Himmel stehet/", "tokens": ["Wie", "das", "Ge\u00b7l\u00fcck", "noch", "un\u00b7term", "Him\u00b7mel", "ste\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wie auch/ warum der Sternen g\u00f6ldne Pracht/", "tokens": ["Wie", "auch", "/", "wa\u00b7rum", "der", "Ster\u00b7nen", "g\u00f6ld\u00b7ne", "Pracht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$(", "PWAV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So in der Lufft bald her bald wiederstreichet/", "tokens": ["So", "in", "der", "Lufft", "bald", "her", "bald", "wie\u00b7der\u00b7strei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So wunder sch\u00f6n und artig ist gemacht/", "tokens": ["So", "wun\u00b7der", "sch\u00f6n", "und", "ar\u00b7tig", "ist", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "KON", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und wie man sie mit dem Gesicht erreichet.", "tokens": ["Und", "wie", "man", "sie", "mit", "dem", "Ge\u00b7sicht", "er\u00b7rei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}