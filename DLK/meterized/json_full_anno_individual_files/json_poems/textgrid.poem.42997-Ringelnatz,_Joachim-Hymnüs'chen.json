{"textgrid.poem.42997": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Hymn\u00fcs'chen", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es geh\u00f6rt zu den fr\u00f6hlichen Sachen,", "tokens": ["Es", "ge\u00b7h\u00f6rt", "zu", "den", "fr\u00f6h\u00b7li\u00b7chen", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Sich bei den Schleimh\u00e4uten beliebt zu machen.", "tokens": ["Sich", "bei", "den", "Schleim\u00b7h\u00e4u\u00b7ten", "be\u00b7liebt", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und demjenigen, das so hinein", "tokens": ["Und", "dem\u00b7je\u00b7ni\u00b7gen", ",", "das", "so", "hin\u00b7ein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDS", "$,", "PRELS", "ADV", "APZR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Kommt, m\u00fcssen Schleimh\u00e4ute auch dankbar sein.", "tokens": ["Kommt", ",", "m\u00fcs\u00b7sen", "Schleim\u00b7h\u00e4u\u00b7te", "auch", "dank\u00b7bar", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VMFIN", "NN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Im Reizen und im Befriedigtwerden", "tokens": ["Im", "Rei\u00b7zen", "und", "im", "Be\u00b7frie\u00b7digt\u00b7wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Liegt alles Gl\u00fcck auf Erden.", "tokens": ["Liegt", "al\u00b7les", "Gl\u00fcck", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kriegen Sie aber diese Worte", "tokens": ["Krie\u00b7gen", "Sie", "a\u00b7ber", "die\u00b7se", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "PDAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Bitte nicht in den falschen Hals.", "tokens": ["Bit\u00b7te", "nicht", "in", "den", "fal\u00b7schen", "Hals", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Denn die sind vieldeutig. Diesenfalls", "tokens": ["Denn", "die", "sind", "viel\u00b7deu\u00b7tig", ".", "Die\u00b7sen\u00b7falls"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PDS", "VAFIN", "ADJD", "$.", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Red ich von Tabak jedweder Sorte.", "tokens": ["Red", "ich", "von", "Ta\u00b7bak", "jed\u00b7we\u00b7der", "Sor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Tabak zum Rauchen, Schnupfen und Kauen.", "tokens": ["Ta\u00b7bak", "zum", "Rau\u00b7chen", ",", "Schnup\u00b7fen", "und", "Kau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und da ist nichts wichtiger als", "tokens": ["Und", "da", "ist", "nichts", "wich\u00b7ti\u00b7ger", "als"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PIS", "ADJD", "KOKOM"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Guter Geschmack und gutes Verdauen.", "tokens": ["Gu\u00b7ter", "Ge\u00b7schmack", "und", "gu\u00b7tes", "Ver\u00b7dau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Es geh\u00f6rt zu den fr\u00f6hlichen Sachen,", "tokens": ["Es", "ge\u00b7h\u00f6rt", "zu", "den", "fr\u00f6h\u00b7li\u00b7chen", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Sich bei den Schleimh\u00e4uten beliebt zu machen.", "tokens": ["Sich", "bei", "den", "Schleim\u00b7h\u00e4u\u00b7ten", "be\u00b7liebt", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und demjenigen, das so hinein", "tokens": ["Und", "dem\u00b7je\u00b7ni\u00b7gen", ",", "das", "so", "hin\u00b7ein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDS", "$,", "PRELS", "ADV", "APZR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Kommt, m\u00fcssen Schleimh\u00e4ute auch dankbar sein.", "tokens": ["Kommt", ",", "m\u00fcs\u00b7sen", "Schleim\u00b7h\u00e4u\u00b7te", "auch", "dank\u00b7bar", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "