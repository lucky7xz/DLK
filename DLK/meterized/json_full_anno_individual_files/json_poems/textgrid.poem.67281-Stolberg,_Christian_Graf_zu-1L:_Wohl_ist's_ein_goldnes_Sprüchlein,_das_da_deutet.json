{"textgrid.poem.67281": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohl ist's ein goldnes Spr\u00fcchlein, das da deutet", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl ist's ein goldnes Spr\u00fcchlein, das da deutet", "tokens": ["Wohl", "ist's", "ein", "gold\u00b7nes", "Spr\u00fcch\u00b7lein", ",", "das", "da", "deu\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf jenen Ring, der magisch uns umkreis't,", "tokens": ["Auf", "je\u00b7nen", "Ring", ",", "der", "ma\u00b7gisch", "uns", "um\u00b7kreis't", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch den begr\u00e4nzt, sich enget und sich weitet", "tokens": ["Durch", "den", "be\u00b7gr\u00e4nzt", ",", "sich", "en\u00b7get", "und", "sich", "wei\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "VVPP", "$,", "PRF", "VVFIN", "KON", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein eignes Feld dem Odem und dem Geist;", "tokens": ["Sein", "eig\u00b7nes", "Feld", "dem", "O\u00b7dem", "und", "dem", "Geist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aus dem nicht ungestraft der Fu\u00df uns gleitet,", "tokens": ["Aus", "dem", "nicht", "un\u00b7ge\u00b7straft", "der", "Fu\u00df", "uns", "glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PTKNEG", "ADJD", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Weil dort nur uns Natur den Kranz verhei\u00dft,", "tokens": ["Weil", "dort", "nur", "uns", "Na\u00b7tur", "den", "Kranz", "ver\u00b7hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PPER", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den, wenn wir unser wahres Pfund ergr\u00fcnden,", "tokens": ["Den", ",", "wenn", "wir", "un\u00b7ser", "wah\u00b7res", "Pfund", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wir, reicht die Mus' ihn, um die Schl\u00e4f' uns winden.", "tokens": ["Wir", ",", "reicht", "die", "Mus'", "ihn", ",", "um", "die", "Schl\u00e4f'", "uns", "win\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "ART", "NN", "PPER", "$,", "KOUI", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn ich das goldne Spr\u00fcchlein achtsam h\u00e4tte", "tokens": ["Wenn", "ich", "das", "gold\u00b7ne", "Spr\u00fcch\u00b7lein", "acht\u00b7sam", "h\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewahrt in einem guten, feinen Sinn,", "tokens": ["Be\u00b7wahrt", "in", "ei\u00b7nem", "gu\u00b7ten", ",", "fei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So b\u00fc\u00dft ich jetzt nicht, ginge meiner Wette", "tokens": ["So", "b\u00fc\u00dft", "ich", "jetzt", "nicht", ",", "gin\u00b7ge", "mei\u00b7ner", "Wet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verwirkter Preis, mein Ad'lung nicht dahin;", "tokens": ["Ver\u00b7wirk\u00b7ter", "Preis", ",", "mein", "Ad'\u00b7lung", "nicht", "da\u00b7hin", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "PTKNEG", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mir klebte heimlich fest des D\u00fcnkels Klette,", "tokens": ["Mir", "kleb\u00b7te", "heim\u00b7lich", "fest", "des", "D\u00fcn\u00b7kels", "Klet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gewahr ward ich's und mein ist der Gewinn!", "tokens": ["Ge\u00b7wahr", "ward", "ich's", "und", "mein", "ist", "der", "Ge\u00b7winn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "KON", "PPOSAT", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So lehrt das Spr\u00fcchlein, f\u00fcr uns All' ein Muster:", "tokens": ["So", "lehrt", "das", "Spr\u00fcch\u00b7lein", ",", "f\u00fcr", "uns", "All'", "ein", "Mus\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "APPR", "PPER", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bei deinem Leisten bleibe du, o Schuster!", "tokens": ["Bei", "dei\u00b7nem", "Leis\u00b7ten", "blei\u00b7be", "du", ",", "o", "Schus\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was k\u00fcmmern mich \u2013 war's je denn meine Weise", "tokens": ["Was", "k\u00fcm\u00b7mern", "mich", "\u2013", "wa\u00b7r's", "je", "denn", "mei\u00b7ne", "Wei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$(", "VAFIN", "ADV", "KON", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu angeln nach des Polyhistors Tand? \u2013", "tokens": ["Zu", "an\u00b7geln", "nach", "des", "Po\u00b7ly\u00b7his\u00b7tors", "Tand", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was k\u00fcmmern mich des Rads verschlung'ne Gleise,", "tokens": ["Was", "k\u00fcm\u00b7mern", "mich", "des", "Rads", "ver\u00b7schlung'\u00b7ne", "Glei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was Bahnen, die des Gr\u00fcblers Lamp' erfand?", "tokens": ["Was", "Bah\u00b7nen", ",", "die", "des", "Gr\u00fcb\u00b7lers", "Lamp'", "er\u00b7fand", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "PRELS", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach, schier am Ziel der anspruchlosen Reise,", "tokens": ["Ach", ",", "schier", "am", "Ziel", "der", "an\u00b7spruch\u00b7lo\u00b7sen", "Rei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Lockt mich ein Irrwisch in dies Fremdlingsland!", "tokens": ["Lockt", "mich", "ein", "Irr\u00b7wisch", "in", "dies", "Fremd\u00b7lings\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PDS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch mein ist der Gewinn! \u2013 Mit dir sei Friede", "tokens": ["Doch", "mein", "ist", "der", "Ge\u00b7winn", "!", "\u2013", "Mit", "dir", "sei", "Frie\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "VAFIN", "ART", "NN", "$.", "$(", "APPR", "PPER", "VAFIN", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Gestiftet, lebe wohl nun Ciclo\u00efde!", "tokens": ["Ge\u00b7stif\u00b7tet", ",", "le\u00b7be", "wohl", "nun", "Cic\u00b7lo\u00ef\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und du, sieh' de\u00df nicht scheel, dort mehrst die Menge", "tokens": ["Und", "du", ",", "sieh'", "de\u00df", "nicht", "scheel", ",", "dort", "mehrst", "die", "Men\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "VVIMP", "ART", "PTKNEG", "ADJD", "$,", "ADV", "ADV", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Du deiner Ruhmgenossen, Adelung,", "tokens": ["Du", "dei\u00b7ner", "Ruhm\u00b7ge\u00b7nos\u00b7sen", ",", "A\u00b7de\u00b7lung", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Die meines Freundes Saal in Breit' und L\u00e4nge", "tokens": ["Die", "mei\u00b7nes", "Freun\u00b7des", "Saal", "in", "Breit'", "und", "L\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verherrlichen, durch Wahl, durch Werth und Prunk,", "tokens": ["Ver\u00b7herr\u00b7li\u00b7chen", ",", "durch", "Wahl", ",", "durch", "Werth", "und", "Prunk", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dein harrt ein gutes Pl\u00e4tzchen im Gedr\u00e4nge", "tokens": ["Dein", "harrt", "ein", "gu\u00b7tes", "Pl\u00e4tz\u00b7chen", "im", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von jeder Zung' und Art, von Alt und Jung,", "tokens": ["Von", "je\u00b7der", "Zung'", "und", "Art", ",", "von", "Alt", "und", "Jung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Auch pilgr' ich oft \u2013 dein Herr verg\u00f6nnt's, drum letzen", "tokens": ["Auch", "pil\u00b7gr'", "ich", "oft", "\u2013", "dein", "Herr", "ver\u00b7g\u00f6nnt's", ",", "drum", "let\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "PPOSAT", "NN", "NE", "$,", "PAV", "VVINF"], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Getrost wir uns \u2013 zu deines F\u00fcllhorns Sch\u00e4tzen.", "tokens": ["Ge\u00b7trost", "wir", "uns", "\u2013", "zu", "dei\u00b7nes", "F\u00fcll\u00b7horns", "Sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "$(", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wohl ist's ein goldnes Spr\u00fcchlein, das da deutet", "tokens": ["Wohl", "ist's", "ein", "gold\u00b7nes", "Spr\u00fcch\u00b7lein", ",", "das", "da", "deu\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf jenen Ring, der magisch uns umkreis't,", "tokens": ["Auf", "je\u00b7nen", "Ring", ",", "der", "ma\u00b7gisch", "uns", "um\u00b7kreis't", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch den begr\u00e4nzt, sich enget und sich weitet", "tokens": ["Durch", "de