{"dta.poem.12448": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Von Volksliedern .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Frisch auf, ihr Bursche! wandert mit,", "tokens": ["Frisch", "auf", ",", "ihr", "Bur\u00b7sche", "!", "wan\u00b7dert", "mit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Holt B\u00fcndel und Felleisen,", "tokens": ["Holt", "B\u00fcn\u00b7del", "und", "Fell\u00b7ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch eh wir mit dem lezten Schritt", "tokens": ["Doch", "eh", "wir", "mit", "dem", "lez\u00b7ten", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Stadt den R\u00fccken weisen,", "tokens": ["Der", "Stadt", "den", "R\u00fc\u00b7cken", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schenk M\u00e4dchen uns noch Ku\u00df und Wein,", "tokens": ["Schenk", "M\u00e4d\u00b7chen", "uns", "noch", "Ku\u00df", "und", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drauf mit der Sonn zu reisen.", "tokens": ["Drauf", "mit", "der", "Sonn", "zu", "rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}