{"textgrid.poem.34260": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vom freundlichen Olymp sieht der Allm\u00e4chtge nieder", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom freundlichen Olymp sieht der Allm\u00e4chtge nieder", "tokens": ["Vom", "freund\u00b7li\u00b7chen", "O\u00b7lymp", "sieht", "der", "All\u00b7m\u00e4cht\u00b7ge", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf das von ihm gekn\u00fcpfte Paar. \u2013", "tokens": ["Auf", "das", "von", "ihm", "ge\u00b7kn\u00fcpf\u00b7te", "Paar", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "APPR", "PPER", "ADJA", "NN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die Lust erheitert jetzt die blassen Stirnen wieder,", "tokens": ["Die", "Lust", "er\u00b7hei\u00b7tert", "jetzt", "die", "blas\u00b7sen", "Stir\u00b7nen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In welchen Schmerz und Angst tief eingegraben war.", "tokens": ["In", "wel\u00b7chen", "Schmerz", "und", "Angst", "tief", "ein\u00b7ge\u00b7gra\u00b7ben", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es rang der m\u00e4chtge Tod die Freuden zu verscheuchen,", "tokens": ["Es", "rang", "der", "m\u00e4cht\u00b7ge", "Tod", "die", "Freu\u00b7den", "zu", "ver\u00b7scheu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Gott f\u00fcr dieses Paar beschlo\u00df.", "tokens": ["Die", "Gott", "f\u00fcr", "die\u00b7ses", "Paar", "be\u00b7schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hob den d\u00fcrren Arm und unter seinen Streichen", "tokens": ["Er", "hob", "den", "d\u00fcr\u00b7ren", "Arm", "und", "un\u00b7ter", "sei\u00b7nen", "Strei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sank der Geliebte hin, matt, krank und sinnenlo\u00df.", "tokens": ["Sank", "der", "Ge\u00b7lieb\u00b7te", "hin", ",", "matt", ",", "krank", "und", "sin\u00b7nen\u00b7lo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "ADJD", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Da lag er: um ihn bat die zitternde Geliebte", "tokens": ["Da", "lag", "er", ":", "um", "ihn", "bat", "die", "zit\u00b7tern\u00b7de", "Ge\u00b7lieb\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Oft in durchweinter Mitternacht.", "tokens": ["Oft", "in", "durch\u00b7wein\u00b7ter", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit hei\u00dfem Flehn errang die z\u00e4rtliche Betr\u00fcbte", "tokens": ["Mit", "hei\u00b7\u00dfem", "Flehn", "er\u00b7rang", "die", "z\u00e4rt\u00b7li\u00b7che", "Be\u00b7tr\u00fcb\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Leben ihres Freunds vom Wink der h\u00f6chsten Macht.", "tokens": ["Das", "Le\u00b7ben", "ih\u00b7res", "Freunds", "vom", "Wink", "der", "h\u00f6chs\u00b7ten", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Noch floh das schwarze Heer der drohenden Gefahren", "tokens": ["Noch", "floh", "das", "schwar\u00b7ze", "Heer", "der", "dro\u00b7hen\u00b7den", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht ganz zum feurgen Phul hinab.", "tokens": ["Nicht", "ganz", "zum", "feur\u00b7gen", "Phul", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch einmal wagten sich des Ungl\u00fccks blutge Schaaren", "tokens": ["Noch", "ein\u00b7mal", "wag\u00b7ten", "sich", "des", "Un\u00b7gl\u00fccks", "blut\u00b7ge", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An des Geliebten Haupt und zeigten ihm sein Grab.", "tokens": ["An", "des", "Ge\u00b7lieb\u00b7ten", "Haupt", "und", "zeig\u00b7ten", "ihm", "sein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein Ungeheuer rang mit m\u00f6rderischem Stahle", "tokens": ["Ein", "Un\u00b7ge\u00b7heu\u00b7er", "rang", "mit", "m\u00f6r\u00b7de\u00b7ri\u00b7schem", "Stah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf seinen bangen Busen lo\u00df.", "tokens": ["Auf", "sei\u00b7nen", "ban\u00b7gen", "Bu\u00b7sen", "lo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schwei\u00df flo\u00df vom starken Arm, der wiederholte Male", "tokens": ["Schwei\u00df", "flo\u00df", "vom", "star\u00b7ken", "Arm", ",", "der", "wie\u00b7der\u00b7hol\u00b7te", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Mordstahl heulend schwang. Die Wuth that Sto\u00df auf Sto\u00df.", "tokens": ["Den", "Mord\u00b7stahl", "heu\u00b7lend", "schwang", ".", "Die", "Wuth", "that", "Sto\u00df", "auf", "Sto\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$.", "ART", "NN", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch der Allm\u00e4chtge stand erz\u00fcrnt vor seinem Trone", "tokens": ["Doch", "der", "All\u00b7m\u00e4cht\u00b7ge", "stand", "er\u00b7z\u00fcrnt", "vor", "sei\u00b7nem", "Tro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sah des schwarzen M\u00f6rders Wuth,", "tokens": ["Und", "sah", "des", "schwar\u00b7zen", "M\u00f6r\u00b7ders", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6rt des Verwundten Flehn mit bangem hei\u00dfrem Tone,", "tokens": ["H\u00f6rt", "des", "Ver\u00b7wund\u00b7ten", "Flehn", "mit", "ban\u00b7gem", "hei\u00df\u00b7rem", "To\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVFIN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sah h\u00fclflo\u00df, schwach ihn stehn, bedeckt mit Schwei\u00df und Blut.", "tokens": ["Sah", "h\u00fcl\u00b7flo\u00df", ",", "schwach", "ihn", "stehn", ",", "be\u00b7deckt", "mit", "Schwei\u00df", "und", "Blut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "PPER", "VVINF", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und Gott erh\u00f6rte ihn von Todesangst umgeben: \u2013", "tokens": ["Und", "Gott", "er\u00b7h\u00f6r\u00b7te", "ihn", "von", "To\u00b7de\u00b7sangst", "um\u00b7ge\u00b7ben", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Dolch zerbrach noch ungetr\u00e4nkt:", "tokens": ["Der", "Dolch", "zer\u00b7brach", "noch", "un\u00b7ge\u00b7tr\u00e4nkt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das blitzend scharfe Schwerdt entri\u00df ihm nicht das Leben,", "tokens": ["Das", "blit\u00b7zend", "schar\u00b7fe", "Schwerdt", "ent\u00b7ri\u00df", "ihm", "nicht", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil es die Vorsicht selbst vom Herzen abgelenkt.", "tokens": ["Weil", "es", "die", "Vor\u00b7sicht", "selbst", "vom", "Her\u00b7zen", "ab\u00b7ge\u00b7lenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "\u00bbgenug versucht, genug! sprach Gottes Donnerstimme", "tokens": ["\u00bb", "ge\u00b7nug", "ver\u00b7sucht", ",", "ge\u00b7nug", "!", "sprach", "Got\u00b7tes", "Don\u00b7ner\u00b7stim\u00b7me"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "VVPP", "$,", "ADV", "$.", "VVFIN", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom heiterern Olymp herab:", "tokens": ["Vom", "hei\u00b7te\u00b7rern", "O\u00b7lymp", "her\u00b7ab", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schmerz, Angst und Tod entweicht!\u00ab Und mit ohnm\u00e4chtgen Grimme", "tokens": ["Schmerz", ",", "Angst", "und", "Tod", "ent\u00b7weicht", "!", "\u00ab", "Und", "mit", "ohn\u00b7m\u00e4cht\u00b7gen", "Grim\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$.", "$(", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Entwich der schwarze Tod, der Schrecken fand sein Grab.", "tokens": ["Ent\u00b7wich", "der", "schwar\u00b7ze", "Tod", ",", "der", "Schre\u00b7cken", "fand", "sein", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Und froh Entz\u00fccken fiel auf die Verliebten nieder,", "tokens": ["Und", "froh", "Ent\u00b7z\u00fc\u00b7cken", "fiel", "auf", "die", "Ver\u00b7lieb\u00b7ten", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": 