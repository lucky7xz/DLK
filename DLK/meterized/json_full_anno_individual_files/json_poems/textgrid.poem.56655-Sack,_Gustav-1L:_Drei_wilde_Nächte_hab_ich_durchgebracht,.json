{"textgrid.poem.56655": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Drei wilde N\u00e4chte hab ich durchgebracht,", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei wilde N\u00e4chte hab ich durchgebracht,", "tokens": ["Drei", "wil\u00b7de", "N\u00e4ch\u00b7te", "hab", "ich", "durch\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nun h\u00e4ng ich zitternd in der hohlen Stadt,", "tokens": ["Nun", "h\u00e4ng", "ich", "zit\u00b7ternd", "in", "der", "hoh\u00b7len", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "die alle Lichter schon verloren hat", "tokens": ["die", "al\u00b7le", "Lich\u00b7ter", "schon", "ver\u00b7lo\u00b7ren", "hat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "vor Regengraus und Sturm \u2013 weh! welche Nacht!", "tokens": ["vor", "Re\u00b7gen\u00b7graus", "und", "Sturm", "\u2013", "weh", "!", "wel\u00b7che", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$(", "PTKVZ", "$.", "PWAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "All meine Jahre sind hell aufgewacht", "tokens": ["All", "mei\u00b7ne", "Jah\u00b7re", "sind", "hell", "auf\u00b7ge\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "ADJD", "VVPP"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "und haben mir hei\u00dfhungrig, nimmersatt", "tokens": ["und", "ha\u00b7ben", "mir", "hei\u00df\u00b7hun\u00b7grig", ",", "nim\u00b7mer\u00b7satt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "KOUI"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "mein w\u00fcstes Trinkerleben Blatt f\u00fcr Blatt", "tokens": ["mein", "w\u00fcs\u00b7tes", "Trin\u00b7ker\u00b7le\u00b7ben", "Blatt", "f\u00fcr", "Blatt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "auf einem grellen Filmband vorgelacht.", "tokens": ["auf", "ei\u00b7nem", "grel\u00b7len", "Film\u00b7band", "vor\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie winken mir und grinsen: war's nicht so?", "tokens": ["Sie", "win\u00b7ken", "mir", "und", "grin\u00b7sen", ":", "wa\u00b7r's", "nicht", "so", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "$.", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "umfluten mich und fragen: wei\u00dft du noch?", "tokens": ["um\u00b7flu\u00b7ten", "mich", "und", "fra\u00b7gen", ":", "wei\u00dft", "du", "noch", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVINF", "$.", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "