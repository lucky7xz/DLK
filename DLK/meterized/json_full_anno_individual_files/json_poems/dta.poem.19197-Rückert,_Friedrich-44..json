{"dta.poem.19197": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das menschlichste Gesch\u00e4ft ist Menschen zu erziehn;", "tokens": ["Das", "menschlichs\u00b7te", "Ge\u00b7sch\u00e4ft", "ist", "Men\u00b7schen", "zu", "er\u00b7ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Blumen ziehe, wem nicht Kinder sind verliehn.", "tokens": ["Und", "Blu\u00b7men", "zie\u00b7he", ",", "wem", "nicht", "Kin\u00b7der", "sind", "ver\u00b7liehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "PWS", "PTKNEG", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Blumen Jugend l\u00e4\u00dft vor St\u00fcrmen und Gefahren", "tokens": ["Der", "Blu\u00b7men", "Ju\u00b7gend", "l\u00e4\u00dft", "vor", "St\u00fcr\u00b7men", "und", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich immer leichter als die menschliche bewahren.", "tokens": ["Sich", "im\u00b7mer", "leich\u00b7ter", "als", "die", "menschli\u00b7che", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "KOKOM", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Dankbarer sind sie auch, vom Wiegenrand zur Gruft,", "tokens": ["Dank\u00b7ba\u00b7rer", "sind", "sie", "auch", ",", "vom", "Wie\u00b7gen\u00b7rand", "zur", "Gruft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "$,", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erziehunglohnender mit Farbenspiel und Duft.", "tokens": ["Er\u00b7zie\u00b7hung\u00b7loh\u00b7nen\u00b7der", "mit", "Far\u00b7ben\u00b7spiel", "und", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gern mag mein altes Aug' aus ihren Kinderaugen", "tokens": ["Gern", "mag", "mein", "al\u00b7tes", "Aug'", "aus", "ih\u00b7ren", "Kin\u00b7der\u00b7au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Saugen das Licht, das sie selbst aus der Sonne saugen.", "tokens": ["Sau\u00b7gen", "das", "Licht", ",", "das", "sie", "selbst", "aus", "der", "Son\u00b7ne", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}, "stanza.5": {"line.1": {"text": "Dann saugt sie ihnen aus das eingesogene,", "tokens": ["Dann", "saugt", "sie", "ih\u00b7nen", "aus", "das", "ein\u00b7ge\u00b7so\u00b7ge\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Entflogen ist der Glanz der angeflogene.", "tokens": ["Ent\u00b7flo\u00b7gen", "ist", "der", "Glanz", "der", "an\u00b7ge\u00b7flo\u00b7ge\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.6": {"line.1": {"text": "Die Farben auf der Flucht von Kronen stets zu Kronen;", "tokens": ["Die", "Far\u00b7ben", "auf", "der", "Flucht", "von", "Kro\u00b7nen", "stets", "zu", "Kro\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Trauer ist wo sie fliehn, und Freude wo sie wohnen.", "tokens": ["Trau\u00b7er", "ist", "wo", "sie", "fliehn", ",", "und", "Freu\u00b7de", "wo", "sie", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PWAV", "PPER", "VVINF", "$,", "KON", "NN", "PWAV", "PPER", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Sieh deine Blumen an in deiner Luft und Trauer,", "tokens": ["Sieh", "dei\u00b7ne", "Blu\u00b7men", "an", "in", "dei\u00b7ner", "Luft", "und", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und tr\u00f6ste dich, da\u00df auch an dir ist keine Dauer.", "tokens": ["Und", "tr\u00f6s\u00b7te", "dich", ",", "da\u00df", "auch", "an", "dir", "ist", "kei\u00b7ne", "Dau\u00b7er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "ADV", "APPR", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}