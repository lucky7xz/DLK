{"textgrid.poem.34554": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Meeres Gang ist h\u00f6her heut und lauter auch!", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Meeres Gang ist h\u00f6her heut und lauter auch!", "tokens": ["Des", "Mee\u00b7res", "Gang", "ist", "h\u00f6\u00b7her", "heut", "und", "lau\u00b7ter", "auch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "ADV", "KON", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wohl dem, der hinter W\u00e4llen seines Lebens Arbeit fand", "tokens": ["Wohl", "dem", ",", "der", "hin\u00b7ter", "W\u00e4l\u00b7len", "sei\u00b7nes", "Le\u00b7bens", "Ar\u00b7beit", "fand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "APPR", "NN", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "und sicher steht, gefestet auf ererbtem Grund.", "tokens": ["und", "si\u00b7cher", "steht", ",", "ge\u00b7fes\u00b7tet", "auf", "er\u00b7erb\u00b7tem", "Grund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch reichen Boden, den das Meer vordem gen\u00e4hrt,", "tokens": ["Durch", "rei\u00b7chen", "Bo\u00b7den", ",", "den", "das", "Meer", "vor\u00b7dem", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "auf seinem Boden schreitet er und lenkt den Pflug", "tokens": ["auf", "sei\u00b7nem", "Bo\u00b7den", "schrei\u00b7tet", "er", "und", "lenkt", "den", "Pflug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "in grader Bahn und wendet ihn getrost am Ziel.", "tokens": ["in", "gra\u00b7der", "Bahn", "und", "wen\u00b7det", "ihn", "ge\u00b7trost", "am", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "VVFIN", "PPER", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dann rastet er \u2013 und l\u00e4sst die Blicke schweifen rings,", "tokens": ["Dann", "ras\u00b7tet", "er", "\u2013", "und", "l\u00e4sst", "die", "Bli\u00b7cke", "schwei\u00b7fen", "rings", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KON", "VVFIN", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und sieht um sich in Ruhe wachsen seiner H\u00e4nde Werk.", "tokens": ["und", "sieht", "um", "sich", "in", "Ru\u00b7he", "wach\u00b7sen", "sei\u00b7ner", "H\u00e4n\u00b7de", "Werk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Nur manchmal horcht er wohl hin\u00fcber nach dem weiten Meer,", "tokens": ["Nur", "manch\u00b7mal", "horcht"