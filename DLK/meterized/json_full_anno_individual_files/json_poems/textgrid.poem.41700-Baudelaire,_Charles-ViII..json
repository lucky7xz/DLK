{"textgrid.poem.41700": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "ViII.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tod! Greiser Kapit\u00e4n! Zeit ist zum Ankerlichten!", "tokens": ["Tod", "!", "Grei\u00b7ser", "Ka\u00b7pi\u00b7t\u00e4n", "!", "Zeit", "ist", "zum", "An\u00b7ker\u00b7lich\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "NN", "$.", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dies Land sind m\u00fcde wir. O Tod, in See hinein!", "tokens": ["Dies", "Land", "sind", "m\u00fc\u00b7de", "wir", ".", "O", "Tod", ",", "in", "See", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ADJD", "PPER", "$.", "NE", "NN", "$,", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dr\u00e4un, schwarz wie Tinte, Meer und Luft uns zu vernichten, \u2013", "tokens": ["Dr\u00e4un", ",", "schwarz", "wie", "Tin\u00b7te", ",", "Meer", "und", "Luft", "uns", "zu", "ver\u00b7nich\u00b7ten", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "ADJD", "KOKOM", "NN", "$,", "NN", "KON", "NN", "PPER", "PTKZU", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im Herzen, das du kennst, strahlt doch ein lichter Schein!", "tokens": ["Im", "Her\u00b7zen", ",", "das", "du", "kennst", ",", "strahlt", "doch", "ein", "lich\u00b7ter", "Schein", "!"], "token_info": ["word", "word", "pu