{"dta.poem.2748": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die festgegr\u00fcndete Hoffnung/  \n  Fr. M. v. G. g. G. betrachtet den 20. Junii  \n 1680.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Wohlstand bester Ruh/ das Heil erw\u00fcnschter Zei-\nten/", "tokens": ["Den", "Wohl\u00b7stand", "bes\u00b7ter", "Ruh", "/", "das", "Heil", "er\u00b7w\u00fcnschter", "Zei", "ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "ADJA", "TRUNC", "FM", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Stellt\u2019 uns das kluge Rom blo\u00df durch die Hoffnung", "tokens": ["Stellt'", "uns", "das", "klu\u00b7ge", "Rom", "blo\u00df", "durch", "die", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NE", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es lie\u00df auff seine M\u00fcntz ein folches Bild bereiten", "tokens": ["Es", "lie\u00df", "auff", "sei\u00b7ne", "M\u00fcntz", "ein", "fol\u00b7ches", "Bild", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie einer Jungfer Hand prangt\u2019 in der Lilgen Zier.", "tokens": ["Wie", "ei\u00b7ner", "Jung\u00b7fer", "Hand", "prangt'", "in", "der", "Lil\u00b7gen", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So war die Lilie ein Abri\u00df guter Gaben/", "tokens": ["So", "war", "die", "Li\u00b7lie", "ein", "Ab\u00b7ri\u00df", "gu\u00b7ter", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "So trug die keusche Blum ein keusches Jungfern Bild/", "tokens": ["So", "trug", "die", "keu\u00b7sche", "Blum", "ein", "keu\u00b7sches", "Jung\u00b7fern", "Bild", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NE", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und muste bey sich selbst die grosse Deutung haben/", "tokens": ["Und", "mus\u00b7te", "bey", "sich", "selbst", "die", "gros\u00b7se", "Deu\u00b7tung", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PRF", "ADV", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df ihre Blume sey gemeiner Wohlfahrt Schild.", "tokens": ["Da\u00df", "ih\u00b7re", "Blu\u00b7me", "sey", "ge\u00b7mei\u00b7ner", "Wohl\u00b7fahrt", "Schild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn wo uns die Natur Ergr\u00fcnder nicht betriegen/", "tokens": ["Denn", "wo", "uns", "die", "Na\u00b7tur", "Er\u00b7gr\u00fcn\u00b7der", "nicht", "be\u00b7trie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So w\u00e4chst die Lilie in ihren Thr\u00e4nen auff;", "tokens": ["So", "w\u00e4chst", "die", "Li\u00b7lie", "in", "ih\u00b7ren", "Thr\u00e4\u00b7nen", "auff", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Sie wird damit ges\u00e4et/ mu\u00df sich darmit vergn\u00fcgen/", "tokens": ["Sie", "wird", "da\u00b7mit", "ge\u00b7s\u00e4et", "/", "mu\u00df", "sich", "dar\u00b7mit", "ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "VVPP", "$(", "VMFIN", "PRF", "PAV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bi\u00df ihren weissen Kelch bestrahlt der Sonnen Lauff.", "tokens": ["Bi\u00df", "ih\u00b7ren", "weis\u00b7sen", "Kelch", "be\u00b7strahlt", "der", "Son\u00b7nen", "Lauff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nicht anders geht es zu auch mit der Hoffnungs-Blume", "tokens": ["Nicht", "an\u00b7ders", "geht", "es", "zu", "auch", "mit", "der", "Hoff\u00b7nungs\u00b7Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "APPR", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die in dem matten Hertz der Seuffzer-Thau ern\u00e4hrt/", "tokens": ["Die", "in", "dem", "mat\u00b7ten", "Hertz", "der", "Seuff\u00b7zer\u00b7\u00b7T\u00b7hau", "er\u00b7n\u00e4hrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.15": {"text": "Eh als sie wurtzeln kan und kommt zu vollem Ruhme/", "tokens": ["Eh", "als", "sie", "wurt\u00b7zeln", "kan", "und", "kommt", "zu", "vol\u00b7lem", "Ruh\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "VVINF", "VMFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Hat sie manch heisser Tag und kalte Nacht beschwehrt.", "tokens": ["Hat", "sie", "manch", "heis\u00b7ser", "Tag", "und", "kal\u00b7te", "Nacht", "be\u00b7schwehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "ADJA", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch Rom mag Lilien in seiner Hoffnung f\u00fchren/", "tokens": ["Doch", "Rom", "mag", "Li\u00b7li\u00b7en", "in", "sei\u00b7ner", "Hoff\u00b7nung", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VMFIN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sein Hoffen das bestand in Pracht und Eitelkeit/", "tokens": ["Sein", "Hof\u00b7fen", "das", "be\u00b7stand", "in", "Pracht", "und", "Ei\u00b7tel\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und muste nach und nach sich Blumen gleich verliehren/", "tokens": ["Und", "mus\u00b7te", "nach", "und", "nach", "sich", "Blu\u00b7men", "gleich", "ver\u00b7lieh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "KON", "APPR", "PRF", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die in der Sonnen-Gluth des M\u00e4ders Faust abmeyt.", "tokens": ["Die", "in", "der", "Son\u00b7nen\u00b7Gluth", "des", "M\u00e4\u00b7ders", "Faust", "ab\u00b7meyt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Nein/ unsre Seelige trug auch zwar Hoffnungs-Lilgen/", "tokens": ["Nein", "/", "uns\u00b7re", "See\u00b7li\u00b7ge", "trug", "auch", "zwar", "Hoff\u00b7nungs\u00b7Lil\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPOSAT", "NN", "VVFIN", "ADV", "ADV", "NN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Da jeder Garten itzt mit solchen Kindern prangt:", "tokens": ["Da", "je\u00b7der", "Gar\u00b7ten", "itzt", "mit", "sol\u00b7chen", "Kin\u00b7dern", "prangt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Hingegen kunte sie noch Frost noch Gluth vertilgen/", "tokens": ["Hin\u00b7ge\u00b7gen", "kun\u00b7te", "sie", "noch", "Frost", "noch", "Gluth", "ver\u00b7til\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "NN", "ADV", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.24": {"text": "Bi\u00df sie den h\u00f6chsten Grad der Fruchtbarkeit erlangt.", "tokens": ["Bi\u00df", "sie", "den", "h\u00f6chs\u00b7ten", "Grad", "der", "Frucht\u00b7bar\u00b7keit", "er\u00b7langt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Denn ihre Lilie war CHristus in dem Hertzen/", "tokens": ["Denn", "ih\u00b7re", "Li\u00b7lie", "war", "Chris\u00b7tus", "in", "dem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NE", "APPR", "ART", "NN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Als sie die Bangigkeit des Todes \u00fcberfiel.", "tokens": ["Als", "sie", "die", "Ban\u00b7gig\u00b7keit", "des", "To\u00b7des", "\u00fc\u00b7berf\u00b7iel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sein angenehm Geruch vertrieb ihr Angst und Schmertzen/", "tokens": ["Sein", "an\u00b7ge\u00b7nehm", "Ge\u00b7ruch", "ver\u00b7trieb", "ihr", "Angst", "und", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der Glaube war die Frucht/ Best\u00e4ndigkeit ihr Stiel.", "tokens": ["Der", "Glau\u00b7be", "war", "die", "Frucht", "/", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "ihr", "Stiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$(", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "In diesen Lilien da wolte sie sich weiden/", "tokens": ["In", "die\u00b7sen", "Li\u00b7li\u00b7en", "da", "wol\u00b7te", "sie", "sich", "wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Obgleich des Lebens Krafft ihr allgemach entgieng/", "tokens": ["Ob\u00b7gleich", "des", "Le\u00b7bens", "Krafft", "ihr", "all\u00b7ge\u00b7mach", "ent\u00b7gieng", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Obschon der Abend kam/ der Tag sich wolte scheiden", "tokens": ["Ob\u00b7schon", "der", "A\u00b7bend", "kam", "/", "der", "Tag", "sich", "wol\u00b7te", "schei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$(", "ART", "NN", "PRF", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und Leib und Seele blo\u00df an einem Faden hing.", "tokens": ["Und", "Leib", "und", "See\u00b7le", "blo\u00df", "an", "ei\u00b7nem", "Fa\u00b7den", "hing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Sie hoffte doch auff GOtt und r\u00fcff: HErr willstu t\u00f6dten?", "tokens": ["Sie", "hoff\u00b7te", "doch", "auff", "Gott", "und", "r\u00fcff", ":", "Herr", "will\u00b7stu", "t\u00f6d\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "VVFIN", "$.", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So weich ich dennoch nicht und harre doch auff dich.", "tokens": ["So", "weich", "ich", "den\u00b7noch", "nicht", "und", "har\u00b7re", "doch", "auff", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "PTKNEG", "KON", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Mein Beystand in der Angst: Mein Helffer aus den N\u00f6then", "tokens": ["Mein", "Beys\u00b7tand", "in", "der", "Angst", ":", "Mein", "Helf\u00b7fer", "aus", "den", "N\u00f6\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$.", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der mich von Anbegin geliebt/ dem la\u00df ich mich.", "tokens": ["Der", "mich", "von", "An\u00b7be\u00b7gin", "ge\u00b7liebt", "/", "dem", "la\u00df", "ich", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVPP", "$(", "ART", "VVIMP", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So steht ein harter Fel\u00df in ungeheuren Wellen/", "tokens": ["So", "steht", "ein", "har\u00b7ter", "Fel\u00df", "in", "un\u00b7ge\u00b7heu\u00b7ren", "Wel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wenn ihn ein schwartzer Sturm von oben ab verh\u00fcllt/", "tokens": ["Wenn", "ihn", "ein", "schwart\u00b7zer", "Sturm", "von", "o\u00b7ben", "ab", "ver\u00b7h\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ADV", "PTKVZ", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Von unten auff ihn zu die tollen Fluthen bellen/", "tokens": ["Von", "un\u00b7ten", "auff", "ihn", "zu", "die", "tol\u00b7len", "Flut\u00b7hen", "bel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "PPER", "APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und umb ihn Aeolus mit seinen Winden br\u00fcllt.", "tokens": ["Und", "umb", "ihn", "A\u00b7e\u00b7o\u00b7lus", "mit", "sei\u00b7nen", "Win\u00b7den", "br\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "----+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.41": {"text": "Er stehet unbewegt und h\u00e4lt die Donnerschl\u00e4ge/", "tokens": ["Er", "ste\u00b7het", "un\u00b7be\u00b7wegt", "und", "h\u00e4lt", "die", "Don\u00b7ner\u00b7schl\u00e4\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "ART", "NN", "$("], "meter