{"dta.poem.936": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N ACH   D ER   L ESE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Nun s&#228;ume nicht die gaben zu erhaschen               ", "tokens": ["Nun", "s", "&#228;", "u\u00b7me", "nicht", "die", "ga\u00b7ben", "zu", "er\u00b7ha\u00b7schen"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "$(", "VVFIN", "PTKNEG", "ART", "VVFIN", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Des scheidenden gepr&#228;nges vor der wende               ", "tokens": ["Des", "schei\u00b7den\u00b7den", "ge\u00b7pr", "&#228;", "nges", "vor", "der", "wen\u00b7de"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die grauen wolken sammeln sich behende               ", "tokens": ["Die", "grau\u00b7en", "wol\u00b7ken", "sam\u00b7meln", "sich", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die nebel k\u00f6nnen bald uns &#252;berraschen             ", "tokens": ["Die", "ne\u00b7bel", "k\u00f6n\u00b7nen", "bald", "uns", "&#252;", "ber\u00b7ra\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "PPER", "$(", "VVINF"], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein schwaches fl\u00f6ten von zerpfl&#252;cktem aste               ", "tokens": ["Ein", "schwa\u00b7ches", "fl\u00f6\u00b7ten", "von", "zer\u00b7pfl", "&#252;", "ck\u00b7tem", "as\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "NE", "$(", "FM.la", "FM.la"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verk&#252;ndet dir dass lezte g&#252;te weise               ", "tokens": ["Verk", "&#252;", "ndet", "dir", "dass", "lez\u00b7te", "g", "&#252;", "te", "wei\u00b7se"], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["NN", "$(", "VVFIN", "PPER", "ART", "ADJA", "NN", "$(", "FM.la", "FM.la"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Das land eh es im nahen sturm vereise               ", "tokens": ["Das", "land", "eh", "es", "im", "na\u00b7hen", "sturm", "ver\u00b7ei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOUS", "PPER", "APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch h&#252;lle mit begl&#228;nzendem damaste             ", "tokens": ["Noch", "h", "&#252;", "lle", "mit", "begl", "&#228;", "nzen\u00b7dem", "da\u00b7mas\u00b7te"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Die wespen mit den goldengr&#252;nen schuppen               ", "tokens": ["Die", "wes\u00b7pen", "mit", "den", "gol\u00b7den\u00b7gr", "&#252;", "nen", "schup\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind von verschlossnen kelchen fortgeflogen               ", "tokens": ["Sind", "von", "ver\u00b7schloss\u00b7nen", "kel\u00b7chen", "fort\u00b7ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir fahren mit dem kahn in weitem bogen               ", "tokens": ["Wir", "fah\u00b7ren", "mit", "dem", "kahn", "in", "wei\u00b7tem", "bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um bronzebraunen laubes inselgruppen.             ", "tokens": ["Um", "bron\u00b7ze\u00b7brau\u00b7nen", "lau\u00b7bes", "in\u00b7sel\u00b7grup\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}