{"textgrid.poem.31573": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Drey-Einig-Einig-Drey! auf nie begriffne Weise /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drey-Einig-Einig-Drey! auf nie begriffne Weise /", "tokens": ["Drey\u00b7Ei\u00b7nig\u00b7Ei\u00b7nig\u00b7Drey", "!", "auf", "nie", "be\u00b7griff\u00b7ne", "Wei\u00b7se", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ist Gott das h\u00f6chste Gut / des Wesens Wesenheit /", "tokens": ["ist", "Gott", "das", "h\u00f6chs\u00b7te", "Gut", "/", "des", "We\u00b7sens", "We\u00b7sen\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "ADJA", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "des Vrsprungs Vr-Vrsprung und Ewig vor der Zeit /", "tokens": ["des", "Vr\u00b7sprungs", "Vr\u00b7Vr\u00b7sprung", "und", "E\u00b7wig", "vor", "der", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "des Worts und Athems Safft / und alles Lebens Speise!", "tokens": ["des", "Worts", "und", "A\u00b7thems", "Safft", "/", "und", "al\u00b7les", "Le\u00b7bens", "Spei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NE", "NN", "$(", "KON", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "mea