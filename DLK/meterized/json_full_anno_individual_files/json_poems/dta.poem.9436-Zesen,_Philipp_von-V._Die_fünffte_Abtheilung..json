{"dta.poem.9436": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n Die f\u00fcnffte Abtheilung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Liebster komme die Fr\u00fcchte zu kosten/", "tokens": ["Mein", "Liebs\u00b7ter", "kom\u00b7me", "die", "Fr\u00fcch\u00b7te", "zu", "kos\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich warte des Freundes an unseren pfosten/", "tokens": ["Ich", "war\u00b7te", "des", "Freun\u00b7des", "an", "un\u00b7se\u00b7ren", "pfos\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Mein Hertze mein Licht/", "tokens": ["Mein", "Hert\u00b7ze", "mein", "Licht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Verschmehe mich nicht.", "tokens": ["Ver\u00b7schme\u00b7he", "mich", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}