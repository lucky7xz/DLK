{"textgrid.poem.38532": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Mailied", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Get\u00f6n der Dorfschalmeie,", "tokens": ["Im", "Ge\u00b7t\u00f6n", "der", "Dorf\u00b7schal\u00b7mei\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sinkt aus unbew\u00f6lkter Bl\u00e4ue", "tokens": ["Sinkt", "aus", "un\u00b7be\u00b7w\u00f6lk\u00b7ter", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sanft der Maimond nieder;", "tokens": ["Sanft", "der", "Mai\u00b7mond", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Durch die gr\u00fcngelokten Str\u00e4uche,", "tokens": ["Durch", "die", "gr\u00fcn\u00b7ge\u00b7lok\u00b7ten", "Str\u00e4u\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Am beschilften Pappelteiche,", "tokens": ["Am", "be\u00b7schilf\u00b7ten", "Pap\u00b7pel\u00b7tei\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wirbeln Fr\u00fchlingslieder!", "tokens": ["Wir\u00b7beln", "Fr\u00fch\u00b7lings\u00b7lie\u00b7der", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Lieb