{"dta.poem.2681": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die fl\u00fcchtige Anemone/  \n  Bey Absterben Fr. A. E. K. g. H. den 20.  \n April. 1676.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Adonis/ den man mag der Sch\u00f6nheit Seele nennen/", "tokens": ["A\u00b7do\u00b7nis", "/", "den", "man", "mag", "der", "Sch\u00f6n\u00b7heit", "See\u00b7le", "nen\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "PIS", "VMFIN", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den auch die Venus selbst dem Himmel zoge f\u00fcr/", "tokens": ["Den", "auch", "die", "Ve\u00b7nus", "selbst", "dem", "Him\u00b7mel", "zo\u00b7ge", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nachdem ohn unterla\u00df ihr Hertze pflag zubrennen/", "tokens": ["Nach\u00b7dem", "ohn", "un\u00b7ter\u00b7la\u00df", "ihr", "Hert\u00b7ze", "pflag", "zu\u00b7bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "VVIMP", "PPER", "VVFIN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gieng auff ein hauend Schwein voll muthiger Begier/", "tokens": ["Gieng", "auff", "ein", "hau\u00b7end", "Schwein", "voll", "mut\u00b7hi\u00b7ger", "Be\u00b7gier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJD", "NN", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wird/ weil er zu viel wagt/ bi\u00df auff den Tod verletzet.", "tokens": ["Wird", "/", "weil", "er", "zu", "viel", "wagt", "/", "bi\u00df", "auff", "den", "Tod", "ver\u00b7let\u00b7zet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "KOUS", "PPER", "PTKA", "PIS", "VVFIN", "$(", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bald l\u00e4st die G\u00f6ttin sich von der gestirnten H\u00f6h/", "tokens": ["Bald", "l\u00e4st", "die", "G\u00f6t\u00b7tin", "sich", "von", "der", "ge\u00b7stirn\u00b7ten", "H\u00f6h", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und findet den erblast/ den sie so hoch gesch\u00e4tzet/", "tokens": ["Und", "fin\u00b7det", "den", "er\u00b7blast", "/", "den", "sie", "so", "hoch", "ge\u00b7sch\u00e4t\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "VVFIN", "$(", "ART", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Siht ihre Lust verkehrt in ein erb\u00e4rmlich Weh.", "tokens": ["Siht", "ih\u00b7re", "Lust", "ver\u00b7kehrt", "in", "ein", "er\u00b7b\u00e4rm\u00b7lich", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVPP", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie k\u00fcste tausendmahl die schon erkalten Glieder/", "tokens": ["Sie", "k\u00fcs\u00b7te", "tau\u00b7send\u00b7mahl", "die", "schon", "er\u00b7kal\u00b7ten", "Glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und go\u00df den Thr\u00e4nen-Strom auff das geronne Blut/", "tokens": ["Und", "go\u00df", "den", "Thr\u00e4\u00b7nen\u00b7Strom", "auff", "das", "ge\u00b7ron\u00b7ne", "Blut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie rieff: Ach mein Adon/ mein Leben/ kommt nicht wieder!", "tokens": ["Sie", "rieff", ":", "Ach", "mein", "A\u00b7don", "/", "mein", "Le\u00b7ben", "/", "kommt", "nicht", "wie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ITJ", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jhr G\u00f6tter/ da\u00df ihr doch so grausam an mir thut!", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "/", "da\u00df", "ihr", "doch", "so", "grau\u00b7sam", "an", "mir", "thut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "KOUS", "PPER", "ADV", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch solt du meiner Gunst ein pr\u00e4chtig Denckmal haben/", "tokens": ["Doch", "solt", "du", "mei\u00b7ner", "Gunst", "ein", "pr\u00e4ch\u00b7tig", "Denck\u00b7mal", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "ART", "ADJD", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Holdseeligster Adon/ dein Blut wird ewig bl\u00fchn/", "tokens": ["Hold\u00b7see\u00b7ligs\u00b7ter", "A\u00b7don", "/", "dein", "Blut", "wird", "e\u00b7wig", "bl\u00fchn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "PPOSAT", "NN", "VAFIN", "ADJD", "VVINF", "$("], "meter": "----+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Und andrem Garten-Werck und sch\u00f6nen Fr\u00fchlings-Gaben/", "tokens": ["Und", "an\u00b7drem", "Gar\u00b7ten\u00b7\u00b7Werck", "und", "sch\u00f6\u00b7nen", "Fr\u00fch\u00b7lings\u00b7Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "An Glantz und Herrligkeit gar weit seyn f\u00fcrzuziehn.", "tokens": ["An", "Glantz", "und", "Herr\u00b7lig\u00b7keit", "gar", "weit", "seyn", "f\u00fcr\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "ADJD", "VAINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Uhrpl\u00f6tzlich sprosten auff die zarten Anemonen/", "tokens": ["Uhr\u00b7pl\u00f6tz\u00b7lich", "spros\u00b7ten", "auff", "die", "zar\u00b7ten", "A\u00b7ne\u00b7mo\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Man sah\u2019 den Purpur-Safft auff ihren Bl\u00e4ttern stehn", "tokens": ["Man", "sah'", "den", "Pur\u00b7pur\u00b7\u00b7S\u00b7afft", "auff", "ih\u00b7ren", "Bl\u00e4t\u00b7tern", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Zum Zeichen steter Pflicht/ zum Ehren-Mahl Adonen/", "tokens": ["Zum", "Zei\u00b7chen", "ste\u00b7ter", "Pflicht", "/", "zum", "Eh\u00b7ren\u00b7Mahl", "A\u00b7do\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$(", "APPRART", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df treue Liebe nicht im Grabe kan vergehn.", "tokens": ["Da\u00df", "treu\u00b7e", "Lie\u00b7be", "nicht", "im", "Gra\u00b7be", "kan", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PTKNEG", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich wei\u00df/ ", "tokens": ["Ich", "wei\u00df", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "Und tieffem Seelen-Leid er seinen Schatz beklagt/", "tokens": ["Und", "tief\u00b7fem", "See\u00b7len\u00b7Leid", "er", "sei\u00b7nen", "Schatz", "be\u00b7klagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Da\u00df er ein ewig Grab auffbauet in dem Hertzen/", "tokens": ["Da\u00df", "er", "ein", "e\u00b7wig", "Grab", "auff\u00b7bau\u00b7et", "in", "dem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und Leben/ Geist/ und Blut zulieffern nicht versagt.", "tokens": ["Und", "Le\u00b7ben", "/", "Geist", "/", "und", "Blut", "zu\u00b7li\u00b7ef\u00b7fern", "nicht", "ver\u00b7sagt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "NN", "$(", "KON", "NN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Sein An Emone welckt jetzt/ da sonst alles bl\u00fchet/", "tokens": ["Sein", "An", "E\u00b7mo\u00b7ne", "welckt", "jetzt", "/", "da", "sonst", "al\u00b7les", "bl\u00fc\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "NE", "VVFIN", "ADV", "$(", "ADV", "ADV", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.26": {"text": "Und ein Smaragden-Kleid der Erdenkrei\u00df legt an/", "tokens": ["Und", "ein", "Sma\u00b7rag\u00b7den\u00b7Kleid", "der", "Er\u00b7den\u00b7krei\u00df", "legt", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.27": {"text": "Da man der Flora Reich besternt mit Blumen siehet/", "tokens": ["Da", "man", "der", "Flo\u00b7ra", "Reich", "bes\u00b7ternt", "mit", "Blu\u00b7men", "sie\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "NN", "VVPP", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und ihrer Farben Glantz Rubinen trotzen kan.", "tokens": ["Und", "ih\u00b7rer", "Far\u00b7ben", "Glantz", "Ru\u00b7bi\u00b7nen", "trot\u00b7zen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ist di\u00df nicht Thr\u00e4nen werth? die Blume gr\u00fcner Jugend/", "tokens": ["Ist", "di\u00df", "nicht", "Thr\u00e4\u00b7nen", "werth", "?", "die", "Blu\u00b7me", "gr\u00fc\u00b7ner", "Ju\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "NN", "ADJD", "$.", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Au\u00df der die Sch\u00f6nheit lacht/ und Anmuth hat gespielt/", "tokens": ["Au\u00df", "der", "die", "Sch\u00f6n\u00b7heit", "lacht", "/", "und", "An\u00b7muth", "hat", "ge\u00b7spielt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$(", "KON", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die einen s\u00fcssen Ruch gab von der edlen Tugend/", "tokens": ["Die", "ei\u00b7nen", "s\u00fcs\u00b7sen", "Ruch", "gab", "von", "der", "ed\u00b7len", "Tu\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und diese nur allein zum End-Zweck ihr erziehlt/", "tokens": ["Und", "die\u00b7se", "nur", "al\u00b7lein", "zum", "En\u00b7dZweck", "ihr", "er\u00b7ziehlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wird von de\u00df Todes Hand im Wachsthum abgebrochen?", "tokens": ["Wird", "von", "de\u00df", "To\u00b7des", "Hand", "im", "Wach\u00b7sthum", "ab\u00b7ge\u00b7bro\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "De\u00df Ehbetts Morgenr\u00f6th erblast im ersten Schein/", "tokens": ["De\u00df", "Eh\u00b7betts", "Mor\u00b7gen\u00b7r\u00f6th", "er\u00b7blast", "im", "ers\u00b7ten", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Hat in die Finstern\u00fc\u00df de\u00df Grabes sich verkrochen/", "tokens": ["Hat", "in", "die", "Fins\u00b7ter\u00b7n\u00fc\u00df", "de\u00df", "Gra\u00b7bes", "sich", "ver\u00b7kro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df nichts mehr \u00fcbrig kan als Thr\u00e4nen-Regen seyn.", "tokens": ["Da\u00df", "nichts", "mehr", "\u00fcb\u00b7rig", "kan", "als", "Thr\u00e4\u00b7nen\u00b7Re\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJD", "VMFIN", "KOUS", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Doch/ wie ein durstig Feld nach diesen Tropffen l\u00e4chset/", "tokens": ["Doch", "/", "wie", "ein", "durs\u00b7tig", "Feld", "nach", "die\u00b7sen", "Tropf\u00b7fen", "l\u00e4ch\u00b7set", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOKOM", "ART", "ADJD", "NN", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Umb ihre Fruchtbarkeit mit d\u00fcrren Furcheu fleht:", "tokens": ["Umb", "ih\u00b7re", "Frucht\u00b7bar\u00b7keit", "mit", "d\u00fcr\u00b7ren", "Fur\u00b7cheu", "fleht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So glaub er/ ", "tokens": ["So", "glaub", "er", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.40": {"text": "Gantz zur Vollkommenheit auff diesem Trauer-B\u00e4th.", "tokens": ["Gantz", "zur", "Voll\u00b7kom\u00b7men\u00b7heit", "auff", "die\u00b7sem", "Trau\u00b7e\u00b7rB\u00e4th", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Es hat fast der Natur die Kunst die Hand geboten/", "tokens": ["Es", "hat", "fast", "der", "Na\u00b7tur", "die", "Kunst", "die", "Hand", "ge\u00b7bo\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Der Anemonen Pracht vielf\u00e4rbig anzuschau\u2