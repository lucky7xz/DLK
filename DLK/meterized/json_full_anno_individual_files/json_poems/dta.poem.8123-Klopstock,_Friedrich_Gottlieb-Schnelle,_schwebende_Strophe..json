{"dta.poem.8123": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Schnelle, schwebende Strophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Liebe des Sohns, himmlisches Heil, dem Verstande", "tokens": ["Lie\u00b7be", "des", "Sohns", ",", "himm\u00b7li\u00b7sches", "Heil", ",", "dem", "Ver\u00b7stan\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "$,", "ART", "NN"], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "G\u00f6ttliches Licht! vom Altar Glut dem Gef\u00fchle!", "tokens": ["G\u00f6tt\u00b7li\u00b7ches", "Licht", "!", "vom", "Al\u00b7tar", "Glut", "dem", "Ge\u00b7f\u00fch\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "APPRART", "NN", "NN", "ART", "NN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Tag, der erwacht, in das Meer nicht unterzugehn.", "tokens": ["Tag", ",", "der", "er\u00b7wacht", ",", "in", "das", "Meer", "nicht", "un\u00b7ter\u00b7zu\u00b7gehn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVPP", "$,", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Der Erl\u00f6sten ewiger Tag, Liebe des Sohns!", "tokens": ["Der", "Er\u00b7l\u00f6s\u00b7ten", "e\u00b7wi\u00b7ger", "Tag", ",", "Lie\u00b7be", "des", "Sohns", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "--+-+---+--+", "measure": "iambic.tetra.chol"}}}}}