{"dta.poem.5662": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich f\u00fchle Dich in mir, Unendlich Starcke Macht,", "tokens": ["Ich", "f\u00fch\u00b7le", "Dich", "in", "mir", ",", "Un\u00b7end\u00b7lich", "Star\u00b7cke", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$,", "ADJD", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die allenthalben ist, und \u00fcberall regieret,", "tokens": ["Die", "al\u00b7len\u00b7thal\u00b7ben", "ist", ",", "und", "\u00fc\u00b7be\u00b7rall", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "$,", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dich, der den Schmuck der Welt und aller Himmel Pracht", "tokens": ["Dich", ",", "der", "den", "Schmuck", "der", "Welt", "und", "al\u00b7ler", "Him\u00b7mel", "Pracht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ART", "NN", "KON", "PIAT", "NN", "NN"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Belebet, und von Jhr die Harmonie formiret.", "tokens": ["Be\u00b7le\u00b7bet", ",", "und", "von", "Ihr", "die", "Har\u00b7mo\u00b7nie", "for\u00b7mi\u00b7ret", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.5": {"text": "O HERR! durch den das Licht des Tages und der Nacht", "tokens": ["O", "HeRR", "!", "durch", "den", "das", "Licht", "des", "Ta\u00b7ges", "und", "der", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "APPR", "ART", "ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jm Zeiten-Creysse sich zum erstenmal beweget,", "tokens": ["Jm", "Zei\u00b7ten\u00b7Creys\u00b7se", "sich", "zum", "ers\u00b7ten\u00b7mal", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "APPRART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "O Geist! der unsern Geist Dein Bildnis eingepr\u00e4get.", "tokens": ["O", "Geist", "!", "der", "un\u00b7sern", "Geist", "Dein", "Bild\u00b7nis", "ein\u00b7ge\u00b7pr\u00e4\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du Ursprung, Vater, Quell und Meister der Natur!", "tokens": ["Du", "Ur\u00b7sprung", ",", "Va\u00b7ter", ",", "Quell", "und", "Meis\u00b7ter", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zeig uns von ihres Gangs Gesetzen doch die Spur!", "tokens": ["Zeig", "uns", "von", "ih\u00b7res", "Gangs", "Ge\u00b7set\u00b7zen", "doch", "die", "Spur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Erf\u00fcll und mach uns doch das grosse Werck bekannt,", "tokens": ["Er\u00b7f\u00fcll", "und", "mach", "uns", "doch", "das", "gros\u00b7se", "Werck", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das durch ", "tokens": ["Das", "durch"], "token_info": ["word", "word"], "pos": ["PDS", "APPR"], "meter": "-+", "measure": "iambic.single"}}}}}