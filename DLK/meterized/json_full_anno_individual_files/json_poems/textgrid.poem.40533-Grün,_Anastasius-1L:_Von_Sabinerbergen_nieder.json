, "pos": ["PPOSAT", "ADJA", "NN", "ADV", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "Wo es jetzt auch seine echte", "tokens": ["Wo", "es", "jetzt", "auch", "sei\u00b7ne", "ech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ungeschw\u00e4chte, rauhe Kraft,", "tokens": ["Un\u00b7ge\u00b7schw\u00e4ch\u00b7te", ",", "rau\u00b7he", "Kraft", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch gefahn, in Kerkern\u00e4chte,", "tokens": ["Doch", "ge\u00b7fahn", ",", "in", "Ker\u00b7ker\u00b7n\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine R\u00e4uber, hingeschafft!", "tokens": ["Sei\u00b7ne", "R\u00e4u\u00b7ber", ",", "hin\u00b7ge\u00b7schafft", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Seht dort der Gefangnen Einen", "tokens": ["Seht", "dort", "der", "Ge\u00b7fang\u00b7nen", "Ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Rasch, am Fenster, pfeilgeschwind!", "tokens": ["Rasch", ",", "am", "Fens\u00b7ter", ",", "pfeil\u00b7ge\u00b7schwind", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu ihm hebt das Weib den Kleinen:", "