ine.4": {"text": "Weil's unb\u00e4ndig, wild im Frei'n.\u00ab", "tokens": ["Weil's", "un\u00b7b\u00e4n\u00b7dig", ",", "wild", "im", "Frei'", "n.", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "abbreviation", "punct"], "pos": ["NE", "ADJD", "$,", "ADJD", "APPRART", "NN", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Ruhig nebendran im Bauer", "tokens": ["Ru\u00b7hig", "ne\u00b7ben\u00b7dran", "im", "Bau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sitzt ein fremdes T\u00e4ublein zart,", "tokens": ["Sitzt", "ein", "frem\u00b7des", "T\u00e4ub\u00b7lein", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Senkt das Haupt in milder Trauer", "tokens": ["Senkt", "das", "Haupt", "in", "mil\u00b7der", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ins Gefieder wei\u00dfbehaart.", "tokens": ["Ins", "Ge\u00b7fie\u00b7der", "wei\u00df\u00b7be\u00b7haart", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "\u00bbmutter, warum schlie\u00dft die\u00df gute,", "tokens": ["\u00bb", "mut\u00b7ter", ",", "wa\u00b7rum", "schlie\u00dft", "die\u00df", "gu\u00b7te", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "PWAV", "VVFIN", "PDS", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fromme V\u00f6glein auch man ein?", "tokens": ["From\u00b7me", "V\