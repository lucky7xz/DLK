{"textgrid.poem.40533": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von Sabinerbergen nieder", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Sabinerbergen nieder", "tokens": ["Von", "Sa\u00b7bi\u00b7ner\u00b7ber\u00b7gen", "nie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wallt das braune R\u00e4uberweib,", "tokens": ["Wallt", "das", "brau\u00b7ne", "R\u00e4u\u00b7ber\u00b7weib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmiegend ihres Kn\u00e4bleins Glieder", "tokens": ["Schmie\u00b7gend", "ih\u00b7res", "Kn\u00e4\u00b7bleins", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sorglich fest an ihren Leib.", "tokens": ["Sorg\u00b7lich", "fest", "an", "ih\u00b7ren", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie sie tritt durch Roma's Pforte,", "tokens": ["Wie", "sie", "tritt", "durch", "Ro\u00b7ma's", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Glocken, Trommeln und Gebet!", "tokens": ["Glo\u00b7cken", ",", "Trom\u00b7meln", "und", "Ge\u00b7bet", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Ist's ein Fest, ist Markt am Orte?", "tokens": ["Ist's", "ein", "Fest", ",", "ist", "Markt", "am", "Or\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "VAFIN", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beides hier gar nahe steht!", "tokens": ["Bei\u00b7des", "hier", "gar", "na\u00b7he", "steht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Feierkl\u00e4nge von Sankt Peter!", "tokens": ["Fei\u00b7er\u00b7kl\u00e4n\u00b7ge", "von", "Sankt", "Pe\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "VVFIN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dudelsack hier schnarrend grell!", "tokens": ["Du\u00b7del\u00b7sack", "hier", "schnar\u00b7rend", "grell", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Possen rei\u00dfen heil'ge V\u00e4ter,", "tokens": ["Pos\u00b7sen", "rei\u00b7\u00dfen", "heil'\u00b7ge", "V\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Salbung predigt Pulcinell.", "tokens": ["Sal\u00b7bung", "pre\u00b7digt", "Pul\u00b7ci\u00b7nell", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Affen, Charlatane, Springer,", "tokens": ["Af\u00b7fen", ",", "Char\u00b7la\u00b7ta\u00b7ne", ",", "Sprin\u00b7ger", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem Seile Gauklertritt!", "tokens": ["Auf", "dem", "Sei\u00b7le", "Gauk\u00b7ler\u00b7tritt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jetzt an fremder Bestien Zwinger", "tokens": ["Jetzt", "an", "frem\u00b7der", "Be\u00b7sti\u00b7en", "Zwin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Lenkt das R\u00e4uberweib den Schritt.", "tokens": ["Lenkt", "das", "R\u00e4u\u00b7ber\u00b7weib", "den", "Schritt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ab und auf in wildem Satze", "tokens": ["Ab", "und", "auf", "in", "wil\u00b7dem", "Sat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KON", "APPR", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tobt ein K\u00f6nigstiger hier,", "tokens": ["Tobt", "ein", "K\u00f6\u00b7nigs\u00b7ti\u00b7ger", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An den K\u00e4fig schl\u00e4gt die Tatze,", "tokens": ["An", "den", "K\u00e4\u00b7fig", "schl\u00e4gt", "die", "Tat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00fchend flammt das Aug' dem Thier.", "tokens": ["Gl\u00fc\u00b7hend", "flammt", "das", "Aug'", "dem", "Thier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbmutter, warum sperrt das gute,", "tokens": ["\u00bb", "mut\u00b7ter", ",", "wa\u00b7rum", "sperrt", "das", "gu\u00b7te", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "PWAV", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ne Thier so fest man ein?\u00ab", "tokens": ["Sch\u00f6\u00b7ne", "Thier", "so", "fest", "man", "ein", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "ADV", "ADJD", "PIS", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbkind, weil's durstig lechzt nach Blute,", "tokens": ["\u00bb", "kind", ",", "weil's", "durs\u00b7tig", "lechzt", "nach", "Blu\u00b7te", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "KOUS", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil's unb\u00e4ndig, wild im Frei'n.\u00ab", "tokens": ["Weil's", "un\u00b7b\u00e4n\u00b7dig", ",", "wild", "im", "Frei'", "n.", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "abbreviation", "punct"], "pos": ["NE", "ADJD", "$,", "ADJD", "APPRART", "NN", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ruhig nebendran im Bauer", "tokens": ["Ru\u00b7hig", "ne\u00b7ben\u00b7dran", "im", "Bau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sitzt ein fremdes T\u00e4ublein zart,", "tokens": ["Sitzt", "ein", "frem\u00b7des", "T\u00e4ub\u00b7lein", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Senkt das Haupt in milder Trauer", "tokens": ["Senkt", "das", "Haupt", "in", "mil\u00b7der", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ins Gefieder wei\u00dfbehaart.", "tokens": ["Ins", "Ge\u00b7fie\u00b7der", "wei\u00df\u00b7be\u00b7haart", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbmutter, warum schlie\u00dft die\u00df gute,", "tokens": ["\u00bb", "mut\u00b7ter", ",", "wa\u00b7rum", "schlie\u00dft", "die\u00df", "gu\u00b7te", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "PWAV", "VVFIN", "PDS", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fromme V\u00f6glein auch man ein?", "tokens": ["From\u00b7me", "V\u00f6\u00b7glein", "auch", "man", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses lechzt doch nicht nach Blute?\u00ab", "tokens": ["Die\u00b7ses", "lechzt", "doch", "nicht", "nach", "Blu\u00b7te", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbkind, weil's tr\u00e4gt zwei Fl\u00fcgelein.\u00ab", "tokens": ["\u00bb", "kind", ",", "weil's", "tr\u00e4gt", "zwei", "Fl\u00fc\u00b7ge\u00b7lein", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "$,", "PIS", "VVFIN", "CARD", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Kapitols Steintreppen stiegen", "tokens": ["Ka\u00b7pi\u00b7tols", "Stein\u00b7trep\u00b7pen", "stie\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "VVFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Sie empor im Menschenstrom,", "tokens": ["Sie", "em\u00b7por", "im", "Men\u00b7schen\u00b7strom", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo gesehn nach Kr\u00e4nzen fliegen", "tokens": ["Wo", "ge\u00b7sehn", "nach", "Kr\u00e4n\u00b7zen", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine alte Kraft einst Rom!", "tokens": ["Sei\u00b7ne", "al\u00b7te", "Kraft", "einst", "Rom", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wo es jetzt auch seine echte", "tokens": ["Wo", "es", "jetzt", "auch", "sei\u00b7ne", "ech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ungeschw\u00e4chte, rauhe Kraft,", "tokens": ["Un\u00b7ge\u00b7schw\u00e4ch\u00b7te", ",", "rau\u00b7he", "Kraft", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch gefahn, in Kerkern\u00e4chte,", "tokens": ["Doch", "ge\u00b7fahn", ",", "in", "Ker\u00b7ker\u00b7n\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine R\u00e4uber, hingeschafft!", "tokens": ["Sei\u00b7ne", "R\u00e4u\u00b7ber", ",", "hin\u00b7ge\u00b7schafft", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Seht dort der Gefangnen Einen", "tokens": ["Seht", "dort", "der", "Ge\u00b7fang\u00b7nen", "Ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Rasch, am Fenster, pfeilgeschwind!", "tokens": ["Rasch", ",", "am", "Fens\u00b7ter", ",", "pfeil\u00b7ge\u00b7schwind", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu ihm hebt das Weib den Kleinen:", "tokens": ["Zu", "ihm", "hebt", "das", "Weib", "den", "Klei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbsiehe deinen Vater, Kind!\u00ab", "tokens": ["\u00bb", "sie\u00b7he", "dei\u00b7nen", "Va\u00b7ter", ",", "Kind", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPOSAT", "NN", "$,", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Auf das Kind durch Eisenstangen", "tokens": ["Auf", "das", "Kind", "durch", "Ei\u00b7sen\u00b7stan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickt der Mann so bla\u00df und mild,", "tokens": ["Blickt", "der", "Mann", "so", "bla\u00df", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herzt es lachend, ob die Wangen", "tokens": ["Herzt", "es", "la\u00b7chend", ",", "ob", "die", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Thr\u00e4nenfluth auch \u00fcberquillt;", "tokens": ["Thr\u00e4\u00b7nen\u00b7fluth", "auch", "\u00fc\u00b7berq\u00b7uillt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Ueberdeckt ihm ganz mit K\u00fcssen", "tokens": ["Ue\u00b7berd\u00b7eckt", "ihm", "ganz", "mit", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Z\u00e4rtlich Wang' und Aeugelein;", "tokens": ["Z\u00e4rt\u00b7lich", "Wang'", "und", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und das Kind hat denken m\u00fcssen", "tokens": ["Und", "das", "Kind", "hat", "den\u00b7ken", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "VVINF", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jener Taube, fromm und rein.", "tokens": ["Je\u00b7ner", "Tau\u00b7be", ",", "fromm", "und", "rein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Nun sie Lebewohl ihm sagen,", "tokens": ["Nun", "sie", "Le\u00b7be\u00b7wohl", "ihm", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Str\u00e4ubt sein Haar sich auf in Wuth,", "tokens": ["Str\u00e4ubt", "sein", "Haar", "sich", "auf", "in", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PRF", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine F\u00e4ust' ans Gitter schlagen", "tokens": ["Sei\u00b7ne", "F\u00e4ust'", "ans", "Git\u00b7ter", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sein Auge rollt in Gluth!", "tokens": ["Und", "sein", "Au\u00b7ge", "rollt", "in", "Gluth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Doch die Mutter fest umfangend,", "tokens": ["Doch", "die", "Mut\u00b7ter", "fest", "um\u00b7fan\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flieht das Kind die\u00df grause Bild;", "tokens": ["Flieht", "das", "Kind", "die\u00df", "grau\u00b7se", "Bild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PDS", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und gedenken mu\u00df es bangend", "tokens": ["Und", "ge\u00b7den\u00b7ken", "mu\u00df", "es", "ban\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jenes K\u00f6nigstigers wild.", "tokens": ["Je\u00b7nes", "K\u00f6\u00b7nigs\u00b7ti\u00b7gers", "wild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Von Sabinerbergen nieder", "tokens": ["Von", "Sa\u00b7bi\u00b7ner\u00b7ber\u00b7gen", "nie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wallt das braune R\u00e4uberweib,", "tokens": ["Wallt", "das", "brau\u00b7ne", "R\u00e4u\u00b7ber\u00b7weib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmiegend ihres Kn\u00e4bleins Glieder", "tokens": ["Schmie\u00b7gend", "ih\u00b7res", "Kn\u00e4\u00b7bleins", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sorglich fest an ihren Leib.", "tokens": ["Sorg\u00b7lich", "fest", "an", "ih\u00b7ren", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Wie sie tritt durch Roma's Pforte,", "tokens": ["Wie", "sie", "tritt", "durch", "Ro\u00b7ma's", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Glocken, Trommeln und Gebet!", "tokens": ["Glo\u00b7cken", ",", "Trom\u00b7meln", "und", "Ge\u00b7bet", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Ist's ein Fest, ist Markt am Orte?", "tokens": ["Ist's", "ein", "Fest", ",", "ist", "Markt", "am", "Or\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "VAFIN", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beides hier gar nahe steht!", "tokens": ["Bei\u00b7des", "hier", "gar", "na\u00b7he", "steht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Feierkl\u00e4nge von Sankt Peter!", "tokens": ["Fei\u00b7er\u00b7kl\u00e4n\u00b7ge", "von", "Sankt", "Pe\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "VVFIN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dudelsack hier schnarrend grell!", "tokens": ["Du\u00b7del\u00b7sack", "hier", "schnar\u00b7rend", "grell", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Possen rei\u00dfen heil'ge V\u00e4ter,", "tokens": ["Pos\u00b7sen", "rei\u00b7\u00dfen", "heil'\u00b7ge", "V\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Salbung predigt Pulcinell.", "tokens": ["Sal\u00b7bung", "pre\u00b7digt", "Pul\u00b7ci\u00b7nell", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Affen, Charlatane, Springer,", "tokens": ["Af\u00b7fen", ",", "Char\u00b7la\u00b7ta\u00b7ne", ",", "Sprin\u00b7ger", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem Seile Gauklertritt!", "tokens": ["Auf", "dem", "Sei\u00b7le", "Gauk\u00b7ler\u00b7tritt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jetzt an fremder Bestien Zwinger", "tokens": ["Jetzt", "an", "frem\u00b7der", "Be\u00b7sti\u00b7en", "Zwin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Lenkt das R\u00e4uberweib den Schritt.", "tokens": ["Lenkt", "das", "R\u00e4u\u00b7ber\u00b7weib", "den", "Schritt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Ab und auf in wildem Satze", "tokens": ["Ab", "und", "auf", "in", "wil\u00b7dem", "Sat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KON", "APPR", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tobt ein K\u00f6n