{"dta.poem.10714": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet  Vff H. Michael Starcken  \n Hochzeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ob schon von Kindheit an wir fast zugleich erzogen/", "tokens": ["Ob", "schon", "von", "Kind\u00b7heit", "an", "wir", "fast", "zu\u00b7gleich", "er\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "APPR", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So seind wir ietzund doch in gantz vngleichem Standt/", "tokens": ["So", "seind", "wir", "ie\u00b7tzund", "doch", "in", "gantz", "vn\u00b7glei\u00b7chem", "Standt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du bist/ Herr Breutigam/ bey demem Vatterlandt/", "tokens": ["Du", "bist", "/", "Herr", "Breu\u00b7ti\u00b7gam", "/", "bey", "de\u00b7mem", "Vat\u00b7ter\u00b7landt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "NN", "NE", "$(", "APPR", "ART", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mich hat mein eygne Lust zuziehen weg bewogen/", "tokens": ["Mich", "hat", "mein", "eyg\u00b7ne", "Lust", "zu\u00b7zie\u00b7hen", "weg", "be\u00b7wo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "VVIZU", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Liebe hat mich noch bi\u00dfher nicht viel betrogen/", "tokens": ["Die", "Lie\u00b7be", "hat", "mich", "noch", "bi\u00df\u00b7her", "nicht", "viel", "be\u00b7tro\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd ist das gl\u00fccke Gut/ so bleib ich vnverwandt", "tokens": ["Vnd", "ist", "das", "gl\u00fc\u00b7cke", "Gut", "/", "so", "bleib", "ich", "vn\u00b7ver\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$(", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein alter Junggesell/ schiff\u2019 immer an dem randt/", "tokens": ["Ein", "al\u00b7ter", "Jung\u00b7ge\u00b7sell", "/", "schiff'", "im\u00b7mer", "an", "dem", "randt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VVFIN", "ADV", "APPR", "ART", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Cupido/ wie ich h\u00f6r\u2019/ ist bey dir eingezogen/", "tokens": ["Cu\u00b7pi\u00b7do", "/", "wie", "ich", "h\u00f6r'", "/", "ist", "bey", "dir", "ein\u00b7ge\u00b7zo\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PWAV", "PPER", "VVFIN", "$(", "VAFIN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gibt dir dein eigen theil/ da\u00df du mit Lust vnd Ruh", "tokens": ["Gibt", "dir", "dein", "ei\u00b7gen", "theil", "/", "da\u00df", "du", "mit", "Lust", "vnd", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$(", "KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "(wo ruh bey Weibern ist/) die Zeit kanst bringen zu/", "tokens": ["(", "wo", "ruh", "bey", "Wei\u00b7bern", "ist", "/", ")", "die", "Zeit", "kanst", "brin\u00b7gen", "zu", "/"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "NN", "APPR", "NN", "VAFIN", "$(", "$(", "ART", "NN", "VMFIN", "VVINF", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Vnd darffst nit vieler Gunst/ wie mancher/ dich befleissen/", "tokens": ["Vnd", "darffst", "nit", "vie\u00b7ler", "Gunst", "/", "wie", "man\u00b7cher", "/", "dich", "be\u00b7fleis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PAV", "PTKNEG", "PIAT", "NN", "$(", "KOKOM", "PIAT", "$(", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nun wohl/ ich g\u00f6nn es dir gar gern/ thue nur das dein/", "tokens": ["Nun", "wohl", "/", "ich", "g\u00f6nn", "es", "dir", "gar", "gern", "/", "thue", "nur", "das", "dein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "PPER", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$(", "VVFIN", "ADV", "ART", "PPOSAT", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da\u00df die/ so jetzt nit will vor Fraw gescholten sein/", "tokens": ["Da\u00df", "die", "/", "so", "jetzt", "nit", "will", "vor", "Fraw", "ge\u00b7schol\u00b7ten", "sein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$(", "ADV", "ADV", "PTKNEG", "VMFIN", "APPR", "NN", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sich m\u00fcsse bald hernach gar lassen Mutter heissen.", "tokens": ["Sich", "m\u00fcs\u00b7se", "bald", "her\u00b7nach", "gar", "las\u00b7sen", "Mut\u00b7ter", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VMFIN", "ADV", "ADV", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}