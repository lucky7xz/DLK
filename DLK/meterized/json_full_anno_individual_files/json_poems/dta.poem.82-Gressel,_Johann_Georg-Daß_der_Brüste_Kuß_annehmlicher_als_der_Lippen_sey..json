{"dta.poem.82": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Da\u00df der Br\u00fcste Ku\u00df annehmlicher als  \n der Lippen sey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ob zarte Lippen schon mit Honig-Thaue trieffen/", "tokens": ["Ob", "zar\u00b7te", "Lip\u00b7pen", "schon", "mit", "Ho\u00b7nig\u00b7\u00b7T\u00b7haue", "trief\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schaffet doch mehr Lust mit seiner Lippen-Kahn/", "tokens": ["So", "schaf\u00b7fet", "doch", "mehr", "Lust", "mit", "sei\u00b7ner", "Lip\u00b7pen\u00b7Kahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Br\u00fcste ", "tokens": ["Der", "Br\u00fcs\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Denn/ wenn man Mund und Brust zu k\u00fcssen bietet an;", "tokens": ["Denn", "/", "wenn", "man", "Mund", "und", "Brust", "zu", "k\u00fcs\u00b7sen", "bie\u00b7tet", "an", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "PIS", "NN", "KON", "NN", "PTKZU", "VVINF", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So b\u00fcckt man sich zur Brust/ und l\u00e4st die Lippen fahren/", "tokens": ["So", "b\u00fcckt", "man", "sich", "zur", "Brust", "/", "und", "l\u00e4st", "die", "Lip\u00b7pen", "fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "APPRART", "NN", "$(", "KON", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil auf dem Schnee-Geb\u00fcrg des ", "tokens": ["Weil", "auf", "dem", "Schnee\u00b7Ge\u00b7b\u00fcrg", "des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hymettens Bienen-Safft sich mit der Anmuht paaren;", "tokens": ["Hy\u00b7met\u00b7tens", "Bie\u00b7nen\u00b7\u00b7S\u00b7afft", "sich", "mit", "der", "An\u00b7muht", "paa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.8": {"text": "Dis Lust-Gefilde bringt des ", "tokens": ["Dis", "Lust\u00b7Ge\u00b7fil\u00b7de", "bringt", "des"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "NN", "VVFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Man kostet Ambra Fladen/", "tokens": ["Man", "kos\u00b7tet", "A\u00b7mbra", "Fla\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NE", "NE", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und s\u00fcsse Marmeladen.", "tokens": ["Und", "s\u00fcs\u00b7se", "Mar\u00b7me\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Wann man die zarte Brust an seine Lippen dr\u00fcckt/", "tokens": ["Wann", "man", "die", "zar\u00b7te", "Brust", "an", "sei\u00b7ne", "Lip\u00b7pen", "dr\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und mit der ", "tokens": ["Und", "mit", "der"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.13": {"text": "Prahlt gleich der Mund Rubin mit seiner Purpur Farbe/", "tokens": ["Prahlt", "gleich", "der", "Mund", "Ru\u00b7bin", "mit", "sei\u00b7ner", "Pur\u00b7pur", "Far\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NE", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So mu\u00df er doch dem Schnee der Liljen Br\u00fcste weichen;", "tokens": ["So", "mu\u00df", "er", "doch", "dem", "Schnee", "der", "Lil\u00b7jen", "Br\u00fcs\u00b7te", "wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "K\u00fcst einer gleich den Mund/ sagt er doch/ da\u00df er darbe/", "tokens": ["K\u00fcst", "ei\u00b7ner", "gleich", "den", "Mund", "/", "sagt", "er", "doch", "/", "da\u00df", "er", "dar\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADV", "ART", "NN", "$(", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wenn er die Brust nicht kan zur K\u00fchlung mit erreichen.", "tokens": ["Wenn", "er", "die", "Brust", "nicht", "kan", "zur", "K\u00fch\u00b7lung", "mit", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VMFIN", "APPRART", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Hier setzt man Kirschen blo\u00df dem heissen Munde vor/", "tokens": ["Hier", "setzt", "man", "Kir\u00b7schen", "blo\u00df", "dem", "heis\u00b7sen", "Mun\u00b7de", "vor", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dort quilt gekr\u00f6nte Frucht in gr\u00f6\u00dfrer Zier empor.", "tokens": ["Dort", "quilt", "ge\u00b7kr\u00f6n\u00b7te", "Frucht", "in", "gr\u00f6\u00df\u00b7rer", "Zier", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Kirschen mu\u00df man zwar aus Hoch muht nicht verachten/", "tokens": ["Die", "Kir\u00b7schen", "mu\u00df", "man", "zwar", "aus", "Hoch", "muht", "nicht", "ver\u00b7ach\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "ADV", "APPR", "NE", "VVFIN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Doch der ", "tokens": ["Doch", "der"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}}}}}