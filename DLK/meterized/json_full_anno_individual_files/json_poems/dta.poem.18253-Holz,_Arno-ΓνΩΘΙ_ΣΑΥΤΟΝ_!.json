{"dta.poem.18253": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "\u0393\u03bd\u03a9\u0398\u0399 \u03a3\u0391\u03a5\u03a4\u039f\u039d !", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Auf den glitschrigen Asphalt", "tokens": ["Auf", "den", "glitschri\u00b7gen", "As\u00b7phalt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Pl\u00e4tscherte der Novemberregen", "tokens": ["Pl\u00e4t\u00b7scher\u00b7te", "der", "No\u00b7vem\u00b7ber\u00b7re\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und, windgepeitscht, flackerte rothgelb", "tokens": ["Und", ",", "wind\u00b7ge\u00b7peitscht", ",", "fla\u00b7cker\u00b7te", "roth\u00b7gelb"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "VVFIN", "$,", "VVFIN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durch den Nebeldunst das Licht der Laternen.", "tokens": ["Durch", "den", "Ne\u00b7bel\u00b7dunst", "das", "Licht", "der", "La\u00b7ter\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nur hie und da noch humpelte schwerf\u00e4llig", "tokens": ["Nur", "hie", "und", "da", "noch", "hum\u00b7pel\u00b7te", "schwer\u00b7f\u00e4l\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KON", "ADV", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Durch die dunklen Gassen der tr\u00e4umenden Weltstadt", "tokens": ["Durch", "die", "dunk\u00b7len", "Gas\u00b7sen", "der", "tr\u00e4u\u00b7men\u00b7den", "Welt\u00b7stadt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Ein schl\u00e4friger Droschkengaul", "tokens": ["Ein", "schl\u00e4f\u00b7ri\u00b7ger", "Droschken\u00b7gaul"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und vor der Hausth\u00fcr, hart unter meinem Fenster,", "tokens": ["Und", "vor", "der", "Hau\u00b7sth\u00fcr", ",", "hart", "un\u00b7ter", "mei\u00b7nem", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.9": {"text": "Stand, wie immer um diese Stunde,", "tokens": ["Stand", ",", "wie", "im\u00b7mer", "um", "die\u00b7se", "Stun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So auch heute, mein Stubennachbar,", "tokens": ["So", "auch", "heu\u00b7te", ",", "mein", "Stu\u00b7ben\u00b7nach\u00b7bar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Der neugebackene Referendar,", "tokens": ["Der", "neu\u00b7ge\u00b7ba\u00b7cke\u00b7ne", "Re\u00b7fe\u00b7ren\u00b7dar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.12": {"text": "Und deklamirte h\u00f6chst gef\u00fchlvoll,", "tokens": ["Und", "de\u00b7kla\u00b7mir\u00b7te", "h\u00f6chst", "ge\u00b7f\u00fchl\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Mit seinem St\u00f6ckchen die Luft durchfuchtelnd", "tokens": ["Mit", "sei\u00b7nem", "St\u00f6ck\u00b7chen", "die", "Luft", "durch\u00b7fuch\u00b7telnd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVPP"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Und das Schl\u00fcsselloch immer vergeblich suchend,", "tokens": ["Und", "das", "Schl\u00fcs\u00b7sel\u00b7loch", "im\u00b7mer", "ver\u00b7geb\u00b7lich", "su\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.15": {"text": "Den Monolog der Schillerschen Jungfrau.", "tokens": ["Den", "Mo\u00b7no\u00b7log", "der", "Schil\u00b7ler\u00b7schen", "Jung\u00b7frau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Von dr\u00fcben \u00fcber die Stra\u00dfe her", "tokens": ["Von", "dr\u00fc\u00b7ben", "\u00fc\u00b7ber", "die", "Stra\u00b7\u00dfe", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "APZR"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Blitzten die Spiegelscheiben des Wiener Caf", "tokens": ["Blitz\u00b7ten", "die", "Spie\u00b7gel\u00b7schei\u00b7ben", "des", "Wie\u00b7ner", "Caf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}}}}}