{"dta.poem.21243": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "XxIII.  \n  Was der Erfinder von seinem Jupiter  \n w\u00fcnschen vnd bieten wil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Weil niemand ohn Gebeth in seine Tempel geht;", "tokens": ["Weil", "nie\u00b7mand", "ohn", "Ge\u00b7beth", "in", "sei\u00b7ne", "Tem\u00b7pel", "geht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So h\u00f6hr/ O bleicher Neid/ vmb was ich werde flehen/", "tokens": ["So", "h\u00f6hr", "/", "O", "blei\u00b7cher", "Neid", "/", "vmb", "was", "ich", "wer\u00b7de", "fle\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "NE", "ADJA", "NN", "$(", "APPR", "PRELS", "PPER", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo mich ", "tokens": ["Wo", "mich"], "token_info": ["word", "word"], "pos": ["PWAV", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Dann jeder mag es h\u00f6hrn/ der mir zur Seiten steht.", "tokens": ["Dann", "je\u00b7der", "mag", "es", "h\u00f6hrn", "/", "der", "mir", "zur", "Sei\u00b7ten", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "PPER", "VVINF", "$(", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich rechne keine dinst an Bley vnd Kugeln au\u00df/", "tokens": ["Ich", "rech\u00b7ne", "kei\u00b7ne", "dinst", "an", "Bley", "vnd", "Ku\u00b7geln", "au\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie mancher Hertules/ der sie noch soll empfangen:", "tokens": ["Wie", "man\u00b7cher", "Her\u00b7tu\u00b7les", "/", "der", "sie", "noch", "soll", "emp\u00b7fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$(", "PRELS", "PPER", "ADV", "VMFIN", "VVPP", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Ich suche keinen Sold/ da\u00df ich je durchgegangen/", "tokens": ["Ich", "su\u00b7che", "kei\u00b7nen", "Sold", "/", "da\u00df", "ich", "je", "durch\u00b7ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$(", "KOUS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "Vnd bringe keinen Wirth vmb Leib vnd Gutt vnd Hau\u00df.", "tokens": ["Vnd", "brin\u00b7ge", "kei\u00b7nen", "Wirth", "vmb", "Leib", "vnd", "Gutt", "vnd", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich lege keine Klag auff ", "tokens": ["Ich", "le\u00b7ge", "kei\u00b7ne", "Klag", "auff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die meine Kleider k\u00f6nt\u2019 in statlich Aembter setzen:", "tokens": ["Die", "mei\u00b7ne", "Klei\u00b7der", "k\u00f6nt'", "in", "stat\u00b7lich", "A\u00b7emb\u00b7ter", "set\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VMFIN", "APPR", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich wil mit reicher M\u00fch den Kummer nicht ergetzen/", "tokens": ["Ich", "wil", "mit", "rei\u00b7cher", "M\u00fch", "den", "Kum\u00b7mer", "nicht", "er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJD", "NN", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd trag in Sicherheit vor grossen sachen schew.", "tokens": ["Vnd", "trag", "in", "Si\u00b7cher\u00b7heit", "vor", "gros\u00b7sen", "sa\u00b7chen", "schew", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "ADJA", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich halte di\u00dfmal nicht vmb leere Tittull an/", "tokens": ["Ich", "hal\u00b7te", "di\u00df\u00b7mal", "nicht", "vmb", "lee\u00b7re", "Tit\u00b7tull", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd kan den freyen Sinn ohn Goldne Kethen zwingen:", "tokens": ["Vnd", "kan", "den", "frey\u00b7en", "Sinn", "ohn", "Gold\u00b7ne", "Ke\u00b7then", "zwin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man h\u00f6hrt mich keine Post vor ", "tokens": ["Man", "h\u00f6hrt", "mich", "kei\u00b7ne", "Post", "vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PIAT", "NN", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Die alles ja erh\u00f6hrn/ vnd auch gewehren kan.", "tokens": ["Die", "al\u00b7les", "ja", "er\u00b7h\u00f6hrn", "/", "vnd", "auch", "ge\u00b7weh\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVINF", "$(", "KON", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich hab/ jedoch das sich jtzt ", "tokens": ["Ich", "hab", "/", "je\u00b7doch", "das", "sich", "jtzt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$(", "ADV", "ART", "PRF", "ADV"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Nicht auffzuhalten wei\u00df/ wo ohne Schlacht vnd Siegen", "tokens": ["Nicht", "auff\u00b7zu\u00b7hal\u00b7ten", "wei\u00df", "/", "wo", "oh\u00b7ne", "Schlacht", "vnd", "Sie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVIZU", "VVFIN", "$(", "PWAV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vmb ", "tokens": ["Vmb"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Die sind mein K\u00f6nigreich/ mein Herd vnd mein Altar.", "tokens": ["Die", "sind", "mein", "K\u00f6\u00b7nig\u00b7reich", "/", "mein", "Herd", "vnd", "mein", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Weil seinen Herren numehr keines kan ernehrn/", "tokens": ["Weil", "sei\u00b7nen", "Her\u00b7ren", "nu\u00b7mehr", "kei\u00b7nes", "kan", "er\u00b7nehrn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PIS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd meine Musen dar/ wie vnterm Himmel liegen/", "tokens": ["Vnd", "mei\u00b7ne", "Mu\u00b7sen", "dar", "/", "wie", "vn\u00b7term", "Him\u00b7mel", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKVZ", "$(", "KOKOM", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So la\u00df/ ", "tokens": ["So", "la\u00df", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVIMP", "$("], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Der keinen auff der Welt lest gar zusehr heschwehrn.", "tokens": ["Der", "kei\u00b7nen", "auff", "der", "Welt", "lest", "gar", "zu\u00b7sehr", "hesc\u00b7hwehrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "APPR", "ART", "NN", "VVFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}