{"dta.poem.9968": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "77.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie viel nutzen pfleg ich nicht bey der wirthschafft zu gew\u00e4hre\u0303/", "tokens": ["Wie", "viel", "nut\u00b7zen", "pfleg", "ich", "nicht", "bey", "der", "wirth\u00b7schafft", "zu", "ge\u00b7w\u00e4hr\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVINF", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "APPR", "NE", "$("], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Starck- und grosse kan ich halten/ ob ich schon bin schwach und", "tokens": ["Sta\u00b7rck", "und", "gros\u00b7se", "kan", "ich", "hal\u00b7ten", "/", "ob", "ich", "schon", "bin", "schwach", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["TRUNC", "KON", "ADJA", "VMFIN", "PPER", "VVINF", "$(", "KOUS", "PPER", "ADV", "VAFIN", "ADJD", "KON"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "klein.", "tokens": ["klein", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Offt b\u00fcsst der/ so mich gemachet/ selbst durch mich das leben ein;", "tokens": ["Offt", "b\u00fcsst", "der", "/", "so", "mich", "ge\u00b7ma\u00b7chet", "/", "selbst", "durch", "mich", "das", "le\u00b7ben", "ein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "$(", "ADV", "PPER", "VVPP", "$(", "ADV", "APPR", "PPER", "PDS", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Doch ein ieder wird gar gern mein in diesem fall entbehren.", "tokens": ["Doch", "ein", "ie\u00b7der", "wird", "gar", "gern", "mein", "in", "die\u00b7sem", "fall", "ent\u00b7beh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "VAFIN", "ADV", "ADV", "PPOSAT", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}