{"textgrid.poem.35346": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tod! der du meine innersten Gedanken", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tod! der du meine innersten Gedanken", "tokens": ["Tod", "!", "der", "du", "mei\u00b7ne", "in\u00b7ners\u00b7ten", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Beherrschest, unbezwingbar, unaufhaltsam,", "tokens": ["Be\u00b7herr\u00b7schest", ",", "un\u00b7be\u00b7zwing\u00b7bar", ",", "un\u00b7auf\u00b7halt\u00b7sam", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der du mein ganzes Sein durchdringst, gewaltsam", "tokens": ["Der", "du", "mein", "gan\u00b7zes", "Sein", "durch\u00b7dringst", ",", "ge\u00b7walt\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ersch\u00fctternd meines Wissens enge Schranken;", "tokens": ["Er\u00b7sch\u00fct\u00b7ternd", "mei\u00b7nes", "Wis\u00b7sens", "en\u00b7ge", "Schran\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vernichter, der du weit, unendlich weit", "tokens": ["Ver\u00b7nich\u00b7ter", ",", "der", "du", "weit", ",", "un\u00b7end\u00b7lich", "weit"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "$,", "ADJD", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von Fr\u00fchlingsschauern, die mein Herz durchbebten,", "tokens": ["Von", "Fr\u00fch\u00b7lings\u00b7schau\u00b7ern", ",", "die", "mein", "Herz", "durch\u00b7beb\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Von Wonnen, schon ertr\u00e4umten und erstrebten,", "tokens": ["Von", "Won\u00b7nen", ",", "schon", "er\u00b7tr\u00e4um\u00b7ten", "und", "er\u00b7streb\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Erschienst, in meines Lebens Bl\u00fctezeit;", "tokens": ["Er\u00b7schienst", ",", "in", "mei\u00b7nes", "Le\u00b7bens", "Bl\u00fc\u00b7te\u00b7zeit", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Den ich gef\u00fcrchtet, als des Schaffens Drang", "tokens": ["Den", "ich", "ge\u00b7f\u00fcrch\u00b7tet", ",", "als", "des", "Schaf\u00b7fens", "Drang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VVPP", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Aus meiner Heimat Gauen mich verbannte,", "tokens": ["Aus", "mei\u00b7ner", "Hei\u00b7mat", "Gau\u00b7en", "mich", "ver\u00b7bann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Als ich zu freien Thaten mich ermannte,", "tokens": ["Als", "ich", "zu", "frei\u00b7en", "Tha\u00b7ten", "mich", "er\u00b7mann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Entw\u00f6hnt von Orgelton und Glockenklang;", "tokens": ["Ent\u00b7w\u00f6hnt", "von", "Or\u00b7gel\u00b7ton", "und", "Glo\u00b7cken\u00b7klang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Tod, den ich scheu betrachtet und betastet", "tokens": ["Tod", ",", "den", "ich", "scheu", "be\u00b7trach\u00b7tet", "und", "be\u00b7tas\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "KON", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.14": {"text": "Trotz der Verhei\u00dfung seliger Gefilde;", "tokens": ["Trotz", "der", "Ver\u00b7hei\u00b7\u00dfung", "se\u00b7li\u00b7ger", "Ge\u00b7fil\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.15": {"text": "Den ich nicht liebe, weil des Daseins milde", "tokens": ["Den", "ich", "nicht", "lie\u00b7be", ",", "weil", "des", "Da\u00b7seins", "mil\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PTKNEG", "VVFIN", "$,", "KOUS", "ART", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Gewohnheit auch auf meinen Schultern lastet;", "tokens": ["Ge\u00b7wohn\u00b7heit", "auch", "auf", "mei\u00b7nen", "Schul\u00b7tern", "las\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Den ich getrost erwarte, weil das Ende,", "tokens": ["Den", "ich", "ge\u00b7trost", "er\u00b7war\u00b7te", ",", "weil", "das", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "VVFIN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Der letzte Schlaf, den keine Tr\u00e4ume st\u00f6ren,", "tokens": ["Der", "letz\u00b7te", "Schlaf", ",", "den", "kei\u00b7ne", "Tr\u00e4u\u00b7me", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Des Auferstehens schmerzliche Legende", "tokens": ["Des", "Auf\u00b7er\u00b7ste\u00b7hens", "schmerz\u00b7li\u00b7che", "Le\u00b7gen\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Mich weder schrecken k\u00f6nnen noch beth\u00f6ren;", "tokens": ["Mich", "we\u00b7der", "schre\u00b7cken", "k\u00f6n\u00b7nen", "noch", "be\u00b7th\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "VVINF", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Herr \u00fcber alles, was die Sonnen wecken,", "tokens": ["Herr", "\u00fc\u00b7ber", "al\u00b7les", ",", "was", "die", "Son\u00b7nen", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.22": {"text": "Was kreucht und fleucht \u2013 erh\u00f6re mein Gebet!", "tokens": ["Was", "kreucht", "und", "fleucht", "\u2013", "er\u00b7h\u00f6\u00b7re", "mein", "Ge\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "$(", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Ich will dein S\u00e4nger sein und dein Prophet,", "tokens": ["Ich", "will", "dein", "S\u00e4n\u00b7ger", "sein", "und", "dein", "Pro\u00b7phet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Doch nur, um dich mit Rosen zu bedecken.", "tokens": ["Doch", "nur", ",", "um", "dich", "mit", "Ro\u00b7sen", "zu", "be\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUI", "PRF", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Ich preise des Vernichters Sch\u00f6pfungskraft,", "tokens": ["Ich", "prei\u00b7se", "des", "Ver\u00b7nich\u00b7ters", "Sch\u00f6p\u00b7fungs\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Ewig verj\u00fcngend das f\u00fcr uns Verlorne.", "tokens": ["E\u00b7wig", "ver\u00b7j\u00fcn\u00b7gend", "das", "f\u00fcr", "uns", "Ver\u00b7lor\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ART", "APPR", "PPER", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.27": {"text": "Der eingepflanzte wie der angeborne,", "tokens": ["Der", "ein\u00b7ge\u00b7pflanz\u00b7te", "wie", "der", "an\u00b7ge\u00b7bor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Der alte Glaube weicht der Wissenschaft.", "tokens": ["Der", "al\u00b7te", "Glau\u00b7be", "weicht", "der", "Wis\u00b7sen\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Das ist ein schweres Wort, vielleicht ein herbes;", "tokens": ["Das", "ist", "ein", "schwe\u00b7res", "Wort", ",", "viel\u00b7leicht", "ein", "her\u00b7bes", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Doch flie\u00dft es nicht aus giftgetr\u00e4nkter Feder,", "tokens": ["Doch", "flie\u00dft", "es", "nicht", "aus", "gift\u00b7ge\u00b7tr\u00e4nk\u00b7ter", "Fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Unw\u00fcrdig meines v\u00e4terlichen Erbes,", "tokens": ["Un\u00b7w\u00fcr\u00b7dig", "mei\u00b7nes", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "Er\u00b7bes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Und nicht erschallt es trotzig vom Katheder;", "tokens": ["Und", "nicht", "er\u00b7schallt", "es", "trot\u00b7zig", "vom", "Ka\u00b7the\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Nein! was in stillen, weihevollen Stunden,", "tokens": ["Nein", "!", "was", "in", "stil\u00b7len", ",", "wei\u00b7he\u00b7vol\u00b7len", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PWS", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Was ich von dir erhoffe und erflehe,", "tokens": ["Was", "ich", "von", "dir", "er\u00b7hof\u00b7fe", "und", "er\u00b7fle\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "O Tod, sei Balsam f\u00fcr der Menschheit Wunden,", "tokens": ["O", "Tod", ",", "sei", "Bal\u00b7sam", "f\u00fcr", "der", "Menschheit", "Wun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "NE", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "Sei s\u00fc\u00dfer Trost f\u00fcr meiner Br\u00fcder Wehe!", "tokens": ["Sei", "s\u00fc\u00b7\u00dfer", "Trost", "f\u00fcr", "mei\u00b7ner", "Br\u00fc\u00b7der", "We\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Vergebens schweifen von des Himmels Flur", "tokens": ["Ver\u00b7ge\u00b7bens", "schwei\u00b7fen", "von", "des", "Him\u00b7mels", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Verweinte Augen nach ergrauten Domen", "tokens": ["Ver\u00b7wein\u00b7te", "Au\u00b7gen", "nach", "er\u00b7grau\u00b7ten", "Do\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Und suchen neues Leben bei Phantomen", "tokens": ["Und", "su\u00b7chen", "neu\u00b7es", "Le\u00b7ben", "bei", "Phan\u00b7to\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Statt in dem lichten Tempel der Natur.", "tokens": ["Statt", "in", "dem", "lich\u00b7ten", "Tem\u00b7pel", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Dem Tode, der sein Werk nicht ganz vollendet,", "tokens": ["Dem", "To\u00b7de", ",", "der", "sein", "Werk", "nicht", "ganz", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Der Hirngeburt, die, m\u00fcde Herzen brechend,", "tokens": ["Der", "Hirn\u00b7ge\u00b7burt", ",", "die", ",", "m\u00fc\u00b7de", "Her\u00b7zen", "bre\u00b7chend", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Bald drohend, bald vers\u00f6hnend und versprechend", "tokens": ["Bald", "dro\u00b7hend", ",", "bald", "ver\u00b7s\u00f6h\u00b7nend", "und", "ver\u00b7spre\u00b7chend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Mit Sterbefackeln uns die Augen blendet;", "tokens": ["Mit", "Ster\u00b7be\u00b7fac\u00b7keln", "uns", "die", "Au\u00b7gen", "blen\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Dem Tode, dem verj\u00e4hrter Aberglaube", "tokens": ["Dem", "To\u00b7de", ",", "dem", "ver\u00b7j\u00e4hr\u00b7ter", "A\u00b7berg\u00b7lau\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Ein morschgewordnes Monument errichtet", "tokens": ["Ein", "morschge\u00b7word\u00b7nes", "Mo\u00b7nu\u00b7ment", "er\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.47": {"text": "In thr\u00e4nenfeuchtem, blutvermischtem Staube,", "tokens": ["In", "thr\u00e4\u00b7nen\u00b7feuch\u00b7tem", ",", "blut\u00b7ver\u00b7mischtem", "Stau\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.48": {"text": "Ihm selber ist mein Requiem gedichtet;", "tokens": ["Ihm", "sel\u00b7ber", "ist", "mein", "Re\u00b7qui\u00b7em", "ge\u00b7dich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+--+-", 