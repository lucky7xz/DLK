{"dta.poem.17443": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "64.  Wir dienen uns/ nicht GOtt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch/ Gott ist nichts gedient/ mit fasten bethen wach", "tokens": ["Mensch", "/", "Gott", "ist", "nichts", "ge\u00b7dient", "/", "mit", "fas\u00b7ten", "be\u00b7then", "wach"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "NN", "VAFIN", "PIS", "VVPP", "$(", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du dienst mehr dir damit/ weils dich kan heilig machen.", "tokens": ["Du", "dienst", "mehr", "dir", "da\u00b7mit", "/", "weils", "dich", "kan", "hei\u00b7lig", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PPER", "PAV", "$(", "KOUS", "PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}