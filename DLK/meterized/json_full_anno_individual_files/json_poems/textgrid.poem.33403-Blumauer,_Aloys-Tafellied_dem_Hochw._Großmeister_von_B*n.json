{"textgrid.poem.33403": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Tafellied dem Hochw. Gro\u00dfmeister von B*n", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freude schweb' auf uns hernieder,", "tokens": ["Freu\u00b7de", "schweb'", "auf", "uns", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie begleite froher Scherz;", "tokens": ["Sie", "be\u00b7glei\u00b7te", "fro\u00b7her", "Scherz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der heut'ge Tag, ihr Br\u00fcder,", "tokens": ["Denn", "der", "heut'\u00b7ge", "Tag", ",", "ihr", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Ist ein Fest f\u00fcr unser Herz.", "tokens": ["Ist", "ein", "Fest", "f\u00fcr", "un\u00b7ser", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seht, mit allen seinen T\u00fccken", "tokens": ["Seht", ",", "mit", "al\u00b7len", "sei\u00b7nen", "T\u00fc\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat das Gl\u00fcck uns ausges\u00f6hnt;", "tokens": ["Hat", "das", "Gl\u00fcck", "uns", "aus\u00b7