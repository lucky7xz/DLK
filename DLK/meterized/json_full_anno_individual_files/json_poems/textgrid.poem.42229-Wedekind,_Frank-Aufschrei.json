{"textgrid.poem.42229": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Aufschrei", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ich getan, das l\u00e4\u00dft sich nicht bessern,", "tokens": ["Was", "ich", "ge\u00b7tan", ",", "das", "l\u00e4\u00dft", "sich", "nicht", "bes\u00b7sern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "PDS", "VVFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es l\u00e4\u00dft das Gewissen sich nicht verw\u00e4ssern.", "tokens": ["Es", "l\u00e4\u00dft", "das", "Ge\u00b7wis\u00b7sen", "sich", "nicht", "ver\u00b7w\u00e4s\u00b7sern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ich stehe schuldlos vor meinem Verstand", "tokens": ["Ich", "ste\u00b7he", "schuld\u00b7los", "vor", "mei\u00b7nem", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+----+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und f\u00fchle des Schicksals zermalmende Hand.", "tokens": ["Und", "f\u00fch\u00b7le", "des", "Schick\u00b7sals", "zer\u00b7mal\u00b7men\u00b7de", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Der Mut versiegt, es wachsen die Schmerzen,", "tokens": ["Der", "Mut", "ver\u00b7siegt", ",", "es", "wach\u00b7sen", "die", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und \u00f6d und trostlos wird es im Herzen.", "tokens": ["Und", "\u00f6d", "und", "trost\u00b7los", "wird", "es", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich bin versto\u00dfen, ich bin verdammt,", "tokens": ["Ich", "bin", "ver\u00b7sto\u00b7\u00dfen", ",", "ich", "bin", "ver\u00b7dammt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ringsher von Rachegluten umflammt.", "tokens": ["Rings\u00b7her", "von", "Ra\u00b7che\u00b7glu\u00b7ten", "um\u00b7flammt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Wenn jetzt mich Irrsinn lindernd umfinge,", "tokens": ["Wenn", "jetzt", "mich", "Irr\u00b7sinn", "lin\u00b7dernd", "um\u00b7fin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn ich verkappt in den Himmel ginge!", "tokens": ["Wenn", "ich", "ver\u00b7kappt", "in", "den", "Him\u00b7mel", "gin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-",