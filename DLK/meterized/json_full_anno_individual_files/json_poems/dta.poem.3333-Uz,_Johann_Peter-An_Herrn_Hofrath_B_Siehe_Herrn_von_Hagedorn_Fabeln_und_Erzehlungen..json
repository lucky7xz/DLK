{"dta.poem.3333": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Hofrath B   Siehe Herrn von Hagedorn Fabeln und Erzehlungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Ein silberweisser Bart flie\u00dft ihm von muntern Wan-", "tokens": ["Ein", "sil\u00b7ber\u00b7weis\u00b7ser", "Bart", "flie\u00dft", "ihm", "von", "mun\u00b7tern", "Wan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gen", "tokens": ["gen"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Bis auf den G\u00fcrtel ab, wo schwere Schl\u00fcssel hangen:", "tokens": ["Bis", "auf", "den", "G\u00fcr\u00b7tel", "ab", ",", "wo", "schwe\u00b7re", "Schl\u00fcs\u00b7sel", "han\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PTKVZ", "$,", "PWAV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sein blendendes Gewand schleppt auf dem Boden hin:", "tokens": ["Sein", "blen\u00b7den\u00b7des", "Ge\u00b7wand", "schleppt", "auf", "dem", "Bo\u00b7den", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er geht; ich folg ihm nach; ich weis nicht, wo ich bin.", "tokens": ["Er", "geht", ";", "ich", "folg", "ihm", "nach", ";", "ich", "weis", "nicht", ",", "wo", "ich", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "PTKVZ", "PTKNEG", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein zweifelhaftes Licht stielt sich durch seltne Ritzen,", "tokens": ["Ein", "zwei\u00b7fel\u00b7haf\u00b7tes", "Licht", "stielt", "sich", "durch", "selt\u00b7ne", "Rit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Wie in den W\u00e4ldern herrscht, wann die Gestirne bli-", "tokens": ["Wie", "in", "den", "W\u00e4l\u00b7dern", "herrscht", ",", "wann", "die", "Ge\u00b7stir\u00b7ne", "bli"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "tzen,", "tokens": ["tzen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Noch ehe Cynthia mit vollem Angesicht", "tokens": ["Noch", "e\u00b7he", "Cyn\u00b7thia", "mit", "vol\u00b7lem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "NE", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Aus neidischem Gew\u00f6lke bricht.", "tokens": ["Aus", "nei\u00b7di\u00b7schem", "Ge\u00b7w\u00f6l\u00b7ke", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ich sehe tief hinein viel grosser F\u00e4sser liegen:", "tokens": ["Ich", "se\u00b7he", "tief", "hin\u00b7ein", "viel", "gros\u00b7ser", "F\u00e4s\u00b7ser", "lie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Huy! denk ich, hier giebts Wein! F\u00fcr Sehnsucht und", "tokens": ["Huy", "!", "denk", "ich", ",", "hier", "giebts", "Wein", "!", "F\u00fcr", "Sehn\u00b7sucht", "und"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "NN", "$.", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Vergn\u00fcgen", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Leckt meine d\u00fcrre Zunge schon", "tokens": ["Leckt", "mei\u00b7ne", "d\u00fcr\u00b7re", "Zun\u00b7ge", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die Lippen, die dem Fa\u00df mit ihrem Durste drohn.", "tokens": ["Die", "Lip\u00b7pen", ",", "die", "dem", "Fa\u00df", "mit", "ih\u00b7rem", "Durs\u00b7te", "drohn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Du siehest, sprach der Geist, den ehrlichsten der Geister!", "tokens": ["Du", "sie\u00b7hest", ",", "sprach", "der", "Geist", ",", "den", "ehr\u00b7lichs\u00b7ten", "der", "Geis\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,", "ART", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Ich war in be\u00dfrer Zeit hier ehmals Kellermeister:", "tokens": ["Ich", "war", "in", "be\u00df\u00b7rer", "Zeit", "hier", "eh\u00b7mals", "Kel\u00b7ler\u00b7meis\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "O Zeiten! euch verge\u00df ich nie,", "tokens": ["O", "Zei\u00b7ten", "!", "euch", "ver\u00b7ge\u00df", "ich", "nie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Da Weins die F\u00fclle war, und alles trank und spie!", "tokens": ["Da", "Weins", "die", "F\u00fcl\u00b7le", "war", ",", "und", "al\u00b7les", "trank", "und", "spie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VAFIN", "$,", "KON", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Auf diesen H\u00f6hen stund Ly\u00e4ens liebster Tempel:", "tokens": ["Auf", "die\u00b7sen", "H\u00f6\u00b7hen", "stund", "Ly\u00b7\u00e4\u00b7ens", "liebs\u00b7ter", "Tem\u00b7pel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mein Schatten schwebet noch um den geliebten Ort.", "tokens": ["Mein", "Schat\u00b7ten", "schwe\u00b7bet", "noch", "um", "den", "ge\u00b7lieb\u00b7ten", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wie ofte taumelt\u2019 ich, den J\u00fcngern zum Exempel,", "tokens": ["Wie", "of\u00b7te", "tau\u00b7melt'", "ich", ",", "den", "J\u00fcn\u00b7gern", "zum", "Ex\u00b7em\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Um jene fruchtbarn F\u00e4sser dort!", "tokens": ["Um", "je\u00b7ne", "frucht\u00b7barn", "F\u00e4s\u00b7ser", "dort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Doch damals waren auch die g\u00fcldensten der Zeiten:", "tokens": ["Doch", "da\u00b7mals", "wa\u00b7ren", "auch", "die", "g\u00fcl\u00b7dens\u00b7ten", "der", "Zei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da wuste R\u00f6mhild nichts von Unruh, Zank und Strei-", "tokens": ["Da", "wus\u00b7te", "R\u00f6m\u00b7hild", "nichts", "von", "Un\u00b7ruh", ",", "Zank", "und", "Strei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "PIS", "APPR", "NN", "$,", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "ten:", "tokens": ["ten", ":"], "token_info": ["word", "punct"], "pos": ["FM", "$."], "meter": "-", "measure": "single.down"}, "line.27": {"text": "Man zankte nur, wenn Wein gebrach:", "tokens": ["Man", "zank\u00b7te", "nur", ",", "wenn", "Wein", "ge\u00b7brach", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Nur seit Ly\u00e4us floh, flog ihm der Friede nach.", "tokens": ["Nur", "seit", "Ly\u00b7\u00e4us", "floh", ",", "flog", "ihm", "der", "Frie\u00b7de", "nach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "O R\u00f6mhild! R\u00f6mhild! sieh, was dir mit ihm entge-", "tokens": ["O", "R\u00f6m\u00b7hild", "!", "R\u00f6m\u00b7hild", "!", "sieh", ",", "was", "dir", "mit", "ihm", "ent\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$.", "NN", "$.", "VVFIN", "$,", "PWS", "PPER", "APPR", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "het!", "tokens": ["het", "!"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "-", "measure": "single.down"}, "line.31": {"text": "Die Zwietracht raste stets, die stille Ruhe wich,", "tokens": ["Die", "Zwiet\u00b7racht", "ras\u00b7te", "stets", ",", "die", "stil\u00b7le", "Ru\u00b7he", "wich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Seit Hartenburg verheeret stehet:", "tokens": ["Seit", "Har\u00b7ten\u00b7burg", "ver\u00b7hee\u00b7ret", "ste\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Ein Gott hat hier gewohnt, ein Gott verfolget dich.", "tokens": ["Ein", "Gott", "hat", "hier", "ge\u00b7wohnt", ",", "ein", "Gott", "ver\u00b7fol\u00b7get", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Parodie der Worte Horatii in der 6. Ode des 3ten\nBuchs: Delicta Majorum immeritus luis &amp;c. nach\nHerrn von Hagedorn Uebersetzung in Oden und Liedern\nS. 8. Du b\u00fcssest unverdient der V\u00e4ter Missethaten,", "tokens": ["Pa\u00b7ro\u00b7die", "der", "Wor\u00b7te", "Ho\u00b7ra\u00b7tii", "in", "der", "6.", "O\u00b7de", "des", "3ten", "Buchs", ":", "De\u00b7lic\u00b7ta", "Ma\u00b7jo\u00b7rum", "im\u00b7me\u00b7ri\u00b7tus", "luis", "&amp;", "c.", "nach", "Herrn", "von", "Ha\u00b7ge\u00b7dorn", "Ue\u00b7ber\u00b7set\u00b7zung", "in", "O\u00b7den", "und", "Lie\u00b7dern", "S.", "8.", "Du", "b\u00fcs\u00b7sest", "un\u00b7ver\u00b7di\u00b7ent", "der", "V\u00e4\u00b7ter", "Mis\u00b7se\u00b7tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "ordinal", "word", "word", "number_compound", "word", "punct", "word", "word", "word", "word", "XML_entity", "abbreviation", "word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation", "ordinal", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$.", "FM", "FM", "FM", "FM", "FM", "NE", "APPR", "NN", "APPR", "NE", "NN", "APPR", "NE", "KON", "NN", "NN", "ADJA", "PPER", "VVFIN", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+--+--+-+---+-+-+--+-+--+-+--+--+--+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.35": {"text": "Bis du den Tempel wieder baust,", "tokens": ["Bis", "du", "den", "Tem\u00b7pel", "wie\u00b7der", "baust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Das Haus des Rebengotts, das in Verfall gerathen,", "tokens": ["Das", "Haus", "des", "Re\u00b7ben\u00b7gotts", ",", "das", "in", "Ver\u00b7fall", "ge\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Auf dessen Tr\u00fcmmern du nur Gras und Moder schaust:", "tokens": ["Auf", "des\u00b7sen", "Tr\u00fcm\u00b7mern", "du", "nur", "Gras", "und", "Mo\u00b7der", "schaust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Bis du die F\u00e4sser f\u00fcllst, wo sonst Ly\u00e4us brauste;", "tokens": ["Bis", "du", "die", "F\u00e4s\u00b7ser", "f\u00fcllst", ",", "wo", "sonst", "Ly\u00b7\u00e4us", "braus\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PWAV", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Nun, leider! sind sie leer!", "tokens": ["Nun", ",", "lei\u00b7der", "!", "sind", "sie", "leer", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$.", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.40": {"text": "Der Alte seufzt\u2019 und sprach nicht mehr:", "tokens": ["Der", "Al\u00b7te", "seufzt'", "und", "sprach", "nicht", "mehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Die schreckenvolle H\u00f6hle sauste", "tokens": ["Die", "schre\u00b7cken\u00b7vol\u00b7le", "H\u00f6h\u00b7le", "saus\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Und seufzte kl\u00e4glich: sie sind leer!", "tokens": ["Und", "seufz\u00b7te", "kl\u00e4g\u00b7lich", ":", "sie", "sind", "leer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Auch ich, der schon in Hoffnung schmauste,", "tokens": ["Auch", "ich", ",", "der", "schon", "in", "Hoff\u00b7nung", "schmaus\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Schrie kl\u00e4glich: sie sind leer!", "tokens": ["Schrie", "kl\u00e4g\u00b7lich", ":", "sie", "sind", "leer", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}