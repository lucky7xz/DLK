{"textgrid.poem.66138": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Auf den schnellen Tod seines Sohnes Johann Jakob", "genre": "verse", "period": "N.A.", "pub_year": 1766, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun kenn' ich ihn, den t\u00f6dtlichsten der Schmerzen,", "tokens": ["Nun", "kenn'", "ich", "ihn", ",", "den", "t\u00f6dt\u00b7lichs\u00b7ten", "der", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Nun wei\u00df ich auch, was Mutterliebe thut;", "tokens": ["Nun", "wei\u00df", "ich", "auch", ",", "was", "Mut\u00b7ter\u00b7lie\u00b7be", "thut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn ach! es f\u00e4llt aus meinem Herzen", "tokens": ["Denn", "ach", "!", "es", "f\u00e4llt", "aus", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Mein erstes Kind, die sch\u00f6nste meiner Freuden,", "tokens": ["Mein", "ers\u00b7tes", "Kind", ",", "die", "sch\u00f6ns\u00b7te", "mei\u00b7ner", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "PP