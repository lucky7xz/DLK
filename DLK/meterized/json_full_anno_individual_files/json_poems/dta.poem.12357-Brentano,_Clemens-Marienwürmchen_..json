{"dta.poem.12357": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Marienw\u00fcrmchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Marienw\u00fcrmchen setze dich,               ", "tokens": ["Ma\u00b7rien\u00b7w\u00fcrm\u00b7chen", "set\u00b7ze", "dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf meine Hand, auf meine Hand,", "tokens": ["Auf", "mei\u00b7ne", "Hand", ",", "auf", "mei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich thu dir nichts zu Leide.", "tokens": ["Ich", "thu", "dir", "nichts", "zu", "Lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es soll dir nichts zu Leid geschehn,", "tokens": ["Es", "soll", "dir", "nichts", "zu", "Leid", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Will nur deine bunte Fl\u00fcgel sehn,", "tokens": ["Will", "nur", "dei\u00b7ne", "bun\u00b7te", "Fl\u00fc\u00b7gel", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Bunte Fl\u00fcgel, meine Freude.", "tokens": ["Bun\u00b7te", "Fl\u00fc\u00b7gel", ",", "mei\u00b7ne", "Freu\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Marienw\u00fcrmchen fliege weg,", "tokens": ["Ma\u00b7rien\u00b7w\u00fcrm\u00b7chen", "flie\u00b7ge", "weg", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein H\u00e4uschen brennt, die Kinder schrein", "tokens": ["Dein", "H\u00e4usc\u00b7hen", "brennt", ",", "die", "Kin\u00b7der", "schrein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So sehre, wie so sehre.", "tokens": ["So", "seh\u00b7re", ",", "wie", "so", "seh\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die b\u00f6se Spinne spinnt sie ein,", "tokens": ["Die", "b\u00f6\u00b7se", "Spin\u00b7ne", "spinnt", "sie", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Marienw\u00fcrmchen flieg hinein,", "tokens": ["Ma\u00b7rien\u00b7w\u00fcrm\u00b7chen", "flieg", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine Kinder schreien sehre.", "tokens": ["Dei\u00b7ne", "Kin\u00b7der", "schrei\u00b7en", "seh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Marienw\u00fcrmchen fliege hin", "tokens": ["Ma\u00b7rien\u00b7w\u00fcrm\u00b7chen", "flie\u00b7ge", "hin"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu Nachbars Kind, zu Nachbars Kind,", "tokens": ["Zu", "Nach\u00b7bars", "Kind", ",", "zu", "Nach\u00b7bars", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie thun dir nichts zu Leide;", "tokens": ["Sie", "thun", "dir", "nichts", "zu", "Lei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es soll dir da kein Leid geschehn,", "tokens": ["Es", "soll", "dir", "da", "kein", "Leid", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie wollen deine bunte Fl\u00fcgel sehn,", "tokens": ["Sie", "wol\u00b7len", "dei\u00b7ne", "bun\u00b7te", "Fl\u00fc\u00b7gel", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und gr\u00fc\u00df sie alle beyde.", "tokens": ["Und", "gr\u00fc\u00df", "sie", "al\u00b7le", "bey\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}