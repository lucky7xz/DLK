{"textgrid.poem.33652": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Gottesbraut", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deinem Haupt entsank die Locke;", "tokens": ["Dei\u00b7nem", "Haupt", "ent\u00b7sank", "die", "Lo\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eingesegnet wie die Glocke,", "tokens": ["Ein\u00b7ge\u00b7seg\u00b7net", "wie", "die", "Glo\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die nur Gott ert\u00f6nt, bist nun auch du.", "tokens": ["Die", "nur", "Gott", "er\u00b7t\u00f6nt", ",", "bist", "nun", "auch", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "$,", "VAFIN", "ADV", "ADV", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wie ihr Klang nur Andacht k\u00fcndet,", "tokens": ["Wie", "ihr", "Klang", "nur", "An\u00b7dacht", "k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleich der Kerze, am Altar entz\u00fcndet,", "tokens": ["Gleich", "der", "Ker\u00b7ze", ",", "am", "Al\u00b7tar", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Streb auch