": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sinnend Spiel in leeren R\u00e4umen,", "tokens": ["Sin\u00b7nend", "Spiel", "in", "lee\u00b7ren", "R\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In das Nichts den Weg zu lenken?", "tokens": ["In", "das", "Nichts", "den", "Weg", "zu", "len\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ein Verlieren, ein Vers\u00e4umen,", "tokens": ["Ein", "Ver\u00b7lie\u00b7ren", ",", "ein", "Ver\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo sich selber sucht das Herz,", "tokens": ["Wo", "sich", "sel\u00b7ber", "sucht", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fchrt es uns doch himmelw\u00e4rts.", "tokens": ["F\u00fchrt", "es", "uns", "doch", "him\u00b7mel\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "Andre sehn da nur ein Spielen,", "tokens": ["And\u00b7re", "sehn", "da", "nur", "ein", "Spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nennen es ein selig F\u00fchlen;", "token