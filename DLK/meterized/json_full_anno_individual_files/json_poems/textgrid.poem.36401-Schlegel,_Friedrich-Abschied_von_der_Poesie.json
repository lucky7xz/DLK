}, "line.7": {"text": "Z\u00fcrnend drob aus hohem Mute", "tokens": ["Z\u00fcr\u00b7nend", "drob", "aus", "ho\u00b7hem", "Mu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jenem, der in leichtem Blute", "tokens": ["Je\u00b7nem", ",", "der", "in", "leich\u00b7tem", "Blu\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fr\u00fchlingshauch im Liebesgarten,", "tokens": ["Fr\u00fch\u00b7lings\u00b7hauch", "im", "Lie\u00b7bes\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klagelaut im Abendscheine,", "tokens": ["Kla\u00b7ge\u00b7laut", "im", "A\u00b7bend\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Strahl der Lust im Zauberhaine,", "tokens": ["Strahl", "der", "Lust", "im", "Zau\u00b7ber\u00b7hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer soll deiner Blumen warten?", "tokens": ["Wer", "soll", "dei\u00b7ner", "Blu\u00b7men", "war\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Mag'sche Dichtung! Du alleine,", "tokens": ["Mag'\u00b7sche", "Dich\u00b7tung", "!", "Du", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "P