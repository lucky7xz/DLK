{"dta.poem.10565": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XiI.  \n  Frieden .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Hoch am Himmel stand die Sonne,", "tokens": ["Hoch", "am", "Him\u00b7mel", "stand", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von wei\u00dfen Wolken umwogt,", "tokens": ["Von", "wei\u00b7\u00dfen", "Wol\u00b7ken", "um\u00b7wogt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das Meer war still,", "tokens": ["Das", "Meer", "war", "still", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und sinnend lag ich am Steuer des Schiffes,", "tokens": ["Und", "sin\u00b7nend", "lag", "ich", "am", "Steu\u00b7er", "des", "Schif\u00b7fes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Tr\u00e4umerisch sinnend, \u2014 und halb im Wachen", "tokens": ["Tr\u00e4u\u00b7me\u00b7risch", "sin\u00b7nend", ",", "und", "halb", "im", "Wa\u00b7chen"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "$(", "KON", "ADJD", "APPRART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und halb im Schlummer, schaute ich Christus,", "tokens": ["Und", "halb", "im", "Schlum\u00b7mer", ",", "schau\u00b7te", "ich", "Chris\u00b7tus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "$,", "VVFIN", "PPER", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den Heiland der Welt.", "tokens": ["Den", "Hei\u00b7land", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Im wallend wei\u00dfen Gewande", "tokens": ["Im", "wal\u00b7lend", "wei\u00b7\u00dfen", "Ge\u00b7wan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJD", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Wandelt' er riesengro\u00df", "tokens": ["Wan\u00b7delt'", "er", "rie\u00b7sen\u00b7gro\u00df"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.10": {"text": "Ueber Land und Meer;", "tokens": ["Ue\u00b7ber", "Land", "und", "Meer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Es ragte sein Haupt in den Himmel,", "tokens": ["Es", "rag\u00b7te", "sein", "Haupt", "in", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Die H\u00e4nde streckte er segnend", "tokens": ["Die", "H\u00e4n\u00b7de", "streck\u00b7te", "er", "seg\u00b7nend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Ueber Land und Meer;", "tokens": ["Ue\u00b7ber", "Land", "und", "Meer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "Und als ein Herz in der Brust", "tokens": ["Und", "als", "ein", "Herz", "in", "der", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Trug er die Sonne,", "tokens": ["Trug", "er", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Die rothe, flammende Sonne,", "tokens": ["Die", "ro\u00b7the", ",", "flam\u00b7men\u00b7de", "Son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und das rothe, flammende Sonnenherz", "tokens": ["Und", "das", "ro\u00b7the", ",", "flam\u00b7men\u00b7de", "Son\u00b7nen\u00b7herz"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Go\u00df seine Gnadenstrahlen", "tokens": ["Go\u00df", "sei\u00b7ne", "Gna\u00b7den\u00b7strah\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NE", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sein holdes, liebseliges Licht,", "tokens": ["Und", "sein", "hol\u00b7des", ",", "lieb\u00b7se\u00b7li\u00b7ges", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Erleuchtend und w\u00e4rmend,", "tokens": ["Er\u00b7leuch\u00b7tend", "und", "w\u00e4r\u00b7mend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ueber Land und Meer.", "tokens": ["Ue\u00b7ber", "Land", "und", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Glockenkl\u00e4nge zogen feierlich", "tokens": ["Glo\u00b7cken\u00b7kl\u00e4n\u00b7ge", "zo\u00b7gen", "fei\u00b7er\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Hin und her, zogen wie Schw\u00e4ne,", "tokens": ["Hin", "und", "her", ",", "zo\u00b7gen", "wie", "Schw\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKVZ", "$,", "VVFIN", "KOKOM", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Am Rosenbande, das gleitende Schiff,", "tokens": ["Am", "Ro\u00b7sen\u00b7ban\u00b7de", ",", "das", "glei\u00b7ten\u00b7de", "Schiff", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und zogen es spielend an's gr\u00fcne Ufer,", "tokens": ["Und", "zo\u00b7gen", "es", "spie\u00b7lend", "an's", "gr\u00fc\u00b7ne", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Wo Menschen wohnen, in hochgeth\u00fcrmter,", "tokens": ["Wo", "Men\u00b7schen", "woh\u00b7nen", ",", "in", "hoch\u00b7get\u00b7h\u00fcrm\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,", "APPR", "ADJA", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ragender Stadt.", "tokens": ["Ra\u00b7gen\u00b7der", "Stadt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "O Friedenswunder! Wie still die Stadt!", "tokens": ["O", "Frie\u00b7dens\u00b7wun\u00b7der", "!", "Wie", "still", "die", "Stadt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PWAV", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es ruhte das dumpfe Ger\u00e4usch", "tokens": ["Es", "ruh\u00b7te", "das", "dump\u00b7fe", "Ge\u00b7r\u00e4usch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der schwatzenden, schw\u00fclen Gewerbe,", "tokens": ["Der", "schwat\u00b7zen\u00b7den", ",", "schw\u00fc\u00b7len", "Ge\u00b7wer\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und durch die reinen, hallenden Stra\u00dfen", "tokens": ["Und", "durch", "die", "rei\u00b7nen", ",", "hal\u00b7len\u00b7den", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Zogen Menschen, wei\u00dfgekleidete,", "tokens": ["Zo\u00b7gen", "Men\u00b7schen", ",", "wei\u00df\u00b7ge\u00b7klei\u00b7de\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Palmzweig-tragende,", "tokens": ["Palm\u00b7zweig\u00b7tra\u00b7gen\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und wo sich Zwei begegneten,", "tokens": ["Und", "wo", "sich", "Zwei", "be\u00b7ge\u00b7gne\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PRF", "CARD", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sahn sie sich an, verst\u00e4ndni\u00dfinnig,", "tokens": ["Sahn", "sie", "sich", "an", ",", "ver\u00b7st\u00e4nd\u00b7ni\u00b7\u00dfin\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und schauernd, in Liebe und s\u00fc\u00dfer Entsagung,", "tokens": ["Und", "schau\u00b7ernd", ",", "in", "Lie\u00b7be", "und", "s\u00fc\u00b7\u00dfer", "Ent\u00b7sa\u00b7gung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "K\u00fc\u00dften sie sich auf die Stirne,", "tokens": ["K\u00fc\u00df\u00b7ten", "sie", "sich", "auf", "die", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und schauten hinauf", "tokens": ["Und", "schau\u00b7ten", "hin\u00b7auf"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Nach des Heilands Sonnenherzen,", "tokens": ["Nach", "des", "Hei\u00b7lands", "Son\u00b7nen\u00b7her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Das freudig vers\u00f6hnend sein rothes Blut", "tokens": ["Das", "freu\u00b7dig", "ver\u00b7s\u00f6h\u00b7nend", "sein", "ro\u00b7thes", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "VVPP", "VAINF", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Hinunterstrahlte,", "tokens": ["Hin\u00b7un\u00b7ter\u00b7strahl\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und dreimalselig sprachen sie:", "tokens": ["Und", "drei\u00b7mal\u00b7se\u00b7lig", "spra\u00b7chen", "sie", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gelobt sey Jesu Christ!", "tokens": ["Ge\u00b7lobt", "sey", "Je\u00b7su", "Christ", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}