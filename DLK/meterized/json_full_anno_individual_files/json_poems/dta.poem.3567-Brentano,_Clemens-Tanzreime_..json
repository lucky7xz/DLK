{"dta.poem.3567": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Tanzreime .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann mein Schatz Hochzeit macht,               ", "tokens": ["Wann", "mein", "Schatz", "Hoch\u00b7zeit", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hab ich einen traurigen Tag:", "tokens": ["Hab", "ich", "ei\u00b7nen", "trau\u00b7ri\u00b7gen", "Tag", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Geh ich in mein K\u00e4mmerlein,", "tokens": ["Geh", "ich", "in", "mein", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wein um meinen Schatz.", "tokens": ["Wein", "um", "mei\u00b7nen", "Schatz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Bl\u00fcmlein blau, verdorre nicht,", "tokens": ["Bl\u00fcm\u00b7lein", "blau", ",", "ver\u00b7dor\u00b7re", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Du stehst auf gr\u00fcner Heide;", "tokens": ["Du", "stehst", "auf", "gr\u00fc\u00b7ner", "Hei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Abends, wenn ich schlafen geh,", "tokens": ["Des", "A\u00b7bends", ",", "wenn", "ich", "schla\u00b7fen", "geh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So denk ich an das Lieben.", "tokens": ["So", "denk", "ich", "an", "das", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O du mein liebes Hergottle,", "tokens": ["O", "du", "mein", "lie\u00b7bes", "Her\u00b7gott\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Was han i der denn thaun;", "tokens": ["Was", "han", "i", "der", "denn", "thaun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NE", "ART", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du mir an mein lebelang,", "tokens": ["Da\u00df", "du", "mir", "an", "mein", "le\u00b7be\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Net willst heurathen laun.", "tokens": ["Net", "willst", "heu\u00b7ra\u00b7then", "laun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "VVINF", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Jezt will i nimmer betta,", "tokens": ["Jezt", "will", "i", "nim\u00b7mer", "bet\u00b7ta", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "ADV", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Will net in Kirche gaun;", "tokens": ["Will", "net", "in", "Kir\u00b7che", "gaun", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Geb acht, i kann de n\u00f6tha,", "tokens": ["Geb", "acht", ",", "i", "kann", "de", "n\u00f6\u00b7tha", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "$,", "NE", "VMFIN", "NE", "NE", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Du wirst me heura laun.", "tokens": ["Du", "wirst", "me", "heu\u00b7ra", "laun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Adam und Eva, habens Lieben erdacht,", "tokens": ["A\u00b7dam", "und", "E\u00b7va", ",", "ha\u00b7bens", "Lie\u00b7ben", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NE", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ich und mein Sch\u00e4tzle habens auch so gemacht.", "tokens": ["Ich", "und", "mein", "Sch\u00e4tz\u00b7le", "ha\u00b7bens", "auch", "so", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Mein Gott und mein Herr,", "tokens": ["Mein", "Gott", "und", "mein", "Herr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wie f\u00e4llt mirs so schwer;", "tokens": ["Wie", "f\u00e4llt", "mirs", "so", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NE", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Kein Vater, kein Mutter nit mehr,", "tokens": ["Kein", "Va\u00b7ter", ",", "kein", "Mut\u00b7ter", "nit", "mehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kein lieb Sch\u00e4tzele mehr!", "tokens": ["Kein", "lieb", "Sch\u00e4t\u00b7ze\u00b7le", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Wegen eim Sch\u00e4tzele trauern,", "tokens": ["We\u00b7gen", "eim", "Sch\u00e4t\u00b7ze\u00b7le", "trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Das w\u00e4r mir ein Schand;", "tokens": ["Das", "w\u00e4r", "mir", "ein", "Schand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Kehr mich nur herummer,", "tokens": ["Kehr", "mich", "nur", "her\u00b7um\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Geb der andern die Hand.", "tokens": ["Geb", "der", "an\u00b7dern", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Jin der Kirch, da ist ein Tritt,", "tokens": ["Jin", "der", "Kirch", ",", "da", "ist", "ein", "Tritt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo man zwei Lieben zusammen giebt.", "tokens": ["Wo", "man", "zwei", "Lie\u00b7ben", "zu\u00b7sam\u00b7men", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "CARD", "ADJA", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Hab ein Ringlein am Finger,", "tokens": ["Hab", "ein", "Rin\u00b7glein", "am", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Dadurch seh ich nur;", "tokens": ["Da\u00b7durch", "seh", "ich", "nur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da seh ich ", "tokens": ["Da", "seh", "ich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Seine falsche Natur.", "tokens": ["Sei\u00b7ne", "fal\u00b7sche", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Aus ist es mir dir,", "tokens": ["Aus", "ist", "es", "mir", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "PPER", "PPER", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mein Haus hat kein Th\u00fcr;", "tokens": ["Mein", "Haus", "hat", "kein", "Th\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mein Th\u00fcr hat kein Schlo\u00df,", "tokens": ["Mein", "Th\u00fcr", "hat", "kein", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Von dir bin ich los.", "tokens": ["Von", "dir", "bin", "ich", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Dort dr\u00fcben am Rhein,", "tokens": ["Dort", "dr\u00fc\u00b7ben", "am", "Rhein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NE", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Da liegen drei Stein;", "tokens": ["Da", "lie\u00b7gen", "drei", "Stein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dort f\u00fchrt mir ein Andrer", "tokens": ["Dort", "f\u00fchrt", "mir", "ein", "A\u00b7ndrer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mein Sch\u00e4tzele heim!", "tokens": ["Mein", "Sch\u00e4t\u00b7ze\u00b7le", "heim", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "F\u00fchrt er mir sie heim,", "tokens": ["F\u00fchrt", "er", "mir", "sie", "heim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "So ist mir es recht;", "tokens": ["So", "ist", "mir", "es", "recht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "So ist er der Meister,", "tokens": ["So", "ist", "er", "der", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und ich bin der Knecht.", "tokens": ["Und", "ich", "bin", "der", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}