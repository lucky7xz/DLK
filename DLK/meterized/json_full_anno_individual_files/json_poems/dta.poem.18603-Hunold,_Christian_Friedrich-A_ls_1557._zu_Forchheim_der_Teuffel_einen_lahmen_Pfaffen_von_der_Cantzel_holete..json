{"dta.poem.18603": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A ls 1557. zu Forchheim der  \n Teuffel einen lahmen Pfaffen von  \n der Cantzel holete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Ein lahmer von Person/ ein lahmer in der Schrifft/", "tokens": ["Ein", "lah\u00b7mer", "von", "Per\u00b7son", "/", "ein", "lah\u00b7mer", "in", "der", "Schrifft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "NE", "$(", "ART", "ADJA", "APPR", "ART", "NN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Dem kan der Teuffel bald geschwinde Peine schaffen.", "tokens": ["Dem", "kan", "der", "Teuf\u00b7fel", "bald", "ge\u00b7schwin\u00b7de", "Pei\u00b7ne", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lehrt den geraden Weg ihr geistlich lahmen Pfaffen/", "tokens": ["Lehrt", "den", "ge\u00b7ra\u00b7den", "Weg", "ihr", "geist\u00b7lich", "lah\u00b7men", "Pfaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ehe euch das Lauffen auch wie diesen M\u00fcnch betriefft.", "tokens": ["E\u00b7he", "euch", "das", "Lauf\u00b7fen", "auch", "wie", "die\u00b7sen", "M\u00fcnch", "be\u00b7triefft", "."], "token_info": ["word", "word", "