asure": "iambic.tetra"}, "line.4": {"text": "Mein Herz blieb mir nicht schwer.", "tokens": ["Mein", "Herz", "blieb", "mir", "nicht", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Wir gingen heim, wir zogen fort,", "tokens": ["Wir", "gin\u00b7gen", "heim", ",", "wir", "zo\u00b7gen", "fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir schauten Flu\u00df und Land. \u2013", "tokens": ["Wir", "schau\u00b7ten", "Flu\u00df", "und", "Land", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun liegt der Knab' am fremden Ort", "tokens": ["Nun", "liegt", "der", "Knab'", "am", "frem\u00b7den", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Begraben unter dem Sand.", "tokens": ["Be\u00b7gra\u00b7ben", "un\u00b7ter", "dem", "Sand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.16": {"line.1": {"text": "Vier Wochen sind vor\u00fcber kaum,", "tokens": ["Vier", "Wo\u00b7chen", "sind", "vor\u00b7\u00fc\u00b7ber", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier steh' ich, ohne mein Kind,", "tokens": ["Hier", "steh'", "ich", ",", "oh\u00b7ne", "mein", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUI", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Vor dem zerrissnen Lindenbaum,", "tokens": ["Vor", "dem", "zer\u00b7riss\u00b7nen", "Lin\u00b7den\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich selbst gespalten vom Wind.", "tokens": ["Ich", "selbst", "ge\u00b7spal\u00b7ten", "vom", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}