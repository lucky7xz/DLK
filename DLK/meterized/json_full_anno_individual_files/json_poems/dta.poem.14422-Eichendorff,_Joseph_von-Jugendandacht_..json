{"dta.poem.14422": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So wart' ich still, schau in den Fr\u00fchling milde,", "tokens": ["So", "wart'", "ich", "still", ",", "schau", "in", "den", "Fr\u00fch\u00b7ling", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADJD", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das ganze Herz weint nach dem s\u00fc\u00dfen Bilde,", "tokens": ["Das", "gan\u00b7ze", "Herz", "weint", "nach", "dem", "s\u00fc\u00b7\u00dfen", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor Freud', vor Schmerz? \u2014 ich wei\u00df es nicht zu", "tokens": ["Vor", "Freud'", ",", "vor", "Schmerz", "?", "ich", "wei\u00df", "es", "nicht", "zu"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$.", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKZU"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.4": {"text": "sagen.", "tokens": ["sa\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}