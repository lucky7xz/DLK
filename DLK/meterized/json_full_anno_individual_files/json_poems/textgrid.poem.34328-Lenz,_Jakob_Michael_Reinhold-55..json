{"textgrid.poem.34328": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie mach ich es? wo heb ich Berge aus", "tokens": ["Wie", "mach", "ich", "es", "?", "wo", "heb", "ich", "Ber\u00b7ge", "aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "$.", "PWAV", "VVFIN", "PPER", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mich ihr zu n\u00e4hern? wer kommt mir zu H\u00fclfe?", "tokens": ["Mich", "ihr", "zu", "n\u00e4\u00b7hern", "?", "wer", "kommt", "mir", "zu", "H\u00fcl\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "PTKZU", "VVINF", "$.", "PWS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "O w\u00e4r ich leicht wie Zefir, wie ein Sylphe,", "tokens": ["O", "w\u00e4r", "ich", "leicht", "wie", "Ze\u00b7fir", ",", "wie", "ein", "Syl\u00b7phe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "KOKOM", "NE", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ach oder d\u00fcrft ich in ihr Haus", "tokens": ["Ach", "o\u00b7der", "d\u00fcrft", "ich", "in", "ihr", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "KON", "VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Unmerkbar leise wie die Maus!", "tokens": ["Un\u00b7merk\u00b7bar", "lei\u00b7se", "wie", "die", "Maus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "O w\u00e4r ein Zaubrer da, mich zu zerschneiden, spalten", "tokens": ["O", "w\u00e4r", "ein", "Zaub\u00b7rer", "da", ",", "mich", "zu", "zer\u00b7schnei\u00b7den", ",", "spal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mich tausendartig zu gestalten:", "tokens": ["Mich", "tau\u00b7send\u00b7ar\u00b7tig", "zu", "ge\u00b7stal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "G\u00f6nnt er mir nur das Gl\u00fcck ihr Angesicht zu sehn,", "tokens": ["G\u00f6nnt", "er", "mir", "nur", "das", "Gl\u00fcck", "ihr", "An\u00b7ge\u00b7sicht", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In tausend Tode wollt ich gehn.", "tokens": ["In", "tau\u00b7send", "To\u00b7de", "wollt", "ich", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die schwarzen Augen, deren s\u00fc\u00dfes Feuer", "tokens": ["Die", "schwar\u00b7zen", "Au\u00b7gen", ",", "de\u00b7ren", "s\u00fc\u00b7\u00dfes", "Feu\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Zu Boden wirft, was ihnen naht, der Schleier", "tokens": ["Zu", "Bo\u00b7den", "wirft", ",", "was", "ih\u00b7nen", "naht", ",", "der", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Des unbezwungnen Geistes, der von jedermann", "tokens": ["Des", "un\u00b7be\u00b7zwung\u00b7nen", "Geis\u00b7tes", ",", "der", "von", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PIS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Anbetung sich erzwingt, auch wer ihn ha\u00dfen kann.", "tokens": ["An\u00b7be\u00b7tung", "sich", "er\u00b7zwingt", ",", "auch", "wer", "ihn", "ha\u00b7\u00dfen", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVFIN", "$,", "ADV", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.14": {"text": "Das holde M\u00fcndchen, das so fein empfindet,", "tokens": ["Das", "hol\u00b7de", "M\u00fcnd\u00b7chen", ",", "das", "so", "fein", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "So z\u00e4rtlich liebet, das schalkhafte Kinn", "tokens": ["So", "z\u00e4rt\u00b7lich", "lie\u00b7bet", ",", "das", "schalk\u00b7haf\u00b7te", "Kinn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN",