{"dta.poem.56": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der Wind .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott! wir k\u00f6nnen in den Winden", "tokens": ["Gott", "!", "wir", "k\u00f6n\u00b7nen", "in", "den", "Win\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine grosse Herrlichkeit,", "tokens": ["Dei\u00b7ne", "gros\u00b7se", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Macht und Weisheit finden;", "tokens": ["Dei\u00b7ne", "Macht", "und", "Weis\u00b7heit", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne allen Wiederstreit,", "tokens": ["Oh\u00b7ne", "al\u00b7len", "Wie\u00b7der\u00b7streit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bist du in den starken Brausen,", "tokens": ["Bist", "du", "in", "den", "star\u00b7ken", "Brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und auch in den sanften Sausen", "tokens": ["Und", "auch", "in", "den", "sanf\u00b7ten", "Sau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Unsichtbahr doch anzusehn,", "tokens": ["Un\u00b7sicht\u00b7bahr", "doch", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVIZU", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Weil sie auch von dir entstehn.", "tokens": ["Weil", "sie", "auch", "von", "dir", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Winde sind ein Strom von D\u00fcften,", "tokens": ["Win\u00b7de", "sind", "ein", "Strom", "von", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine stark bewegte Flut,", "tokens": ["Ei\u00b7ne", "stark", "be\u00b7weg\u00b7te", "Flut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die in fortgetriebnen L\u00fcften", "tokens": ["Die", "in", "fort\u00b7ge\u00b7trieb\u00b7nen", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch der Sonnen heisse Glut,", "tokens": ["Durch", "der", "Son\u00b7nen", "heis\u00b7se", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus den Meeren aufgezogen", "tokens": ["Aus", "den", "Mee\u00b7ren", "auf\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ausgedehnet, fort bewogen,", "tokens": ["Aus\u00b7ge\u00b7deh\u00b7net", ",", "fort", "be\u00b7wo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "PTKVZ", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die kein Auge siehet an,", "tokens": ["Die", "kein", "Au\u00b7ge", "sie\u00b7het", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber doch empfinden kan.", "tokens": ["A\u00b7ber", "doch", "emp\u00b7fin\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn die Luft ganz kalt erfroren", "tokens": ["Wenn", "die", "Luft", "ganz", "kalt", "er\u00b7fro\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sich drauf zusammen zieht,", "tokens": ["Und", "sich", "drauf", "zu\u00b7sam\u00b7men", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PAV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird der Winde Braus gebohren,", "tokens": ["Wird", "der", "Win\u00b7de", "Braus", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der im Hauchen weiter flieht:", "tokens": ["Der", "im", "Hau\u00b7chen", "wei\u00b7ter", "flieht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die D\u00fcnste aufwerts steigen,", "tokens": ["Wenn", "die", "D\u00fcns\u00b7te", "auf\u00b7werts", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sich durch die Schwere neigen,", "tokens": ["Und", "sich", "durch", "die", "Schwe\u00b7re", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So sto\u00dft wieder Duft auf Duft,", "tokens": ["So", "sto\u00dft", "wie\u00b7der", "Duft", "auf", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und macht Wind in dikker Luft.", "tokens": ["Und", "macht", "Wind", "in", "dik\u00b7ker", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Wenn des Himmels blauer Bogen,", "tokens": ["Wenn", "des", "Him\u00b7mels", "blau\u00b7er", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der Wolken Dunst-Gewand,", "tokens": ["Mit", "der", "Wol\u00b7ken", "Dunst\u00b7Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie mit schwarzen Flor umzogen,", "tokens": ["Wie", "mit", "schwar\u00b7zen", "Flor", "um\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit Schleiern \u00fcberspannt:", "tokens": ["Und", "mit", "Schlei\u00b7ern", "\u00fc\u00b7bers\u00b7pannt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alsdenn werffen sie die Blikke", "tokens": ["Als\u00b7denn", "werf\u00b7fen", "sie", "die", "Blik\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von dem Sonnenlicht zur\u00fckke,", "tokens": ["Von", "dem", "Son\u00b7nen\u00b7licht", "zu\u00b7r\u00fck\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da die Luft denn wird verd\u00fcnnt,", "tokens": ["Da", "die", "Luft", "denn", "wird", "ver\u00b7d\u00fcnnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und macht dehnend wieder Wind.", "tokens": ["Und", "macht", "deh\u00b7nend", "wie\u00b7der", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Wenn der Sonnen Feuerstrahlen", "tokens": ["Wenn", "der", "Son\u00b7nen", "Feu\u00b7er\u00b7strah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch gewebte L\u00fcfte gehn", "tokens": ["Durch", "ge\u00b7web\u00b7te", "L\u00fcf\u00b7te", "gehn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dunst auf Dunst zu vielen mahlen", "tokens": ["Dunst", "auf", "Dunst", "zu", "vie\u00b7len", "mah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "APPR", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dichte an einander drehn;", "tokens": ["Dich\u00b7te", "an", "ein\u00b7an\u00b7der", "drehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Oder aus einander treiben,", "tokens": ["O\u00b7der", "aus", "ein\u00b7an\u00b7der", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eines an das andre reiben:", "tokens": ["Ei\u00b7nes", "an", "das", "and\u00b7re", "rei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So entsteht ein starker Flus,", "tokens": ["So", "ent\u00b7steht", "ein", "star\u00b7ker", "Flus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Daraus Wind entspringen mu\u00df.", "tokens": ["Da\u00b7raus", "Wind", "ent\u00b7sprin\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Diese und noch andre Quellen", "tokens": ["Die\u00b7se", "und", "noch", "and\u00b7re", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "KON", "ADV", "ADJA", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Sind im Reiche der Natur,", "tokens": ["Sind", "im", "Rei\u00b7che", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NE", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Woraus Winde gleichsam schwellen,", "tokens": ["Wo\u00b7raus", "Win\u00b7de", "gleich\u00b7sam", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deren ganz verborgne Spur", "tokens": ["De\u00b7ren", "ganz", "ver\u00b7borg\u00b7ne", "Spur"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kein Verstand noch kan ergr\u00fcnden,", "tokens": ["Kein", "Ver\u00b7stand", "noch", "kan", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Wiz wird schwerlich finden,", "tokens": ["Und", "der", "Wiz", "wird", "schwer\u00b7lich", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie der Weg des Windes geht,", "tokens": ["Wie", "der", "Weg", "des", "Win\u00b7des", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der durch alle Kreise weht.", "tokens": ["Der", "durch", "al\u00b7le", "Krei\u00b7se", "weht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "In der Kl\u00fcfte dunklen B\u00e4uchen", "tokens": ["In", "der", "Kl\u00fcf\u00b7te", "dunk\u00b7len", "B\u00e4u\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist uns vieles noch verdekt,", "tokens": ["Ist", "uns", "vie\u00b7les", "noch", "ver\u00b7dekt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wer weis in welchen Schl\u00e4uchen,", "tokens": ["Und", "wer", "weis", "in", "wel\u00b7chen", "Schl\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKVZ", "APPR", "PWAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sein verborgner Hauch verstekt.", "tokens": ["Sein", "ver\u00b7borg\u00b7ner", "Hauch", "vers\u00b7tekt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was wir aber davon wissen,", "tokens": ["Was", "wir", "a\u00b7ber", "da\u00b7von", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird uns kl\u00e4rlich zeugen m\u00fcssen,", "tokens": ["Wird", "uns", "kl\u00e4r\u00b7lich", "zeu\u00b7gen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df der grosse Zebaoth,", "tokens": ["Da\u00df", "der", "gros\u00b7se", "Ze\u00b7bao\u00b7th", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sei auch aller Winde GOtt.", "tokens": ["Sei", "auch", "al\u00b7ler", "Win\u00b7de", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Was die Heiden albern tr\u00e4umen,", "tokens": ["Was", "die", "Hei\u00b7den", "al\u00b7bern", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den Wind-Gott Aeolus,", "tokens": ["Von", "den", "Win\u00b7dGott", "A\u00b7e\u00b7o\u00b7lus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wie er ihre Wuth k\u00f6nn z\u00e4umen", "tokens": ["Wie", "er", "ih\u00b7re", "Wuth", "k\u00f6nn", "z\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie er ihren starken Gus", "tokens": ["Wie", "er", "ih\u00b7ren", "star\u00b7ken", "Gus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nach Gefa