], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df der Wind uns sch\u00e4dlich sei.", "tokens": ["Da\u00df", "der", "Wind", "uns", "sch\u00e4d\u00b7lich", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Man beschwert sich wenn sein St\u00fcrmen,", "tokens": ["Man", "be\u00b7schwert", "sich", "wenn", "sein", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "KOUS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch den Luftkreis tobend dringt", "tokens": ["Durch", "den", "Luft\u00b7kreis", "to\u00b7bend", "dringt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von denen H\u00e4usern, Th\u00fcrmen,", "tokens": ["Und", "von", "de\u00b7nen", "H\u00e4u\u00b7sern", ",", "Th\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "NN", "$,", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Ziegel auf die Erde bringt:", "tokens": ["Zie\u00b7gel", "auf", "die", "Er\u00b7de", "bringt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja! man will so gar in Winden,", "tokens": ["Ja", "!", "man", "will", "so", "gar", "in", "Win\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PIS", "VMFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.