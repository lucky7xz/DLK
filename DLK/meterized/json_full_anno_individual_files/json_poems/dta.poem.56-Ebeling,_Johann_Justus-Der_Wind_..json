, "line.3": {"text": "Auf dem Erdkreis will erhalten,", "tokens": ["Auf", "dem", "Erd\u00b7kreis", "will", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So mu\u00df er die Luft bewegt", "tokens": ["So", "mu\u00df", "er", "die", "Luft", "be\u00b7wegt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "D\u00fcnste in dem Umlauf stellen,", "tokens": ["D\u00fcns\u00b7te", "in", "dem", "Um\u00b7lauf", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df sie kr\u00e4useln, weben, schwellen:", "tokens": ["Da\u00df", "sie", "kr\u00e4u\u00b7seln", ",", "we\u00b7ben", ",", "schwel\u00b7len", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00fcrde dieses nicht geschehn,", "tokens": ["W\u00fcr\u00b7de", "die\u00b7ses", "nicht", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "M\u00fcsten wir gar bald vergehn.", "tokens": ["M\u00fcs\u00b7ten", "wir", "gar", "bald", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "W\u00e4r kein Wind in denen L\u00fcften,", "tokens": ["W\u00e4r", "kein", "Wind", "in", "de\u00b7nen", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "APPR", "PRELS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bliebe alle Feuchtigkeit", "tokens": ["Blie\u00b7be", "al\u00b7le", "Feuch\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dampf und Qualm so aus den Gr\u00fcften", "tokens": ["Dampf", "und", "Qualm", "so", "aus", "den", "Gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aufwerts geht, ja allezeit", "tokens": ["Auf\u00b7werts", "geht", ",", "ja", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unbeweglich; und im Wallen", "tokens": ["Un\u00b7be\u00b7weg\u00b7lich", ";", "und", "im", "Wal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$.", "KON", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00fcrd es immer dahin fallen,", "tokens": ["W\u00fcrd", "es", "im\u00b7mer", "da\u00b7hin", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Durch die Schwere unterdr\u00fckt,", "tokens": ["Durch", "die", "Schwe\u00b7re", "un\u00b7ter\u00b7dr\u00fckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo es vorher aufger\u00fckt.", "tokens": ["Wo", "es", "vor\u00b7her", "auf\u00b7ge\u00b7r\u00fckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Und so m\u00fcsten die da leben", "tokens": ["Und", "so", "m\u00fcs\u00b7ten", "die", "da", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "ART", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem ungesunden Rauch,", "tokens": ["In", "dem", "un\u00b7ge\u00b7sun\u00b7den", "Rauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jhren Geist gar bald aufgeben;", "tokens": ["Ih\u00b7ren", "Geist", "gar", "bald", "auf\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-++--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Weil der L\u00fcfte weiter Schlauch,", "tokens": ["Weil", "der", "L\u00fcf\u00b7te", "wei\u00b7ter", "Schlauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus den Pfulen vieles sauget,", "tokens": ["Aus", "den", "Pfu\u00b7len", "vie\u00b7les", "sau\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das da stinkt und gar nichts tauget,", "tokens": ["Das", "da", "stinkt", "und", "gar", "nichts", "tau\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "KON", "ADV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Vor die Brust; weil der Gestank", "tokens": ["Vor", "die", "Brust", ";", "weil", "der", "Ge\u00b7stank"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Machet unsern K\u00f6rper krank.", "tokens": ["Ma\u00b7chet", "un\u00b7sern", "K\u00f6r\u00b7per", "krank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Frische Luft ern\u00e4hrt die Lungen,", "tokens": ["Fri\u00b7sche", "Luft", "er\u00b7n\u00e4hrt", "die", "Lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ein Balsam reicher G\u00fct,", "tokens": ["Als", "ein", "Bal\u00b7sam", "rei\u00b7cher", "G\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wenn die in uns gedrungen,", "tokens": ["Und", "wenn", "die", "in", "uns", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird ermuntert das Gebl\u00fct:", "tokens": ["Wird", "er\u00b7mun\u00b7tert", "das", "Ge\u00b7bl\u00fct", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn wir vielen Dunst einschnauben,", "tokens": ["Wenn", "wir", "vie\u00b7len", "Dunst", "ein\u00b7schnau\u