{"textgrid.poem.42085": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auch diesem wei\u00dfen Haar entstiegest Du", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch diesem wei\u00dfen Haar entstiegest Du", "tokens": ["Auch", "die\u00b7sem", "wei\u00b7\u00dfen", "Haar", "ent\u00b7stie\u00b7gest", "Du"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Unfa\u00dfbar, ein verhangenes Gesicht.", "tokens": ["Un\u00b7fa\u00df\u00b7bar", ",", "ein", "ver\u00b7han\u00b7ge\u00b7nes", "Ge\u00b7sicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich beugte mich ganz \u00fcberst\u00fcrzt ihm zu", "tokens": ["Ich", "beug\u00b7te", "mich", "ganz", "\u00fc\u00b7bers\u00b7t\u00fcrzt", "ihm", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVFIN", "PPER", "PTKZU"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von einem fremden vielgespaltnen Licht", "tokens": ["Von", "ei\u00b7nem", "frem\u00b7den", "viel\u00b7ge\u00b7spalt\u00b7nen", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und sah darin Dich lange Wege schreiten,", "tokens": ["Und", "sah", "da\u00b7rin", "Dich", "lan\u00b7ge", "We\u00b7ge", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wechselnd gewandt in wechselndes Geschick,", "tokens": ["Wech\u00b7selnd", "ge\u00b7wandt", "in", "wech\u00b7seln\u00b7des", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und sah Dich in die ungelebten Zeiten", "tokens": ["Und", "sah", "Dich", "in", "die", "un\u00b7ge\u00b7leb\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Eingehn. Es losch mir der gesenkte Blick", "tokens": ["Ein\u00b7gehn", ".", "Es", "losch", "mir", "der", "ge\u00b7senk\u00b7te", "Blick"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zuweilen so, da\u00df Du wie in den Weihern", "tokens": ["Zu\u00b7wei\u00b7len", "so", ",", "da\u00df", "Du", "wie", "in", "den", "Wei\u00b7hern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Wolkenbild, ein fast verlornes, schwanktest.", "tokens": ["Ein", "Wol\u00b7ken\u00b7bild", ",", "ein", "fast", "ver\u00b7lor\u00b7nes", ",", "schwank\u00b7test", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADV", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich schlug das Auge auf aus seinen Schleiern", "tokens": ["Ich", "schlug", "das", "Au\u00b7ge", "auf", "aus", "sei\u00b7nen", "Schlei\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "In das Bestimmte wieder. Und ich sah", "tokens": ["In", "das", "Be\u00b7stimm\u00b7te", "wie\u00b7der", ".", "Und", "ich", "sah"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "$.", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dir folgend ferne: Wohin Du gelangtest,", "tokens": ["Dir", "fol\u00b7gend", "fer\u00b7ne", ":", "Wo\u00b7hin", "Du", "ge\u00b7lang\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "$.", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Stand meine Liebe gro\u00df und wartend da.", "tokens": ["Stand", "mei\u00b7ne", "Lie\u00b7be", "gro\u00df", "und", "war\u00b7tend", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Auch diesem wei\u00dfen Haar entstiegest Du", "tokens": ["Auch", "die\u00b7sem", "wei\u00b7\u00dfen", "Haar", "ent\u00b7stie\u00b7gest", "Du"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Unfa\u00dfbar, ein verhangenes Gesicht.", "tokens": ["Un\u00b7fa\u00df\u00b7bar", ",", "ein", "ver\u00b7han\u00b7ge\u00b7nes", "Ge\u00b7sicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJA",