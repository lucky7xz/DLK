{"dta.poem.8424": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied im Winter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Der Nordwind br\u00fcllt;", "tokens": ["Der", "Nord\u00b7wind", "br\u00fcllt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die Sonn' ist niedergesunken!", "tokens": ["Die", "Sonn'", "ist", "nie\u00b7der\u00b7ge\u00b7sun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der kalte B\u00e4r.", "tokens": ["Der", "kal\u00b7te", "B\u00e4r", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Blinkt Frost daher!", "tokens": ["Blinkt", "Frost", "da\u00b7her", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Getrunken, Br\u00fcder, getrunken!", "tokens": ["Ge\u00b7trun\u00b7ken", ",", "Br\u00fc\u00b7der", ",", "ge\u00b7trun\u00b7ken", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Tannen gl\u00fchn", "tokens": ["Die", "Tan\u00b7nen", "gl\u00fchn"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Hell im Kamin,", "tokens": ["Hell", "im", "Ka\u00b7min", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und knatternd fliegen die Funken!", "tokens": ["Und", "knat\u00b7ternd", "flie\u00b7gen", "die", "Fun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der edle Rhein", "tokens": ["Der", "ed\u00b7le", "Rhein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Gab uns den Wein!", "tokens": ["Gab", "uns", "den", "Wein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Getrunken, Br\u00fcder, getrunken!", "tokens": ["Ge\u00b7trun\u00b7ken", ",", "Br\u00fc\u00b7der", ",", "ge\u00b7trun\u00b7ken", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der edle Most", "tokens": ["Der", "ed\u00b7le", "Most"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Verscheucht den Frost,", "tokens": ["Ver\u00b7scheucht", "den", "Frost", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und zaubert Fr\u00fchling hernieder:", "tokens": ["Und", "zau\u00b7bert", "Fr\u00fch\u00b7ling", "her\u00b7nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Trinker sieht", "tokens": ["Der", "Trin\u00b7ker", "sieht"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Den Hain entbl\u00fcht,", "tokens": ["Den", "Hain", "ent\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und B\u00fcsche wirbeln ihm Lieder!", "tokens": ["Und", "B\u00fc\u00b7sche", "wir\u00b7beln", "ihm", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}}, "stanza.4": {"line.1": {"text": "Er h\u00f6rt Gesang", "tokens": ["Er", "h\u00f6rt", "Ge\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und Harfenklang,", "tokens": ["Und", "Har\u00b7fen\u00b7klang", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und schwankt durch bl\u00fchende Lauben;", "tokens": ["Und", "schwankt", "durch", "bl\u00fc\u00b7hen\u00b7de", "Lau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein M\u00e4dchenchor", "tokens": ["Ein", "M\u00e4d\u00b7chen\u00b7chor"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Rauscht schnell hervor,", "tokens": ["Rauscht", "schnell", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und bringt ihm goldene Trauben!", "tokens": ["Und", "bringt", "ihm", "gol\u00b7de\u00b7ne", "Trau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Saus' immerfort,", "tokens": ["Saus'", "im\u00b7mer\u00b7fort", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "O Winternord,", "tokens": ["O", "Win\u00b7ter\u00b7nord", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Im schneebelasteten Haine!", "tokens": ["Im", "schnee\u00b7be\u00b7las\u00b7te\u00b7ten", "Hai\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nur streu dein Eis,", "tokens": ["Nur", "streu", "dein", "Eis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "O lieber Greis,", "tokens": ["O", "lie\u00b7ber", "Greis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "In keine Flaschen mit Weine!", "tokens": ["In", "kei\u00b7ne", "Fla\u00b7schen", "mit", "Wei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Der stolzen Frau", "tokens": ["Der", "stol\u00b7zen", "Frau"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "F\u00e4rb braun und blau", "tokens": ["F\u00e4rb", "braun", "und", "blau"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Den Kamm, der adlich ihr schwillet!", "tokens": ["Den", "Kamm", ",", "der", "ad\u00b7lich", "ihr", "schwil\u00b7let", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nur musst du fliehn", "tokens": ["Nur", "musst", "du", "fliehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Den Hermelin,", "tokens": ["Den", "Her\u00b7me\u00b7lin", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der junge Busen verh\u00fcllet!", "tokens": ["Der", "jun\u00b7ge", "Bu\u00b7sen", "ver\u00b7h\u00fcl\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}