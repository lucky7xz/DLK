{"textgrid.poem.24021": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Hilf, heiliger Sankt Florian!", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einer roten Strau\u00dfenfeder", "tokens": ["Ei\u00b7ner", "ro\u00b7ten", "Strau\u00b7\u00dfen\u00b7fe\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sagt ich eben Guten Abend;", "tokens": ["Sagt", "ich", "e\u00b7ben", "Gu\u00b7ten", "A\u00b7bend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heil'ger Florian, da dacht ich", "tokens": ["Heil'\u00b7ger", "Flo\u00b7ri\u00b7an", ",", "da", "dacht", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dein und deiner roten Fahne.", "tokens": ["Dein", "und", "dei\u00b7ner", "ro\u00b7ten", "Fah\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dacht auch deines Wasserk\u00fcbels", "tokens": ["Dacht", "auch", "dei\u00b7nes", "Was\u00b7ser\u00b7k\u00fc\u00b7bels"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der gro\u00dfen roten Flamme,", "tokens": ["Und", "der", "gro\u00b7\u00dfen", "ro\u00b7ten", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", 