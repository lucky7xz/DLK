{"dta.poem.4186": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "C\u00f6rperliche und geistige Gr\u00f6sse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In des Himmels tiefen Ferne", "tokens": ["In", "des", "Him\u00b7mels", "tie\u00b7fen", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seh ich Millionen Sterne,", "tokens": ["Seh", "ich", "Mil\u00b7lion\u00b7en", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und bewundre Zahl und Schein,", "tokens": ["Und", "be\u00b7wund\u00b7re", "Zahl", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch die ungeheure Gr\u00f6sse", "tokens": ["Auch", "die", "un\u00b7ge\u00b7heu\u00b7re", "Gr\u00f6s\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieser herrlichen Gef\u00e4sse,", "tokens": ["Die\u00b7ser", "herr\u00b7li\u00b7chen", "Ge\u00b7f\u00e4s\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Welche nicht zu messen seyn.", "tokens": ["Wel\u00b7che", "nicht", "zu", "mes\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "PTKZU", "VVINF", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich erstaunt\u2019 ob ihrer Schaar!", "tokens": ["Ich", "er\u00b7staunt'", "ob", "ih\u00b7rer", "Schaar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie ich nun mit ihnen mich", "tokens": ["Wie", "ich", "nun", "mit", "ih\u00b7nen", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und mein C\u00f6rperchen verglich;", "tokens": ["Und", "mein", "C\u00f6r\u00b7per\u00b7chen", "ver\u00b7glich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "So verschwand ich ganz und gar.", "tokens": ["So", "ver\u00b7schwand", "ich", "ganz", "und", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ach! fiel mir dar\u00fcber ein:", "tokens": ["Ach", "!", "fiel", "mir", "da\u00b7r\u00fc\u00b7ber", "ein", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "PAV", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Kann ein Wesen, das so klein,", "tokens": ["Kann", "ein", "We\u00b7sen", ",", "das", "so", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Denn so \u00fcberm\u00fchtig seyn?", "tokens": ["Denn", "so", "\u00fc\u00b7ber\u00b7m\u00fch\u00b7tig", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "A. Jmmer bringst du solche Dinge,", "tokens": ["A.", "Jm\u00b7mer", "bringst", "du", "sol\u00b7che", "Din\u00b7ge", ","], "token_info": ["abbreviation", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jmmer machst du so geringe,", "tokens": ["Jm\u00b7mer", "machst", "du", "so", "ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was doch so unsch\u00e4tzbar ist.", "tokens": ["Was", "doch", "so", "un\u00b7sch\u00e4tz\u00b7bar", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ist der Mensch denn etwan, blo\u00df", "tokens": ["Ist", "der", "Mensch", "denn", "et\u00b7wan", ",", "blo\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "ART", "NN", "KON", "ADV", "$,", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seines C\u00f6rpers halben, gro\u00df?", "tokens": ["Sei\u00b7nes", "C\u00f6r\u00b7pers", "hal\u00b7ben", ",", "gro\u00df", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nein. Sein unumschr\u00e4nkter Geist", "tokens": ["Nein", ".", "Sein", "un\u00b7um\u00b7schr\u00e4nk\u00b7ter", "Geist"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ist, der unsre Gr\u00f6sse weist.", "tokens": ["Ist", ",", "der", "uns\u00b7re", "Gr\u00f6s\u00b7se", "weist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "B. Mit recht aufgeblas'nem Muhte", "tokens": ["B.", "Mit", "recht", "auf\u00b7ge\u00b7blas'\u00b7nem", "Muh\u00b7te"], "token_info": ["abbreviation", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Thust du dich, auf deinen Geist,", "tokens": ["Thust", "du", "dich", ",", "auf", "dei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jmmer etwas Rechts zu gute,", "tokens": ["Jm\u00b7mer", "et\u00b7was", "Rechts", "zu", "gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der sich unumschr\u00e4nkt fast hei\u00dft.", "tokens": ["Der", "sich", "un\u00b7um\u00b7schr\u00e4nkt", "fast", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber h\u00f6r! der Gottheit Wesen,", "tokens": ["A\u00b7ber", "h\u00f6r", "!", "der", "Got\u00b7theit", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das du dir, vermessentlich,", "tokens": ["Das", "du", "dir", ",", "ver\u00b7mes\u00b7sent\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "PPER", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Oft, zum Vorwurf, selbst erlesen,", "tokens": ["Oft", ",", "zum", "Vor\u00b7wurf", ",", "selbst", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "NN", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00df, bey deinem Geiste, sich,", "tokens": ["Mu\u00df", ",", "bey", "dei\u00b7nem", "Geis\u00b7te", ",", "sich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "$,", "APPR", "PPOSAT", "NN", "$,", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ohne Zweifel, gr\u00f6sser zeigen,", "tokens": ["Oh\u00b7ne", "Zwei\u00b7fel", ",", "gr\u00f6s\u00b7ser", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als die C\u00f6rper aller Welt", "tokens": ["Als", "die", "C\u00f6r\u00b7per", "al\u00b7ler", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deinen C\u00f6rper \u00fcbersteigen.", "tokens": ["Dei\u00b7nen", "C\u00f6r\u00b7per", "\u00fc\u00b7bers\u00b7tei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch bleibt bey dir fest gestellt:", "tokens": ["Doch", "bleibt", "bey", "dir", "fest", "ge\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADJD", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Von dir werde GOtt sich fassen,", "tokens": ["Von", "dir", "wer\u00b7de", "Gott", "sich", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und Sein Raht begreifen lassen.", "tokens": ["Und", "Sein", "Raht", "be\u00b7grei\u00b7fen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}