{"dta.poem.3851": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein Kind, so an einer Schnecke gestorben.  \n  C. G. Schoenland.  1739.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Grab bringt euch ein grosses Schrecken,", "tokens": ["Mein", "Grab", "bringt", "euch", "ein", "gros\u00b7ses", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da ich nach eingeschluckten Schnecken", "tokens": ["Da", "ich", "nach", "ein\u00b7ge\u00b7schluck\u00b7ten", "Schne\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der W\u00fcrmer Speise worden bin;", "tokens": ["Der", "W\u00fcr\u00b7mer", "Spei\u00b7se", "wor\u00b7den", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Wurm heist itzt das bange Mittel", "tokens": ["Ein", "Wurm", "heist", "itzt", "das", "ban\u00b7ge", "Mit\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zu meinem j\u00e4hen Sterbe-Kittel,", "tokens": ["Zu", "mei\u00b7nem", "j\u00e4\u00b7hen", "Ster\u00b7be\u00b7Kit\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und rei\u00dft mich auf die Baare hin.", "tokens": ["Und", "rei\u00dft", "mich", "auf", "die", "Baa\u00b7re", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Betr\u00fcbte Eltern! seyd gelassen,", "tokens": ["Be\u00b7tr\u00fcb\u00b7te", "El\u00b7tern", "!", "seyd", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und suchet euer Hertz zu fassen.", "tokens": ["Und", "su\u00b7chet", "eu\u00b7er", "Hertz", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}