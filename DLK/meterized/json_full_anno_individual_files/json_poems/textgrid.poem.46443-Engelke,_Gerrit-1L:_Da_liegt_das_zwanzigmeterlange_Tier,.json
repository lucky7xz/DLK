{"textgrid.poem.46443": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da liegt das zwanzigmeterlange Tier,", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da liegt das zwanzigmeterlange Tier,", "tokens": ["Da", "liegt", "das", "zwan\u00b7zig\u00b7me\u00b7ter\u00b7lan\u00b7ge", "Tier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Dampfmaschine,", "tokens": ["Die", "Dampf\u00b7ma\u00b7schi\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Auf blankgeschliffener Schiene", "tokens": ["Auf", "blank\u00b7ge\u00b7schlif\u00b7fe\u00b7ner", "Schie\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Voll hei\u00dfer Wut und sprungbereiter Gier \u2013", "tokens": ["Voll", "hei\u00b7\u00dfer", "Wut", "und", "sprung\u00b7be\u00b7rei\u00b7ter", "Gier", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da lauert, liegt das langgestreckte Eisen-Biest \u2013", "tokens": ["Da", "lau\u00b7ert", ",", "liegt", "das", "lang\u00b7ge\u00b7streck\u00b7te", "Ei\u00b7sen\u00b7Biest", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sieh da: wie Oel- und Wasserschwei\u00df", "tokens": ["Sieh", "da", ":", "wie", "O\u00b7el", "und", "Was\u00b7ser\u00b7schwei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "PWAV", "TRUNC", "KON", "NN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Wie Lebensblut, gef\u00e4hrlich hei\u00df", "tokens": ["Wie", "Le\u00b7bens\u00b7blut", ",", "ge\u00b7f\u00e4hr\u00b7lich", "hei\u00df"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "NN", "$,", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihm aus den Radgest\u00e4ngen: den offnen Weichen flie\u00dft.", "tokens": ["Ihm", "aus", "den", "Rad\u00b7ge\u00b7st\u00e4n\u00b7gen", ":", "den", "off\u00b7nen", "Wei\u00b7chen", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$.", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Es liegt auf sechzehn roten R\u00e4der-Pranken,", "tokens": ["Es", "liegt", "auf", "sech\u00b7zehn", "ro\u00b7ten", "R\u00e4\u00b7der\u00b7Pran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wie fiebernd, langgeduckt zum Sprunge", "tokens": ["Wie", "fie\u00b7bernd", ",", "lang\u00b7ge\u00b7duckt", "zum", "Sprun\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVPP", "$,", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und Fieberdampf st\u00f6\u00dft r\u00f6chelnd aus den Flanken.", "tokens": ["Und", "Fie\u00b7berd\u00b7ampf", "st\u00f6\u00dft", "r\u00f6\u00b7chelnd", "aus", "den", "Flan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Es kocht und kocht die R\u00f6hrenlunge \u2013", "tokens": ["Es", "kocht", "und", "kocht", "die", "R\u00f6h\u00b7ren\u00b7lun\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Den ganzen Rumpf die Feuerkraft durchzittert,", "tokens": ["Den", "gan\u00b7zen", "Rumpf", "die", "Feu\u00b7er\u00b7kraft", "durch\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Er \u00e4chzt und siedet, zischt und hackt", "tokens": ["Er", "\u00e4chzt", "und", "sie\u00b7det", ",", "zischt", "und", "hackt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Im hastigen Dampf- und Eisentakt, \u2013", "tokens": ["Im", "has\u00b7ti\u00b7gen", "Dampf", "und", "Ei\u00b7sen\u00b7takt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "TRUNC", "KON", "NN", "$,", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Dein Menschenwort wie nichts im Qualm zerflittert.", "tokens": ["Dein", "Men\u00b7schen\u00b7wort", "wie", "nichts", "im", "Qualm", "zer\u00b7flit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOKOM", "PIS", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Das Schnauben w\u00e4chst und w\u00e4chst \u2013", "tokens": ["Das", "Schnau\u00b7ben", "w\u00e4chst", "und", "w\u00e4chst", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Du stummer Mensch erschreckst \u2013", "tokens": ["Du", "stum\u00b7mer", "Mensch", "er\u00b7schreckst", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Du siehst die Wut aus allen Ritzen g\u00e4hren \u2013", "tokens": ["Du", "siehst", "die", "Wut", "aus", "al\u00b7len", "Rit\u00b7zen", "g\u00e4h\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Der Kesselr\u00f6hren-Atemdampf", "tokens": ["Der", "Kes\u00b7sel\u00b7r\u00f6h\u00b7ren\u00b7A\u00b7tem\u00b7dampf"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Ist hochgew\u00fchlt auf sechzehn Atmosph\u00e4ren:", "tokens": ["Ist", "hoch\u00b7ge\u00b7w\u00fchlt", "auf", "sech\u00b7zehn", "At\u00b7mo\u00b7sph\u00e4\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Gewalt hat jetzt der hei\u00dfe Krampf:", "tokens": ["Ge\u00b7walt", "hat", "jetzt", "der", "hei\u00b7\u00dfe", "Krampf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Das Biest es br\u00fcllt, das Biest es br\u00fcllt,", "tokens": ["Das", "Biest", "es", "br\u00fcllt", ",", "das", "Biest", "es", "br\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Der F\u00fchrer ist in Dampf geh\u00fcllt \u2013", "tokens": ["Der", "F\u00fch\u00b7rer", "ist", "in", "Dampf", "ge\u00b7h\u00fcllt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Der Regulatorhebel steigt nach links:", "tokens": ["Der", "Re\u00b7gu\u00b7la\u00b7tor\u00b7he\u00b7bel", "steigt", "nach", "links", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Der Eisen-Stier harrt dieses Winks!:", "tokens": ["Der", "Ei\u00b7sen\u00b7S\u00b7tier", "harrt", "die\u00b7ses", "Winks", "!", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PDAT", "NN", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Nun bafft vom Rauchrohr Kraftgeschnauf:", "tokens": ["Nun", "bafft", "vom", "Rauch\u00b7rohr", "Kraft\u00b7ge\u00b7schnauf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Nun springt es auf! nun springt es auf!", "tokens": ["Nun", "springt", "es", "auf", "!", "nun", "springt", "es", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch:", "tokens": ["Doch", ":"], "token_info": ["word", "punct"], "pos": ["KON", "$."], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Ruhig gleiten und kreisen auf endloser Schiene", "tokens": ["Ru\u00b7hig", "glei\u00b7ten", "und", "krei\u00b7sen", "auf", "end\u00b7lo\u00b7ser", "Schie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVINF", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Die treibenden R\u00e4der hinaus auf dem bl\u00e4nkernden Band,", "tokens": ["Die", "trei\u00b7ben\u00b7den", "R\u00e4\u00b7der", "hin\u00b7aus", "auf", "dem", "bl\u00e4n\u00b7kern\u00b7den", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APZR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+---+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gemessen und massig die kraftangef\u00fcllte Maschine,", "tokens": ["Ge\u00b7mes\u00b7sen", "und", "mas\u00b7sig", "die", "kraf\u00b7tan\u00b7ge\u00b7f\u00fcll\u00b7te", "Ma\u00b7schi\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "Der schleppende, stampfende Rumpf hinterher \u2013", "tokens": ["Der", "schlep\u00b7pen\u00b7de", ",", "stamp\u00b7fen\u00b7de", "Rumpf", "hin\u00b7ter\u00b7her", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Dahinter \u2013 ein dunkler \u2013 verschwimmender Punkt", "tokens": ["Da\u00b7hin\u00b7ter", "\u2013", "ein", "dunk\u00b7ler", "\u2013", "ver\u00b7schwim\u00b7men