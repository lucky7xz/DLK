{"dta.poem.9616": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An den Celadon.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.57", "cy:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Celadon sol meine glut/", "tokens": ["Mein", "Ce\u00b7la\u00b7don", "sol", "mei\u00b7ne", "glut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mir durchwandert marck und blut/", "tokens": ["Die", "mir", "durch\u00b7wan\u00b7dert", "marck", "und", "blut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht rauch und flammen von sich treiben/", "tokens": ["Nicht", "rauch", "und", "flam\u00b7men", "von", "sich", "trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KON", "VVFIN", "APPR", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Soll Aetna in dem hertzen stehn/", "tokens": ["Soll", "A\u00b7et\u00b7na", "in", "dem", "hert\u00b7zen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und Phlegeton in adern gehn/", "tokens": ["Und", "Phle\u00b7ge\u00b7ton", "in", "a\u00b7dern", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihre kraft verborgen bleiben.", "tokens": ["Und", "ih\u00b7re", "kraft", "ver\u00b7bor\u00b7gen", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich mameluckin der natur", "tokens": ["Ich", "ma\u00b7me\u00b7lu\u00b7ckin", "der", "na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NE", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Darf keine rechte liebes-spur", "tokens": ["Darf", "kei\u00b7ne", "rech\u00b7te", "lie\u00b7bes\u00b7spur"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor meines liebsten augen la\u00dfen/", "tokens": ["Vor", "mei\u00b7nes", "liebs\u00b7ten", "au\u00b7gen", "la\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein hertze soll entz\u00fcndet seyn/", "tokens": ["Mein", "hert\u00b7ze", "soll", "ent\u00b7z\u00fcn\u00b7det", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein hertze f\u00fchlt die s\u00fcsse pein/", "tokens": ["Mein", "hert\u00b7ze", "f\u00fchlt", "die", "s\u00fcs\u00b7se", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und mit den lippen mu\u00df ich ha\u00dfen.", "tokens": ["Und", "mit", "den", "lip\u00b7pen", "mu\u00df", "ich", "ha\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df nicht/ wie die kluge welt", "tokens": ["Ich", "wei\u00df", "nicht", "/", "wie", "die", "klu\u00b7ge", "welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die thorheit zum gesetze stellt/", "tokens": ["Die", "thor\u00b7heit", "zum", "ge\u00b7set\u00b7ze", "stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und netze macht sich selbst zu plagen.", "tokens": ["Und", "net\u00b7ze", "macht", "sich", "selbst", "zu", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie kan man/ wenn die starcke hand", "tokens": ["Wie", "kan", "man", "/", "wenn", "die", "star\u00b7cke", "hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PIS", "$(", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Uns trift mit feuer/ glut und brand/", "tokens": ["Uns", "trift", "mit", "feu\u00b7er", "/", "glut", "und", "brand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$(", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von wasser und von k\u00e4lte sagen.", "tokens": ["Von", "was\u00b7ser", "und", "von", "k\u00e4l\u00b7te", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "APPR", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich zwinge mich so viel ich kan/", "tokens": ["Ich", "zwin\u00b7ge", "mich", "so", "viel", "ich", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "PPER", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nehme was verstelltes an/", "tokens": ["Und", "neh\u00b7me", "was", "ver\u00b7stell\u00b7tes", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADJA", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beherrsche lippen und gesichte/", "tokens": ["Be\u00b7herr\u00b7sche", "lip\u00b7pen", "und", "ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil doch die gar zu kluge welt", "tokens": ["Weil", "doch", "die", "gar", "zu", "klu\u00b7ge", "welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu keiner zeit vor schwestern h\u00e4lt/", "tokens": ["Zu", "kei\u00b7ner", "zeit", "vor", "schwes\u00b7tern", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bekante brunst und gut ger\u00fcchte.", "tokens": ["Be\u00b7kan\u00b7te", "brunst", "und", "gut", "ge\u00b7r\u00fcch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mein Celadon bleibt doch geliebt/", "tokens": ["Mein", "Ce\u00b7la\u00b7don", "bleibt", "doch", "ge\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob schon die glut nicht funcken gibt/", "tokens": ["Ob", "schon", "die", "glut", "nicht", "fun\u00b7cken", "gibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKNEG", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und er mich nicht kan sehen brennen.", "tokens": ["Und", "er", "mich", "nicht", "kan", "se\u00b7hen", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "PTKNEG", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zeigt mein beseeltes helffenbein", "tokens": ["Zeigt", "mein", "be\u00b7seel\u00b7tes", "helf\u00b7fen\u00b7bein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gleich nicht der hellen flammen schein/", "tokens": ["Gleich", "nicht", "der", "hel\u00b7len", "flam\u00b7men", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "ADJA", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So wird man doch die asche kennen.", "tokens": ["So", "wird", "man", "doch", "die", "asc\u00b7he", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Macht gleich mein unverdienter mund", "tokens": ["Macht", "gleich", "mein", "un\u00b7ver\u00b7dien\u00b7ter", "mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nicht meiner seelen regung kund/", "tokens": ["Nicht", "mei\u00b7ner", "see\u00b7len", "re\u00b7gung", "kund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schwebt brunst und gunst nicht auf der stirne;", "tokens": ["Schwebt", "brunst", "und", "gunst", "nicht", "auf", "der", "stir\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "ADV", "PTKNEG", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So kan ich doch bey stiller nacht/", "tokens": ["So", "kan", "ich", "doch", "bey", "stil\u00b7ler", "nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn nichts als meine liebe wacht/", "tokens": ["Wenn", "nichts", "als", "mei\u00b7ne", "lie\u00b7be", "wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KOKOM", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein bildni\u00df k\u00fc\u00dfen im gehirne.", "tokens": ["Dein", "bild\u00b7ni\u00df", "k\u00fc\u00b7\u00dfen", "im", "ge\u00b7hir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "H\u00f6rt gleich mein Celadon itzt nicht/", "tokens": ["H\u00f6rt", "gleich", "mein", "Ce\u00b7la\u00b7don", "itzt", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN", "ADV", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was mein verliebtes hertze spricht", "tokens": ["Was", "mein", "ver\u00b7lieb\u00b7tes", "hert\u00b7ze", "spricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und meine treue seele bringet/", "tokens": ["Und", "mei\u00b7ne", "treu\u00b7e", "see\u00b7le", "brin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wei\u00df schon die meisterin der zucht/", "tokens": ["Wei\u00df", "schon", "die", "meis\u00b7te\u00b7rin", "der", "zucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df meine brunst die ma\u00dfe sucht/", "tokens": ["Da\u00df", "mei\u00b7ne", "brunst", "die", "ma\u00b7\u00dfe", "sucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sich mein reines auge zwinget.", "tokens": ["Und", "sich", "mein", "rei\u00b7nes", "au\u00b7ge", "zwin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "So hof ich dennoch auf die zeit/", "tokens": ["So", "hof", "ich", "den\u00b7noch", "auf", "die", "zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sich der glieder zanck und streit", "tokens": ["Da", "sich", "der", "glie\u00b7der", "zanck", "und", "streit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In fried und freyheit wird verkehren;", "tokens": ["In", "fried", "und", "frey\u00b7heit", "wird", "ver\u00b7keh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und da\u00df des hertzens s\u00fcsser brand/", "tokens": ["Und", "da\u00df", "des", "hert\u00b7zens", "s\u00fcs\u00b7ser", "brand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des willens unbequ\u00e4mes band", "tokens": ["Des", "wil\u00b7lens", "un\u00b7be\u00b7qu\u00e4\u00b7mes", "band"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch seine flammen wird verzehren.", "tokens": ["Durch", "sei\u00b7ne", "flam\u00b7men", "wird", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVINF", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und solte meiner jugend pracht/", "tokens": ["Und", "sol\u00b7te", "mei\u00b7ner", "ju\u00b7gend", "pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dieses was mich lieblich macht/", "tokens": ["Und", "die\u00b7ses", "was", "mich", "lieb\u00b7lich", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PRELS", "PRF", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das grab mit aller hofnung haben;", "tokens": ["Das", "grab", "mit", "al\u00b7ler", "hof\u00b7nung", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wei\u00df ich doch/ da\u00df jederman", "tokens": ["So", "wei\u00df", "ich", "doch", "/", "da\u00df", "je\u00b7der\u00b7man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von meinem hertzen sagen kan:", "tokens": ["Von", "mei\u00b7nem", "hert\u00b7zen", "sa\u00b7gen", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Alhier liegt Celadon begraben.", "tokens": ["Al\u00b7hier", "liegt", "Ce\u00b7la\u00b7don", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}