{"textgrid.poem.65243": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Trinker von Gottes und Rechts wegen", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hatt' in meiner Mutter Leib", "tokens": ["Ich", "hatt'", "in", "mei\u00b7ner", "Mut\u00b7ter", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gewohnt ein halbes Jahr,", "tokens": ["Ge\u00b7wohnt", "ein", "hal\u00b7bes", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sprang zu hoch das junge Weib,", "tokens": ["Da", "sprang", "zu", "hoch", "das", "jun\u00b7ge", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKA", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dacht' nicht an die Gefahr.", "tokens": ["Dacht'", "nicht", "an", "die", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Auf einem Weinberg tanzte sie", "tokens": ["Auf", "ei\u00b7nem", "Wein\u00b7berg", "tanz\u00b7te", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bei einem Winzerfest;", "tokens": ["Bei", "ei\u00b7nem", "Win\u00b7zer\u00b7fest", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das R\u00f6cklein flog bis an die Knie',", "tokens": ["Das", "R\u00f6ck\u00b7lein", "flog", "bis", "an", "die", "Knie'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Mieder sa\u00df nicht fest.", "tokens": ["Das", "Mie\u00b7der", "sa\u00df", "nicht", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da roch ich was von Rebensaft,", "tokens": ["Da", "roch", "ich", "was", "von", "Re\u00b7ben\u00b7saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da h\u00f6rt' ich Gl\u00e4serklang,", "tokens": ["Da", "h\u00f6rt'", "ich", "Gl\u00e4\u00b7ser\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und flugs heraus aus meiner Haft", "tokens": ["Und", "flugs", "he\u00b7raus", "aus", "mei\u00b7ner", "Haft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sprang ich in wildem Drang.", "tokens": ["Sprang", "ich", "in", "wil\u00b7dem", "Drang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie legten mich auf Rebenlaub,", "tokens": ["Sie", "leg\u00b7ten", "mich", "auf", "Re\u00b7ben\u00b7laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie sprengten mich mit Wein,", "tokens": ["Sie", "spreng\u00b7ten", "mich", "mit", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich blieb nicht blind und stumm und taub,", "tokens": ["Ich", "blieb", "nicht", "blind", "und", "stumm", "und", "taub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und sog die Tropfen ein.", "tokens": ["Und", "sog", "die", "Trop\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Schenkwirth war mein Herr Papa,", "tokens": ["Ein", "Schenk\u00b7wirth", "war", "mein", "Herr", "Pa\u00b7pa", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Go\u00df immer ein und aus.", "tokens": ["Go\u00df", "im\u00b7mer", "ein", "und", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Wasser stand dem Weine nah", "tokens": ["Das", "Was\u00b7ser", "stand", "dem", "Wei\u00b7ne", "nah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Allzeit in seinem Haus.", "tokens": ["All\u00b7zeit", "in", "sei\u00b7nem", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und als der Pfaff nach Wasser rief',", "tokens": ["Und", "als", "der", "Pfaff", "nach", "Was\u00b7ser", "rie\u00b7f'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NE", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Da\u00df er mich taufte drein,", "tokens": ["Da\u00df", "er", "mich", "tauf\u00b7te", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Vater sich in Eil' verlief", "tokens": ["Mein", "Va\u00b7ter", "sich", "in", "Eil'", "ver\u00b7lief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PRF", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und brachte blanken Wein.", "tokens": ["Und", "brach\u00b7te", "blan\u00b7ken", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Damit bego\u00df der heil'ge Mann", "tokens": ["Da\u00b7mit", "be\u00b7go\u00df", "der", "heil'\u00b7ge", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Haupt und mein Gesicht,", "tokens": ["Mein", "Haupt", "und", "mein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sprach dazu den Segen dann,", "tokens": ["Und", "sprach", "da\u00b7zu", "den", "Se\u00b7gen", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich schrie und muckte nicht.", "tokens": ["Ich", "schrie", "und", "muck\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In sel'gem Rausche lag ich da", "tokens": ["In", "sel'\u00b7gem", "Rau\u00b7sche", "lag", "ich", "da"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den ganzen lieben Tag;", "tokens": ["Den", "gan\u00b7zen", "lie\u00b7ben", "Tag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie glaubten schon mein Ende nah,", "tokens": ["Sie", "glaub\u00b7ten", "schon", "mein", "En\u00b7de", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da ward ich jauchzend wach.", "tokens": ["Da", "ward", "ich", "jauch\u00b7zend", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und als ich lernte selber stehn,", "tokens": ["Und", "als", "ich", "lern\u00b7te", "sel\u00b7ber", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trieb ich's, wie mein Papa:", "tokens": ["Trieb", "ich's", ",", "wie", "mein", "Pa\u00b7pa", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOKOM", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sollt' ich zum Wasserfasse gehn,", "tokens": ["Sollt'", "ich", "zum", "Was\u00b7ser\u00b7fas\u00b7se", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar oft ich mich versah,", "tokens": ["Gar", "oft", "ich", "mich", "ver\u00b7sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und sch\u00f6pfte nebenbei heraus", "tokens": ["Und", "sch\u00f6pf\u00b7te", "ne\u00b7ben\u00b7bei", "he\u00b7raus"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nebenbei hinein;", "tokens": ["Und", "ne\u00b7ben\u00b7bei", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich war der einz'ge Gast im Haus,", "tokens": ["Ich", "war", "der", "einz'\u00b7ge", "Gast", "im", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der zechte reinen Wein.", "tokens": ["Der", "zech\u00b7te", "rei\u00b7nen", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und nun, ihr Leute, sagt mir an,", "tokens": ["Und", "nun", ",", "ihr", "Leu\u00b7te", ",", "sagt", "mir", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sollt' es anders sein,", "tokens": ["Wie", "sollt'", "es", "an\u00b7ders", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als da\u00df mein Mund nichts trinken kann,", "tokens": ["Als", "da\u00df", "mein", "Mund", "nichts", "trin\u00b7ken", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPOSAT", "NN", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als guten reinen Wein?", "tokens": ["Als", "gu\u00b7ten", "rei\u00b7nen", "Wein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er ist's, der vor der Zeit mich rief", "tokens": ["Er", "ist's", ",", "der", "vor", "der", "Zeit", "mich", "rief"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In diese Welt heraus;", "tokens": ["In", "die\u00b7se", "Welt", "he\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "W\u00e4r' er nicht mehr, f\u00fcrwahr, ich lief'", "tokens": ["W\u00e4r'", "er", "nicht", "mehr", ",", "f\u00fcr\u00b7wahr", ",", "ich", "lie\u00b7f'"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "$,", "ADV", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Auch vor der Zeit hinaus.", "tokens": ["Auch", "vor", "der", "Zeit", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er ist es auch, der mich hernach", "tokens": ["Er", "ist", "es", "auch", ",", "der", "mich", "her\u00b7nach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "PRELS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Christen hat gemacht,", "tokens": ["Zum", "Chris\u00b7ten", "hat", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das hab' ich mir so manchen Tag.", "tokens": ["Das", "hab'", "ich", "mir", "so", "man\u00b7chen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fein christlich \u00fcberdacht.", "tokens": ["Fein", "christ\u00b7lich", "\u00fc\u00b7berd\u00b7acht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und weil's muhamedanisch ist,", "tokens": ["Und", "weil's", "mu\u00b7ha\u00b7me\u00b7da\u00b7nisch", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu trinken keinen Wein,", "tokens": ["Zu", "trin\u00b7ken", "kei\u00b7nen", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Will ich beim Wein ein guter Christ", "tokens": ["Will", "ich", "beim", "Wein", "ein", "gu\u00b7ter", "Christ"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Trotz T\u00fcrk' und Teufel sein!", "tokens": ["Trotz", "T\u00fcrk'", "und", "Teu\u00b7fel", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich hatt' in meiner Mutter Leib", "tokens": ["Ich", "hatt'", "in", "mei\u00b7ner", "Mut\u00b7ter", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gewohnt ein halbes Jahr,", "tokens": ["Ge\u00b7wohnt", "ein", "hal\u00b7bes", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sprang zu hoch das junge Weib,", "tokens": ["Da", "sprang", "zu", "hoch", "das", "jun\u00b7ge", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKA", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dacht' nicht an die Gefahr.", "tokens": ["Dacht'", "nicht", "an", "die", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Auf einem Weinberg tanzte sie", "tokens": ["Auf", "ei\u00b7nem", "Wein\u00b7berg", "tanz\u00b7te", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bei einem Winzerfest;", "tokens": ["Bei", "ei\u00b7nem", "Win\u00b7zer\u00b7fest", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das R\u00f6cklein flog bis an die Knie',", "tokens": ["Das", "R\u00f6ck\u00b7lein", "flog", "bis", "an", "die", "Knie'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Mieder sa\u00df nicht fest.", "tokens": ["Das", "Mie\u00b7der", "sa\u00df", "nicht", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da roch ich was von Rebensaft,", "tokens": ["Da", "roch", "ich", "was", "von", "Re\u00b7ben\u00b7saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da h\u00f6rt' ich Gl\u00e4serklang,", "tokens": ["Da", "h\u00f6rt'", "ich", "Gl\u00e4\u00b7ser\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und flugs heraus aus meiner Haft", "tokens": ["Und", "flugs", "he\u00b7raus", "aus", "mei\u0