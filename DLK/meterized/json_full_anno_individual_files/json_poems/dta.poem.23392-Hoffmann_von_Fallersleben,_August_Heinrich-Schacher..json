{"dta.poem.23392": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schacher.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sieht, ihr wollt nur Honorare,", "tokens": ["Man", "sieht", ",", "ihr", "wollt", "nur", "Ho\u00b7no\u00b7ra\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VMFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man sieht's aus allem was ihr schreibt;", "tokens": ["Man", "sieht's", "aus", "al\u00b7lem", "was", "ihr", "schreibt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PIS", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Freiheit ist euch eine Waare,", "tokens": ["Die", "Frei\u00b7heit", "ist", "euch", "ei\u00b7ne", "Waa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Womit ihr nur Gesch\u00e4fte treibt.", "tokens": ["Wo\u00b7mit", "ihr", "nur", "Ge\u00b7sch\u00e4f\u00b7te", "treibt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr la\u00dft um euer lumpig Ichlein", "tokens": ["Ihr", "la\u00dft", "um", "eu\u00b7er", "lum\u00b7pig", "I\u00b7chlein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Welt sich drehn bei Tag und Nacht;", "tokens": ["Die", "Welt", "sich", "drehn", "bei", "Tag", "und", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "CARD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr Freiheit macht ihr nicht ein Strichlein,", "tokens": ["F\u00fcr", "Frei\u00b7heit", "macht", "ihr", "nicht", "ein", "Strich\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ihr's zugleich f\u00fcr euch nicht macht.", "tokens": ["Wenn", "ih\u00b7r's", "zu\u00b7gleich", "f\u00fcr", "euch", "nicht", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und liegt die Freiheit auf der Bahre,", "tokens": ["Und", "liegt", "die", "Frei\u00b7heit", "auf", "der", "Bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dann lebet ", "tokens": ["Dann", "le\u00b7bet"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Dann habt ihr eine andre Waare,", "tokens": ["Dann", "habt", "ihr", "ei\u00b7ne", "and\u00b7re", "Waa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Womit ihr noch Gesch\u00e4fte treibt.", "tokens": ["Wo\u00b7mit", "ihr", "noch", "Ge\u00b7sch\u00e4f\u00b7te", "treibt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}