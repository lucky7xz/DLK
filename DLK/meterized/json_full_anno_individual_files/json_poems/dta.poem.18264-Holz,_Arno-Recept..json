{"dta.poem.18264": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Recept.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "So sprich, was ist zum Dichten n\u00fctze?", "tokens": ["So", "sprich", ",", "was", "ist", "zum", "Dich\u00b7ten", "n\u00fct\u00b7ze", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWS", "VAFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Eine Perryfeder, ein Bogen Papier,", "tokens": ["Ei\u00b7ne", "Per\u00b7ry\u00b7fe\u00b7der", ",", "ein", "Bo\u00b7gen", "Pa\u00b7pier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ein Tintenfa\u00df \u2014 und ein Sch\u00e4del voll Gr\u00fctze!", "tokens": ["Ein", "Tin\u00b7ten\u00b7fa\u00df", "und", "ein", "Sch\u00e4\u00b7del", "voll", "Gr\u00fct\u00b7ze", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}