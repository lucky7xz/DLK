{"textgrid.poem.51140": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die \u00f6ffentlichen Verleumder", "genre": "verse", "period": "N.A.", "pub_year": 1878, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Ungeziefer ruht", "tokens": ["Ein", "Un\u00b7ge\u00b7zie\u00b7fer", "ruht"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In Staub und trocknem Schlamme", "tokens": ["In", "Staub", "und", "trock\u00b7nem", "Schlam\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verborgen, wie die Flamme", "tokens": ["Ver\u00b7bor\u00b7gen", ",", "wie", "die", "Flam\u00b7me"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In leichter Asche tut.", "tokens": ["In", "leich\u00b7ter", "A\u00b7sche", "tut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Regen, Windeshauch", "tokens": ["Ein", "Re\u00b7gen", ",", "Win\u00b7des\u00b7hauch"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Erweckt das schlimme Leben,", "tokens": ["Er\u00b7weckt", "das", "schlim\u00b7me", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und aus dem Nichts erheben", "tokens": ["Und", "aus", "dem", "Nichts", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich Seuchen, Glut und Rauch.", "tokens": ["Sich", "Seu\u00b7chen", ",", "Glut", "und", "Rauch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Aus dunkler H\u00f6hle f\u00e4hrt", "tokens": ["Aus", "dunk\u00b7ler", "H\u00f6h\u00b7le", "f\u00e4hrt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Sch\u00e4cher, um zu schweifen;", "tokens": ["Ein", "Sch\u00e4\u00b7cher", ",", "um", "zu", "schwei\u00b7fen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nach Beuteln m\u00f6cht er greifen", "tokens": ["Nach", "Beu\u00b7teln", "m\u00f6cht", "er", "grei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und findet bessern Wert:", "tokens": ["Und", "fin\u00b7det", "bes\u00b7sern", "Wert", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er findet einen Streit", "tokens": ["Er", "fin\u00b7det", "ei\u00b7nen", "Streit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Um nichts, ein irres Wissen,", "tokens": ["Um", "nichts", ",", "ein", "ir\u00b7res", "Wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PIS", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein Banner, das zerrissen,", "tokens": ["Ein", "Ban\u00b7ner", ",", "das", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Volk in Bl\u00f6digkeit.", "tokens": ["Ein", "Volk", "in", "Bl\u00f6\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er findet, wo er geht,", "tokens": ["Er", "fin\u00b7det", ",", "wo", "er", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Leere d\u00fcrft'ger Zeiten,", "tokens": ["Die", "Lee\u00b7re", "d\u00fcrft'\u00b7ger", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da kann er schamlos schreiten,", "tokens": ["Da", "kann", "er", "scham\u00b7los", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun wird er ein Prophet;", "tokens": ["Nun", "wird", "er", "ein", "Pro\u00b7phet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auf einen Kehricht stellt", "tokens": ["Auf", "ei\u00b7nen", "Keh\u00b7richt", "stellt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Er seine Schelmenf\u00fc\u00dfe", "tokens": ["Er", "sei\u00b7ne", "Schel\u00b7men\u00b7f\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und zischelt seine Gr\u00fc\u00dfe", "tokens": ["Und", "zi\u00b7schelt", "sei\u00b7ne", "Gr\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In die verbl\u00fcffte Welt.", "tokens": ["In", "die", "ver\u00b7bl\u00fcff\u00b7te", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Geh\u00fcllt in Niedertracht,", "tokens": ["Ge\u00b7h\u00fcllt", "in", "Nie\u00b7der\u00b7tracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gleichwie in einer Wolke,", "tokens": ["Gleich\u00b7wie", "in", "ei\u00b7ner", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein L\u00fcgner vor dem Volke,", "tokens": ["Ein", "L\u00fcg\u00b7ner", "vor", "dem", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ragt bald er gro\u00df an Macht", "tokens": ["Ragt", "bald", "er", "gro\u00df", "an", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "ADJD", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit seiner Helfer Zahl,", "tokens": ["Mit", "sei\u00b7ner", "Hel\u00b7fer", "Zahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die, hoch und niedrig stehend,", "tokens": ["Die", ",", "hoch", "und", "nied\u00b7rig", "ste\u00b7hend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gelegenheit ersp\u00e4hend,", "tokens": ["Ge\u00b7le\u00b7gen\u00b7heit", "er\u00b7sp\u00e4\u00b7hend", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich bieten seiner Wahl.", "tokens": ["Sich", "bie\u00b7ten", "sei\u00b7ner", "Wahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie teilen aus sein Wort,", "tokens": ["Sie", "tei\u00b7len", "aus", "sein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie einst die Gottesboten", "tokens": ["Wie", "einst", "die", "Got\u00b7tes\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Getan mit den f\u00fcnf Broten,", "tokens": ["Ge\u00b7tan", "mit", "den", "f\u00fcnf", "Bro\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das klecket fort und fort!", "tokens": ["Das", "kle\u00b7cket", "fort", "und", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Erst log allein der Hund,", "tokens": ["Erst", "log", "al\u00b7lein", "der", "Hund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nun l\u00fcgen ihrer tausend;", "tokens": ["Nun", "l\u00fc\u00b7gen", "ih\u00b7rer", "tau\u00b7send", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "CARD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wie ein Sturm erbrausend,", "tokens": ["Und", "wie", "ein", "Sturm", "er\u00b7brau\u00b7send", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So wuchert jetzt sein Pfund.", "tokens": ["So", "wu\u00b7chert", "jetzt", "sein", "Pfund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Hoch schie\u00dft empor die Saat,", "tokens": ["Hoch", "schie\u00dft", "em\u00b7por", "die", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Verwandelt sind die Lande,", "tokens": ["Ver\u00b7wan\u00b7delt", "sind", "die", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Menge lebt in Schande", "tokens": ["Die", "Men\u00b7ge", "lebt", "in", "Schan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und lacht der Schofeltat!", "tokens": ["Und", "lacht", "der", "Scho\u00b7fel\u00b7tat", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jetzt hat sich auch erwahrt,", "tokens": ["Jetzt", "hat", "sich", "auch", "er\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Was erstlich war erfunden:", "tokens": ["Was", "erst\u00b7lich", "war", "er\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Guten sind verschwunden,", "tokens": ["Die", "Gu\u00b7ten", "sind", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Schlechten stehn geschart!", "tokens": ["Die", "Schlech\u00b7ten", "stehn", "ge\u00b7schart", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wenn einstmals diese Not", "tokens": ["Wenn", "einst\u00b7mals", "die\u00b7se", "Not"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Lang wie ein Eis gebrochen,", "tokens": ["Lang", "wie", "ein", "Eis", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann wird davon gesprochen", "tokens": ["Dann", "wird", "da\u00b7von", "ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PAV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie von dem schwarzen Tod;", "tokens": ["Wie", "von", "dem", "schwar\u00b7zen", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und einen Strohmann baun", "tokens": ["Und", "ei\u00b7nen", "Stroh\u00b7mann", "baun"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Kinder auf der Heide,", "tokens": ["Die", "Kin\u00b7der", "auf", "der", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zu brennen Lust aus Leide", "tokens": ["Zu", "bren\u00b7nen", "Lust", "aus", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Licht aus altem Graun.", "tokens": ["Und", "Licht", "aus", "al\u00b7tem", "Graun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ein Ungeziefer ruht", "tokens": ["Ein", "Un\u00b7ge\u00b7zie\u00b7fer", "ruht"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In Staub und trocknem Schlamme", "tokens": ["In", "Staub", "und", "trock\u00b7nem", "Schlam\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verborgen, wie die Flamme", "tokens": ["Ver\u00b7bor\u00b7gen", ",", "wie", "die", "Flam\u00b7me"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In leichter Asche tut.", "tokens": ["In", "leich\u00b7ter", "A\u00b7sche", "tut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Regen, Windeshauch", "tokens": ["Ein", "Re\u00b7gen", ",", "Win\u00b7des\u00b7hauch"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Erweckt das schlimme Leben,", "tokens": ["Er\u00b7weckt", "das", "schlim\u00b7me", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und aus dem Nichts erheben", "tokens": ["Und", "aus", "dem", "Nichts", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich Seuchen, Glut und Rauch.", "tokens": ["Sich", "Seu\u00b7chen", ",", "Glut", "und", "Rauch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Aus dunkler H\u00f6hle f\u00e4hrt", "tokens": ["Aus", "dunk\u00b7ler", "H\u00f6h\u00b7le", "f\u00e4hrt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Sch\u00e4cher, um zu schweifen;", "tokens": ["Ein", "Sch\u00e4\u00b7cher", ",", "um", "zu", "schwei\u00b7fen",