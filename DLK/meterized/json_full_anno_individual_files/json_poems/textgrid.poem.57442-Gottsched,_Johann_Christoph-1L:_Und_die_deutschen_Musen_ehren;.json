{"textgrid.poem.57442": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und die deutschen Musen ehren;", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und die deutschen Musen ehren;", "tokens": ["Und", "die", "deut\u00b7schen", "Mu\u00b7sen", "eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Darf die Glut, so in mir brennt,", "tokens": ["Darf", "die", "Glut", ",", "so", "in", "mir", "brennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$,", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deutschland ", "tokens": ["Deutschland"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Darf ich hier im Mei\u00dfnerlande,", "tokens": ["Darf", "ich", "hier", "im", "Mei\u00df\u00b7ner\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Welches Muld und Elbe netzt,", "tokens": ["Wel\u00b7ches", "Muld", "und", "El\u00b7be", "netzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "KON", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Melden, was am Donaustrande", "tokens": ["Mel\u00b7den", ",", "was", "am", "Do\u00b7naus\u00b7tran\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mit freyem Kiel es wagen", "tokens": ["Und", "mit", "frey\u00b7em", "Kiel", "es", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "PPER", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Ja! ich wag es. Ph\u00f6bus winkt,", "tokens": ["Ja", "!", "ich", "wag", "es", ".", "Ph\u00f6\u00b7bus", "winkt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "PPER", "$.", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und gebeut mir, nicht zu schweigen.", "tokens": ["Und", "ge\u00b7beut", "mir", ",", "nicht", "zu", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Klio will mir, wie mich d\u00fcnkt,", "tokens": ["Klio", "will", "mir", ",", "wie", "mich", "d\u00fcnkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Singe, ruft sie mit Entz\u00fccken,", "tokens": ["Sin\u00b7ge", ",", "ruft", "sie", "mit", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Singe von den heitern Blicken,", "tokens": ["Sin\u00b7ge", "von", "den", "hei\u00b7tern", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und von ", "tokens": ["Und", "von"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Wenn der ", "tokens": ["Wenn", "der"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Mund und Kiel zu Rednern dienen.", "tokens": ["Mund", "und", "Kiel", "zu", "Red\u00b7nern", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Holde Muse, gib mirs ein!", "tokens": ["Hol\u00b7de", "Mu\u00b7se", ",", "gib", "mirs", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lehre mich so Sylb als Reime;", "tokens": ["Leh\u00b7re", "mich", "so", "Sylb", "als", "Rei\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "NE", "KOUS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn mein Witz ist sonst zu klein,", "tokens": ["Denn", "mein", "Witz", "ist", "sonst", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich leicht die Pflicht vers\u00e4ume.", "tokens": ["Da\u00df", "ich", "leicht", "die", "Pflicht", "ver\u00b7s\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Duldet kein gemeines Lied:", "tokens": ["Dul\u00b7det", "kein", "ge\u00b7mei\u00b7nes", "Lied", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie man ihn nicht h\u00e4ufig sieht;", "tokens": ["Wie", "man", "ihn", "nicht", "h\u00e4u\u00b7fig", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und vor ", "tokens": ["Und", "vor"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Kann nichts schlechtes Platz gewinnen.", "tokens": ["Kann", "nichts", "schlech\u00b7tes", "Platz", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Regt sich Pallas nicht in ", "tokens": ["Regt", "sich", "Pal\u00b7las", "nicht", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "NE", "PTKNEG", "APPR"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Und mit lechzender Begier", "tokens": ["Und", "mit", "lech\u00b7zen\u00b7der", "Be\u00b7gier"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kluger Schriften Vorzug merket?", "tokens": ["Klu\u00b7ger", "Schrif\u00b7ten", "Vor\u00b7zug", "mer\u00b7ket", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.5": {"text": "Pr\u00fcft der B\u00fccher wahren Werth;", "tokens": ["Pr\u00fcft", "der", "B\u00fc\u00b7cher", "wah\u00b7ren", "Werth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Weis das Innre zu ergr\u00fcnden,", "tokens": ["Weis", "das", "Inn\u00b7re", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das sich Kennern nur erkl\u00e4rt.", "tokens": ["Das", "sich", "Ken\u00b7nern", "nur", "er\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ja ", "tokens": ["Ja"], "token_info": ["word"], "pos": ["PTKANT"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "In der Menschen Herz zu dringen.", "tokens": ["In", "der", "Men\u00b7schen", "Herz", "zu", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Drum hat ", "tokens": ["Drum", "hat"], "token_info": ["word", "word"], "pos": ["PAV", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Deutschlands ", "tokens": ["Deutschlands"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Eh ", "tokens": ["Eh"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Doch der ", "tokens": ["Doch", "der"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Fand ", "tokens": ["Fand"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Fand ", "tokens": ["Fand"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Und von seltner Eigenschaft;", "tokens": ["Und", "von", "selt\u00b7ner", "Ei\u00b7gen\u00b7schaft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kurz, die Zucht von theuren Zweigen", "tokens": ["Kurz", ",", "die", "Zucht", "von", "theu\u00b7ren", "Zwei\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Fand ", "tokens": ["Fand"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "So viel lehrt die Muse mich,", "tokens": ["So", "viel", "lehrt", "die", "Mu\u00b7se", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mehr wird Dir die\u00df Buch entdecken,", "tokens": ["Mehr", "wird", "Dir", "die\u00df", "Buch", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PDS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo, ", "tokens": ["Wo", ","], "token_info": ["word", "punct"], "pos": ["PWAV", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Deutsche Lieder nicht erschrecken.", "tokens": ["Deut\u00b7sche", "Lie\u00b7der", "nicht", "er\u00b7schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo mein Rohr ", "tokens": ["Wo", "mein", "Rohr"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Treuer Ehrfurcht Weihrauch streut;", "tokens": ["Treu\u00b7er", "Ehr\u00b7furcht", "Weih\u00b7rauch", "streut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ein Theil der Lorberreiser", "tokens": ["Und", "ein", "Theil", "der", "Lor\u00b7berr\u00b7ei\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00dft ein Blatt die Welt auch lehren,", "tokens": ["Mu\u00dft", "ein", "Blatt", "die", "Welt", "auch", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und die deutschen Musen ehren;", "tokens": ["Und", "die", "deut\u00b7schen", "Mu\u00b7sen", "eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Darf die Glut, so in mir brennt,", "tokens": ["Darf", "die", "Glut", ",", "so", "in", "mir", "brennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$,", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deutschland ", "tokens": ["Deutschland"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.4": {