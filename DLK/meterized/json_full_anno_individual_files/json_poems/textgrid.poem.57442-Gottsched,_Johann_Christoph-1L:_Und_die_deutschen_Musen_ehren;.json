, "pos": ["NN", "PPER", "ADV", "NE", "KOUS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn mein Witz ist sonst zu klein,", "tokens": ["Denn", "mein", "Witz", "ist", "sonst", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich leicht die Pflicht vers\u00e4ume.", "tokens": ["Da\u00df", "ich", "leicht", "die", "Pflicht", "ver\u00b7s\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Duldet kein gemeines Lied:", "tokens": ["Dul\u00b7det", "kein", "ge\u00b7mei\u00b7nes", "Lied", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie man ihn nicht h\u00e4ufig sieht;", "tokens": ["Wie", "man", "ihn", "nicht", "h\u00e4u\u00b7fig", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und vor ", "tokens": ["Und", "vor"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Kann nichts schlechtes Platz gewinnen.", "tokens": ["Kann", "nichts", "schlech\u00b7tes", "Platz", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10"