 "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mehr wird Dir die\u00df Buch entdecken,", "tokens": ["Mehr", "wird", "Dir", "die\u00df", "Buch", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PDS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo, ", "tokens": ["Wo", ","], "token_info": ["word", "punct"], "pos": ["PWAV", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Deutsche Lieder nicht erschrecken.", "tokens": ["Deut\u00b7sche", "Lie\u00b7der", "nicht", "er\u00b7schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo mein Rohr ", "tokens": ["Wo", "mein", "Rohr"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Treuer Ehrfurcht Weihrauch streut;", "tokens": ["Treu\u00b7er", "Ehr\u00b7furcht", "Weih\u00b7rauch", "streut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ein Theil der Lorberreiser", "tokens": ["Und", "ein", "Theil", "der", "Lor\u00b7berr\u00b7ei\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00dft ein Blatt die Welt auch lehren,", "tokens": ["Mu\u00dft", "ein", "Blatt", "die", "Welt", "auch", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NN",