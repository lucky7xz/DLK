{"textgrid.poem.26818": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00f6r ich das Pf\u00f6rtchen nicht gehen?", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6r ich das Pf\u00f6rtchen nicht gehen?", "tokens": ["H\u00f6r", "ich", "das", "Pf\u00f6rt\u00b7chen", "nicht", "ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hat nicht der Riegel geklirrt?", "tokens": ["Hat", "nicht", "der", "Rie\u00b7gel", "ge\u00b7klirrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nein, es war des Windes Wehen,", "tokens": ["Nein", ",", "es", "war", "des", "Win\u00b7des", "We\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der durch diese Pappeln schwirrt.", "tokens": ["Der", "durch", "die\u00b7se", "Pap\u00b7peln", "schwirrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O schm\u00fccke dich, du gr\u00fcn belaubtes Dach,", "tokens": ["O", "schm\u00fc\u00b7cke", "dich", ",", "du", "gr\u00fcn", "be\u00b7laub\u00b7tes", "Dach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du sollst die Anmutstrahlende empfangen,", "tokens": ["Du", "sollst", "die", "An\u00b7muts\u00b7trah\u00b7len\u00b7de", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Zweige, baut ein schattendes Gemach,", "tokens": ["Ihr", "Zwei\u00b7ge", ",", "baut", "ein", "schat\u00b7ten\u00b7des", "Ge\u00b7mach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit holder Nacht sie heimlich zu umfangen,", "tokens": ["Mit", "hol\u00b7der", "Nacht", "sie", "heim\u00b7lich", "zu", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und all ihr Schmeichell\u00fcfte, werdet wach", "tokens": ["Und", "all", "ihr", "Schmei\u00b7chel\u00b7l\u00fcf\u00b7te", ",", "wer\u00b7det", "wach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "$,", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und scherzt und spielt um ihre Rosenwangen,", "tokens": ["Und", "scherzt", "und", "spielt", "um", "ih\u00b7re", "Ro\u00b7sen\u00b7wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wenn seine sch\u00f6ne B\u00fcrde, leicht bewegt,", "tokens": ["Wenn", "sei\u00b7ne", "sch\u00f6\u00b7ne", "B\u00fcr\u00b7de", ",", "leicht", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der zarte Fu\u00df zum Sitz der Liebe tr\u00e4gt.", "tokens": ["Der", "zar\u00b7te", "Fu\u00df", "zum", "Sitz", "der", "Lie\u00b7be", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Stille, was schl\u00fcpft durch die Hecken", "tokens": ["Stil\u00b7le", ",", "was", "schl\u00fcpft", "durch", "die", "He\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Raschelnd mit eilendem Lauf?", "tokens": ["Ra\u00b7schelnd", "mit", "ei\u00b7len\u00b7dem", "Lauf", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Nein, es scheuchte nur der Schrecken", "tokens": ["Nein", ",", "es", "scheuch\u00b7te", "nur", "der", "Schre\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem Busch den Vogel auf.", "tokens": ["Aus", "dem", "Busch", "den", "Vo\u00b7gel", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O! l\u00f6sche deine Fackel, Tag! Hervor,", "tokens": ["O", "!", "l\u00f6\u00b7sche", "dei\u00b7ne", "Fa\u00b7ckel", ",", "Tag", "!", "Her\u00b7vor", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPOSAT", "NN", "$,", "NN", "$.", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du geistge Nacht, mit deinem holden Schweigen,", "tokens": ["Du", "geist\u00b7ge", "Nacht", ",", "mit", "dei\u00b7nem", "hol\u00b7den", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Breit um uns her den purpurroten Flor,", "tokens": ["Breit", "um", "uns", "her", "den", "pur\u00b7pur\u00b7ro\u00b7ten", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "APZR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Umspinn uns mit geheimnisvollen Zweigen,", "tokens": ["Um\u00b7spinn", "uns", "mit", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7len", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Liebe Wonne flieht des Lauschers Ohr,", "tokens": ["Der", "Lie\u00b7be", "Won\u00b7ne", "flieht", "des", "Lau\u00b7schers", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sie flieht des Strahles unbescheidnen Zeugen!", "tokens": ["Sie", "flieht", "des", "Strah\u00b7les", "un\u00b7be\u00b7scheid\u00b7nen", "Zeu\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nur Hesper, der verschwiegene, allein", "tokens": ["Nur", "Hes\u00b7per", ",", "der", "ver\u00b7schwie\u00b7ge\u00b7ne", ",", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "NN", "$,", "ART", "ADJA", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Darf still herblickend ihr Vertrauter sein.", "tokens": ["Darf", "still", "her\u00b7bli\u00b7ckend", "ihr", "Ver\u00b7trau\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVPP", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Rief es von ferne nicht leise,", "tokens": ["Rief", "es", "von", "fer\u00b7ne", "nicht", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Fl\u00fcsternden Stimmen gleich?", "tokens": ["Fl\u00fcs\u00b7tern\u00b7den", "Stim\u00b7men", "gleich", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Nein, der Schwan ists, der die Kreise", "tokens": ["Nein", ",", "der", "Schwan", "ists", ",", "der", "die", "Krei\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ziehet durch den Silberteich.", "tokens": ["Zie\u00b7het", "durch", "den", "Sil\u00b7ber\u00b7teich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Mein Ohr umt\u00f6nt ein Harmonienflu\u00df,", "tokens": ["Mein", "Ohr", "um\u00b7t\u00f6nt", "ein", "Har\u00b7mo\u00b7ni\u00b7en\u00b7flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Springquell f\u00e4llt mit angenehmem Rauschen,", "tokens": ["Der", "Spring\u00b7quell", "f\u00e4llt", "mit", "an\u00b7ge\u00b7neh\u00b7mem", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Blume neigt sich bei des Westes Ku\u00df,", "tokens": ["Die", "Blu\u00b7me", "neigt", "sich", "bei", "des", "Wes\u00b7tes", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und alle Wesen seh ich Wonne tauschen,", "tokens": ["Und", "al\u00b7le", "We\u00b7sen", "seh", "ich", "Won\u00b7ne", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Traube winkt, die Pfirsche zum Genu\u00df,", "tokens": ["Die", "Trau\u00b7be", "winkt", ",", "die", "Pfir\u00b7sche", "zum", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die \u00fcppig schwellend hinter Bl\u00e4ttern lauschen,", "tokens": ["Die", "\u00fcp\u00b7pig", "schwel\u00b7lend", "hin\u00b7ter", "Bl\u00e4t\u00b7tern", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Luft, getaucht in der Gew\u00fcrze Flut,", "tokens": ["Die", "Luft", ",", "ge\u00b7taucht", "in", "der", "Ge\u00b7w\u00fcr\u00b7ze", "Flut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Trinkt von der hei\u00dfen Wange mir die Glut.", "tokens": ["Trinkt", "von", "der", "hei\u00b7\u00dfen", "Wan\u00b7ge", "mir", "die", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "H\u00f6r ich nicht Tritte erschallen?", "tokens": ["H\u00f6r", "ich", "nicht", "Trit\u00b7te", "er\u00b7schal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Rauschts nicht den Laubgang daher?", "tokens": ["Rauschts", "nicht", "den", "Laub\u00b7gang", "da\u00b7her", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "PAV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Nein, die Frucht ist dort gefallen,", "tokens": ["Nein", ",", "die", "Frucht", "ist", "dort", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der eignen F\u00fclle schwer.", "tokens": ["Von", "der", "eig\u00b7nen", "F\u00fcl\u00b7le", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Des Tages Flammenauge selber bricht", "tokens": ["Des", "Ta\u00b7ges", "Flam\u00b7men\u00b7au\u00b7ge", "sel\u00b7ber", "bricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In s\u00fc\u00dfem Tod und seine Farben blassen,", "tokens": ["In", "s\u00fc\u00b7\u00dfem", "Tod", "und", "sei\u00b7ne", "Far\u00b7ben", "blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "K\u00fchn \u00f6ffnen sich im holden D\u00e4mmerlicht", "tokens": ["K\u00fchn", "\u00f6ff\u00b7nen", "sich", "im", "hol\u00b7den", "D\u00e4m\u00b7mer\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Kelche schon, die seine Gluten hassen,", "tokens": ["Die", "Kel\u00b7che", "schon", ",", "die", "sei\u00b7ne", "Glu\u00b7ten", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Still hebt der Mond sein strahlend Angesicht,", "tokens": ["Still", "hebt", "der", "Mond", "sein", "strah\u00b7lend", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Welt zerschmilzt in ruhig gro\u00dfe Massen,", "tokens": ["Die", "Welt", "zer\u00b7schmilzt", "in", "ru\u00b7hig", "gro\u00b7\u00dfe", "Mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der G\u00fcrtel ist von jedem Reiz gel\u00f6st,", "tokens": ["Der", "G\u00fcr\u00b7tel", "ist", "von", "je\u00b7dem", "Reiz", "ge\u00b7l\u00f6st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und alles Sch\u00f6ne zeigt sich mir entbl\u00f6\u00dft.", "tokens": ["Und", "al\u00b7les", "Sch\u00f6\u00b7ne", "zeigt", "sich", "mir", "ent\u00b7bl\u00f6\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PRF", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Seh ich nichts Wei\u00dfes dort schimmern?", "tokens": ["Seh", "ich", "nichts", "Wei\u00b7\u00dfes", "dort", "schim\u00b7mern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "NN", "ADV", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Gl\u00e4nzts nicht wie seidnes Gewand?", "tokens": ["Gl\u00e4nzts", "nicht", "wie", "seid\u00b7nes", "Ge\u00b7wand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nein, es ist der S\u00e4ule Flimmern", "tokens": ["Nein", ",", "es", "ist", "der", "S\u00e4u\u00b7le", "Flim\u00b7mern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An der dunkeln Taxuswand.", "tokens": ["An", "der", "dun\u00b7keln", "Ta\u00b7xus\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "O! sehnend Herz, erg\u00f6tze dich nicht mehr,", "tokens": ["O", "!", "seh\u00b7nend", "Herz", ",", "er\u00b7g\u00f6t\u00b7ze", "dich", "nicht", "mehr", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit s\u00fc\u00dfen Bildern wesenlos zu spielen,", "tokens": ["Mit", "s\u00fc\u00b7\u00dfen", "Bil\u00b7dern", "we\u00b7sen\u00b7los", "zu", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Arm, der sie umfassen will, ist leer,", "tokens": ["Der", "Arm", ",", "der", "sie", "um\u00b7fas\u00b7sen", "will", ",", "ist", "leer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kein Schattengl\u00fcck kann diesen Busen k\u00fchlen;", "tokens": ["Kein", "Schat\u00b7ten\u00b7gl\u00fcck", "kann", "die\u00b7sen", "Bu\u00b7sen", "k\u00fch\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O! f\u00fchre mir die Lebende daher,", "tokens": ["O", "!", "f\u00fch\u00b7re", "mir", "die", "Le\u00b7ben\u00b7de", "da\u00b7her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.6": {"text": "La\u00df ihre Hand, die z\u00e4rtliche, mich f\u00fchlen,", "tokens": ["La\u00df", "ih\u00b7re", "Hand", ",", "die", "z\u00e4rt\u00b7li\u00b7che", ",", "mich", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Den Schatten nur von ihres Mantels Saum,", "tokens": ["Den", "Schat\u00b7ten", "nur", "von", "ih\u00b7res", "Man\u00b7tels", "Saum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und in das Leben tritt der hohle Traum.", "tokens": ["Und", "in", "das", "Le\u00b7ben", "tritt", "der", "hoh\u00b7le", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Und leis, wie aus himmlischen H\u00f6hen", "tokens": ["Und", "leis", ",", "wie", "aus", "himm\u00b7li\u00b7schen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amp