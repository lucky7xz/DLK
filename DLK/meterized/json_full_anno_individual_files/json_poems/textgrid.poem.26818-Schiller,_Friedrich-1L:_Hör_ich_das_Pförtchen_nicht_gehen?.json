, "word"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "$,", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und scherzt und spielt um ihre Rosenwangen,", "tokens": ["Und", "scherzt", "und", "spielt", "um", "ih\u00b7re", "Ro\u00b7sen\u00b7wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wenn seine sch\u00f6ne B\u00fcrde, leicht bewegt,", "tokens": ["Wenn", "sei\u00b7ne", "sch\u00f6\u00b7ne", "B\u00fcr\u00b7de", ",", "leicht", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der zarte Fu\u00df zum Sitz der Liebe tr\u00e4gt.", "tokens": ["Der", "zar\u00b7te", "Fu\u00df", "zum", "Sitz", "der", "Lie\u00b7be", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Stille, was schl\u00fcpft durch die Hecken", "tokens": ["Stil\u00b7le", ",", "was", "schl\u00fcpft", "durch", "die", "He\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Raschelnd mit eilendem Lauf?", "tokens": ["Ra\u00b7schelnd", "mit", "ei\u00b7len\u00b7dem", "Lauf", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Nein, es scheuchte nur der Schrecken", "tokens": ["Nein", ",", "es", "scheuch