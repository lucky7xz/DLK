{"textgrid.poem.49442": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[es pfeift der Nord, die B\u00fcsche sind]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es pfeift der Nord, die B\u00fcsche sind", "tokens": ["Es", "pfeift", "der", "Nord", ",", "die", "B\u00fc\u00b7sche", "sind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tiefschwarz und gr\u00fcn im scharfen Wind", "tokens": ["Tief\u00b7schwarz", "und", "gr\u00fcn", "im", "schar\u00b7fen", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eis wird der Schnee, dess weisse Flecken", "tokens": ["Eis", "wird", "der", "Schnee", ",", "dess", "weis\u00b7se", "Fle\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die sonnenhellen Lande decken.", "tokens": ["Die", "son\u00b7nen\u00b7hel\u00b7len", "Lan\u00b7de", "de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vom Wald her weht ein herber Duft,", "tokens": ["Vom", "Wald", "her", "weht", "ein", "her\u00b7ber", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Horizonte singt die Luft.", "tokens": ["Am", "Ho\u00b7ri\u00b7zon\u00b7te", "singt", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Dorfes Kirchturmh\u00e4hne blenden", "tokens": ["Des", "Dor\u00b7fes", "Kirch\u00b7turm\u00b7h\u00e4h\u00b7ne", "blen\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das Aug' vor dunklen Wolkenw\u00e4nden.", "tokens": ["Das", "Aug'", "vor", "dunk\u00b7len", "Wol\u00b7ken\u00b7w\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wie herrlich ist es, so zu gehn", "tokens": ["Wie", "herr\u00b7lich", "ist", "es", ",", "so", "zu", "gehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$,", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In Nebelschleiern, die verwehn,", "tokens": ["In", "Ne\u00b7bel\u00b7schlei\u00b7ern", ",", "die", "ver\u00b7wehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wann Winde keck die Flur durchstreichen.", "tokens": ["Wann", "Win\u00b7de", "keck", "die", "Flur", "durch\u00b7strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "O pfui, mein Husten will nicht weichen!", "tokens": ["O", "pfui", ",", "mein", "Hus\u00b7ten", "will", "nicht", "wei\u