{"dta.poem.7478": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Pferdeausfuhrverbot.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir m\u00fcssen uns bei Zeiten t\u00fcchtig r\u00fchren,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "uns", "bei", "Zei\u00b7ten", "t\u00fcch\u00b7tig", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und k\u00f6nnen drum, trotz manchem sch\u00f6nen Gulden,", "tokens": ["Und", "k\u00f6n\u00b7nen", "drum", ",", "trotz", "man\u00b7chem", "sch\u00f6\u00b7nen", "Gul\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PAV", "$,", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Getreue Unterthanen, nimmer dulden,", "tokens": ["Ge\u00b7treu\u00b7e", "Un\u00b7ter\u00b7tha\u00b7nen", ",", "nim\u00b7mer", "dul\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df Franken Eure Pferde uns entf\u00fchren.", "tokens": ["Da\u00df", "Fran\u00b7ken", "Eu\u00b7re", "Pfer\u00b7de", "uns", "ent\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir wollen nicht zu fr\u00fch das Feuer sch\u00fcren,", "tokens": ["Wir", "wol\u00b7len", "nicht", "zu", "fr\u00fch", "das", "Feu\u00b7er", "sch\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PTKA", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir thun nur, was wir unsern Liebden schulden,", "tokens": ["Wir", "thun", "nur", ",", "was", "wir", "un\u00b7sern", "Lieb\u00b7den", "schul\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Beschlossen demgem\u00e4\u00df in allen Hulden,", "tokens": ["Be\u00b7schlos\u00b7sen", "dem\u00b7ge\u00b7m\u00e4\u00df", "in", "al\u00b7len", "Hul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Also zu steuern solchen Ungeb\u00fchren:", "tokens": ["Al\u00b7so", "zu", "steu\u00b7ern", "sol\u00b7chen", "Un\u00b7ge\u00b7b\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Habt uns ein Aug' auf jede M\u00e4klerschar,", "tokens": ["Habt", "uns", "ein", "Aug'", "auf", "je\u00b7de", "M\u00e4k\u00b7ler\u00b7schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sie uns keinen Huf contrebandiren,", "tokens": ["Da\u00df", "sie", "uns", "kei\u00b7nen", "Huf", "cont\u00b7re\u00b7ban\u00b7di\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vom Karrengaule bis zum Bairaktar!", "tokens": ["Vom", "Kar\u00b7ren\u00b7gau\u00b7le", "bis", "zum", "Bai\u00b7rak\u00b7tar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch naht sich eins von unsern Fl\u00fcgelthieren,", "tokens": ["Doch", "naht", "sich", "eins", "von", "un\u00b7sern", "Fl\u00fc\u00b7gelt\u00b7hie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sind zum Kriegsdienst v\u00f6llig unbrauchbar \u2014", "tokens": ["Die", "sind", "zum", "Kriegs\u00b7dienst", "v\u00f6l\u00b7lig", "un\u00b7brauch\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "La\u00dft sie die Grenzen immerhin passiren!", "tokens": ["La\u00dft", "sie", "die", "Gren\u00b7zen", "im\u00b7mer\u00b7hin", "pas\u00b7si\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}