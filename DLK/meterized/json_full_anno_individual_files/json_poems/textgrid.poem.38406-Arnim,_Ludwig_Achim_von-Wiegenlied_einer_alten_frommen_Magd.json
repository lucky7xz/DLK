{"textgrid.poem.38406": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Wiegenlied einer alten frommen Magd", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wollte mich zur lieben Maria vermiethen,", "tokens": ["Ich", "woll\u00b7te", "mich", "zur", "lie\u00b7ben", "Ma\u00b7ria", "ver\u00b7mie\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich sollte ihr Kindlein helfen wiegen;", "tokens": ["Ich", "soll\u00b7te", "ihr", "Kin\u00b7dlein", "hel\u00b7fen", "wie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie f\u00fchrt mich in ihr K\u00e4mmerlein,", "tokens": ["Sie", "f\u00fchrt", "mich", "in", "ihr", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da waren die lieben Engelein,", "tokens": ["Da", "wa\u00b7ren", "die", "lie\u00b7ben", "En\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die sangen alle Gloria!", "tokens": ["Die", "san\u00b7gen", "al\u00b7le", "Glo\u00b7ria", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"t