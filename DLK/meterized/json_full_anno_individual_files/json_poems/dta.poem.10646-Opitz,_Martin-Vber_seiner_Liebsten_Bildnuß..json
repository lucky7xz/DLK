{"dta.poem.10646": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Vber seiner Liebsten Bildnu\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sehe was ich will/ die Tafel mir nicht leuget/", "tokens": ["Ich", "se\u00b7he", "was", "ich", "will", "/", "die", "Ta\u00b7fel", "mir", "nicht", "leu\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "VMFIN", "$(", "ART", "NN", "PPER", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Liecht wirdt durch die Kunst des Mahlers mir gezeiget/", "tokens": ["Mein", "Liecht", "wirdt", "durch", "die", "Kunst", "des", "Mah\u00b7lers", "mir", "ge\u00b7zei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann ich mein Lieb anseh/ deucht sie das Bild mich sein/", "tokens": ["Wann", "ich", "mein", "Lieb", "an\u00b7seh", "/", "deucht", "sie", "das", "Bild", "mich", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$(", "VVFIN", "PPER", "ART", "NN", "PPER", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wann ich das Bild anseh/ so kompt mein Lieb mir ein.", "tokens": ["Wann", "ich", "das", "Bild", "an\u00b7seh", "/", "so", "kompt", "mein", "Lieb", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$(", "ADV", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie soll sie doch in mir nicht grosse Brunst erregen?", "tokens": ["Wie", "soll", "sie", "doch", "in", "mir", "nicht", "gros\u00b7se", "Brunst", "er\u00b7re\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "PTKNEG", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ihr blosses Bildnu\u00df kan zur Liebe mich bewegen.", "tokens": ["Ihr", "blos\u00b7ses", "Bild\u00b7nu\u00df", "kan", "zur", "Lie\u00b7be", "mich", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}