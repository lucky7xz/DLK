{"textgrid.poem.46297": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Cartel des ehrwerbenden teutschen adels, aufgef\u00fchret von H. Achilles Friderichen Herzogen zu Wirtemberg", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir kommen nicht hieher, uns selbsten vil zu r\u00fchmen,", "tokens": ["Wir", "kom\u00b7men", "nicht", "hie\u00b7her", ",", "uns", "selbs\u00b7ten", "vil", "zu", "r\u00fch\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PAV", "$,", "PPER", "VVFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "oder mit fremder sprach die warheit zu verbl\u00fcmen,", "tokens": ["o\u00b7der", "mit", "frem\u00b7der", "sprach", "die", "war\u00b7heit", "zu", "ver\u00b7bl\u00fc\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "als ob wir k\u00e4men her aus einem end der welt,", "tokens": ["als", "ob", "wir", "k\u00e4\u00b7men", "her", "aus", "ei\u00b7nem", "end", "der", "welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "oder wider belebt von elisischem feld.", "tokens": ["o\u00b7der", "wi\u00b7der", "be\u00b7lebt", "von", "e\u00b7li\u00b7si\u00b7schem", "feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "nein. Teufel seind wir nicht, noch risen, noch halbg\u00f6tter,", "tokens": ["nein", ".", "Teu\u00b7fel", "seind", "wir", "nicht", ",", "noch", "ri\u00b7sen", ",", "noch", "halb\u00b7g\u00f6t\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NN", "VAFIN", "PPER", "PTKNEG", "$,", "ADV", "VVINF", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "noch helden, noch wildleut, noch unsers lands versp\u00f6tter.", "tokens": ["noch", "hel\u00b7den", ",", "noch", "wild\u00b7leut", ",", "noch", "un\u00b7sers", "lands", "ver\u00b7sp\u00f6t\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "ADJD", "$,", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "das edle teutsche reich ist unser vatterland,", "tokens": ["das", "ed\u00b7le", "teut\u00b7sche", "reich", "ist", "un\u00b7ser", "vat\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "teutsch seind wir von geburt, von stammen, herz und hand.", "tokens": ["teutsch", "seind", "wir", "von", "ge\u00b7burt", ",", "von", "stam\u00b7men", ",", "herz", "und", "hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "APPR", "NE", "$,", "APPR", "VVFIN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "was dient es, fremden preis und namen zu entlehnen?", "tokens": ["was", "dient", "es", ",", "frem\u00b7den", "preis", "und", "na\u00b7men", "zu", "ent\u00b7leh\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Teutschland bedarf sich nicht mit ausl\u00e4ndern besch\u00f6nen:", "tokens": ["Teutschland", "be\u00b7darf", "sich", "nicht", "mit", "aus\u00b7l\u00e4n\u00b7dern", "be\u00b7sch\u00f6\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "PTKNEG", "APPR", "VVINF", "VVINF", "$."], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "wie dan die welt wol wei\u00df, da\u00df Teutschland alle zeit", "tokens": ["wie", "dan", "die", "welt", "wol", "wei\u00df", ",", "da\u00df", "Teutschland", "al\u00b7le", "zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "VVFIN", "$,", "KOUS", "NE", "PIAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "hat leut f\u00fcrtreflich gnug zum friden und zum streit.", "tokens": ["hat", "leut", "f\u00fcr\u00b7tre\u00b7flich", "gnug", "zum", "fri\u00b7den", "und", "zum", "streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ADV", "APPRART", "ADJA", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Darum, ob wir wol jung, nicht sonders vil erfahren,", "tokens": ["Da\u00b7rum", ",", "ob", "wir", "wol", "jung", ",", "nicht", "son\u00b7ders", "vil", "er\u00b7fah\u00b7ren", ","],