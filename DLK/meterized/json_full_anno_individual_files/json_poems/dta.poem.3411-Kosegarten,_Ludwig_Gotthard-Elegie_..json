{"dta.poem.3411": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Elegie .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sey mir gegr&#252;sst in deinem Hervorgehn, Tochter", "tokens": ["Sey", "mir", "ge\u00b7gr", "&#252;", "sst", "in", "dei\u00b7nem", "Her\u00b7vor\u00b7gehn", ",", "Toch\u00b7ter"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "NE", "$(", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "NN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "der Unschuld!", "tokens": ["der", "Un\u00b7schuld", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Blumig schwebst du einher, \u00e4hnlich dem", "tokens": ["Blu\u00b7mig", "schwebst", "du", "ein\u00b7her", ",", "\u00e4hn\u00b7lich", "dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "ADJD", "ART"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "blumigen May!", "tokens": ["blu\u00b7mi\u00b7gen", "May", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Reine Jungfrau, wie r\u00f6thet die Rose der holden", "tokens": ["Rei\u00b7ne", "Jung\u00b7frau", ",", "wie", "r\u00f6\u00b7thet", "die", "Ro\u00b7se", "der", "hol\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "VVFIN", "ART", "NN", "ART", "ADJA"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Versch\u00e4mtheit", "tokens": ["Ver\u00b7sch\u00e4m\u00b7theit"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Deine Wange! Wie bebt bl\u00f6de dein sch\u00fcch-", "tokens": ["Dei\u00b7ne", "Wan\u00b7ge", "!", "Wie", "bebt", "bl\u00f6\u00b7de", "dein", "sch\u00fcch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "VVFIN", "ADJA", "PPOSAT", "TRUNC"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "terner Blick!", "tokens": ["ter\u00b7ner", "Blick", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.9": {"text": "Bebe nicht, meine Geliebte, und fliehe nicht,", "tokens": ["Be\u00b7be", "nicht", ",", "mei\u00b7ne", "Ge\u00b7lieb\u00b7te", ",", "und", "flie\u00b7he", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.10": {"text": "meine Erw\u00e4hlte!", "tokens": ["mei\u00b7ne", "Er\u00b7w\u00e4hl\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Fliehe nicht, Ida! Vor wem fliehst du?", "tokens": ["Flie\u00b7he", "nicht", ",", "I\u00b7da", "!", "Vor", "wem", "fliehst", "du", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "NE", "$.", "APPR", "PWS", "VVFIN", "PPER", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Ich lasse dich nicht!", "tokens": ["Ich", "las\u00b7se", "dich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Tage hab' ich um dich verschmachtet mit rastlosem", "tokens": ["Ta\u00b7ge", "hab'", "ich", "um", "dich", "ver\u00b7schmach\u00b7tet", "mit", "rast\u00b7lo\u00b7sem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PPER", "VVFIN", "APPR", "ADJA"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.14": {"text": "Schmachten.", "tokens": ["Schmach\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "N\u00e4chte hab' ich um dich sinnend und seh-", "tokens": ["N\u00e4ch\u00b7te", "hab'", "ich", "um", "dich", "sin\u00b7nend", "und", "seh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PRF", "ADJD", "KON", "TRUNC"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "nend verwacht.", "tokens": ["nend", "ver\u00b7wacht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.17": {"text": "Hingeschlichen sind nun mit Schneckentr\u00e4gheit die", "tokens": ["Hin\u00b7ge\u00b7schli\u00b7chen", "sind", "nun", "mit", "Schne\u00b7cken\u00b7tr\u00e4g\u00b7heit", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "NN", "ART"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "\u00d6den;", "tokens": ["\u00d6\u00b7den", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Und ich liesse dich schon? Ida, ich lasse", "tokens": ["Und", "ich", "lies\u00b7se", "dich", "schon", "?", "I\u00b7da", ",", "ich", "las\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$.", "NE", "$,", "PPER", "VVFIN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "dich nicht.", "tokens": ["dich", "nicht", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$."], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "Halten will ich dich, Bange, mit k\u00fchnumschlin-", "tokens": ["Hal\u00b7ten", "will", "ich", "dich", ",", "Ban\u00b7ge", ",", "mit", "k\u00fchn\u00b7um\u00b7schlin"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PRF", "$,", "NN", "$,", "APPR", "TRUNC"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "gendem Arme,", "tokens": ["gen\u00b7dem", "Ar\u00b7me", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.23": {"text": "Dr\u00fccken will ich dich fest an das verlan-", "tokens": ["Dr\u00fc\u00b7cken", "will", "ich", "dich", "fest", "an", "das", "ver\u00b7lan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PRF", "ADJD", "APPR", "ART", "TRUNC"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "gende Herz.", "tokens": ["gen\u00b7de", "Herz", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.25": {"text": "Laben will ich sein heisses Verlangen mit labenden", "tokens": ["La\u00b7ben", "will", "ich", "sein", "heis\u00b7ses", "Ver\u00b7lan\u00b7gen", "mit", "la\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "VVINF"], "meter": "+-+--+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "Str\u00f6men,", "tokens": ["Str\u00f6\u00b7men", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Letzen den lechzenden Durst, letzen mit", "tokens": ["Let\u00b7zen", "den", "lech\u00b7zen\u00b7den", "Durst", ",", "let\u00b7zen", "mit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "VVFIN", "APPR"], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.28": {"text": "k\u00f6stlichem Wein,", "tokens": ["k\u00f6st\u00b7li\u00b7chem", "Wein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.29": {"text": "Mit dem s\u00fcssen berauschenden Weine des Habens", "tokens": ["Mit", "dem", "s\u00fcs\u00b7sen", "be\u00b7rau\u00b7schen\u00b7den", "Wei\u00b7ne", "des", "Ha\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.30": {"text": "und Haltens,", "tokens": ["und", "Hal\u00b7tens", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.31": {"text": "Und Geniessens, bis mir schwinden das", "tokens": ["Und", "Ge\u00b7nies\u00b7sens", ",", "bis", "mir", "schwin\u00b7den", "das"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "KOUS", "PPER", "VVFIN", "ART"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.32": {"text": "Licht und der Schall \u2014", "tokens": ["Licht", "und", "der", "Schall"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.33": {"text": "Ida, ich habe dich wieder. Ich hab', ich halte", "tokens": ["I\u00b7da", ",", "ich", "ha\u00b7be", "dich", "wie\u00b7der", ".", "Ich", "hab'", ",", "ich", "hal\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PPER", "VAFIN", "PPER", "ADV", "$.", "PPER", "VAFIN", "$,", "PPER", "VVFIN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.34": {"text": "dich wieder,", "tokens": ["dich", "wie\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.35": {"text": "F\u00fchle wieder der Hand lebenersch\u00fctternden", "tokens": ["F\u00fch\u00b7le", "wie\u00b7der", "der", "Hand", "le\u00b7be\u00b7ner\u00b7sch\u00fct\u00b7tern\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "VVINF"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.36": {"text": "Druck,", "tokens": ["Druck", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.37": {"text": "Schaue wieder dein grosses blaues schmachtendes", "tokens": ["Schau\u00b7e", "wie\u00b7der", "dein", "gros\u00b7ses", "blau\u00b7es", "schmach\u00b7ten\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Auge,", "tokens": ["Au\u00b7ge", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.39": {"text": "G\u00fcrte den schlanken Leib wieder mit br\u00fcn-", "tokens": ["G\u00fcr\u00b7te", "den", "schlan\u00b7ken", "Leib", "wie\u00b7der", "mit", "br\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "ADV", "APPR", "TRUNC"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.40": {"text": "stigem Arm,", "tokens": ["sti\u00b7gem", "Arm", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.41": {"text": "F\u00fchle mich wieder von deines Odems Duften um-", "tokens": ["F\u00fch\u00b7le", "mich", "wie\u00b7der", "von", "dei\u00b7nes", "O\u00b7dems", "Duf\u00b7ten", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.42": {"text": "s\u00e4uselt,", "tokens": ["s\u00e4u\u00b7selt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.43": {"text": "Wieder gehoben mein Haupt von der geho-", "tokens": ["Wie\u00b7der", "ge\u00b7ho\u00b7ben", "mein", "Haupt", "von", "der", "ge\u00b7ho"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "PPOSAT", "NN", "APPR", "ART", "TRUNC"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.44": {"text": "benen Brust \u2014", "tokens": ["be\u00b7nen", "Brust"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "--+", "measure": "anapaest.init"}, "line.45": {"text": "Meine Ida, wie selig . . . wie tr\u00e4umend . . . w\u00e4h-", "tokens": ["Mei\u00b7ne", "I\u00b7da", ",", "wie", "se\u00b7lig", ".", ".", ".", "wie", "tr\u00e4u\u00b7mend", ".", ".", ".", "w\u00e4h"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "punct", "punct", "word"], "pos": ["PPOSAT", "NE", "$,", "PWAV", "ADJD", "$.", "$.", "$.", "PWAV", "VVPP", "$.", "$.", "$.", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.46": {"text": "nend und . . . selig! \u2014", "tokens": ["nend", "und", ".", ".", ".", "se\u00b7lig", "!"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "punct", "punct"], "pos": ["ADJD", "KON", "$.", "$.", "$.", "ADJD", "$.", "$("], "meter": "--+-", "measure": "anapaest.init"}, "line.47": {"text": "Meine Ida, warum str\u00e4ubest du wegernd", "tokens": ["Mei\u00b7ne", "I\u00b7da", ",", "wa\u00b7rum", "str\u00e4u\u00b7best", "du", "we\u00b7gernd"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "$,", "PWAV", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.48": {"text": "dich weg?", "tokens": ["dich", "weg", "?"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.49": {"text": "Warum wendest du seitw\u00e4rts dein Aug'? Und", "tokens": ["Wa\u00b7rum", "wen\u00b7dest", "du", "seit\u00b7w\u00e4rts", "dein", "Aug'", "?", "Und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$.", "KON"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.50": {"text": "warum entstrebst du", "tokens": ["wa\u00b7rum", "ent\u00b7strebst", "du"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.51": {"text": "Meinem liebenden Arm? \u2014 Ida, liebst du", "tokens": ["Mei\u00b7nem", "lie\u00b7ben\u00b7den", "Arm", "?", "I\u00b7da", ",", "liebst", "du"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$(", "NE", "$,", "VVFIN", "PPER"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.52": {"text": "mich nicht? \u2014", "tokens": ["mich", "nicht", "?"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PPER", "PTKNEG", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.53": {"text": "Wehe, sie liebet mich nicht! Verlass mich, M\u00e4d-", "tokens": ["We\u00b7he", ",", "sie", "lie\u00b7bet", "mich", "nicht", "!", "Ver\u00b7lass", "mich", ",", "M\u00e4d"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "VVFIN", "PPER", "$,", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.54": {"text": "chen, verlass mich,", "tokens": ["chen", ",", "ver\u00b7lass", "mich", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.55": {"text": "Wenn du mich nicht liebst! Spotte des", "tokens": ["Wenn", "du", "mich", "nicht", "liebst", "!", "Spot\u00b7te", "des"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "$.", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "J\u00fcnglinges nicht,", "tokens": ["J\u00fcng\u00b7lin\u00b7ges", "nicht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.57": {"text": "Dem du Liebe versprachst, und den du nicht lie-", "tokens": ["Dem", "du", "Lie\u00b7be", "ver\u00b7sprachst", ",", "und", "den", "du", "nicht", "lie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "NN", "VVFIN", "$,", "KON", "ART", "PPER", "PTKNEG", "TRUNC"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.58": {"text": "best \u2014 Verl\u00e4ugn' es,", "tokens": ["best", "Verl\u00b7\u00e4ugn'", "es", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVIMP", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.59": {"text": "Falsche, wenn du es darfst, dass du mir", "tokens": ["Fal\u00b7sche", ",", "wenn", "du", "es", "darfst", ",", "dass", "du", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "PAV", "$,", "KOUS", "PPER", "PPER"], "meter": "+-+---+-+", "mea