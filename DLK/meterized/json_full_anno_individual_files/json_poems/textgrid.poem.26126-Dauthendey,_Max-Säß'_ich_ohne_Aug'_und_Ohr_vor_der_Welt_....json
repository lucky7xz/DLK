{"textgrid.poem.26126": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "S\u00e4\u00df' ich ohne Aug' und Ohr vor der Welt ...", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00e4\u00df' ich ohne Aug' und Ohr vor der Welt, die Leid und Liebe tauscht,", "tokens": ["S\u00e4\u00df'", "ich", "oh\u00b7ne", "Aug'", "und", "Ohr", "vor", "der", "Welt", ",", "die", "Leid", "und", "Lie\u00b7be", "tauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "H\u00f6rt' ich doch, wenn's Kleid der Liebsten um mich rauscht;", "tokens": ["H\u00f6rt'", "ich", "doch", ",", "wenn's", "Kleid", "der", "Liebs\u00b7ten", "um", "mich", "rauscht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "NN", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "S\u00e4h' ich doch, ob sie err\u00f6tend stille h\u00e4lt", "tokens": ["S\u00e4h'", "ich", "doch", ",", "ob", "sie", "er\u00b7r\u00f6\u00b7tend", "stil\u00b7le", "h\u00e4lt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADJD", "ADJA", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Und mein Herz wie's Uhrwerk stumm belauscht;", "tokens": ["Und", "mein", "Herz", "wie's", "Uhr\u00b7werk", "stumm", "be\u00b7lauscht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "H\u00f6rte, ob ihr Haar im Kissen knistert neben mir;", "tokens": ["H\u00f6r\u00b7te", ",", "ob", "ihr", "Haar", "im", "Kis\u00b7sen", "knis\u00b7tert", "ne\u00b7ben", "mir", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "W\u00fcrde an dem Pochen ihrer Br\u00fcste wissen,", "tokens": ["W\u00fcr\u00b7de", "an", "dem", "Po\u00b7chen", "ih\u00b7rer", "Br\u00fcs\u00b7te", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Ist der Mai am Fenster voll Gel\u00fcste,", "tokens": ["Ist", "der", "Mai", "am", "Fens\u00b7ter", "voll", "Ge\u00b7l\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ist es Nacht oder Tag, wenn sie meine Lippen k\u00fc\u00dfte;", "tokens": ["Ist", "es", "Nacht", "o\u00b7der", "Tag", ",", "wenn", "sie", "mei\u00b7ne", "Lip\u00b7pen", "k\u00fc\u00df\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "W\u00fc\u00dfte, ob sie totenbla\u00df ist, und ich sterben m\u00fc\u00dfte.", "tokens": ["W\u0