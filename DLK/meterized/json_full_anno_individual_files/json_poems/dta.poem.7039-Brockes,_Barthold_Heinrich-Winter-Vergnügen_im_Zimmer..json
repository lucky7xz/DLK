{"dta.poem.7039": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Winter-Vergn\u00fcgen  \n im Zimmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Kinder stehen auch, vergn\u00fcget durch den Schein,", "tokens": ["Die", "Kin\u00b7der", "ste\u00b7hen", "auch", ",", "ver\u00b7gn\u00fc\u00b7get", "durch", "den", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und halten gegen\u2019s Feur, von ihrer kalten Hand", "tokens": ["Und", "hal\u00b7ten", "ge\u00b7gen's", "Feur", ",", "von", "ih\u00b7rer", "kal\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die kleinen Finger ausgespannt;", "tokens": ["Die", "klei\u00b7nen", "Fin\u00b7ger", "aus\u00b7ge\u00b7spannt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wobey sie sich denn sonderlich ergetzen,", "tokens": ["Wo\u00b7bey", "sie", "sich", "denn", "son\u00b7der\u00b7lich", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn sie mit kindischem Gew\u00fchl", "tokens": ["Wenn", "sie", "mit", "kin\u00b7di\u00b7schem", "Ge\u00b7w\u00fchl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Aepfelchen ans Feur zu braten setzen.", "tokens": ["Ein", "A\u00b7e\u00b7pfel\u00b7chen", "ans", "Feur", "zu", "bra\u00b7ten", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}