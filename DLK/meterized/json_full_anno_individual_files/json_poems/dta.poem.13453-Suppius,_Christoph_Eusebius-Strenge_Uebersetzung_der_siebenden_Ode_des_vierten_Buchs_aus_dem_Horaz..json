{"dta.poem.13453": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Strenge Uebersetzung  \n der siebenden Ode des vierten Buchs  \n aus dem Horaz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Der Schnee ist fort! das Gras besucht die Fel-", "tokens": ["Der", "Schnee", "ist", "fort", "!", "das", "Gras", "be\u00b7sucht", "die", "Fel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der wieder,", "tokens": ["der", "wie\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Den B\u00e4umen w\u00e4chst aufs neue schon das Haar,", "tokens": ["Den", "B\u00e4u\u00b7men", "w\u00e4chst", "aufs", "neu\u00b7e", "schon", "das", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In anderer Gestalt stellt sich das Erdreich dar,", "tokens": ["In", "an\u00b7de\u00b7rer", "Ge\u00b7stalt", "stellt", "sich", "das", "Er\u00b7dreich", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und fallend l\u00e4uft der Flu\u00df am Ufer nieder.", "tokens": ["Und", "fal\u00b7lend", "l\u00e4uft", "der", "Flu\u00df", "am", "U\u00b7fer", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das Drey der Gratien nebst Nymphen ihrer Arten", "tokens": ["Das", "Drey", "der", "Gra\u00b7ti\u00b7en", "nebst", "Nym\u00b7phen", "ih\u00b7rer", "Ar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Beginnt den Reihn entbl\u00f6\u00dft an Leib und Haupt,", "tokens": ["Be\u00b7ginnt", "den", "Reihn", "ent\u00b7bl\u00f6\u00dft", "an", "Leib", "und", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das Jahr, die Stunde, so den g\u00fcldnen Tag ge-", "tokens": ["Das", "Jahr", ",", "die", "Stun\u00b7de", ",", "so", "den", "g\u00fcld\u00b7nen", "Tag", "ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "raubt,", "tokens": ["raubt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Erinnert uns, nichts Ewigs zu erwarten.", "tokens": ["E\u00b7rin\u00b7nert", "uns", ",", "nichts", "E\u00b7wigs", "zu", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PIS", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Der Zephir schmelzt den Frost, der Lenz schleppt im", "tokens": ["Der", "Ze\u00b7phir", "schmelzt", "den", "Frost", ",", "der", "Lenz", "schleppt", "im"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "APPRART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Verschwinden", "tokens": ["Ver\u00b7schwin\u00b7den"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Den heissen Sommer hinter sich darein,", "tokens": ["Den", "heis\u00b7sen", "Som\u00b7mer", "hin\u00b7ter", "sich", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PRF", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Schenkt der obstschwangre Herbst aus seinem F\u00fcll-", "tokens": ["Schenkt", "der", "obst\u00b7schwang\u00b7re", "Herbst", "aus", "sei\u00b7nem", "F\u00fcll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "horn ein,", "tokens": ["horn", "ein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "So folgt der tr\u00e4ge Winter wieder hinden.", "tokens": ["So", "folgt", "der", "tr\u00e4\u00b7ge", "Win\u00b7ter", "wie\u00b7der", "hin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Den himmlischen Verlust ersetzt des Monden", "tokens": ["Den", "himm\u00b7li\u00b7schen", "Ver\u00b7lust", "er\u00b7setzt", "des", "Mon\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Schnelle,", "tokens": ["Schnel\u00b7le", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Hingegen fallen wir erst einmahl ab,", "tokens": ["Hin\u00b7ge\u00b7gen", "fal\u00b7len", "wir", "erst", "ein\u00b7mahl", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.20": {"text": "Wie Ank, der reiche Tull, der fromm Enee ins", "tokens": ["Wie", "Ank", ",", "der", "rei\u00b7che", "Tull", ",", "der", "fromm", "E\u00b7nee", "ins"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "NN", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Grab,", "tokens": ["Grab", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.22": {"text": "So sind wir Staub, ein Schatten dieser Schwelle.", "tokens": ["So", "sind", "wir", "Staub", ",", "ein", "Schat\u00b7ten", "die\u00b7ser", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Wer wei\u00df, ob zu der Zahl von heutgen Lebenstagen", "tokens": ["Wer", "wei\u00df", ",", "ob", "zu", "der", "Zahl", "von", "heut\u00b7gen", "Le\u00b7bens\u00b7ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Noch GOtt einmahl die Zeit von Morgen legt?", "tokens": ["Noch", "Gott", "ein\u00b7mahl", "die", "Zeit", "von", "Mor\u00b7gen", "legt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Mit dem, was du verschenkt, aus Freundschafts-", "tokens": ["Mit", "dem", ",", "was", "du", "ver\u00b7schenkt", ",", "aus", "Freund\u00b7schafts"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "$,", "PWS", "PPER", "VVFIN", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Trieb bewegt,", "tokens": ["Trieb", "be\u00b7wegt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.27": {"text": "Wird sich des Erben karge Hand nicht tragen.", "tokens": ["Wird", "sich", "des", "Er\u00b7ben", "kar\u00b7ge", "Hand", "nicht", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Bist du erst todt, da\u00df es beym Minos dir gelinget,", "tokens": ["Bist", "du", "erst", "todt", ",", "da\u00df", "es", "beym", "Mi\u00b7nos", "dir", "ge\u00b7lin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "KOUS", "PPER", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.29": {"text": "Indem er dir das Urtheil herrlich spricht,", "tokens": ["In\u00b7dem", "er", "dir", "das", "Ur\u00b7theil", "herr\u00b7lich", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "So wird dich, mein Torqvat, Geschlecht und Zun-", "tokens": ["So", "wird", "dich", ",", "mein", "Tor\u00b7qvat", ",", "Ge\u00b7schlecht", "und", "Zun"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "NN", "KON", "TRUNC"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.31": {"text": "ge nicht", "tokens": ["ge", "nicht"], "token_info": ["word", "word"], "pos": ["NE", "PTKNEG"], "meter": "-+", "measure": "iambic.single"}, "line.32": {"text": "Noch deine Gottesfurcht herwieder bringen.", "tokens": ["Noch", "dei\u00b7ne", "Got\u00b7tes\u00b7furcht", "her\u00b7wie\u00b7der", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Diana selbst befreyt von H\u00f6llenfinsternissen", "tokens": ["Di\u00b7a\u00b7na", "selbst", "be\u00b7freyt", "von", "H\u00f6l\u00b7len\u00b7fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Auch nicht einmahl den keuschen Hyppolit,", "tokens": ["Auch", "nicht", "ein\u00b7mahl", "den", "keu\u00b7schen", "Hyp\u00b7po\u00b7lit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Das Heldenband, worinn sich Pirithous sieht,", "tokens": ["Das", "Hel\u00b7den\u00b7band", ",", "wo\u00b7rinn", "sich", "Pi\u00b7rit\u00b7hous", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PRF", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Hat Theseus, der ihn liebt, noch nicht zerrissen.", "tokens": ["Hat", "The\u00b7seus", ",", "der", "ihn", "liebt", ",", "noch", "nicht", "zer\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}