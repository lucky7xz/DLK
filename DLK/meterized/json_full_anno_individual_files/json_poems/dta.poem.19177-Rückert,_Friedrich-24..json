{"dta.poem.19177": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh, wie die Fantasie des Fr\u00fchlings einen Raum", "tokens": ["Sieh", ",", "wie", "die", "Fan\u00b7ta\u00b7sie", "des", "Fr\u00fch\u00b7lings", "ei\u00b7nen", "Raum"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Blumen dort bes\u00e4t, hier schm\u00fcckt den Bl\u00fctenbaum,", "tokens": ["Mit", "Blu\u00b7men", "dort", "be\u00b7s\u00e4t", ",", "hier", "schm\u00fcckt", "den", "Bl\u00fc\u00b7ten\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Worin ein ganzer Wald von Trieben ist vereinigt,", "tokens": ["Wo\u00b7rin", "ein", "gan\u00b7zer", "Wald", "von", "Trie\u00b7ben", "ist", "ver\u00b7ei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch hat er seine Kunst wie hier auch dort bescheinigt;", "tokens": ["Doch", "hat", "er", "sei\u00b7ne", "Kunst", "wie", "hier", "auch", "dort", "be\u00b7schei\u00b7nigt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN", "KOKOM", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie dich ein Dichter freut, ob einzeln er verstreut", "tokens": ["Wie", "dich", "ein", "Dich\u00b7ter", "freut", ",", "ob", "ein\u00b7zeln", "er", "ver\u00b7streut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "KOUS", "ADJD", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel Sch\u00f6nes, ob er dir ein sch\u00f6nes Ganzes beut.", "tokens": ["Viel", "Sch\u00f6\u00b7nes", ",", "ob", "er", "dir", "ein", "sch\u00f6\u00b7nes", "Gan\u00b7zes", "beut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "KOUS", "PPER", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}