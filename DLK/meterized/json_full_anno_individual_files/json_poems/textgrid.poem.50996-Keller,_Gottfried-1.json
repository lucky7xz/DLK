{"textgrid.poem.50996": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Arm in Arm und Kron an Krone steht der Eichenwald verschlungen,", "tokens": ["Arm", "in", "Arm", "und", "Kron", "an", "Kro\u00b7ne", "steht", "der", "Ei\u00b7chen\u00b7wald", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Heut hat er bei guter Laune mir sein altes Lied gesungen.", "tokens": ["Heut", "hat", "er", "bei", "gu\u00b7ter", "Lau\u00b7ne", "mir", "sein", "al\u00b7tes", "Lied", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Fern am Rande fing ein junges B\u00e4umchen an sich sacht zu wiegen,", "tokens": ["Fern", "am", "Ran\u00b7de", "fing", "ein", "jun\u00b7ges", "B\u00e4um\u00b7chen", "an", "sich", "sacht", "zu", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "APPR", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und dann ging es immer weiter an ein Sausen, an ein Biegen;", "tokens": ["Und", "dann", "ging", "es", "im\u00b7mer", "wei\u00b7ter", "an", "ein", "Sau\u00b7sen", ",", "an", "ein", "Bie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Kam es her in m\u00e4cht'gem Zuge, schwoll es an zu breiten Wogen,", "tokens": ["Kam", "es", "her", "in", "m\u00e4cht'\u00b7gem", "Zu\u00b7ge", ",", "schwoll", "es", "an", "zu", "brei\u00b7ten", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPR", "ADJA", "NN", "$,", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Hoch sich durch die Wipfel w\u00e4lzend kam die Sturmesflut gezogen.", "tokens": ["Hoch", "sich", "durch", "die", "Wip\u00b7fel", "w\u00e4l\u00b7zend", "kam", "die", "Stur\u00b7mes\u00b7flut", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "ART", "NN", "VVPP", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Und nun sang und pfiff es graulich in den Kronen, in den L\u00fcften,", "tokens": ["Und", "nun", "sang", "und", "pfiff", "es", "grau\u00b7lich", "in", "den", "Kro\u00b7nen", ",", "in", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und dazwischen knarrt' und dr\u00f6hnt' es unten in den Wurzelgr\u00fcften.", "tokens": ["Und", "da\u00b7zwi\u00b7schen", "knarrt'", "und", "dr\u00f6hnt'", "es", "un\u00b7ten", "in", "den", "Wur\u00b7zel\u00b7gr\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "--+-+-+-+-+-+-+-", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Manchmal schwang die h\u00f6chste Eiche gellend ihren Schaft alleine,", "tokens": ["Manch\u00b7mal", "schwang", "die", "h\u00f6chs\u00b7te", "Ei\u00b7che", "gel\u00b7lend", "ih\u00b7ren", "Schaft", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "VVPP", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Donnernder erscholl nur immer drauf der Chor vom ganzen Haine!", "tokens": ["Don\u00b7nern\u00b7der", "er\u00b7scholl", "nur", "im\u00b7mer", "drauf", "der", "Chor", "vom", "gan\u00b7zen", "Hai\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "ADV", "PAV", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Einer wilden Meeresbrandung hat das sch\u00f6ne Spiel geglichen;", "tokens": ["Ei\u00b7ner", "wil\u00b7den", "Mee\u00b7res\u00b7bran\u00b7dung", "hat", "das", "sch\u00f6\u00b7ne", "Spiel", "ge\u00b7gli\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Alles Laub war, wei\u00dflich schimmernd, nach Nordosten hin gestrichen.", "tokens": ["Al\u00b7les", "Laub", "war", ",", "wei\u00df\u00b7lich", "schim\u00b7mernd", ",", "nach", "Nord\u00b7os\u00b7ten", "hin", "ge\u00b7stri\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$,", "ADJD", "VVPP", "$,", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Also streicht die alte Geige Pan der Alte laut und leise,", "tokens": ["Al\u00b7so", "streicht", "die", "al\u00b7te", "Gei\u00b7ge", "Pan", "der", "Al\u00b7te", "laut", "und", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "NE", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Unterrichtend seine W\u00e4lder in der alten Weltenweise.", "tokens": ["Un\u00b7ter\u00b7rich\u00b7tend", "sei\u00b7ne", "W\u00e4l\u00b7der", "in", "der", "al\u00b7ten", "Wel\u00b7ten\u00b7wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "In den sieben T\u00f6nen schweift er unersch\u00f6pflich auf und nieder,", "tokens": ["In", "den", "sie\u00b7ben", "T\u00f6\u00b7nen", "schweift", "er", "un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "In den sieben alten T\u00f6nen, die umfassen alle Lieder.", "tokens": ["In", "den", "sie\u00b7ben", "al\u00b7ten", "T\u00f6\u00b7nen", ",", "die", "um\u00b7fas\u00b7sen", "al\u00b7le", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "ADJA", "NN", "$,", "PRELS", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Und es lauschen still die jungen Dichter und die jungen Finken,", "tokens": ["Und", "es", "lau\u00b7schen", "still", "die", "jun\u00b7gen", "Dich\u00b7ter", "und", "die", "jun\u00b7gen", "Fin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "VMFIN", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Kauernd in den dunklen B\u00fcschen sie die Melodien trinken.", "tokens": ["Kau\u00b7ernd", "in", "den", "dunk\u00b7len", "B\u00fc\u00b7schen", "sie", "die", "Me\u00b7lo\u00b7dien", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+---+--+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Arm in Arm und Kron an Krone steht der Eichenwald verschlungen,", "tokens": ["Arm", "in", "Arm", "und", "Kron", "an", "Kro\u00b7ne", "steht", "der", "Ei\u00b7chen\u00b7wald", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Heut hat er bei guter Laune mir sein altes Lied gesung