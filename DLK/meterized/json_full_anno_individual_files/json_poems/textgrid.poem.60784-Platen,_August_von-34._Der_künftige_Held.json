{"textgrid.poem.60784": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "34. Der k\u00fcnftige Held", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "R\u00fcckw\u00e4rts gewandt blickt oft in der Fabel Nacht", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "ge\u00b7wandt", "blickt", "oft", "in", "der", "Fa\u00b7bel", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "APPR", "ART", "NN", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Der Dichter, sp\u00e4ht Heroen sich aus, und forscht", "tokens": ["Der", "Dich\u00b7ter", ",", "sp\u00e4ht", "He\u00b7roen", "sich", "aus", ",", "und", "forscht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "NN", "PRF", "PTKVZ", "$,", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch manches Zeitlaufs Tatenwirrwarr,", "tokens": ["Durch", "man\u00b7ches", "Zeit\u00b7laufs", "Ta\u00b7ten\u00b7wirr\u00b7warr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NE", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Liederbegierigen Sinns, nach Helden:", "tokens": ["Lie\u00b7der\u00b7be\u00b7gie\u00b7ri\u00b7gen", "Sinns", ",", "nach", "Hel\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Ich w\u00e4hle den mir, welcher dereinst erscheint", "tokens": ["Ich", "w\u00e4h\u00b7le", "den", "mir", ",", "wel\u00b7cher", "de\u00b7reinst", "er\u00b7scheint"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "PPER", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und will vom Tod nicht wecken Gemoderte:", "tokens": ["Und", "will", "vom", "Tod", "nicht", "we\u00b7cken", "Ge\u00b7mo\u00b7der\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "NN", "PTKNEG", "VVINF", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den Mann der Zukunft preisend, wandelt", "tokens": ["Den", "Mann", "der", "Zu\u00b7kunft", "prei\u00b7send", ",", "wan\u00b7delt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor dem Erwarteten mein Gesang her!", "tokens": ["Vor", "dem", "Er\u00b7war\u00b7te\u00b7ten", "mein", "Ge\u00b7sang", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Er komme bald uns, welchem des Ewigen", "tokens": ["Er", "kom\u00b7me", "bald", "uns", ",", "wel\u00b7chem", "des", "E\u00b7wi\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "$,", "PRELS", "ART", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ratschlu\u00df verliehn ruhmw\u00fcrdiges R\u00e4cheramt", "tokens": ["Rat\u00b7schlu\u00df", "ver\u00b7liehn", "ruhm\u00b7w\u00fcr\u00b7di\u00b7ges", "R\u00e4\u00b7cher\u00b7amt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVPP", "ADJA", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Geh\u00e4ufter Untat, aus den Z\u00e4hnen", "tokens": ["Ge\u00b7h\u00e4uf\u00b7ter", "Un\u00b7tat", ",", "aus", "den", "Z\u00e4h\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rei\u00df er dem Wolfe das Lamm, er komme", "tokens": ["Rei\u00df", "er", "dem", "Wol\u00b7fe", "das", "Lamm", ",", "er", "kom\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Dem Stamm verderblich jener Semiramis", "tokens": ["Dem", "Stamm", "ver\u00b7derb\u00b7lich", "je\u00b7ner", "Se\u00b7mi\u00b7ra\u00b7mis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PDAT", "NE"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Mit ihrem zahllos wimmelnden Buhlerheer,", "tokens": ["Mit", "ih\u00b7rem", "zahl\u00b7los", "wim\u00b7meln\u00b7den", "Buh\u00b7ler\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die schon der Vorzeit graues Wort uns", "tokens": ["Die", "schon", "der", "Vor\u00b7zeit", "grau\u00b7es", "Wort", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als babylonische Metze weissagt!", "tokens": ["Als", "ba\u00b7by\u00b7lo\u00b7ni\u00b7sche", "Met\u00b7ze", "weis\u00b7sagt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Er komme, der mit strafendem Gei\u00dfelhieb", "tokens": ["Er", "kom\u00b7me", ",", "der", "mit", "stra\u00b7fen\u00b7dem", "Gei\u00b7\u00df\u00b7el\u00b7hieb"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nach Asien heim stumpfn\u00fcstrige Sklaven peitscht,", "tokens": ["Nach", "A\u00b7sien", "heim", "stumpf\u00b7n\u00fcst\u00b7ri\u00b7ge", "Skla\u00b7ven", "peitscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PTKVZ", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie selbst und ihre l\u00e4ngst entnervten,", "tokens": ["Sie", "selbst", "und", "ih\u00b7re", "l\u00e4ngst", "ent\u00b7nerv\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "PPOSAT", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weibisch entg\u00fcrteten Dschingiskhane,", "tokens": ["Wei\u00b7bisch", "ent\u00b7g\u00fcr\u00b7te\u00b7ten", "Dschin\u00b7gis\u00b7kha\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Die nur des Mords noch pflegen, und nicht der Schlacht,", "tokens": ["Die", "nur", "des", "Mords", "noch", "pfle\u00b7gen", ",", "und", "nicht", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADV", "VVINF", "$,", "KON", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des V\u00f6lkermords! Dir, Siegender, m\u00f6ge dann", "tokens": ["Des", "V\u00f6l\u00