{"textgrid.poem.49928": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "2. Brunnen-Kapelle am Kreuzgang", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier einst sah man die Scheiben gemalt, und Fenster an Fenster", "tokens": ["Hier", "einst", "sah", "man", "die", "Schei\u00b7ben", "ge\u00b7malt", ",", "und", "Fens\u00b7ter", "an", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "ART", "NN", "VVPP", "$,", "KON", "NN", "APPR", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Strahlte der d\u00e4mmernde Raum, welcher ein Br\u00fcnnlein umschlo\u00df,", "tokens": ["Strahl\u00b7te", "der", "d\u00e4m\u00b7mern\u00b7de", "Raum", ",", "wel\u00b7cher", "ein", "Br\u00fcnn\u00b7lein", "um\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Da\u00df auf der tauenden Fl\u00e4che die farbigen Lichter sich wiegten,", "tokens": ["Da\u00df", "auf", "der", "tau\u00b7en\u00b7den", "Fl\u00e4\u00b7che", "die", "far\u00b7bi\u00b7gen", "Lich\u00b7ter", "sich", "wieg\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Zauberisch, wenn du wie heut, herbstliche Sonne, gegl\u00e4nzt.", "tokens": ["Zau\u00b7be\u00b7risch", ",", "wenn", "du", "wie", "heut", ",", "herbst\u00b7li\u00b7che", "Son\u00b7ne", ",", "ge\u00b7gl\u00e4nzt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "KOKOM", "ADV", "$,", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "+---+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Jetzo schattest du nur gleichg\u00fcltig das steinerne Schmuckwerk", "tokens": ["Jet\u00b7zo", "schat\u00b7test", "du", "nur", "gleich\u00b7g\u00fcl\u00b7tig", "das", "stei\u00b7ner\u00b7ne", "Schmuck\u00b7werk"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+---+--+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Ab am Boden, und l\u00e4ngst f\u00fcllt sich die Schale nicht mehr.", "tokens": ["Ab", "am", "Bo\u00b7den", ",", "und", "l\u00e4ngst", "f\u00fcllt", "sich", "die", "Scha\u00b7le", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$,", "KON", "ADV", "VVFIN", "PRF", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Aber du zeigst mir tr\u00f6stlich im Garten ein bl\u00fchendes Leben,", "tokens": ["A\u00b7ber", "du", "zeigst", "mir", "tr\u00f6st\u00b7lich", "im", "Gar\u00b7ten", "ein", "bl\u00fc\u00b7hen\u00b7des", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Das dein wonniger Strahl locket aus Moder und Schutt.", "tokens": ["Das", "dein", "won\u00b7ni\u00b7ger", "Strahl", "lo\u00b7cket", "aus", "Mo\u00b7der", "und", "Schutt", "."], "token_info": ["word", "w