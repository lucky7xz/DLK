{"dta.poem.15635": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In meinem Glauben bin ich eins mit eurem, weil", "tokens": ["In", "mei\u00b7nem", "Glau\u00b7ben", "bin", "ich", "eins", "mit", "eu\u00b7rem", ",", "weil"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "PIS", "APPR", "PPOSAT", "$,", "KOUS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich glaube, wie ihr glaubt, im Glauben sei das Heil,", "tokens": ["Ich", "glau\u00b7be", ",", "wie", "ihr", "glaubt", ",", "im", "Glau\u00b7ben", "sei", "das", "Heil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPRART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im Glauben f\u00fcr den Geist des letzten Ziels Erreichung", "tokens": ["Im", "Glau\u00b7ben", "f\u00fcr", "den", "Geist", "des", "letz\u00b7ten", "Ziels", "Er\u00b7rei\u00b7chung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sei des Unendlichen und Endlichen Ausgleichung.", "tokens": ["Sei", "des", "Un\u00b7end\u00b7li\u00b7chen", "und", "End\u00b7li\u00b7chen", "Aus\u00b7glei\u00b7chung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ihr aber glaubt dabei, ein einzig einer sei", "tokens": ["Ihr", "a\u00b7ber", "glaubt", "da\u00b7bei", ",", "ein", "ein\u00b7zig", "ei\u00b7ner", "sei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PAV", "$,", "ART", "ADJD", "PIS", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Glauben, und ich glaub', es seien vielerlei.", "tokens": ["Der", "Glau\u00b7ben", ",", "und", "ich", "glaub'", ",", "es", "sei\u00b7en", "vie\u00b7ler\u00b7lei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PIAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich glaub' auch, da\u00df f\u00fcr euch sei euer Glaube gut,", "tokens": ["Ich", "glaub'", "auch", ",", "da\u00df", "f\u00fcr", "euch", "sei", "eu\u00b7er", "Glau\u00b7be", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "APPR", "PPER", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Obgleich entgegen ihr mir nicht das gleiche thut.", "tokens": ["Ob\u00b7gleich", "ent\u00b7ge\u00b7gen", "ihr", "mir", "nicht", "das", "glei\u00b7che", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "PPER", "PTKNEG", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Leugnung gegen mich mu\u00df ich euch auch erlauben,", "tokens": ["Die", "Leug\u00b7nung", "ge\u00b7gen", "mich", "mu\u00df", "ich", "euch", "auch", "er\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil diese Leugnung mitgeh\u00f6rt zu eurem Glauben.", "tokens": ["Weil", "die\u00b7se", "Leug\u00b7nung", "mit\u00b7ge\u00b7h\u00f6rt", "zu", "eu\u00b7rem", "Glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Er, der als Glaubenst\u00fcck mir selber gab die Duldung,", "tokens": ["Er", ",", "der", "als", "Glau\u00b7ben\u00b7st\u00fcck", "mir", "sel\u00b7ber", "gab", "die", "Dul\u00b7dung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "KOUS", "NN", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gab euch Unduldsamkeit ohn' euere Verschuldung.", "tokens": ["Gab", "euch", "Un\u00b7duld\u00b7sam\u00b7keit", "ohn'", "eu\u00b7e\u00b7re", "Ver\u00b7schul\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-++--+-", "measure": "iambic.hexa.invert"}}}}}