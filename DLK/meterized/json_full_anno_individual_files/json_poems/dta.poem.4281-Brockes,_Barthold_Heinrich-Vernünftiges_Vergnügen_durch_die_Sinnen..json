{"dta.poem.4281": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vern\u00fcnftiges Vergn\u00fcgen durch die  \n Sinnen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es pranget, ihrem HErrn zum Ruhme,", "tokens": ["Es", "pran\u00b7get", ",", "ih\u00b7rem", "Herrn", "zum", "Ruh\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein jedes Kraut, ein\u2019 jede Bluhme,", "tokens": ["Ein", "je\u00b7des", "Kraut", ",", "ein'", "je\u00b7de", "Bluh\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die V\u00f6gel lassen, GOtt zu Ehren,", "tokens": ["Die", "V\u00f6\u00b7gel", "las\u00b7sen", ",", "Gott", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In klingenden und hellen Ch\u00f6ren,", "tokens": ["In", "klin\u00b7gen\u00b7den", "und", "hel\u00b7len", "Ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bewundrungs-wehrte Lieder h\u00f6ren:", "tokens": ["Be\u00b7wun\u00b7drungs\u00b7wehr\u00b7te", "Lie\u00b7der", "h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Weil sie es aber selbst nicht wissen;", "tokens": ["Weil", "sie", "es", "a\u00b7ber", "selbst", "nicht", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So wird man ja gestehen m\u00fcssen,", "tokens": ["So", "wird", "man", "ja", "ge\u00b7ste\u00b7hen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nur Geistern sey die Kraft gegeben,", "tokens": ["Nur", "Geis\u00b7tern", "sey", "die", "Kraft", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df sie, in ihnen, eine Macht,", "tokens": ["Da\u00df", "sie", ",", "in", "ih\u00b7nen", ",", "ei\u00b7ne", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und eine weise Lieb\u2019 erheben", "tokens": ["Und", "ei\u00b7ne", "wei\u00b7se", "Lieb'", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Des Wesens, das ihr Seyn, ihr Leben,", "tokens": ["Des", "We\u00b7sens", ",", "das", "ihr", "Seyn", ",", "ihr", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und ihre Pracht hervorgebracht.", "tokens": ["Und", "ih\u00b7re", "Pracht", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Du hast, f\u00fcr der Gesch\u00f6pfe Prangen,", "tokens": ["Du", "hast", ",", "f\u00fcr", "der", "Ge\u00b7sch\u00f6p\u00b7fe", "Pran\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Von GOtt Empfindlichkeit empfangen.", "tokens": ["Von", "Gott", "Emp\u00b7find\u00b7lich\u00b7keit", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Durch die f\u00fcnf Sinnen, als f\u00fcnf Th\u00fcren,", "tokens": ["Durch", "die", "f\u00fcnf", "Sin\u00b7nen", ",", "als", "f\u00fcnf", "Th\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "NN", "$,", "KOUS", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Kann deine Seel\u2019 ihr\u2019 Anmuht r\u00fchren.", "tokens": ["Kann", "dei\u00b7ne", "Seel'", "ih\u00b7r'", "An\u00b7muht", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Er giebt uns Sinnen und Verstand,", "tokens": ["Er", "giebt", "uns", "Sin\u00b7nen", "und", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Durch beydes wird Sein Werk erkannt.", "tokens": ["Durch", "bey\u00b7des", "wird", "Sein", "Werk", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die Pflanzen haben gleichsam keines,", "tokens": ["Die", "Pflan\u00b7zen", "ha\u00b7ben", "gleich\u00b7sam", "kei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Die Thiere von den beyden eines,", "tokens": ["Die", "Thie\u00b7re", "von", "den", "bey\u00b7den", "ei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "PIAT", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Wir haben beydes. Doch vereinen wir beydes im Gebrau-", "tokens": ["Wir", "ha\u00b7ben", "bey\u00b7des", ".", "Doch", "ver\u00b7ei\u00b7nen", "wir", "bey\u00b7des", "im", "Ge\u00b7brau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "$.", "KON", "VVFIN", "PPER", "PIS", "APPRART", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "che nicht;", "tokens": ["che", "nicht", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKNEG", "$."], "meter": "-+", "measure": "iambic.single"}, "line.23": {"text": "So thun wir gegen unsre Pflicht.", "tokens": ["So", "thun", "wir", "ge\u00b7gen", "uns\u00b7re", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}