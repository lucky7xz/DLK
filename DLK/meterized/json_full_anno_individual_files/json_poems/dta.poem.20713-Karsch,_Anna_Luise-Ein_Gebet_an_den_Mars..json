{"dta.poem.20713": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Ein Gebet an den Mars.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du Gott des Krieges, la\u00df die Erde!                 ", "tokens": ["Du", "Gott", "des", "Krie\u00b7ges", ",", "la\u00df", "die", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "VVIMP", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Schritt, mit Blut bemerkt, ist f\u00fcrchterlich, ist", "tokens": ["Dein", "Schritt", ",", "mit", "Blut", "be\u00b7merkt", ",", "ist", "f\u00fcrch\u00b7ter\u00b7lich", ",", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "APPR", "NN", "VVPP", "$,", "VAFIN", "ADJD", "$,", "VAFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "schwer,", "tokens": ["schwer", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Ver\u00e4ndre doch die schreckliche Geb\u00e4rde,", "tokens": ["Ver\u00b7\u00e4nd\u00b7re", "doch", "die", "schreck\u00b7li\u00b7che", "Ge\u00b7b\u00e4r\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und sch\u00fcttle l\u00e4nger nicht den Speer.", "tokens": ["Und", "sch\u00fctt\u00b7le", "l\u00e4n\u00b7ger", "nicht", "den", "Speer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein wartet der Olymp, und Amor mit dem Bogen", "tokens": ["Dein", "war\u00b7tet", "der", "O\u00b7lymp", ",", "und", "A\u00b7mor", "mit", "dem", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVFIN", "ART", "NN", "$,", "KON", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Lauscht an der Mutter Fu\u00df. Steig von des Mordens", "tokens": ["Lauscht", "an", "der", "Mut\u00b7ter", "Fu\u00df", ".", "Steig", "von", "des", "Mor\u00b7dens"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$.", "NN", "APPR", "ART", "NN"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Bahn", "tokens": ["Bahn"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Zur G\u00f6ttin; dann betr\u00fcg\u2019 den schlafenden Vulkan,", "tokens": ["Zur", "G\u00f6t\u00b7tin", ";", "dann", "be\u00b7tr\u00fcg'", "den", "schla\u00b7fen\u00b7den", "Vul\u00b7kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Wie er vor Zeiten ward betrogen.", "tokens": ["Wie", "er", "vor", "Zei\u00b7ten", "ward", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Von Waffenschmieden ist er matt,", "tokens": ["Von", "Waf\u00b7fen\u00b7schmie\u00b7den", "ist", "er", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wie Venus, die nach dir sechs Jahr geschmachtet hat.", "tokens": ["Wie", "Ve\u00b7nus", ",", "die", "nach", "dir", "sechs", "Jahr", "ge\u00b7schmach\u00b7tet", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "APPR", "PPER", "CARD", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie reizend liegt sie da im Elis\u00e4er Lenze!", "tokens": ["Wie", "rei\u00b7zend", "liegt", "sie", "da", "im", "E\u00b7li\u00b7s\u00e4\u00b7er", "Len\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Nymphe windet dir und Venus Mirtenkr\u00e4nze,", "tokens": ["Die", "Nym\u00b7phe", "win\u00b7det", "dir", "und", "Ve\u00b7nus", "Mir\u00b7ten\u00b7kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mit Blumen untermengt. Schon gie\u00dft sie Nectartrank", "tokens": ["Mit", "Blu\u00b7men", "un\u00b7ter\u00b7mengt", ".", "Schon", "gie\u00dft", "sie", "Nec\u00b7tar\u00b7trank"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In goldne Schaalen ein; und wenn auch G\u00f6tter", "tokens": ["In", "gold\u00b7ne", "Schaa\u00b7len", "ein", ";", "und", "wenn", "auch", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$.", "KON", "KOUS", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "krank", "tokens": ["krank"], "token_info": ["word"], "pos": ["ADJD"], "meter": "-", "measure": "single.down"}, "line.18": {"text": "F\u00fcr hei\u00dfer Sehnsucht sind, so ist\u2019s gewi\u00df Cythere!", "tokens": ["F\u00fcr", "hei\u00b7\u00dfer", "Sehn\u00b7sucht", "sind", ",", "so", "ist's", "ge\u00b7wi\u00df", "Cy\u00b7the\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "$,", "ADV", "VAFIN", "ADV", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Horch im Get\u00fcmmel auf, sie seufzet g\u00f6ttlich, h\u00f6re!", "tokens": ["Horch", "im", "Ge\u00b7t\u00fcm\u00b7mel", "auf", ",", "sie", "seuf\u00b7zet", "g\u00f6tt\u00b7lich", ",", "h\u00f6\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "ADJD", "$,", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.20": {"text": "Begieb vom Kampfplatz dich zur\u00fcck,", "tokens": ["Be\u00b7gieb", "vom", "Kampf\u00b7platz", "dich", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Geharnischt wie du bist, an Haupt, an Arm und", "tokens": ["Ge\u00b7har\u00b7nischt", "wie", "du", "bist", ",", "an", "Haupt", ",", "an", "Arm", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KOKOM", "PPER", "VAFIN", "$,", "APPR", "NN", "$,", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Fu\u00dfe.", "tokens": ["Fu\u00b7\u00dfe", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Cupido zieht dich aus, und deinem ersten Kusse", "tokens": ["Cu\u00b7pi\u00b7do", "zieht", "dich", "aus", ",", "und", "dei\u00b7nem", "ers\u00b7ten", "Kus\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Dankt unsre ganze Welt ihr Gl\u00fcck.", "tokens": ["Dankt", "uns\u00b7re", "gan\u00b7ze", "Welt", "ihr", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Der Zorn in einer Frau rief, Mavors, dich hernieder,", "tokens": ["Der", "Zorn", "in", "ei\u00b7ner", "Frau", "rief", ",", "Ma\u00b7vors", ",", "dich", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,", "NE", "$,", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Sehnsucht einer Frau hol\u2019 dich den G\u00f6ttern", "tokens": ["Die", "Sehn\u00b7sucht", "ei\u00b7ner", "Frau", "hol'", "dich", "den", "G\u00f6t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "wieder,", "tokens": ["wie\u00b7der", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.28": {"text": "Und ewig komm\u2019 uns nicht zur\u00fcck.", "tokens": ["Und", "e\u00b7wig", "komm'", "uns", "nicht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}