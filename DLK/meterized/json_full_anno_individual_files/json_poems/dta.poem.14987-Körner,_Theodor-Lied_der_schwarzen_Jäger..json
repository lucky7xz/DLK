{"dta.poem.14987": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Lied der schwarzen J\u00e4ger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Auf deutsches Volk, zum Krieg!", "tokens": ["Auf", "deut\u00b7sches", "Volk", ",", "zum", "Krieg", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ins Feld, ins Feld! hoch flattern unsre Fahnen,", "tokens": ["Ins", "Feld", ",", "ins", "Feld", "!", "hoch", "flat\u00b7tern", "uns\u00b7re", "Fah\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$.", "ADJD", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie f\u00fchren uns zum Sieg.", "tokens": ["Sie", "f\u00fch\u00b7ren", "uns", "zum", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Klein ist die Schaar, doch gro\u00df ist das Vertrauen", "tokens": ["Klein", "ist", "die", "Schaar", ",", "doch", "gro\u00df", "ist", "das", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf den gerechten Gott!", "tokens": ["Auf", "den", "ge\u00b7rech\u00b7ten", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo seine Engel ihre Vesten bauen,", "tokens": ["Wo", "sei\u00b7ne", "En\u00b7gel", "ih\u00b7re", "Ves\u00b7ten", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind H\u00f6llenk\u00fcnste Spott.", "tokens": ["Sind", "H\u00f6l\u00b7len\u00b7k\u00fcns\u00b7te", "Spott", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gebt kein Pardon! K\u00f6nnt Ihr das Schwerdt nicht heben:", "tokens": ["Gebt", "kein", "Par\u00b7don", "!", "K\u00f6nnt", "Ihr", "das", "Schwerdt", "nicht", "he\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "$.", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "So w\u00fcrgt sie ohne Scheu,", "tokens": ["So", "w\u00fcrgt", "sie", "oh\u00b7ne", "Scheu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hoch verkauft den letzten Tropfen Leben,", "tokens": ["Und", "hoch", "ver\u00b7kauft", "den", "letz\u00b7ten", "Trop\u00b7fen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Tod macht alle frei.", "tokens": ["Der", "Tod", "macht", "al\u00b7le", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Noch trauren wir im schwarzen R\u00e4cherkleide", "tokens": ["Noch", "trau\u00b7ren", "wir", "im", "schwar\u00b7zen", "R\u00e4\u00b7cher\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um den gestorbnen Muth,", "tokens": ["Um", "den", "ge\u00b7storb\u00b7nen", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch fragt man euch, was dieses Roth bedeute,", "tokens": ["Doch", "fragt", "man", "euch", ",", "was", "die\u00b7ses", "Roth", "be\u00b7deu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPER", "$,", "PWS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das deutet Frankenblut.", "tokens": ["Das", "deu\u00b7tet", "Fran\u00b7ken\u00b7blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mit Gott! \u2014 Einst geht hoch \u00fcber Feindes Leichen", "tokens": ["Mit", "Gott", "!", "Einst", "geht", "hoch", "\u00fc\u00b7ber", "Fein\u00b7des", "Lei\u00b7chen"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "$(", "ADV", "VVFIN", "ADJD", "APPR", "NN", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Der Stern des Friedens auf,", "tokens": ["Der", "Stern", "des", "Frie\u00b7dens", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann pflanzen wir ein wei\u00dfes Siegeszeichen", "tokens": ["Dann", "pflan\u00b7zen", "wir", "ein", "wei\u00b7\u00dfes", "Sie\u00b7ges\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am freien Rheinstrom auf.", "tokens": ["Am", "frei\u00b7en", "Rhein\u00b7strom", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}