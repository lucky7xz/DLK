{"textgrid.poem.34832": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf dem Faubourg Saint-Marceau", "tokens": ["Auf", "dem", "Faub\u00b7ourg", "Saint\u00b7Ma\u00b7rceau"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lag der Nebel heute morgen,", "tokens": ["Lag", "der", "Ne\u00b7bel", "heu\u00b7te", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sp\u00e4therbstnebel, dicht und schwer,", "tokens": ["Sp\u00e4t\u00b7herbst\u00b7ne\u00b7bel", ",", "dicht", "und", "schwer", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Einer wei\u00dfen Nacht vergleichbar.", "tokens": ["Ei\u00b7ner", "wei\u00b7\u00dfen", "Nacht", "ver\u00b7gleich\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wandelnd durch die wei\u00dfe Nacht,", "tokens": ["Wan\u00b7delnd", "durch", "die", "wei\u00b7\u00dfe", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaut ich mir vor\u00fcbergleiten", "tokens": ["Schaut", "ich", "mir", "vor\u00b7\u00fc\u00b7berg\u00b7lei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "P