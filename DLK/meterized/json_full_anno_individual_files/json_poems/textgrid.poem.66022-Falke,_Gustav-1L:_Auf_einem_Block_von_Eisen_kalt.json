eter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr braunblond Haar, den K\u00fcssemund,", "tokens": ["Ihr", "braun\u00b7blond", "Haar", ",", "den", "K\u00fcs\u00b7se\u00b7mund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den schlanken Leib, maifrisch, gesund,", "tokens": ["Den", "schlan\u00b7ken", "Leib", ",", "maif\u00b7risch", ",", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die gro\u00dfen, grauen Augen, trug", "tokens": ["Die", "gro\u00b7\u00dfen", ",", "grau\u00b7en", "Au\u00b7gen", ",", "trug"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein erdbeerfarben Kleid, und schlug", "tokens": ["Ein", "erd\u00b7beer\u00b7far\u00b7ben", "Kleid", ",", "und", "schlug"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit ihrem kleinen Hammer brav", "tokens": ["Mit", "ih\u00b7rem", "klei\u00b7nen", "Ham\u00b7mer", "brav"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Aufs Herz mir. Jeder Schlag der traf.", "tokens": ["Aufs", "Herz", "mir", ".", "Je\u00b7der", "Schlag", "der", "traf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "$.", "PIAT", "NN", "ART", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und von den frischen Lippen klang", "tokens": ["Und", "von", "den", "fri\u00b7schen", "Lip\u00b7pen", "klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ein r\u00fchrend s\u00fc\u00dfer Kindersang.", "tokens": ["Ein", "r\u00fch\u00b7rend", "s\u00fc\u00b7\u00dfer", "Kin\u00b7der\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wechselnd fielen Schlag auf Schlag", "tokens": ["Und", "wech\u00b7selnd", "fie\u00b7len", "Schlag", "auf", "Schlag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die beiden Hammer mir aufs Herz,", "tokens": ["Die", "bei\u00b7den", "Ham\u00b7mer", "mir", "aufs", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der h\u00fclflos ich gefesselt lag,", "tokens": ["Der", "h\u00fcl\u00b7flos", "ich", "ge\u00b7fes\u00b7selt", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lippen biss und schrie vor Schmerz.", "tokens": ["Die", "Lip\u00b7pen", "biss", "und", "schrie", "vor", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis unertr\u00e4glich war die Qual.", "tokens": ["Bis", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "war", "die", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Ruck! Hinklirrt der Kettenstahl.", "tokens": ["Ein", "Ruck", "!", "Hink\u00b7lirrt", "der", "Ket\u00b7ten\u00b7stahl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Sorge rei\u00df' ich aus der Faust", "tokens": ["Der", "Sor\u00b7ge", "rei\u00df'", "ich", "aus", "der", "Faust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Eisenhammer. Niedersaust", "tokens": ["Den", "Ei\u00b7sen\u00b7ham\u00b7mer", ".", "Nie\u00b7der\u00b7saust"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$.", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der angstgef\u00fchrte, wuchtige Hieb", "tokens": ["Der", "angst\u00b7ge\u00b7f\u00fchr\u00b7te", ",", "wuch\u00b7ti\u00b7ge", "Hieb"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Und trifft, o Gott, und trifft mein Lieb.", "tokens": ["Und", "trifft", ",", "o", "Gott", ",", "und", "trifft", "mein", "Lieb", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "FM", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sie sinkt, sie seufzt \u2013 \u2013 \u2013", "tokens": ["Sie", "sinkt", ",", "sie", "seufzt", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$(", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Vergieb! Vergieb!", "tokens": ["Ver\u00b7gieb", "!", "Ver\u00b7gieb", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVIMP", "$.", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Am Boden wein' ich bitterlich.", "tokens": ["Am", "Bo\u00b7den", "wein'", "ich", "bit\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die Sorge aber weidet sich", "tokens": ["Die", "Sor\u00b7ge", "a\u00b7ber", "wei\u00b7det", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "An meinem Schmerz mit kaltem Hohn", "tokens": ["An", "mei\u00b7nem", "Schmerz", "mit", "kal\u00b7tem", "Hohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und hebt den schweren Hammer schon,", "tokens": ["Und", "hebt", "den", "schwe\u00b7ren", "Ham\u00b7mer", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Schlag zu, schlag zu \u2013 \u2013 \u2013.", "tokens": ["Schlag", "zu", ",", "schlag", "zu", "\u2013", "\u2013", "\u2013", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "APPR", "$(", "$(", "$(", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.