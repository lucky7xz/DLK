{"textgrid.poem.66022": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf einem Block von Eisen kalt", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf einem Block von Eisen kalt", "tokens": ["Auf", "ei\u00b7nem", "Block", "von", "Ei\u00b7sen", "kalt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lag r\u00fccklings ich und festgeschnallt,", "tokens": ["Lag", "r\u00fcck\u00b7lings", "ich", "und", "fest\u00b7ge\u00b7schnallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und neben mir die Sorge stand.", "tokens": ["Und", "ne\u00b7ben", "mir", "die", "Sor\u00b7ge", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit sehnigem Arm und harter Hand", "tokens": ["Mit", "seh\u00b7ni\u00b7gem", "Arm", "und", "har\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sie ihren schweren Hammer schwang,", "tokens": ["Sie", "ih\u00b7ren", "schwe\u00b7ren", "Ham\u00b7mer", "schwang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein knochig Weib mit welken Br\u00fcsten,", "tokens": ["Ein", "kno\u00b7chig", "Weib", "mit", "wel\u00b7ken", "Br\u00fcs\u0