{"dta.poem.20037": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "St. Walderichs Kapelle zu Murrhardt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In alter Burg auf wolk'ger H\u00f6h'", "tokens": ["In", "al\u00b7ter", "Burg", "auf", "wol\u00b7k'\u00b7ger", "H\u00f6h'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der fromme Kaiser Ludwig sa\u00df,", "tokens": ["Der", "from\u00b7me", "Kai\u00b7ser", "Lud\u00b7wig", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er trug im Herzen manches Weh,", "tokens": ["Er", "trug", "im", "Her\u00b7zen", "man\u00b7ches", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Schmerz er nimmermehr genas.", "tokens": ["Vom", "Schmerz", "er", "nim\u00b7mer\u00b7mehr", "ge\u00b7nas", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl sang durch Waldes Einsamkeit", "tokens": ["Wohl", "sang", "durch", "Wal\u00b7des", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit s\u00fc\u00dfem Ton die Nachtigall,", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "Ton", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nicht verscheucht des Kaisers Leid", "tokens": ["Doch", "nicht", "ver\u00b7scheucht", "des", "Kai\u00b7sers", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In stiller Nacht der liebe Schall.", "tokens": ["In", "stil\u00b7ler", "Nacht", "der", "lie\u00b7be", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wohl sah des Mondes milder Schein", "tokens": ["Wohl", "sah", "des", "Mon\u00b7des", "mil\u00b7der", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch manchen dichtbelaubten Baum,", "tokens": ["Durch", "man\u00b7chen", "dicht\u00b7be\u00b7laub\u00b7ten", "Baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Kaiser schlief in Thr\u00e4nen ein,", "tokens": ["Der", "Kai\u00b7ser", "schlief", "in", "Thr\u00e4\u00b7nen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch tr\u00e4umt' er wundersamen Traum.", "tokens": ["Doch", "tr\u00e4umt'", "er", "wun\u00b7der\u00b7sa\u00b7men", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Bei einem Kreutz im gr\u00fcnen Thal,", "tokens": ["Bei", "ei\u00b7nem", "Kreutz", "im", "gr\u00fc\u00b7nen", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sah er einen Greisen knien,", "tokens": ["Da", "sah", "er", "ei\u00b7nen", "Grei\u00b7sen", "kni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Haupt bekr\u00f6nt mit heil'gem Stral,", "tokens": ["Das", "Haupt", "be\u00b7kr\u00f6nt", "mit", "heil'\u00b7gem", "Stral", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu seinen F\u00fc\u00dfen Lilien bl\u00fch'n.", "tokens": ["Zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", "Li\u00b7li\u00b7en", "bl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PPOSAT", "NN", "NE", "VVFIN", "NE"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Vom Himmel eine Stimme ruft:", "tokens": ["Vom", "Him\u00b7mel", "ei\u00b7ne", "Stim\u00b7me", "ruft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201efolg' ihm, er wird dein Helfer seyn!\u201c", "tokens": ["\u201e", "fol\u00b7g'", "ihm", ",", "er", "wird", "dein", "Hel\u00b7fer", "seyn", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da ward so gl\u00e4nzend blau die Luft,", "tokens": ["Da", "ward", "so", "gl\u00e4n\u00b7zend", "blau", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aufbl\u00fcht' das Thal in Duft und Schein.", "tokens": ["Auf\u00b7bl\u00fcht'", "das", "Thal", "in", "Duft", "und", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es schwand der Traum, sein Auge war", "tokens": ["Es", "schwand", "der", "Traum", ",", "sein", "Au\u00b7ge", "war"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch thr\u00e4nenschwer am lichten Tag:", "tokens": ["Noch", "thr\u00e4\u00b7nen\u00b7schwer", "am", "lich\u00b7ten", "Tag", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Kind der Nacht, der Thau, so klar", "tokens": ["Das", "Kind", "der", "Nacht", ",", "der", "Thau", ",", "so", "klar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf himmelblauer Blume lag.", "tokens": ["Auf", "him\u00b7mel\u00b7blau\u00b7er", "Blu\u00b7me", "lag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Es schwang auf's treue Ro\u00df sobald", "tokens": ["Es", "schwang", "auf's", "treu\u00b7e", "Ro\u00df", "so\u00b7bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kaiser sich und ritt zu Thal,", "tokens": ["Der", "Kai\u00b7ser", "sich", "und", "ritt", "zu", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die V\u00f6gel sangen hell im Wald,", "tokens": ["Die", "V\u00f6\u00b7gel", "san\u00b7gen", "hell", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gr\u00fc\u00dfend die Sonn' und ihn zumal.", "tokens": ["Gr\u00fc\u00b7\u00dfend", "die", "Sonn'", "und", "ihn", "zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "KON", "PPER", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Er ritt hinab vom Wolkenstein,", "tokens": ["Er", "ritt", "hin\u00b7ab", "vom", "Wol\u00b7ken\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Also ward seine Burg genannt,", "tokens": ["Al\u00b7so", "ward", "sei\u00b7ne", "Burg", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.3": {"text": "Es lag das Thal in lichtem Schein,", "tokens": ["Es", "lag", "das", "Thal", "in", "lich\u00b7tem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es stand so segenreich das Land.", "tokens": ["Es", "stand", "so", "se\u00b7gen\u00b7reich", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Jezt sah er fern drei Lilien bl\u00fch'n,", "tokens": ["Jezt", "sah", "er", "fern", "drei", "Li\u00b7li\u00b7en", "bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie warfen milden Schein in's Thal!", "tokens": ["Sie", "war\u00b7fen", "mil\u00b7den", "Schein", "in's", "Thal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er sah beim Kreutz den Heil'gen knien,", "tokens": ["Er", "sah", "beim", "Kreutz", "den", "Heil'\u00b7gen", "kni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Haupt bekr\u00f6nt mit Himmelsstral.", "tokens": ["Sein", "Haupt", "be\u00b7kr\u00f6nt", "mit", "Him\u00b7mels\u00b7stral", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Da sprang er von dem treuen Ro\u00df,", "tokens": ["Da", "sprang", "er", "von", "dem", "treu\u00b7en", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eilt' fr\u00f6hlich auf den Greisen zu,", "tokens": ["Eilt'", "fr\u00f6h\u00b7lich", "auf", "den", "Grei\u00b7sen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Go\u00df allen Schmerz in seinen Schoo\u00df,", "tokens": ["Go\u00df", "al\u00b7len", "Schmerz", "in", "sei\u00b7nen", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schon erf\u00fchlt' er alte Ruh'.", "tokens": ["Und", "schon", "er\u00b7f\u00fchlt'", "er", "al\u00b7te", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "\u201etrag' ab den Wolkenstein zur Stund' \u2014", "tokens": ["\u201e", "trag'", "ab", "den", "Wol\u00b7ken\u00b7stein", "zur", "Stund'"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Also der heil'ge ", "tokens": ["Al\u00b7so", "der", "heil'\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Stell' eine Kirch' in Thales Grund,", "tokens": ["Stell'", "ei\u00b7ne", "Kirch'", "in", "Tha\u00b7les", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und denk' an des Erl\u00f6sers Schmach!\u201c", "tokens": ["Und", "denk'", "an", "des", "Er\u00b7l\u00f6\u00b7sers", "Schmach", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Drauf schwand dahin der heil'ge Greis,", "tokens": ["Drauf", "schwand", "da\u00b7hin", "der", "heil'\u00b7ge", "Greis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn fand nicht mehr des Kaisers Blick',", "tokens": ["Ihn", "fand", "nicht", "mehr", "des", "Kai\u00b7sers", "Blick'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch blieben die drei Lilien wei\u00df,", "tokens": ["Doch", "blie\u00b7ben", "die", "drei", "Li\u00b7li\u00b7en", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "CARD", "NN", "VVFIN", "$,"], "meter": "-+--+---+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Doch blieb das Kreutz im Thal zur\u00fcck.", "tokens": ["Doch", "blieb", "das", "Kreutz", "im", "Thal", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Der fromme Ludwig lie\u00df sobald", "tokens": ["Der", "from\u00b7me", "Lud\u00b7wig", "lie\u00df", "so\u00b7bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Abtragen seinen Wolkenstein,", "tokens": ["Ab\u00b7tra\u00b7gen", "sei\u00b7nen", "Wol\u00b7ken\u00b7stein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Er sezt ihn aus dem d\u00fcstern Wald", "tokens": ["Er", "sezt", "ihn", "aus", "dem", "d\u00fcs\u00b7tern", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Thal in Mond- und Sonnenschein.", "tokens": ["Zu", "Thal", "in", "Mon\u00b7d", "und", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Zur Kirche ward er umgebaut.", "tokens": ["Zur", "Kir\u00b7che", "ward", "er", "um\u00b7ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Kreutze kniet von dieser Zeit", "tokens": ["Beim", "Kreut\u00b7ze", "kniet", "von", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Duldsam der Kaiser, bald vertraut", "tokens": ["Duld\u00b7sam", "der", "Kai\u00b7ser", ",", "bald", "ver\u00b7traut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "ADV", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mit des Erl\u00f6sers h\u00f6her'm Leid.", "tokens": ["Mit", "des", "Er\u00b7l\u00f6\u00b7sers", "h\u00f6\u00b7her'm", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}