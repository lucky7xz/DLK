{"dta.poem.11595": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Seltsamer Traum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sahe n\u00e4chtlich hinter Traumgardinen", "tokens": ["Ich", "sa\u00b7he", "n\u00e4cht\u00b7lich", "hin\u00b7ter", "Traum\u00b7gar\u00b7di\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Viel Fr\u00fchlingsg\u00e4rten bl\u00fchn und immer \u00e4ndern;", "tokens": ["Viel", "Fr\u00fch\u00b7lings\u00b7g\u00e4r\u00b7ten", "bl\u00fchn", "und", "im\u00b7mer", "\u00e4n\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es tanzten, klein, auf zierlichen Gel\u00e4ndern", "tokens": ["Es", "tanz\u00b7ten", ",", "klein", ",", "auf", "zier\u00b7li\u00b7chen", "Ge\u00b7l\u00e4n\u00b7dern"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An hundert Figaros mit Cherubinen.", "tokens": ["An", "hun\u00b7dert", "Fi\u00b7ga\u00b7ros", "mit", "Che\u00b7ru\u00b7bi\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie alle Dinge hundertfach erschienen,", "tokens": ["Wie", "al\u00b7le", "Din\u00b7ge", "hun\u00b7dert\u00b7fach", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So sah ich zwischen \u201eMasken, Blumen, B\u00e4ndern,\u201c", "tokens": ["So", "sah", "ich", "zwi\u00b7schen", "\u201e", "Mas\u00b7ken", ",", "Blu\u00b7men", ",", "B\u00e4n\u00b7dern", ",", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "$(", "NN", "$,", "NN", "$,", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und zwischen all den \u201eseidenen Gew\u00e4ndern\u201c", "tokens": ["Und", "zwi\u00b7schen", "all", "den", "\u201e", "sei\u00b7de\u00b7nen", "Ge\u00b7w\u00e4n\u00b7dern", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "ART", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Einfach die Einzigen, ", "tokens": ["Ein\u00b7fach", "die", "Ein\u00b7zi\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Und aus dem sammtnen Fr\u00fchlingsboden stiegen,", "tokens": ["Und", "aus", "dem", "sammt\u00b7nen", "Fr\u00fch\u00b7lings\u00b7bo\u00b7den", "stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gehoben von melodischen Gewalten,", "tokens": ["Ge\u00b7ho\u00b7ben", "von", "me\u00b7lo\u00b7di\u00b7schen", "Ge\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die Leidenschaften auf als ernste Schatten;", "tokens": ["Die", "Lei\u00b7den\u00b7schaf\u00b7ten", "auf", "als", "erns\u00b7te", "Schat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da sah ich, still, mit tief gefurchten Z\u00fcgen,", "tokens": ["Da", "sah", "ich", ",", "still", ",", "mit", "tief", "ge\u00b7furch\u00b7ten", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einfach ", "tokens": ["Ein\u00b7fach"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}}}}