{"dta.poem.13613": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Dichtergl\u00fcck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "O Welt, bin dein Kind nicht von Hause,", "tokens": ["O", "Welt", ",", "bin", "dein", "Kind", "nicht", "von", "Hau\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Du hast mir nichts geschenkt,", "tokens": ["Du", "hast", "mir", "nichts", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So hab' ich denn frisch meine Klause", "tokens": ["So", "hab'", "ich", "denn", "frisch", "mei\u00b7ne", "Klau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "PPOSAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "In Morgenroth mir versenkt.", "tokens": ["In", "Mor\u00b7gen\u00b7roth", "mir", "ver\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Fortuna, streif' nur die H\u00f6hen", "tokens": ["For\u00b7tu\u00b7na", ",", "streif'", "nur", "die", "H\u00f6\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wende dein Angesicht,", "tokens": ["Und", "wen\u00b7de", "dein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich bleibe im Wald bei den Rehen,", "tokens": ["Ich", "blei\u00b7be", "im", "Wald", "bei", "den", "Re\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Flieg' zu, wir brauchen dich nicht.", "tokens": ["Flieg'", "zu", ",", "wir", "brau\u00b7chen", "dich", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Und ob auf H\u00f6h'n und im Grunde", "tokens": ["Und", "ob", "auf", "H\u00f6h'n", "und", "im", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "NN", "KON", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Kein Streifchen auch meine blieb,", "tokens": ["Kein", "Streif\u00b7chen", "auch", "mei\u00b7ne", "blieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "PPOSAT", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich segne dich, sch\u00f6ne Runde,", "tokens": ["Ich", "seg\u00b7ne", "dich", ",", "sch\u00f6\u00b7ne", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich habe dich dennoch so lieb!", "tokens": ["Ich", "ha\u00b7be", "dich", "den\u00b7noch", "so", "lieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}