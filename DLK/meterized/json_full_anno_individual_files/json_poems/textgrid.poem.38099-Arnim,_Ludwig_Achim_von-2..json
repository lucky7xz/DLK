{"textgrid.poem.38099": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herr der sprach: \u00bbFrau k\u00f6nnt ihr mich bescheiden nun,", "tokens": ["Der", "Herr", "der", "sprach", ":", "\u00bb", "Frau", "k\u00f6nnt", "ihr", "mich", "be\u00b7schei\u00b7den", "nun", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "$.", "$(", "NN", "VVFIN", "PPER", "PRF", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ihr jetzund gegessen hand,", "tokens": ["Was", "ihr", "je\u00b7tzund", "ge\u00b7ges\u00b7sen", "hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df euchs der lieb Gott lohne.\u00ab", "tokens": ["Da\u00df", "euchs", "der", "lieb", "Gott", "loh\u00b7ne", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJD", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Frau die sprach: \u00bbUnd das wei\u00df ich sicher nicht", "tokens": ["Die", "Frau", "die", "sprach", ":", "\u00bb", "Und", "das", "wei\u00df", "ich", "si\u00b7cher", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VVFIN", "$.", "$(", "KON", "PDS", "VVFIN", "PPER", "PRF", "PTKNEG"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ich wollts also gern wissen thun,", "tokens": ["Ich", "wollts", "a