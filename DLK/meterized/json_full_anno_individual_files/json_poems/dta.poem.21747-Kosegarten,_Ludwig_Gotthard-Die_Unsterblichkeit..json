{"dta.poem.21747": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die Unsterblichkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die ihr des freundlichen Lichts", "tokens": ["Die", "ihr", "des", "freund\u00b7li\u00b7chen", "Lichts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Euch daseynsselig erfreuet,", "tokens": ["Euch", "da\u00b7seyns\u00b7se\u00b7lig", "er\u00b7freu\u00b7et", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00f6stet euch, Br\u00fcder, ihr werdet", "tokens": ["Tr\u00f6s\u00b7tet", "euch", ",", "Br\u00fc\u00b7der", ",", "ihr", "wer\u00b7det"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "PPER", "VAFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Ewig des Lichtes euch freun.", "tokens": ["E\u00b7wig", "des", "Lich\u00b7tes", "euch", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Was wir ersehnten,", "tokens": ["Was", "wir", "er\u00b7sehn\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mit des J\u00fcnglings Sehnsucht", "tokens": ["Mit", "des", "J\u00fcng\u00b7lings", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nach dem Kuss der Geliebten,", "tokens": ["Nach", "dem", "Kuss", "der", "Ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Es ist, es ist mir erschienen.", "tokens": ["Es", "ist", ",", "es", "ist", "mir", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Was wir ersehnten, erflehten,", "tokens": ["Was", "wir", "er\u00b7sehn\u00b7ten", ",", "er\u00b7fleh\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es hat, es hat mich ergriffen,", "tokens": ["Es", "hat", ",", "es", "hat", "mich", "er\u00b7grif\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie den J\u00fcngling die Eidschwurgewissheit,", "tokens": ["Wie", "den", "J\u00fcng\u00b7ling", "die", "Eid\u00b7schwur\u00b7ge\u00b7wiss\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Dass, die er liebet, ihn liebe.", "tokens": ["Dass", ",", "die", "er", "lie\u00b7bet", ",", "ihn", "lie\u00b7be", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie den S\u00fcnder die Gnade ergreift,", "tokens": ["Wie", "den", "S\u00fcn\u00b7der", "die", "Gna\u00b7de", "er\u00b7greift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Wie den B\u00fcsser der Vergebung Gef\u00fchl,", "tokens": ["Wie", "den", "B\u00fcs\u00b7ser", "der", "Ver\u00b7ge\u00b7bung", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "So ergriff den Vernichtungscheuen", "tokens": ["So", "er\u00b7griff", "den", "Ver\u00b7nich\u00b7tung\u00b7scheu\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Unsterblichkeit, dein grosses Gef\u00fchl.", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7keit", ",", "dein", "gros\u00b7ses", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Ich ahnet', ich hofft' es, jetzt glaub' ich, dass ich bin!", "tokens": ["Ich", "ah\u00b7net'", ",", "ich", "hofft'", "es", ",", "jetzt", "glaub'", "ich", ",", "dass", "ich", "bin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VAFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ich glaub' es, ich schau' es, dass ich ewig", "tokens": ["Ich", "glaub'", "es", ",", "ich", "schau'", "es", ",", "dass", "ich", "e\u00b7wig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "bin! \u2014", "tokens": ["bin", "!"], "token_info": ["word", "punct", "punct"], "pos": ["VAFIN", "$.", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Neige deine Wipfel, Eiche!", "tokens": ["Nei\u00b7ge", "dei\u00b7ne", "Wip\u00b7fel", ",", "Ei\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ein Unsterblicher wandelt unter dir.", "tokens": ["Ein", "U\u00b7nsterb\u00b7li\u00b7cher", "wan\u00b7delt", "un\u00b7ter", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "R\u00fcnde die silberne Scheibe, Mond!", "tokens": ["R\u00fcn\u00b7de", "die", "sil\u00b7ber\u00b7ne", "Schei\u00b7be", ",", "Mond", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Entblinket dem Nachtged\u00fcft, schimmer\u00e4ugige", "tokens": ["Ent\u00b7blin\u00b7ket", "dem", "Nacht\u00b7ge\u00b7d\u00fcft", ",", "schim\u00b7mer\u00b7\u00e4u\u00b7gi\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJA"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sterne!", "tokens": ["Ster\u00b7ne", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Sirius, w\u00e4lze dein Flammenrad! Glanzge-", "tokens": ["Si\u00b7rius", ",", "w\u00e4l\u00b7ze", "dein", "Flam\u00b7men\u00b7rad", "!", "Glanz\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "VVFIN", "PPOSAT", "NN", "$.", "TRUNC"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Wandle stattlich den Riesengang!", "tokens": ["Wand\u00b7le", "statt\u00b7lich", "den", "Rie\u00b7sen\u00b7gang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Minder, ihr Stolzen, als ich,", "tokens": ["Min\u00b7der", ",", "ihr", "Stol\u00b7zen", ",", "als", "ich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Seyd ihr, ihr seyd verg\u00e4nglich!", "tokens": ["Seyd", "ihr", ",", "ihr", "seyd", "ver\u00b7g\u00e4ng\u00b7lich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mehr als die Eich' und der Mond, mehr", "tokens": ["Mehr", "als", "die", "Eich'", "und", "der", "Mond", ",", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "KOKOM", "ART", "NN", "KON", "ART", "NN", "$,", "ADV"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "als Orion und Sirius", "tokens": ["als", "O\u00b7rion", "und", "Si\u00b7rius"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NE"], "meter": "-+---+", "measure": "dactylic.init"}, "line.5": {"text": "Bin ich \u2014 bin unverg\u00e4nglich.", "tokens": ["Bin", "ich", "bin", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Himmel und Erde vergehn!", "tokens": ["Him\u00b7mel", "und", "Er\u00b7de", "ver\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Nimmer vergehet das Ich! \u2014", "tokens": ["Nim\u00b7mer", "ver\u00b7ge\u00b7het", "das", "Ich", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "PPER", "$.", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ha, wenn das Ich verginge,", "tokens": ["Ha", ",", "wenn", "das", "Ich", "ver\u00b7gin\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PDS", "PPER", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Was w\u00e4re diess nichtige Seyn?", "tokens": ["Was", "w\u00e4\u00b7re", "diess", "nich\u00b7ti\u00b7ge", "Seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Eines Traumes Schatten,", "tokens": ["Ei\u00b7nes", "Trau\u00b7mes", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Getr\u00e4umt im zweifelnden Zwielicht,", "tokens": ["Ge\u00b7tr\u00e4umt", "im", "zwei\u00b7feln\u00b7den", "Zwie\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zerschwunden mit des Tages D\u00e4mmerung,", "tokens": ["Zer\u00b7schwun\u00b7den", "mit", "des", "Ta\u00b7ges", "D\u00e4m\u00b7me\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "W\u00e4re diess nichtige Seyn!", "tokens": ["W\u00e4\u00b7re", "diess", "nich\u00b7ti\u00b7ge", "Seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.10": {"line.1": {"text": "\u00c4rmer noch w\u00e4r' ich, als der Halm und das Gras;", "tokens": ["\u00c4r\u00b7mer", "noch", "w\u00e4r'", "ich", ",", "als", "der", "Halm", "und", "das", "Gras", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ver\u00e4chtlicher noch, als der Kiesel der Gasse.", "tokens": ["Ver\u00b7\u00e4cht\u00b7li\u00b7cher", "noch", ",", "als", "der", "Kie\u00b7sel", "der", "Gas\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Des Daseyns Entz\u00fccken empfanden sie", "tokens": ["Des", "Da\u00b7seyns", "Ent\u00b7z\u00fc\u00b7cken", "emp\u00b7fan\u00b7den", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "nicht;", "tokens": ["nicht", ";"], "token_info": ["word", "punct"], "pos": ["PTKNEG", "$."], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Dein Grauen Vernichtung empfinden sie", "tokens": ["Dein", "Grau\u00b7en", "Ver\u00b7nich\u00b7tung", "emp\u00b7fin\u00b7den", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "nimmer.", "tokens": ["nim\u00b7mer", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.11": {"line.1": {"text": "Ach, wenn ich ewig nicht w\u00e4re,", "tokens": ["Ach", ",", "wenn", "ich", "e\u00b7wig", "nicht", "w\u00e4\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADJD", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So \u00e4chzt' ich dem kommenden Tag'", "tokens": ["So", "\u00e4chzt'", "ich", "dem", "kom\u00b7men\u00b7den", "Tag'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Entgegen, so \u00e4chzt' ich, k\u00e4me die Nacht,", "tokens": ["Ent\u00b7ge\u00b7gen", ",", "so", "\u00e4chzt'", "ich", ",", "k\u00e4\u00b7me", "die", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und verh\u00fcllte mich, und schwiege ver-", "tokens": ["Und", "ver\u00b7h\u00fcll\u00b7te", "mich", ",", "und", "schwie\u00b7ge", "ver"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KON", "ADJA", "TRUNC"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "So w\u00fcrd' ich unter die Blumen des Fr\u00fchlings", "tokens": ["So", "w\u00fcrd'", "ich", "un\u00b7ter", "die", "Blu\u00b7men", "des", "Fr\u00fch\u00b7lings"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mich strecken, und die Blume beneiden.", "tokens": ["Mich", "stre\u00b7cken", ",", "und", "die", "Blu\u00b7me", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du, o bl\u00fchende Erde, d\u00e4uchtest mir ein", "tokens": ["Du", ",", "o", "bl\u00fc\u00b7hen\u00b7de", "Er\u00b7de", ",", "d\u00e4uch\u00b7test", "mir", "ein"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "FM", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "offnes Grab;", "tokens": ["off\u00b7nes", "Grab", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Die Menschen zerfliessende Schatten.", "tokens": ["Die", "Men\u00b7schen", "zer\u00b7flies\u00b7sen\u00b7de", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.13": {"line.1": {"text": "Dich, herrliches Vorrecht des Geistes,", "tokens": ["Dich", ",", "herr\u00b7li\u00b7ches", "Vor\u00b7recht", "des", "Geis\u00b7tes", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Unergrundliches hohes Bewusstseyn,", "tokens": ["Un\u