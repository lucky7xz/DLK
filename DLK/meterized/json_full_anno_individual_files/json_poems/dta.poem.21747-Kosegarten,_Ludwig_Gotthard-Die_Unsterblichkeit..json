, "ret"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "ITJ", "TRUNC"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wohin, Verirrte, wohin? Ermanne dich,", "tokens": ["Wo\u00b7hin", ",", "Ver\u00b7irr\u00b7te", ",", "wo\u00b7hin", "?", "Er\u00b7man\u00b7ne", "dich", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVFIN", "$,", "PWAV", "$.", "VVFIN", "PPER", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Seele! Ein Schall ist's,", "tokens": ["See\u00b7le", "!", "Ein", "Schall", "ist's", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VAFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ein hohler Schall, der dich \u00e4ngstet.", "tokens": ["Ein", "hoh\u00b7ler", "Schall", ",", "der", "dich", "\u00e4ngs\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.18": {"line.1": {"text": "Ist hienieden auch Tod?", "tokens": ["Ist", "hien\u00b7ie\u00b7den", "auch", "Tod", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Auch Untergang hienieden, und Vertilgung?", "tokens": ["Auch", "Un\u00b7ter\u00b7gang", "hien\u00b7ie\u00b7den", ",", "und", "Ver\u00b7til\u00b7gung", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist, was Tod wir nennen und Untergang