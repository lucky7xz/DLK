{"dta.poem.21383": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XVIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gensd\u2019armen in Leichenlaken geh\u00fcllt,", "tokens": ["Gens\u00b7d'\u00b7ar\u00b7men", "in", "Lei\u00b7chen\u00b7la\u00b7ken", "ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ein wei\u00dfes Spukgewirre,", "tokens": ["Ein", "wei\u00b7\u00dfes", "Spuk\u00b7ge\u00b7wir\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Umringte mein Bett, ich h\u00f6rte auch", "tokens": ["Um\u00b7ring\u00b7te", "mein", "Bett", ",", "ich", "h\u00f6r\u00b7te", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Unheimliches Kettengeklirre.", "tokens": ["Un\u00b7heim\u00b7li\u00b7ches", "Ket\u00b7ten\u00b7ge\u00b7klir\u00b7re", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Ach! die Gespenster schleppten mich fort,", "tokens": ["Ach", "!", "die", "Ge\u00b7spens\u00b7ter", "schlepp\u00b7ten", "mich", "fort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und ich hab\u2019 mich endlich befunden", "tokens": ["Und", "ich", "hab'", "mich", "end\u00b7lich", "be\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "An einer steilen Felsenwand;", "tokens": ["An", "ei\u00b7ner", "stei\u00b7len", "Fel\u00b7sen\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dort war ich festgebunden.", "tokens": ["Dort", "war", "ich", "fest\u00b7ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der b\u00f6se schmutzige Betthimmelquast!", "tokens": ["Der", "b\u00f6\u00b7se", "schmut\u00b7zi\u00b7ge", "Bet\u00b7thim\u00b7mel\u00b7quast", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich fand ihn gleichfalls wieder,", "tokens": ["Ich", "fand", "ihn", "gleich\u00b7falls", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch sah er jetzt wie ein Geyer aus,", "tokens": ["Doch", "sah", "er", "jetzt", "wie", "ein", "Ge\u00b7yer", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "KOKOM", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Krallen und schwarzem Gefieder.", "tokens": ["Mit", "Kral\u00b7len", "und", "schwar\u00b7zem", "Ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Er glich dem preu\u00dfischen Adler jetzt,", "tokens": ["Er", "glich", "dem", "preu\u00b7\u00dfi\u00b7schen", "Ad\u00b7ler", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und hielt meinen Leib umklammert;", "tokens": ["Und", "hielt", "mei\u00b7nen", "Leib", "um\u00b7klam\u00b7mert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er fra\u00df mir die Leber aus der Brust,", "tokens": ["Er", "fra\u00df", "mir", "die", "Le\u00b7ber", "aus", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich habe gest\u00f6hnt und gejammert.", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7st\u00f6hnt", "und", "ge\u00b7jam\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}