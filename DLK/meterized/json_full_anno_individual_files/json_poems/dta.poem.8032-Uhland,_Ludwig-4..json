{"dta.poem.8032": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Schwarze Wolken ziehn hinunter,", "tokens": ["Schwar\u00b7ze", "Wol\u00b7ken", "ziehn", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Golden stralt die Sonne wieder,", "tokens": ["Gol\u00b7den", "stralt", "die", "Son\u00b7ne", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fern verhallen schon die Donner,", "tokens": ["Fern", "ver\u00b7hal\u00b7len", "schon", "die", "Don\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die V\u00f6gelch\u00f6re singen;", "tokens": ["Und", "die", "V\u00f6\u00b7gelc\u00b7h\u00f6\u00b7re", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Blumen heben sich und B\u00e4ume,", "tokens": ["Blu\u00b7men", "he\u00b7ben", "sich", "und", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind erfrischet vom Gewitter,", "tokens": ["Sind", "er\u00b7fri\u00b7schet", "vom", "Ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wanderer, die sich geborgen,", "tokens": ["Wan\u00b7de\u00b7rer", ",", "die", "sich", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schreiten wieder rasch von hinnen:", "tokens": ["Schrei\u00b7ten", "wie\u00b7der", "rasch", "von", "hin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "APPR", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nur des Waldes h\u00f6chste Eiche", "tokens": ["Nur", "des", "Wal\u00b7des", "h\u00f6chs\u00b7te", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Hebt nicht mehr die stolzen Wipfel,", "tokens": ["Hebt", "nicht", "mehr", "die", "stol\u00b7zen", "Wip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Nur Kastiliens bester Streiter", "tokens": ["Nur", "Kas\u00b7ti\u00b7li\u00b7ens", "bes\u00b7ter", "Strei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Bleibt am Fu\u00df der Eiche liegen.", "tokens": ["Bleibt", "am", "Fu\u00df", "der", "Ei\u00b7che", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}