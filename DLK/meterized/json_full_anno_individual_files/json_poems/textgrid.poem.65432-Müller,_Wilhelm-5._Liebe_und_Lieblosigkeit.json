{"textgrid.poem.65432": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "5. Liebe und Lieblosigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sage mir, mein spr\u00f6der Bruder, ob ein tr\u00fcber Tag im Mai", "tokens": ["Sa\u00b7ge", "mir", ",", "mein", "spr\u00f6\u00b7der", "Bru\u00b7der", ",", "ob", "ein", "tr\u00fc\u00b7ber", "Tag", "im", "Mai"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dir nicht lieber, als die helle Sternennacht des Jenners sei?", "tokens": ["Dir", "nicht", "lie\u00b7ber", ",", "als", "die", "hel\u00b7le", "Ster\u00b7nen\u00b7nacht", "des", "Jen\u00b7ners", "sei", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "$,", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und dann deut' ich dir es aus, um wie viel der Liebe Leiden", "tokens": ["Und", "dann", "deut'", "ich", "dir", "es", "aus", ",", "um", "wie", "viel", "der", "Lie\u00b7be", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "PPER", "PTKVZ", "$,", "KOUI", "PWAV", "PIS", "ART", "ADJA", "NN"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mir begehrenswerther sind, als die liebelosen Freuden.", "tokens": ["Mir", "be\u00b7geh\u00b7rens\u00b7wert\u00b7her", "sind", ",", "als",