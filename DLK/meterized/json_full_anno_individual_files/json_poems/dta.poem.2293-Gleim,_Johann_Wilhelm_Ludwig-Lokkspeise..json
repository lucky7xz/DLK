{"dta.poem.2293": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Lokkspeise.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Meinem Vater in der Grube", "tokens": ["Mei\u00b7nem", "Va\u00b7ter", "in", "der", "Gru\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dank ich noch f\u00fcr seine Liebe.", "tokens": ["Dank", "ich", "noch", "f\u00fcr", "sei\u00b7ne", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er hat einst durch seine Lehren", "tokens": ["Er", "hat", "einst", "durch", "sei\u00b7ne", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dis mein iunges Herz gebildet;", "tokens": ["Dis", "mein", "i\u00b7un\u00b7ges", "Herz", "ge\u00b7bil\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er gab mir, durch seine Lehren,", "tokens": ["Er", "gab", "mir", ",", "durch", "sei\u00b7ne", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Liebe zu den sch\u00f6nen K\u00fcnsten,", "tokens": ["Lie\u00b7be", "zu", "den", "sch\u00f6\u00b7nen", "K\u00fcns\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ein Herz voll Lehrbegierde,", "tokens": ["Und", "ein", "Herz", "voll", "Lehr\u00b7be\u00b7gier\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "La\u00dft uns doch die V\u00e4ter loben,", "tokens": ["La\u00dft", "uns", "doch", "die", "V\u00e4\u00b7ter", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Die uns nicht mit harten Worten,", "tokens": ["Die", "uns", "nicht", "mit", "har\u00b7ten", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die uns mit Vernunft und Schmeicheln", "tokens": ["Die", "uns", "mit", "Ver\u00b7nunft", "und", "Schmei\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Klug und Lehrbegierig machen.", "tokens": ["Klug", "und", "Lehr\u00b7be\u00b7gie\u00b7rig", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "La\u00dft uns k\u00fcnftig unsern Kindern", "tokens": ["La\u00dft", "uns", "k\u00fcnf\u00b7tig", "un\u00b7sern", "Kin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Lust und Liebe gr\u00f6sser machen.", "tokens": ["Lust", "und", "Lie\u00b7be", "gr\u00f6s\u00b7ser", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "La\u00dft uns unsre lieben V\u00e4ter", "tokens": ["La\u00dft", "uns", "uns\u00b7re", "lie\u00b7ben", "V\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "In der Lehrart \u00fcbertreffen!", "tokens": ["In", "der", "Le\u00b7hrart", "\u00fc\u00b7bert\u00b7ref\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Ja! ich will schon meine Kinder", "tokens": ["Ja", "!", "ich", "will", "schon", "mei\u00b7ne", "Kin\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VMFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "St\u00e4rker zu den K\u00fcnsten reitzen,", "tokens": ["St\u00e4r\u00b7ker", "zu", "den", "K\u00fcns\u00b7ten", "reit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Als mich einst mein Vater reitzte.", "tokens": ["Als", "mich", "einst", "mein", "Va\u00b7ter", "reitz\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Knabe, sprach er: Lerne schreiben,", "tokens": ["Kna\u00b7be", ",", "sprach", "er", ":", "Ler\u00b7ne", "schrei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$.", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Denn sonst kannst du bei dem F\u00fcrsten", "tokens": ["Denn", "sonst", "kannst", "du", "bei", "dem", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "K\u00fcnftig keine Sch\u00e4tze sammlen.", "tokens": ["K\u00fcnf\u00b7tig", "kei\u00b7ne", "Sch\u00e4t\u00b7ze", "samm\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Hurtig lernt ich alles schreiben.", "tokens": ["Hur\u00b7tig", "lernt", "ich", "al\u00b7les", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Denn ich liebte Kutsch und Sch\u00e4tze.", "tokens": ["Denn", "ich", "lieb\u00b7te", "Kutsch", "und", "Sch\u00e4t\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Aber, warlich, meine Knabeu", "tokens": ["A\u00b7ber", ",", "war\u00b7lich", ",", "mei\u00b7ne", "Kna\u00b7beu"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "ADV", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Sollens doch noch schneller lernen;", "tokens": ["Sol\u00b7lens", "doch", "noch", "schnel\u00b7ler", "ler\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Denn ich will sie besser reitzen.", "tokens": ["Denn", "ich", "will", "sie", "bes\u00b7ser", "reit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Liebste, ja! so will ich sagen,", "tokens": ["Liebs\u00b7te", ",", "ja", "!", "so", "will", "ich", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "$.", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Liebste Knaben, lernt doch schreiben,", "tokens": ["Liebs\u00b7te", "Kna\u00b7ben", ",", "lernt", "doch", "schrei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Denn sonst k\u00f6nnt ihr einst im Alter", "tokens": ["Denn", "sonst", "k\u00f6nnt", "ihr", "einst", "im", "Al\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "Keine Liebesbriefe wechseln.", "tokens": ["Kei\u00b7ne", "Lie\u00b7bes\u00b7brie\u00b7fe", "wech\u00b7seln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "O! wie werden sie dann lernen.", "tokens": ["O", "!", "wie", "wer\u00b7den", "sie", "dann", "ler\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Lerne tanzen, sprach mein Vater,", "tokens": ["Ler\u00b7ne", "tan\u00b7zen", ",", "sprach", "mein", "Va\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Denn es macht geschikkte Glieder,", "tokens": ["Denn", "es", "macht", "ge\u00b7schikk\u00b7te", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Und ich lernte hurtig tanzen;", "tokens": ["Und", "ich", "lern\u00b7te", "hur\u00b7tig", "tan\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Aber h\u00e4tt er nur gesprochen:", "tokens": ["A\u00b7ber", "h\u00e4tt", "er", "nur", "ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Lieber Sohn! man kann beim Tanzen", "tokens": ["Lie\u00b7ber", "Sohn", "!", "man", "kann", "beim", "Tan\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$.", "PIS", "VMFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Manche sch\u00f6ne H\u00e4nde dr\u00fckken,", "tokens": ["Man\u00b7che", "sch\u00f6\u00b7ne", "H\u00e4n\u00b7de", "dr\u00fck\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Die sich sonst nicht dr\u00fckken lassen,", "tokens": ["Die", "sich", "sonst", "nicht", "dr\u00fck\u00b7ken", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Und man kann im sanften Dr\u00fckken,", "tokens": ["Und", "man", "kann", "im", "sanf\u00b7ten", "Dr\u00fck\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Klugen Sch\u00f6nen alles sagen,", "tokens": ["Klu\u00b7gen", "Sch\u00f6\u00b7nen", "al\u00b7les", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Was wir sonst nicht sagen d\u00fcrfen;", "tokens": ["Was", "wir", "sonst", "nicht", "sa\u00b7gen", "d\u00fcr\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Drum so rath ich, lerne tantzen:", "tokens": ["Drum", "so", "rath", "ich", ",", "ler\u00b7ne", "tant\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "O! so w\u00fcrd ich itzt im Tanzen", "tokens": ["O", "!", "so", "w\u00fcrd", "ich", "itzt", "im", "Tan\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Dich, o Lani! \u00fcbertreffen.", "tokens": ["Dich", ",", "o", "La\u00b7ni", "!", "\u00fc\u00b7bert\u00b7ref\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "FM", "FM", "$.", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "O! wie will ich meine Kinder", "tokens": ["O", "!", "wie", "will", "ich", "mei\u00b7ne", "Kin\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Zu den Wissenschaften reitzen!", "tokens": ["Zu", "den", "Wis\u00b7sen\u00b7schaf\u00b7ten", "reit\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "O! was vor gelehrte Knaben", "tokens": ["O", "!", "was", "vor", "ge\u00b7lehr\u00b7te", "Kna\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Werden meine Lehren ziehen.", "tokens": ["Wer\u00b7den", "mei\u00b7ne", "Leh\u00b7ren", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}