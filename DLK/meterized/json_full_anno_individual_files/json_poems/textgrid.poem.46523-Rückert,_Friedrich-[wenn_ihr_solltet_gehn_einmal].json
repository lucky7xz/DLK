{"textgrid.poem.46523": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wenn ihr solltet gehn einmal]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ihr solltet gehn einmal,", "tokens": ["Wenn", "ihr", "soll\u00b7tet", "gehn", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "War es gut, ihr gingt zusammen,", "tokens": ["War", "es", "gut", ",", "ihr", "gingt", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht, da\u00df ging in Eins die Qual,", "tokens": ["Nicht", ",", "da\u00df", "ging", "in", "Eins", "die", "Qual", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sondern da\u00df als Doppelflammen", "tokens": ["Son\u00b7dern", "da\u00df", "als", "Dop\u00b7pel\u00b7flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihr im Tod auch ungetrennet", "tokens": ["Ihr", "im", "Tod", "auch", "un\u00b7ge\u00b7tren\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf dem Hausaltar uns brennet.", "tokens": ["Auf", "dem", "Haus\u00b7al\u00b7tar", "uns", "bren\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was verloren, ist dahin,", "tokens": ["Was", "ver\u00b7lo\u00b7ren", ",", "ist", "da\u00b7hin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVPP", "$,", "VAFIN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und kein Wunsch kann es regieren,", "tokens": ["Und", "kein", "Wunsch", "kann", "es", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch Gefahr l\u00e4uft schwacher Sinn,", "tokens": ["Doch", "Ge\u00b7fahr", "l\u00e4uft", "schwa\u00b7cher", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch einmal es zu verlieren;", "tokens": ["Noch", "ein\u00b7mal", "es", "zu", "ver\u00b7lie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn erst ganz, wenn wir's vergessen,", "tokens": ["Denn", "erst", "ganz", ",", "wenn", "wir's", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "KOUS", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist verloren, was besessen.", "tokens": ["Ist", "ver\u00b7lo\u00b7ren", ",", "was", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PWS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Darum brennt euch so in's Herz,", "tokens": ["Da\u00b7rum", "brennt", "euch", "so", "in's", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewige Geschwisterflammen,", "tokens": ["E\u00b7wi\u00b7ge", "Ge\u00b7schwis\u00b7ter\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df nie der lebend'ge Schmerz", "tokens": ["Da\u00df", "nie", "der", "le\u00b7ben\u00b7d'\u00b7ge", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sink' in Aschentod zusammen,", "tokens": ["Sink'", "in", "A\u00b7schen\u00b7tod", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sondern gl\u00fche fort in milder", "tokens": ["Son\u00b7dern", "gl\u00fc\u00b7he", "fort", "in", "mil\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Farbengluth wie eure Bilder!", "tokens": ["Far\u00b7beng\u00b7luth", "wie", "eu\u00b7re", "Bil\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn ihr solltet gehn einmal,", "tokens": ["Wenn", "ihr", "soll\u00b7tet", "gehn", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos":