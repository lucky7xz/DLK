{"textgrid.poem.63674": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wenn ich einst mich werde schlafen legen,", "tokens": ["Und", "wenn", "ich", "einst", "mich", "wer\u00b7de", "schla\u00b7fen", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPER", "VAFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gern wacht' ich wieder auf nach hundert Jahren,", "tokens": ["Gern", "wacht'", "ich", "wie\u00b7der", "auf", "nach", "hun\u00b7dert", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur einen kurzen Tag, um zu erfahren,", "tokens": ["Nur", "ei\u00b7nen", "kur\u00b7zen", "Tag", ",", "um", "zu", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie es die Menschen dann wohl treiben m\u00f6gen.", "tokens": ["Wie", "es", "die", "Men\u00b7schen", "dann", "wohl", "trei\u00b7ben", "m\u00f6\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da\u00df dann erbl\u00fcht des ew'gen Friedens Segen,", "tokens": ["Da\u00df", "dann", "er\u00b7bl\u00fcht", "des", "ew'\u00b7gen", "Frie\u00b7dens", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht mehr geknutet wird im Reich des Zaren,", "tokens": ["Nicht", "mehr", "ge\u00b7knu\u00b7tet", "wird", "im", "Reich", "des", "Za\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVPP", "VAFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht jene mehr sich liegen in den Haaren,", "tokens": ["Nicht", "je\u00b7ne", "mehr", "sich", "lie\u00b7gen", "in", "den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDS", "ADV", "PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Liebe predigen von Amtes wegen,", "tokens": ["Die", "Lie\u00b7be", "pre\u00b7di\u00b7gen", "von", "Am\u00b7tes", "we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bezweifl' ich sehr. Doch da\u00df auf deutschen B\u00fchnen,", "tokens": ["Be\u00b7zweifl'", "ich", "sehr", ".", "Doch", "da\u00df", "auf", "deut\u00b7schen", "B\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$.", "KON", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Fremde gar zu oft ein Gastspiel geben,", "tokens": ["Wenn", "Frem\u00b7de", "gar", "zu", "oft", "ein", "Gast\u00b7spiel", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PTKA", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir unsres Hausrechts endlich uns bedienen,", "tokens": ["Wir", "uns\u00b7res", "Haus\u00b7rechts", "end\u00b7lich", "uns", "be\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nicht unterw\u00fcrfig in den Himmel heben,", "tokens": ["Nicht", "un\u00b7ter\u00b7w\u00fcr\u00b7fig", "in", "den", "Him\u00b7mel", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was besser uns, nur weil es fremd, erschienen,", "tokens": ["Was", "bes\u00b7ser", "uns", ",", "nur", "weil", "es", "fremd", ",", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "$,", "ADV", "KOUS", "PPER", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das hoff' ich wohl posthum noch zu erleben.", "tokens": ["Das", "hoff'", "ich", "wohl", "post\u00b7hum", "noch", "zu", "er\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und wenn ich einst mich werde schlafen legen,", "tokens": ["Und", "wenn", "ich", "einst", "mich", "wer\u00b7de", "schla\u00b7fen", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word",