{"textgrid.poem.31410": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dv fette Wunder Weid / ein Schatz-Platz seltner Gaben /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dv fette Wunder Weid / ein Schatz-Platz seltner Gaben /", "tokens": ["Dv", "fet\u00b7te", "Wun\u00b7der", "Weid", "/", "ein", "Schatz\u00b7Platz", "selt\u00b7ner", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Werkstatt der Gottes G\u00fct' / ein schaub\u00fchn seiner Pracht!", "tokens": ["Werk\u00b7statt", "der", "Got\u00b7tes", "G\u00fct'", "/", "ein", "schau\u00b7b\u00fchn", "sei\u00b7ner", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$(", "ART", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wo Noht und Glaube sich zur Allmacht Zunder macht /", "tokens": ["wo", "Noht", "und", "Glau\u00b7be", "sich", "zur", "All\u00b7macht", "Zun\u00b7der", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PRF", "APPRART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ein Wunder-Thaten Liecht sie bald entflammet haben.", "tokens": ["ein", "Wun\u00b7der\u00b7Tha\u00b7ten", "Liecht", "sie", "bald", "ent\u00b7flam\u00b7met", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Felse mu\u00df mit Safft / die Lufft mit Wachteln / laben.", "tokens": ["Der", "Fel\u00b7se", "mu\u00df", "mit", "Safft", "/", "die", "Lufft", "mit", "Wach\u00b7teln", "/", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN", "$(", "ART", "NN", "APPR", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "das unerdenklichste / von Gott wird ausgedacht /", "tokens": ["das", "un\u00b7er\u00b7denk\u00b7lichs\u00b7te", "/", "von", "Gott", "wird", "aus\u00b7ge\u00b7dacht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "zu seines Volkes H\u00fclff / so hoch ist es geacht.", "tokens": ["zu", "sei\u00b7nes", "Vol\u00b7kes", "H\u00fclff", "/", "so", "hoch", "ist", "es", "ge\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$(", "ADV", "ADJD", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "die W\u00fcsten wuste nicht / was Schatz' in ihr vergraben.", "tokens": ["die", "W\u00fcs\u00b7ten", "wus\u00b7te", "nicht", "/", "was", "Schatz'", "in", "ihr", "ver\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$(", "PWS", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O wunderreicher Gott! Ach Allheit \u00fcberal