{"textgrid.poem.32413": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "91. Auf einen gewissen Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihn singen so viel m\u00e4\u00df'ge Dichter,", "tokens": ["Ihn", "sin\u00b7gen", "so", "viel", "m\u00e4\u00df'\u00b7ge", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihn preisen so viel dunkle Richter,", "tokens": ["Ihn", "prei\u00b7sen", "so", "viel", "dunk\u00b7le", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihn ahmt so mancher St\u00fcmper nach,", "tokens": ["Ihn", "ahmt", "so", "man\u00b7cher", "St\u00fcm\u00b7per", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm nicht zum Ruhm, und sich zur Schmach.", "tokens": ["Ihm", "nicht", "zum", "Ruhm", ",", "und", "sich", "zur", "Schmach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPRART", "NN", "$,", "KON", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Freund, dir die Wahrheit zu gestehen,", "tokens": ["Freund", ",", "dir", "die", "Wahr\u00b7heit", "zu", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ART", "NN", "PTKZU"