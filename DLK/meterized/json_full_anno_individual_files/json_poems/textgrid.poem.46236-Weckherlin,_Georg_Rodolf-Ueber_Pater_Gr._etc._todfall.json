{"textgrid.poem.46236": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ueber Pater Gr. etc. todfall", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df kein schimpf bleib ohn erst, da\u00df kein spil end ohn spot,", "tokens": ["Da\u00df", "kein", "schimpf", "bleib", "ohn", "erst", ",", "da\u00df", "kein", "spil", "end", "ohn", "spot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJD", "VVFIN", "APPR", "ADV", "$,", "KOUS", "PIAT", "ADJD", "VVPP", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "hat neulich man zu Rom in euerm spil gesehen:", "tokens": ["hat", "neu\u00b7lich", "man", "zu", "Rom", "in", "eu\u00b7erm", "spil", "ge\u00b7se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "APPR", "NE", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der pater fand in ernst in der h\u00f6ll seinen tod,", "tokens": ["der", "pa\u00b7ter", "fand", "in", "ernst", "in", "der", "h\u00f6ll", "sei\u00b7nen", "tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJD", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ihr suchtet euch in schimpf im himmel zu erh\u00f6hen;", "tokens": ["ihr", "such\u00b7tet", "euch", "in", "schimpf", "im", "him\u00b7mel", "zu", "er\u00b7h\u00f6\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr sa\u00dfet in dem spil lobsingend euerm got,", "tokens": ["Ihr", "sa\u00b7\u00dfet", "in", "dem", "spil", "lob\u00b7sin\u00b7gend", "eu\u00b7erm", "got", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "er aber lag in ernst f\u00fcr Lucifer mit flehen;", "tokens": ["er", "a\u00b7ber", "lag", "in", "ernst", "f\u00fcr", "Lu\u00b7ci\u00b7fer", "mit", "fle\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJD", "APPR", "NE", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "in dem spil zeigtet ihr all euern pracht ohn not,", "tokens": ["in", "dem", "spil", "zeig\u00b7tet", "ihr", "all", "eu\u00b7ern", "pracht", "ohn", "not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "er, wahin nach dem spil ihr alle m\u00fcsset gehen.", "tokens": ["er", ",", "wa\u00b7hin", "nach", "dem", "spil", "ihr", "al\u00b7le", "m\u00fcs\u00b7set", "ge\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "APPR", "ART", "VVFIN", "PPER", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O hohe pfaffenwitz, wert des bapsts gro\u00dfen gunst,", "tok