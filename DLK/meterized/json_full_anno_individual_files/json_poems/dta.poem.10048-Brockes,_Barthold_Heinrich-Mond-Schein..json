{"dta.poem.10048": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mond-Schein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hatte j\u00fcngst, des holden Fr\u00fchlings-Pracht,", "tokens": ["Ich", "hat\u00b7te", "j\u00fcngst", ",", "des", "hol\u00b7den", "Fr\u00fch\u00b7lings\u00b7Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie alles auf der Welt so sch\u00f6n, so Wunder-sch\u00f6n,", "tokens": ["Wie", "al\u00b7les", "auf", "der", "Welt", "so", "sch\u00f6n", ",", "so", "Wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zum Ruhm des Sch\u00f6pfers, anzusehn,", "tokens": ["Zum", "Ruhm", "des", "Sch\u00f6p\u00b7fers", ",", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den gantzen Nachmittag vergn\u00fcglich zugebracht.", "tokens": ["Den", "gant\u00b7zen", "Nach\u00b7mit\u00b7tag", "ver\u00b7gn\u00fcg\u00b7lich", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}