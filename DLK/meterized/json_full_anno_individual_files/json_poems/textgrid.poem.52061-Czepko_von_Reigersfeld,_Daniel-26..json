{"textgrid.poem.52061": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "I\u00df'st du? Der Artzt der nimmt die besten Austern dir,", "tokens": ["I\u00df'st", "du", "?", "Der", "Artzt", "der", "nimmt", "die", "bes\u00b7ten", "Aus\u00b7tern", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$.", "ART", "NN", "ART", "VVFIN", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00e4lt sie vor Ungesund, ob sie Ihm gleich nichts schaden.", "tokens": ["H\u00e4lt", "sie", "vor", "Un\u00b7ge\u00b7sund", ",", "ob", "sie", "Ihm", "gleich", "nichts", "scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "KOUS", "PPER", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Trinckst du? alsbald du wilt die Zung im Golde baden", "tokens": ["Trinckst", "du", "?", "als\u00b7bald", "du", "wilt", "die", "Zung", "im", "Gol\u00b7de", "ba\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "KOUS", "PPER", "VMFIN", "ART", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Fast er den Kelch und setzt vor sich den Malvasier.", "tokens": ["Fast", "er", "den", "Kelch", "und", "setzt", "vor", "sich", "den", "Mal\u00b7va\u00b7sier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "APPR", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kranckst du? er kocht dir Tr\u00e4nck, ob er nicht gleich davon", "tokens": ["Kranckst", "du", "?", "er", "kocht", "dir", "Tr\u00e4nck", ",", "ob", "er", "nicht", "gleich", "da\u00b7von"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "NN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den minsten Tropffen schmeckt, und macht dir viel Beschwerden.", "tokens": ["Den", "mins\u00b7ten", "Tropf\u00b7fen", "schmeckt", ",", "und", "macht", "dir", "viel", "Be\u00b7schwer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Stirbst du? Weil er dir hilfft fein nach der Kunst zur Erden,", "tokens": ["Stirbst", "du", "?", "Weil", "er", "dir", "hilfft", "fein", "nach", "der", "Kunst", "zur", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "KOUS", "PPER", "PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Heischt als ein Hencker er noch vor den Tod sein Lohn.", "tokens": ["Heischt", "als", "ein", "Hen\u00b7cker", "er", "noch", "vor", "den", "Tod", "sein", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "PPER", "ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ja wol, man kan ihm kaum gerecht im Zahlen seyn,", "tokens": ["Ja", "wol", ",", "man", "kan", "ihm", "kaum", "ge\u00b7recht", "im", "Zah\u00b7len", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "PIS", "VMFIN", "PPER", "ADV", "ADJD", "APPRART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verdr\u00fc\u00dflich, weil man lebt, beschwerlich, wann wir sterben,", "tokens": ["Ver\u00b7dr\u00fc\u00df\u00b7lich", ",", "weil", "man", "lebt", ",", "be\u00b7schwer\u00b7lich", ",", "wann", "wir", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PIS", "VVFIN", "$,", "ADJD", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und unrecht nach dem Tod ist dieser Leute Werben:", "tokens": ["Und", "un\u00b7recht", "nach", "dem", "Tod", "ist", "die\u00b7ser", "Leu\u00b7te", "Wer\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VAFIN", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es steht da selten wol, wo sie gehn aus und ein.", "tokens": ["Es", "steht", "da", "sel\u00b7ten", "wol", ",", "wo", "sie", "gehn", "aus", "und", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ADV", "$,", "PWAV", "PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+