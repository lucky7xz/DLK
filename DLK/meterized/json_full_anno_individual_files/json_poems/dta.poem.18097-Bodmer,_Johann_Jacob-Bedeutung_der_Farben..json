{"dta.poem.18097": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Bedeutung der Farben.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00df, ist gantz keusche Reinigkeit,", "tokens": ["Wei\u00df", ",", "ist", "gantz", "keu\u00b7sche", "Rei\u00b7nig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Leibfarbe, Weh und Schmertzen leiden,", "tokens": ["Leib\u00b7far\u00b7be", ",", "Weh", "und", "Schmert\u00b7zen", "lei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Meergr\u00fcne, von einander scheiden,", "tokens": ["Meer\u00b7gr\u00fc\u00b7ne", ",", "von", "ein\u00b7an\u00b7der", "schei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PRF", "VVINF", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwartz, ist Betr\u00fcbni\u00df, Angst, und Leid,", "tokens": ["Schwartz", ",", "ist", "Be\u00b7tr\u00fcb\u00b7ni\u00df", ",", "Angst", ",", "und", "Leid", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "NN", "$,", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Roth, innigliche Liebes-Brunst,", "tokens": ["Roth", ",", "in\u00b7nig\u00b7li\u00b7che", "Lie\u00b7bes\u00b7Brunst", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Und Himmelblau, sehr hohe Sinnen,", "tokens": ["Und", "Him\u00b7mel\u00b7blau", ",", "sehr", "ho\u00b7he", "Sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bleich Leichfarb, argen Wahn gewinnen,", "tokens": ["Bleich", "Leich\u00b7farb", ",", "ar\u00b7gen", "Wahn", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gelb, End und Ausgang aller Gunst,", "tokens": ["Gelb", ",", "End", "und", "Aus\u00b7gang", "al\u00b7ler", "Gunst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "KON", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Haarfarbe, deutet auf Geduld,", "tokens": ["Haar\u00b7far\u00b7be", ",", "deu\u00b7tet", "auf", "Ge\u00b7duld", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Bleich Aschenfarben, heimlich Huld,", "tokens": ["Bleich", "A\u00b7schen\u00b7far\u00b7ben", ",", "heim\u00b7lich", "Huld", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Braun, aller Liebe gantz vergessen,", "tokens": ["Braun", ",", "al\u00b7ler", "Lie\u00b7be", "gantz", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Gr\u00fcn, Hoffnung; Und weil jetzund ich", "tokens": ["Gr\u00fcn", ",", "Hoff\u00b7nung", ";", "Und", "weil", "je\u00b7tzund", "ich"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "KON", "KOUS", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Gebrauche dieser Farbe mich,", "tokens": ["Ge\u00b7brau\u00b7che", "die\u00b7ser", "Far\u00b7be", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ist wol mein Zustand zu ermessen.", "tokens": ["Ist", "wol", "mein", "Zu\u00b7stand", "zu", "er\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}