{"textgrid.poem.24653": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen ungl\u00fcckseeligen und pl\u00f6tzlichen Todes-Fall eines Studiosi", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das dieser Erden Krei\u00df ein Unbestand der Freuden/", "tokens": ["Das", "die\u00b7ser", "Er\u00b7den", "Krei\u00df", "ein", "Un\u00b7be\u00b7stand", "der", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PDAT", "NN", "NN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Sammel-Platz der Qvaal/ ein Hau\u00df der Eitelkeit/", "tokens": ["Ein", "Sam\u00b7mel\u00b7Platz", "der", "Qvaal", "/", "ein", "Hau\u00df", "der", "Ei\u00b7tel\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Garten schn\u00f6der Pracht und kurtzer Sinnen-Weiden/", "tokens": ["Ein", "Gar\u00b7ten", "schn\u00f6\u00b7der", "Pracht", "und", "kurt\u00b7zer", "Sin\u00b7nen\u00b7Wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo Blumen unsrer Lust ein schneller Raub der Zeit/", "tokens": ["Wo", "Blu\u00b7men", "uns\u00b7rer", "Lust", "ein", "schnel\u00b7ler", "Raub", "der", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "L\u00e4\u00dft jene Wunder Macht den Menschen offt erschallen;", "tokens": ["L\u00e4\u00dft", "je\u00b7ne", "Wun\u00b7der", "Macht", "den", "Men\u00b7schen", "offt", "er\u00b7schal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch mu\u00df am schmertzlichsten itzt dieser Donner knallen.", "tokens": ["Doch", "mu\u00df", "am", "schmertz\u00b7lichs\u00b7ten", "itzt", "die\u00b7ser", "Don\u00b7ner", "knal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "ADJA", "ADV", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Ungl\u00fcck laurt auf uns/ indem wir kaum gebohren.", "tokens": ["Das", "Un\u00b7gl\u00fcck", "laurt", "auf", "uns", "/", "in\u00b7dem", "wir", "kaum", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$(", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Kindheit schont es nicht/ w\u00e4chst mit den Jahren gro\u00df/", "tokens": ["Der", "Kind\u00b7heit", "schont", "es", "nicht", "/", "w\u00e4chst", "mit", "den", "Jah\u00b7ren", "gro\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$(", "VVFIN", "APPR", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein Pfeil hat diesen bald/ bald jenen sich erkohren.", "tokens": ["Sein", "Pfeil", "hat", "die\u00b7sen", "bald", "/", "bald", "je\u00b7nen", "sich", "er\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PDS", "ADV", "$(", "ADV", "PDAT", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jemehr man lebt/ jemehr bricht aller Unfall lo\u00df.", "tokens": ["Je\u00b7mehr", "man", "lebt", "/", "je\u00b7mehr", "bricht", "al\u00b7ler", "Un\u00b7fall", "lo\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$(", "ADV", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn k\u00f6mmt zuletzt der Todt/ l\u00e4\u00dft seine Str\u00f6hme fliessen/", "tokens": ["Denn", "k\u00f6mmt", "zu\u00b7letzt", "der", "Todt", "/", "l\u00e4\u00dft", "sei\u00b7ne", "Str\u00f6h\u00b7me", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$(", "VVFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das Ubel dieser Welt durch unsern Sarg zu schliessen.", "tokens": ["Das", "U\u00b7bel", "die\u00b7ser", "Welt", "durch", "un\u00b7sern", "Sarg", "zu", "schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wohl diesem/ welcher sich wei\u00df kl\u00fcglich abzusterben/", "tokens": ["Wohl", "die\u00b7sem", "/", "wel\u00b7cher", "sich", "wei\u00df", "kl\u00fcg\u00b7lich", "ab\u00b7zu\u00b7ster\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "$(", "PRELS", "PRF", "VVFIN", "ADJD", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein Grab im Lentzen baut/ statt aller Jugend Lust", "tokens": ["Sein", "Grab", "im", "Lent\u00b7zen", "baut", "/", "statt", "al\u00b7ler", "Ju\u00b7gend", "Lust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$(", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An die Verwesung denckt/ des Leibes sein Verderben/", "tokens": ["An", "die", "Ver\u00b7we\u00b7sung", "denckt", "/", "des", "Lei\u00b7bes", "sein", "Ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$(", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und fa\u00dft zur Seelen Heil Gott st\u00fcndlich in die Brust.", "tokens": ["Und", "fa\u00dft", "zur", "See\u00b7len", "Heil", "Gott", "st\u00fcnd\u00b7lich", "in", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "NN", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kommt denn ein Ungl\u00fccks-Fall/ so mag die Schale brechen/", "tokens": ["Kommt", "denn", "ein", "Un\u00b7gl\u00fccks\u00b7Fall", "/", "so", "mag", "die", "Scha\u00b7le", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$(", "ADV", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn in der Muschel nur die Perle frey zu sprechen.", "tokens": ["Wenn", "in", "der", "Mu\u00b7schel", "nur", "die", "Per\u00b7le", "frey", "zu", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Jugend unsrer Lust/ die Anmuth sch\u00f6ner Glieder/", "tokens": ["Die", "Ju\u00b7gend", "uns\u00b7rer", "Lust", "/", "die", "An\u00b7muth", "sch\u00f6\u00b7ner", "Glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der hohen Eltern Trost/ der treuen Freunde Hertz/", "tokens": ["Der", "ho\u00b7hen", "El\u00b7tern", "Trost", "/", "der", "treu\u00b7en", "Freun\u00b7de", "Hertz", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sinckt j\u00e4mmerlich durch dich von \u2013 \u2013 nieder.", "tokens": ["Sinckt", "j\u00e4m\u00b7mer\u00b7lich", "durch", "dich", "von", "\u2013", "\u2013", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "APPR", "$(", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da du erkalten must bl\u00fcht deiner Jahre M\u00e4rtz.", "tokens": ["Da", "du", "er\u00b7kal\u00b7ten", "must", "bl\u00fcht", "dei\u00b7ner", "Jah\u00b7re", "M\u00e4rtz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Trost/ Anmuth/ Hertz und Lust/ hat nun der Todt getroffen/", "tokens": ["Trost", "/", "An\u00b7muth", "/", "Hertz", "und", "Lust", "/", "hat", "nun", "der", "Todt", "ge\u00b7trof\u00b7fen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "$(", "VAFIN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und legt damit ins Grab/ was noch von dir zu hoffen.", "tokens": ["Und", "legt", "da\u00b7mit", "ins", "Grab", "/", "was", "noch", "von", "dir", "zu", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPRART", "NN", "$(", "PWS", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie viel Geschicklichkeit dir die Natur gegeben/", "tokens": ["Wie", "viel", "Ge\u00b7schick\u00b7lich\u00b7keit", "dir", "die", "Na\u00b7tur", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Redligkeit und Treu in deiner Brust gebrandt/", "tokens": ["Wie", "Red\u00b7lig\u00b7keit", "und", "Treu", "in", "dei\u00b7ner", "Brust", "ge\u00b7brandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie weit dein Witz und Geist sich durch ein l\u00e4nger Leben", "tokens": ["Wie", "weit", "dein", "Witz", "und", "Geist", "sich", "durch", "ein", "l\u00e4n\u00b7ger", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "KON", "NN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gewi\u00df hervor gethan/ wei\u00df der/ so dich gekandt.", "tokens": ["Ge\u00b7wi\u00df", "her\u00b7vor", "ge\u00b7than", "/", "wei\u00df", "der", "/", "so", "dich", "ge\u00b7kandt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "VVPP", "$(", "VVFIN", "ART", "$(", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch nun erblassest du: zu tieff sind diese Wunden/", "tokens": ["Doch", "nun", "er\u00b7blas\u00b7sest", "du", ":", "zu", "tieff", "sind", "die\u00b7se", "Wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "PTKA", "ADJD", "VAFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und werden gantz durch nichts als Gottesfurcht verbunden.", "tokens": ["Und", "wer\u00b7den", "gantz", "durch", "nichts", "als", "Got\u00b7tes\u00b7furcht", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PIS", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Hochwohlgebohrnes Hau\u00df/ das grauen Adel zieret/", "tokens": ["Hoch\u00b7wohl\u00b7ge\u00b7bohr\u00b7nes", "Hau\u00df", "/", "das", "grau\u00b7en", "A\u00b7del", "zie\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das sein Verdienst erhebt/ das reife Klugheit prei\u00dft/", "tokens": ["Das", "sein", "Ver\u00b7dienst", "er\u00b7hebt", "/", "das", "rei\u00b7fe", "Klug\u00b7heit", "prei\u00dft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Wappen Ehr und Ruhm/ im Hertzen Tugend f\u00fchret/", "tokens": ["Im", "Wap\u00b7pen", "Ehr", "und", "Ruhm", "/", "im", "Hert\u00b7zen", "Tu\u00b7gend", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "$(", "APPRART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das sich bey jeden Fall mit wahrer Gro\u00dfmuth spei\u00dft/", "tokens": ["Das", "sich", "bey", "je\u00b7den", "Fall", "mit", "wah\u00b7rer", "Gro\u00df\u00b7muth", "spei\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PIAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df Creutz wird dein Gem\u00fcth so zu dem Himmel schicken/", "tokens": ["Di\u00df", "Creutz", "wird", "dein", "Ge\u00b7m\u00fcth", "so", "zu", "dem", "Him\u00b7mel", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df wir den Helden-Muth des Christenthums erblicken.", "tokens": ["Da\u00df", "wir", "den", "Hel\u00b7den\u00b7Muth", "des", "Chris\u00b7ten\u00b7thums", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Mehr Worte wollen uns die Thr\u00e4nen nicht erlauben/", "tokens": ["Mehr", "Wor\u00b7te", "wol\u00b7len", "uns", "die", "Thr\u00e4\u00b7nen", "nicht", "er\u00b7lau\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es schlie\u00dft ein wehrter Freund zwar pl\u00f6tzlich seinen Lauff/", "tokens": ["Es", "schlie\u00dft", "ein", "wehr\u00b7ter", "Freund", "zwar", "pl\u00f6tz\u00b7lich", "sei\u00b7nen", "Lauff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch sagt der Christen Lehr: ein Augenblick im Glauben/", "tokens": ["Doch", "sagt", "der", "Chris\u00b7ten", "Lehr", ":", "ein", "Au\u00b7gen\u00b7blick", "im", "Glau\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$.", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": 