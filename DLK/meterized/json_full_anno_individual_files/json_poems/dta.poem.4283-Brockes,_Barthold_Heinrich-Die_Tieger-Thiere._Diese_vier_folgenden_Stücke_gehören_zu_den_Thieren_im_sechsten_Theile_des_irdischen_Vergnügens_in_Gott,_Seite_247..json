{"dta.poem.4283": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Tieger-Thiere.   Diese vier folgenden St\u00fccke geh\u00f6ren zu den Thieren  \n im sechsten Theile des  irdischen Vergn\u00fcgens in  \n Gott,  Seite 247.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die gr\u00e4\u00dflich-sch\u00f6nen Tieger-Thiere verm\u00f6gen Lust,", "tokens": ["Die", "gr\u00e4\u00df\u00b7lich\u00b7sch\u00f6\u00b7nen", "Tie\u00b7ger\u00b7Thie\u00b7re", "ver\u00b7m\u00f6\u00b7gen", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "zugleich auch Schrecken,", "tokens": ["zu\u00b7gleich", "auch", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "So wie sie, wenn sie leben, thun, auch hier im Abri\u00df zu", "tokens": ["So", "wie", "sie", ",", "wenn", "sie", "le\u00b7ben", ",", "thun", ",", "auch", "hier", "im", "Ab\u00b7ri\u00df", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "$,", "KOUS", "PPER", "VVINF", "$,", "VVINF", "$,", "ADV", "ADV", "APPRART", "NN", "PTKZU"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "erwecken.", "tokens": ["er\u00b7we\u00b7cken", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Uns reizet ihre sch\u00f6ne Haut; jedoch ein falscher, finstrer", "tokens": ["Uns", "rei\u00b7zet", "ih\u00b7re", "sch\u00f6\u00b7ne", "Haut", ";", "je\u00b7doch", "ein", "fal\u00b7scher", ",", "finst\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "ADV", "ART", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Blick,", "tokens": ["Blick", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Der aus den schielen Augen strahlet, treibt auch den Tapfer-", "tokens": ["Der", "aus", "den", "schie\u00b7len", "Au\u00b7gen", "strah\u00b7let", ",", "treibt", "auch", "den", "Tap\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "sten zur\u00fcck.", "tokens": ["sten", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Bewundre, der du dieses siehst, wie weit sich hier die Kunst", "tokens": ["Be\u00b7wund\u00b7re", ",", "der", "du", "die\u00b7ses", "siehst", ",", "wie", "weit", "sich", "hier", "die", "Kunst"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "PDS", "VVFIN", "$,", "PWAV", "ADJD", "PRF", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "erstrecket,", "tokens": ["er\u00b7stre\u00b7cket", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Da man in wenig dunklen Strichen den Tieger-Geist im", "tokens": ["Da", "man", "in", "we\u00b7nig", "dunk\u00b7len", "Stri\u00b7chen", "den", "Tie\u00b7ger\u00b7Geist", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PIAT", "ADJA", "NN", "ART", "NN", "APPRART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Aug' entdecket.", "tokens": ["Aug'", "ent\u00b7de\u00b7cket", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Mein Blick glaubt in des Apfels Klarheit, als durch ein", "tokens": ["Mein", "Blick", "glaubt", "in", "des", "Ap\u00b7fels", "Klar\u00b7heit", ",", "als", "durch", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,", "KOUS", "APPR", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Glas, ins Hirn zu dringen,", "tokens": ["Glas", ",", "ins", "Hirn", "zu", "drin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Die schwarze Seele selbst zu seh\u2019n von dem, der unten ruht", "tokens": ["Die", "schwar\u00b7ze", "See\u00b7le", "selbst", "zu", "seh'n", "von", "dem", ",", "der", "un\u00b7ten", "ruht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "APPR", "ART", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "und schielt.", "tokens": ["und", "schielt", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Dort zeigt der halb-geschlo\u00dfne Blick des obern, welche Lust", "tokens": ["Dort", "zeigt", "der", "halb\u00b7ge\u00b7schlo\u00df\u00b7ne", "Blick", "des", "o\u00b7bern", ",", "wel\u00b7che", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "$,", "PWAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.18": {"text": "er f\u00fchlt", "tokens": ["er", "f\u00fchlt"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Von seiner Welpen starken Saugen, die man sich regen,", "tokens": ["Von", "sei\u00b7ner", "Wel\u00b7pen", "star\u00b7ken", "Sau\u00b7gen", ",", "die", "man", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,", "PRELS", "PIS", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "w\u00e4lzen, schwingen,", "tokens": ["w\u00e4l\u00b7zen", ",", "schwin\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Sich dreh\u2019n, ja sie fast schmatzen sieht. Wie schrecklich", "tokens": ["Sich", "dreh'n", ",", "ja", "sie", "fast", "schmat\u00b7zen", "sieht", ".", "Wie", "schreck\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "VVINF", "$,", "ADV", "PPER", "ADV", "VVINF", "VVFIN", "$.", "PWAV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "diese Thiere nun,", "tokens": ["die\u00b7se", "Thie\u00b7re", "nun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.23": {"text": "Wie wild die Wut, wie gro\u00df die St\u00e4rke, wodurch sie \u00f6fters", "tokens": ["Wie", "wild", "die", "Wut", ",", "wie", "gro\u00df", "die", "St\u00e4r\u00b7ke", ",", "wo\u00b7durch", "sie", "\u00f6f\u00b7ters"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "PWAV", "ADJD", "ART", "NN", "$,", "PWAV", "PPER", "ADV"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Schaden thun;", "tokens": ["Scha\u00b7den", "thun", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.25": {"text": "Hat Gott sie uns doch unterworfen, es wei\u00df der Mensch sie", "tokens": ["Hat", "Gott", "sie", "uns", "doch", "un\u00b7ter\u00b7wor\u00b7fen", ",", "es", "wei\u00df", "der", "Mensch", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "PPER", "PPER", "ADV", "VVPP", "$,", "PPER", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "doch zu zwingen.", "tokens": ["doch", "zu", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Was k\u00f6nnen nicht die Tieger-H\u00e4ute durch Handlung uns", "tokens": ["Was", "k\u00f6n\u00b7nen", "nicht", "die", "Tie\u00b7ger\u00b7H\u00e4u\u00b7te", "durch", "Hand\u00b7lung", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PTKNEG", "ART", "NN", "APPR", "NN", "PPER"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "f\u00fcr Vortheil bringen!", "tokens": ["f\u00fcr", "Vor\u00b7theil", "brin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "Sie dienen uns zur Zier und Pracht, sie k\u00f6nnen uns vor K\u00e4lte", "tokens": ["Sie", "die\u00b7nen", "uns", "zur", "Zier", "und", "Pracht", ",", "sie", "k\u00f6n\u00b7nen", "uns", "vor", "K\u00e4l\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "KON", "NN", "$,", "PPER", "VMFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.30": {"text": "sch\u00fctzen;", "tokens": ["sch\u00fct\u00b7zen", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.31": {"text": "Und also mu\u00df das Grimmigste der Thiere doch dem Menschen", "tokens": ["Und", "al\u00b7so", "mu\u00df", "das", "Grim\u00b7migs\u00b7te", "der", "Thie\u00b7re", "doch", "dem", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "ART", "NN", "ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+---+-+-+-", "measure": "unknown.measure.hexa"}, "line.32": {"text": "n\u00fctzen.", "tokens": ["n\u00fct\u00b7zen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}