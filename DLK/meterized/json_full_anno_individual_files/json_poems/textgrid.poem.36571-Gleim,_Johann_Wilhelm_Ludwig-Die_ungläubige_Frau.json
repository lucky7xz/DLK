{"textgrid.poem.36571": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die ungl\u00e4ubige Frau", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unbest\u00e4ndig ist er nicht", "tokens": ["Un\u00b7be\u00b7st\u00e4n\u00b7dig", "ist", "er", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser z\u00e4rtliche Geliebte,", "tokens": ["Die\u00b7ser", "z\u00e4rt\u00b7li\u00b7che", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser \u00fcber mich Betr\u00fcbte,", "tokens": ["Die\u00b7ser", "\u00fc\u00b7ber", "mich", "Be\u00b7tr\u00fcb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der von meinem Sterben spricht,", "tokens": ["Der", "von", "mei\u00b7nem", "Ster\u00b7ben", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unbest\u00e4ndig ist er nicht!", "tokens": ["Un\u00b7be\u00b7st\u00e4n\u00b7dig", "ist", "er", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber da\u00df er, st\u00fcrb' ich, nimmer", "tokens": ["A\u00b7ber", "da\u00df", "er", ",", "st\u00fcrb'", "ich", ",", "nim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "KOU