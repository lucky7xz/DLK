{"dta.poem.9601": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie ein lied in die darzu gespielte  \n theorbe sang.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann/ sch\u00f6nstes Fr\u00e4ulein/ sich dein kluger mund bewegt/", "tokens": ["Wann", "/", "sch\u00f6ns\u00b7tes", "Fr\u00e4u\u00b7lein", "/", "sich", "dein", "klu\u00b7ger", "mund", "be\u00b7wegt", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "ADJA", "NN", "$(", "PRF", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ein beseelter thon durch deine lippen dringet/", "tokens": ["Und", "ein", "be\u00b7seel\u00b7ter", "thon", "durch", "dei\u00b7ne", "lip\u00b7pen", "drin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der durch ein s\u00fcsses lied dem H\u00f6chsten opffer bringet/", "tokens": ["Der", "durch", "ein", "s\u00fcs\u00b7ses", "lied", "dem", "H\u00f6chs\u00b7ten", "opf\u00b7fer", "brin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird der himmel selbst zur andacht angeregt.", "tokens": ["So", "wird", "der", "him\u00b7mel", "selbst", "zur", "an\u00b7dacht", "an\u00b7ge\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein etwas/ das uns auch schier aus uns selber tr\u00e4gt/", "tokens": ["Ein", "et\u00b7was", "/", "das", "uns", "auch", "schier", "aus", "uns", "sel\u00b7ber", "tr\u00e4gt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$(", "PRELS", "PPER", "ADV", "ADJD", "APPR", "PPER", "ADV", "VVFIN", "$("], "meter": "++-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Ein gantz geheimer zug/ der unser hertz umringet", "tokens": ["Ein", "gantz", "ge\u00b7hei\u00b7mer", "zug", "/", "der", "un\u00b7ser", "hertz", "um\u00b7rin\u00b7get"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und durch verborgne krafft die geister selbst bezwinget/", "tokens": ["Und", "durch", "ver\u00b7borg\u00b7ne", "krafft", "die", "geis\u00b7ter", "selbst", "be\u00b7zwin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zeigt/ da\u00df dein hoher schall was g\u00f6ttlichs in sich hegt.", "tokens": ["Zeigt", "/", "da\u00df", "dein", "ho\u00b7her", "schall", "was", "g\u00f6tt\u00b7lichs", "in", "sich", "hegt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "PPOSAT", "ADJA", "NN", "PWS", "PIS", "APPR", "PRF", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Es scheint/ die engel sind von oben rab gestiegen/", "tokens": ["Es", "scheint", "/", "die", "en\u00b7gel", "sind", "von", "o\u00b7ben", "rab", "ge\u00b7stie\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ART", "NN", "VAFIN", "APPR", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und wollen sich durch dich zu unsrem chor verf\u00fcgen:", "tokens": ["Und", "wol\u00b7len", "sich", "durch", "dich", "zu", "uns\u00b7rem", "chor", "ver\u00b7f\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn was man \u00fcberall nur um sich h\u00f6rt und sp\u00fcrt/", "tokens": ["Denn", "was", "man", "\u00fc\u00b7be\u00b7rall", "nur", "um", "sich", "h\u00f6rt", "und", "sp\u00fcrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "ADV", "ADV", "APPR", "PRF", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und was dein sch\u00f6ner mund recht himmlisch abgefungen/", "tokens": ["Und", "was", "dein", "sch\u00f6\u00b7ner", "mund", "recht", "himm\u00b7lisch", "ab\u00b7ge\u00b7fun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das ist ein meisterst\u00fcck von ihren reinen zungen/", "tokens": ["Das", "ist", "ein", "meis\u00b7ter\u00b7st\u00fcck", "von", "ih\u00b7ren", "rei\u00b7nen", "zun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wodurch die seele gantz wird aus sich selbst gef\u00fchrt.", "tokens": ["Wo\u00b7durch", "die", "see\u00b7le", "gantz", "wird", "aus", "sich", "selbst", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VAFIN", "APPR", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}