}, "line.130": {"text": "Doch sie werden dem Erz\u00e4len", "tokens": ["Doch", "sie", "wer\u00b7den", "dem", "Er\u00b7z\u00e4\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.131": {"text": "Meiner lieben Doris glauben,", "tokens": ["Mei\u00b7ner", "lie\u00b7ben", "Do\u00b7ris", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.132": {"text": "Denn man wei\u00df, sie kan nicht l\u00fcgen.", "tokens": ["Denn", "man", "wei\u00df", ",", "sie", "kan", "nicht", "l\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.133": {"text": "Ja, sie werden alles glauben,", "tokens": ["Ja", ",", "sie", "wer\u00b7den", "al\u00b7les", "glau\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.134": {"text": "Wenn sie k\u00fcnftig sehen werden,", "tokens": ["Wenn", "sie", "k\u00fcnf\u00b7tig", "se\u00b7hen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.135": {"text": "Da\u00df die Rosen nie verwelken,", "tokens": ["Da\u00df", "die", "Ro\u00b7sen", "nie", "ver\u00b7wel\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.136": {"text": "Die auf ihrem Busen bl\u00fchen.", "tokens": ["Die", "auf", 