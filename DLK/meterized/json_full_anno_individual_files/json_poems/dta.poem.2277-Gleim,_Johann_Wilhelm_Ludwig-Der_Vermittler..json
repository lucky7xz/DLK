{"dta.poem.2277": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Vermittler.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In dem Garten, den ich liebe,", "tokens": ["In", "dem", "Gar\u00b7ten", ",", "den", "ich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolt ich, mitten unter Rosen,", "tokens": ["Wolt", "ich", ",", "mit\u00b7ten", "un\u00b7ter", "Ro\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der artigsten Brunette", "tokens": ["Mit", "der", "ar\u00b7tigs\u00b7ten", "Bru\u00b7net\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--++-+--", "measure": "anapaest.init"}, "line.4": {"text": "Frohe Gartenspiele spielen.", "tokens": ["Fro\u00b7he", "Gar\u00b7ten\u00b7spie\u00b7le", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schatten, West und Nachtigallen", "tokens": ["Schat\u00b7ten", ",", "West", "und", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Pries ich ihr als Spielgefellen;", "tokens": ["Pries", "ich", "ihr", "als", "Spiel\u00b7ge\u00b7fel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "KOUS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber die vergn\u00fcgte Sch\u00f6ne", "tokens": ["A\u00b7ber", "die", "ver\u00b7gn\u00fcg\u00b7te", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lie\u00df sich nicht zum Spiele reitzen;", "tokens": ["Lie\u00df", "sich", "nicht", "zum", "Spie\u00b7le", "reit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ob sie gleich die Lust zum spielen", "tokens": ["Ob", "sie", "gleich", "die", "Lust", "zum", "spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "APPRART", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "R\u00f6thend auf den Wangen zeigte.", "tokens": ["R\u00f6\u00b7thend", "auf", "den", "Wan\u00b7gen", "zeig\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Neue Gr\u00fcnde, neue Bitten", "tokens": ["Neu\u00b7e", "Gr\u00fcn\u00b7de", ",", "neu\u00b7e", "Bit\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Schaften endlich Ja und Willen,", "tokens": ["Schaf\u00b7ten", "end\u00b7lich", "Ja", "und", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKANT", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Da\u00df ich mir mit Rosenknospen", "tokens": ["Da\u00df", "ich", "mir", "mit", "Ro\u00b7sen\u00b7knos\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Ihren Ku\u00df erwerben solte,", "tokens": ["Ih\u00b7ren", "Ku\u00df", "er\u00b7wer\u00b7ben", "sol\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Wenn ich sie damit, von weiten,", "tokens": ["Wenn", "ich", "sie", "da\u00b7mit", ",", "von", "wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PAV", "$,", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "In der Laube treffen k\u00f6nnte.", "tokens": ["In", "der", "Lau\u00b7be", "tref\u00b7fen", "k\u00f6nn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Niemals hab ich mehr gezielet,", "tokens": ["Nie\u00b7mals", "hab", "ich", "mehr", "ge\u00b7zie\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Als ich mit den Knospen zielte;", "tokens": ["Als", "ich", "mit", "den", "Knos\u00b7pen", "ziel\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Niemals traf mein Bogen besser.", "tokens": ["Nie\u00b7mals", "traf", "mein", "Bo\u00b7gen", "bes\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Aber Doris, die Geliebte", "tokens": ["A\u00b7ber", "Do\u00b7ris", ",", "die", "Ge\u00b7lieb\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "$,", "ART", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.21": {"text": "Weigerte den Preis der Wette", "tokens": ["Wei\u00b7ger\u00b7te", "den", "Preis", "der", "Wet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.22": {"text": "Dem Gewinner abznliefern,", "tokens": ["Dem", "Ge\u00b7win\u00b7ner", "abzn\u00b7lie\u00b7fern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Und versprach bei iedem Treffer", "tokens": ["Und", "ver\u00b7sprach", "bei", "ie\u00b7dem", "Tref\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Alle Schulden auszul\u00f6schen,", "tokens": ["Al\u00b7le", "Schul\u00b7den", "aus\u00b7zu\u00b7l\u00f6\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Wenn noch eine Knospe tr\u00e4fe.", "tokens": ["Wenn", "noch", "ei\u00b7ne", "Knos\u00b7pe", "tr\u00e4\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Als nun eine unter dreien", "tokens": ["Als", "nun", "ei\u00b7ne", "un\u00b7ter", "drei\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "APPR", "CARD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Treffen oder fehlen solte,", "tokens": ["Tref\u00b7fen", "o\u00b7der", "feh\u00b7len", "sol\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Traf sie pl\u00f6tzlich an den Busen", "tokens": ["Traf", "sie", "pl\u00f6tz\u00b7lich", "an", "den", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Eine schwere Rosenknospe.", "tokens": ["Ei\u00b7ne", "schwe\u00b7re", "Ro\u00b7sen\u00b7knos\u00b7pe", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Augenblikks, indem sies f\u00fchlte,", "tokens": ["Au\u00b7gen\u00b7blikks", ",", "in\u00b7dem", "sies", "f\u00fchl\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Oefnete die Rosenknospe", "tokens": ["Oef\u00b7ne\u00b7te", "die", "Ro\u00b7sen\u00b7knos\u00b7pe"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Das Beh\u00e4ltni\u00df der Ger\u00fcche,", "tokens": ["Das", "Be\u00b7h\u00e4lt\u00b7ni\u00df", "der", "Ge\u00b7r\u00fc\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Und, ihr Sch\u00f6nen, welch ein Wunder!", "tokens": ["Und", ",", "ihr", "Sch\u00f6\u00b7nen", ",", "welch", "ein", "Wun\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "NN", "$,", "PWAT", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Amor kam heraus gesprungen.", "tokens": ["A\u00b7mor", "kam", "he\u00b7raus", "ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Kleine Anmuts volle Lokken", "tokens": ["Klei\u00b7ne", "An\u00b7muts", "vol\u00b7le", "Lok\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Fielen von der zarten Scheitel,", "tokens": ["Fie\u00b7len", "von", "der", "zar\u00b7ten", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Von den K\u00fcssenswerten Lippen", "tokens": ["Von", "den", "K\u00fcs\u00b7sens\u00b7wer\u00b7ten", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Treufelten die K\u00fcsse sichtbar,", "tokens": ["Treu\u00b7fel\u00b7ten", "die", "K\u00fcs\u00b7se", "sicht\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.39": {"text": "Und ein Trupp verliebter Geister", "tokens": ["Und", "ein", "Trupp", "ver\u00b7lieb\u00b7ter", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Und ein Schwarm vergn\u00fcgter Silfen", "tokens": ["Und", "ein", "Schwarm", "ver\u00b7gn\u00fcg\u00b7ter", "Sil\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "War gesch\u00e4ftig, sie zu sammlen.", "tokens": ["War", "ge\u00b7sch\u00e4f\u00b7tig", ",", "sie", "zu", "samm\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Mit vergn\u00fcgten Wollustminen", "tokens": ["Mit", "ver\u00b7gn\u00fcg\u00b7ten", "Wol\u00b7lust\u00b7mi\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "L\u00e4chelte der G\u00f6tterknabe.", "tokens": ["L\u00e4\u00b7chel\u00b7te", "der", "G\u00f6t\u00b7ter\u00b7kna\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.44": {"text": "Schwebend flog er, wie ein Engel,", "tokens": ["Schwe\u00b7bend", "flog", "er", ",", "wie", "ein", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Zwischen mir und meiner Sch\u00f6ne,", "tokens": ["Zwi\u00b7schen", "mir", "und", "mei\u00b7ner", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Welche voller Furcht und Schrekken", "tokens": ["Wel\u00b7che", "vol\u00b7ler", "Furcht", "und", "Schrek\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Hurtig aus der Laube flohe.", "tokens": ["Hur\u00b7tig", "aus", "der", "Lau\u00b7be", "flo\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Aber Amor rief sie freundlich:", "tokens": ["A\u00b7ber", "A\u00b7mor", "rief", "sie", "freund\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Kleines N\u00e4rrchen, bist du bl\u00f6de?", "tokens": ["Klei\u00b7nes", "N\u00e4rr\u00b7chen", ",", "bist", "du", "bl\u00f6\u00b7de", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "PPER", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Bleib nur hier, sonst schie\u00dft mein Bogen,", "tokens": ["Bleib", "nur", "hier", ",", "sonst", "schie\u00dft", "mein", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Und du wirst ihm nicht entrinnen.", "tokens": ["Und", "du", "wirst", "ihm", "nicht", "ent\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "A", "tokens": ["A"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.53": {"text": "Kam sie wieder nach der Laube,", "tokens": ["Kam", "sie", "wie\u00b7der", "nach", "der", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Wo sich Amor ihren Augen,", "tokens": ["Wo", "sich", "A\u00b7mor", "ih\u00b7ren", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Ohne Kleid und Hemde zeigte.", "tokens": ["Oh\u00b7ne", "Kleid", "und", "Hem\u00b7de", "zeig\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Hurtig wandte sie die Augen", "tokens": ["Hur\u00b7tig", "wand\u00b7te", "sie", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Nach der G\u00e4rtnerin im Garten;", "tokens": ["Nach", "der", "G\u00e4rt\u00b7ne\u00b7rin", "im", "Gar\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Wie sie schamhaft kluge Sch\u00f6nen,", "tokens": ["Wie", "sie", "scham\u00b7haft", "klu\u00b7ge", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.59": {"text": "In Gesellschaft wehrter Freunde,", "tokens": ["In", "Ge\u00b7sell\u00b7schaft", "wehr\u00b7ter", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "Von geschnitzten Liebesg\u00f6ttern", "tokens": ["Von", "ge\u00b7schnitz\u00b7ten", "Lie\u00b7bes\u00b7g\u00f6t\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.61": {"text": "Lieber nach Citheren wenden.", "tokens": ["Lie\u00b7ber", "nach", "Cit\u00b7he\u00b7ren", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.62": {"text": "Aber Amor flog ihr n\u00e4her,", "tokens": ["A\u00b7ber", "A\u00b7mor", "flog", "ihr", "n\u00e4\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "Und befahl mir, da\u00df sies h\u00f6rte:", "tokens": ["Und", "be\u00b7fahl", "mir", ",", "da\u00df", "sies", "h\u00f6r\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.64": {"text": "Liebling, pfl\u00fckke Rosenknospen,", "tokens": ["Lieb\u00b7ling", ",", "pfl\u00fck\u00b7ke", "Ro\u00b7sen\u00b7knos\u00b7pen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.65": {"text": "Ich will sehn, ob deine Knospen", "tokens": ["Ich", "will", "sehn", ",", "ob", "dei\u00b7ne", "Knos\u00b7pen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.66": {"text": "So, wie meine Pfeile, treffen.", "tokens": ["So", ",", "wie", "mei\u00b7ne", "Pfei\u00b7le", ",", "tref\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.67": {"text": "Ich gehorchte dem Befehle;", "tokens": ["Ich", "ge\u00b7horch\u00b7te", "dem", "Be\u00b7feh\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.68": {"text": "Als ich aber unterwegens", "tokens": ["Als", "ich", "a\u00b7ber", "un\u00b7ter\u00b7we\u00b7gens"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.69": {"text": "Die gepfl\u00fckten Rosenknospen", "tokens": ["Die", "ge\u00b7pfl\u00fck\u00b7ten", "Ro\u00b7sen\u00b7knos\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.70": {"text": "In die Tasche stekken wollte:", "tokens": ["In", "die", "Ta\u00b7sche", "stek\u00b7ken", "woll\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.71": {"text": "Fand ich, Freunde glaubt dem Finder!", "tokens": ["Fand", "ich", ",", "Freun\u00b7de", "glaubt", "dem", "Fin\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.72": {"text": "Be\u00dfre Knospen in der Tasche.", "tokens": ["Be\u00df\u00b7re", "Knos\u00b7pen", "in", "der", "Ta\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.73": {"text": "Diese nahm ich, statt der andern,", "tokens": ["Die\u00b7se", "nahm", "ich", ",", "statt", "der", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "KOUI", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.74": {"text": "Und indem mich Amor winkte,", "tokens": ["Und", "in\u00b7dem", "mich", "A\u00b7mor", "wink\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.75": {"text": "Und indem sie Amor k\u00fc\u00dfte,", "tokens": ["Und", "in\u00b7dem", "sie", "A\u00b7mor", "k\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.76": {"text": "Lie\u00df ich schnell die Knospe fliegen.", "tokens": ["Lie\u00df", "ich", "schnell", "die", "Knos\u00b7pe", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.77": {"text": "Kaum war sie der Hand entflogen,", "tokens": ["Kaum", "war", "sie", "der", "Hand", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.78": {"text": "Als mich schon der Wurf gereute;", "tokens": ["Als", "mich", "schon", "der", "Wurf", "ge\u00b7reu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.79": {"text": "Denn sie sank in Amors Arme,", "tokens": ["Denn", "sie", "sank", "in", "A\u00b7mors", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.80": {"text": "Und ich dachte, meine Knospe", "tokens": ["Und", "ich", "dach\u00b7te", ",", "mei\u00b7ne", "Knos\u00b7pe"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.81": {"text": "H\u00e4tte sie so stark getroffen,", "tokens": ["H\u00e4t\u00b7te", "sie", "so", "stark", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.82": {"text": "Da\u00df sie hurtig sterben w\u00fcrde.", "tokens": ["Da\u00df", "sie", "hur\u00b7tig", "ster\u00b7ben", "w\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.83": {"text": "Denn sie seufzte: Welche Wunde!", "tokens": ["Denn", "sie", "seufz\u00b7te", ":", "Wel\u00b7che", "Wun\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "PWAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.84": {"text": "Seht nur her! ich bin verwundet!", "tokens": ["Seht", "nur", "her", "!", "ich", "bin", "ver\u00b7wun\u00b7det", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.85": {"text": "Aber Amor lachte fr\u00f6lich,", "tokens": ["A\u00b7ber", "A\u00b7mor", "lach\u00b7te", "fr\u00f6\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.86": {"text": "Und besichtigte die Wunde,", "tokens": ["Und", "be\u00b7sich\u00b7tig\u00b7te", "die", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.87": {"text": "Und wies mit dem kleinen Finger", "tokens": ["Und", "wies", "mit", "dem", "klei\u00b7nen", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.88": {"text": "Knosp und Pfeil und Wund am Busen.", "tokens": ["Knosp", "und", "Pfeil", "und", "Wund", "am", "Bu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.89": {"text": "Siehst du, sprach er, deine Knospe", "tokens": ["Siehst", "du", ",", "sprach", "er", ",", "dei\u00b7ne", "Knos\u00b7pe"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.90": {"text": "Muste diesen Pfeil verwahren,", "tokens": ["Mus\u00b7te", "die\u00b7sen", "Pfeil", "ver\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.91": {"text": "Denn du soltest diese Lose,", "tokens": ["Denn", "du", "sol\u00b7test", "die\u00b7se", "Lo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.92": {"text": "Die mich oft, wie dich, verspottet,", "tokens": ["Die", "mich", "oft", ",", "wie", "dich", ",", "ver\u00b7spot\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADV", "$,", "PWAV", "PPER", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.93": {"text": "F\u00fcr die Sp\u00f6tterei bestrafen.", "tokens": ["F\u00fcr", "die", "Sp\u00f6t\u00b7te\u00b7rei", "be\u00b7stra\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.94": {"text": "La\u00df sie noch ein bisgen qu\u00e4len,", "tokens": ["La\u00df", "sie", "noch", "ein", "bis\u00b7gen", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.95": {"text": "Und dann nimm den Liebesbalsam,", "tokens": ["Und", "dann", "nimm", "den", "Lie\u00b7bes\u00b7bal\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVIMP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.96": {"text": "Das Geschenk von meiner Mutter,", "tokens": ["Das", "Ge\u00b7schenk", "von", "mei\u00b7ner", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.97": {"text": "Und bestreich damit die Wunde.", "tokens": ["Und", "be\u00b7streich", "da\u00b7mit", "die", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PAV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.98": {"text": "K\u00fcsse sie, nun wird sie k\u00fcssen;", "tokens": ["K\u00fcs\u00b7se", "sie", ",", "nun", "wird", "sie", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.99": {"text": "La\u00df dir den Gewinst bezalen,", "tokens": ["La\u00df", "dir", "den", "Ge\u00b7winst", "be\u00b7za\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.100": {"text": "Und bezale du sie wieder,", "tokens": ["Und", "be\u00b7za\u00b7le", "du", "sie", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.101": {"text": "Wenn sie dich in Zukunft mahnet;", "tokens": ["Wenn", "sie", "dich", "in", "Zu\u00b7kunft", "mah\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.102": {"text": "Denn, mein Freund, so und nicht anders", "tokens": ["Denn", ",", "mein", "Freund", ",", "so", "und", "nicht", "an\u00b7ders"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PPOSAT", "NN", "$,", "ADV", "KON", "PTKNEG", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.103": {"text": "Hab ich dich und sie vermittelt.", "tokens": ["Hab", "ich", "dich", "und", "sie", "ver\u00b7mit\u00b7telt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "KON", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.104": {"text": "O wie oft, wie sanft, wie z\u00e4rtlich", "tokens": ["O", "wie", "oft", ",", "wie", "sanft", ",", "wie", "z\u00e4rt\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "KOKOM", "ADV", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.105": {"text": "K\u00fc\u00dfte mich die liebe Sch\u00f6ne,", "tokens": ["K\u00fc\u00df\u00b7te", "mich", "die", "lie\u00b7be", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.106": {"text": "Als sie Amors Vorwurf h\u00f6rte!", "tokens": ["Als", "sie", "A\u00b7mors", "Vor\u00b7wurf", "h\u00f6r\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.107": {"text": "Reuerf\u00fcllte Freudentr\u00e4nen", "tokens": ["Reu\u00b7er\u00b7f\u00fcll\u00b7te", "Freu\u00b7den\u00b7tr\u00e4\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.108": {"text": "Flossen von den sch\u00f6nen Wangen.", "tokens": ["Flos\u00b7sen", "von", "den", "sch\u00f6\u00b7nen", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.109": {"text": "Amor lie\u00df sie von den Silfen,", "tokens": ["A\u00b7mor", "lie\u00df", "sie", "von", "den", "Sil\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.110": {"text": "Die wie Sonnenst\u00e4ubchen schw\u00e4rmten,", "tokens": ["Die", "wie", "Son\u00b7nen\u00b7st\u00e4ub\u00b7chen", "schw\u00e4rm\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.111": {"text": "In ihr Ku\u00dfgef\u00e4sse sammlen,", "tokens": ["In", "ihr", "Ku\u00df