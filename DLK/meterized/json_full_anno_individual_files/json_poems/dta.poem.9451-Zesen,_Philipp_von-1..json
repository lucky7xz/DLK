{"dta.poem.9451": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Oda\u00df ich dich/ Bruder/ solt k\u00fcssen alleine/", "tokens": ["O\u00b7da\u00df", "ich", "dich", "/", "Bru\u00b7der", "/", "solt", "k\u00fcs\u00b7sen", "al\u00b7lei\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "$(", "NN", "$(", "VMFIN", "VVINF", "ADV", "$("], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Da\u00df keiner mich h\u00f6hnte/ noch schaute mir zu/", "tokens": ["Da\u00df", "kei\u00b7ner", "mich", "h\u00f6hn\u00b7te", "/", "noch", "schau\u00b7te", "mir", "zu", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "PTKZU", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich wolte dich f\u00fchren aus dieser gemeine/", "tokens": ["Ich", "wol\u00b7te", "dich", "f\u00fch\u00b7ren", "aus", "die\u00b7ser", "ge\u00b7mei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "APPR", "PDAT", "ADJA", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Nach hause zur Mutter/ ", "tokens": ["Nach", "hau\u00b7se", "zur", "Mut\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Da wolt\u2019 ich dich h\u00f6ren/", "tokens": ["Da", "wolt'", "ich", "dich", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Du soltest mich lehren", "tokens": ["Du", "sol\u00b7test", "mich", "leh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Dein heiliges Wort;", "tokens": ["Dein", "hei\u00b7li\u00b7ges", "Wort", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Ich wolte dich tr\u00e4ncken", "tokens": ["Ich", "wol\u00b7te", "dich", "tr\u00e4n\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "und Kreuter-Wein schencken/", "tokens": ["und", "Kreu\u00b7ter\u00b7Wein", "schen\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$("], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.10": {"text": "Ich wolte Dir \u00e4pfelmust geben/ mein Port.", "tokens": ["Ich", "wol\u00b7te", "Dir", "\u00e4p\u00b7fel\u00b7must", "ge\u00b7ben", "/", "mein", "Port", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$(", "PPOSAT", "NE", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Es liebet mich hertzlich und treulich mein Leben/", "tokens": ["Es", "lie\u00b7bet", "mich", "hertz\u00b7lich", "und", "treu\u00b7lich", "mein", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mein Liebster mich liebet/ das wei\u00df ich gewi\u00df/", "tokens": ["Mein", "Liebs\u00b7ter", "mich", "lie\u00b7bet", "/", "das", "wei\u00df", "ich", "ge\u00b7wi\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$(", "PDS", "VVFIN", "PPER", "ADV", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Lincke/ so unter dem Heupte ligt eben/", "tokens": ["Die", "Lin\u00b7cke", "/", "so", "un\u00b7ter", "dem", "Heup\u00b7te", "ligt", "e\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVFIN", "ADV", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Zeiget die Liebe/ der Falschheit gebi\u00df/", "tokens": ["Zei\u00b7get", "die", "Lie\u00b7be", "/", "der", "Falschheit", "ge\u00b7bi\u00df", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Befestigt die Treue", "tokens": ["Be\u00b7fes\u00b7tigt", "die", "Treu\u00b7e"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Nun wider aufs neue/", "tokens": ["Nun", "wi\u00b7der", "aufs", "neu\u00b7e", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPRART", "ADJA", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Das herrliche pfand:", "tokens": ["Das", "herr\u00b7li\u00b7che", "pfand", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Die Rechte mich hertzet/", "tokens": ["Die", "Rech\u00b7te", "mich", "hert\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Die Lincke die schertzet/", "tokens": ["Die", "Lin\u00b7cke", "die", "schert\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Nun hab\u2019 ich das hertze des Freundes erkant.", "tokens": ["Nun", "hab'", "ich", "das", "hert\u00b7ze", "des", "Freun\u00b7des", "er\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}}}}}