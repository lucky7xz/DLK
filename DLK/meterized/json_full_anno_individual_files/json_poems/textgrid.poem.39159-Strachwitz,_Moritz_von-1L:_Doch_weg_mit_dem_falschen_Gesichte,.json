{"textgrid.poem.39159": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Doch weg mit dem falschen Gesichte,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch weg mit dem falschen Gesichte,", "tokens": ["Doch", "weg", "mit", "dem", "fal\u00b7schen", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und weg mit dem falschen Meer!", "tokens": ["Und", "weg", "mit", "dem", "fal\u00b7schen", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nun hol' ich vom treu'sten Lichte", "tokens": ["Nun", "hol'", "ich", "vom", "treu'\u00b7sten", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den treu'sten Spiegel mir her.", "tokens": ["Den", "treu'\u00b7sten", "Spie\u00b7gel", "mir", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ich rei\u00dfe aus dunkler Scheide", "tokens": ["Ich", "rei\u00b7\u00dfe", "aus", "dunk\u00b7ler", "Schei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Klinge breit und blau,", "tokens": ["Die", "Klin\u00b7ge", "breit", "und", "blau", ","], "token_info"