{"dta.poem.18582": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A n  T halestris/ als er von ihr  \n Abschied nahme/", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Ein treuer Knecht erk\u00fchnet sich zu letzt/", "tokens": ["Ein", "treu\u00b7er", "Knecht", "er\u00b7k\u00fch\u00b7net", "sich", "zu", "letzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dir sch\u00f6nstes Kind das Blat zu \u00fcberreichen.", "tokens": ["Dir", "sch\u00f6ns\u00b7tes", "Kind", "das", "Blat", "zu", "\u00fc\u00b7berr\u00b7ei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du hast mich ja der Gnade werth gesch\u00e4tzt/", "tokens": ["Du", "hast", "mich", "ja", "der", "Gna\u00b7de", "werth", "ge\u00b7sch\u00e4tzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein sch\u00f6nes Hertz durch Seufftzer zu erweichen;", "tokens": ["Dein", "sch\u00f6\u00b7nes", "Hertz", "durch", "Seufft\u00b7zer", "zu", "er\u00b7wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Drum g\u00f6nne doch ein gn\u00e4dig Angesicht", "tokens": ["Drum", "g\u00f6n\u00b7ne", "doch", "ein", "gn\u00e4\u00b7dig", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der letzten Pflicht.", "tokens": ["Der", "letz\u00b7ten", "Pflicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Es gr\u00fcnet jetzt der Blumen Lust-Revier/", "tokens": ["Es", "gr\u00fc\u00b7net", "jetzt", "der", "Blu\u00b7men", "Lust\u00b7Re\u00b7vier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie lachen nicht die bunt bemahlten Auen?", "tokens": ["Wie", "la\u00b7chen", "nicht", "die", "bunt", "be\u00b7mahl\u00b7ten", "Au\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PTKNEG", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur meine Lust/ ja meine sch\u00f6nste Zier", "tokens": ["Nur", "mei\u00b7ne", "Lust", "/", "ja", "mei\u00b7ne", "sch\u00f6ns\u00b7te", "Zier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$(", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "L\u00e4st mir der Tod auff seiner Bahre schauen/", "tokens": ["L\u00e4st", "mir", "der", "Tod", "auff", "sei\u00b7ner", "Bah\u00b7re", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und dieses ist/ was den geqv\u00e4lten Geist", "tokens": ["Und", "die\u00b7ses", "ist", "/", "was", "den", "ge\u00b7qv\u00e4l\u00b7ten", "Geist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "$(", "PWS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Fast sterben heist.", "tokens": ["Fast", "ster\u00b7ben", "heist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Man st\u00f6sset mich aus meinen Paradie\u00df/", "tokens": ["Man", "st\u00f6s\u00b7set", "mich", "aus", "mei\u00b7nen", "Pa\u00b7ra\u00b7die\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und habe nichts verbothnes anger\u00fchret:", "tokens": ["Und", "ha\u00b7be", "nichts", "ver\u00b7both\u00b7nes", "an\u00b7ge\u00b7r\u00fch\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Engels-Kind/ das mich erst kommen hie\u00df/", "tokens": ["Das", "En\u00b7gels\u00b7Kind", "/", "das", "mich", "erst", "kom\u00b7men", "hie\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "ADV", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist auch allein/ das mir den Schmertz geb\u00fchret.", "tokens": ["Ist", "auch", "al\u00b7lein", "/", "das", "mir", "den", "Schmertz", "ge\u00b7b\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$(", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ach da\u00df ich doch nur in die W\u00fcsteney", "tokens": ["Ach", "da\u00df", "ich", "doch", "nur", "in", "die", "W\u00fcs\u00b7te\u00b7ney"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "KOUS", "PPER", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verstossen sey.", "tokens": ["Ver\u00b7stos\u00b7sen", "sey", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ich werde nun mein allzuherbes Leyd", "tokens": ["Ich", "wer\u00b7de", "nun", "mein", "all\u00b7zu\u00b7her\u00b7bes", "Leyd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In steter Traur mit matter Stimme lallen/", "tokens": ["In", "ste\u00b7ter", "Traur", "mit", "mat\u00b7ter", "Stim\u00b7me", "lal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch denck' ich nur an deine Lieblichkeit/", "tokens": ["Doch", "denck", "ich", "nur", "an", "dei\u00b7ne", "Lieb\u00b7lich\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und an den Mund von tausend Nachtigallen/", "tokens": ["Und", "an", "den", "Mund", "von", "tau\u00b7send", "Nach\u00b7ti\u00b7gal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So schweig' ich still/ und seufftze nur bey mir", "tokens": ["So", "schweig'", "ich", "still", "/", "und", "seufft\u00b7ze", "nur", "bey", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$(", "KON", "VVFIN", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Allein nach dir.", "tokens": ["Al\u00b7lein", "nach", "dir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ach \u00f6ffne denn die Schwanen-weisse Brust/", "tokens": ["Ach", "\u00f6ff\u00b7ne", "denn", "die", "Schwa\u00b7nen\u00b7weis\u00b7se", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und la\u00df' es hier gew\u00fcnschte Ruhe finden.", "tokens": ["Und", "la\u00df'", "es", "hier", "ge\u00b7w\u00fcnschte", "Ru\u00b7he", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Verg\u00f6nne mir zum Troste diese Lust/", "tokens": ["Ver\u00b7g\u00f6n\u00b7ne", "mir", "zum", "Tros\u00b7te", "die\u00b7se", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ich mich kan in deinen Hertzen gr\u00fcnden/", "tokens": ["Da\u00df", "ich", "mich", "kan", "in", "dei\u00b7nen", "Hert\u00b7zen", "gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So hab' ich doch die gr\u00f6ste Qvaal besiegt", "tokens": ["So", "hab'", "ich", "doch", "die", "gr\u00f6s\u00b7te", "Qvaal", "be\u00b7siegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und bin vergn\u00fcgt.", "tokens": ["Und", "bin", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$."], "meter": "++-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "So lebe denn zu tausend mahl begl\u00fcckt/", "tokens": ["So", "le\u00b7be", "denn", "zu", "tau\u00b7send", "mahl", "be\u00b7gl\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "CARD", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geniesse stets der h\u00f6chst-erw\u00fcnschten Freuden.", "tokens": ["Ge\u00b7nies\u00b7se", "stets", "der", "h\u00f6chst\u00b7er\u00b7w\u00fcnschten", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dein Knecht der sich zu deinen F\u00fcssen b\u00fcckt/", "tokens": ["Dein", "Knecht", "der", "sich", "zu", "dei\u00b7nen", "F\u00fcs\u00b7sen", "b\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mu\u00df nun von dir und dem Vergn\u00fcgen scheiden:", "tokens": ["Mu\u00df", "nun", "von", "dir", "und", "dem", "Ver\u00b7gn\u00fc\u00b7gen", "schei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mein Freuden-Stern/ und du mein Anderich", "tokens": ["Mein", "Freu\u00b7den\u00b7S\u00b7tern", "/", "und", "du", "mein", "An\u00b7de\u00b7rich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "KON", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verbergen sich.", "tokens": ["Ver\u00b7ber\u00b7gen", "sich", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Ich bleibe dir/ Annehmlichste der Welt!", "tokens": ["Ich", "blei\u00b7be", "dir", "/", "An\u00b7nehm\u00b7lichs\u00b7te", "der", "Welt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "NN", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor alle Gunst in Ewigkeit verbunden/", "tokens": ["Vor", "al\u00b7le", "Gunst", "in", "E\u00b7wig\u00b7keit", "ver\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein' Anmuths-Pracht/ der mich gefangen h\u00e4lt/", "tokens": ["Dein'", "An\u00b7muths\u00b7Pracht", "/", "der", "mich", "ge\u00b7fan\u00b7gen", "h\u00e4lt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PRF", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hat mich durchs Garn der Dienstbarkeit \u00fcmwunden/", "tokens": ["Hat", "mich", "durchs", "Garn", "der", "Dienst\u00b7bar\u00b7keit", "\u00fcm\u00b7wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Sonne mu\u00df/ die ich bey dir gesehn/", "tokens": ["Die", "Son\u00b7ne", "mu\u00df", "/", "die", "ich", "bey", "dir", "ge\u00b7sehn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$(", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Stets mit mir gehn.", "tokens": ["Stets", "mit", "mir", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Ach Weissenfels! du h\u00f6chst-beliebter Ort!", "tokens": ["Ach", "Weis\u00b7sen\u00b7fels", "!", "du", "h\u00f6chst\u00b7be\u00b7lieb\u00b7ter", "Ort", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Talestris Ach! du allerliebste Seele!", "tokens": ["Ta\u00b7le\u00b7stris", "Ach", "!", "du", "al\u00b7ler\u00b7liebs\u00b7te", "See\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ITJ", "$.", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Schicksal rufft mich nun nach Jena fort/", "tokens": ["Das", "Schick\u00b7sal", "rufft", "mich", "nun", "nach", "Je\u00b7na", "fort", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "NE", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dr\u00fcm ruff auch noch aus deiner Purpur-H\u00f6le", "tokens": ["Dr\u00fcm", "ruff", "auch", "noch", "aus", "dei\u00b7ner", "Pur\u00b7pur\u00b7H\u00f6le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das letzte Wort/ das meine Schmertzen macht/", "tokens": ["Das", "letz\u00b7te", "Wort", "/", "das", "mei\u00b7ne", "Schmert\u00b7zen", "macht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PDS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zu guter Nacht.", "tokens": ["Zu", "gu\u00b7ter", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}