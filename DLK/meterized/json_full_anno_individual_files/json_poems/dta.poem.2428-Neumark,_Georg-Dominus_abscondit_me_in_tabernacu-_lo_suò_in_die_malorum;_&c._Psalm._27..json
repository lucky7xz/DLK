{"dta.poem.2428": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Dominus abscondit me in tabernacu-  \n lo su\u00f2 in die malorum; &c.  Psalm.  27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Ob schon das Ungl\u00fckk tobt/ und grim\u0303tg auf mich w\u00fctet/", "tokens": ["Ob", "schon", "das", "Un\u00b7gl\u00fckk", "tobt", "/", "und", "grim\u0303tg", "auf", "mich", "w\u00fc\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$(", "KON", "VVFIN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So acht\u2019 ich es doch nicht/ ich bleibe doch beh\u00fctet/", "tokens": ["So", "acht'", "ich", "es", "doch", "nicht", "/", "ich", "blei\u00b7be", "doch", "be\u00b7h\u00fc\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "$(", "PPER", "VVFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dieweil mein h\u00f6chster Gott in dieser b\u00f6sen Welt", "tokens": ["Die\u00b7weil", "mein", "h\u00f6chs\u00b7ter", "Gott", "in", "die\u00b7ser", "b\u00f6\u00b7sen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mich in ein Zeit verstekkt/ und mich gantz sicher f\u00fchret", "tokens": ["Mich", "in", "ein", "Zeit", "vers\u00b7tekkt", "/", "und", "mich", "gantz", "si\u00b7cher", "f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$(", "KON", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf einen hohen Fel\u00df/ da mich kein Leiden r\u00fchret/", "tokens": ["Auf", "ei\u00b7nen", "ho\u00b7hen", "Fel\u00df", "/", "da", "mich", "kein", "Lei\u00b7den", "r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dr\u00fcm geb. ich So viel drauf/ weit Gott sebst Fels und", "tokens": ["Dr\u00fcm", "ge\u00b7b.", "ich", "So", "viel", "drauf", "/", "weit", "Gott", "sebst", "Fels", "und"], "token_info": ["word", "abbreviation", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PPER", "ADV", "ADV", "PAV", "$(", "ADJD", "NN", "VAFIN", "NN", "KON"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Zelt.", "tokens": ["Zelt", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}}}}