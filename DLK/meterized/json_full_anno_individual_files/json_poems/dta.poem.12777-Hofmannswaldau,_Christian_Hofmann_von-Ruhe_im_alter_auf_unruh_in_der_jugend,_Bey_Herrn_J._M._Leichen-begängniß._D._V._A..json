{"dta.poem.12777": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ruhe im alter auf unruh in der jugend,  \n  Bey  \n Herrn J. M. Leichen-beg\u00e4ngni\u00df.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer in dem fr\u00fchling seiner jahre", "tokens": ["Wer", "in", "dem", "fr\u00fch\u00b7ling", "sei\u00b7ner", "jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gar wenig still gesessen hat,", "tokens": ["Gar", "we\u00b7nig", "still", "ge\u00b7ses\u00b7sen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und k\u00f6mmt im alter auf die bahre,", "tokens": ["Und", "k\u00f6mmt", "im", "al\u00b7ter", "auf", "die", "bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der schwitzet fr\u00fch und ruhet spat.", "tokens": ["Der", "schwit\u00b7zet", "fr\u00fch", "und", "ru\u00b7het", "spat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er wei\u00df sich in die zeit zu schicken,", "tokens": ["Er", "wei\u00df", "sich", "in", "die", "zeit", "zu", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und wenn sie kommt, auch abzudr\u00fccken.", "tokens": ["Und", "wenn", "sie", "kommt", ",", "auch", "ab\u00b7zu\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie, wenn ein schiff die stoltzen wellen,", "tokens": ["Wie", ",", "wenn", "ein", "schiff", "die", "stolt\u00b7zen", "wel\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "ART", "VVFIN", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Sud und Nord, nach Ost und West", "tokens": ["Nach", "Sud", "und", "Nord", ",", "nach", "Ost", "und", "West"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NE", "$,", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich hin und her hat lassen prellen,", "tokens": ["Sich", "hin", "und", "her", "hat", "las\u00b7sen", "prel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKVZ", "KON", "ADV", "VAFIN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jm port die segel fallen l\u00e4st:", "tokens": ["Jm", "port", "die", "se\u00b7gel", "fal\u00b7len", "l\u00e4st", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So kan, wer jung viel ausgestanden,", "tokens": ["So", "kan", ",", "wer", "jung", "viel", "aus\u00b7ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "PWS", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit guter ruh im alter landen.", "tokens": ["Mit", "gu\u00b7ter", "ruh", "im", "al\u00b7ter", "lan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Herr M\u00fcnchen ist es so gel\u00fccket,", "tokens": ["Herr", "M\u00fcn\u00b7chen", "ist", "es", "so", "ge\u00b7l\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er, nach seiner jugend last", "tokens": ["Da\u00df", "er", ",", "nach", "sei\u00b7ner", "ju\u00b7gend", "last"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ruh im alter hat erblicket,", "tokens": ["Die", "ruh", "im", "al\u00b7ter", "hat", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nun im himmel gantz umfa\u00dft.", "tokens": ["Und", "nun", "im", "him\u00b7mel", "gantz", "um\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die sonn ist ihm hier aufgegangen,", "tokens": ["Die", "sonn", "ist", "ihm", "hier", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dort aber kann er sie umfangen.", "tokens": ["Dort", "a\u00b7ber", "kann", "er", "sie", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er stammte her aus edlen blute,", "tokens": ["Er", "stamm\u00b7te", "her", "aus", "ed\u00b7len", "blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wurde jung zur friedens-zeit.", "tokens": ["Und", "wur\u00b7de", "jung", "zur", "frie\u00b7dens\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jhm aber kam es nicht zu gute;", "tokens": ["Jhm", "a\u00b7ber", "kam", "es", "nicht", "zu", "gu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf friede folgte krieg und streit,", "tokens": ["Auf", "frie\u00b7de", "folg\u00b7te", "krieg", "und", "streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was hat er in den fr\u00fchlings-jahren", "tokens": ["Was", "hat", "er", "in", "den", "fr\u00fch\u00b7lings\u00b7jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vor frost und hitze nicht erfahren?", "tokens": ["Vor", "frost", "und", "hit\u00b7ze", "nicht", "er\u00b7fah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein edler geist ist wie die slammen,", "tokens": ["Ein", "ed\u00b7ler", "geist", "ist", "wie", "die", "slam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "KOKOM", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die \u00fcber rauch und flammen gehn:", "tokens": ["Die", "\u00fc\u00b7ber", "rauch", "und", "flam\u00b7men", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJD", "KON", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn alles ungl\u00fcck kommt zusammen,", "tokens": ["Wenn", "al\u00b7les", "un\u00b7gl\u00fcck", "kommt", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bleibt er in vollem glantze stehn.", "tokens": ["Bleibt", "er", "in", "vol\u00b7lem", "glant\u00b7ze", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wuste sich Herr M\u00fcnch zu schicken,", "tokens": ["So", "wus\u00b7te", "sich", "Herr", "M\u00fcnch", "zu", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "NE", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was hat ihn k\u00f6nnen unterdr\u00fccken?", "tokens": ["Was", "hat", "ihn", "k\u00f6n\u00b7nen", "un\u00b7ter\u00b7dr\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er gieng aus seinem vaterlande,", "tokens": ["Er", "gieng", "aus", "sei\u00b7nem", "va\u00b7ter\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und kam bey gro\u00dfen Herren an,", "tokens": ["Und", "kam", "bey", "gro\u00b7\u00dfen", "Her\u00b7ren", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit was vor treu, witz und verstande", "tokens": ["Mit", "was", "vor", "treu", ",", "witz", "und", "ver\u00b7stan\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "APPR", "ADJD", "$,", "VVIMP", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat er es andern vorgethan?", "tokens": ["Hat", "er", "es", "an\u00b7dern", "vor\u00b7ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des K\u00e4ysers freund, rath und legate", "tokens": ["Des", "K\u00e4y\u00b7sers", "freund", ",", "rath", "und", "le\u00b7ga\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erfuhr, was seine feder thate.", "tokens": ["Er\u00b7fuhr", ",", "was", "sei\u00b7ne", "fe\u00b7der", "tha\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was sag ich mehr von andern proben,", "tokens": ["Was", "sag", "ich", "mehr", "von", "an\u00b7dern", "pro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Franckreich, Deutsch- und Niederland", "tokens": ["Die", "Fran\u00b7ck\u00b7reich", ",", "Deut\u00b7sch", "und", "Nie\u00b7der\u00b7land"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "$,", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In seinen reisen muste loben,", "tokens": ["In", "sei\u00b7nen", "rei\u00b7sen", "mus\u00b7te", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVINF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es liebt ihn, wer ihn nur gekannt.", "tokens": ["Es", "liebt", "ihn", ",", "wer", "ihn", "nur", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was wunder, da\u00df er hier in Sachsen", "tokens": ["Was", "wun\u00b7der", ",", "da\u00df", "er", "hier", "in", "Sach\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "$,", "KOUS", "PPER", "ADV", "APPR", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In frieden endlich hochgewachsen?", "tokens": ["In", "frie\u00b7den", "end\u00b7lich", "hoch\u00b7ge\u00b7wach\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die g\u00f6ldne zeit, der edle frieden,", "tokens": ["Die", "g\u00f6ld\u00b7ne", "zeit", ",", "der", "ed\u00b7le", "frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kam endlich wieder bey uns an,", "tokens": ["Kam", "end\u00b7lich", "wie\u00b7der", "bey", "uns", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie hat ihm da, als einem m\u00fcden,", "tokens": ["Wie", "hat", "ihm", "da", ",", "als", "ei\u00b7nem", "m\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "KOUS", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ruh auch sanfft und wohlgethan.", "tokens": ["Die", "ruh", "auch", "sanfft", "und", "wohl\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des landes schatz war bey den linden,", "tokens": ["Des", "lan\u00b7des", "schatz", "war", "bey", "den", "lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In seiner treuen hand zu finden.", "tokens": ["In", "sei\u00b7ner", "treu\u00b7en", "hand", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Da ruht\u2019 er nun bey viel beschwerden,", "tokens": ["Da", "ruht'", "er", "nun", "bey", "viel", "be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "War GOtt und seinem F\u00fcrsten treu;", "tokens": ["War", "Gott", "und", "sei\u00b7nem", "F\u00fcrs\u00b7ten", "treu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da ihn GOtt nimmt von der erden,", "tokens": ["Und", "da", "ihn", "Gott", "nimmt", "von", "der", "er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bleibt sein andencken frisch und neu.", "tokens": ["Bleibt", "sein", "an\u00b7den\u00b7cken", "frisch", "und", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein guter ruhm wird bey uns bleiben,", "tokens": ["Sein", "gu\u00b7ter", "ruhm", "wird", "bey", "uns", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Weil man wird steuer-zeddul schreiben.", "tokens": ["Weil", "man", "wird", "steu\u00b7er\u00b7zed\u00b7dul", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}