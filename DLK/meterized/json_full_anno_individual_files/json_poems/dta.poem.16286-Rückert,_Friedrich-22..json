{"dta.poem.16286": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um eine Blum' im Korn, von Knabenaug' erblickt,", "tokens": ["Um", "ei\u00b7ne", "Blum'", "im", "Korn", ",", "von", "Kna\u00b7be\u00b7naug'", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPRART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um eine Blume wird wie mancher Halm geknickt!", "tokens": ["Um", "ei\u00b7ne", "Blu\u00b7me", "wird", "wie", "man\u00b7cher", "Halm", "ge\u00b7knickt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VAFIN", "KOKOM", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dem Landmann w\u00e4r' es gut, wenn unter seinem Rocken", "tokens": ["Dem", "Land\u00b7mann", "w\u00e4r'", "es", "gut", ",", "wenn", "un\u00b7ter", "sei\u00b7nem", "Ro\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gar keine Blume w\u00fcchs', um Knaben anzulocken.", "tokens": ["Gar", "kei\u00b7ne", "Blu\u00b7me", "w\u00fcchs'", ",", "um", "Kna\u00b7ben", "an\u00b7zu\u00b7lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$,", "KOUI", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dem Landmann w\u00e4r' es recht, wenn unter seinem Weizen", "tokens": ["Dem", "Land\u00b7mann", "w\u00e4r'", "es", "recht", ",", "wenn", "un\u00b7ter", "sei\u00b7nem", "Wei\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gar keine Bl\u00fcte st\u00fcnd', um Knabenlust zu reizen.", "tokens": ["Gar", "kei\u00b7ne", "Bl\u00fc\u00b7te", "st\u00fcnd'", ",", "um", "Kna\u00b7ben\u00b7lust", "zu", "rei\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$,", "KOUI", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Recht w\u00e4r' es ihm und gut, wenn unter seinen Saaten", "tokens": ["Recht", "w\u00e4r'", "es", "ihm", "und", "gut", ",", "wenn", "un\u00b7ter", "sei\u00b7nen", "Saa\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PPER", "KON", "ADJD", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht w\u00e4re, weshalb sie die Knaben ihm zertraten.", "tokens": ["Nicht", "w\u00e4\u00b7re", ",", "we\u00b7shalb", "sie", "die", "Kna\u00b7ben", "ihm", "zer\u00b7tra\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "$,", "PWAV", "PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Blumen nennet er Unkraut mit Recht, sie sind", "tokens": ["Die", "Blu\u00b7men", "nen\u00b7net", "er", "Un\u00b7kraut", "mit", "Recht", ",", "sie", "sind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "APPR", "NN", "$,", "PPER", "VAFIN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das allersch\u00e4dlichste f\u00fcr seiner Pflege Kind.", "tokens": ["Das", "al\u00b7ler\u00b7sch\u00e4d\u00b7lichs\u00b7te", "f\u00fcr", "sei\u00b7ner", "Pfle\u00b7ge", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Alswie am T\u00f6chterchen ein strenger Vater schalt", "tokens": ["Als\u00b7wie", "am", "T\u00f6ch\u00b7ter\u00b7chen", "ein", "stren\u00b7ger", "Va\u00b7ter", "schalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sch\u00f6nheit, die bei ihm nur als Verf\u00fchrung galt;", "tokens": ["Die", "Sch\u00f6n\u00b7heit", ",", "die", "bei", "ihm", "nur", "als", "Ver\u00b7f\u00fch\u00b7rung", "galt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "ADV", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Nur da\u00df der Vater nicht wie jener auch ausraufen", "tokens": ["Nur", "da\u00df", "der", "Va\u00b7ter", "nicht", "wie", "je\u00b7ner", "auch", "aus\u00b7rau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "PTKNEG", "KOKOM", "PDS", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Unkraut will noch darf, wonach die Knaben laufen.", "tokens": ["Das", "Un\u00b7kraut", "will", "noch", "darf", ",", "wo\u00b7nach", "die", "Kna\u00b7ben", "lau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VMFIN", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}