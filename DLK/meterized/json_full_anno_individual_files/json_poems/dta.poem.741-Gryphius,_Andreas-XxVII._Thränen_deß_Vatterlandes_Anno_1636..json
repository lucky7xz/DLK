{"dta.poem.741": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxVII.  Thr\u00e4nen de\u00df Vatterlandes/  \n  Anno  1636.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Wjr sindt doch nunmehr gantz/ ja mehr denn gantz verhe- ", "tokens": ["Wjr", "sindt", "doch", "nun\u00b7mehr", "gantz", "/", "ja", "mehr", "denn", "gantz", "ver\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ADV", "ADV", "$(", "ADV", "ADV", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ret!", "tokens": ["ret", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Der frechen V\u00f6lcker schaar/ die rasende Posaun", "tokens": ["Der", "fre\u00b7chen", "V\u00f6l\u00b7cker", "schaar", "/", "die", "ra\u00b7sen\u00b7de", "Po\u00b7saun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das vom Blutt fette ", "tokens": ["Das", "vom", "Blutt", "fet\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPRART", "ADJD", "ADJA"], "meter": "--++-", "measure": "anapaest.init"}, "line.5": {"text": "thaun.", "tokens": ["thaun", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Hat aller schwei\u00df/ vnd flei\u00df/ vnd vorraht auff gezehret.", "tokens": ["Hat", "al\u00b7ler", "schwei\u00df", "/", "vnd", "flei\u00df", "/", "vnd", "vor\u00b7raht", "auff", "ge\u00b7zeh\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "VVFIN", "$(", "KON", "NN", "$(", "KON", "VVFIN", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die T\u00fcrme stehn in glutt/ die Kirch ist vmbgekehret.", "tokens": ["Die", "T\u00fcr\u00b7me", "stehn", "in", "glutt", "/", "die", "Kirch", "ist", "vmb\u00b7ge\u00b7keh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJD", "$(", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Rahthau\u00df ligt im graus/ ", "tokens": ["Das", "Rah\u00b7thau\u00df", "ligt", "im", "graus", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Jungfrawn sindt gesch\u00e4n", "tokens": ["Die", "Jung\u00b7frawn", "sindt", "ge\u00b7sch\u00e4n"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "schawn", "tokens": ["schawn"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Ist ", "tokens": ["Ist"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Hier durch die Schantz vnd Stadt/ rinnt alzeit frisches", "tokens": ["Hier", "durch", "die", "Schantz", "vnd", "Stadt", "/", "rinnt", "al\u00b7zeit", "fri\u00b7sches"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "NN", "$(", "VVFIN", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Blutt.", "tokens": ["Blutt", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Dreymal sindt schon sechs Jahr als vnser str\u00f6me flutt/", "tokens": ["Drey\u00b7mal", "sindt", "schon", "sechs", "Jahr", "als", "vn\u00b7ser", "str\u00f6\u00b7me", "flutt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "CARD", "NN", "KOUS", "PPER", "VVFIN", "ADJD", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Von so viel Leichen schwer/ sich langsam fortgedrungen", "tokens": ["Von", "so", "viel", "Lei\u00b7chen", "schwer", "/", "sich", "lang\u00b7sam", "fort\u00b7ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN", "ADJD", "$(", "PRF", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch schweig ich noch von dem was \u00e4rger als der Todt.", "tokens": ["Doch", "schweig", "ich", "noch", "von", "dem", "was", "\u00e4r\u00b7ger", "als", "der", "Todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "PRELS", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was grimmer denn die Pest/ vnd glutt vnd hungers noth ", "tokens": ["Was", "grim\u00b7mer", "denn", "die", "Pest", "/", "vnd", "glutt", "vnd", "hun\u00b7gers", "noth"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "KON", "ART", "NN", "$(", "KON", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df auch der Seelen ", "tokens": ["Da\u00df", "auch", "der", "See\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}