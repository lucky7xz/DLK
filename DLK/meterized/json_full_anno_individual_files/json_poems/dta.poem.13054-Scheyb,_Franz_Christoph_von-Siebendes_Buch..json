{"dta.poem.13054": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eseht das geschm\u00fcckte Reich! den Schimmer dieser Nacht!", "tokens": ["\u201e", "seht", "das", "ge\u00b7schm\u00fcck\u00b7te", "Reich", "!", "den", "Schim\u00b7mer", "die\u00b7ser", "Nacht", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "ADJA", "NN", "$.", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eallein was haltet ihr von eines Tages Pracht?", "tokens": ["\u201e", "al\u00b7lein", "was", "hal\u00b7tet", "ihr", "von", "ei\u00b7nes", "Ta\u00b7ges", "Pracht", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "475\u201d(Trug er derselben vor) ihr kennt den Schein, die Helle", "tokens": ["\"", "(", "Trug", "er", "der\u00b7sel\u00b7ben", "vor", ")", "ihr", "kennt", "den", "Schein", ",", "die", "Hel\u00b7le"], "token_info": ["punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "$(", "NN", "PPER", "PDAT", "APPR", "$(", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201edes Strahlen-reichen Runds, der h\u00f6chsten Goldes-Quelle,", "tokens": ["\u201e", "des", "Strah\u00b7len\u00b7rei\u00b7chen", "Runds", ",", "der", "h\u00f6chs\u00b7ten", "Gol\u00b7des\u00b7Quel\u00b7le", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eder Sonne Majest\u00e4t? geb\u00fchrt es, oder nicht,", "tokens": ["\u201e", "der", "Son\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", "?", "ge\u00b7b\u00fchrt", "es", ",", "o\u00b7der", "nicht", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "$.", "VVFIN", "PPER", "$,", "KON", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eda\u00df, weil es euch und mir am gr\u00f6\u00dften Schmuck gebricht", "tokens": ["\u201e", "da\u00df", ",", "weil", "es", "euch", "und", "mir", "am", "gr\u00f6\u00df\u00b7ten", "Schmuck", "ge\u00b7bricht"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "$,", "KOUS", "PPER", "PPER", "KON", "PPER", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ederselben gleich zu seyn, wir sie als Haupt erkennen,", "tokens": ["\u201e", "der\u00b7sel\u00b7ben", "gleich", "zu", "seyn", ",", "wir", "sie", "als", "Haupt", "er\u00b7ken\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDAT", "ADV", "PTKZU", "VAINF", "$,", "PPER", "PPER", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "480\u201dUnd uns von selbiger die Untergebnen nennen?", "tokens": ["\"", "Und", "uns", "von", "sel\u00b7bi\u00b7ger", "die", "Un\u00b7ter\u00b7geb\u00b7nen", "nen\u00b7nen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "APPR", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201egef\u00e4llt es euch, da\u00df wir; wann sie das Schlaf-Gemach", "tokens": ["\u201e", "ge\u00b7f\u00e4llt", "es", "euch", ",", "da\u00df", "wir", ";", "wann", "sie", "das", "Schlaf\u00b7Ge\u00b7mach"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "$.", "PWAV", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201edes Morgens \u00f6ffnen wird, den Vorhang nach und nach", "tokens": ["\u201e", "des", "Mor\u00b7gens", "\u00f6ff\u00b7nen", "wird", ",", "den", "Vor\u00b7hang", "nach", "und", "nach"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADV", "VVINF", "VAFIN", "$,", "ART", "NN", "APPR", "KON", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201evon ihrem Auge zieht; auf ihren Anblick warten?", "tokens": ["\u201e", "von", "ih\u00b7rem", "Au\u00b7ge", "zieht", ";", "auf", "ih\u00b7ren", "An\u00b7blick", "war\u00b7ten", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201ewie wann wir unsern Schlu\u00df und Dienst ihr offenbarten?", "tokens": ["\u201e", "wie", "wann", "wir", "un\u00b7sern", "Schlu\u00df", "und", "Dienst", "ihr", "of\u00b7fen\u00b7bar\u00b7ten", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWAV", "PPER", "PPOSAT", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "485\u201dWollt ihr, sag' ich, hernach auf ihrer ganzen Reis,", "tokens": ["\"", "Wollt", "ihr", ",", "sag'", "ich", ",", "her\u00b7nach", "auf", "ih\u00b7rer", "gan\u00b7zen", "Reis", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201emit eurer Strahlen Licht geziert, in einem Krei\u00df", "tokens": ["\u201e", "mit", "eu\u00b7rer", "Strah\u00b7len", "Licht", "ge\u00b7ziert", ",", "in", "ei\u00b7nem", "Krei\u00df"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "APPR", "PPOSAT", "NN", "NN", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201erings um ihr Angesicht sie, wo sie f\u00e4hrt, begleiten?", "tokens": ["\u201e", "rings", "um", "ihr", "An\u00b7ge\u00b7sicht", "sie", ",", "wo", "sie", "f\u00e4hrt", ",", "be\u00b7glei\u00b7ten", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "APPR", "PPOSAT", "NN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201esagt, ob ihr zu dem Zug euch wollet vorbereiten?", "tokens": ["\u201e", "sagt", ",", "ob", "ihr", "zu", "dem", "Zug", "euch", "wol\u00b7let", "vor\u00b7be\u00b7rei\u00b7ten", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}