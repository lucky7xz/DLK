{"dta.poem.6398": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geschmack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie, oder wenn der Speisen scharffe Spitzen", "tokens": ["Wie", ",", "o\u00b7der", "wenn", "der", "Spei\u00b7sen", "scharf\u00b7fe", "Spit\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "KON", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu feste sitzen,", "tokens": ["Zu", "fes\u00b7te", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und keines aus einander weicht;", "tokens": ["Und", "kei\u00b7nes", "aus", "ein\u00b7an\u00b7der", "weicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch wenn ihr Stoff zu trocken, auch zu feucht;", "tokens": ["Auch", "wenn", "ihr", "Stoff", "zu", "tro\u00b7cken", ",", "auch", "zu", "feucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So wird fast kein Geschmack versp\u00fcret;", "tokens": ["So", "wird", "fast", "kein", "Ge\u00b7schmack", "ver\u00b7sp\u00fc\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dieweil die Nerven nicht dadurch ger\u00fchret.", "tokens": ["Die\u00b7weil", "die", "Ner\u00b7ven", "nicht", "da\u00b7durch", "ge\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}