{"dta.poem.12147": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Pflanzen lieb' ich, die im Bl\u00fchn und Welken gleichen", "tokens": ["Die", "Pflan\u00b7zen", "lieb'", "ich", ",", "die", "im", "Bl\u00fchn", "und", "Wel\u00b7ken", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "APPRART", "NN", "KON", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Menschen, aber sch\u00f6n und lieblich sind als Leichen.", "tokens": ["Den", "Men\u00b7schen", ",", "a\u00b7ber", "sch\u00f6n", "und", "lieb\u00b7lich", "sind", "als", "Lei\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dem Leben widerig ist jede Todesspur,", "tokens": ["Dem", "Le\u00b7ben", "wi\u00b7de\u00b7rig", "ist", "je\u00b7de", "To\u00b7des\u00b7spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und malerisch ein Baum ein abgestorbner nur.", "tokens": ["Und", "ma\u00b7le\u00b7risch", "ein", "Baum", "ein", "ab\u00b7ge\u00b7storb\u00b7ner", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}