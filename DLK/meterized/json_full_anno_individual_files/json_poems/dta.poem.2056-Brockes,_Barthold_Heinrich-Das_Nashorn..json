{"dta.poem.2056": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Nashorn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dieses wunderbare Thier, das so fremd, von wel-", "tokens": ["Die\u00b7ses", "wun\u00b7der\u00b7ba\u00b7re", "Thier", ",", "das", "so", "fremd", ",", "von", "wel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "APPR", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "chem man", "tokens": ["chem", "man"], "token_info": ["word", "word"], "pos": ["NE", "PIS"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Die besondere Figur schwerlich recht beschreiben kann,", "tokens": ["Die", "be\u00b7son\u00b7de\u00b7re", "Fi\u00b7gur", "schwer\u00b7lich", "recht", "be\u00b7schrei\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Soll dennoch an Kopf und Ohren unsern Schweinen et-", "tokens": ["Soll", "den\u00b7noch", "an", "Kopf", "und", "Oh\u00b7ren", "un\u00b7sern", "Schwei\u00b7nen", "et"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "NN", "KON", "NN", "PPOSAT", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "was gleichen,", "tokens": ["was", "glei\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["PWS", "ADJA", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Doch an Gr\u00f6\u00dfe seines K\u00f6rpers fast an Elephanten rei-", "tokens": ["Doch", "an", "Gr\u00f6\u00b7\u00dfe", "sei\u00b7nes", "K\u00f6r\u00b7pers", "fast", "an", "E\u00b7le\u00b7phan\u00b7ten", "rei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "PPOSAT", "NN", "ADV", "APPR", "NN", "TRUNC"], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "chen.", "tokens": ["chen", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Ob wir nun gleich viel von ihm und von seiner Haut er-", "tokens": ["Ob", "wir", "nun", "gleich", "viel", "von", "ihm", "und", "von", "sei\u00b7ner", "Haut", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "KON", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "z\u00e4hlen,", "tokens": ["z\u00e4h\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Da\u00df sie wirklich panzerf\u00f6rmig und wir sie mit starken", "tokens": ["Da\u00df", "sie", "wirk\u00b7lich", "pan\u00b7zer\u00b7f\u00f6r\u00b7mig", "und", "wir", "sie", "mit", "star\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "KON", "PPER", "PPER", "APPR", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "Schilden", "tokens": ["Schil\u00b7den"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Recht, als einen Harnisch mahlen, und wie wahre", "tokens": ["Recht", ",", "als", "ei\u00b7nen", "Har\u00b7nisch", "mah\u00b7len", ",", "und", "wie", "wah\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "VVINF", "$,", "KON", "PWAV", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Schuppen bilden,", "tokens": ["Schup\u00b7pen", "bil\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Dennoch wird an diesem Thier, und zumal an seiner", "tokens": ["Den\u00b7noch", "wird", "an", "die\u00b7sem", "Thier", ",", "und", "zu\u00b7mal", "an", "sei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PDAT", "NN", "$,", "KON", "ADV", "APPR", "PPOSAT"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.15": {"text": "Haut,", "tokens": ["Haut", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Etwas recht Betr\u00e4chtliches und Verwunderlichs ge-", "tokens": ["Et\u00b7was", "recht", "Be\u00b7tr\u00e4cht\u00b7li\u00b7ches", "und", "Ver\u00b7wun\u00b7der\u00b7lichs", "ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJA", "KON", "NN", "TRUNC"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "schaut,", "tokens": ["schaut", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.18": {"text": "Da sie blo\u00df durch ihre Falten, die so dick, so stark, und", "tokens": ["Da", "sie", "blo\u00df", "durch", "ih\u00b7re", "Fal\u00b7ten", ",", "die", "so", "dick", ",", "so", "stark", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.19": {"text": "fest,", "tokens": ["fest", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "-", "measure": "single.down"}, "line.20": {"text": "Dergestalt das Thier besch\u00fctzen, da\u00df sichs nicht durch-", "tokens": ["Der\u00b7ge\u00b7stalt", "das", "Thier", "be\u00b7sch\u00fct\u00b7zen", ",", "da\u00df", "sichs", "nicht", "durch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF", "$,", "KOUS", "PIS", "PTKNEG", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "dringen l\u00e4\u00dft,", "tokens": ["drin\u00b7gen", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "Und man schreibt, da\u00df es nicht nur vor den S\u00e4bel-", "tokens": ["Und", "man", "schreibt", ",", "da\u00df", "es", "nicht", "nur", "vor", "den", "S\u00e4\u00b7bel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "APPR", "ART", "TRUNC"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "streichen frey,", "tokens": ["strei\u00b7chen", "frey", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.24": {"text": "Sondern auch vor nicht zu starke Sch\u00fcsse selber sicher", "tokens": ["Son\u00b7dern", "auch", "vor", "nicht", "zu", "star\u00b7ke", "Sch\u00fcs\u00b7se", "sel\u00b7ber", "si\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PTKNEG", "PTKZU", "ADJA", "NN", "ADV", "ADJD"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.25": {"text": "sey.", "tokens": ["sey", "."], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+", "measure": "single.up"}, "line.26": {"text": "Seine Waffen sind ein Horn, das er auf der Nase tr\u00e4-", "tokens": ["Sei\u00b7ne", "Waf\u00b7fen", "sind", "ein", "Horn", ",", "das", "er", "auf", "der", "Na\u00b7se", "tr\u00e4"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.27": {"text": "get,", "tokens": ["get", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-", "measure": "single.down"}, "line.28": {"text": "Und dadurch den Elephanten, der sein steter Feind, er-", "tokens": ["Und", "da\u00b7durch", "den", "E\u00b7le\u00b7phan\u00b7ten", ",", "der", "sein", "ste\u00b7ter", "Feind", ",", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PAV", "ART", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "$,", "TRUNC"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "leget.", "tokens": ["le\u00b7get", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.30": {"text": "Seine Zunge soll so rauch und von solcher Sch\u00e4rfe seyn,", "tokens": ["Sei\u00b7ne", "Zun\u00b7ge", "soll", "so", "rauch", "und", "von", "sol\u00b7cher", "Sch\u00e4r\u00b7fe", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "ADJD", "KON", "APPR", "PIAT", "NN", "VAINF", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}}}}