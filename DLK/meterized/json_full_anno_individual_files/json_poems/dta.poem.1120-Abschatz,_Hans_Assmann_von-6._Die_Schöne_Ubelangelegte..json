{"dta.poem.1120": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "6. Die Sch\u00f6ne Ubelangelegte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht wo ich nur soll Speil und N\u00e4gel kriegen/ ", "tokens": ["Ich", "wei\u00df", "nicht", "wo", "ich", "nur", "soll", "Speil", "und", "N\u00e4\u00b7gel", "krie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWAV", "PPER", "ADV", "PIAT", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geschweige Nadeln gnug und B\u00e4nder auffzubringen/", "tokens": ["Ge\u00b7schwei\u00b7ge", "Na\u00b7deln", "gnug", "und", "B\u00e4n\u00b7der", "auff\u00b7zu\u00b7brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dadurch man Kleid und Haar in Ordnung k\u00f6nte zwingen.", "tokens": ["Da\u00b7durch", "man", "Kleid", "und", "Haar", "in", "Ord\u00b7nung", "k\u00f6n\u00b7te", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "NN", "KON", "NN", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch/ la\u00df es immerhin nach alter Weise fliegen.", "tokens": ["Doch", "/", "la\u00df", "es", "im\u00b7mer\u00b7hin", "nach", "al\u00b7ter", "Wei\u00b7se", "flie\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVIMP", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sol streut die Strahlen aus/ die alles Licht besiegen; ", "tokens": ["Sol", "streut", "die", "Strah\u00b7len", "aus", "/", "die", "al\u00b7les", "Licht", "be\u00b7sie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "ART", "NN", "PTKVZ", "$(", "ART", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Viel sch\u00f6ner sind zertheilt des Papagoyens Schwingen.", "tokens": ["Viel", "sch\u00f6\u00b7ner", "sind", "zer\u00b7theilt", "des", "Pa\u00b7pa\u00b7go\u00b7yens", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "VVPP", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "H\u00f6rt V\u00f6gel in dem Wald und aus dem Keficht singen/", "tokens": ["H\u00f6rt", "V\u00f6\u00b7gel", "in", "dem", "Wald", "und", "aus", "dem", "Ke\u00b7ficht", "sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die freyen werden euch weit mehr als die vergn\u00fcgen.", "tokens": ["Die", "frey\u00b7en", "wer\u00b7den", "euch", "weit", "mehr", "als", "die", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "ADJD", "PIAT", "KOKOM", "ART", "VVINF", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Ein muthig Ro\u00df nimmt nicht zu harte Z\u00e4umung an.", "tokens": ["Ein", "mut\u00b7hig", "Ro\u00df", "nimmt", "nicht", "zu", "har\u00b7te", "Z\u00e4u\u00b7mung", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PTKNEG", "PTKZU", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dein auff gel\u00f6\u00dftes Gold gleicht Berenizens Haaren/", "tokens": ["Dein", "auff", "ge\u00b7l\u00f6\u00df\u00b7tes", "Gold", "gleicht", "Be\u00b7re\u00b7ni\u00b7zens", "Haa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "ADJA", "NN", "VVFIN", "NE", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die an dem Himmel sich zu weisen w\u00fcrdig waren.", "tokens": ["Die", "an", "dem", "Him\u00b7mel", "sich", "zu", "wei\u00b7sen", "w\u00fcr\u00b7dig", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "PTKZU", "VVINF", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was nicht zu sehen taugt/ bindt und verdecket man.", "tokens": ["Was", "nicht", "zu", "se\u00b7hen", "taugt", "/", "bindt", "und", "ver\u00b7de\u00b7cket", "man", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$(", "VVFIN", "KON", "VVFIN", "PIS", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Uns wei\u00dft ein P\u00f6rsicht Haubt halb-blosser Br\u00fcste Zier/", "tokens": ["Uns", "wei\u00dft", "ein", "P\u00f6r\u00b7sicht", "Haubt", "halb\u00b7\u00b7blos\u00b7ser", "Br\u00fcs\u00b7te", "Zier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df dein Unachtsam-seyn geht andrer Auffputz f\u00fcr.", "tokens": ["Da\u00df", "dein", "Un\u00b7acht\u00b7sam\u00b7seyn", "geht", "an\u00b7drer", "Auff\u00b7putz", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}