{"textgrid.poem.36228": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf der Pilgrimschaft", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jungfrau, ewig Braut am Throne", "tokens": ["Jung\u00b7frau", ",", "e\u00b7wig", "Braut", "am", "Thro\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dessen, der vor allen Zeiten", "tokens": ["Des\u00b7sen", ",", "der", "vor", "al\u00b7len", "Zei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich zum Troste uns bereiten", "tokens": ["Dich", "zum", "Tros\u00b7te", "uns", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollte, f\u00fcr des Lebens Frone.", "tokens": ["Woll\u00b7te", ",", "f\u00fcr", "des", "Le\u00b7bens", "Fro\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Du des heil'gen Gartens Krone,", "tokens": ["Du", "des", "heil'\u00b7gen", "Gar\u00b7tens", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hohe Perle, so uns bliebe,", "tokens": ["Ho\u00b7he", "Per\u00b7le", ",", "so", "uns", "blie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Quell der gottgeweihten Triebe,", "tokens": ["Quell", "der", "gott\u00b7ge\u00b7weih\u00b7ten", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Strahlenglanz der ew'gen Liebe,", "tokens": ["Strah\u00b7len\u00b7glanz", "der", "ew'\u00b7gen", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Du, von der Gott selber schriebe,", "tokens": ["Du", ",", "von", "der", "Gott", "sel\u00b7ber", "schrie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.10": {"text": "K\u00f6nigin dich hie\u00df zum Lohne.", "tokens": ["K\u00f6\u00b7ni\u00b7gin", "dich", "hie\u00df", "zum", "Loh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Teure Zuflucht f\u00fcr Entfloh'ne,", "tokens": ["Teu\u00b7re", "Zu\u00b7flucht", "f\u00fcr", "Ent\u00b7floh'\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Milder \u00d6lbaum reich an Fr\u00fcchten,", "tokens": ["Mil\u00b7der", "\u00d6l\u00b7baum", "reich", "an", "Fr\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In des Schatten wir uns fl\u00fcchten,", "tokens": ["In", "des", "Schat\u00b7ten", "wir", "uns", "fl\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da der Friede selig wohne.", "tokens": ["Da", "der", "Frie\u00b7de", "se\u00b7lig", "woh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deiner Glorien lichte Krone", "tokens": ["Dei\u00b7ner", "Glo\u00b7ri\u00b7en", "lich\u00b7te", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Wollte Salomo schon zeigen,", "tokens": ["Woll\u00b7te", "Sa\u00b7lo\u00b7mo", "schon", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Engel feiern's in den Reigen;", "tokens": ["En\u00b7gel", "fei\u00b7ern's", "in", "den", "Rei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Du, der sich die Himmel neigen,", "tokens": ["Du", ",", "der", "sich", "die", "Him\u00b7mel", "nei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Stumm die Sch\u00f6nsten alle schweigen,", "tokens": ["Stumm", "die", "Sch\u00f6ns\u00b7ten", "al\u00b7le", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PIAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Vor der Mutter mit dem Sohne.", "tokens": ["Vor", "der", "Mut\u00b7ter", "mit", "dem", "Soh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, wie spricht in sanftem Tone", "tokens": ["Ach", ",", "wie", "spricht", "in", "sanf\u00b7tem", "To\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die hodsel'ge Heiterkeit,", "tokens": ["Die", "hod\u00b7sel'\u00b7ge", "Hei\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gnadenvolle G\u00fctigkeit,", "tokens": ["Gna\u00b7den\u00b7vol\u00b7le", "G\u00fc\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie freundlich unser schone.", "tokens": ["Da\u00df", "sie", "freund\u00b7lich", "un\u00b7ser", "scho\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In den Feldern von Sione,", "tokens": ["In", "den", "Fel\u00b7dern", "von", "Si\u00b7o\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lilienblume hold verschlossen,", "tokens": ["Li\u00b7li\u00b7en\u00b7blu\u00b7me", "hold", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Frommer Demut Palm' entsprossen,", "tokens": ["From\u00b7mer", "De\u00b7mut", "Palm'", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die des Segens F\u00fcll' ergossen,", "tokens": ["Die", "des", "Se\u00b7gens", "F\u00fcll'", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Uns gewaffnet mit Geschossen,", "tokens": ["Uns", "ge\u00b7waff\u00b7net", "mit", "Ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Allen Schrecken gar zum Hohne.", "tokens": ["Al\u00b7len", "Schre\u00b7cken", "gar", "zum", "Hoh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lieb' entquillt aus jeder Zone", "tokens": ["Lieb'", "ent\u00b7quillt", "aus", "je\u00b7der", "Zo\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir, des Lebens neue Sonne;", "tokens": ["Dir", ",", "des", "Le\u00b7bens", "neu\u00b7e", "Son\u00b7ne", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leuchtend Licht, das uns, o Wonne,", "tokens": ["Leuch\u00b7tend", "Licht", ",", "das", "uns", ",", "o", "Won\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "PPER", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neu erschuf im ird'schen Tone!", "tokens": ["Neu", "er\u00b7schuf", "im", "ird'\u00b7schen", "To\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Herrin! ach was sind wir ohne", "tokens": ["Her\u00b7rin", "!", "ach", "was", "sind", "wir", "oh\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ITJ", "PWS", "VAFIN", "PPER", "APPR"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine s\u00fc\u00dfe Huld zu achten?", "tokens": ["Dei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Huld", "zu", "ach\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn wir gleich die Pein verlachten,", "tokens": ["Wenn", "wir", "gleich", "die", "Pein", "ver\u00b7lach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird die Schuld uns tr\u00fcb umnachten,", "tokens": ["Wird", "die", "Schuld", "uns", "tr\u00fcb", "um\u00b7nach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn es nicht die Augen machten,", "tokens": ["Wenn", "es", "nicht", "die", "Au\u00b7gen", "mach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Lichter Hoffnung Chalcedone.", "tokens": ["Lich\u00b7ter", "Hoff\u00b7nung", "Chal\u00b7ce\u00b7do\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schau herab von deinem Throne,", "tokens": ["Schau", "her\u00b7ab", "von", "dei\u00b7nem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nigin, zu der wir trachten,", "tokens": ["K\u00f6\u00b7ni\u00b7gin", ",", "zu", "der", "wir", "trach\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsern Feind durch dich verachten,", "tokens": ["Un\u00b7sern", "Feind", "durch", "dich", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeden Schmerz in Frieden brachten,", "tokens": ["Je\u00b7den", "Schmerz", "in", "Frie\u00b7den", "brach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ende du mein tiefes Schmachten,", "tokens": ["En\u00b7de", "du", "mein", "tie\u00b7fes", "Schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], 