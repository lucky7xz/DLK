{"dta.poem.882": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "MuSIK IM MIRABELL", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Brunnen singt. Die Wolken stehn", "tokens": ["Ein", "Brun\u00b7nen", "singt", ".", "Die", "Wol\u00b7ken", "stehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im klaren Blau, die wei\u00dfen, zarten.", "tokens": ["Im", "kla\u00b7ren", "Blau", ",", "die", "wei\u00b7\u00dfen", ",", "zar\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bed\u00e4chtig stille Menschen gehn", "tokens": ["Be\u00b7d\u00e4ch\u00b7tig", "stil\u00b7le", "Men\u00b7schen", "gehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Abend durch den alten Garten.", "tokens": ["Am", "A\u00b7bend", "durch", "den", "al\u00b7ten", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Ahnen Marmor ist ergraut.", "tokens": ["Der", "Ah\u00b7nen", "Mar\u00b7mor", "ist", "er\u00b7graut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Vogelzug streift in die Weiten.", "tokens": ["Ein", "Vo\u00b7gel\u00b7zug", "streift", "in", "die", "Wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Faun mit toten Augen schaut", "tokens": ["Ein", "Faun", "mit", "to\u00b7ten", "Au\u00b7gen", "schaut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Schatten, die ins Dunkel gleiten.", "tokens": ["Nach", "Schat\u00b7ten", ",", "die", "ins", "Dun\u00b7kel", "glei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das Laub f\u00e4llt rot vom alten Baum", "tokens": ["Das", "Laub", "f\u00e4llt", "rot", "vom", "al\u00b7ten", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kreist herein durchs offne Fenster.", "tokens": ["Und", "kreist", "her\u00b7ein", "durchs", "off\u00b7ne", "Fens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Feuerschein gl\u00fcht auf im Raum", "tokens": ["Ein", "Feu\u00b7er\u00b7schein", "gl\u00fcht", "auf", "im", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und malet tr\u00fcbe Angstgespenster.", "tokens": ["Und", "ma\u00b7let", "tr\u00fc\u00b7be", "Angst\u00b7ge\u00b7spens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein wei\u00dfer Fremdling tritt ins Haus.", "tokens": ["Ein", "wei\u00b7\u00dfer", "Fremd\u00b7ling", "tritt", "ins", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Hund st\u00fcrzt durch verfallene G\u00e4nge.", "tokens": ["Ein", "Hund", "st\u00fcrzt", "durch", "ver\u00b7fal\u00b7le\u00b7ne", "G\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Magd l\u00f6scht eine Lampe aus,", "tokens": ["Die", "Magd", "l\u00f6scht", "ei\u00b7ne", "Lam\u00b7pe", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Ohr h\u00f6rt nachts Sonatenkl\u00e4nge.", "tokens": ["Das", "Ohr", "h\u00f6rt", "nachts", "So\u00b7na\u00b7ten\u00b7kl\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}