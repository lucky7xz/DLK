{"dta.poem.974": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "IiI", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Welche beiden mittern&#228;chte!                 ", "tokens": ["Wel\u00b7che", "bei\u00b7den", "mit\u00b7tern", "&#228;", "ch\u00b7te", "!"], "token_info": ["word", "word", "word", "XML_entity", "word", "punct"], "pos": ["PWS", "PIS", "VVINF", "$(", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als der selber schmerzdurchbohrte                 ", "tokens": ["Als", "der", "sel\u00b7ber", "schmerz\u00b7durch\u00b7bohr\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An der dulderin sich r&#228;chte               ", "tokens": ["An", "der", "dul\u00b7de\u00b7rin", "sich", "r", "&#228;", "ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["APPR", "ART", "NN", "PRF", "NE", "$(", "VVFIN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Dass dein blick sich weich umflorte                 ", "tokens": ["Dass", "dein", "blick", "sich", "weich", "um\u00b7flor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ADJD", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Dass dein wink ihr mildrung br&#228;chte!                 ", "tokens": ["Dass", "dein", "wink", "ihr", "mil\u00b7drung", "br", "&#228;", "ch\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "PPOSAT", "NN", "FM", "FM", "FM", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Eines sah des andren wunden               ", "tokens": ["Ei\u00b7nes", "sah", "des", "an\u00b7dren", "wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Durch des dunkels dichte m&#228;hne                 ", "tokens": ["Durch", "des", "dun\u00b7kels", "dich\u00b7te", "m", "&#228;", "hne"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$(", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zucken rieseln unverbunden &#183; &#183;                 ", "tokens": ["Zu\u00b7cken", "rie\u00b7seln", "un\u00b7ver\u00b7bun\u00b7den", "&#183;", "&#183;"], "token_info": ["word", "word", "word", "XML_entity", "XML_entity"], "pos": ["NN", "VVFIN", "ADJD", "$(", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nicht wort nicht thr&#228;ne.               ", "tokens": ["Und", "nicht", "wort", "nicht", "thr", "&#228;", "ne", "."], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["KON", "PTKNEG", "NN", "PTKNEG", "ADJD", "$(", "NE", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}}}}