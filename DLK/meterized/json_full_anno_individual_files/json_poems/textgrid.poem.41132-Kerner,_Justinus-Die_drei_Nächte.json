{"textgrid.poem.41132": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Die drei N\u00e4chte", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei N\u00e4chte sind es, die mich plagen.", "tokens": ["Drei", "N\u00e4ch\u00b7te", "sind", "es", ",", "die", "mich", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wer die sind, will ich dir sagen.", "tokens": ["Und", "wer", "die", "sind", ",", "will", "ich", "dir", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "VAFIN", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Der Himmel ohne Himmelblau,", "tokens": ["Der", "Him\u00b7mel", "oh\u00b7ne", "Him\u00b7mel\u00b7blau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Auge krank, umschleiert grau,", "tokens": ["Das", "Au\u00b7ge", "krank", ",", "um\u00b7schlei\u00b7ert", "grau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tief innen finstrer Gram und Reu',", "tokens": ["Tief", "in\u00b7nen", "finst\u00b7rer", "Gram", "und", "Reu'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADJA", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sind der schwarzen N\u00e4chte drei.", "tokens": ["Das", "sind", "der", "schwar\u00b7zen", "N\u00e4ch\u00b7te", "drei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun kommt noch eine vierte Nacht,", "tokens": ["Nun", "kommt", "noch", "ei\u00b7ne", "vier\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mir die drei vergessen macht.", "tokens": ["Die", "mir", "die", "drei", "ver\u00b7ges\u00b7sen", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "CARD", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Komm, vierte Nacht, und bring mir Ruh':", "tokens": ["Komm", ",", "vier\u00b7te", "Nacht", ",", "und", "bring", "mir", "Ruh'", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Grabnacht! deck' die drei andern zu!", "tokens": ["Grab\u00b7nacht", "!", "deck'", "die", "drei", "an\u00b7dern", "zu", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "CARD", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Drei N\u00e4chte sind es, die mich plagen.", "tokens": ["Drei", "N\u00e4ch\u00b7te", "sind", "es", ",", "die", "mich", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PP