{"dta.poem.16271": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Einzelliebe wer beginnet zu verschwenden", "tokens": ["Mit", "Ein\u00b7zel\u00b7lie\u00b7be", "wer", "be\u00b7gin\u00b7net", "zu", "ver\u00b7schwen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PWS", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Schatz des Herzens, wird mit Eigenliebe enden.", "tokens": ["Den", "Schatz", "des", "Her\u00b7zens", ",", "wird", "mit", "Ei\u00b7gen\u00b7lie\u00b7be", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Allliebe sei es die zuerst das Herz erf\u00fcllt,", "tokens": ["A\u00b7lllie\u00b7be", "sei", "es", "die", "zu\u00b7erst", "das", "Herz", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus deren Zauberduft sich Einzellieb' enth\u00fcllt.", "tokens": ["Aus", "de\u00b7ren", "Zau\u00b7ber\u00b7duft", "sich", "Ein\u00b7zel\u00b7lieb'", "ent\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PRF", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Einzelliebe bl\u00fcht und welkt, der Traum sinkt nieder,", "tokens": ["Die", "Ein\u00b7zel\u00b7lie\u00b7be", "bl\u00fcht", "und", "welkt", ",", "der", "Traum", "sinkt", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie am Anfang steht am End' Allliebe wieder:", "tokens": ["Und", "wie", "am", "An\u00b7fang", "steht", "am", "End'", "A\u00b7lllie\u00b7be", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "VVFIN", "APPRART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Allliebe zur Natur, zu jeder Kreatur,", "tokens": ["A\u00b7lllie\u00b7be", "zur", "Na\u00b7tur", ",", "zu", "je\u00b7der", "Kre\u00b7a\u00b7tur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu Gott, und in dir selbst zu jeder Gottesspur.", "tokens": ["Zu", "Gott", ",", "und", "in", "dir", "selbst", "zu", "je\u00b7der", "Got\u00b7tes\u00b7spur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "APPR", "PPER", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}