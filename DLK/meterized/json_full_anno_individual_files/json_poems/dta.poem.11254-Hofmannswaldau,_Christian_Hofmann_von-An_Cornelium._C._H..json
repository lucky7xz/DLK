{"dta.poem.11254": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Cornelium.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die g\u00f6tter mahlte man mit h\u00f6rnern bey den alten;", "tokens": ["Die", "g\u00f6t\u00b7ter", "mahl\u00b7te", "man", "mit", "h\u00f6r\u00b7nern", "bey", "den", "al\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PIS", "APPR", "VVFIN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich glaube, da\u00df dein weib, Corneli! dieses wei\u00df,", "tokens": ["Ich", "glau\u00b7be", ",", "da\u00df", "dein", "weib", ",", "Cor\u00b7ne\u00b7li", "!", "die\u00b7ses", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "$,", "NE", "$.", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum giebt sie ihren leib aus lieb und hoffart prei\u00df,", "tokens": ["Drum", "giebt", "sie", "ih\u00b7ren", "leib", "aus", "lieb", "und", "hof\u00b7fart", "prei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJD", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df man sie soll hernach vor eine g\u00f6ttin halten.", "tokens": ["Da\u00df", "man", "sie", "soll", "her\u00b7nach", "vor", "ei\u00b7ne", "g\u00f6t\u00b7tin", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}