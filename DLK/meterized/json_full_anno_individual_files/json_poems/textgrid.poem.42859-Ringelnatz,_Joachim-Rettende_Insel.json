nct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und wir trinken. Es gesellen", "tokens": ["Und", "wir", "trin\u00b7ken", ".", "Es", "ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Andre sich dazu. Die Wellen", "tokens": ["And\u00b7re", "sich", "da\u00b7zu", ".", "Die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "PRF", "PAV", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00e4tten sich. Der Ha\u00df zerstiebt.", "tokens": ["Gl\u00e4t\u00b7ten", "sich", ".", "Der", "Ha\u00df", "zer\u00b7stiebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis zuletzt in s\u00fc\u00dfer Ruhe", "tokens": ["Bis", "zu\u00b7letzt", "in", "s\u00fc\u00b7\u00dfer", "Ru\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Niemand noch was in die Schuhe", "tokens": ["Nie\u00b7mand", "noch", "was", "in", "die", "Schu\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "PWS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Andrer schiebt,", "tokens": ["A\u00b7ndrer", "schiebt", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und sich alles gegenseitig", "tokens": ["Und", "sich", "al\u00b7les", "ge\u00b7gen\u00b7sei\u00b7tig"], "token_info": ["word", "word", "word", "w