{"textgrid.poem.42859": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Rettende Insel", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Parteien sich und Massen", "tokens": ["Wenn", "Par\u00b7tei\u00b7en", "sich", "und", "Mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PRF", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sichtbar und ger\u00e4uschvoll hassen,", "tokens": ["Sicht\u00b7bar", "und", "ge\u00b7r\u00e4uschvoll", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Klingt das mir wie Meeresrauschen.", "tokens": ["Klingt", "das", "mir", "wie", "Mee\u00b7res\u00b7rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PPER", "KOKOM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dann mag ich henkelltrocken", "tokens": ["Und", "dann", "mag", "ich", "hen\u00b7kell\u00b7tro\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Still auf einer Insel hocken,", "tokens": ["Still", "auf", "ei\u00b7ner", "In\u00b7sel", "ho\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die mich zusehn l\u00e4\u00dft und lauschen.", "tokens": ["Die", "mich", "zu\u00b7sehn", "l\u00e4\u00dft", "und", "lau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], 