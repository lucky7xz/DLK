za.3": {"line.1": {"text": "Wechselnd laut und wieder leise", "tokens": ["Wech\u00b7selnd", "laut", "und", "wie\u00b7der", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "KON", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rauscht das Meer in weitem Kreise", "tokens": ["Rauscht", "das", "Meer", "in", "wei\u00b7tem", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir vertraute Melodie.", "tokens": ["Mir", "ver\u00b7trau\u00b7te", "Me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo blind oder falsch gestempelt", "tokens": ["Wo", "blind", "o\u00b7der", "falsch", "ge\u00b7stem\u00b7pelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mi\u00dfklang sich an Mi\u00dfklang rempelt,", "tokens": ["Mi\u00df\u00b7klang", "sich", "an", "Mi\u00df\u00b7klang", "rem\u00b7pelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Windelt neue Harmonie.", "tokens": ["Win\u00b7delt", "neu\u00b7e", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und dann schwimmt \u2013 fast ist es schade \u2013", "tokens": ["Und", "dann", "schwimmt", "\u2013", "fast", "ist", "es", "scha\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "