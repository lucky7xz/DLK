{"dta.poem.13152": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eder Vorschlag schien mir schwer, jedoch erkl\u00e4rt ich mich,", "tokens": ["\u201e", "der", "Vor\u00b7schlag", "schien", "mir", "schwer", ",", "je\u00b7doch", "er\u00b7kl\u00e4rt", "ich", "mich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201e(Weil mir die M\u00f6glichkeit schon durch die Sinnen schlich)", "tokens": ["\u201e", "(", "Weil", "mir", "die", "M\u00f6g\u00b7lich\u00b7keit", "schon", "durch", "die", "Sin\u00b7nen", "schlich", ")"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "KOUS", "PPER", "ART", "NN", "ADV", "APPR", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eda\u00df ich zu solchem Bau woll\u2019 einen Ri\u00df entwerffen;", "tokens": ["\u201e", "da\u00df", "ich", "zu", "sol\u00b7chem", "Bau", "woll'", "ei\u00b7nen", "Ri\u00df", "ent\u00b7werf\u00b7fen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "APPR", "PIAT", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eich wei\u00df nicht, werd\u2019 ich ihn auch hier beschreiben d\u00e4rffen.", "tokens": ["\u201e", "ich", "wei\u00df", "nicht", ",", "werd'", "ich", "ihn", "auch", "hier",