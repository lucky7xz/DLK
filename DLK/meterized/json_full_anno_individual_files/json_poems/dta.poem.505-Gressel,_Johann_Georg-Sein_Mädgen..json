{"dta.poem.505": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sein M\u00e4dgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein M\u00e4dgen das soll seyn", "tokens": ["Mein", "M\u00e4d\u00b7gen", "das", "soll", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PDS", "VMFIN", "VAINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ein Kind von b\u00f6sen Sinnen/", "tokens": ["Ein", "Kind", "von", "b\u00f6\u00b7sen", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Lippen sich erfreun", "tokens": ["Des", "Lip\u00b7pen", "sich", "er\u00b7freun"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn sie frech schwatzen k\u00fcnnen.", "tokens": ["Wenn", "sie", "frech", "schwat\u00b7zen", "k\u00fcn\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das frech und sch\u00f6ne ist", "tokens": ["Das", "frech", "und", "sch\u00f6\u00b7ne", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "KON", "ADJA", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das Schl\u00e4ge nimmt und giebet/", "tokens": ["Das", "Schl\u00e4\u00b7ge", "nimmt", "und", "gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das nach den Schl\u00e4gen k\u00fc\u00dft/", "tokens": ["Das", "nach", "den", "Schl\u00e4\u00b7gen", "k\u00fc\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und desto hefftger liebet.", "tokens": ["Und", "des\u00b7to", "hefft\u00b7ger", "lie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wenn es nicht so gesinnt/", "tokens": ["Wenn", "es", "nicht", "so", "ge\u00b7sinnt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVPP", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Kensch/ sitsahm ist hingegen/", "tokens": ["Kensch", "/", "sit\u00b7sahm", "ist", "hin\u00b7ge\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "VAFIN", "ADV", "$("], "meter": "-+-+++-", "measure": "unknown.measure.tetra"}, "line.11": {"text": "So schlag\u2019 ichs in den Wind/", "tokens": ["So", "schlag'", "ichs", "in", "den", "Wind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Und wils nicht bey mir hegen.", "tokens": ["Und", "wils", "nicht", "bey", "mir", "he\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}