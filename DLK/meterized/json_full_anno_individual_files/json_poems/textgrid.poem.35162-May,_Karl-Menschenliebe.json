{"textgrid.poem.35162": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Menschenliebe", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Tagesgrauen schlief das stille Thal,", "tokens": ["Im", "Ta\u00b7ges\u00b7grau\u00b7en", "schlief", "das", "stil\u00b7le", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und seine Sch\u00f6nheit war mir noch verborgen;", "tokens": ["Und", "sei\u00b7ne", "Sch\u00f6n\u00b7heit", "war", "mir", "noch", "ver\u00b7bor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann pl\u00f6tzlich kam der erste Sonnenstrahl,", "tokens": ["Dann", "pl\u00f6tz\u00b7lich", "kam", "der", "ers\u00b7te", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und mit ihm ward es heller, goldner Morgen.", "tokens": ["Und", "mit", "ihm", "ward", "es", "hel\u00b7ler", ",", "gold\u00b7ner", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "PPER", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es fluthete das Licht vom Himmel nieder,", "tokens": ["Es", "flut\u00b7he\u00b7te", "das", "Licht", "vom", "Him\u00b7mel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als habe er sich selbst herabgesenkt,", "tokens": ["Als", "ha\u00b7be", "er", "sich", "selbst", "her\u00b7ab\u00b7ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und laut erklangen alle Morgenlieder,", "tokens": ["Und", "laut", "er\u00b7klan\u00b7gen", "al\u00b7le", "Mor\u00b7gen\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die er allein, allein dem Walde schenkt.", "tokens": ["Die", "er", "al\u00b7lein", ",", "al\u00b7lein", "dem", "Wal\u00b7de", "schenkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "$,", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun ging des Tages Engel \u00fcber Land,", "tokens": ["Nun", "ging", "des", "Ta\u00b7ges", "En\u00b7gel", "\u00fc\u00b7ber", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ging durch den Hag, ging \u00fcber Feld und Auen,", "tokens": ["Ging", "durch", "den", "Hag", ",", "ging", "\u00fc\u00b7ber", "Feld", "und", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und \u00fcberall, wo er ein Bl\u00fcmlein fand,", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", ",", "wo", "er", "ein", "Bl\u00fcm\u00b7lein", "fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bog er sich nieder, um es anzuschauen.", "tokens": ["Bog", "er", "sich", "nie\u00b7der", ",", "um", "es", "an\u00b7zu\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$,", "KOUI", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er kam auf allen Wegen hergeschritten,", "tokens": ["Er", "kam", "auf", "al\u00b7len", "We\u00b7gen", "her\u00b7ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und sah er wo ein wartend Fensterlein,", "tokens": ["Und", "sah", "er", "wo", "ein", "war\u00b7tend", "Fens\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PWAV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So lie\u00df er sich nicht lange darum bitten,", "tokens": ["So", "lie\u00df", "er", "sich", "nicht", "lan\u00b7ge", "da\u00b7rum", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er gab ihm Licht und gab ihm Sonnenschein.", "tokens": ["Er", "gab", "ihm", "Licht", "und", "gab", "ihm", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er stieg den Berg, den steilen Fels hinan,", "tokens": ["Er", "stieg", "den", "Berg", ",", "den", "stei\u00b7len", "Fels", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Klomm auf die Firnen, in die Kletscherspalten;", "tokens": ["Klomm", "auf", "die", "Fir\u00b7nen", ",", "in", "die", "Klet\u00b7scher\u00b7spal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er kletterte in alle Tiefen dann,", "tokens": ["Er", "klet\u00b7ter\u00b7te", "in", "al\u00b7le", "Tie\u00b7fen", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kam \u00fcber schroffe H\u00e4nge, todte Halden,", "tokens": ["Kam", "\u00fc\u00b7ber", "schrof\u00b7fe", "H\u00e4n\u00b7ge", ",", "tod\u00b7te", "Hal\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und \u00fcberall, am h\u00f6chsten, tiefsten Orte", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", ",", "am", "h\u00f6chs\u00b7ten", ",", "tiefs\u00b7ten", "Or\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ward ihm der Mensch, das Thier, der Baum, der Stein", "tokens": ["Ward", "ihm", "der", "Mensch", ",", "das", "Thier", ",", "der", "Baum", ",", "der", "Stein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zum mahnenden, zum heilgen Gottesworte:", "tokens": ["Zum", "mah\u00b7nen\u00b7den", ",", "zum", "heil\u00b7gen", "Got\u00b7tes\u00b7wor\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbgieb Liebe hier; auch diese Welt ist mein!\u00ab \u2013", "tokens": ["\u00bb", "gieb", "Lie\u00b7be", "hier", ";", "auch", "die\u00b7se", "Welt", "ist", "mein", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVIMP", "NN", "ADV", "$.", "ADV", "PDAT", "NN", "VAFIN", "PPOSAT", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So liegt des Menschen Herz in dunkler Nacht,", "tokens": ["So", "liegt", "des", "Men\u00b7schen", "Herz", "in", "dunk\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sich die Andern ihm nicht g\u00fctig zeigen;", "tokens": ["Wenn", "sich", "die", "An\u00b7dern", "ihm", "nicht", "g\u00fc\u00b7tig", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch, wird der Strahl der Liebe ihm gebracht,", "tokens": ["Doch", ",", "wird", "der", "Strahl", "der", "Lie\u00b7be", "ihm", "ge\u00b7bracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wird das Dunkel bald dem Lichte weichen.", "tokens": ["So", "wird", "das", "Dun\u00b7kel", "bald", "dem", "Lich\u00b7te", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann zeigen sich in Bl\u00fcthen seine Auen;", "tokens": ["Dann", "zei\u00b7gen", "sich", "in", "Bl\u00fc\u00b7then", "sei\u00b7ne", "Au\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Es sprudeln alle Quellen hell und klar,", "tokens": ["Es", "spru\u00b7deln", "al\u00b7le", "Quel\u00b7len", "hell", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und du kannst Alles, Alles deutlich schauen,", "tokens": ["Und", "du", "kannst", "Al\u00b7les", ",", "Al\u00b7les", "deut\u00b7lich", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIS", "$,", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was ohne Liebe dir verborgen war.", "tokens": ["Was", "oh\u00b7ne", "Lie\u00b7be", "dir", "ver\u00b7bor\u00b7gen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dann steig empor, steig nieder in das Land,", "tokens": ["Dann", "steig", "em\u00b7por", ",", "steig", "nie\u00b7der", "in", "das", "Land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das sich in reicher Sch\u00f6nheit vor dir breitet;", "tokens": ["Das", "sich", "in", "rei\u00b7cher", "Sch\u00f6n\u00b7heit", "vor", "dir", "brei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch thue es mit schonendem Verstand,", "tokens": ["Doch", "thue", "es", "mit", "scho\u00b7nen\u00b7dem", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der niemals \u00fcber Heiligth\u00fcmer schreitet.", "tokens": ["Der", "nie\u00b7mals", "\u00fc\u00b7ber", "Hei\u00b7ligt\u00b7h\u00fc\u00b7mer", "schrei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und willst du weiter, immer weiter gehen,", "tokens": ["Und", "willst", "du", "wei\u00b7ter", ",", "im\u00b7mer", "wei\u00b7ter", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKVZ", "$,", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bis dort, wohin vielleicht noch Niemand kam,", "tokens": ["Bis", "dort", ",", "wo\u00b7hin", "viel\u00b7leicht", "noch", "Nie\u00b7mand", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "PWAV", "ADV", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So wirst du bald erkennen und verstehen,", "tokens": ["So", "wirst", "du", "bald", "er\u00b7ken\u00b7nen", "und", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wer dieser Welt das Licht, die W\u00e4rme nahm.", "tokens": ["Wer", "die\u00b7ser", "Welt", "das", "Licht", ",", "die", "W\u00e4r\u00b7me", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So wird sie dir vielleicht wohl lieb und werth;", "tokens": ["So", "wird", "sie", "dir", "viel\u00b7leicht", "wohl", "lieb", "und", "werth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du lernst sie besser, immer besser kennen;", "tokens": ["Du", "lernst", "sie", "bes\u00b7ser", ",", "im\u00b7mer", "bes\u00b7ser", "ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie bietet dir des Freundes Haus und Herd;", "tokens": ["Sie", "bie\u00b7tet", "dir", "des", "Freun\u00b7des", "Haus", "und", "Herd", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du m\u00f6chtest dich nicht wieder von ihr trennen.", "tokens": ["Du", "m\u00f6ch\u00b7test", "dich", "nicht", "wie\u00b7der", "von", "ihr", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wenn sie so dir eigen ist geworden,", "tokens": ["Und", "wenn", "sie", "so", "dir", "ei\u00b7gen", "ist", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPER", "ADJD", "VAFIN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ist sie, die fr\u00fcher fremde, g\u00e4nzlich dein.", "tokens": ["Ist", "sie", ",", "die", "fr\u00fc\u00b7her", "frem\u00b7de", ",", "g\u00e4nz\u00b7lich", "dein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PRELS", "ADJD", "ADJA", "$,", "ADJD", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es kann die Welt an allen, allen Orten,", "tokens": ["Es", "kann", "die", "Welt", "an", "al\u00b7len", ",", "al\u00b7len", "Or\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PIS", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wenn du die Menschen liebst, die Deine sein.", "tokens": ["Wenn", "du", "die", "Men\u00b7schen", "liebst", ",", "die", "Dei\u00b7ne", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Im Tagesgrauen schlief das stille Thal,", "tokens": ["Im", "Ta\u00b7ges\u00b7grau\u00b7en", "schlief", "das", "stil\u00b7le", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und seine Sch\u00f6nheit war mir noch verborgen;", "tokens": ["Und", "sei\u00b7ne", "Sch\u00f6n\u00b7heit", "war", "mir", "noch", "ver\u00b7bor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann pl\u00f6tzlich kam der erste Sonnenstrahl,", "tokens": ["Dann", "pl\u00f6tz\u00b7lich", "kam", "der", "ers\u00b7te", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und mit ihm ward es heller, goldner Morgen.", "tokens": ["Und", "mit", "ihm", "ward", "es", "hel\u00b7ler", ",", "gold\u00b7ner", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "PPER", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es fluthete das Licht vom Himmel nieder,", "tokens": ["Es", "flut\u00b7he\u00b7te", "das", "Licht", "vom", "Him\u00b7mel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als habe er sich selbst herabgesenkt,", "tokens": ["Als", "ha\u00b7be", "er", "sich", "selbst", "her\u00b7ab\u00b7ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und laut erklangen alle Morgenlieder,", "tokens": ["Und", "laut", "er\u00b7klan\u00b7gen", "al\u00b7le", "Mor\u00b7gen\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die er allein, allein dem Walde schenkt.", "tokens": ["Die", "er", "al\u00b7lein", ",", "al\u00b7lein", "dem", "Wal\u00b7de", "schenkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "$,", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Nun ging des Tages Engel \u00fcber Land,", "tokens": ["Nun", "ging", "des", "Ta\u00b7ges", "En\u00b7gel", "\u00fc\u00b7ber", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ging durch den Hag, ging \u00fcber Feld und Auen,", "tokens": ["Ging", "durch", "den", "Hag", ",", "ging", "\u00fc\u00b7ber", "Feld", "und", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und \u00fcberall, wo er ein Bl\u00fcmlein fand,", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", ",", "wo", "er", "ein", "Bl\u00fcm\u00b7lein", "fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bog er sich nieder, um es anzuschauen.", "tokens": ["Bog", "er", "sich", "nie\u00b7der", ",", "um", "es", "an\u00b7zu\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$,", "KOUI", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er kam auf allen Wegen hergeschritten,", "tokens": ["Er", "kam", "auf", "al\u00b7len", "We\u00b7gen", "her\u00b7ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und sah er wo ein wartend Fensterlein,", "tokens": ["Und", "sah", "er", "wo", "ein", "war\u00b7tend", "Fens\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PWAV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So lie\u00df er sich nicht lange darum bitten,", "tokens": ["So", "lie\u00df", "er", "sich", "nicht", "lan\u00b7ge", "da\u00b7rum", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er gab ihm Licht und gab ihm Sonnenschein.", "tokens": ["Er", "gab", "ihm", "Licht", "und", "gab", "ihm", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Er stieg den Berg, den steilen Fels hinan,", "tokens": ["Er", "stieg", "den", "Berg", ",", "den", "stei\u00b7len", "Fels", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Klomm auf die Firnen, in die Kletscherspalten;", "tokens": ["Klomm", "auf", "die", "Fir\u00b7nen", ",", "in", "die", "Klet\u00b7scher\u00b7spal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er kletterte in alle Tiefen dann,", "tokens": ["Er", "klet\u00b7ter\u00b7te", "in", "al\u00b7le", "Tie\u00b7fen", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kam \u00fcber schroffe H\u00e4nge, todte Halden,", "tokens": ["Kam", "\u00fc\u00b7ber", "schrof\u00b7fe", "H\u00e4n\u00b7ge", ",", "tod\u00b7te", "Hal\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und \u00fcberall, am h\u00f6chsten, tiefsten Orte", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", ",", "am", "h\u00f6chs\u00b7ten", ",", "tiefs\u00b7ten", "Or\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ward ihm der Mensch, das Thier, der Baum, der Stein", "tokens": ["Ward", "ihm", "der", "Mensch", ",", "das", "Thier", ",", "der", "Baum", ",", "der", "Stein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zum mahnenden, zum heilgen Gottesworte:", "tokens": ["Zum", "mah\u00b7nen\u00b7den", ",", "zum", "heil\u00b7gen", "Got\u00b7tes\u00b7wor\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbgieb Liebe hier; auch diese Welt ist mein!\u00ab \u2013", "tokens": ["\u00bb", "gieb", "Lie\u00b7be", "hier", ";", "auch", "die\u00b7se", "Welt", "ist", "mein", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVIMP", "NN", "ADV", "$.", "ADV", "PDAT", "NN", "VAFIN", "PPOSAT", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "So liegt des Menschen Her