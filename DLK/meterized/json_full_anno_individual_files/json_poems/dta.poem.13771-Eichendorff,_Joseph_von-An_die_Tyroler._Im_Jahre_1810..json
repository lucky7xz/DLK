{"dta.poem.13771": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Tyroler.  \n Im Jahre 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Hochherzig Volk, Genosse gr\u00f6\u00dfrer Zeiten!", "tokens": ["Hoch\u00b7her\u00b7zig", "Volk", ",", "Ge\u00b7nos\u00b7se", "gr\u00f6\u00df\u00b7rer", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du sinkst nun in der eignen H\u00e4user Brande,", "tokens": ["Du", "sinkst", "nun", "in", "der", "eig\u00b7nen", "H\u00e4u\u00b7ser", "Bran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Himmel noch gestreckt die freien H\u00e4nde.", "tokens": ["Zum", "Him\u00b7mel", "noch", "ge\u00b7streckt", "die", "frei\u00b7en", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}