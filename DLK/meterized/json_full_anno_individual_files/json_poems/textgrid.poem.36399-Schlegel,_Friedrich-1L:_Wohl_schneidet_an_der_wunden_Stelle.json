{"textgrid.poem.36399": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohl schneidet an der wunden Stelle", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl schneidet an der wunden Stelle", "tokens": ["Wohl", "schnei\u00b7det", "an", "der", "wun\u00b7den", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am tiefsten ein des Lebens Schmerz.", "tokens": ["Am", "tiefs\u00b7ten", "ein", "des", "Le\u00b7bens", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ART", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Starrend am innern Tr\u00e4nenquelle", "tokens": ["Star\u00b7rend", "am", "in\u00b7nern", "Tr\u00e4\u00b7nen\u00b7quel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "L\u00f6st sich in Klagen auf das Herz.", "tokens": ["L\u00f6st", "sich", "in", "Kla\u00b7gen", "auf", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gekr\u00e4nkt von Kummer, bittern Sorgen", "tokens": ["Ge\u00b7kr\u00e4nkt", "von", "Kum\u00b7mer", ",", "bit\u00b7tern", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "APPR", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erneut die Last sich jeden Morgen,", "tokens": ["Er\u00b7neut", "die", "Last", "sich", "je\u00b7den", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und dr\u00fcckt die Seele niederw\u00e4rts.", "tokens": ["Und", "dr\u00fcckt", "die", "See\u00b7le", "nie\u00b7der\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da blitzt ein Glanz herab von oben,", "tokens": ["Da", "blitzt", "ein", "Glanz", "her\u00b7ab", "von", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durchleuchtend tief der Seele Grund;", "tokens": ["Durch\u00b7leuch\u00b7tend", "tief", "der", "See\u00b7le", "Grund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Himmel hell hinauf gehoben,", "tokens": ["Zum", "Him\u00b7mel", "hell", "hin\u00b7auf", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird ihr der Morgenstern nun kund.", "tokens": ["Wird", "ihr", "der", "Mor\u00b7gens\u00b7tern", "nun", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.5": {"text": "O k\u00f6nnte wer im Lichte leben,", "tokens": ["O", "k\u00f6nn\u00b7te", "wer", "im", "Lich\u00b7te", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PWS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Kerker w\u00fcrd' er bald entschweben,", "tokens": ["Dem", "Ker\u00b7ker", "w\u00fcrd'", "er", "bald", "ent\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Noch sterbend jubelte sein Mund.", "tokens": ["Noch", "ster\u00b7bend", "ju\u00b7bel\u00b7te", "sein", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Herabgest\u00fcrzt zur Erde wieder,", "tokens": ["Her\u00b7ab\u00b7ge\u00b7st\u00fcrzt", "zur", "Er\u00b7de", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00e4llt neue Qual die Seele an;", "tokens": ["F\u00e4llt", "neu\u00b7e", "Qual", "die", "See\u00b7le", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verfolgung wirft sie w\u00fctend nieder,", "tokens": ["Ver\u00b7fol\u00b7gung", "wirft", "sie", "w\u00fc\u00b7tend", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was nur die H\u00f6ll' ersinnen kann.", "tokens": ["Was", "nur", "die", "H\u00f6ll'", "er\u00b7sin\u00b7nen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gefesselt an dem Schmerzenbette,", "tokens": ["Ge\u00b7fes\u00b7selt", "an", "dem", "Schmer\u00b7zen\u00b7bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erliegt sie solcher Leidenkette;", "tokens": ["Er\u00b7liegt", "sie", "sol\u00b7cher", "Lei\u00b7den\u00b7ket\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es blickt der Feind sie drohend an.", "tokens": ["Es", "blickt", "der", "Feind", "sie", "dro\u00b7hend", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da treten freundlich Lichtgestalten", "tokens": ["Da", "tre\u00b7ten", "freund\u00b7lich", "Licht\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Dunkel trostvoll zu ihr hin.", "tokens": ["Im", "Dun\u00b7kel", "trost\u00b7voll", "zu", "ihr", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Geister liebend um sie walten,", "tokens": ["Wie", "Geis\u00b7ter", "lie\u00b7bend", "um", "sie", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchlt sie nun klar im hellen Sinn.", "tokens": ["F\u00fchlt", "sie", "nun", "klar", "im", "hel\u00b7len", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Auge mitleidsvoll befeuchtet,", "tokens": ["Das", "Au\u00b7ge", "mit\u00b7le\u00b7ids\u00b7voll", "be\u00b7feuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Vom reinsten Lilienglanz umleuchtet,", "tokens": ["Vom", "reins\u00b7ten", "Li\u00b7li\u00b7en\u00b7glanz", "um\u00b7leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Winkt ihr des Himmels K\u00f6nigin.", "tokens": ["Winkt", "ihr", "des", "Him\u00b7mels", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "In Nacht ist schnell der Trost verschwunden,", "tokens": ["In", "Nacht", "ist", "schnell", "der", "Trost", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Angstvoll f\u00fchlt sich die Seel' allein.", "tokens": ["Angst\u00b7voll", "f\u00fchlt", "sich", "die", "Seel'", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Da wird kein Rat noch Licht gefunden,", "tokens": ["Da", "wird", "kein", "Rat", "noch", "Licht", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durchbohrt das innerste Gebein.", "tokens": ["Durch\u00b7bohrt", "das", "in\u00b7ners\u00b7te", "Ge\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Verh\u00f6hnt, zerrissen, ganz zertreten,", "tokens": ["Ver\u00b7h\u00f6hnt", ",", "zer\u00b7ris\u00b7sen", ",", "ganz", "zer\u00b7tre\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Innern ohne Mut zu beten;", "tokens": ["Im", "In\u00b7nern", "oh\u00b7ne", "Mut", "zu", "be\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Soll sie, o Gott, verlassen sein!", "tokens": ["Soll", "sie", ",", "o", "Gott", ",", "ver\u00b7las\u00b7sen", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "FM", "NN", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Noch einmal schl\u00e4gt, ihr Himmelsgeister!", "tokens": ["Noch", "ein\u00b7mal", "schl\u00e4gt", ",", "ihr", "Him\u00b7mels\u00b7geis\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um Rettung sie den Blick hinauf.", "tokens": ["Um", "Ret\u00b7tung", "sie", "den", "Blick", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und vor ihr steht ihr Gott und Meister,", "tokens": ["Und", "vor", "ihr", "steht", "ihr", "Gott", "und", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schlie\u00dft ihr alle Wahrheit auf.", "tokens": ["Und", "schlie\u00dft", "ihr", "al\u00b7le", "Wahr\u00b7heit", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er zeigt ihr blutend seine H\u00e4nde;", "tokens": ["Er", "zeigt", "ihr", "blu\u00b7tend", "sei\u00b7ne", "H\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbdie Wunder, die ich jetzt dir sende,", "tokens": ["\u00bb", "die", "Wun\u00b7der", ",", "die", "ich", "jetzt", "dir", "sen\u00b7de", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Leuchten dir vor im Erdenlauf!\u00ab", "tokens": ["Leuch\u00b7ten", "dir", "vor", "im", "Er\u00b7den\u00b7lauf", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "APPR", "APPRART", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Dann lehrt er selbst das Kreuz sie tragen,", "tokens": ["Dann", "lehrt", "er", "selbst", "das", "Kreuz", "sie", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und allen Feinden gern verzeihn;", "tokens": ["Und", "al\u00b7len", "Fein\u00b7den", "gern", "ver\u00b7zeihn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie darf der Mensch noch bitter klagen,", "tokens": ["Wie", "darf", "der", "Mensch", "noch", "bit\u00b7ter", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Gott im seligsten Verein?", "tokens": ["Mit", "Gott", "im", "se\u00b7ligs\u00b7ten", "Ver\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Des innern Wortes Licht und Segen", "tokens": ["Des", "in\u00b7nern", "Wor\u00b7tes", "Licht", "und", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist wie ein Stab auf dunkeln Wegen,", "tokens": ["Ist", "wie", "ein", "Stab", "auf", "dun\u00b7keln", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und macht der Seele s\u00fc\u00df die Pein.", "tokens": ["Und", "macht", "der", "See\u00b7le", "s\u00fc\u00df", "die", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Er dr\u00fcckt ihr auf die Dornenkrone,", "tokens": ["Er", "dr\u00fcckt", "ihr", "auf", "die", "Dor\u00b7nen\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als seiner Leiden h\u00f6chstes Ziel;", "tokens": ["Als", "sei\u00b7ner", "Lei\u00b7den", "h\u00f6chs\u00b7tes", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die leuchtet jetzt am Gnadenthrone", "tokens": ["Die", "leuch\u00b7tet", "jetzt", "am", "Gna\u00b7dent\u00b7hro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unter der ew'gen Harfe Spiel.", "tokens": ["Un\u00b7ter", "der", "ew'\u00b7gen", "Har\u00b7fe", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Er warnt die Seele vor dem Feinde;", "tokens": ["Er", "warnt", "die", "See\u00b7le", "vor", "dem", "Fein\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APP