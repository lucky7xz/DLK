rd", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Soll sie, o Gott, verlassen sein!", "tokens": ["Soll", "sie", ",", "o", "Gott", ",", "ver\u00b7las\u00b7sen", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "FM", "NN", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Noch einmal schl\u00e4gt, ihr Himmelsgeister!", "tokens": ["Noch", "ein\u00b7mal", "schl\u00e4gt", ",", "ihr", "Him\u00b7mels\u00b7geis\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um Rettung sie den Blick hinauf.", "tokens": ["Um", "Ret\u00b7tung", "sie", "den", "Blick", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und vor ihr steht ihr Gott und Meister,", "tokens": ["Und", "vor", "ihr", "steht", "ihr", "Gott", "und", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schlie\u00dft ihr alle Wahrheit auf.", "tokens": ["Und", "schlie\u00dft", "ihr", "al\u00b7le", "Wahr\u00b7heit", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er zeigt ihr blutend seine H\u00e4nde;", "tokens": ["Er", "zeigt", "ihr", "blu\u00b7tend", "sei\u00b7ne", "H\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbdie Wunder, die ich jetzt dir sende,", "tokens": ["\u00bb", "die", "Wun\u00b7der", ",", "die", "ich", "jetzt", "dir", "sen\u00b7de", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Leuchten dir vor im Erdenlauf!\u00ab", "tokens": ["Leuch\u00b7ten", "dir", "vor", "im", "Er\u00b7den\u00b7lauf", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "APPR", "APPRART", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.16": {"line.1": {"text": "Dann lehrt er selbst das Kreuz sie tragen,", "tokens": ["Dann", "lehrt", "er", "selbst", "das", "Kreuz", "sie", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und allen Feinden gern verzeihn;", "tokens": ["Und", "al\u00b7len", "Fein\u00b7den", "gern", "ver\u00b7zeihn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie darf der Mensch noch bitter klagen,", "tokens": ["Wie", "darf", "der", "Mensch", "noch", "bit\u00b7ter", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Gott im seligsten Verein?", "tokens": ["Mit", "Gott", "im", "se\u00b7ligs\u00b7ten", "Ver\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Des innern Wortes Licht und Segen", "tokens": ["Des", "in\u00b7nern", "Wor\u00b7tes", "Licht", "und", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist wie ein Stab auf dunkeln Wegen,", "tokens": ["Ist", "wie", "ein", "Stab", "auf", "dun\u00b7keln", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und macht der Seele s\u00fc\u00df die Pein.", "tokens": ["Und", "macht", "der", "See\u00b7le", "s\u00fc\u00df", "die", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Er dr\u00fcckt ihr auf die Dornenkrone,", "tokens": ["Er", "dr\u00fcckt", "ihr", "auf", "die", "Dor\u00b7nen\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als seiner Leiden h\u00f6chstes Ziel;", "tokens": ["Als", "sei\u00b7ner", "Lei\u00b7den", "h\u00f6chs\u00b7tes", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die leuchtet jetzt am Gnadenthrone", "tokens": ["Die", "leuch\u00b7tet", "jetzt", "am", "Gna\u00b7dent\u00b7hro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unter der ew'gen Harfe Spiel.", "tokens": ["Un\u00b7ter", "der", "ew'\u00b7gen", "Har\u00b7fe", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Er warnt die Seele vor dem Feinde;", "tokens": ["Er", "warnt", "die", "See\u00b7le", "vor", "dem", "Fein\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So wie ein Freund spricht mit dem Freunde,", "tokens": ["So", "wie", "ein", "Freund", "spricht", "mit", "dem", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sagt ihr geheimer Worte viel.", "tokens": ["Sagt", "ihr", "ge\u00b7hei\u00b7mer", "Wor\u00b7te", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Erstarkt in seinem Liebesschmerzen", "tokens": ["Er\u00b7starkt", "in", "sei\u00b7nem", "Lie\u00b7bes\u00b7schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durchk\u00e4mpft sie duldend ihre Zeit;", "tokens": ["Durch\u00b7k\u00e4mpft", "sie", "dul\u00b7dend", "ih\u00b7re", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er wohnt nun in dem reinen Herzen,", "tokens": ["Er", "wohnt", "nun", "in", "dem", "rei\u00b7nen", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis er sie kr\u00f6nend ganz befreit. \u2013", "tokens": ["Bis", "er", "sie", "kr\u00f6\u00b7nend", "ganz", "be\u00b7freit", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbich werde fortan bei dir bleiben,", "tokens": ["\u00bb", "ich", "wer\u00b7de", "for\u00b7tan", "bei", "dir", "blei\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dir in die Brust ein Zeichen schreiben,", "tokens": ["Dir", "in", "die", "Brust", "ein", "Zei\u00b7chen", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das leuchtet dir in Ewigkeit.\u00ab \u2013", "tokens": ["Das", "leuch\u00b7tet", "dir", "in", "E\u00b7wig\u00b7keit", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}