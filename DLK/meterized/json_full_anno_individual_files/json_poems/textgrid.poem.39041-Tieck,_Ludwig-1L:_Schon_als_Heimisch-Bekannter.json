{"textgrid.poem.39041": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schon als Heimisch-Bekannter", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon als Heimisch-Bekannter", "tokens": ["Schon", "als", "Hei\u00b7mischBe\u00b7kann\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOUS", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gr\u00fc\u00df' ich deine Stein' und H\u00e4user wieder,", "tokens": ["Gr\u00fc\u00df'", "ich", "dei\u00b7ne", "Stein'", "und", "H\u00e4u\u00b7ser", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "KON", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Du Wiege Italischer Kunst,", "tokens": ["Du", "Wie\u00b7ge", "I\u00b7ta\u00b7li\u00b7scher", "Kunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NE", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Du dem Deutschen verwandtes Land.", "tokens": ["Du", "dem", "Deut\u00b7schen", "ver\u00b7wand\u00b7tes", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Schaaren an Schaaren wandeln die armen", "tokens": ["Schaa\u00b7ren", "an", "Schaa\u00b7ren", "wan\u00b7deln", "die", "ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN", "ART", "ADJA"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Gequ\u00e4lten Spanier,", "tokens": ["Ge\u00b7qu\u00e4l\u00b7ten", "Spa\u00b7nier", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und seufzen in der Ferne", "tokens": ["Und", "seuf\u00b7zen", "in", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nach der verrathenen Heimath.", "tokens": ["Nach", "der", "ver\u00b7ra\u00b7the\u00b7nen", "Hei\u00b7math", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Freundlich scheinen sie nicht gestimmt,", "tokens": ["Freund\u00b7lich", "schei\u00b7nen", "sie", "nicht", "ge\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Und aus verzerrtem Verdru\u00df", "tokens": ["Und", "aus", "ver\u00b7zerr\u00b7tem", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Blickt entstellt Castiliens Stolz.", "tokens": ["Blickt", "ent\u00b7stellt", "Cas\u00b7ti\u00b7li\u00b7ens", "Stolz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "NE", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Unfern dem Dome such' ich ein Haus,", "tokens": ["Un\u00b7fern", "dem", "Do\u00b7me", "such'", "ich", "ein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Da schreitet ernst und feierlich", "tokens": ["Da", "schrei\u00b7tet", "ernst", "und", "fei\u00b7er\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit dreiecktem Hut und Tresse,", "tokens": ["Mit", "drei\u00b7eck\u00b7tem", "Hut", "und", "Tres\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Einen langen Degen schleppend,", "tokens": ["Ei\u00b7nen", "lan\u00b7gen", "De\u00b7gen", "schlep\u00b7pend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Mit ellenbreitem Stichblatt,", "tokens": ["Mit", "el\u00b7len\u00b7brei\u00b7tem", "Stich\u00b7blatt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit zinnernen Schnallen, die rund", "tokens": ["Mit", "zin\u00b7ner\u00b7nen", "Schnal\u00b7len", ",", "die", "rund"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Den Schuh und Fu\u00df umgittern,", "tokens": ["Den", "Schuh", "und", "Fu\u00df", "um\u00b7git\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein ernster Mann gemessen auf und ab.", "tokens": ["Ein", "erns\u00b7ter", "Mann", "ge\u00b7mes\u00b7sen", "auf", "und", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ist er ein Pf\u00f6rtner? Ein Castellan?", "tokens": ["Ist", "er", "ein", "Pf\u00f6rt\u00b7ner", "?", "Ein", "Cas\u00b7tel\u00b7lan", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.10": {"text": "Wo find' ich, red' ich ihn freundlich an,", "tokens": ["Wo", "find'", "ich", ",", "red'", "ich", "ihn", "freund\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Wohl den und den Pallast? \u2013", "tokens": ["Wohl", "den", "und", "den", "Pal\u00b7last", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "KON", "ART", "NN", "$.", "$("], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.12": {"text": "Pr\u00fcfenden Blicks betrachtet er mich,", "tokens": ["Pr\u00fc\u00b7fen\u00b7den", "Blicks", "be\u00b7trach\u00b7tet", "er", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Wirft das Haupt zur\u00fcck", "tokens": ["Wirft", "das", "Haupt", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKVZ"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "Und stemmt die beiden H\u00e4nde auf seine H\u00fcften;", "tokens": ["Und", "stemmt", "die", "bei\u00b7den", "H\u00e4n\u00b7de", "auf", "sei\u00b7ne", "H\u00fcf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Nach langer feierlicher Pause", "tokens": ["Nach", "lan\u00b7ger", "fei\u00b7er\u00b7li\u00b7cher", "Pau\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Beginnt er im schlechten Itali\u00e4nisch:", "tokens": ["Be\u00b7ginnt", "er", "im", "schlech\u00b7ten", "I\u00b7ta\u00b7li\u00b7\u00e4\u00b7nisch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.17": {"text": "Ist es mir, als Castilianer,", "tokens": ["Ist", "es", "mir", ",", "als", "Cas\u00b7ti\u00b7li\u00b7a\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "KOUS", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Nicht Strafe des Himmels f\u00fcr S\u00fcnden genug,", "tokens": ["Nicht", "Stra\u00b7fe", "des", "Him\u00b7mels", "f\u00fcr", "S\u00fcn\u00b7den", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ART", "NN", "APPR", "NN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.19": {"text": "Da\u00df ich im verfluchten Lande", "tokens": ["Da\u00df", "ich", "im", "ver\u00b7fluch\u00b7ten", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "In der noch verfluchteren Stadt", "tokens": ["In", "der", "noch", "ver\u00b7fluch\u00b7te\u00b7ren", "Stadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Hier auf dem allerverfluchtesten Platz mu\u00df Wache stehn?", "tokens": ["Hier", "auf", "dem", "al\u00b7ler\u00b7ver\u00b7fluch\u00b7tes\u00b7ten", "Platz", "mu\u00df", "Wa\u00b7che", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Ihr verlangt auch noch, ich soll", "tokens": ["Ihr", "ver\u00b7langt", "auch", "noch", ",", "ich", "soll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PPER", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Euren mehr als allerverfluchtesten Pallast kennen?", "tokens": ["Eu\u00b7ren", "mehr", "als", "al\u00b7ler\u00b7ver\u00b7fluch\u00b7tes\u00b7ten", "Pal\u00b7last", "ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIAT", "KOKOM", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+--+--+---", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Da sah ich meinen Irrthum,", "tokens": ["Da", "sah", "ich", "mei\u00b7nen", "Irr\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und bat den Hochergrimmten", "tokens": ["Und", "bat", "den", "Ho\u00b7cher\u00b7grimm\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In spanischer edelster Mundart", "tokens": ["In", "spa\u00b7ni\u00b7scher", "e\u00b7dels\u00b7ter", "Mund\u00b7art"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er m\u00f6ge mir verzeihn.", "tokens": ["Er", "m\u00f6\u00b7ge", "mir", "ver\u00b7zeihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wie ich ihn \u00f6fter Usted genannt", "tokens": ["Und", "wie", "ich", "ihn", "\u00f6f\u00b7ter", "Us\u00b7ted", "ge\u00b7nannt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PPER", "ADV", "NE", "VVPP"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und Castilien und das Volk gepriesen,", "tokens": ["Und", "Cas\u00b7ti\u00b7li\u00b7en", "und", "das", "Volk", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ward der Alte freundlich", "tokens": ["Ward", "der", "Al\u00b7te", "freund\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Und klagte in Menschent\u00f6nen", "tokens": ["Und", "klag\u00b7te", "in", "Men\u00b7schen\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Sein Leid und Ungemach.", "tokens": ["Sein", "Leid", "und", "Un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Am Abend sahn wir uns wieder,", "tokens": ["Am", "A\u00b7bend", "sahn", "wir", "uns", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Und tranken im guten Florentiner", "tokens": ["Und", "tran\u00b7ken", "im", "gu\u00b7ten", "Flo\u00b7ren\u00b7ti\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Unsrer neuen Freundschaft.", "tokens": ["Uns\u00b7rer", "neu\u00b7en", "Freund\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Schon als Heimisch-Bekannter", "tokens": ["Schon", "als", "Hei\u00b7mischBe\u00b7kann\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOUS", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gr\u00fc\u00df' ich deine Stein' und H\u00e4user wieder,", "tokens": ["Gr\u00fc\u00df'", "ich", "dei\u00b7ne", "Stein'", "und", "H\u00e4u\u00b7ser", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "KON", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Du Wiege Italischer Kunst,", "tokens": ["Du", "Wie\u00b7ge", "I\u00b7ta\u00b7li\u00b7scher", "Kunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NE", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Du dem Deutschen verwandtes Land.", "tokens": ["Du", "dem", "Deut\u00b7schen", "ver\u00b7wand\u00b7tes", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Schaaren an Schaaren wandeln die armen", "tokens": ["Schaa\u00b7ren", "an", "Schaa\u00b7ren", "wan\u00b7deln", "die", "ar\u00b7men"], "token_info": ["word", "word", "word", "word", 