{"dta.poem.23784": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Das Vogelnest.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Da hab ich es, das H\u00e4nflingsnest!", "tokens": ["Da", "hab", "ich", "es", ",", "das", "H\u00e4nf\u00b7lings\u00b7nest", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun ist mirs endlich doch gelungen:", "tokens": ["Nun", "ist", "mirs", "end\u00b7lich", "doch", "ge\u00b7lun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das ganze Nest und mit vier Jungen: \u2014", "tokens": ["Das", "gan\u00b7ze", "Nest", "und", "mit", "vier", "Jun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "APPR", "CARD", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ja str\u00e4ubt Euch nur, ich halt Euch fest.", "tokens": ["Ja", "str\u00e4ubt", "Euch", "nur", ",", "ich", "halt", "Euch", "fest", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch wie barmt nicht der Aeltern", "tokens": ["Doch", "wie", "barmt", "nicht", "der", "A\u00b7el\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Paar!", "tokens": ["Paar", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Soll oder soll ich sie nicht nehmen?", "tokens": ["Soll", "o\u00b7der", "soll", "ich", "sie", "nicht", "neh\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KON", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nein, nein, de\u00df m\u00fc\u00dft ich mich ja sch\u00e4men,", "tokens": ["Nein", ",", "nein", ",", "de\u00df", "m\u00fc\u00dft", "ich", "mich", "ja", "sch\u00e4\u00b7men", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PDS", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich handelte, wie ein Barbar.", "tokens": ["Ich", "han\u00b7del\u00b7te", ",", "wie", "ein", "Bar\u00b7bar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie oft hat mich nicht ihr Gesang,", "tokens": ["Wie", "oft", "hat", "mich", "nicht", "ihr", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Lag ich im Grase dort gestrecket,", "tokens": ["Lag", "ich", "im", "Gra\u00b7se", "dort", "ge\u00b7stre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu Harmonie und Lust erwecket,", "tokens": ["Zu", "Har\u00b7mo\u00b7nie", "und", "Lust", "er\u00b7we\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und die\u00df w\u00e4r nun der ganze Dank?", "tokens": ["Und", "die\u00df", "w\u00e4r", "nun", "der", "gan\u00b7ze", "Dank", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich ri\u00df ihr armes H\u00e4uschen ab,", "tokens": ["Ich", "ri\u00df", "ihr", "ar\u00b7mes", "H\u00e4usc\u00b7hen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sie nach Gastrecht mir vertrauet,", "tokens": ["Das", "sie", "nach", "Gast\u00b7recht", "mir", "ver\u00b7trau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von Moos und Stroh sich selbst erbauet,", "tokens": ["Von", "Moos", "und", "Stroh", "sich", "selbst", "er\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu dem ich nicht ein H\u00e4lmchen gab.", "tokens": ["Zu", "dem", "ich", "nicht", "ein", "H\u00e4lm\u00b7chen", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn eine r\u00e4uberische Hand", "tokens": ["Wenn", "ei\u00b7ne", "r\u00e4u\u00b7be\u00b7ri\u00b7sche", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich meinen Aeltern nun entrissen?", "tokens": ["Mich", "mei\u00b7nen", "A\u00b7el\u00b7tern", "nun", "ent\u00b7ris\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was w\u00fcrden da f\u00fcr Thr\u00e4nen flie\u00dfen,", "tokens": ["Was", "w\u00fcr\u00b7den", "da", "f\u00fcr", "Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie jammervoll w\u00e4r unser Stand!", "tokens": ["Wie", "jam\u00b7mer\u00b7voll", "w\u00e4r", "un\u00b7ser", "Stand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nein, liebe S\u00e4nger, bleibt in Ruh,", "tokens": ["Nein", ",", "lie\u00b7be", "S\u00e4n\u00b7ger", ",", "bleibt", "in", "Ruh", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier habt Jhr Eure Kinder wieder:", "tokens": ["Hier", "habt", "Ihr", "Eu\u00b7re", "Kin\u00b7der", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vervielfacht singt Jhr Eure Lieder,", "tokens": ["Ver\u00b7viel\u00b7facht", "singt", "Ihr", "Eu\u00b7re", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir dann aufs n\u00e4chste Fr\u00fchjahr zu.", "tokens": ["Mir", "dann", "aufs", "n\u00e4chs\u00b7te", "Fr\u00fch\u00b7jahr", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}