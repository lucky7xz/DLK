{"dta.poem.5940": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sein schw\u00e4rmend ", "tokens": ["Sein", "schw\u00e4r\u00b7mend"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Voll Fruchtbarkeit, lie\u00df Himmel, Sternen, Erden,", "tokens": ["Voll", "Frucht\u00b7bar\u00b7keit", ",", "lie\u00df", "Him\u00b7mel", ",", "Ster\u00b7nen", ",", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "NN", "$,", "VVFIN", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Blo\u00df durch ein ", "tokens": ["Blo\u00df", "durch", "ein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "An allen Orten werden.", "tokens": ["An", "al\u00b7len", "Or\u00b7ten", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sein schimmernd Lehr-Geb\u00e4u, das sich aufs ", "tokens": ["Sein", "schim\u00b7mernd", "Lehr\u00b7Ge\u00b7b\u00e4u", ",", "das", "sich", "aufs"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NE", "$,", "PRELS", "PRF", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "H\u00e4lt das f\u00fcr seinen Grund, sammt C\u00f6rpern, welche fest.", "tokens": ["H\u00e4lt", "das", "f\u00fcr", "sei\u00b7nen", "Grund", ",", "sammt", "C\u00f6r\u00b7pern", ",", "wel\u00b7che", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "NE", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja, \u00fcber dem, was leer in allen C\u00f6rpern w\u00e4re,", "tokens": ["Ja", ",", "\u00fc\u00b7ber", "dem", ",", "was", "leer", "in", "al\u00b7len", "C\u00f6r\u00b7pern", "w\u00e4\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ART", "$,", "PRELS", "ADJD", "APPR", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Setzt er annoch ein gantz unendlichs Leere,", "tokens": ["Setzt", "er", "an\u00b7noch", "ein", "gantz", "un\u00b7end\u00b7lichs", "Lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Das der Materie den Durchgang l\u00e4sst,", "tokens": ["Das", "der", "Ma\u00b7te\u00b7rie", "den", "Durch\u00b7gang", "l\u00e4sst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Weil, seiner Meinung nach, im Vollen nichts sich regen,", "tokens": ["Weil", ",", "sei\u00b7ner", "Mei\u00b7nung", "nach", ",", "im", "Vol\u00b7len", "nichts", "sich", "re\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PPOSAT", "NN", "APPR", "$,", "APPRART", "NN", "PIS", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und die Materie nicht k\u00f6nne drinn bewegen.", "tokens": ["Und", "die", "Ma\u00b7te\u00b7rie", "nicht", "k\u00f6n\u00b7ne", "drinn", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}