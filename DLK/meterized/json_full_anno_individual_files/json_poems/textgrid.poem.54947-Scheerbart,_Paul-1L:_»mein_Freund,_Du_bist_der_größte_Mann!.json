{"textgrid.poem.54947": {"metadata": {"author": {"name": "Scheerbart, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbmein Freund, Du bist der gr\u00f6\u00dfte Mann!", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmein Freund, Du bist der gr\u00f6\u00dfte Mann!", "tokens": ["\u00bb", "mein", "Freund", ",", "Du", "bist", "der", "gr\u00f6\u00df\u00b7te", "Mann", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es zweifelt keine Seele dran!", "tokens": ["Es", "zwei\u00b7felt", "kei\u00b7ne", "See\u00b7le", "dran", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lese jedes Wort von Dir.", "tokens": ["Ich", "le\u00b7se", "je\u00b7des", "Wort", "von", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Andern liefern nur Geschmier.", "tokens": ["Die", "An\u00b7dern", "lie\u00b7fern", "nur", "Ge\u00b7schmier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist der Einz'ge, der was kann!", "tokens": ["Du", "bist", "der", "Einz'\u00b7ge", ",", "der", "was", "kann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PWS", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O glaub's, Du bist der gr\u00f6\u00dfte Mann!", "tokens": ["O", "glaub's", ",", "Du", "bist", "der", "gr\u00f6\u00df\u00b7te", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was Andre reden, ist nur Quatsch.", "tokens": ["Was", "And\u00b7re", "re\u00b7den", ",", "ist", "nur", "Quatsch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVINF", "$,", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Drum reich mir freundlich Deine Patsch!", "tokens": ["Drum", "reich", "mir", "freund\u00b7lich", "Dei\u00b7ne", "Patsch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wir gr\u00fcnden einen M\u00e4nnerbund", "tokens": ["Wir", "gr\u00fcn\u00b7den", "ei\u00b7nen", "M\u00e4n\u00b7ner\u00b7bund"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und hauen los auf jeden Schund!", "tokens": ["Und", "hau\u00b7en", "los", "auf", "je\u00b7den", "Schund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Damit man endlich doch mal seh,", "tokens": ["Da\u00b7mit", "man", "end\u00b7lich", "doch", "mal", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Worin die wahre Kunst besteh!", "tokens": ["Wo\u00b7rin", "die", "wah\u00b7re", "Kunst", "be\u00b7steh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und will einmal ein Schweinehund", "tokens": ["Und", "will", "ein\u00b7mal", "ein", "Schwei\u00b7ne\u00b7hund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Verh\u00f6hnen unsern M\u00e4nnerbund,", "tokens": ["Ver\u00b7h\u00f6h\u00b7nen", "un\u00b7sern", "M\u00e4n\u00b7ner\u00b7bund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "So kommen wir mit Kn\u00fcppeln an", "tokens": ["So", "kom\u00b7men", "wir", "mit", "Kn\u00fcp\u00b7peln", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und zeigen, was ein Mann noch kann.", "tokens": ["Und", "zei\u00b7gen", ",", "was", "ein", "Mann", "noch", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PRELS", "ART", "NN", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Vor uns mu\u00df Jeder tief sich b\u00fccken", "tokens": ["Vor", "uns", "mu\u00df", "Je\u00b7der", "tief", "sich", "b\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "PIS", "ADJD", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und dabei weg sein vor Entz\u00fccken!\u00ab", "tokens": ["Und", "da\u00b7bei", "weg", "sein", "vor", "Ent\u00b7z\u00fc\u00b7cken", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PAV", "ADV", "PPOSAT", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "So sang voll Hohn ein B\u00f6sewicht", "tokens": ["So", "sang", "voll", "Hohn", "ein", "B\u00f6\u00b7se\u00b7wicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Dem Freunde S\u00fc\u00dfes ins Gesicht.", "tokens": ["Dem", "Freun\u00b7de", "S\u00fc\u00b7\u00dfes", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und dieser Gute merkte nicht,", "tokens": ["Und", "die\u00b7ser", "Gu\u00b7te", "merk\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Wie leicht das S\u00fc\u00dfe an Gewicht.", "tokens": ["Wie", "leicht", "das", "S\u00fc\u00b7\u00dfe", "an", "Ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "\u00bbder gr\u00f6\u00dfte Mann\u00ab, rief er voll Stolz,", "tokens": ["\u00bb", "der", "gr\u00f6\u00df\u00b7te", "Mann", "\u00ab", ",", "rief", "er", "voll", "Stolz", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$(", "$,", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "\u00bbder sei jetzt l\u00e4nger nicht von Holz!\u00ab", "tokens": ["\u00bb", "der", "sei", "jetzt", "l\u00e4n\u00b7ger", "nicht", "von", "Holz", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "VAFIN", "ADV", "ADJD", "PTKNEG", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und er begann vergn\u00fcgt zu zechen", "tokens": ["Und", "er", "be\u00b7gann", "ver\u00b7gn\u00fcgt", "zu", "ze\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "VVPP", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Und mu\u00dfte schrecklich dabei blechen.", "tokens": ["Und", "mu\u00df\u00b7te", "schreck\u00b7lich", "da\u00b7bei", "ble\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Der B\u00f6sewicht, der freut sich drob,", "tokens": ["Der", "B\u00f6\u00b7se\u00b7wicht", ",", "der", "freut", "sich", "drob", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Er wird beim zw\u00f6lften Glase grob.", "tokens": ["Er", "wird", "beim", "zw\u00f6lf\u00b7ten", "Gla\u00b7se", "grob", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Jedoch der gr\u00f6\u00dfte Mann vergi\u00dft,", "tokens": ["Je\u00b7doch", "der", "gr\u00f6\u00df\u00b7te", "Mann", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Da\u00df ihm sein Freund oft l\u00e4stig ist.", "tokens": ["Da\u00df", "ihm", "sein", "Freund", "oft", "l\u00e4s\u00b7tig", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Er freut sich seines gro\u00dfen Ruhms,", "tokens": ["Er", "freut", "sich", "sei\u00b7nes", "gro\u00b7\u00dfen", "Ruhms", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Gedenkt nicht seines Eigentums.", "tokens": ["Ge\u00b7denkt", "nicht", "sei\u00b7nes", "Ei\u00b7gen\u00b7tums", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Bald ist sein Hab und Gut verschwendet.", "tokens": ["Bald", "ist", "sein", "Hab", "und", "Gut", "ver\u00b7schwen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Der B\u00f6sewicht sich von ihm wendet.", "tokens": ["Der", "B\u00f6\u00b7se\u00b7wicht", "sich", "von", "ihm", "wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Denn gro\u00dfe M\u00e4nner ohne Geld", "tokens": ["Denn", "gro\u00b7\u00dfe", "M\u00e4n\u00b7ner", "oh\u00b7ne", "Geld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Sind doch das Schlimmste in der Welt.", "tokens": ["Sind", "doch", "das", "Schlimms\u00b7te", "in", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "So geht's dem Dummen, der gem\u00fctlich", "tokens": ["So", "geht's", "dem", "Dum\u00b7men", ",", "der", "ge\u00b7m\u00fct\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Des Freundes Lob h\u00e4lt f\u00fcr sehr g\u00fctlich!", "tokens": ["Des", "Freun\u00b7des", "Lob", "h\u00e4lt", "f\u00fcr", "sehr", "g\u00fct\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Der Schmeichler ist ein B\u00f6sewicht \u2013", "tokens": ["Der", "Schmeich\u00b7ler", "ist", "ein", "B\u00f6\u00b7se\u00b7wicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Oh, kluger Mensch, vergi\u00df das nicht!", "tokens": ["Oh", ",", "klu\u00b7ger", "Mensch", ",", "ver\u00b7gi\u00df", "das", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "VVIMP", "PDS", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Auch arme Menschen sollen l\u00e4cheln,", "tokens": ["Auch", "ar\u00b7me", "Men\u00b7schen", "sol\u00b7len", "l\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VMFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Wenn sie ein Schmeichler will umf\u00e4cheln.", "tokens": ["Wenn", "sie", "ein", "Schmeich\u00b7ler", "will", "um\u00b7f\u00e4\u00b7cheln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Verrate deine Gr\u00f6\u00dfe nie!", "tokens": ["Ver\u00b7ra\u00b7te", "dei\u00b7ne", "Gr\u00f6\u00b7\u00dfe", "nie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Sei nur ein heimliches Genie!", "tokens": ["Sei", "nur", "ein", "heim\u00b7li\u00b7ches", "Ge\u00b7nie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbmein Freund, Du bist der gr\u00f6\u00dfte Mann!", "tokens": ["\u00bb", "mein", "Freund", ",", "Du", "bist", "der", "gr\u00f6\u00df\u00b7te", "Mann", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es zweifelt keine Seele dran!", "tokens": ["Es", "zwei\u00b7felt", "kei\u00b7ne", "See\u00b7le", "dran", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lese jedes Wort von Dir.", "tokens": ["Ich", "le\u00b7se", "je\u00b7des", "Wort", "von", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Andern liefern nur Geschmier.", "tokens": ["Die", "An\u00b7dern", "lie\u00b7fern", "nur", "Ge\u00b7schmier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist der Einz'ge, der was kann!", "tokens": ["Du", "bist", "der", "Einz'\u00b7ge", ",", "der", "was", "kann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PWS", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O glaub's, Du bist der gr\u00f6\u00dfte Mann!", "tokens": ["O", "glaub's", ",", "Du", "bist", "der", "gr\u00f6\u00df\u00b7te", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was Andre reden, ist nur Quatsch.", "tokens": ["Was", "And\u00b7re", "re\u00b7den", ",", "ist", "nur", "Quatsch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVINF", "$,", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Drum reich mir freundlich Deine Patsch!", "tokens": ["Drum", "reich", "mir", "freund\u00b7lich", "Dei\u00b7ne", "Patsch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wir gr\u00fcnden einen M\u00e4nnerbund", "tokens": ["Wir", "gr\u00fcn\u00b7den", "ei\u00b7nen", "M\u00e4n\u00b7ner\u00b7bund"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und hauen los auf jeden Schund!", "tokens": ["Und", "hau\u00b7en", "los", "auf", "je\u00b7den", "Schund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Damit man endlich doch mal seh,", "tokens": ["Da\u00b7mit", "man", "end\u00b7lich", "doch", "mal", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Worin die wahre Kunst besteh!", "tokens": ["Wo\u00b7rin", "die", "wah\u00b7re", "Kunst", "be\u00b7steh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und will einmal ein Schweinehund", "tokens": ["Und", "will", "ein\u00b7mal", "ein", "Schwei\u00b7ne\u00b7hund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Verh\u00f6hnen unsern M\u00e4nnerbund,", "tokens": ["Ver\u00b7h\u00f6h\u00b7nen", "un\u00b7sern", "M\u00e4n\u00b7ner\u00b7bund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "So kommen wir mit Kn\u00fcppeln an", "tokens": ["So", "kom\u00b7men", "wir", "mit", "Kn\u00fcp\u00b7peln", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und zeigen, was ein Mann noch kann.", "tokens": ["Und", "zei\u00b7gen", ",", "was", "ein", "Mann", "noch", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PRELS", "ART", "NN", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Vor uns mu\u00df Jeder tief sich b\u00fccken", "tokens": ["Vor", "uns", "mu\u00df", "Je\u00b7der", "tief", "sich", "b\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "PIS", "ADJD", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und dabei weg sein vor Entz\u00fccken!\u00ab", "tokens": ["Und", "da\u00b7bei", "weg", "sein", "vor", "Ent\u00b7z\u00fc\u00b7cken", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PAV", "ADV", "PPOSAT", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "So sang voll Hohn ein B\u00f6sewicht", "tokens": ["So", "sang", "voll", "Hohn", "ein", "B\u00f6\u00b7se\u00b7wicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Dem Freunde S\u00fc\u00dfes ins Gesicht.", "tokens": ["Dem", "Freun\u00b7de", "S\u00fc\u00b7\u00dfes", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und dieser Gute merkte nicht,", "tokens": ["Und", "die\u00b7ser", "Gu\u00b7te", "merk\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Wie leicht das S\u00fc\u00dfe an Gewicht.", "tokens": ["Wie", "leicht", "das", "S\u00fc\u00b7\u00dfe", "an", "Ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "\u00bbder gr\u00f6\u00dfte Mann\u00ab, rief er voll Stolz,", "tokens": ["\u00bb", "der", "gr\u00f6\u00df\u00b7te", "Mann", "\u00ab", ",", "rief", "er", "voll", "Stolz", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$(", "$,", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "\u00bbder sei jetzt l\u00e4nger nicht von Holz!\u00ab", "tokens": ["\u00bb", "der", "sei", "jetzt", "l\u00e4n\u00b7ger", "nicht", "von", "Holz", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "VAFIN", "ADV", "ADJD", "PTKNEG", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und er begann vergn\u00fcgt zu zechen", "tokens": ["Und", "er", "be\u00b7gann", "ver\u00b7gn\u00fcgt", "zu", "ze\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "VVPP", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Und mu\u00dfte schrecklich dabei blechen.", "tokens": ["Und", "mu\u00df\u00b7te", "schreck\u00b7lich", "da\u00b7bei", "ble\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Der B\u00f6sewicht, der freut sich drob,", "tokens": ["Der", "B\u00f6\u00b7se\u00b7wicht", ",", "der", "freut", "sich", "drob", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2