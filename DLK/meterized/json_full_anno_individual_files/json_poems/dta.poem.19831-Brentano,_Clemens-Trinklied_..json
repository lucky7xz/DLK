{"dta.poem.19831": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sagt wohl in dem Mayen,               ", "tokens": ["Man", "sagt", "wohl", "in", "dem", "Ma\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da sind die Br\u00fcnnlein gesund,", "tokens": ["Da", "sind", "die", "Br\u00fcnn\u00b7lein", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich glaubs nicht meiner Treuen,", "tokens": ["Ich", "glaubs", "nicht", "mei\u00b7ner", "Treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es schwenkt eim nur den Mund,", "tokens": ["Es", "schwenkt", "eim", "nur", "den", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und thut im Magen schweben,", "tokens": ["Und", "thut", "im", "Ma\u00b7gen", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Drum will mirs auch nicht ein,", "tokens": ["Drum", "will", "mirs", "auch", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "NE", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich lob die edlen Reben,", "tokens": ["Ich", "lob", "die", "ed\u00b7len", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die bringen uns gut Wein.", "tokens": ["Die", "brin\u00b7gen", "uns", "gut", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo Heu w\u00e4chst auf der Matten,", "tokens": ["Wo", "Heu", "w\u00e4chst", "auf", "der", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem frag ich gar nichts nach,", "tokens": ["Dem", "frag", "ich", "gar", "nichts", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es hab Sonn oder Schatten,", "tokens": ["Es", "hab", "Sonn", "o\u00b7der", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ist mir geringe Sach.", "tokens": ["Ist", "mir", "ge\u00b7rin\u00b7ge", "Sach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gut Heu, das w\u00e4chst an Reben,", "tokens": ["Gut", "Heu", ",", "das", "w\u00e4chst", "an", "Re\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PDS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dasselbig wolln wir han,", "tokens": ["Das\u00b7sel\u00b7big", "wolln", "wir", "han", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gut Streu thut es auch geben,", "tokens": ["Gut", "Streu", "thut", "es", "auch", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Das wei\u00df wohl Weib und Mann.", "tokens": ["Das", "wei\u00df", "wohl", "Weib", "und", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wer es nicht kann kauen,", "tokens": ["Und", "wer", "es", "nicht", "kann", "kau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der geh auch nicht zum Wein,", "tokens": ["Der", "geh", "auch", "nicht", "zum", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch seh ich an dem Hauen", "tokens": ["Doch", "seh", "ich", "an", "dem", "Hau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df wir gut M\u00e4her sein:", "tokens": ["Da\u00df", "wir", "gut", "M\u00e4\u00b7her", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir rechens mit den Z\u00e4hnen,", "tokens": ["Wir", "re\u00b7chens", "mit", "den", "Z\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und worflens mit dem Glas,", "tokens": ["Und", "worf\u00b7lens", "mit", "dem", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Magen mu\u00df sich dehnen,", "tokens": ["Der", "Ma\u00b7gen", "mu\u00df", "sich", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "PDS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df ers in Scheuer la\u00df.", "tokens": ["Da\u00df", "ers", "in", "Scheu\u00b7er", "la\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wir han gar kleine Sorgen", "tokens": ["Wir", "han", "gar", "klei\u00b7ne", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl um das r\u00f6misch Reich,", "tokens": ["Wohl", "um", "das", "r\u00f6\u00b7misch", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sterb heut oder morgen,", "tokens": ["Es", "sterb", "heut", "o\u00b7der", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das gilt uns alles gleich;", "tokens": ["Das", "gilt", "uns", "al\u00b7les", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und gieng es auch in St\u00fccke,", "tokens": ["Und", "gieng", "es", "auch", "in", "St\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn nur das Heu ger\u00e4th,", "tokens": ["Wenn", "nur", "das", "Heu", "ge\u00b7r\u00e4\u00b7th", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Daraus drehn wir ein Stricke", "tokens": ["Da\u00b7raus", "drehn", "wir", "ein", "Stri\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Der es zusammen n\u00e4ht.", "tokens": ["Der", "es", "zu\u00b7sam\u00b7men", "n\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Specksupp ist gerathen,", "tokens": ["Die", "Speck\u00b7supp", "ist", "ge\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Schlaftrunk bringt uns her,", "tokens": ["Den", "Schlaf\u00b7trunk", "bringt", "uns", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist noch ein Weck am Laden,", "tokens": ["Ist", "noch", "ein", "Weck", "am", "La\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er ist nit sicher mehr,", "tokens": ["Er", "ist", "nit", "si\u00b7cher", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Kaiser steckt zum Spiese,", "tokens": ["Ein", "Kai\u00b7ser", "steckt", "zum", "Spie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein K\u00fcnglein in Pastet,", "tokens": ["Ein", "K\u00fcn\u00b7glein", "in", "Pas\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Arm Ritter macht recht s\u00fc\u00dfe,", "tokens": ["Arm", "Rit\u00b7ter", "macht", "recht", "s\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bis da\u00df der Hahn gekr\u00e4ht.", "tokens": ["Bis", "da\u00df", "der", "Hahn", "ge\u00b7kr\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das Liedlein will sich enden,", "tokens": ["Das", "Lied\u00b7lein", "will", "sich", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo ist daheime nu?", "tokens": ["Wo", "ist", "da\u00b7hei\u00b7me", "nu", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tapt hin nur an den W\u00e4nden,", "tokens": ["Tapt", "hin", "nur", "an", "den", "W\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Und legt das Heu zur Ruh,", "tokens": ["Und", "legt", "das", "Heu", "zur", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Wagen schwankt hereine,", "tokens": ["Der", "Wa\u00b7gen", "schwankt", "her\u00b7ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie han geladen schwer,", "tokens": ["Sie", "han", "ge\u00b7la\u00b7den", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er br\u00e4ch, wenn nicht am Rheine", "tokens": ["Er", "br\u00e4ch", ",", "wenn", "nicht", "am", "Rhei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PTKNEG", "APPRART", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Strick gewachsen w\u00e4r.", "tokens": ["Der", "Strick", "ge\u00b7wach\u00b7sen", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich bind mein Schwerdt zur Seiten,", "tokens": ["Ich", "bind", "mein", "Schwerdt", "zur", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und mach mich bald davon,", "tokens": ["Und", "mach", "mich", "bald", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hab ich dann nit zu reiten,", "tokens": ["Hab", "ich", "dann", "nit", "zu", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Fu\u00dfe mu\u00df ich gon,", "tokens": ["Zu", "Fu\u00b7\u00dfe", "mu\u00df", "ich", "gon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich taumle als ein G\u00e4nselein,", "tokens": ["Ich", "taum\u00b7le", "als", "ein", "G\u00e4n\u00b7sel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das ziehet auf die Wacht,", "tokens": ["Das", "zie\u00b7het", "auf", "die", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das thut das Heu und auch der Wein,", "tokens": ["Das", "thut", "das", "Heu", "und", "auch", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ade zur guten Nacht.", "tokens": ["A\u00b7de", "zur", "gu\u00b7ten", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}