{"textgrid.poem.57457": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Geburtstag eines Mannes, im Namen seiner Ehegattinn", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm hin die\u00df h\u00f6chsterfreute Blatt,", "tokens": ["Nimm", "hin", "die\u00df", "h\u00f6chs\u00b7ter\u00b7freu\u00b7te", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geliebter Schatz, von deren H\u00e4nden,", "tokens": ["Ge\u00b7lieb\u00b7ter", "Schatz", ",", "von", "de\u00b7ren", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die sich vorl\u00e4ngst entschlossen hat,", "tokens": ["Die", "sich", "vor\u00b7l\u00e4ngst", "ent\u00b7schlos\u00b7sen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "VVPP", "VAFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dir Herz und Seele zu verpf\u00e4nden.", "tokens": ["Dir", "Herz", "und", "See\u00b7le", "zu", "ver\u00b7pf\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nimm hin das Zeichen wahrer Treu,", "tokens": ["Nimm", "hin", "das", "Zei\u00b7chen", "wah\u00b7rer", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das zarte Liebe dir geweihet,", "tokens": ["Das", "zar\u00b7te", "Lie\u00b7be", "dir", "ge\u00b7wei\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und glaube, da\u00df mein Sinn dabey", "tokens": ["Und", "glau\u00b7be", ",", "da\u00df", "mein", "Sinn", "da\u00b7bey"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich \u00fcber deine Wohlfahrt freuet.", "tokens": ["Sich", "\u00fc\u00b7ber", "dei\u00b7ne", "Wohl\u00b7fahrt", "freu\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dein froher Jahrstag stellt sich ein;", "tokens": ["Dein", "fro\u00b7her", "Jahr\u00b7stag", "stellt", "sich", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und was kann mir vergn\u00fcgter fallen,", "tokens": ["Und", "was", "kann", "mir", "ver\u00b7gn\u00fcg\u00b7ter", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Als wenn nach \u00fcberstandner Pein", "tokens": ["Als", "wenn", "nach", "\u00fc\u00b7bers\u00b7tand\u00b7ner", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mir Blut und Adern freudig wallen?", "tokens": ["Mir", "Blut", "und", "A\u00b7dern", "freu\u00b7dig", "wal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die Traurigkeit hat dieses Jahr", "tokens": ["Die", "Trau\u00b7rig\u00b7keit", "hat", "die\u00b7ses", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Um meines Vaters Gruft geweinet;", "tokens": ["Um", "mei\u00b7nes", "Va\u00b7ters", "Gruft", "ge\u00b7wei\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Nun stellt sich auch die Freude dar,", "tokens": ["Nun", "stellt", "sich", "auch", "die", "Freu\u00b7de", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Da dein erw\u00fcnschtes Fest erscheinet.", "tokens": ["Da", "dein", "er\u00b7w\u00fcnschtes", "Fest", "er\u00b7schei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Mein nasses Auge, stille dich,", "tokens": ["Mein", "nas\u00b7ses", "Au\u00b7ge", ",", "stil\u00b7le", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sey bem\u00fcht, dich aufzukl\u00e4ren;", "tokens": ["Und", "sey", "be\u00b7m\u00fcht", ",", "dich", "auf\u00b7zu\u00b7kl\u00e4\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$,", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Thr\u00e4nenbrunn verstopfe sich;", "tokens": ["Der", "Thr\u00e4\u00b7nen\u00b7brunn", "ver\u00b7stop\u00b7fe", "sich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was soll das Aechzen l\u00e4nger w\u00e4hren?", "tokens": ["Was", "soll", "das", "A\u00b7ech\u00b7zen", "l\u00e4n\u00b7ger", "w\u00e4h\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Leichen k\u00f6nnen von der Fluth", "tokens": ["Die", "Lei\u00b7chen", "k\u00f6n\u00b7nen", "von", "der", "Fluth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht den geringsten Trost versp\u00fcren;", "tokens": ["Nicht", "den", "ge\u00b7rings\u00b7ten", "Trost", "ver\u00b7sp\u00fc\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn man noch so kl\u00e4glich thut,", "tokens": ["Und", "wenn", "man", "noch", "so", "kl\u00e4g\u00b7lich", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So l\u00e4\u00dft sich doch der Tod nicht r\u00fchren.", "tokens": ["So", "l\u00e4\u00dft", "sich", "doch", "der", "Tod", "nicht", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So kehr ich denn den frohen Blick,", "tokens": ["So", "kehr", "ich", "denn", "den", "fro\u00b7hen", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein andres Herz, nach deiner Wiegen,", "tokens": ["Mein", "and\u00b7res", "Herz", ",", "nach", "dei\u00b7ner", "Wie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und seh darinnen auch mein Gl\u00fcck,", "tokens": ["Und", "seh", "da\u00b7rin\u00b7nen", "auch", "mein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit dir, auf weichen K\u00fcssen, liegen.", "tokens": ["Mit", "dir", ",", "auf", "wei\u00b7chen", "K\u00fcs\u00b7sen", ",", "lie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Himmel hat dich ausersehn,", "tokens": ["Der", "Him\u00b7mel", "hat", "dich", "au\u00b7ser\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und mir zum Ehgemahl erkohren:", "tokens": ["Und", "mir", "zum", "Eh\u00b7ge\u00b7mahl", "er\u00b7koh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ja, die\u00df ist auch bey mir geschehn;", "tokens": ["Ja", ",", "die\u00df", "ist", "auch", "bey", "mir", "ge\u00b7schehn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Ich selber bin f\u00fcr dich gebohren.", "tokens": ["Ich", "sel\u00b7ber", "bin", "f\u00fcr", "dich", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich denke noch der langen Zeit,", "tokens": ["Ich", "den\u00b7ke", "noch", "der", "lan\u00b7gen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Zeit von zweymal sieben Jahren,", "tokens": ["Der", "Zeit", "von", "zwey\u00b7mal", "sie\u00b7ben", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die mir von deiner Z\u00e4rtlichkeit", "tokens": ["Die", "mir", "von", "dei\u00b7ner", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die allerst\u00e4rksten Zeugen waren.", "tokens": ["Die", "al\u00b7ler\u00b7st\u00e4rks\u00b7ten", "Zeu\u00b7gen", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich denke deiner Treue noch,", "tokens": ["Ich", "den\u00b7ke", "dei\u00b7ner", "Treu\u00b7e", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die mir ganz unverr\u00fcckt geblieben,", "tokens": ["Die", "mir", "ganz", "un\u00b7ver\u00b7r\u00fcckt", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bis meine Brust sich endlich doch,", "tokens": ["Bis", "mei\u00b7ne", "Brust", "sich", "end\u00b7lich", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auch dir geneigt zu seyn, verschrieben.", "tokens": ["Auch", "dir", "ge\u00b7neigt", "zu", "seyn", ",", "ver\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "PTKZU", "VAINF", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zwey volle Jahre sind es fast,", "tokens": ["Zwey", "vol\u00b7le", "Jah\u00b7re", "sind", "es", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seit dem sich Herz und Hand verbunden,", "tokens": ["Seit", "dem", "sich", "Herz", "und", "Hand", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Seit dem ich dich, in Lust und Last,", "tokens": ["Seit", "dem", "ich", "dich", ",", "in", "Lust", "und", "Last", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Lieb und Leid, bew\u00e4hrt erfunden.", "tokens": ["In", "Lieb", "und", "Leid", ",", "be\u00b7w\u00e4hrt", "er\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Je l\u00e4nger unser Ehstand w\u00e4hrt,", "tokens": ["Je", "l\u00e4n\u00b7ger", "un\u00b7ser", "Eh\u00b7stand", "w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Je fester wird das Band sich schlingen;", "tokens": ["Je", "fes\u00b7ter", "wird", "das", "Band", "sich", "schlin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und da die Zeit auch Stahl verzehrt,", "tokens": ["Und", "da", "die", "Zeit", "auch", "Stahl", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit uns bis in die Grube dringen.", "tokens": ["Mit", "uns", "bis", "in", "die", "Gru\u00b7be", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O sollt ich nur den Jammertag", "tokens": ["O", "sollt", "ich", "nur", "den", "Jam\u00b7mer\u00b7tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von deinem Sterben nicht erleben!", "tokens": ["Von", "dei\u00b7nem", "Ster\u00b7ben", "nicht", "er\u00b7le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn was ein Mensch ersinnen mag,", "tokens": ["Denn", "was", "ein", "Mensch", "er\u00b7sin\u00b7nen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das wollt ich, dich zu retten, geben.", "tokens": ["Das", "wollt", "ich", ",", "dich", "zu", "ret\u00b7ten", ",", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O w\u00fcrde mir dereinst von dir", "tokens": ["O", "w\u00fcr\u00b7de", "mir", "de\u00b7reinst", "von", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein sterbend Auge zugedr\u00fccket:", "tokens": ["Mein", "ster\u00b7bend", "Au\u00b7ge", 