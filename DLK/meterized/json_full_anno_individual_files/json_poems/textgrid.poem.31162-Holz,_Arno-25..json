{"textgrid.poem.31162": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still, still, Kind, still! es war ein Traum.", "tokens": ["Still", ",", "still", ",", "Kind", ",", "still", "!", "es", "war", "ein", "Traum", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "NN", "$,", "ADJD", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wellen gr\u00fcn und weiss der Schaum.", "tokens": ["Die", "Wel\u00b7len", "gr\u00fcn", "und", "weiss", "der", "Schaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er rollt durch den Sonnenschein, blitzt und zerstiebt \u2013", "tokens": ["Er", "rollt", "durch", "den", "Son\u00b7nen\u00b7schein", ",", "blitzt", "und", "zer\u00b7stiebt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Es war ein Traum, dass es Rosen giebt!", "tokens": ["Es", "war", "ein", "Traum", ",", "dass", "es", "Ro\u00b7sen", "giebt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Es war ein Traum, dass ein deutscher Wald", "tokens": ["Es", "war", "ein", "Traum", ",",