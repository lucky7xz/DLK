{"dta.poem.19438": {"metadata": {"author": {"name": "Stolberg-Stolberg, Friedrich Leopold zu", "birth": "N.A.", "death": "N.A."}, "title": "Anakreons drei und dreissigste Ode.  \n \u03a3\u03c5 \u03bc\u1f72\u03bd \u03c6\u03af\u03bb\u03b7 \u03c7\u03b5\u03bb\u03b9\u03b4\u03c9\u03bd.  \n  An die Schwalbe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1779", "urn": "urn:nbn:de:kobv:b4-20589-5", "language": ["de:0.99"], "booktitle": "Stolberg-Stolberg, Christian zu; Stolberg-Stolberg, Friedrich Leopold zu: Gedichte. Leipzig, 1779."}, "poem": {"stanza.1": {"line.1": {"text": "Du liebe kleine Schwalbe,", "tokens": ["Du", "lie\u00b7be", "klei\u00b7ne", "Schwal\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du kehrest j\u00e4hrlich wieder,", "tokens": ["Du", "keh\u00b7rest", "j\u00e4hr\u00b7lich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und baust dein Nest im Sommer.", "tokens": ["Und", "baust", "dein", "Nest", "im", "Som\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn dann der Winter nahet,", "tokens": ["Wenn", "dann", "der", "Win\u00b7ter", "na\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So fliehst du zu dem Nile;", "tokens": ["So", "fliehst", "du", "zu", "dem", "Ni\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch Amor bauet immer", "tokens": ["Doch", "A\u00b7mor", "bau\u00b7et", "im\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sein Nest in meinem Herzen.", "tokens": ["Sein", "Nest", "in", "mei\u00b7nem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Amor ist schon fl\u00fccke,", "tokens": ["Ein", "A\u00b7mor", "ist", "schon", "fl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Das Ei verbirgt noch jenen,", "tokens": ["Das", "Ei", "ver\u00b7birgt", "noch", "je\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PDS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und diesem birst die Schale.", "tokens": ["Und", "die\u00b7sem", "birst", "die", "Scha\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Ohn\u2019 Ende schallt die Stimme", "tokens": ["Ohn'", "En\u00b7de", "schallt", "die", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Der Nestlinge, die pipen.", "tokens": ["Der", "Nest\u00b7lin\u00b7ge", ",", "die", "pi\u00b7pen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}}}}}