 "line.4": {"text": "Und ohne dich ver\u00fcbt, auf deiner Rechnung stehn;", "tokens": ["Und", "oh\u00b7ne", "dich", "ver\u00b7\u00fcbt", ",", "auf", "dei\u00b7ner", "Rech\u00b7nung", "stehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So must du, Herr, dich auch bequemen,", "tokens": ["So", "must", "du", ",", "Herr", ",", "dich", "auch", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "NN", "$,", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das B\u00f6se, das durch sie geschehn,", "tokens": ["Das", "B\u00f6\u00b7se", ",", "das", "durch", "sie", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als eigne Schuld auf dich zu nehmen.", "tokens": ["Als", "eig\u00b7ne", "Schuld", "auf", "dich", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}