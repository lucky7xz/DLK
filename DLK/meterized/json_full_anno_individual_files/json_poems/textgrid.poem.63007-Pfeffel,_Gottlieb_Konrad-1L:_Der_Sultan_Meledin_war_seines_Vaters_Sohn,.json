{"textgrid.poem.63007": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Sultan Meledin war seines Vaters Sohn,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sultan Meledin war seines Vaters Sohn,", "tokens": ["Der", "Sul\u00b7tan", "Me\u00b7le\u00b7din", "war", "sei\u00b7nes", "Va\u00b7ters", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das wichtigste Verdienst der meisten Prinzen,", "tokens": ["Das", "wich\u00b7tigs\u00b7te", "Ver\u00b7dienst", "der", "meis\u00b7ten", "Prin\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sa\u00df so schief als er, auf der Aliden Thron,", "tokens": ["Und", "sa\u00df", "so", "schief", "als", "er", ",", "auf", "der", "A\u00b7li\u00b7den", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "KOKOM", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Mufti, der Vezier und Rabbi Sabulon,", "tokens": ["Der", "Muf\u00b7ti", ",", "der", "Ve\u00b7zier", "und", "Rab\u00b7bi", "Sa\u00b7bu\u00b7lon", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Colbert seines Hofs, beherrschten die Provinzen.", "tokens": ["Der", "Col\u00b7bert", "sei\u00b7nes", "Hofs", ",", "be\u00b7herrschten", "die", "Pro\u00b7vin\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Indessen hie\u00df man ihn der Perser Salomon", "tokens": ["In\u00b7des\u00b7sen", "hie\u00df", "man", "ihn", "der", "Per\u00b7ser", "Sa\u00b7lo\u00b7mon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ART", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und pries sein Regiment auf S\u00e4ulen und auf M\u00fcnzen", "tokens": ["Und", "pries", "sein", "Re\u00b7gi\u00b7ment", "auf", "S\u00e4u\u00b7len", "und", "auf", "M\u00fcn\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Im h\u00f6chsten Epop\u00f6enton.", "tokens": ["Im", "h\u00f6chs\u00b7ten", "E\u00b7po\u00b7p\u00f6en\u00b7ton", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Einst kam ein alter Mandarin", "tokens": ["Einst", "kam", "ein", "al\u00b7ter", "Man\u00b7da\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An seinen Hof, der Sinas Reichsgeschichte", "tokens": ["An", "sei\u00b7nen", "Hof", ",", "der", "Si\u00b7nas", "Reichs\u00b7ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "(so lautete sein Pa\u00df) in einem neuen Lichte", "tokens": ["(", "so", "lau\u00b7te\u00b7te", "sein", "Pa\u00df", ")", "in", "ei\u00b7nem", "neu\u00b7en", "Lich\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPOSAT", "NN", "$(", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Nachwelt dargestellt. Wohlan! sprach Meledin,", "tokens": ["Der", "Nach\u00b7welt", "dar\u00b7ge\u00b7stellt", ".", "Wo\u00b7hlan", "!", "sprach", "Me\u00b7le\u00b7din", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "ADV", "$.", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich mache dich zu meinen Biographen,", "tokens": ["Ich", "ma\u00b7che", "dich", "zu", "mei\u00b7nen", "Bio\u00b7gra\u00b7phen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und schenke dir ein Haus, vier Pferde, sieben Sklaven,", "tokens": ["Und", "schen\u00b7ke", "dir", "ein", "Haus", ",", "vier", "Pfer\u00b7de", ",", "sie\u00b7ben", "Skla\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "CARD", "NN", "$,", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und j\u00e4hrlich zehn Pfund Golds f\u00fcr Federn und Papier,", "tokens": ["Und", "j\u00e4hr\u00b7lich", "zehn", "Pfund", "Golds", "f\u00fcr", "Fe\u00b7dern", "und", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "CARD", "NN", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch ehe Sonn und Mond noch dreymal untergehen,", "tokens": ["Doch", "e\u00b7he", "Sonn", "und", "Mond", "noch", "drey\u00b7mal", "un\u00b7ter\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "KON", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Will ich mit meinem Gro\u00dfvezier", "tokens": ["Will", "ich", "mit", "mei\u00b7nem", "Gro\u00df\u00b7ve\u00b7zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Den Grundri\u00df deiner Arbeit sehen.", "tokens": ["Den", "Grun\u00b7dri\u00df", "dei\u00b7ner", "Ar\u00b7beit", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ger\u00fchret und gespornt durch des Monarchen Gunst", "tokens": ["Ge\u00b7r\u00fch\u00b7ret", "und", "ge\u00b7spornt", "durch", "des", "Mon\u00b7ar\u00b7chen", "Gunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVPP", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entwarf der Mandarin auf einem Drachenfelle,", "tokens": ["Ent\u00b7warf", "der", "Man\u00b7da\u00b7rin", "auf", "ei\u00b7nem", "Dra\u00b7chen\u00b7fel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In einer pr\u00e4chtigen Tabelle,", "tokens": ["In", "ei\u00b7ner", "pr\u00e4ch\u00b7ti\u00b7gen", "Ta\u00b7bel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Skizze seines Werks, ein Meisterst\u00fcck der Kunst,", "tokens": ["Die", "Skiz\u00b7ze", "sei\u00b7nes", "Werks", ",", "ein", "Meis\u00b7ter\u00b7st\u00fcck", "der", "Kunst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Worauf der Name seines M\u00e4cenaten", "tokens": ["Wo\u00b7rauf", "der", "Na\u00b7me", "sei\u00b7nes", "M\u00e4\u00b7ce\u00b7na\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als Hauptfigur in goldner Schrift erschien,", "tokens": ["Als", "Haupt\u00b7fi\u00b7gur", "in", "gold\u00b7ner", "Schrift", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und jede seiner gro\u00dfen Thaten", "tokens": ["Und", "je\u00b7de", "sei\u00b7ner", "gro\u00b7\u00dfen", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit einem Denkstrich von Karmin.", "tokens": ["Mit", "ei\u00b7nem", "Denk\u00b7strich", "von", "Kar\u00b7min", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Vortreflich! rief der Schach, und las mit stolzer Miene", "tokens": ["Vor\u00b7tre\u00b7flich", "!", "rief", "der", "Schach", ",", "und", "las", "mit", "stol\u00b7zer", "Mie\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die Schlachten her, die sein Vezier gewann,", "tokens": ["Die", "Schlach\u00b7ten", "her", ",", "die", "sein", "Ve\u00b7zier", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Inde\u00df er in dem Arm der sch\u00f6nen Nuredine", "tokens": ["In\u00b7de\u00df", "er", "in", "dem", "Arm", "der", "sch\u00f6\u00b7nen", "Nu\u00b7re\u00b7di\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald Betel zog, bald Seide spann.", "tokens": ["Bald", "Be\u00b7tel", "zog", ",", "bald", "Sei\u00b7de", "spann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vortreflich, rief er bey den Monumenten", "tokens": ["Vor\u00b7tre\u00b7flich", ",", "rief", "er", "bey", "den", "Mo\u00b7nu\u00b7men\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Der Menschenhuld und des Genies,", "tokens": ["Der", "Men\u00b7schen\u00b7huld", "und", "des", "Ge\u00b7nies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Sabulon zur Ehre des Regenten,", "tokens": ["Die", "Sa\u00b7bu\u00b7lon", "zur", "Eh\u00b7re", "des", "Re\u00b7gen\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Auch wohl zu Mehrung eigner Renten", "tokens": ["Auch", "wohl", "zu", "Meh\u00b7rung", "eig\u00b7ner", "Ren\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "In seinem Namen werden hie\u00df.", "tokens": ["In", "sei\u00b7nem", "Na\u00b7men", "wer\u00b7den", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf einmal stutzt der F\u00fcrst: was hast du hier geschrieben?", "tokens": ["Auf", "ein\u00b7mal", "stutzt", "der", "F\u00fcrst", ":", "was", "hast", "du", "hier", "ge\u00b7schrie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "NN", "$.", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Gebern, die, wie deine Tafel sagt,", "tokens": ["Die", "Ge\u00b7bern", ",", "die", ",", "wie", "dei\u00b7ne", "Ta\u00b7fel", "sagt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein blinder Eifer aus dem Reich verjagt,", "tokens": ["Mein", "blin\u00b7der", "Ei\u00b7fer", "aus", "dem", "Reich", "ver\u00b7jagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hat Ibrahim, der Mufti, fortgetrieben;", "tokens": ["Hat", "Ib\u00b7ra\u00b7him", ",", "der", "Muf\u00b7ti", ",", "fort\u00b7ge\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NE", "$,", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auf ihn must du den Fehler schieben.", "tokens": ["Auf", "ihn", "must", "du", "den", "Feh\u00b7ler", "schie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich hab am Aechtungsbrief dein Siegel h\u00e4ngen sehn,", "tokens": ["Ich", "hab", "am", "A\u00b7ech\u00b7tungs\u00b7brief", "dein", "Sie\u00b7gel", "h\u00e4n\u00b7gen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PPOSAT", "NN", "VVINF", "VVINF", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Versetzt der Mandarin: wenn die ber\u00fchmten Thaten,", "tokens": ["Ver\u00b7setzt", "der", "Man\u00b7da\u00b7rin", ":", "wenn", "die", "be\u00b7r\u00fchm\u00b7ten", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die deine Diener angerathen", "tokens": ["Die", "dei\u00b7ne", "Die\u00b7ner", "an\u00b7ge\u00b7ra\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ohne dich ver\u00fcbt, auf deiner Rechnung stehn;", "tokens": ["Und", "oh\u00b7ne", "dich", "ver\u00b7\u00fcbt", ",", "auf", "dei\u00b7ner", "Rech\u00b7nung", "stehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So must du, Herr, dich auch bequemen,", "tokens": ["So", "must", "du", ",", "Herr", ",", "dich", "auch", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "NN", "$,", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das B\u00f6se, das durch sie geschehn,", "tokens": ["Das", "B\u00f6\u00b7se", ",", "das", "durch", "sie", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als eigne Schuld auf dich zu nehmen.", "tokens": ["Als", "eig\u00b7ne", "Schuld", "auf", "dich", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Sultan Meledin war seines Vaters Sohn,", "