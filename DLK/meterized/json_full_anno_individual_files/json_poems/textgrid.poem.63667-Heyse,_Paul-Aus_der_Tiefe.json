{"textgrid.poem.63667": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Aus der Tiefe", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber mir, ein dunkles Meer,", "tokens": ["\u00dc\u00b7ber", "mir", ",", "ein", "dunk\u00b7les", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4gt Vergessenheit zusammen.", "tokens": ["Schl\u00e4gt", "Ver\u00b7ges\u00b7sen\u00b7heit", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Still, wie still ist's um mich her,", "tokens": ["Still", ",", "wie", "still", "ist's", "um", "mich", "her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ADJD", "VAFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stumm von Klagen und Verdammen.", "tokens": ["Stumm", "von", "Kla\u00b7gen", "und", "Ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur wie durch des Tauchers Glas", "tokens": ["Nur", "wie", "durch", "des", "Tau\u00b7chers", "Glas"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPR", "ART", "NN", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Seh ich rings der Tiefe Schrecken,", "tokens": ["Seh", "ich", "rings", "der", "Tie\u00b7fe", "Schre\u00b7cken", ","], "token_in