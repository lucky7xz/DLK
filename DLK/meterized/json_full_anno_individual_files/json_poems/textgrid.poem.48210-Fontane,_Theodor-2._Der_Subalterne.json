{"textgrid.poem.48210": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "2. Der Subalterne", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbimmer Achselzucken (es ist zum Lachen),", "tokens": ["\u00bb", "im\u00b7mer", "Ach\u00b7sel\u00b7zu\u00b7cken", "(", "es", "ist", "zum", "La\u00b7chen", ")", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "NN", "$(", "PPER", "VAFIN", "APPRART", "NN", "$(", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und doch sind ", "tokens": ["Und", "doch", "sind"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Das Bi\u00dfchen Deutschland zusammenzuschwei\u00dfen,", "tokens": ["Das", "Bi\u00df\u00b7chen", "Deutschland", "zu\u00b7sam\u00b7men\u00b7zu\u00b7schwei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das lag in der Zeit, das will nicht viel hei\u00dfen \u2013", "tokens": ["Das", "lag", "in", "der", "Zeit", ",", "das", "will", "nicht", "viel", "hei\u00b7\u00dfen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,", "PDS", "VMFIN", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und Sedan? Nach links und rechts zu schwenken,", "tokens": ["Und", "Se\u00b7dan", "?", "Nach", "links", "und", "rechts", "zu", "schwen\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$.", "APPR", "ADV", "KON", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ist auch nichts Gef\u00e4hrlichs auszudenken.", "tokens": ["Ist", "auch", "nichts", "Ge\u00b7f\u00e4hr\u00b7lichs", "aus\u00b7zu\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "NN", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ich bin nicht f\u00fcr Ruhm, ich bin nicht f\u00fcr Ehr',", "tokens": ["Ich", "bin", "nicht", "f\u00fcr", "Ruhm", ",", "ich", "bin", "nicht", "f\u00fcr", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es ist mit alledem nicht weit her,", "tokens": ["Es", "ist", "mit", "al\u00b7le\u00b7dem", "nicht", "weit", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "PTKNEG", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und es w\u00e4r' mir ein Leichtes, mich drin zu finden,", "tokens": ["Und", "es", "w\u00e4r'", "mir", "ein", "Leich\u00b7tes", ",", "mich", "drin", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ART", "NN", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "W\u00e4r' nicht die Frau \u2013 ", "tokens": ["W\u00e4r'", "nicht", "die", "Frau", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "So hie\u00df es um Weihnacht. Am Ordensfest", "tokens": ["So", "hie\u00df", "es", "um", "Weih\u00b7nacht", ".", "Am", "Or\u00b7dens\u00b7fest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$.", "APPRART", "NN"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sprang um der Wind von Ost nach West,", "tokens": ["Sprang", "um", "der", "Wind", "von", "Ost", "nach", "West", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "NE", "APPR", "NE", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Der Glauben an Gottes Gnad' und G\u00fcte", "tokens": ["Der", "Glau\u00b7ben", "an", "Got\u00b7tes", "Gnad'", "und", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "NN", "KON", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schlug wieder Wurzel in seinem Gem\u00fcte.", "tokens": ["Schlug", "wie\u00b7der", "Wur\u00b7zel", "in", "sei\u00b7nem", "Ge\u00b7m\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Wie's blinkt, wie's schillert! Er strahlt, er bebt.", "tokens": ["Wie's", "blinkt", ",", "wie's", "schil\u00b7lert", "!", "Er", "strahlt", ",", "er", "bebt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWAV", "VVFIN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u