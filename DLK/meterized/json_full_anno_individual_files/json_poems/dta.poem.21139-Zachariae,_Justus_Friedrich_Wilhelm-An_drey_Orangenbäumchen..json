{"dta.poem.21139": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An drey Orangenb\u00e4umchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Euch, die aus einer Orange", "tokens": ["Euch", ",", "die", "aus", "ei\u00b7ner", "O\u00b7ran\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Seline dankbar ges\u00e4t;", "tokens": ["Se\u00b7li\u00b7ne", "dank\u00b7bar", "ge\u00b7s\u00e4t", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Euch, von holdseligen H\u00e4nden", "tokens": ["Euch", ",", "von", "hold\u00b7se\u00b7li\u00b7gen", "H\u00e4n\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Liebe s\u00e4uselnd erzogen;", "tokens": ["Der", "Lie\u00b7be", "s\u00e4u\u00b7selnd", "er\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Euch, B\u00e4umchen, sing ich dieses Lied.", "tokens": ["Euch", ",", "B\u00e4um\u00b7chen", ",", "sing", "ich", "die\u00b7ses", "Lied", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Den sanften Grazien \u00e4hnlich,", "tokens": ["Den", "sanf\u00b7ten", "Gra\u00b7zi\u00b7en", "\u00e4hn\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wachst ihr freundschaftlich empor,", "tokens": ["Wachst", "ihr", "freund\u00b7schaft\u00b7lich", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den geselligen Schatten,", "tokens": ["Und", "den", "ge\u00b7sel\u00b7li\u00b7gen", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und eure spielenden Bl\u00e4tter,", "tokens": ["Und", "eu\u00b7re", "spie\u00b7len\u00b7den", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Umtanzt der Weste leichtes Volk.", "tokens": ["Um\u00b7tanzt", "der", "Wes\u00b7te", "leich\u00b7tes", "Volk", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das Reich der farbigten Blumen,", "tokens": ["Das", "Reich", "der", "far\u00b7big\u00b7ten", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wenn es der Fr\u00fchling beherrscht;", "tokens": ["Wenn", "es", "der", "Fr\u00fch\u00b7ling", "be\u00b7herrscht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Selbst die monarchische Staude,", "tokens": ["Selbst", "die", "mon\u00b7ar\u00b7chi\u00b7sche", "Stau\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Die nach Jahrhunderten bl\u00fchet,", "tokens": ["Die", "nach", "Jahr\u00b7hun\u00b7der\u00b7ten", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Aloe, reizt mich nicht so.", "tokens": ["Die", "A\u00b7loe", ",", "reizt", "mich", "nicht", "so", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Der Reif der schimmernden N\u00e4chte", "tokens": ["Der", "Reif", "der", "schim\u00b7mern\u00b7den", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Geh sanfter \u00fcber euch weg!", "tokens": ["Geh", "sanf\u00b7ter", "\u00fc\u00b7ber", "euch", "weg", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die bunte G\u00f6ttin der Blumen,", "tokens": ["Die", "bun\u00b7te", "G\u00f6t\u00b7tin", "der", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ja selbst die m\u00e4chtige Liebe", "tokens": ["Ja", "selbst", "die", "m\u00e4ch\u00b7ti\u00b7ge", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Besch\u00fctz euch vor der R\u00e4uber Hand!", "tokens": ["Be\u00b7sch\u00fctz", "euch", "vor", "der", "R\u00e4u\u00b7ber", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}