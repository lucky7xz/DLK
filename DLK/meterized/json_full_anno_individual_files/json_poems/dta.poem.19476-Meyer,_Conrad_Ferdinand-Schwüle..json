{"dta.poem.19476": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Schw\u00fcle.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00fcb verglomm der schw\u00fcle Sommertag,", "tokens": ["Tr\u00fcb", "ver\u00b7glomm", "der", "schw\u00fc\u00b7le", "Som\u00b7mer\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Dumpf und traurig t\u00f6nt mein Ruderschlag \u2014", "tokens": ["Dumpf", "und", "trau\u00b7rig", "t\u00f6nt", "mein", "Ru\u00b7der\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sterne, Sterne \u2014 Abend ist es ja \u2014", "tokens": ["Ster\u00b7ne", ",", "Ster\u00b7ne", "A\u00b7bend", "ist", "es", "ja"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$(", "NN", "VAFIN", "PPER", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sterne, warum seid ihr noch nicht da?", "tokens": ["Ster\u00b7ne", ",", "wa\u00b7rum", "seid", "ihr", "noch", "nicht", "da", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Bleich das Leben! Bleich der Felsenhang!", "tokens": ["Bleich", "das", "Le\u00b7ben", "!", "Bleich", "der", "Fel\u00b7sen\u00b7hang", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$.", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Schilf, was fl\u00fcsterst du so frech und bang?", "tokens": ["Schilf", ",", "was", "fl\u00fcs\u00b7terst", "du", "so", "frech", "und", "bang", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Fern der Himmel und die Tiefe nah \u2014", "tokens": ["Fern", "der", "Him\u00b7mel", "und", "die", "Tie\u00b7fe", "nah"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "ADJD", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sterne, warum seid ihr noch nicht da?", "tokens": ["Ster\u00b7ne", ",", "wa\u00b7rum", "seid", "ihr", "noch", "nicht", "da", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Eine liebe, liebe Stimme ruft", "tokens": ["Ei\u00b7ne", "lie\u00b7be", ",", "lie\u00b7be", "Stim\u00b7me", "ruft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mich best\u00e4ndig aus der Wassergruft \u2014", "tokens": ["Mich", "be\u00b7st\u00e4n\u00b7dig", "aus", "der", "Was\u00b7ser\u00b7gruft"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Weg, Gespenst, das oft ich winken sah!", "tokens": ["Weg", ",", "Ge\u00b7spenst", ",", "das", "oft", "ich", "win\u00b7ken", "sah", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PRELS", "ADV", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sterne, Sterne, seid ihr nicht mehr da?", "tokens": ["Ster\u00b7ne", ",", "Ster\u00b7ne", ",", "seid", "ihr", "nicht", "mehr", "da", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VAFIN", "PPER", "PTKNEG", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Endlich, endlich durch das Dunkel bricht \u2014", "tokens": ["End\u00b7lich", ",", "end\u00b7lich", "durch", "das", "Dun\u00b7kel", "bricht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Es war Zeit! \u2014 ein schwaches Flimmerlicht \u2014", "tokens": ["Es", "war", "Zeit", "!", "ein", "schwa\u00b7ches", "Flim\u00b7mer\u00b7licht"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$.", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn ich wu\u00dfte nicht wie mir geschah.", "tokens": ["Denn", "ich", "wu\u00df\u00b7te", "nicht", "wie", "mir", "ge\u00b7schah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sterne, Sterne, bleibt mir immer nah!", "tokens": ["Ster\u00b7ne", ",", "Ster\u00b7ne", ",", "bleibt", "mir", "im\u00b7mer", "nah", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}