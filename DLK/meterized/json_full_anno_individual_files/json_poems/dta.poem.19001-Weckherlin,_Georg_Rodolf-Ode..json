{"dta.poem.19001": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ode.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Die Natur hat ein jedes thier", "tokens": ["Die", "Na\u00b7tur", "hat", "ein", "je\u00b7des", "thier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "PIAT", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mit sonderbarer gaab vnd zier", "tokens": ["Mit", "son\u00b7der\u00b7ba\u00b7rer", "ga\u00b7ab", "vnd", "zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sorgf\u00e4ltiglich so wol versehen/", "tokens": ["Sorg\u00b7f\u00e4l\u00b7tig\u00b7lich", "so", "wol", "ver\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df jhrer jedes mag (billich", "tokens": ["Da\u00df", "jhrer", "je\u00b7des", "mag", "(", "bil\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPOSAT", "PIAT", "VMFIN", "$(", "ADJD"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Vern\u00fcget) dessen r\u00fchmen sich/", "tokens": ["Ver\u00b7n\u00fc\u00b7get", ")", "des\u00b7sen", "r\u00fch\u00b7men", "sich", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PDS", "VVFIN", "PRF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd neben andern wol bestehen.", "tokens": ["Vnd", "ne\u00b7ben", "an\u00b7dern", "wol", "be\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein horn dem Einhorn auff das hirn", "tokens": ["Ein", "horn", "dem", "Ein\u00b7horn", "auff", "das", "hirn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Stier zwey h\u00f6rner auff die stirn/", "tokens": ["Dem", "Stier", "zwey", "h\u00f6r\u00b7ner", "auff", "die", "stirn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Hirsch ein geweyh ist ges\u00f6tzet:", "tokens": ["Dem", "Hirsch", "ein", "ge\u00b7weyh", "ist", "ge\u00b7s\u00f6t\u00b7zet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die V\u00f6gel hat sie durch den flug/", "tokens": ["Die", "V\u00f6\u00b7gel", "hat", "sie", "durch", "den", "flug", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd die F\u00fcchs mit list vnd betrug", "tokens": ["Vnd", "die", "F\u00fcchs", "mit", "list", "vnd", "be\u00b7trug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN", "KON", "VVFIN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Zu jhrer sicherheit erg\u00f6tzet.", "tokens": ["Zu", "jhrer", "si\u00b7cher\u00b7heit", "er\u00b7g\u00f6t\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Fisch kan schwimmen/ vnd das Pferd", "tokens": ["Der", "Fisch", "kan", "schwim\u00b7men", "/", "vnd", "das", "Pferd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$(", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist wegen guten hufs mehr wehrt/", "tokens": ["Ist", "we\u00b7gen", "gu\u00b7ten", "hufs", "mehr", "wehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die L\u00f6wen haben z\u00e4hn vnd klawen;", "tokens": ["Die", "L\u00f6\u00b7wen", "ha\u00b7ben", "z\u00e4hn", "vnd", "kla\u00b7wen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das lauffen ist der Hasen pfand/", "tokens": ["Das", "lauf\u00b7fen", "ist", "der", "Ha\u00b7sen", "pfand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VAFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Man hat g\u00f6tlichen verstand;", "tokens": ["Der", "Man", "hat", "g\u00f6t\u00b7li\u00b7chen", "ver\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was haben dan die zarte Frawen?", "tokens": ["Was", "ha\u00b7ben", "dan", "die", "zar\u00b7te", "Fra\u00b7wen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Frawen seind mit der Lieb pracht/", "tokens": ["Die", "Fra\u00b7wen", "seind", "mit", "der", "Lieb", "pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Vnd mit der Sch\u00f6nheit h\u00f6chsten macht", "tokens": ["Vnd", "mit", "der", "Sch\u00f6n\u00b7heit", "h\u00f6chs\u00b7ten", "macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So vnvermeydenlich gezieret/", "tokens": ["So", "vn\u00b7ver\u00b7mey\u00b7den\u00b7lich", "ge\u00b7zie\u00b7ret", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df jhr holdseelige gestalt", "tokens": ["Da\u00df", "jhr", "hold\u00b7see\u00b7li\u00b7ge", "ge\u00b7stalt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Allein regierend/ ohn gewalt", "tokens": ["Al\u00b7lein", "re\u00b7gie\u00b7rend", "/", "ohn", "ge\u00b7walt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVPP", "$(", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vber die hertzen triumfieret.", "tokens": ["Vber", "die", "hert\u00b7zen", "tri\u00b7um\u00b7fie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}