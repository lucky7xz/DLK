{"textgrid.poem.60036": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als neulich Vater Jupiter", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als neulich Vater Jupiter", "tokens": ["Als", "neu\u00b7lich", "Va\u00b7ter", "Ju\u00b7pi\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "NE"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Sein m\u00fcdes Haupt, von Sorgen schwer,", "tokens": ["Sein", "m\u00fc\u00b7des", "Haupt", ",", "von", "Sor\u00b7gen", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf seine G\u00f6tterrechte st\u00fctzte,", "tokens": ["Auf", "sei\u00b7ne", "G\u00f6t\u00b7ter\u00b7rech\u00b7te", "st\u00fctz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit der Linken m\u00e4chtig blitzte;", "tokens": ["Und", "mit", "der", "Lin\u00b7ken", "m\u00e4ch\u00b7tig", "blitz\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die gro\u00dfen Augen hin und her", "tokens": ["Die", "gro\u00b7\u00dfen", "Au\u00b7gen", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gedreht, im Lehnstuhl \u00fcberdachte,", "tokens": ["Ge\u00b7dreht", ",", "im", "Lehn\u00b7stuhl", "\u00fc\u00b7berd\u00b7ach\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Was nun sein Erdenv\u00f6lkchen machte:", "tokens": ["Was", "nun", "sein", "Er\u00b7den\u00b7v\u00f6l\u00b7kchen", "mach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da kam zum hohen Jupiter,", "tokens": ["Da", "kam", "zum", "ho\u00b7hen", "Ju\u00b7pi\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+----", "measure": "unknown.measure.di"}, "line.9": {"text": "Mit einem Kasten auf dem R\u00fccken,", "tokens": ["Mit", "ei\u00b7nem", "Kas\u00b7ten", "auf", "dem", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Freund Momus, unter vielem B\u00fccken,", "tokens": ["Freund", "Mo\u00b7mus", ",", "un\u00b7ter", "vie\u00b7lem", "B\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und gr\u00fc\u00dfte den Olymp, und bat", "tokens": ["Und", "gr\u00fc\u00df\u00b7te", "den", "O\u00b7lymp", ",", "und", "bat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Den ganzen g\u00f6ttlichen Senat,", "tokens": ["Den", "gan\u00b7zen", "g\u00f6tt\u00b7li\u00b7chen", "Se\u00b7nat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Zu seinen sch\u00f6nen Rarit\u00e4ten", "tokens": ["Zu", "sei\u00b7nen", "sch\u00f6\u00b7nen", "Ra\u00b7ri\u00b7t\u00e4\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ein wenig n\u00e4her hin zu treten.", "tokens": ["Ein", "we\u00b7nig", "n\u00e4\u00b7her", "hin", "zu", "tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Man sah, und sah die weite Welt", "tokens": ["Man", "sah", ",", "und", "sah", "die", "wei\u00b7te", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Von Sonn' und Monden \u00fcberschimmert,", "tokens": ["Von", "Sonn'", "und", "Mon\u00b7den", "\u00fc\u00b7bers\u00b7chim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Im Kleinen trefflich nachgezimmert;", "tokens": ["Im", "Klei\u00b7nen", "treff\u00b7lich", "nach\u00b7ge\u00b7zim\u00b7mert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und Erde, Feuer, Luft und See,", "tokens": ["Und", "Er\u00b7de", ",", "Feu\u00b7er", ",", "Luft", "und", "See", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und alles, was darinnen je", "tokens": ["Und", "al\u00b7les", ",", "was", "da\u00b7rin\u00b7nen", "je"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "$,", "PRELS", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Gewesen: Leopard und Wurm,", "tokens": ["Ge\u00b7we\u00b7sen", ":", "Le\u00b7o\u00b7pard", "und", "Wurm", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und Nachtigall, und Krieg und Sturm", "tokens": ["Und", "Nach\u00b7ti\u00b7gall", ",", "und", "Krieg", "und", "Sturm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und W\u00e4ldchen, reich an Melodieen,", "tokens": ["Und", "W\u00e4ld\u00b7chen", ",", "reich", "an", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Und Berge, welche Flammen spieen:", "tokens": ["Und", "Ber\u00b7ge", ",", "wel\u00b7che", "Flam\u00b7men", "spie\u00b7en", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PWAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Das erste Paradies; den Thurm", "tokens": ["Das", "ers\u00b7te", "Pa\u00b7ra\u00b7dies", ";", "den", "Thurm"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Zu Babel, neben einer Grotte", "tokens": ["Zu", "Ba\u00b7bel", ",", "ne\u00b7ben", "ei\u00b7ner", "Grot\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.26": {"text": "Bewohnt vom j\u00fcngsten Liebesgotte;", "tokens": ["Be\u00b7wohnt", "vom", "j\u00fcng\u00b7sten", "Lie\u00b7bes\u00b7got\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Der Ninon stilles Cabinett,", "tokens": ["Der", "Ni\u00b7non", "stil\u00b7les", "Ca\u00b7bi\u00b7nett", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Und Magdalenens hartes Bett,", "tokens": ["Und", "Mag\u00b7da\u00b7le\u00b7nens", "har\u00b7tes", "Bett", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Umtanzt mit h\u00f6llischem Gewimmel,", "tokens": ["Um\u00b7tanzt", "mit", "h\u00f6l\u00b7li\u00b7schem", "Ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Und Heilige, schon halb im Himmel;", "tokens": ["Und", "Hei\u00b7li\u00b7ge", ",", "schon", "halb", "im", "Him\u00b7mel", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Des jungen Peleiden Zorn,", "tokens": ["Des", "jun\u00b7gen", "Pe\u00b7lei\u00b7den", "Zorn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.32": {"text": "Des Epikur ger\u00fchmten Frieden;", "tokens": ["Des", "E\u00b7pi\u00b7kur", "ge\u00b7r\u00fchm\u00b7ten", "Frie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Und hier Egyptens Pyramiden,", "tokens": ["Und", "hier", "E\u00b7gyp\u00b7tens", "Py\u00b7ra\u00b7mi\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Und dort ein Lied von Hagedorn.", "tokens": ["Und", "dort", "ein", "Lied", "von", "Ha\u00b7ge\u00b7dorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Nicht minder k\u00fcnstlich war zu sehn", "tokens": ["Nicht", "min\u00b7der", "k\u00fcnst\u00b7lich", "war", "zu", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADJD", "VAFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "In seinem Fasse Diogen,", "tokens": ["In", "sei\u00b7nem", "Fas\u00b7se", "Dio\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Und auf der B\u00fchne Carl der Zw\u00f6lfte;", "tokens": ["Und", "auf", "der", "B\u00fch\u00b7ne", "Carl", "der", "Zw\u00f6lf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Petrarch mit seiner lieben H\u00e4lfte;", "tokens": ["Pe\u00b7trarch", "mit", "sei\u00b7ner", "lie\u00b7ben", "H\u00e4lf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Semiramis und Helena;", "tokens": ["Se\u00b7mi\u00b7ra\u00b7mis", "und", "He\u00b7le\u00b7na", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.40": {"text": "Musarion und Pamela;", "tokens": ["Mu\u00b7sa\u00b7ri\u00b7on", "und", "Pa\u00b7me\u00b7la", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Mein Bayle zwischen seinen Zweifeln,", "tokens": ["Mein", "Bay\u00b7le", "zwi\u00b7schen", "sei\u00b7nen", "Zwei\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Und Doktor Faust mit seinen Teufeln,", "tokens": ["Und", "Dok\u00b7tor", "Faust", "mit", "sei\u00b7nen", "Teu\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Und Robinson auf seiner Fahrt;", "tokens": ["Und", "Ro\u00b7bin\u00b7son", "auf", "sei\u00b7ner", "Fahrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Am schattenvollen Traubenh\u00fcgel", "tokens": ["Am", "schat\u00b7ten\u00b7vol\u00b7len", "Trau\u00b7ben\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Anakreons gesalbter Bart:", "tokens": ["A\u00b7nak\u00b7re\u00b7ons", "ge\u00b7salb\u00b7ter", "Bart", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Candide, Solon, Eulenspiegel,", "tokens": ["Can\u00b7di\u00b7de", ",", "So\u00b7lon", ",", "Eu\u00b7len\u00b7spie\u00b7gel", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Confucius und Aretin,", "tokens": ["Con\u00b7fu\u00b7cius", "und", "A\u00b7re\u00b7tin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.48": {"text": "Und Schwedenborg und Harlekin", "tokens": ["Und", "Schwe\u00b7den\u00b7borg", "und", "Har\u00b7le\u00b7kin"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Aus einem Ey hervorgekrochen;", "tokens": ["Aus", "ei\u00b7nem", "Ey", "her\u00b7vor\u00b7ge\u00b7kro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Der Eremit bey Todtenknochen;", "tokens": ["Der", "E\u00b7re\u00b7mit", "bey", "Tod\u00b7ten\u00b7kno\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Armida bey Rinaldens Ku\u00df;", "tokens": ["Ar\u00b7mi\u00b7da", "bey", "Ri\u00b7nal\u00b7dens", "Ku\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.52": {"text": "Und endlich machten den Beschlu\u00df", "tokens": ["Und", "end\u00b7lich", "mach\u00b7ten", "den", "Be\u00b7schlu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Chymisten, Critiker, Propheten,", "tokens": ["Chy\u00b7mis\u00b7ten", ",", "Cri\u00b7ti\u00b7ker", ",", "Pro\u00b7phe\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Druiden, Zauberer, Poeten,", "tokens": ["Dru\u00b7i\u00b7den", ",", "Zau\u00b7be\u00b7rer", ",", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.55": {"text": "Nebst Sittenspr\u00fcchen, Wunderlehr',", "tokens": ["Nebst", "Sit\u00b7ten\u00b7spr\u00fc\u00b7chen", ",", "Wun\u00b7der\u00b7lehr'", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Und tausend andern sch\u00f6nen Sachen.", "tokens": ["Und", "tau\u00b7send", "an\u00b7dern", "sch\u00f6\u00b7nen", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.57": {"text": "Da blitzte Jupiter nicht mehr,", "tokens": ["Da", "blitz\u00b7te", "Ju\u00b7pi\u00b7ter", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Und alle G\u00f6tter mu\u00dften lachen.", "tokens": ["Und", "al\u00b7le", "G\u00f6t\u00b7ter", "mu\u00df\u00b7ten", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word"