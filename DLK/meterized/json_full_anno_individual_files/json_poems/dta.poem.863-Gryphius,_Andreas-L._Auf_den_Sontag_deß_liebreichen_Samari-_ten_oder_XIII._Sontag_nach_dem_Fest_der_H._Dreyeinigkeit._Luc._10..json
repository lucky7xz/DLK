{"dta.poem.863": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "L.  Auf den Sontag de\u00df liebreichen Samari-  \n ten/ oder  XIII.  Sontag nach dem Fest der  \n H. Dreyeinigkeit. Luc. 10.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Bj\u00df auff den Tod verwundt/ zerfleischt/ zumalmm't/ zu- ", "tokens": ["Bj\u00df", "auff", "den", "Tod", "ver\u00b7wundt", "/", "zer\u00b7fleischt", "/", "zu\u00b7malm\u00b7m't", "/", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$(", "VVPP", "$(", "VVFIN", "$(", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "schlagen ", "tokens": ["schla\u00b7gen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Verschmacht ich vnd vergeh/ jtzt schwindet mein Gesicht", "tokens": ["Ver\u00b7schmacht", "ich", "vnd", "ver\u00b7geh", "/", "jtzt", "schwin\u00b7det", "mein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "$(", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der schwache Leib erstirbt/ mein mattes Hertze bricht", "tokens": ["Der", "schwa\u00b7che", "Leib", "er\u00b7stirbt", "/", "mein", "mat\u00b7tes", "Hert\u00b7ze", "bricht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der m\u00fcde Geist vergeht in tausendfachen plagen.", "tokens": ["Der", "m\u00fc\u00b7de", "Geist", "ver\u00b7geht", "in", "tau\u00b7send\u00b7fa\u00b7chen", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Adern starren schon/ der Mund kau nicht mehr klagen.", "tokens": ["Die", "A\u00b7dern", "star\u00b7ren", "schon", "/", "der", "Mund", "kau", "nicht", "mehr", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$(", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Todt schwebt \u00fcber mir/ ich wei\u00df schier selber nicht", "tokens": ["Der", "Todt", "schwebt", "\u00fc\u00b7ber", "mir", "/", "ich", "wei\u00df", "schier", "sel\u00b7ber", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$(", "PPER", "VVFIN", "ADV", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie schwer der Schmertzen sey! O wahres Lebens Licht?", "tokens": ["Wie", "schwer", "der", "Schmert\u00b7zen", "sey", "!", "O", "wah\u00b7res", "Le\u00b7bens", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VAFIN", "$.", "NE", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "HeRR ", "tokens": ["HeRR"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Al\u00df Priester vnd Levit! Ach Sammarite kom", "tokens": ["Al\u00df", "Pries\u00b7ter", "vnd", "Le\u00b7vit", "!", "Ach", "Sam\u00b7ma\u00b7ri\u00b7te", "kom"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "$.", "ITJ", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd geu\u00df mir Oel vnd Wein/ den Blutt vnd Wasser-", "tokens": ["Vnd", "geu\u00df", "mir", "O\u00b7el", "vnd", "Wein", "/", "den", "Blutt", "vnd", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "NN", "KON", "NN", "$(", "ART", "NN", "KON", "TRUNC"], "meter": "----+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "strom ", "tokens": ["strom"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Au\u00df deiner Seiten eyn. Die rawen W\u00fcsteneyen", "tokens": ["Au\u00df", "dei\u00b7ner", "Sei\u00b7ten", "eyn", ".", "Die", "ra\u00b7wen", "W\u00fcs\u00b7te\u00b7ne\u00b7yen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAINF", "$.", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}, "stanza.4": {"line.1": {"text": "Der M\u00f6rder vollen Welt vermehren nur die Noht", "tokens": ["Der", "M\u00f6r\u00b7der", "vol\u00b7len", "Welt", "ver\u00b7meh\u00b7ren", "nur", "die", "Noht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wil ins Kirchen Hau\u00df/ da man auff dein Gebotr", "tokens": ["Ich", "wil", "ins", "Kir\u00b7chen", "Hau\u00df", "/", "da", "man", "auff", "dein", "Ge\u00b7bo\u00b7tr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", 