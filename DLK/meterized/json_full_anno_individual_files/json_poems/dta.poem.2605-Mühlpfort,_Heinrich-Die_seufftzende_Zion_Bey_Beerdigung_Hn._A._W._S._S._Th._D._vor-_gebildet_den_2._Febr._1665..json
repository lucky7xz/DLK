{"dta.poem.2605": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die seufftzende Zion/  \n  Bey Beerdigung Hn.   A.   W.   S. S. Th. D.   vor-  \n gebildet/ den 2. Febr. 1665.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr T\u00f6chter Solyme/ helfft eurer Schwester klagen/", "tokens": ["Ihr", "T\u00f6ch\u00b7ter", "So\u00b7ly\u00b7me", "/", "helfft", "eu\u00b7rer", "Schwes\u00b7ter", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$(", "VVFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Schmertz so Zion druckt ist tausend Zehren werth/", "tokens": ["Der", "Schmertz", "so", "Zi\u00b7on", "druckt", "ist", "tau\u00b7send", "Zeh\u00b7ren", "werth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NE", "VVFIN", "VAFIN", "CARD", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erstarr\u2019t nicht da\u00df ihr mich seht beyde Br\u00fcste schlagen/", "tokens": ["Er\u00b7starr't", "nicht", "da\u00df", "ihr", "mich", "seht", "bey\u00b7de", "Br\u00fcs\u00b7te", "schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "KOUS", "PPER", "PRF", "VVFIN", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df mein verh\u00fclltes Haupt mit Aschen ist beschwert.", "tokens": ["Da\u00df", "mein", "ver\u00b7h\u00fcll\u00b7tes", "Haupt", "mit", "A\u00b7schen", "ist", "be\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich reisse Schmuck und Gold von dem gekr\u00f6nten Haare/", "tokens": ["Ich", "reis\u00b7se", "Schmuck", "und", "Gold", "von", "dem", "ge\u00b7kr\u00f6n\u00b7ten", "Haa\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Thr\u00e4nen seht ihr hier statt rundter Perlen stehn/", "tokens": ["Die", "Thr\u00e4\u00b7nen", "seht", "ihr", "hier", "statt", "rund\u00b7ter", "Per\u00b7len", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In meiner heilgen Burg/ des Traurens Ziel/ die Bahre;", "tokens": ["In", "mei\u00b7ner", "heil\u00b7gen", "Burg", "/", "des", "Trau\u00b7rens", "Ziel", "/", "die", "Bah\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "ART", "NN", "NN", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auf Schwestern auf und eilt ihr solt zur Leiche gehn!", "tokens": ["Auf", "Schwes\u00b7tern", "auf", "und", "eilt", "ihr", "solt", "zur", "Lei\u00b7che", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "KON", "VVFIN", "PPER", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Stimme so man vor im Tempel h\u00f6rt\u2019 erschallen/", "tokens": ["Die", "Stim\u00b7me", "so", "man", "vor", "im", "Tem\u00b7pel", "h\u00f6rt'", "er\u00b7schal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIS", "APPR", "APPRART", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Mund so Welt und Tod mit Lehren hat besiegt/", "tokens": ["Der", "Mund", "so", "Welt", "und", "Tod", "mit", "Leh\u00b7ren", "hat", "be\u00b7siegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "KON", "NN", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mein Ober-Priester ist/ (unendlich Leid!) verfallen/", "tokens": ["Mein", "O\u00b7ber\u00b7Pries\u00b7ter", "ist", "/", "(", "un\u00b7end\u00b7lich", "Leid", "!", ")", "ver\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$(", "$(", "ADJD", "NN", "$.", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ach Angst die allen Schmertz der Schmertzen \u00fcberwiegt!", "tokens": ["Ach", "Angst", "die", "al\u00b7len", "Schmertz", "der", "Schmert\u00b7zen", "\u00fc\u00b7berw\u00b7iegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "ART", "PIAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mu\u00df mir das Neue Jahr auch neue Pein geb\u00e4ren?", "tokens": ["Mu\u00df", "mir", "das", "Neu\u00b7e", "Jahr", "auch", "neu\u00b7e", "Pein", "ge\u00b7b\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Schleicht blasse Traurigkeit zu Pfort und Mauren ein?", "tokens": ["Schleicht", "blas\u00b7se", "Trau\u00b7rig\u00b7keit", "zu", "Pfort", "und", "Mau\u00b7ren", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und ist dem End-Schlu\u00df nicht/ der schon vollbracht zu wehren?", "tokens": ["Und", "ist", "dem", "En\u00b7dSchlu\u00df", "nicht", "/", "der", "schon", "voll\u00b7bracht", "zu", "weh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PTKNEG", "$(", "ART", "ADV", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So mu\u00df nur ", "tokens": ["So", "mu\u00df", "nur"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV"], "meter": "+--", "measure": "dactylic.init"}, "line.17": {"text": "Frolockte nicht mein Volck im Eingang dieser Tage/", "tokens": ["Fro\u00b7lock\u00b7te", "nicht", "mein", "Volck", "im", "Ein\u00b7gang", "die\u00b7ser", "Ta\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "APPRART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Hat Harff und Seitenspiel die Andacht nicht geweyht/", "tokens": ["Hat", "Harff", "und", "Sei\u00b7ten\u00b7spiel", "die", "An\u00b7dacht", "nicht", "ge\u00b7weyht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "KON", "NN", "ART", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Als so ein s\u00fc\u00df Geruch auf so viel Opffern lage/", "tokens": ["Als", "so", "ein", "s\u00fc\u00df", "Ge\u00b7ruch", "auf", "so", "viel", "Opf\u00b7fern", "la\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJD", "NN", "APPR", "ADV", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und meine Priesterschafft den Weyhrauch ausgestreut.", "tokens": ["Und", "mei\u00b7ne", "Pries\u00b7ter\u00b7schafft", "den", "Wey\u00b7hrauch", "aus\u00b7ge\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Kirche schwebt in Wonn\u2019 und unergr\u00fcndten Freuden/", "tokens": ["Die", "Kir\u00b7che", "schwebt", "in", "Wonn'", "und", "un\u00b7er\u00b7gr\u00fcnd\u00b7ten", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Himmels-Liebe wies sich scheinbar umbs Altar/", "tokens": ["Die", "Him\u00b7mels\u00b7Lie\u00b7be", "wies", "sich", "schein\u00b7bar", "umbs", "Al\u00b7tar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Jetzt aber soll in Flor mein Heiligthum sich kleiden/", "tokens": ["Jetzt", "a\u00b7ber", "soll", "in", "Flor", "mein", "Hei\u00b7lig\u00b7thum", "sich", "klei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "APPR", "NN", "PPOSAT", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und vor dem Predigtstuhl erscheint mir Sarg und Bahr.", "tokens": ["Und", "vor", "dem", "Pre\u00b7digt\u00b7stuhl", "er\u00b7scheint", "mir", "Sarg", "und", "Bahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich traure nicht vermummt/ wie die verstellten Hertzen/", "tokens": ["Ich", "trau\u00b7re", "nicht", "ver\u00b7mummt", "/", "wie", "die", "ver\u00b7stell\u00b7ten", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "VVPP", "$(", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der Augen reiche Bach entdeckt der Seelen Ri\u00df/", "tokens": ["Der", "Au\u00b7gen", "rei\u00b7che", "Bach", "ent\u00b7deckt", "der", "See\u00b7len", "Ri\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sie sind auch nicht so stumm/ es reden tieffe Schmertzen/", "tokens": ["Sie", "sind", "auch", "nicht", "so", "stumm", "/", "es", "re\u00b7den", "tief\u00b7fe", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$(", "PPER", "VVINF", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die Wunde sperrt sich auf nach eingesetztem Bi\u00df.", "tokens": ["Die", "Wun\u00b7de", "sperrt", "sich", "auf", "nach", "ein\u00b7ge\u00b7setz\u00b7tem", "Bi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Doch Thr\u00e4nen thun es nicht/ der Weiber beste W\u00fcrtze/", "tokens": ["Doch", "Thr\u00e4\u00b7nen", "thun", "es", "nicht", "/", "der", "Wei\u00b7ber", "bes\u00b7te", "W\u00fcrt\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKNEG", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df ich den theuren Mann damit soll salben ein/", "tokens": ["Da\u00df", "ich", "den", "theu\u00b7ren", "Mann", "da\u00b7mit", "soll", "sal\u00b7ben", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PAV", "VMFIN", "VVINF", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich wei\u00df da\u00df klagen nur der Todten Ruhm verk\u00fcrtze/", "tokens": ["Ich", "wei\u00df", "da\u00df", "kla\u00b7gen", "nur", "der", "Tod\u00b7ten", "Ruhm", "ver\u00b7k\u00fcrt\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "VVFIN", "ADV", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df Jammer und Verdienst ungerne Nachbarn seyn.", "tokens": ["Da\u00df", "Jam\u00b7mer", "und", "Ver\u00b7dienst", "un\u00b7ger\u00b7ne", "Nach\u00b7barn", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Das herrliche Ger\u00fccht/ so meinen Lehrer zieret/", "tokens": ["Das", "herr\u00b7li\u00b7che", "Ge\u00b7r\u00fccht", "/", "so", "mei\u00b7nen", "Leh\u00b7rer", "zie\u00b7ret", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ist w\u00fcrdig da\u00df es auch der Nachwelt werde kund/", "tokens": ["Ist", "w\u00fcr\u00b7dig", "da\u00df", "es", "auch", "der", "Nach\u00b7welt", "wer\u00b7de", "kund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wie er zum Beyspiel hat de\u00df Lebens-Lauff vollf\u00fchret/", "tokens": ["Wie", "er", "zum", "Bey\u00b7spiel", "hat", "de\u00df", "Le\u00b7bens\u00b7Lauff", "voll\u00b7f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VAFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wie GOtt mit Menschen sich bespracht durch seinen Mund.", "tokens": ["Wie", "Gott", "mit", "Men\u00b7schen", "sich", "be\u00b7spracht", "durch", "sei\u00b7nen", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die Quellen Jsraels sind durch ihn hell geflossen/", "tokens": ["Die", "Quel\u00b7len", "Js\u00b7raels", "sind", "durch", "ihn", "hell", "ge\u00b7flos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "APPR", "PPER", "ADJD", "VVPP", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Sein Finger hat mit Flei\u00df de\u00df Lebens Brunn entdeckt/", "tokens": ["Sein", "Fin\u00b7ger", "hat", "mit", "Flei\u00df", "de\u00df", "Le\u00b7bens", "Brunn", "ent\u00b7deckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und ein verschmachtend Hertz mit diesem Thau begossen/", "tokens": ["Und", "ein", "ver\u00b7schmach\u00b7tend", "Hertz", "mit", "die\u00b7sem", "Thau", "be\u00b7gos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVPP", "NN", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Dem Jesse Wurtzel gibt/ der nach dem Himmel schmeckt.", "tokens": ["Dem", "Jes\u00b7se", "Wurt\u00b7zel", "gibt", "/", "der", "nach", "dem", "Him\u00b7mel", "schmeckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Der Moyses unsrer Zeit/ hat offt das Volck vers\u00f6hnet/", "tokens": ["Der", "Moy\u00b7ses"