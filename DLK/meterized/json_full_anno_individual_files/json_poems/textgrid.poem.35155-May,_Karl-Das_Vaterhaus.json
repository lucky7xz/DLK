{"textgrid.poem.35155": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Das Vaterhaus", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hab Dank, hab Dank, du liebes Vaterhaus!", "tokens": ["Hab", "Dank", ",", "hab", "Dank", ",", "du", "lie\u00b7bes", "Va\u00b7ter\u00b7haus", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VAFIN", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du schirmtest meiner Jugend selges Leben.", "tokens": ["Du", "schirm\u00b7test", "mei\u00b7ner", "Ju\u00b7gend", "sel\u00b7ges", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie denk ich dein und denk es doch nicht aus,", "tokens": ["Wie", "denk", "ich", "dein", "und", "denk", "es", "doch", "nicht", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "KON", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was du mir warst und was du mir gegeben.", "tokens": ["Was", "du", "mir", "warst", "und", "was", "du", "mir", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VAFIN", "KON", "PWS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Kein einziger von allen Erdenorten", "tokens": ["Kein", "ein\u00b7zi\u00b7ger", "von", "al\u00b7len", "Er\u00b7de\u00b7nor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wird jemals so, wie du, mir heilig sein,", "tokens": ["Wird", "je\u00b7mals", "so", ",", "wie", "du", ",", "mir", "hei\u00b7lig", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$,", "PWAV", "PPER", "$,", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Denn was ich bin, bin ich durch dich geworden;", "tokens": ["Denn", "was", "ich", "bin", ",", "bin", "ich", "durch", "dich", "ge\u00b7wor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VAFIN", "$,", "VAFIN", "PPER", "APPR", "PPER", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das wei\u00df ich nun; ich seh es endlich ein.", "tokens": ["Das", "wei\u00df", "ich", "nun", ";", "ich", "seh", "es", "end\u00b7lich", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich mu\u00dfte fort von dir und meinem Gl\u00fcck,", "tokens": ["Ich", "mu\u00df\u00b7te", "fort", "von", "dir", "und", "mei\u00b7nem", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKVZ", "APPR", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hab nirgendwo ein andres Gl\u00fcck gefunden.", "tokens": ["Hab", "nir\u00b7gend\u00b7wo", "ein", "and\u00b7res", "Gl\u00fcck", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann kehrte ich zu sp\u00e4t, zu sp\u00e4t zur\u00fcck,", "tokens": ["Dann", "kehr\u00b7te", "ich", "zu", "sp\u00e4t", ",", "zu", "sp\u00e4t", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn als ich kam, da warest du verschwunden.", "tokens": ["Denn", "als", "ich", "kam", ",", "da", "wa\u00b7rest", "du", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch hab ich dich ganz so, wie du gewesen,", "tokens": ["Doch", "hab", "ich", "dich", "ganz", "so", ",", "wie", "du", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "ADV", "ADV", "$,", "PWAV", "PPER", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In meinem Herzen wieder aufgebaut", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen", "wie\u00b7der", "auf\u00b7ge\u00b7baut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und steig zu dir hinab, um zu genesen,", "tokens": ["Und", "steig", "zu", "dir", "hin\u00b7ab", ",", "um", "zu", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wenn ich mich an der Welt hab krank geschaut.", "tokens": ["Wenn", "ich", "mich", "an", "der", "Welt", "hab", "krank", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hab Dank, hab Dank, du lichtes Vaterhaus!", "tokens": ["Hab", "Dank", ",", "hab", "Dank", ",", "du", "lich\u00b7tes", "Va\u00b7ter\u00b7haus", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VAFIN", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du warst ein Gnadenwerk, von Gott geschehen.", "tokens": ["Du", "warst", "ein", "Gna\u00b7den\u00b7werk", ",", "von", "Gott", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du dehntest deine Hallen um mich aus,", "tokens": ["Du", "dehn\u00b7test", "dei\u00b7ne", "Hal\u00b7len", "um", "mich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ich, das Kind, ich hab sie nicht gesehen.", "tokens": ["Und", "ich", ",", "das", "Kind", ",", "ich", "hab", "sie", "nicht", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch, als ich dann die R\u00e4ume meiner Jugend", "tokens": ["Doch", ",", "als", "ich", "dann", "die", "R\u00e4u\u00b7me", "mei\u00b7ner", "Ju\u00b7gend"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bei meiner Heimkehr nicht mehr stehen sah,", "tokens": ["Bei", "mei\u00b7ner", "Heim\u00b7kehr", "nicht", "mehr", "ste\u00b7hen", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da standest du, hell wie die Himmelstugend,", "tokens": ["Da", "stan\u00b7dest", "du", ",", "hell", "wie", "die", "Him\u00b7mels\u00b7tu\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Vor meinem innern Auge pl\u00f6tzlich da.", "tokens": ["Vor", "mei\u00b7nem", "in\u00b7nern", "Au\u00b7ge", "pl\u00f6tz\u00b7lich", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich sehne mich hinauf, zu dir empor.", "tokens": ["Ich", "seh\u00b7ne", "mich", "hin\u00b7auf", ",", "zu", "dir", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich glaube ja und m\u00f6chte gern auch hoffen.", "tokens": ["Ich", "glau\u00b7be", "ja", "und", "m\u00f6ch\u00b7te", "gern", "auch", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie gro\u00df, wie weitge\u00f6ffnet ist dein Thor;", "tokens": ["Wie", "gro\u00df", ",", "wie", "weit\u00b7ge\u00f6ff\u00b7net", "ist", "dein", "Thor", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "O st\u00fcnde es f\u00fcr mich, f\u00fcr mich auch offen!", "tokens": ["O", "st\u00fcn\u00b7de", "es", "f\u00fcr", "mich", ",", "f\u00fcr", "mich", "auch", "of\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPER", "$,", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich seh den Weg und will ihn ernstlich gehen;", "tokens": ["Ich", "seh", "den", "Weg", "und", "will", "ihn", "ernst\u00b7lich", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der H\u00fcter winkt; er l\u00e4\u00dft, er l\u00e4\u00dft mich ein!", "tokens": ["Der", "H\u00fc\u00b7ter", "winkt", ";", "er", "l\u00e4\u00dft", ",", "er", "l\u00e4\u00dft", "mich", "ein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich mu\u00df hinauf, darf nicht mehr wartend stehen;", "tokens": ["Ich", "mu\u00df", "hin\u00b7auf", ",", "darf", "nicht", "mehr", "war\u00b7tend", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "VMFIN", "PTKNEG", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich will in meinem Vaterhause sein!", "tokens": ["Ich", "will", "in", "mei\u00b7nem", "Va\u00b7ter\u00b7hau\u00b7se", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Hab Dank, hab Dank, du liebes Vaterhaus!", "tokens": ["Hab", "Dank", ",", "hab", "Dank", ",", "du", "lie\u00b7bes", "Va\u00b7ter\u00b7haus", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VAFIN", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du schirmtest meiner Jugend selges Leben.", "tokens": ["Du", "schirm\u00b7test", "mei\u00b7ner", "Ju\u00b7gend", "sel\u00b7ges", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie denk ich dein und denk es doch nicht aus,", "tokens": ["Wie", "denk", "ich", "dein", "und", "denk", "es", "doch", "nicht", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER",