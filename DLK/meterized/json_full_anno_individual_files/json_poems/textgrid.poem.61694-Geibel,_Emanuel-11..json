{"textgrid.poem.61694": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wecke, wecke die Sehnsucht nicht!", "tokens": ["We\u00b7cke", ",", "we\u00b7cke", "die", "Sehn\u00b7sucht", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "La\u00df mich meiden dein Angesicht,", "tokens": ["La\u00df", "mich", "mei\u00b7den", "dein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Meine Seele zu wahren!", "tokens": ["Mei\u00b7ne", "See\u00b7le", "zu", "wah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Nicht ertr\u00fcg' ich der Stimme Laut,", "tokens": ["Nicht", "er\u00b7tr\u00fcg'", "ich", "der", "Stim\u00b7me", "Laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "APPR", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Die dein Heimlichstes mir vertraut,", "tokens": ["Die", "dein", "Heim\u00b7lichs\u00b7tes", "mir", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "ADJD", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Ach, vor Jahren, vor Jahren.", "tokens": ["Ach", ",", "vor", "Jah\u00b7ren", ",", "vor", "Jah\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Was dein bebender Mund gestand,", "tokens": ["Was", "dein", "be\u00b7ben\u00b7der", "Mund", "ge\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ich gl\u00fchend am Waldesrand", "tokens": ["Als", "ich", "gl\u00fc\u00b7hend", "am", "Wal\u00b7des\u00b7rand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Dir zu F\u00fc\u00dfen gesessen,", "tokens": ["Dir", "zu", "F\u00fc\u00b7\u00dfen", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Was beim Scheiden im Burggemach", "tokens": ["Was", "beim", "Schei\u00b7den", "im", "Burg\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPRART", "NN", "APPRART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Mir dein str\u00f6mendes Auge sprach,", "tokens": ["Mir", "dein", "str\u00f6\u00b7men\u00b7des", "Au\u00b7ge", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Nimmer kann ich's vergessen.", "tokens": ["Nim\u00b7mer", "kann", "ich's", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Ach, drum rufe mich nicht zur\u00fcck!", "to