{"dta.poem.161": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Silvien  da sie den Tod eines Sperlings  \n beweinete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "So h\u00f6ret doch einst auf den Vogel zu beklagen", "tokens": ["So", "h\u00f6\u00b7ret", "doch", "einst", "auf", "den", "Vo\u00b7gel", "zu", "be\u00b7kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+++-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Und la\u00dft das Augen-Paar mit Weinen stille seyn/", "tokens": ["Und", "la\u00dft", "das", "Au\u00b7gen\u00b7Paar", "mit", "Wei\u00b7nen", "stil\u00b7le", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "APPR", "NN", "VVFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr habet nichts von Noht und herber Angst zu sagen/", "tokens": ["Ihr", "ha\u00b7bet", "nichts", "von", "Noht", "und", "her\u00b7ber", "Angst", "zu", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "NN", "KON", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ermuntert euren Geist/ und stellt das Hermen ein;", "tokens": ["Er\u00b7mun\u00b7tert", "eu\u00b7ren", "Geist", "/", "und", "stellt", "das", "Her\u00b7men", "ein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Vogel ists nicht werth was ihr um ihn ver\u00fcbet/", "tokens": ["Der", "Vo\u00b7gel", "ists", "nicht", "werth", "was", "ihr", "um", "ihn", "ver\u00b7\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PTKNEG", "ADJD", "PWS", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wendt jene Kostbarkeit der Thr\u00e4nen besser an.", "tokens": ["Wendt", "je\u00b7ne", "Kost\u00b7bar\u00b7keit", "der", "Thr\u00e4\u00b7nen", "bes\u00b7ser", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und weil euch lang genug die treue Brust geliebet/", "tokens": ["Und", "weil", "euch", "lang", "ge\u00b7nug", "die", "treu\u00b7e", "Brust", "ge\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So g\u00f6nnet/ da\u00df ich euch vor ihn bedienen kan.", "tokens": ["So", "g\u00f6n\u00b7net", "/", "da\u00df", "ich", "euch", "vor", "ihn", "be\u00b7die\u00b7nen", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PPER", "PRF", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}