{"dta.poem.5321": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Wieder-Schein.  \n Nachdem  B.  bey Erblickung sch\u00f6ner, in einem  \n klaren Wasser sich spiegelnder B\u00e4ume  \n  A.  zu deren Betrachtung aufzu-  \n muntern gesucht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du machst von diesem Schein und seiner Sch\u00f6nheit\nmir", "tokens": ["Du", "machst", "von", "die\u00b7sem", "Schein", "und", "sei\u00b7ner", "Sch\u00f6n\u00b7heit", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "KON", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So viele Wort\u2019, und bringst so viel Erzehlens f\u00fcr:", "tokens": ["So", "vie\u00b7le", "Wort'", ",", "und", "bringst", "so", "viel", "Er\u00b7zeh\u00b7lens", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "KON", "VVFIN", "ADV", "PIAT", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da dennoch alle Pracht nichts wesentlichs; ein Schein,", "tokens": ["Da", "den\u00b7noch", "al\u00b7le", "Pracht", "nichts", "we\u00b7sent\u00b7lichs", ";", "ein", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "PIS", "PIS", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und weiter nichts.", "tokens": ["Und", "wei\u00b7ter", "nichts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die\u00df ist zwar wahr; allein", "tokens": ["Die\u00df", "ist", "zwar", "wahr", ";", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$.", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mir dienet dieser Schein selbst durch die Nichtigkeit,", "tokens": ["Mir", "die\u00b7net", "die\u00b7ser", "Schein", "selbst", "durch", "die", "Nich\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Indem er mich aufs Urbild f\u00fchret,", "tokens": ["In\u00b7dem", "er", "mich", "aufs", "Ur\u00b7bild", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So leider, ungeacht\u2019t der Sch\u00f6n- und Seltenheit,", "tokens": ["So", "lei\u00b7der", ",", "un\u00b7ge\u00b7acht't", "der", "Sch\u00f6n", "und", "Sel\u00b7ten\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJD", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Blos durch Gewohnheit mich bi\u00dfhero nicht ger\u00fchret.", "tokens": ["Blos", "durch", "Ge\u00b7wohn\u00b7heit", "mich", "bi\u00df\u00b7he\u00b7ro", "nicht", "ge\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Ja es liegt in der wandelbahren Klarheit", "tokens": ["Ja", "es", "liegt", "in", "der", "wan\u00b7del\u00b7bah\u00b7ren", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Von diesem Schein noch eine gr\u00f6ssre Wahrheit:", "tokens": ["Von", "die\u00b7sem", "Schein", "noch", "ei\u00b7ne", "gr\u00f6ss\u00b7re", "Wahr\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das Jrdische, so gleichfals fl\u00fccht- und nichtig,", "tokens": ["Das", "Jr\u00b7di\u00b7sche", ",", "so", "gleich\u00b7fals", "fl\u00fccht", "und", "nich\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "TRUNC", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sollt uns mit Recht, wie hier der Schein", "tokens": ["Sollt", "uns", "mit", "Recht", ",", "wie", "hier", "der", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "NN", "$,", "PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Uns auf das Urbild f\u00fchrt, zu dem allein", "tokens": ["Uns", "auf", "das", "Ur\u00b7bild", "f\u00fchrt", ",", "zu", "dem", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,", "APPR", "PRELS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Unwandelbar- und wesentlichem Seyn,", "tokens": ["Un\u00b7wan\u00b7del\u00b7ba\u00b7r", "und", "we\u00b7sent\u00b7li\u00b7chem", "Seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+---+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Dem Ursprung aller Herrlichkeiten,", "tokens": ["Dem", "Ur\u00b7sprung", "al\u00b7ler", "Herr\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Durch die von ihm erschaffne Sch\u00f6nheit, leiten.", "tokens": ["Durch", "die", "von", "ihm", "er\u00b7schaff\u00b7ne", "Sch\u00f6n\u00b7heit", ",", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "APPR", "PPER", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}