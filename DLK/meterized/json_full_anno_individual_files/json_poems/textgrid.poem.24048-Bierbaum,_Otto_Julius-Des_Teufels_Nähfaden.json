r": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hui, wie er den Faden dar\u00fcber zieht", "tokens": ["Hui", ",", "wie", "er", "den", "Fa\u00b7den", "da\u00b7r\u00fc\u00b7ber", "zieht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "$,", "PWAV", "PPER", "ART", "NN", "PAV", "VVFIN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Mit seiner krummen Klaue, und wie er pr\u00fcft", "tokens": ["Mit", "sei\u00b7ner", "krum\u00b7men", "Klau\u00b7e", ",", "und", "wie", "er", "pr\u00fcft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "KON", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ob er fest und geschmeidig.", "tokens": ["Ob", "er", "fest", "und", "ge\u00b7schmei\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "KON", "ADJD", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Wo hat der Teufel den Faden her,", "tokens": ["Wo", "hat", "der", "Teu\u00b7fel", "den", "Fa\u00b7den", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Sackleinfaden, mit dem er n\u00e4ht?", "tokens": ["Den", "Sack\u00b7lein\u00b7fa\u00b7den", ",", "mit", "dem", "er", "n\u00e4ht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Er hat ihn gedreht aus den Seelen der H\u00e4mischen,", "tokens": ["Er", "hat", "ihn", "ge\u00b7dreht", "aus", "den", "See\u00b7len", "der", "H\u00e