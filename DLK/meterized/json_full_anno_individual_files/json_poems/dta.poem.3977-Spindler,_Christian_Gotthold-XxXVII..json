{"dta.poem.3977": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "XxXVII.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Geh hin, getreues Blat, geh, k\u00fc\u00df die zarten H\u00e4nde", "tokens": ["Geh", "hin", ",", "ge\u00b7treu\u00b7es", "Blat", ",", "geh", ",", "k\u00fc\u00df", "die", "zar\u00b7ten", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der sch\u00f6nsten Dorilis; da ich dich schleunigst", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "Do\u00b7ri\u00b7lis", ";", "da", "ich", "dich", "schleu\u00b7nigst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "$.", "KOUS", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "sende,", "tokens": ["sen\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Geh, bring den treusten Ku\u00df, geh, halte dich nicht", "tokens": ["Geh", ",", "bring", "den", "treus\u00b7ten", "Ku\u00df", ",", "geh", ",", "hal\u00b7te", "dich", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "auf,", "tokens": ["auf", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Cupido sey dein Schutz, der fl\u00fcgle deinen Lauf.", "tokens": ["Cu\u00b7pi\u00b7do", "sey", "dein", "Schutz", ",", "der", "fl\u00fcg\u00b7le", "dei\u00b7nen", "Lauf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}