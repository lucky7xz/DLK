{"textgrid.poem.24278": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Lebenszwiegesang", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber H\u00fcgel, sanft gebogen,", "tokens": ["\u00dc\u00b7ber", "H\u00fc\u00b7gel", ",", "sanft", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drauf der Sonne Lachen lag,", "tokens": ["Drauf", "der", "Son\u00b7ne", "La\u00b7chen", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin als Knabe ich gezogen,", "tokens": ["Bin", "als", "Kna\u00b7be", "ich", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOUS", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Froh gezogen,", "tokens": ["Froh", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Singend in den hellen Tag.", "tokens": ["Sin\u00b7gend", "in", "den", "hel\u00b7len", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Singe, singe, singe die Sonne an!", "tokens": ["Sin\u00b7ge", ",", "sin\u00b7ge", ",", "sin\u00b7ge", "die", "Son\u00b7ne", "an", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Siehe, siehe, wieviel sie kann.", "tokens": ["Sie\u00b7he", ",", "sie\u00b7he", ",", "wie\u00b7viel", "sie", "kann", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVIMP", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Berge nahm ich mir zum Ziele,", "tokens": ["Ber\u00b7ge", "nahm", "ich", "mir", "zum", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ich J\u00fcngling worden war,", "tokens": ["Als", "ich", "J\u00fcng\u00b7ling", "wor\u00b7den", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich fand auf ihnen viele,", "tokens": ["Und", "ich", "fand", "auf", "ih\u00b7nen", "vie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wunderviele", "tokens": ["Wun\u00b7der\u00b7vie\u00b7le"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Lieder, wie die H\u00f6hen klar.", "tokens": ["Lie\u00b7der", ",", "wie", "die", "H\u00f6\u00b7hen", "klar", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "St\u00fcrme, st\u00fcrme, st\u00fcrme den H\u00f6hen zu!", "tokens": ["St\u00fcr\u00b7me", ",", "st\u00fcr\u00b7me", ",", "st\u00fcr\u00b7me", "den", "H\u00f6\u00b7hen", "zu", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Oben, oben bist ruhig du.", "tokens": ["O\u00b7ben", ",", "o\u00b7ben", "bist", "ru\u00b7hig", "du", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "ADJD", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Doch ein See war, da ich ruhte,", "tokens": ["Doch", "ein", "See", "war", ",", "da", "ich", "ruh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhte als ein junger Mann.", "tokens": ["Ruh\u00b7te", "als", "ein", "jun\u00b7ger", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort nahm ich der Liebe gute,", "tokens": ["Dort", "nahm", "ich", "der", "Lie\u00b7be", "gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "O ja, gute", "tokens": ["O", "ja", ",", "gu\u00b7te"], "token_info": ["word", "word", "punct", "word"], "pos": ["ITJ", "ITJ", "$,", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Gaben staunend dankbar an.", "tokens": ["Ga\u00b7ben", "stau\u00b7nend", "dank\u00b7bar", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Trinke, trinke, trinke den Becher leer!", "tokens": ["Trin\u00b7ke", ",", "trin\u00b7ke", ",", "trin\u00b7ke", "den", "Be\u00b7cher", "leer", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Balde, balde hast ihn nicht mehr.", "tokens": ["Bal\u00b7de", ",", "bal\u00b7de", "hast", "ihn", "nicht", "mehr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Und ich zog mit meinem Glase", "tokens": ["Und", "ich", "zog", "mit", "mei\u00b7nem", "Gla\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die tiefste Einsamkeit.", "tokens": ["In", "die", "tiefs\u00b7te", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fern dem Staube und der Stra\u00dfe,", "tokens": ["Fern", "dem", "Stau\u00b7be", "und", "der", "Stra\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fern der Stra\u00dfe", "tokens": ["Fern", "der", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Trank ich Glut der Sommerzeit.", "tokens": ["Trank", "ich", "Glut", "der", "Som\u00b7mer\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Liebe, liebe, liebe! Sauge das Sein!", "tokens": ["Lie\u00b7be", ",", "lie\u00b7be", ",", "lie\u00b7be", "!", "Sau\u00b7ge", "das", "Sein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$.", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Morgen, morgen bist du allein.", "tokens": ["Mor\u00b7gen", ",", "mor\u00b7gen", "bist", "du", "al\u00b7lein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Und ich hab mein Glas verloren,", "tokens": ["Und", "ich", "hab", "mein", "Glas", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich kroch im Kreis herum,", "tokens": ["Und", "ich", "kroch", "im", "Kreis", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Ward verdrossen mit den Toren,", "tokens": ["Ward", "ver\u00b7dros\u00b7sen", "mit", "den", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tristen Toren,", "tokens": ["Tris\u00b7ten", "To\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Und so ward ich alt und stumm.", "tokens": ["Und", "so", "ward", "ich", "alt", "und", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Schaue, schaue, schaue den Weg zur\u00fcck!", "tokens": ["Schau\u00b7e", ",", "schau\u00b7e", ",", "schau\u00b7e", "den", "Weg", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Hinten, hinten glimmert das Gl\u00fcck.", "tokens": ["Hin\u00b7ten", ",", "hin\u00b7ten", "glim\u00b7mert", "das", "Gl\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.11": {"line.1": {"text": "Wieder H\u00fcgel, wieder Berge,", "tokens": ["Wie\u00b7der", "H\u00fc\u00b7gel", ",", "wie\u00b7der", "Ber\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder See und Einsamkeit.", "tokens": ["Wie\u00b7der", "See", "und", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00e4rge, S\u00e4rge, leere S\u00e4rge!", "tokens": ["S\u00e4r\u00b7ge", ",", "S\u00e4r\u00b7ge", ",", "lee\u00b7re", "S\u00e4r\u00b7ge", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leere S\u00e4rge ...", "tokens": ["Lee\u00b7re", "S\u00e4r\u00b7ge", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Greis, dein Weg ist nicht mehr weit.", "tokens": ["Greis", ",", "dein", "Weg", "ist", "nicht", "mehr", "weit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schreite, schreite, schreite dem Dunkel zu!", "tokens": ["Schrei\u00b7te", ",", "schrei\u00b7te", ",", "schrei\u00b7te", "dem", "Dun\u00b7kel", "zu", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Unten, unten d\u00e4mmert die Ruh.", "tokens": ["Un\u00b7ten", ",", "un\u00b7ten", "d\u00e4m\u00b7mert", "die", "Ruh", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.12": {"line.1": {"text": "\u00dcber H\u00fcgel, sanft gebogen,", "tokens": ["\u00dc\u00b7ber", "H\u00fc\u00b7gel", ",", "sanft", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drauf der Sonne Lachen lag,", "tokens": ["Drauf", "der", "Son\u00b7ne", "La\u00b7chen", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin als Knabe ich gezogen,", "tokens": ["Bin", "als", "Kna\u00b7be", "ich", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOUS", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Froh gezogen,", "tokens": ["Froh", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Singend in den hellen Tag.", "tokens": ["Sin\u00b7gend", "in", "den", "hel\u00b7len", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Singe, singe, singe die Sonne an!", "tokens": ["Sin\u00b7ge", ",", "sin\u00b7ge", ",", "sin\u00b7ge", "die", "Son\u00b7ne", "an", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Siehe, siehe, wieviel sie kann.", "tokens": ["Sie\u00b7he", ",", "sie\u00b7he", ",", "wie\u00b7viel", "sie", "kann", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVIMP", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Berge nahm ich mir zum Ziele,", "tokens": ["Ber\u00b7ge", "nahm", "ich", "mir", "zum", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ich J\u00fcngling worden war,", "tokens": ["Als", "ich", "J\u00fcng\u00b7ling", "wor\u00b7den", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich fand auf ihnen viele,", "tokens": ["Und", "ich", "fand", "auf", "ih\u00b7nen", "vie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wunderviele", "tokens": ["Wun\u00b7der\u00b7vie\u00b7le"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Lieder, wie die H\u00f6hen klar.", "tokens": ["Lie\u00b7der", ",", "wie", "die", "H\u00f6\u00b7hen", "klar", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "St\u00fcrme, st\u00fcrme, st\u00fcrme den H\u00f6hen zu!", "tokens": ["St\u00fcr\u00b7me", ",", "st\u00fcr\u00b7me", ",", "st\u00fcr\u00b7me", "den", "H\u00f6\u00b7hen", "zu", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Oben, oben bist ruhig du.", "tokens": ["O\u00b7ben", ",", "o\u00b7ben", "bist", "ru\u00b7hig", "du", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "ADJD", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.16": {"line.1": {"text": "Doch ein See war, da ich ruhte,", "tokens": ["Doch", "ein", "See", "war", ",", "da", "ich", "ruh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhte als ein junger Mann.", "tokens": ["Ruh\u00b7te", "als", "ein", "jun\u00b7ger", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort nahm ich der Liebe gute,", "tokens": ["Dort", "nahm", "ich", "der", "Lie\u00b7be", "gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "O ja, gute", "tokens": ["O", "ja", ",", "gu\u00b7te"], "token_info": ["word", "word", "punct", "word"], "pos": ["ITJ", "ITJ", "$,", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Gaben staunend dankbar an.", "tokens": ["Ga\u00b7ben", "stau\u00b7nend", "dank\u00b7bar", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Trinke, trinke, trinke den Becher leer!", "tokens": ["Trin\u00b7ke", ",", "trin\u00b7ke", ",", "trin\u00b7ke", "den", "Be\u00b7cher", "leer", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Balde, balde hast ihn nicht mehr.", "tokens": ["Bal\u00b7de", ",", "bal\u00b7de", "hast", "ihn", "nicht", "mehr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.18": {"line.1": {"text": "Und ich zog mit meinem Glase", "tokens": ["Und", "ich", "zog", "mit", "mei\u00b7nem", "Gla\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die tiefste Einsamkeit.", "tokens": ["In", "die", "tiefs\u00b7te", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fern dem Staube und der Stra\u00dfe,", "tokens": ["Fern", "dem", "Stau\u00b7be", "und", "der", "Stra\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fern der Stra\u00dfe", "tokens": ["Fern", "der", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Trank ich Glut der Sommerzeit.", "tokens": ["Trank", "ich", "Glut", "der", "Som\u00b7mer\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Liebe, liebe, liebe! Sauge das Sein!", "tokens": ["Lie\u00b7be", ",", "lie\u00b7be", ",", "lie\u00b7be", "!", "Sau\u00b7ge", "das", "Sein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$.", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Morgen, morgen bist du allein.", "tokens": ["Mor\u00b7gen", ",", "mor\u00b7gen", "bist", "du", "al\u00b7lein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.20": {"line.1": {"text": "Und ich hab mein Glas verloren,", "tokens": ["Und", "ich", "hab", "mein", "Glas", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich kroch im Kreis herum,", "tokens": ["Und", "ich", "kroch", "im", "Kreis", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Ward verdrossen mit den Toren,", "tokens": ["Ward", "ver\u00b7dros\u00b7sen", "mit", "den", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tristen Toren,", "tokens": ["Tris\u00b7ten", "To\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Und so ward ich alt und stumm.", "tokens": ["Und", "so", "ward", "ich", "alt", "und", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Schaue, schaue, schaue den Weg zur\u00fcck!", "tokens": ["Schau\u00b7e", ",", "schau\u00b7e", ",", "schau\u00b7e", "den", "Weg", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Hinten, hinten glimmert das Gl\u00fcck.", "tokens": ["Hin\u00b7ten", ",", "hin\u00b7ten", "glim\u00b7mert", "das", "Gl\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.22": {"line.1": {"text": "Wieder H\u00fcgel, wieder Berge,", "tokens": ["Wie\u00b7der", "H\u00fc\u00b7gel", ",", "wie\u00b7der", "Ber\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder See und Einsamkeit.", "tokens": ["Wie\u00b7der", "See", "und", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00e4rge, S\u00e4rge, leere S\u00e4rge!", "tokens": ["S\u00e4r\u00b7ge", ",", "S\u00e4r\u00b7ge", ",", "lee\u00b7re", "S\u00e4r\u00b7ge", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leere S\u00e4rge ...", "tokens": ["Lee\u00b7re", "S\u00e4r\u00b7ge", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Greis, dein Weg ist nicht mehr weit.", "tokens": ["Greis", ",", "dein", "Weg", "ist", "nicht", "mehr", "weit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schreite, schreite, schreite dem Dunkel zu!", "tokens": ["Schrei\u00b7te", ",", "schrei\u00b7te", ",", "schrei\u00b7te", "dem", "Dun\u00b7kel", "zu", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Unten, unten d\u00e4mmert die Ruh.", "tokens": ["Un\u00b7ten", ",", "un\u00b7ten", "d\u00e4m\u00b7mert", "die", "Ruh", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}