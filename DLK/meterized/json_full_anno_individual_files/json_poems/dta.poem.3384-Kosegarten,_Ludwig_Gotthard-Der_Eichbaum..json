{"dta.poem.3384": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Der Eichbaum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du Starker, du Edler,", "tokens": ["Du", "Star\u00b7ker", ",", "du", "Ed\u00b7ler", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Es gr\u00fcsst dich mein Lied.", "tokens": ["Es", "gr\u00fcs\u00b7st", "dich", "mein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du K\u00f6nig des Haynes,", "tokens": ["Du", "K\u00f6\u00b7nig", "des", "Hay\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Du Vater der Waldnacht,", "tokens": ["Du", "Va\u00b7ter", "der", "Wald\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Im d\u00e4mmernden Mondschein begr\u00fcsst dich mein Lied.", "tokens": ["Im", "d\u00e4m\u00b7mern\u00b7den", "Mond\u00b7schein", "be\u00b7gr\u00fcs\u00b7st", "dich", "mein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Du Stolzer, du Starker,", "tokens": ["Du", "Stol\u00b7zer", ",", "du", "Star\u00b7ker", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Du th\u00fcrmest dein Haupt", "tokens": ["Du", "th\u00fcr\u00b7mest", "dein", "Haupt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Seit grauenden Altern;", "tokens": ["Seit", "grau\u00b7en\u00b7den", "Al\u00b7tern", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Es streute kein G\u00e4rtner", "tokens": ["Es", "streu\u00b7te", "kein", "G\u00e4rt\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Den kr\u00e4ftigen Samen,", "tokens": ["Den", "kr\u00e4f\u00b7ti\u00b7gen", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der einst dich gebar.", "tokens": ["Der", "einst", "dich", "ge\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Es sahe kein Aug',", "tokens": ["Es", "sa\u00b7he", "kein", "Aug'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Als S\u00e4ugling, den Starken;", "tokens": ["Als", "S\u00e4ug\u00b7ling", ",", "den", "Star\u00b7ken", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Es h\u00f6rte kein Ohr", "tokens": ["Es", "h\u00f6r\u00b7te", "kein", "Ohr"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Das Pfeifen des Sch\u00f6sslings im sausenden Nord.", "tokens": ["Das", "Pfei\u00b7fen", "des", "Sch\u00f6ss\u00b7lings", "im", "sau\u00b7sen\u00b7den", "Nord", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Dort steht er, der Starke,", "tokens": ["Dort", "steht", "er", ",", "der", "Star\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und schweigt und gebeut.", "tokens": ["Und", "schweigt", "und", "ge\u00b7beut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ihn zeugte die Erde;", "tokens": ["Ihn", "zeug\u00b7te", "die", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ihn s\u00e4ugte die Sonne;", "tokens": ["Ihn", "s\u00e4ug\u00b7te", "die", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ihn wiegten liebkosend", "tokens": ["Ihn", "wieg\u00b7ten", "lieb\u00b7ko\u00b7send"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.6": {"text": "Die Winde des Himmels.", "tokens": ["Die", "Win\u00b7de", "des", "Him\u00b7mels", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Ihn kleidet der Lenz in duftendes Saftgr\u00fcn,", "tokens": ["Ihn", "klei\u00b7det", "der", "Lenz", "in", "duf\u00b7ten\u00b7des", "Saft\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "In Crocus und Saffran der Herbst.", "tokens": ["In", "Cro\u00b7cus", "und", "Saf\u00b7fran", "der", "Herbst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Sein freuten sich schon", "tokens": ["Sein", "freu\u00b7ten", "sich", "schon"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "VVFIN", "PRF", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die Kinder der Vorwelt.", "tokens": ["Die", "Kin\u00b7der", "der", "Vor\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--++", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du Grauer, der Barde besang dich, und kr\u00e4nzte", "tokens": ["Du", "Grau\u00b7er", ",", "der", "Bar\u00b7de", "be\u00b7sang", "dich", ",", "und", "kr\u00e4nz\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Mit heiligem Laube den heiligen Schlaf.", "tokens": ["Mit", "hei\u00b7li\u00b7gem", "Lau\u00b7be", "den", "hei\u00b7li\u00b7gen", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Der sterbende Barde", "tokens": ["Der", "ster\u00b7ben\u00b7de", "Bar\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Hing Horn und Harfe", "tokens": ["Hing", "Horn", "und", "Har\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Am seufzenden Ast.", "tokens": ["Am", "seuf\u00b7zen\u00b7den", "Ast", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Den liebenden J\u00fcngling,", "tokens": ["Den", "lie\u00b7ben\u00b7den", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Das liebende M\u00e4gdlein", "tokens": ["Das", "lie\u00b7ben\u00b7de", "M\u00e4gd\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Umfingst du vertraulich,", "tokens": ["Um\u00b7fingst", "du", "ver\u00b7trau\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Und h\u00fclltest in sch\u00fctzende Schatten sie ein.", "tokens": ["Und", "h\u00fcll\u00b7test", "in", "sch\u00fct\u00b7zen\u00b7de", "Schat\u00b7ten", "sie", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Dort steht er, der Starke,", "tokens": ["Dort", "steht", "er", ",", "der", "Star\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In \u00fcppiger Kraft.", "tokens": ["In", "\u00fcp\u00b7pi\u00b7ger", "Kraft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wie schattet die Krone des Wipfels!", "tokens": ["Wie", "schat\u00b7tet", "die", "Kro\u00b7ne", "des", "Wip\u00b7fels", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wie th\u00fcrmet die S\u00e4ule des Stammes!", "tokens": ["Wie", "th\u00fcr\u00b7met", "die", "S\u00e4u\u00b7le", "des", "Stam\u00b7mes", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die tausendzweigigen Wurzeln", "tokens": ["Die", "tau\u00b7send\u00b7zwei\u00b7gi\u00b7gen", "Wur\u00b7zeln"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Durchflechten der Insel verborgenstes Mark.", "tokens": ["Durch\u00b7flech\u00b7ten", "der", "In\u00b7sel", "ver\u00b7bor\u00b7gens\u00b7tes", "Mark", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Nachtsturm braust", "tokens": ["Nachts\u00b7turm", "braust"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Im Walde der Scheitel.", "tokens": ["Im", "Wal\u00b7de", "der", "Schei\u00b7tel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Es sch\u00fcttelt den Grauen, und heult in den Grotten", "tokens": ["Es", "sch\u00fct\u00b7telt", "den", "Grau\u00b7en", ",", "und", "heult", "in", "den", "Grot\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Des tausendj\u00e4hrigen Stamms.", "tokens": ["Des", "tau\u00b7send\u00b7j\u00e4h\u00b7ri\u00b7gen", "Stamms", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Die Linde verbeugt sich. Es spaltet die Pappel.", "tokens": ["Die", "Lin\u00b7de", "ver\u00b7beugt", "sich", ".", "Es", "spal\u00b7tet", "die", "Pap\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PRF", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Ulme zerschillt.", "tokens": ["Die", "Ul\u00b7me", "zer\u00b7schillt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die schwankende Tann' entwurzelt der Orkan,", "tokens": ["Die", "schwan\u00b7ken\u00b7de", "Tann'", "ent\u00b7wur\u00b7zelt", "der", "Or\u00b7kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und schleudert sie nieder ins sandige Thal.", "tokens": ["Und", "schleu\u00b7dert", "sie", "nie\u00b7der", "ins", "san\u00b7di\u00b7ge", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Baum Gottes, du stehst.", "tokens": ["Baum", "Got\u00b7tes", ",", "du", "stehst", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Baum Gottes, es gr\u00fcsst", "tokens": ["Baum", "Got\u00b7tes", ",", "es", "gr\u00fcs\u00b7st"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Dein Wipfel die Sterne.", "tokens": ["Dein", "Wip\u00b7fel", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Es webte die Wurzeln", "tokens": ["Es", "web\u00b7te", "die", "Wur\u00b7zeln"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Dein Sch\u00f6pfer die Rippen der Erde hindurch.", "tokens": ["Dein", "Sch\u00f6p\u00b7fer", "die", "Rip\u00b7pen", "der", "Er\u00b7de", "hin\u00b7durch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ART", "NN", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "Mag heulen der Orkan! Mag prasseln der Donner!", "tokens": ["Mag", "heu\u00b7len", "der", "Or\u00b7kan", "!", "Mag", "pras\u00b7seln", "der", "Don\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "$.", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mag zucken der rothe kreuzende Blitz!", "tokens": ["Mag", "zu\u00b7cken", "der", "ro\u00b7the", "kreu\u00b7zen\u00b7de", "Blitz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dir bricht sich der Orkan. Dir schweigen die Donner.", "tokens": ["Dir", "bricht", "sich", "der", "Or\u00b7kan", ".", "Dir", "schwei\u00b7gen", "die", "Don\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Es kreuzen die schonenden Blitze vorbey.", "tokens": ["Es", "kreu\u00b7zen", "die", "scho\u00b7nen\u00b7den", "Blit\u00b7ze", "vor\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "\u201eso sang ich und schwieg.", "tokens": ["\u201e", "so", "sang", "ich", "und", "schwieg", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "\u201ees neigte der Starke", "tokens": ["\u201e", "es", "neig\u00b7te", "der", "Star\u00b7ke"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ART", 