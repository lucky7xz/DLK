{"dta.poem.12792": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als  \n Der Durchlauchtigste F\u00fcrst und Herr,  \n Herr Wilhelm Friedrich, Marg-  \n graf zu Brandenburg rc. rc. durch  \n Wittenberg reisete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr l\u00fcffte! kl\u00e4rt euch aus, und hei\u00dfet unser feld", "tokens": ["Ihr", "l\u00fcff\u00b7te", "!", "kl\u00e4rt", "euch", "aus", ",", "und", "hei\u00b7\u00dfet", "un\u00b7ser", "feld"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nebst seinen gegenden mit holden blicken lachen:", "tokens": ["Nebst", "sei\u00b7nen", "ge\u00b7gen\u00b7den", "mit", "hol\u00b7den", "bli\u00b7cken", "la\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Baut von vergn\u00fcgungen ein anmuths-volles zelt,", "tokens": ["Baut", "von", "ver\u00b7gn\u00fc\u00b7gun\u00b7gen", "ein", "an\u00b7muths\u00b7vol\u00b7les", "zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "VVFIN", "ART", "ADJA", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Und last heut alle lust der sch\u00f6nen zeit erwachen:", "tokens": ["Und", "last", "heut", "al\u00b7le", "lust", "der", "sch\u00f6\u00b7nen", "zeit", "er\u00b7wa\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Streut mosch und ambra aus, und balsamirt den krei\u00df,", "tokens": ["Streut", "mosch", "und", "am\u00b7bra", "aus", ",", "und", "bal\u00b7sa\u00b7mirt", "den", "krei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "NE", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der nur itzund von lust und hertzens-w\u00fcnschen wei\u00df.", "tokens": ["Der", "nur", "it\u00b7zund", "von", "lust", "und", "hert\u00b7zens\u00b7w\u00fcn\u00b7schen", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "NN", "KON", "ADJA", "VVFIN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Wie ist der himmel doch der Francken brust geneigt,", "tokens": ["Wie", "ist", "der", "him\u00b7mel", "doch", "der", "Fran\u00b7cken", "brust", "ge\u00b7neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ihr sein vater-sinn hat Anspachs hohe Sonnen", "tokens": ["Da\u00df", "ihr", "sein", "va\u00b7ter\u00b7sinn", "hat", "An\u00b7spachs", "ho\u00b7he", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In unserm Horizont und Sions-berg gezeigt,", "tokens": ["In", "un\u00b7serm", "Ho\u00b7ri\u00b7zont", "und", "Si\u00b7ons\u00b7berg", "ge\u00b7zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und uns dadurch den schatz des gl\u00fcckes giebt gewonnen:", "tokens": ["Und", "uns", "da\u00b7durch", "den", "schatz", "des", "gl\u00fc\u00b7ckes", "giebt", "ge\u00b7won\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PAV", "ART", "NN", "ART", "ADJA", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie wird der ort von uns in tieffster pflicht gek\u00fc\u00dft,", "tokens": ["Wie", "wird", "der", "ort", "von", "uns", "in", "tieffs\u00b7ter", "pflicht", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo Wilhelmina war, und Wilhelm Friedrich ist.", "tokens": ["Wo", "Wil\u00b7hel\u00b7mi\u00b7na", "war", ",", "und", "Wil\u00b7helm", "Fried\u00b7rich", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VAFIN", "$,", "KON", "NE", "NE", "VAFIN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Durchlauchtes F\u00fcrsten-Haupt, und hoher Helden-Geist,", "tokens": ["Durch\u00b7lauch\u00b7tes", "F\u00fcrs\u00b7ten\u00b7Haupt", ",", "und", "ho\u00b7her", "Hel\u00b7den\u00b7Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den n\u00e4chst der himmel hie\u00df des landes Vater werden,", "tokens": ["Den", "n\u00e4chst", "der", "him\u00b7mel", "hie\u00df", "des", "lan\u00b7des", "Va\u00b7ter", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie wurde sinn und brust mit g\u00f6tter-kost gespeist,", "tokens": ["Wie", "wur\u00b7de", "sinn", "und", "brust", "mit", "g\u00f6t\u00b7ter\u00b7kost", "ge\u00b7speist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "KON", "VVFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als wie des landes wohl und lustige geberden,", "tokens": ["Als", "wie", "des", "lan\u00b7des", "wohl", "und", "lus\u00b7ti\u00b7ge", "ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "ADV", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So ihm der himmel hat durch dich, o Held! verehrt,", "tokens": ["So", "ihm", "der", "him\u00b7mel", "hat", "durch", "dich", ",", "o", "Held", "!", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "VAFIN", "APPR", "PPER", "$,", "FM", "NN", "$.", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch einen freuden-schall der gantzen welt geh\u00f6rt.", "tokens": ["Durch", "ei\u00b7nen", "freu\u00b7den\u00b7schall", "der", "gant\u00b7zen", "welt", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch itzt lacht uns das gl\u00fcck mit mehrern strahlen an,", "tokens": ["Doch", "itzt", "lacht", "uns", "das", "gl\u00fcck", "mit", "meh\u00b7rern", "strah\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil unsre brust sich selbst zu deinen f\u00fc\u00dfen legen,", "tokens": ["Weil", "uns\u00b7re", "brust", "sich", "selbst", "zu", "dei\u00b7nen", "f\u00fc\u00b7\u00dfen", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und deinem scepter heyl und segen w\u00fcnschen kan,", "tokens": ["Und", "dei\u00b7nem", "scep\u00b7ter", "heyl", "und", "se\u00b7gen", "w\u00fcn\u00b7schen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "KON", "VVINF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Damit kein naher sturm kan seinen grund bewegen,", "tokens": ["Da\u00b7mit", "kein", "na\u00b7her", "sturm", "kan", "sei\u00b7nen", "grund", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit unsrer meynung stimmt der himmel selbsten ein,", "tokens": ["Mit", "uns\u00b7rer", "mey\u00b7nung", "stimmt", "der", "him\u00b7mel", "selbs\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was wunder! wenn dein wohl wird diamanten seyn.", "tokens": ["Was", "wun\u00b7der", "!", "wenn", "dein", "wohl", "wird", "di\u00b7a\u00b7man\u00b7ten", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$.", "KOUS", "PPOSAT", "ADV", "VAFIN", "ADJD", "VAINF", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Dis w\u00fcnscht der unterthan, dis hofft das edle land,", "tokens": ["Dis", "w\u00fcnscht", "der", "un\u00b7ter\u00b7than", ",", "dis", "hofft", "das", "ed\u00b7le", "land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJD", "$,", "PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und unser hertze singt bereits die freuden-lieder,", "tokens": ["Und", "un\u00b7ser", "hert\u00b7ze", "singt", "be\u00b7reits", "die", "freu\u00b7den\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil sich der himmel selbst durch dich, o Werthes Pfand!", "tokens": ["Weil", "sich", "der", "him\u00b7mel", "selbst", "durch", "dich", ",", "o", "Wert\u00b7hes", "Pfand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "APPR", "PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf unsre felder l\u00e4st nebst dem gel\u00fccke nieder,", "tokens": ["Auf", "uns\u00b7re", "fel\u00b7der", "l\u00e4st", "nebst", "dem", "ge\u00b7l\u00fc\u00b7cke", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man h\u00f6ret, wie mit uns, durch die zertheilte lufft,", "tokens": ["Man", "h\u00f6\u00b7ret", ",", "wie", "mit", "uns", ",", "durch", "die", "zer\u00b7theil\u00b7te", "lufft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "APPR", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jedwedes landes-kind dergleichen w\u00fcnsche rufft:", "tokens": ["Jed\u00b7we\u00b7des", "lan\u00b7des\u00b7kind", "derg\u00b7lei\u00b7chen", "w\u00fcn\u00b7sche", "rufft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PIS", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der himmel wohne da, wo Wilhelm Friedrich lebt,", "tokens": ["Der", "him\u00b7mel", "woh\u00b7ne", "da", ",", "wo", "Wil\u00b7helm", "Fried\u00b7rich", "lebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PWAV", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seine felder sch\u00fctzt mit seinem hohen schatten,", "tokens": ["Und", "sei\u00b7ne", "fel\u00b7der", "sch\u00fctzt", "mit", "sei\u00b7nem", "ho\u00b7hen", "schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo er durch tugend ihm den helden-purpur webt,", "tokens": ["Wo", "er", "durch", "tu\u00b7gend", "ihm", "den", "hel\u00b7den\u00b7pur\u00b7pur", "webt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den viele seines stamms schon l\u00e4ngst erstritten hatten,", "tokens": ["Den", "vie\u00b7le", "sei\u00b7nes", "stamms", "schon", "l\u00e4ngst", "er\u00b7strit\u00b7ten", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der segen sch\u00fctte sich auf sein Durchlauchtes Hau\u00df,", "tokens": ["Der", "se\u00b7gen", "sch\u00fct\u00b7te", "sich", "auf", "sein", "Durch\u00b7lauch\u00b7tes", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Und auf das gantze land mit tausend str\u00f6men aus!", "tokens": ["Und", "auf", "das", "gant\u00b7ze", "land", "mit", "tau\u00b7send", "str\u00f6\u00b7men", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "CARD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Nun f\u00fchre dich begl\u00fcckt der himmel in die stadt", "tokens": ["Nun", "f\u00fch\u00b7re", "dich", "be\u00b7gl\u00fcckt", "der", "him\u00b7mel", "in", "die", "stadt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo dein Geschlechte Cron und K\u00f6nigs-purpur tr\u00e4get,", "tokens": ["Wo", "dein", "Ge\u00b7schlech\u00b7te", "Cron", "und", "K\u00f6\u00b7nigs\u00b7pur\u00b7pur", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo dem gel\u00fccke schon ein stern befohlen hat,", "tokens": ["Wo", "dem", "ge\u00b7l\u00fc\u00b7cke", "schon", "ein", "stern", "be\u00b7foh\u00b7len", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ART", "ADJA", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df es stets deinen wunsch mit einem ja beleget.", "tokens": ["Da\u00df", "es", "stets", "dei\u00b7nen", "wunsch", "mit", "ei\u00b7nem", "ja", "be\u00b7le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "APPR", "ART", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein wohlseyn ist der stamm, worauf die freude liegt,", "tokens": ["Dein", "wohl\u00b7seyn", "ist", "der", "stamm", ",", "wo\u00b7rauf",