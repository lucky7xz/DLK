{"textgrid.poem.39443": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbmein s\u00fc\u00dfes Kindlein, w\u00fc\u00dft' ich dein zu pflegen!", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmein s\u00fc\u00dfes Kindlein, w\u00fc\u00dft' ich dein zu pflegen!", "tokens": ["\u00bb", "mein", "s\u00fc\u00b7\u00dfes", "Kin\u00b7dlein", ",", "w\u00fc\u00dft'", "ich", "dein", "zu", "pfle\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich bin noch matt, doch ruh' am Busen warm;", "tokens": ["Ich", "bin", "noch", "matt", ",", "doch", "ruh'", "am", "Bu\u00b7sen", "warm", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "VVFIN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Nacht ist dunkel, klein die H\u00fctt' und arm:", "tokens": ["Die", "Nacht", "ist", "dun\u00b7kel", ",", "klein", "die", "H\u00fctt'", "und", "arm", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADJD", "ART", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie mu\u00dften dich in diese Krippe legen.\u00ab", "tokens": ["Sie", "mu\u00df\u00b7ten", "dich", "in", "die\u00b7se", "Krip\u00b7pe", "le\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PDAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So sprach Maria; drau\u00dfen rief's dagegen:", "tokens": ["So", "sprach", "Ma\u00b7ria", ";", "drau\u00b7\u00dfen", "rie\u00b7f's", "da\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$.", "ADV", "VVFIN", "PAV", "$."], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "La\u00dft uns hinein, wir wollen keinen Harm!", "tokens": ["La\u00dft", "uns", "hin\u00b7ein", ",", "wir", "wol\u00b7len", "kei\u00b7nen", "Harm", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "PPER", "VMFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Uns wies hieher der Engel froher Schwarm,", "tokens": ["Uns", "wies", "hie\u00b7her", "der", "En\u00b7gel", "fro\u00b7her", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verk\u00fcndigend den neugebornen Segen.", "tokens": ["Ver\u00b7k\u00fcn\u00b7di\u00b7gend", "den", "neu\u00b7ge\u00b7bor\u00b7nen", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Dach empf\u00e4ngt sie, und ein g\u00f6ttlich Licht,", "tokens": ["Das", "Dach", "emp\u00b7f\u00e4ngt", "sie", ",", "und", "ein", "g\u00f6tt\u00b7lich", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KON", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambi