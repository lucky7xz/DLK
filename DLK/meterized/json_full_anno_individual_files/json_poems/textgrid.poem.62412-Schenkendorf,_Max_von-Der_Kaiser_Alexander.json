{"textgrid.poem.62412": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Der Kaiser Alexander", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Held ist ausgezogen,", "tokens": ["Ein", "Held", "ist", "aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Held der Freundlichkeit,", "tokens": ["Ein", "Held", "der", "Freund\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihn trug auf rauhen Wogen", "tokens": ["Ihn", "trug", "auf", "rau\u00b7hen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die wildbewegte Zeit.", "tokens": ["Die", "wild\u00b7be\u00b7weg\u00b7te", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er nahm zu Schwert und Schilde", "tokens": ["Er", "nahm", "zu", "Schwert", "und", "Schil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Glauben und die Treu,", "tokens": ["Den", "Glau\u00b7ben", "und", "die", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sein G\u00fcrtel hei\u00dfet Milde", "tokens": ["Sein", "G\u00fcr\u00b7tel", "hei\u00b7\u00dfet", "Mil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Gott sein Feldgeschrei.", "tokens": ["Und", "Gott", "sein", "Feld\u00b7ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Held ist ausgezogen,", "tokens": ["Ein", "Held", "ist", "aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Retter dieser Zeit", "tokens": ["Ein", "Ret\u00b7ter", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Ro\u00df und Mann und Bogen", "tokens": ["Mit", "Ro\u00df", "und", "Mann", "und", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Gottes heil'gen Streit.", "tokens": ["In", "Got\u00b7tes", "heil'\u00b7gen", "Streit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es drang zu seinen Ohren", "tokens": ["Es", "drang", "zu", "sei\u00b7nen", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein hohes Gotteswort,", "tokens": ["Ein", "ho\u00b7hes", "Got\u00b7tes\u00b7wort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da hat er sich verschworen", "tokens": ["Da", "hat", "er", "sich", "ver\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Freiheit Held und Hort.", "tokens": ["Der", "Frei\u00b7heit", "Held", "und", "Hort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "An seines Volkes Herzen", "tokens": ["An", "sei\u00b7nes", "Vol\u00b7kes", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wuchs ihm die Heldenbrust,", "tokens": ["Wuchs", "ihm", "die", "Hel\u00b7den\u00b7brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Aus Flammen und aus Schmerzen", "tokens": ["Aus", "Flam\u00b7men", "und", "aus", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bl\u00fcht h\u00f6chste Liebeslust!", "tokens": ["Bl\u00fcht", "h\u00f6chs\u00b7te", "Lie\u00b7bes\u00b7lust", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O steiget, Moskaus Flammen,", "tokens": ["O", "stei\u00b7get", ",", "Mos\u00b7kaus", "Flam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie S\u00e4ulen himmelan!", "tokens": ["Wie", "S\u00e4u\u00b7len", "him\u00b7me\u00b7lan", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Flammenburg entstammen", "tokens": ["Der", "Flam\u00b7men\u00b7burg", "ent\u00b7stam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Soll der gew\u00e4hlte Mann.", "tokens": ["Soll", "der", "ge\u00b7w\u00e4hl\u00b7te", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Mann von Gott erlesen,", "tokens": ["Der", "Mann", "von", "Gott", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der seinen Ruf geh\u00f6rt,", "tokens": ["Der", "sei\u00b7nen", "Ruf", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df er des Teufels Wesen", "tokens": ["Da\u00df", "er", "des", "Teu\u00b7fels", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In dieser Zeit zerst\u00f6rt.", "tokens": ["In", "die\u00b7ser", "Zeit", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Frisch auf zum Heldenlaufe,", "tokens": ["Frisch", "auf", "zum", "Hel\u00b7den\u00b7lau\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Weit auf in fernes Land,", "tokens": ["Weit", "auf", "in", "fer\u00b7nes", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O Mann, in heil'ger Taufe", "tokens": ["O", "Mann", ",", "in", "heil'\u00b7ger", "Tau\u00b7fe"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum Helfer schon ernannt.", "tokens": ["Zum", "Hel\u00b7fer", "schon", "er\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wolauf zum Ehrengarten;", "tokens": ["Wo\u00b7lauf", "zum", "Eh\u00b7ren\u00b7gar\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Kaiser, steh am Belt", "tokens": ["O", "Kai\u00b7ser", ",", "steh", "am", "Belt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Waffenbruder warten,", "tokens": ["Den", "Waf\u00b7fen\u00b7bru\u00b7der", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den k\u00f6niglichen Held.", "tokens": ["Den", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Held", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So ist es wol gelungen", "tokens": ["So", "ist", "es", "wol", "ge\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Freunden alter Welt,", "tokens": ["Den", "Freun\u00b7den", "al\u00b7ter", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die manchen Feind bezwungen", "tokens": ["Die", "man\u00b7chen", "Feind", "be\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und manches Thier gef\u00e4llt.", "tokens": ["Und", "man\u00b7ches", "Thier", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gen Deutschland mu\u00dft du ziehen,", "tokens": ["Gen", "Deutschland", "mu\u00dft", "du", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ins m\u00fctterliche Land,", "tokens": ["Ins", "m\u00fct\u00b7ter\u00b7li\u00b7che", "Land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sollst gl\u00e4nzen dort und gl\u00fchen,", "tokens": ["Sollst", "gl\u00e4n\u00b7zen", "dort", "und", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O Schwert in Kaisershand.", "tokens": ["O", "Schwert", "in", "Kai\u00b7ser\u00b7shand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da sollst du treulich halten", "tokens": ["Da", "sollst", "du", "treu\u00b7lich", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein peinliches Gericht,", "tokens": ["Ein", "pein\u00b7li\u00b7ches", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein heil'ges Amt verwalten,", "tokens": ["Ein", "heil'\u00b7ges", "Amt", "ver\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Umstrahlt von Gottes Licht.", "tokens": ["Um\u00b7strahlt", "von", "Got\u00b7tes", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gen Deutschland sollst du ziehen,", "tokens": ["Gen", "Deutschland", "sollst", "du", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Du lieber Gottesheld,", "tokens": ["Du", "lie\u00b7ber", "Got\u00b7tes\u00b7held", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Deutschland soll erbl\u00fchen", "tokens": ["In", "Deutschland", "soll", "er\u00b7bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "VMFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das Heil f\u00fcr alle Welt.", "tokens": ["Das", "Heil", "f\u00fcr", "al\u00b7le", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da wird es dir erscheinen,", "tokens": ["Da", "wird", "es", "dir", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was Gott der Herr gedacht,", "tokens": ["Was", "Gott", "der", "Herr", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als er zum Heil der Seinen", "tokens": ["Als", "er", "zum", "Heil", "der", "Sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den gro\u00dfen Plan gemacht.", "tokens": ["Den", "gro\u00b7\u00dfen", "Plan", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O nehmt ihn auf, ihr Br\u00fcder!", "tokens": ["O", "nehmt", "ihn", "auf", ",", "ihr", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er stammt aus deutschem Blut,", "tokens": ["Er", "stammt", "aus", "deut\u00b7schem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Deutschen bringt er wieder", "tokens": ["Den", "Deut\u00b7schen", "bringt", "er", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Freiheit altes Gut;", "tokens": ["Der", "Frei\u00b7heit", "al\u00b7tes", "Gut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie man die heil'gen Boten", "tokens": ["Wie", "man", "die", "heil'\u00b7gen", "Bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Himmels nur geehrt,", "tokens": ["Des", "Him\u00b7mels", "nur", "ge\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sei ihm der Gru\u00df entboten,", "tokens": ["Sei", "ihm", "der", "Gru\u00df", "ent\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Gottes Ruf geh\u00f6rt.", "tokens": ["Der", "Got\u00b7tes", "Ruf", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ein Held ist ausgezogen,", "tokens": ["Ein", "Held", "ist", "aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Held der Freundlichkeit,", "tokens": ["Ein", "Held", "der", "Freund\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+