{"dta.poem.3646": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Abendgebet .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Abends wenn ich schlafen geh,               ", "tokens": ["A\u00b7bends", "wenn", "ich", "schla\u00b7fen", "geh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vierzehn Engel bei mir stehn,", "tokens": ["Vier\u00b7zehn", "En\u00b7gel", "bei", "mir", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwey zu meiner Rechten,", "tokens": ["Zwey", "zu", "mei\u00b7ner", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Zwey zu meiner Linken,", "tokens": ["Zwey", "zu", "mei\u00b7ner", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Zwey zu meinen H\u00e4upten,", "tokens": ["Zwey", "zu", "mei\u00b7nen", "H\u00e4up\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Zwey zu meinen F\u00fcssen,", "tokens": ["Zwey", "zu", "mei\u00b7nen", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Zwey die mich decken,", "tokens": ["Zwey", "die", "mich", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "PRELS", "PPER", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Zwey die mich wecken,", "tokens": ["Zwey", "die", "mich", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "PRELS", "PPER", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Zwey die mich weisen", "tokens": ["Zwey", "die", "mich", "wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "PRELS", "PPER", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "In das himmlische Paradeischen.", "tokens": ["In", "das", "himm\u00b7li\u00b7sche", "Pa\u00b7ra\u00b7de\u00b7i\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}