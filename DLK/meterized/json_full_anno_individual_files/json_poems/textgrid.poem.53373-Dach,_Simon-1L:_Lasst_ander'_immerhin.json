-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Vnd dies nach seiner Krafft,", "tokens": ["Vnd", "dies", "nach", "sei\u00b7ner", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die, was Er ausdenckt, schafft,", "tokens": ["Die", ",", "was", "Er", "aus\u00b7denckt", ",", "schafft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nach der Er alle Sachen,", "tokens": ["Nach", "der", "Er", "al\u00b7le", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ihn unterthan kan machen,", "tokens": ["Ihn", "un\u00b7ter\u00b7than", "kan", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Daf\u00fcr wir Ihn erheben,", "tokens": ["Da\u00b7f\u00fcr", "wir", "Ihn", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hier vnd in jenem Leben.", "tokens": ["Hier", "vnd", "in", "je\u00b7nem", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}