{"dta.poem.20439": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft  \n Grafens Serini/ welcher/ dem vorge-  \n ben nach/ auff der jagt von einem wilden schwein  \n soll seyn get\u00f6dtet worden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein auge war ein plitz/ mein arm ein donnerschlag/", "tokens": ["Mein", "au\u00b7ge", "war", "ein", "plitz", "/", "mein", "arm", "ein", "don\u00b7ner\u00b7schlag", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$(", "PPOSAT", "ADJD", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein s\u00e4bel war bem\u00fcht/ den monden bleich zu f\u00e4rben/", "tokens": ["Mein", "s\u00e4\u00b7bel", "war", "be\u00b7m\u00fcht", "/", "den", "mon\u00b7den", "bleich", "zu", "f\u00e4r\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VAFIN", "VVPP", "$(", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus meinem hertzen qvillt der T\u00fcrcken ihr verderben/", "tokens": ["Aus", "mei\u00b7nem", "hert\u00b7zen", "qvillt", "der", "T\u00fcr\u00b7cken", "ihr", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn um mein Tzacatur das feld voll leichen lag:", "tokens": ["Wenn", "um", "mein", "Tza\u00b7ca\u00b7tur", "das", "feld", "voll", "lei\u00b7chen", "lag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "ART", "NN", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das blut-vergiessen war mein rother feyertag.", "tokens": ["Das", "blut\u00b7ver\u00b7gies\u00b7sen", "war", "mein", "ro\u00b7ther", "fe\u00b7yer\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Doch lie\u00df der himmel mich nicht das gel\u00fcck erwerben;", "tokens": ["Doch", "lie\u00df", "der", "him\u00b7mel", "mich", "nicht", "das", "ge\u00b7l\u00fcck", "er\u00b7wer\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auff eines Bassen brust/ mit blut bespritzt/ zu sterben.", "tokens": ["Auff", "ei\u00b7nes", "Bas\u00b7sen", "brust", "/", "mit", "blut", "be\u00b7spritzt", "/", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$(", "APPR", "NN", "VVPP", "$(", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ach da\u00df doch nicht Serin wie Simson sterben mag!", "tokens": ["Ach", "da\u00df", "doch", "nicht", "Se\u00b7rin", "wie", "Sim\u00b7son", "ster\u00b7ben", "mag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ADV", "PTKNEG", "NN", "KOKOM", "NE", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So mu\u00df der mensch ein spiel des sternen-himmels bleiben/", "tokens": ["So", "mu\u00df", "der", "mensch", "ein", "spiel", "des", "ster\u00b7nen\u00b7him\u00b7mels", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ART", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bald ist er stern/ bald stein/ bald gold/ bald tr\u00fcber sand/", "tokens": ["Bald", "ist", "er", "stern", "/", "bald", "stein", "/", "bald", "gold", "/", "bald", "tr\u00fc\u00b7ber", "sand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$(", "ADV", "ADJD", "$(", "ADV", "ADJD", "$(", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bald lockt/ bald dr\u00fccket ihn des zufalls harte hand;", "tokens": ["Bald", "lockt", "/", "bald", "dr\u00fc\u00b7cket", "ihn", "des", "zu\u00b7falls", "har\u00b7te", "hand", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ach k\u00f6nt ich diesen reim um meine grabst\u00e4tt schreiben:", "tokens": ["Ach", "k\u00f6nt", "ich", "die\u00b7sen", "reim", "um", "mei\u00b7ne", "grab\u00b7st\u00e4tt", "schrei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der niemahls sich geb\u00fcckt vor seines feindes fahn/", "tokens": ["Der", "nie\u00b7mahls", "sich", "ge\u00b7b\u00fcckt", "vor", "sei\u00b7nes", "fein\u00b7des", "fahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den schl\u00e4gt des feindes feind ein wilder schweines zahn.", "tokens": ["Den", "schl\u00e4gt", "des", "fein\u00b7des", "feind", "ein", "wil\u00b7der", "schwei\u00b7nes", "zahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}