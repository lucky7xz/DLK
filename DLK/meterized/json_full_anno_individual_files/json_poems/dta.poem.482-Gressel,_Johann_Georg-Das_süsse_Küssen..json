{"dta.poem.482": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Das s\u00fcsse K\u00fcssen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Nectar \u00fcbersteigt den angenehmen Most", "tokens": ["Kein", "Nec\u00b7tar", "\u00fc\u00b7bers\u00b7teigt", "den", "an\u00b7ge\u00b7neh\u00b7men", "Most"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ein erfreuter Geist auf sch\u00f6nen Lippen ko\u00dft/", "tokens": ["Das", "ein", "er\u00b7freu\u00b7ter", "Geist", "auf", "sch\u00f6\u00b7nen", "Lip\u00b7pen", "ko\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hymettens Bienen-Safft l\u00e4\u00dft nicht die Lust geniessen/", "tokens": ["Hy\u00b7met\u00b7tens", "Bie\u00b7nen\u00b7\u00b7S\u00b7afft", "l\u00e4\u00dft", "nicht", "die", "Lust", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PTKNEG", "ART", "NN", "VVPP", "$("], "meter": "+--+---+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Die ein Verliebter schmeckt in Zucker-s\u00fcssen K\u00fcssen.", "tokens": ["Die", "ein", "Ver\u00b7lieb\u00b7ter", "schmeckt", "in", "Zu\u00b7cke\u00b7rs\u00fcs\u00b7sen", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN"