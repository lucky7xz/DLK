unct"], "pos": ["NE", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An dem deinen, himmelw\u00e4rts!", "tokens": ["An", "dem", "dei\u00b7nen", ",", "him\u00b7mel\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "$,", "ADV", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.9": {"line.1": {"text": "Fahre fort mit Dornenschl\u00e4gen,", "tokens": ["Fah\u00b7re", "fort", "mit", "Dor\u00b7nen\u00b7schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00dfe Rose, meinem Herzen,", "tokens": ["Wei\u00b7\u00dfe", "Ro\u00b7se", ",", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem verbrannten, quillt ein Segen,", "tokens": ["Dem", "ver\u00b7brann\u00b7ten", ",", "quillt", "ein", "Se\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus den Tr\u00e4nen aus den Schmerzen.", "tokens": ["Aus", "den", "Tr\u00e4\u00b7nen", "aus", "den", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Breche ganz mein altes Leben,", "tokens": ["Bre\u00b7che", "ganz", "mein", "al\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "