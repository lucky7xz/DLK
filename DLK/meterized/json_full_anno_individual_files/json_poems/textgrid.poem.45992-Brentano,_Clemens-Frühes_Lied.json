measure": "single.up"}}, "stanza.15": {"line.1": {"text": "Alles was du je getragen,", "tokens": ["Al\u00b7les", "was", "du", "je", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh, das hab' ich all verschuldet,", "tokens": ["Sieh", ",", "das", "hab'", "ich", "all", "ver\u00b7schul\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDS", "VAFIN", "PPER", "PIAT", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Schuld hat dich geschlagen,", "tokens": ["Mei\u00b7ne", "Schuld", "hat", "dich", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und du hast so fromm geduldet.", "tokens": ["Und", "du", "hast", "so", "fromm", "ge\u00b7dul\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Und nun tr\u00e4gst du dies versunkne,", "tokens": ["Und", "nun", "tr\u00e4gst", "du", "dies", "ver\u00b7sunk\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das dich marterte, dies Herz,", "tokens": ["Das", "dich", "mar\u00b7ter\u00b7te", ",", "dies", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$,", "PDS", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O du Gottesmitleidtrunkne,", "tokens": ["O", "du", "Got\u00b7tes\u00b7mit\u00b7leid\u00b7trunk\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An dem deinen, himmelw\u00e4rts!", "tokens": ["An", "dem", "dei\u00b7nen", ",", "him\u00b7mel\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "$,", "ADV", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}}}}