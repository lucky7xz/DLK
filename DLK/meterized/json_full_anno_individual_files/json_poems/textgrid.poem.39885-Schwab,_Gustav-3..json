{"textgrid.poem.39885": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lag der arme kranke Knecht", "tokens": ["Es", "lag", "der", "ar\u00b7me", "kran\u00b7ke", "Knecht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im milden Haus geborgen,", "tokens": ["Im", "mil\u00b7den", "Haus", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Lang sprach er irre von Mordgefecht,", "tokens": ["Lang", "sprach", "er", "ir\u00b7re", "von", "Mord\u00b7ge\u00b7fecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vom letzten, blutigen Morgen.", "tokens": ["Vom", "letz\u00b7ten", ",", "blu\u00b7ti\u00b7gen", "Mor\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Er sah im wachen Traum die Not,", "tokens": ["Er", "sah", "im", "wa\u00b7chen", "Traum", "die", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Schwarm der Feinde, der Raben,", "tokens": ["Den", "Schwarm", "der", "Fein\u00b7de", ",", "der", "Ra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Banner gesunken, die Edlen tot,", "tokens": ["Die", "Ban\u00b7ner", "ge\u00b7sun\u00b7ken", ",", "die", "Ed\u00b7len", "tot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Den Herrn im Eis begraben.", "tokens": ["Den", "Herrn", "im", "Eis", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bis da\u00df ein Schlummer lang und tief", "tokens": ["Bis", "da\u00df", "ein", "Schlum\u00b7mer", "lang", "und", "tief"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich seiner Qual erbarmte,", "tokens": ["Sich", "sei\u00b7ner", "Qual", "er\u00b7barm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und was in ihm von Leben schlief,", "tokens": ["Und", "was", "in", "ihm", "von", "Le\u00b7ben", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Ruhe lind erwarmte.", "tokens": ["In", "Ru\u00b7he", "lind", "er\u00b7warm\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Jetzt hebt sein Auge leuchtend sich,", "tokens": ["Jetzt", "hebt", "sein", "Au\u00b7ge", "leuch\u00b7tend", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "VVPP", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf springt er von dem Bette,", "tokens": ["Auf", "springt", "er", "von", "dem", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es fragt der Fremdling freudiglich", "tokens": ["Es", "fragt", "der", "Fremd\u00b7ling", "freu\u00b7dig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Pinsel und Palette.", "tokens": ["Nach", "Pin\u00b7sel", "und", "Pa\u00b7let\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Diener sprechen: \u00bbKrankheitswahn", "tokens": ["Die", "Die\u00b7ner", "spre\u00b7chen", ":", "\u00bb", "Krank\u00b7heits\u00b7wahn"], "token_info": ["word", "word", "word", "punct", "punct", "word"], "pos": ["ART", "NN", "VVINF", "$.", "$(", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat ihm den Sinn verst\u00f6ret!\u00ab", "tokens": ["Hat", "ihm", "den", "Sinn", "ver\u00b7st\u00f6\u00b7ret", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie sehn einander fragend an,", "tokens": ["Sie", "sehn", "ein\u00b7an\u00b7der", "fra\u00b7gend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie bringen, was er begehret.", "tokens": ["Sie", "brin\u00b7gen", ",", "was", "er", "be\u00b7ge\u00b7hret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "\u00bbnein Freunde,\u00ab spricht er, \u00bbes ist kein Traum!", "tokens": ["\u00bb", "nein", "Freun\u00b7de", ",", "\u00ab", "spricht", "er", ",", "\u00bb", "es", "ist", "kein", "Traum", "!"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "NN", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "G\u00f6nnt mir das mutige Streben!", "tokens": ["G\u00f6nnt", "mir", "das", "mu\u00b7ti\u00b7ge", "Stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was ich erlebt, das war nur Schaum,", "tokens": ["Was", "ich", "er\u00b7lebt", ",", "das", "war", "nur", "Schaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "PDS", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt naht das wahre Leben!\u00ab", "tokens": ["Jetzt", "naht", "das", "wah\u00b7re", "Le\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und auf das \u00f6de Tuch mit Macht,", "tokens": ["Und", "auf", "das", "\u00f6\u00b7de", "Tuch", "mit", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit k\u00fchnen Pinselstrichen,", "tokens": ["Mit", "k\u00fch\u00b7nen", "Pin\u00b7sel\u00b7stri\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verbreitet er der Farben Pracht,", "tokens": ["Ver\u00b7brei\u00b7tet", "er", "der", "Far\u00b7ben", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die heut noch nicht verblichen.", "tokens": ["Die", "heut", "noch", "nicht", "ver\u00b7bli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Mit B\u00fcrgern f\u00fcllet sich der Saal,", "tokens": ["Mit", "B\u00fcr\u00b7gern", "f\u00fcl\u00b7let", "sich", "der", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie k\u00f6nnen das Gl\u00fcck nicht fassen.", "tokens": ["Sie", "k\u00f6n\u00b7nen", "das", "Gl\u00fcck", "nicht", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "\u00bbdas Heil will wieder mit uns sein,", "tokens": ["\u00bb", "das", "Heil", "will", "wie\u00b7der", "mit", "uns", "sein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "ADV", "APPR", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht alles ist verloren!", "tokens": ["Nicht", "al\u00b7les", "ist", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Ehre stellet sich wieder ein", "tokens": ["Die", "Eh\u00b7re", "stel\u00b7let", "sich", "wie\u00b7der", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "ART"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In unsern schwarzen Thoren.\u00ab", "tokens": ["In", "un\u00b7sern", "schwar\u00b7zen", "Tho\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Der Meister l\u00e4chelt selig, still,", "tokens": ["Der", "Meis\u00b7ter", "l\u00e4\u00b7chelt", "se\u00b7lig", ",", "still", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00e4hrt fort und fort zu malen,", "tokens": ["F\u00e4hrt", "fort", "und", "fort", "zu", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und immer gr\u00f6\u00dfre Wonne will", "tokens": ["Und", "im\u00b7mer", "gr\u00f6\u00df\u00b7re", "Won\u00b7ne", "will"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus seinem Bilde stralen.", "tokens": ["Aus", "sei\u00b7nem", "Bil\u00b7de", "stra\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Von fernen Burgen f\u00fchrt er her", "tokens": ["Von", "fer\u00b7nen", "Bur\u00b7gen", "f\u00fchrt", "er", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die K\u00f6nige mit Geleite,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7ge", "mit", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch nicht mit wildem Kriegesheer", "tokens": ["Doch", "nicht", "mit", "wil\u00b7dem", "Krie\u00b7ges\u00b7heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu unheilvollem Streite.", "tokens": ["Zu", "un\u00b7heil\u00b7vol\u00b7lem", "Strei\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Sie alle treibt ein frommer Mut,", "tokens": ["Sie", "al\u00b7le", "treibt", "ein", "from\u00b7mer", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht Feindschaft, die sich br\u00fcstet,", "tokens": ["Nicht", "Feind\u00b7schaft", ",", "die", "sich", "br\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Kleider hohe Farbenglut", "tokens": ["Der", "Klei\u00b7der", "ho\u00b7he", "Far\u00b7beng\u00b7lut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat nicht der Stolz ger\u00fcstet.", "tokens": ["Hat", "nicht", "der", "Stolz", "ge\u00b7r\u00fcs\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Die Demut w\u00f6lbt den grauen Bau,", "tokens": ["Die", "De\u00b7mut", "w\u00f6lbt", "den", "grau\u00b7en", "Bau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Legt in die Krippe den Knaben,", "tokens": ["Legt", "in", "die", "Krip\u00b7pe", "den", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und setzt zu ihm die selige Frau,", "tokens": ["Und", "setzt", "zu", "ihm", "die", "se\u00b7li\u00b7ge", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und reicht ihm dar die Gaben.", "tokens": ["Und", "reicht", "ihm", "dar", "die", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und Gottes Friede schwebet mild", "tokens": ["Und", "Got\u00b7tes", "Frie\u00b7de", "schwe\u00b7bet", "mild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um die geweihte St\u00e4tte,", "tokens": ["Um", "die", "ge\u00b7weih\u00b7te", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Meister steht vor seinem Bild", "tokens": ["Der", "Meis\u00b7ter", "steht", "vor", "sei\u00b7nem", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit dankendem Gebete.", "tokens": ["Mit", "dan\u00b7ken\u00b7dem", "Ge\u00b7be\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Das ist der Herr, das ist der Held,", "tokens": ["Das", "ist", "der", "Herr", ",", "das", "ist", "der", "Held", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dessen Dienst er lebet,", "tokens": ["In", "des\u00b7sen", "Dienst", "er", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ist die heilige Wunderwelt,", "tokens": ["Das", "ist", "die", "hei\u00b7li\u00b7ge", "Wun\u00b7der\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die stets sein Aug' umschwebet.", "tokens": ["Die", "stets", "sein", "Aug'", "um\u00b7schwe\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Es z\u00fcckt die Kraft ihm durch die Hand,", "tokens": ["Es", "z\u00fcckt", "die", "Kraft", "ihm", "durch", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er wird in vielen Bildern", "tokens": ["Er", "wird", "in", "vie\u00b7len", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das \u00fcberirdische Vaterland,", "tokens": ["Das", "\u00fc\u00b7be\u00b7rir\u00b7di\u00b7sche", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das h\u00f6chste Leben schildern.", "tokens": ["Das", "h\u00f6chs\u00b7te", "Le\u00b7ben", "schil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Und Meer und Str\u00f6m' und Berg und Thal,", "tokens": ["Und", "Meer", "und", "Str\u00f6m'", "und", "Berg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was Herrlich's er gesehen,", "tokens": ["Was", "Herr\u00b7lich's", "er", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verkl\u00e4rt von seines Pinsels Stral", "tokens": ["Ver\u00b7kl\u00e4rt", "von", "sei\u00b7nes", "Pin\u00b7sels", "Stral"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird alles auferstehen.", "tokens": ["Wird", "al\u00b7les", "auf\u00b7er\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "In tausend Z\u00fcgen wird er licht", "tokens": ["In", "tau\u00b7send", "Z\u00fc\u00b7gen", "wird", "er", "licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Menschheit Bild uns malen,", "tokens": ["Der", "Menschheit", "Bild", "uns", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und in Ein g\u00f6ttlich Angesicht", "tokens": ["Und", "in", "Ein", "g\u00f6tt\u00b7lich", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vereinen alle Stralen.", "tokens": ["Ver\u00b7ei\u00b7nen", "al\u00b7le", "Stra\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "So schafft der Meister zu Gottes Ehr',", "tokens": ["So", "schafft", "der", "Meis\u00b7ter", "zu", "Got\u00b7tes", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es leuchten seine Werke,", "tokens": ["Es", "leuch\u00b7ten", "sei\u00b7ne", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo blieb der Herzog und sein Heer?", "tokens": ["Wo", "blieb", "der", "Her\u00b7zog", "und", "sein", "Heer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Stolz, der Glanz, die St\u00e4rke?", "tokens": ["Der", "Stolz", ",", "der", "Glanz", ",", "die", "St\u00e4r\u00b7ke", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Hinunter mu\u00df der Erde Pracht", "tokens": ["Hin\u00b7un\u00b7ter", "mu\u00df", "der", "Er\u00b7de", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In d\u00fcstern Grabesh\u00fcgel,", "tokens": ["In", "d\u00fcs\u00b7tern", "Gra\u00b7bes\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Echte rettet aus der Nacht", "tokens": ["Das", "Ech\u00b7te", "ret\u00