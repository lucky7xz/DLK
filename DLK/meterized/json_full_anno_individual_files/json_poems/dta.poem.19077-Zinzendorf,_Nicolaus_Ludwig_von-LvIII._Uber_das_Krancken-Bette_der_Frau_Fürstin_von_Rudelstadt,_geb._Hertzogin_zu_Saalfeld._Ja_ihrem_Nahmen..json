{"dta.poem.19077": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LvIII.   Uber das Krancken-Bette der Frau  \n F\u00fcrstin von Rudelstadt, geb. Hertzogin  \n zu Saalfeld.   Ja ihrem Nahmen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Seelen-Freund! hier liegt ein Hertze,", "tokens": ["See\u00b7len\u00b7Freund", "!", "hier", "liegt", "ein", "Hert\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "ART", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das dich unter allem Schmertze", "tokens": ["Das", "dich", "un\u00b7ter", "al\u00b7lem", "Schmert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "APPR", "PIS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gerne fr\u00f6lich loben wolte,", "tokens": ["Ger\u00b7ne", "fr\u00f6\u00b7lich", "lo\u00b7ben", "wol\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein treues Hertze solte.", "tokens": ["Wie", "ein", "treu\u00b7es", "Hert\u00b7ze", "sol\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "VVFIN", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wehethun ist bey der Liebe", "tokens": ["We\u00b7het\u00b7hun", "ist", "bey", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einer der gewohntsten Triebe;", "tokens": ["Ei\u00b7ner", "der", "ge\u00b7wohnts\u00b7ten", "Trie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer dem HErrn am Hertzen lieget,", "tokens": ["Wer", "dem", "Herrn", "am", "Hert\u00b7zen", "lie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird nicht allezeit gewieget.", "tokens": ["Wird", "nicht", "al\u00b7le\u00b7zeit", "ge\u00b7wie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6chste Lust und Hertz-Vergn\u00fcgen,", "tokens": ["H\u00f6chs\u00b7te", "Lust", "und", "Hertz\u00b7Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich will dir zu F\u00fcssen liegen,", "tokens": ["Ich", "will", "dir", "zu", "F\u00fcs\u00b7sen", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mag mich doch die Welt verh\u00f6hnen,", "tokens": ["Mag", "mich", "doch", "die", "Welt", "ver\u00b7h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie Marien Magdalenen.", "tokens": ["Wie", "Ma\u00b7ri\u00b7en", "Mag\u00b7da\u00b7le\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ziehe mich, damit ich lauffe,", "tokens": ["Zie\u00b7he", "mich", ",", "da\u00b7mit", "ich", "lauf\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tauffe mich mit deiner Tauffe,", "tokens": ["Tauf\u00b7fe", "mich", "mit", "dei\u00b7ner", "Tauf\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um den Sitz in deinen Reichen", "tokens": ["Um", "den", "Sitz", "in", "dei\u00b7nen", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollen wir uns schon vergleichen.", "tokens": ["Wol\u00b7len", "wir", "uns", "schon", "ver\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sch\u00f6ner Br\u00e4utigam der Seele!", "tokens": ["Sch\u00f6\u00b7ner", "Br\u00e4u\u00b7ti\u00b7gam", "der", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich beschwert die Leibes-H\u00f6le,", "tokens": ["Mich", "be\u00b7schwert", "die", "Lei\u00b7bes\u00b7H\u00f6le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein Geist, das freye Wesen,", "tokens": ["Und", "mein", "Geist", ",", "das", "frey\u00b7e", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird im Sterben erst genesen.", "tokens": ["Wird", "im", "Ster\u00b7ben", "erst", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Christi Last ist leicht zu tragen,", "tokens": ["Chris\u00b7ti", "Last", "ist", "leicht", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der wird niemand gerne plagen:", "tokens": ["Der", "wird", "nie\u00b7mand", "ger\u00b7ne", "pla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die die Z\u00fcchtigung erdulden,", "tokens": ["Die", "die", "Z\u00fcch\u00b7ti\u00b7gung", "er\u00b7dul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00f6gen GOtt, als Vater, hulden.", "tokens": ["M\u00f6\u00b7gen", "Gott", ",", "als", "Va\u00b7ter", ",", "hul\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Unser Wandel ist im Himmel,", "tokens": ["Un\u00b7ser", "Wan\u00b7del", "ist", "im", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uber alles Welt-Get\u00fcmmel:", "tokens": ["U\u00b7ber", "al\u00b7les", "Welt\u00b7Ge\u00b7t\u00fcm\u00b7mel", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der verderbten Erd entweichen,", "tokens": ["Der", "ver\u00b7derb\u00b7ten", "Erd", "ent\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4re mir ein Gnaden-Zeichen.", "tokens": ["W\u00e4\u00b7re", "mir", "ein", "Gna\u00b7den\u00b7Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sch\u00f6pfer, hier ist dein Gesch\u00f6pfe,", "tokens": ["Sch\u00f6p\u00b7fer", ",", "hier", "ist", "dein", "Ge\u00b7sch\u00f6p\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der geringste deiner T\u00f6pfe,", "tokens": ["Der", "ge\u00b7rings\u00b7te", "dei\u00b7ner", "T\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du magst brechen oder bauen,", "tokens": ["Du", "magst", "bre\u00b7chen", "o\u00b7der", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df mich nur dein Antlitz schauen.", "tokens": ["La\u00df", "mich", "nur", "dein", "Ant\u00b7litz", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "F\u00fchrst du meinen Leibes-Schatten", "tokens": ["F\u00fchrst", "du", "mei\u00b7nen", "Lei\u00b7bes\u00b7Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fcckw\u00e4rts zu dem Ehegatten,", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "zu", "dem", "E\u00b7he\u00b7gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zu meinen beyden Kindern,", "tokens": ["Und", "zu", "mei\u00b7nen", "bey\u00b7den", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ichs eben auch nicht hindern.", "tokens": ["Will", "ichs", "e\u00b7ben", "auch", "nicht", "hin\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Zeige mir nur deinen Willen,", "tokens": ["Zei\u00b7ge", "mir", "nur", "dei\u00b7nen", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der soll meine Seele stillen:", "tokens": ["Der", "soll", "mei\u00b7ne", "See\u00b7le", "stil\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn in deinem Willen schweben,", "tokens": ["Denn", "in", "dei\u00b7nem", "Wil\u00b7len", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ist einer Seele Leben.", "tokens": ["Das", "ist", "ei\u00b7ner", "See\u00b7le", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Sieger \u00fcber Tod und H\u00f6lle,", "tokens": ["Sie\u00b7ger", "\u00fc\u00b7ber", "Tod", "und", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df die krancke Lager-Stelle,", "tokens": ["La\u00df", "die", "kran\u00b7cke", "La\u00b7ger\u00b7Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die mancherley Beschwerden,", "tokens": ["Und", "die", "man\u00b7cher\u00b7ley", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir zu einer Schule werden!", "tokens": ["Mir", "zu", "ei\u00b7ner", "Schu\u00b7le", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Ringe nur mit deinem Kinde,", "tokens": ["Rin\u00b7ge", "nur", "mit", "dei\u00b7nem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So doch, da\u00df ich \u00fcberwinde,", "tokens": ["So", "doch", ",", "da\u00df", "ich", "\u00fc\u00b7berw\u00b7in\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So wird aus den bittern Quellen,", "tokens": ["So", "wird", "aus", "den", "bit\u00b7tern", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Eine Fluth des Lebens schwellen!", "tokens": ["Ei\u00b7ne", "Fluth", "des", "Le\u00b7bens", "schwel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}