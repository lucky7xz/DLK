{"dta.poem.18856": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Sonnenhitze war er", "tokens": ["Son\u00b7nen\u00b7hit\u00b7ze", "war", "er"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Am kalten Tag,", "tokens": ["Am", "kal\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und brannte der Sirius", "tokens": ["Und", "brann\u00b7te", "der", "Si\u00b7rius"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "War er Schatten und K\u00fchlung.", "tokens": ["War", "er", "Schat\u00b7ten", "und", "K\u00fch\u00b7lung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}