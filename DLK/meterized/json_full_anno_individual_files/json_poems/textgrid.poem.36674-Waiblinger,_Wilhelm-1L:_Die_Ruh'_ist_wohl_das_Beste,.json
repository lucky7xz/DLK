{"textgrid.poem.36674": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Ruh' ist wohl das Beste,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Ruh' ist wohl das Beste,", "tokens": ["Die", "Ruh'", "ist", "wohl", "das", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von allem Gl\u00fcck der Welt,", "tokens": ["Von", "al\u00b7lem", "Gl\u00fcck", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit jedem Wiegenfeste", "tokens": ["Mit", "je\u00b7dem", "Wie\u00b7gen\u00b7fes\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird neue Lust verg\u00e4llt,", "tokens": ["Wird", "neu\u00b7e", "Lust", "ver\u00b7g\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Rose welkt in Schauern,", "tokens": ["Die", "Ro\u00b7se", "welkt", "in", "Schau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die uns der Fr\u00fchling giebt;", "tokens": ["Die", "uns", "der", "Fr\u00fch\u00b7ling", "giebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer ha\u00dft, ist zu bedauern,", "tokens": ["Wer", "ha\u00dft", ",", "ist", "zu", "be\u00b7dau\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und mehr noch fast, wer liebt.", "tokens": ["Und", "mehr", "noch", "fast", ",", "wer", "liebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "$,", "PWS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es tr\u00fcbt den eignen Frieden", "tokens": ["Es", "tr\u00fcbt", "den", "eig\u00b7nen", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit seiner Gluth das Herz,", "tokens": ["Mit", "sei\u00b7ner", "Gluth", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Kind ist nicht zufrieden,", "tokens": ["Das", "Kind", "ist", "nicht", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Mann bleibt nur der Schmerz.", "tokens": ["Dem", "Mann", "bleibt", "nur", "der", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du hoffst umsonst vom Meere,", "tokens": ["Du", "hoffst", "um\u00b7sonst", "vom", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vom Weltget\u00fcmmel Ruh';", "tokens": ["Vom", "Welt\u00b7ge\u00b7t\u00fcm\u00b7mel", "Ruh'", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Selbst Lorbeer, Ruhm und Ehre", "tokens": ["Selbst", "Lor\u00b7beer", ",", "Ruhm", "und", "Eh\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NE", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Heilt keine Wunden zu.", "tokens": ["Heilt", "kei\u00b7ne", "Wun\u00b7den", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun wei\u00df ich auf der Erde", "tokens": ["Nun", "wei\u00df", "ich", "auf", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein einzig Pl\u00e4tzchen nur,", "tokens": ["Ein", "ein\u00b7zig", "Pl\u00e4tz\u00b7chen", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo jegliche Beschwerde", "tokens": ["Wo", "jeg\u00b7li\u00b7che", "Be\u00b7schwer\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Schoo\u00dfe der Natur,", "tokens": ["Im", "Schoo\u00b7\u00dfe", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo jeder eitle Kummer,", "tokens": ["Wo", "je\u00b7der", "eit\u00b7le", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der mir den Busen schwellt,", "tokens": ["Der", "mir", "den", "Bu\u00b7sen", "schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In langen tiefen Schlummer", "tokens": ["In", "lan\u00b7gen", "tie\u00b7fen", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie's Laub vom Baume f\u00e4llt.", "tokens": ["Wie's", "Laub", "vom", "Bau\u00b7me", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Pl\u00e4tzchen ach! so theuer,", "tokens": ["Ein", "Pl\u00e4tz\u00b7chen", "ach", "!", "so", "theu\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ITJ", "$.", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie mich noch keins entz\u00fcckt,", "tokens": ["Wie", "mich", "noch", "keins", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PIAT", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo Lieb' und liebend Feuer", "tokens": ["Wo", "Lieb'", "und", "lie\u00b7bend", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz einst nicht mehr dr\u00fcckt,", "tokens": ["Mein", "Herz", "einst", "nicht", "mehr", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo's ruht in aller Stille,", "tokens": ["Wo's", "ruht", "in", "al\u00b7ler", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem Sturme nicht mehr blo\u00df,", "tokens": ["Dem", "Stur\u00b7me", "nicht", "mehr", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Entbunden aller H\u00fclle,", "tokens": ["Ent\u00b7bun\u00b7den", "al\u00b7ler", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ja frei und schicksallos.", "tokens": ["Ja", "frei", "und", "schick\u00b7sal\u00b7los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So freundlich ist's und heiter,", "tokens": ["So", "freund\u00b7lich", "ist's", "und", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn du es kennen lernst,", "tokens": ["Wenn", "du", "es", "ken\u00b7nen", "lernst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stets lieblicher und breiter,", "tokens": ["Stets", "lieb\u00b7li\u00b7cher", "und", "brei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und doch voll hohem Ernst,", "tokens": ["Und", "doch", "voll", "ho\u00b7hem", "Ernst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Vorwelt d\u00fcstres Grauen", "tokens": ["Der", "Vor\u00b7welt", "d\u00fcst\u00b7res", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hat's k\u00f6niglich geweiht,", "tokens": ["Hat's", "k\u00f6\u00b7nig\u00b7lich", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und wei\u00dfe Steine schauen", "tokens": ["Und", "wei\u00b7\u00dfe", "Stei\u00b7ne", "schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In all' die Einsamkeit.", "tokens": ["In", "all'", "die", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Pyramide d\u00fcstert", "tokens": ["Die", "Py\u00b7ra\u00b7mi\u00b7de", "d\u00fcs\u00b7tert"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Voll finstrer Pracht empor,", "tokens": ["Voll", "finst\u00b7rer", "Pracht", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus jungen B\u00e4umen fl\u00fcstert", "tokens": ["Aus", "jun\u00b7gen", "B\u00e4u\u00b7men", "fl\u00fcs\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Klagehauch hervor,", "tokens": ["Ein", "Kla\u00b7ge\u00b7hauch", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es weht auf diese Gr\u00fcnde", "tokens": ["Es", "weht", "auf", "die\u00b7se", "Gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das grauste Alterthum,", "tokens": ["Das", "graus\u00b7te", "Al\u00b7ter\u00b7thum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn irgendwo, so finde", "tokens": ["Wenn", "ir\u00b7gend\u00b7wo", ",", "so", "fin\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich hier Elysium.", "tokens": ["Ich", "hier", "E\u00b7ly\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es gl\u00e4nzt im Abendlichte", "tokens": ["Es", "gl\u00e4nzt", "im", "A\u00b7bend\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umher die goldne Au',", "tokens": ["Um\u00b7her", "die", "gold\u00b7ne", "Au'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und himmlische Gesichte", "tokens": ["Und", "himm\u00b7li\u00b7sche", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weckt mir das lautre Blau,", "tokens": ["Weckt", "mir", "das", "laut\u00b7re", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das mit den reinen Fluthen", "tokens": ["Das", "mit", "den", "rei\u00b7nen", "Flut\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dort auf des Berges Nacht,", "tokens": ["Dort", "auf", "des", "Ber\u00b7ges", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In sanften Purpurgluthen,", "tokens": ["In", "sanf\u00b7ten", "Pur\u00b7pur\u00b7glu\u00b7then", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein andrer Lethe, lacht.", "tokens": ["Ein", "an\u00b7drer", "Le\u00b7the", ",", "lacht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Br\u00fcder selbst, sie st\u00f6ren", "tokens": ["Die", "Br\u00fc\u00b7der", "selbst", ",", "sie", "st\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hier meine Ruhe nicht,", "tokens": ["Hier", "mei\u00b7ne", "Ru\u00b7he", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur selten, da\u00df sie h\u00f6ren,", "tokens": ["Nur", "sel\u00b7ten", ",", "da\u00df", "sie", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie mir ein Ach entbricht,", "tokens": ["Wie", "mir", "ein", "Ach", "ent\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie schlafen hier geschieden", "tokens": ["Sie", "schla\u00b7fen", "hier", "ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von aller Welt, allein,", "tokens": ["Von", "al\u00b7ler", "Welt", ",", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O welch ein Gl\u00fcck, hienieden,", "tokens": ["O", "welch", "ein", "Gl\u00fcck", ",", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PIAT", "ART", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kein Gl\u00e4ubiger zu sein!", "tokens": ["Kein", "Gl\u00e4u\u00b7bi\u00b7ger", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Ruh' ist wohl das Beste,", "tokens": ["Die", "Ruh'", "ist", "wohl", "das", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von allem Gl\u00fcck der Welt,", "tokens": ["Von", "al\u00b7lem", "Gl\u00fcck", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit jedem Wiegenfeste", "tokens": ["Mit", "je\u00b7dem", "Wie\u00b7gen\u00b7fes\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird neue Lust verg\u00e4llt,", "tokens": ["Wird", "neu\u00b7e", "Lust", "ver\u00b7g\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Rose welkt in Schauern,", "tokens": ["Die", "Ro\u00b7se", "welkt", "in", "Schau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die uns der Fr\u00fchling giebt;", "tokens": ["Die", "uns", "der", "Fr\u00fch\u00b7ling", "giebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer ha\u00dft, ist zu bedauern,", "tokens": ["Wer", "ha\u00dft", ",", "ist", "zu", "be\u00b7dau\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und mehr noch fast, wer liebt.", "tokens": ["Und", "mehr", "noch", "fast", ",", "wer", "liebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "$,", "PWS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Es tr\u00fcbt den eignen Frieden", "tokens": ["Es", "tr\u00fcbt", "den", "eig\u00b7nen", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit seiner Gluth das Herz,", "tokens": ["Mit", "sei\u00b7ner", "Gluth", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Kind ist nicht zufrieden,", "tokens": ["Das", "Kind", "ist", "nicht", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Mann bleibt nur der Schmerz.", "tokens": ["Dem", "Mann", "bleibt", "nur", "der", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du hoffst umsonst vom Meere,", "tokens": ["Du", "hoffst", "um\u00b7sonst", "vom", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vom Weltget\u00fcmmel Ruh';", "tokens": ["Vom", "Welt\u00b7ge\u00b7t\u00fcm\u00b7mel", "Ruh'", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Selbst Lorbeer, Ruhm und Ehre", "tokens": ["Selbst", "Lor\u00b7beer", ",", "Ruhm", "und", "Eh\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NE", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Heilt keine Wunden zu.", "tokens": ["Heilt", "kei\u00b7ne", "Wun\u00b7den", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Nun wei\u00df ich auf der Erde", "tokens": ["Nun", "wei\u00df", "ich", "auf", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein einzig Pl\u00e4tzchen nur,", "tokens": ["Ein", "ein\u00b7zig", "Pl\u00e4tz\u00b7chen", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo jegliche Beschwerde", "tokens": ["Wo", "jeg\u00b7li\u00b7che", "Be\u00b7schwer\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Schoo\u00dfe der Natur,", "tokens": ["Im", "Schoo\u00b7\u00dfe", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo jeder eitle Kummer,", "tokens": ["Wo", "je\u00b7der", "eit\u00b7le", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der mir den Busen schwellt,", "tokens": ["Der", "mir", "den", "Bu\u00b7sen", "schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In langen tiefen Schlummer", "tokens": ["In", "lan\u00b7gen", "tie\u00b7fen", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie's Laub vom Baume f\u00e4llt.", "tokens": ["Wie's", "Laub", "vom", "Bau\u00b7me", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ein Pl\u00e4tzchen ach! so theuer,", "tokens": ["Ein", "Pl\u00e4tz\u00b7chen", "ach", "!", "so", "theu\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ITJ", "$.", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie mich noch keins entz\u00fcckt,", "tokens": ["Wie", "mich", "noch", "keins", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PIAT", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo Lieb' und liebend Feuer", "tokens": ["Wo", "Lieb'", "und", "lie\u00b7bend", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz einst nicht mehr dr\u00fcckt,", "tokens": ["Mein", "Herz", "einst", "nicht", "mehr", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo's ruht in aller Stille,", "tokens": ["Wo's", "ruht", "in", "al\u00b7ler", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem Sturme nicht mehr blo\u00df,", "tokens": ["Dem", "Stur\u00b7me", "nicht", "mehr", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Entbunden aller H\u00fclle,", "tokens": ["Ent\u00b7bun\u00b7den", "al\u00b7ler", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ja frei und schicksallos.", "tokens": ["Ja", "frei", "und", "schick\u00b7sal\u00b7los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "So freundlich ist's und heiter,", "tokens": ["So", "freund\u00b7lich", "ist's", "und", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn du es kennen lernst,", "tokens": ["Wenn", "du", "es", "ken\u00b7nen", "lernst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stets lieblicher und breiter,", "tokens": ["Stets", "lieb\u00b7li\u00b7cher", "und", "brei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und doch voll hohem Ernst,", "tokens": ["Und", "doch", "voll", "ho\u00b7hem", "Ernst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Vorwelt d\u00fcstres Grauen", "tokens": ["Der", "Vor\u00b7welt", "d\u00fcst\u00b7res", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hat's k\u00f6niglich geweiht,", "tokens": ["Hat's", "k\u00f6\u00b7nig\u00b7lich", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und wei\u00dfe Steine schauen", "tokens": ["Und", "wei\u00b7\u00dfe", "Stei\u00b7ne", "schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In all' die Einsamkeit.", "tokens": ["In", "all'", "die", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Die Pyramide d\u00fcstert", "tokens": ["Die", "Py\u00b7ra\u00b7mi\u00b7de", "d\u00fcs\u00b7tert"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Voll finstrer Pracht empor,", "tokens": ["Voll", "finst\u00b7rer", "Pracht", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus jungen B\u00e4umen fl\u00fcstert", "tokens": ["Aus", "jun\u00b7gen", "B\u00e4u\u00b7men", "fl\u00fcs\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Klagehauch hervor,", "tokens": ["Ein", "Kla\u00b7ge\u00b7hauch", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es weht auf diese Gr\u00fcnde", "tokens": ["Es", "weht", "auf", "die\u00b7se", "Gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das grauste Alterthum,", "tokens": ["Das", "graus\u00b7te", "Al\u00b7ter\u00b7thum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn irgendwo, so finde", "tokens": ["Wenn", "ir\u00b7gend\u00b7wo", ",", "so", "fin\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich hier Elysium.", "tokens": ["Ich", "hier", "E\u00b7ly\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Es gl\u00e4nzt im Abendlichte", "tokens": ["Es", "gl\u00e4nzt", "im", "A\u00b7bend\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umher die goldne Au',", "tokens": ["Um\u00b7her", "die", "gold\u00b7ne", "Au'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und himmlische Gesichte", "tokens": ["Und", "himm\u00b7li\u00b7sche", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weckt mir das lautre Blau,", "tokens": ["Weckt", "mir", "das", "laut\u00b7re", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das mit den reinen Fluthen", "tokens": ["Das", "mit", "den", "rei\u00b7nen", "Flut\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dort auf des Berges Nacht,", "tokens": ["Dort", "auf", "des", "Ber\u00b7ges", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In sanften Purpurgluthen,", "tokens": ["In", "sanf\u00b7ten", "Pur\u00b7pur\u00b7glu\u00b7then", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein andrer Lethe, lacht.", "tokens": ["Ein", "an\u00b7drer", "Le\u00b7the", ",", "lacht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Die Br\u00fcder selbst, sie st\u00f6ren", "tokens": ["Die", "Br\u00fc\u00b7der", "selbst", ",", "sie", "st\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hier meine Ruhe nicht,", "tokens": ["Hier", "mei\u00b7ne", "Ru\u00b7he", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur selten, da\u00df sie h\u00f6ren,", "tokens": ["Nur", "sel\u00b7ten", ",", "da\u00df", "sie", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie mir ein Ach entbricht,", "tokens": ["Wie", "mir", "ein", "Ach", "ent\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie schlafen hier geschieden", "tokens": ["Sie", "schla\u00b7fen", "hier", "ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von aller Welt, allein,", "tokens": ["Von", "al\u00b7ler", "Welt", ",", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O welch ein Gl\u00fcck, hienieden,", "tokens": ["O", "welch", "ein", "Gl\u00fcck", ",", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PIAT", "ART", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kein Gl\u00e4ubiger zu sein!", "tokens": ["Kein", "Gl\u00e4u\u00b7bi\u00b7ger", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}