{"textgrid.poem.53024": {"metadata": {"author": {"name": "Pyra, Jakob Immanuel", "birth": "N.A.", "death": "N.A."}, "title": "Des Thirsis Treue", "genre": "verse", "period": "N.A.", "pub_year": 1729, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Damon, ewiglich von mir geliebter Freund!", "tokens": ["Mein", "Da\u00b7mon", ",", "e\u00b7wig\u00b7lich", "von", "mir", "ge\u00b7lieb\u00b7ter", "Freund", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dessen felsenfester Liebe", "tokens": ["Von", "des\u00b7sen", "fel\u00b7sen\u00b7fes\u00b7ter", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der gantzen Welt verlachter Sturm und Zorn", "tokens": ["Der", "gant\u00b7zen", "Welt", "ver\u00b7lach\u00b7ter", "Sturm", "und", "Zorn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein standhaft Hertz nicht w\u00fcrde reissen k\u00f6nnen;", "tokens": ["Mein", "stand\u00b7haft", "Hertz", "nicht", "w\u00fcr\u00b7de", "reis\u00b7sen", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "PTKNEG", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du dessen hoch und edlen Geist", "tokens": ["Du", "des\u00b7sen", "hoch", "und", "ed\u00b7len", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PDS", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Himmel, der uns einst besonders hold gewesen,", "tokens": ["Der", "Him\u00b7mel", ",", "der", "uns", "einst", "be\u00b7son\u00b7ders", "hold", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit vollem Segen mir zum Trost herabgesandt,", "tokens": ["Mit", "vol\u00b7lem", "Se\u00b7gen", "mir", "zum", "Trost", "her\u00b7ab\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als er, bevor dein Freund der Sonnen Glantz gesehen,", "tokens": ["Als", "er", ",", "be\u00b7vor", "dein", "Freund", "der", "Son\u00b7nen", "Glantz", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mir ein so hohes Gl\u00fcck bestimmt,", "tokens": ["Mir", "ein", "so", "ho\u00b7hes", "Gl\u00fcck", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das kein gemeiner Geist auch nur zu sch\u00e4tzen w\u00fc\u00dfte;", "tokens": ["Das", "kein", "ge\u00b7mei\u00b7ner", "Geist", "auch", "nur", "zu", "sch\u00e4t\u00b7zen", "w\u00fc\u00df\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dein Thirsis bleibt getreu. Und du Zerst\u00f6rerin", "tokens": ["Dein", "Thir\u00b7sis", "bleibt", "ge\u00b7treu", ".", "Und", "du", "Zer\u00b7st\u00f6\u00b7re\u00b7rin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$.", "KON", "PPER", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der eitlen stoltzen Wunderwercke,", "tokens": ["Der", "eit\u00b7len", "stolt\u00b7zen", "Wun\u00b7der\u00b7wer\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Zeit, du tilgst nie aus meiner festen Brust", "tokens": ["Zeit", ",", "du", "tilgst", "nie", "aus", "mei\u00b7ner", "fes\u00b7ten", "Brust"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Des edlen Paars zu tief gegrabne Namen.", "tokens": ["Des", "ed\u00b7len", "Paars", "zu", "tief", "ge\u00b7grab\u00b7ne", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKA", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ja, ja, es soll die Ewigkeit,", "tokens": ["Ja", ",", "ja", ",", "es", "soll", "die", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "O Damon, Doris, einst an daurnden Ehren-M\u00e4hlern", "tokens": ["O", "Da\u00b7mon", ",", "Do\u00b7ris", ",", "einst", "an", "daurn\u00b7den", "Eh\u00b7ren\u00b7M\u00e4h\u00b7lern"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "NE", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die gl\u00e4ntzenden verschlungnen Z\u00fcge sehn,", "tokens": ["Die", "gl\u00e4nt\u00b7zen\u00b7den", "ver\u00b7schlung\u00b7nen", "Z\u00fc\u00b7ge", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Wie sie am sandigten und Erlenvollen Ufer", "tokens": ["Wie", "sie", "am", "san\u00b7dig\u00b7ten", "und", "Er\u00b7len\u00b7vol\u00b7len", "U\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der hellen Krebsereichen Spree", "tokens": ["Der", "hel\u00b7len", "Kreb\u00b7se\u00b7rei\u00b7chen", "Spree"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Durch euers Sch\u00e4fers Hand tief eingeschnitten stehen.", "tokens": ["Durch", "eu\u00b7ers", "Sch\u00e4\u00b7fers", "Hand", "tief", "ein\u00b7ge\u00b7schnit\u00b7ten", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ADJD", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Des Ungl\u00fccks Wolcken ziehn noch \u00fcber meinem Haupt;", "tokens": ["Des", "Un\u00b7gl\u00fccks", "Wol\u00b7cken", "ziehn", "noch", "\u00fc\u00b7ber", "mei\u00b7nem", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ich sitze traurig in dem Dunckeln;", "tokens": ["Ich", "sit\u00b7ze", "trau\u00b7rig", "in", "dem", "Dun\u00b7ckeln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Nichts tr\u00f6stet mich, als Gott und eure Gunst,", "tokens": ["Nichts", "tr\u00f6s\u00b7tet", "mich", ",", "als", "Gott", "und", "eu\u00b7re", "Gunst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "KOUS", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "In meiner arm und frommen Mutter Armen,", "tokens": ["In", "mei\u00b7ner", "arm", "und", "from\u00b7men", "Mut\u00b7ter", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Die mich durch ihren Schwei\u00df ern\u00e4hrt.", "tokens": ["Die", "mich", "durch", "ih\u00b7ren", "Schwei\u00df", "er\u00b7n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Wie oft erz\u00e4hl ich ihr mit Thr\u00e4nen in den Augen,", "tokens": ["Wie", "oft", "er\u00b7z\u00e4hl", "ich", "ihr", "mit", "Thr\u00e4\u00b7nen", "in", "den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Liebe, die ihr mir ohn Eigennutz erzeigt;", "tokens": ["Die", "Lie\u00b7be", ",", "die", "ihr", "mir", "ohn", "Ei\u00b7gen\u00b7nutz", "er\u00b7zeigt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wie gro\u00df und treu dein Hertz, wie liebreich sch\u00f6n die Doris,", "tokens": ["Wie", "gro\u00df", "und", "treu", "dein", "Hertz", ",", "wie", "lieb\u00b7reich", "sch\u00f6n", "die", "Do\u00b7ris", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "ADJD", "ART", "NE", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.29": {"text": "Was Hilas uns f\u00fcr Lust gemacht.", "tokens": ["Was", "Hi\u00b7las", "uns", "f\u00fcr", "Lust", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Sie weint, und segnet euch mit aufgehobnen H\u00e4nden.", "tokens": ["Sie", "weint", ",", "und", "seg\u00b7net", "euch", "mit", "auf\u00b7ge\u00b7hob\u00b7nen", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Freund, ach warum sind wir getrennt!", "tokens": ["Freund", ",", "ach", "wa\u00b7rum", "sind", "wir", "ge\u00b7trennt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "XY", "PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Ach soll ich euch nicht ferner sehen!", "tokens": ["Ach", "soll", "ich", "euch", "nicht", "fer\u00b7ner", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VMFIN", "PPER", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Wo ist ein Freund, so edel, als wie du,", "tokens": ["Wo", "ist", "ein", "Freund", ",", "so", "e\u00b7del", ",", "als", "wie", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "$,", "KOUS", "PWAV", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Wo find ich, Doris, doch so eine holde Freundin?", "tokens": ["Wo", "find", "ich", ",", "Do\u00b7ris", ",", "doch", "so", "ei\u00b7ne", "hol\u00b7de", "Freun\u00b7din", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NE", "$,", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Vergesset euren Thirsis nur", "tokens": ["Ver\u00b7ges\u00b7set", "eu\u00b7ren", "Thir\u00b7sis", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Mein Damon, Doris, nicht, verge\u00dft, verge\u00dft mich nimmer.", "tokens": ["Mein", "Da\u00b7mon", ",", "Do\u00b7ris", ",", "nicht", ",", "ver\u00b7ge\u00dft", ",", "ver\u00b7ge\u00dft", "mich", "nim\u00b7mer", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "$,", "PTKNEG", "$,", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Was hab ich auf der Welt, als euch, das mich erfreut?", "tokens": ["Was", "hab", "ich", "auf", "der", "Welt", ",", "als", "euch", ",", "das", "mich", "er\u00b7freut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und l\u00e4st mein Unstern mich euch nicht mehr hier umarmen,", "tokens": ["Und", "l\u00e4st", "mein", "Uns\u00b7tern", "mich", "euch", "nicht", "mehr", "hier", "um\u00b7ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "PPER", "PTKNEG", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So seuftz ich nach der Ewigkeit,", "tokens": ["So", "seuftz", "ich", "nach", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Ach Freund, mit welcher Lust werd ich euch dort umfangen!", "tokens": ["Ach", "Freund", ",", "mit", "wel\u00b7cher", "Lust", "werd", "ich", "euch", "dort", "um\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "APPR", "PWAT", "NN", "VAFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein Damon, ewiglich von mir geliebter Freund!", "tokens": ["Mein", "Da\u00b7mon", ",", "e\u00b7wig\u00b7lich", "von", "mir", "ge\u00b7lieb\u00b7ter", "Freund", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dessen felsenfester Liebe", "tokens": ["Von", "des\u00b7sen", "fel\u00b7sen\u00b7fes\u00b7ter", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der gantzen Welt verlachter Sturm und Zorn", "tokens": ["Der", "gant\u00b7zen", "Welt", "ver\u00b7lach\u00b7ter", "Sturm", "und", "Zorn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein standhaft Hertz nicht w\u00fcrde reissen k\u00f6nnen;", "tokens": ["Mein", "stand\u00b7haft", "Hertz", "nicht", "w\u00fcr\u00b7de", "reis\u00b7sen", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "PTKNEG", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du dessen hoch und edlen Geist", "tokens": ["Du", "des\u00b7sen", "hoch", "und", "ed\u00b7len", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PDS", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Himmel, der uns einst besonders hold gewesen,", "tokens": ["Der", "Him\u00b7mel", ",", "der", "uns", "einst", "be\u00b7son\u00b7ders", "hold", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", 