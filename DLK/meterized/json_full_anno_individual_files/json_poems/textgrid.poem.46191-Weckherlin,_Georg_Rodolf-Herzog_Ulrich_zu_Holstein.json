{"textgrid.poem.46191": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Herzog Ulrich zu Holstein", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus dem gotlosen rat, da weder glaub noch treu,", "tokens": ["Aus", "dem", "got\u00b7lo\u00b7sen", "rat", ",", "da", "we\u00b7der", "glaub", "noch", "treu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "$,", "ADV", "KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und da man m\u00f6rderm\u00fcnch hoch in den himmel setzet,", "tokens": ["und", "da", "man", "m\u00f6r\u00b7der\u00b7m\u00fcnch", "hoch", "in", "den", "him\u00b7mel", "set\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADJD", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "aus der heillosen schul, da die abg\u00f6tterei", "tokens": ["aus", "der", "heil\u00b7lo\u00b7sen", "schul", ",", "da", "die", "ab\u00b7g\u00f6t\u00b7te\u00b7rei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die schuler in den luft ein reich zu blasen hetzet:", "tokens": ["die", "schu\u00b7ler", "in", "den", "luft", "ein", "reich", "zu", "bla\u00b7sen", "het\u00b7zet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "NN", "ART", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": 