{"textgrid.poem.63806": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "17. \u00bbDie sterbende Meduse\u00ab in Villa Ludovisi", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies jungfr\u00e4uliche Haupt, in des bitteren Todes Umnachtung", "tokens": ["Dies", "jung\u00b7fr\u00e4u\u00b7li\u00b7che", "Haupt", ",", "in", "des", "bit\u00b7te\u00b7ren", "To\u00b7des", "Um\u00b7nach\u00b7tung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Duldend geneigt, die stolz schwellende Braue, der Mund,", "tokens": ["Dul\u00b7dend", "ge\u00b7neigt", ",", "die", "stolz", "schwel\u00b7len\u00b7de", "Brau\u00b7e", ",", "der", "Mund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PRELS", "ADJD", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Nie von niedrigen Worten entweiht, von stummer Verachtung", "tokens": ["Nie", "von", "nied\u00b7ri\u00b7gen", "Wor\u00b7ten", "ent\u00b7weiht", ",", "von", "stum\u00b7mer", "Ver\u00b7ach\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Leise ger\u00fcmpft, noch jetzt, da er das Leben verhaucht \u2013", "tokens": ["Lei\u00b7se", "ge\u00b7r\u00fcmpft", ",", "noch", "jetzt", ",", "da", "er", "das", "Le\u00b7ben", "ver\u00b7haucht", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ADV", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$("], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Wie? ihr nennt sie Meduse? Des Haarschmucks seidene F\u00fclle", "tokens": ["Wie", "?", "ihr", "nennt", "sie", "Me\u00b7du\u00b7se", "?", "Des", "Haar\u00b7schmucks", "sei\u00b7de\u00b7ne", "F\u00fcl\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "PPER", "VVFIN", "PPER", "NN", "$.", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Ringelt an Wangen und Hals wirr sich zum Nacken hinab,", "tokens": ["Rin\u00b7gelt", "an", "Wan\u00b7gen", "und", "Hals", "wirr", "sich", "zum", "Na\u00b7cken", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "ADJD", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Wie von Todesschwei\u00dfe genetzt, vor Schauder erstarrend,", "tokens": ["Wie", "von", "To\u00b7des\u00b7schwei\u00b7\u00dfe", "ge\u00b7netzt", ",", "vor", "Schau\u00b7der", "er\u00b7star\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVPP", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Doch in Schlangen verkehrt nimmer sich dieses Gelock.", "tokens": ["Doch", "in", "Schlan\u00b7gen", "ver\u00b7kehrt", "nim\u00b7mer", "sich", "die\u00b7ses", "Ge\u00b7lock", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ADV", "PRF", "PDAT", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Nie feindselig wird dieses Antlitz blicken, das Leben", "tokens": ["Nie", "feind\u00b7se\u00b7lig", "wird", "die\u00b7ses", "Ant\u00b7litz", "bli\u00b7cken", ",", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PDAT", "NN", "VVINF", "$,", "ART", "NN"], "meter": "-+---+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Rings versteinernd; es sinkt willig hinab in die Nacht.", "tokens": ["Rings", "ver\u00b7stei\u00b7nernd", ";", "es", "sinkt", "wil\u00b7lig", "hin\u00b7ab", "in", "die", "Nacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$.", "PPER", "VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Denn hier oben im Lichte, der Brutstatt niedern Gez\u00fcchtes,", "tokens": ["Denn", "hier", "o\u00b7ben", "im", "Lich\u00b7te", ",", "der", "Bruts\u00b7tatt", "nie\u00b7dern", "Ge\u00b7z\u00fcch\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPRART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Wo in \u00fcppigem Flor nur das Gemeine gedeiht,", "tokens": ["Wo", "in", "\u00fcp\u00b7pi\u00b7gem", "Flor", "nur", "das", "Ge\u00b7mei\u00b7ne", "ge\u00b7deiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Ach, was hielte die Seele zur\u00fcck, die edelgeboren", "tokens": ["Ach", ",", "was", "hiel\u00b7te", "die", "See\u00b7le", "zu\u00b7r\u00fcck", ",", "die", "e\u00b7del\u00b7ge\u00b7bo\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "ADJA"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Ihresgleichen umsonst sucht in dem eklen Gew\u00fchl?", "tokens": ["Ih\u00b7res\u00b7glei\u00b7chen", "um\u00b7sonst", "sucht", "in", "dem", "ek\u00b7len", "Ge\u00b7w\u00fchl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Fremd durchwallt sie die Pfade des fr\u00f6hlichen Haufens; sie ist nicht", "tokens": ["Fremd", "durch\u00b7wallt", "sie", "die", "Pfa\u00b7de", "des", "fr\u00f6h\u00b7li\u00b7chen", "Hau\u00b7fens", ";", "sie", "ist", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "PTKNEG"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Wie die andern, sie hat nicht sich zu schmiegen gelernt.", "tokens": ["Wie", "die", "an\u00b7dern", ",", "sie", "hat", "nicht", "sich", "zu", "schmie\u00b7gen", "ge\u00b7lernt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "$,", "PPER", "VAFIN", "PTKNEG", "PRF", "PTKZU", "VVINF", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Hoffart schelten sie ihr den ruhigen Adel und Kaltsinn", "tokens": ["Hof\u00b7fart", "schel\u00b7ten", "sie", "ihr", "den", "ru\u00b7hi\u00b7gen", "A\u00b7del", "und", "Kal\u00b7tsinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "KON", "NN"], "meter": "+-+--+--+-+--++", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Ihre Trauer; als Schuld schm\u00e4hn sie ihr eigenstes Selbst.", "tokens": ["Ih\u00b7re", "Trau\u00b7er", ";", "als", "Schuld", "schm\u00e4hn", "sie", "ihr", "ei\u00b7gens\u00b7tes", "Selbst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "KOUS", "NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Nirgends ein ebenb\u00fcrtiges Gl\u00fcck im Leben, im Tod nur", "tokens": ["Nir\u00b7gends", "ein", "e\u00b7ben\u00b7b\u00fcr\u00b7ti\u00b7ges", "Gl\u00fcck", "im", "Le\u00b7ben", ",", "im", "Tod", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "ADV"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Darf sie sich hoheitsvoll ihrer Bestimmung erfreun.", "tokens": ["Darf", "sie", "sich", "ho\u00b7heits\u00b7voll", "ih\u00b7rer", "Be\u00b7stim\u00b7mung", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "++-+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Und die Gedankenlosen, die Lustigen, gehn an der Toten", "tokens": ["Und", "die", "Ge\u00b7dan\u00b7ken\u00b7lo\u00b7sen", ",", "die", "Lus\u00b7ti\u00b7gen", ",", "gehn", "an", "der", "To\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "---+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Unversteinert vorbei, h\u00f6chstens die Achseln gezuckt:", "tokens": ["Un\u00b7ver\u00b7stei\u00b7nert", "vor\u00b7bei", ",", "h\u00f6chs\u00b7tens", "die", "Ach\u00b7seln", "ge\u00b7zuckt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "\u00bbwarum wollte sie besser als andere sein? Nun hat sie's", "tokens": ["\u00bb", "wa\u00b7rum", "woll\u00b7te", "sie", "bes\u00b7ser", "als", "an\u00b7de\u00b7re", "sein", "?", "Nun", "hat", "sie's"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "ADJD", "KOKOM", "PIS", "VAINF", "$.", "ADV", "VAFIN", "NE"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.24": {"text": "Schlimmer als andere; ihr ist nach Verdienste geschehn.\u00ab", "tokens": ["Schlim\u00b7mer", "als", "an\u00b7de\u00b7re", ";", "ihr", "ist", "nach", "Ver\u00b7diens\u00b7te", "ge\u00b7schehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KOUS", "PIS", "$.", "PPER", "VAFIN", "APPR", "NN", "VVPP", "$.", "$("], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Und ihr nennt sie Meduse? O nennt sie die Muse der Tragik,", "tokens": ["Und", "ihr", "nennt", "sie", "Me\u00b7du\u00b7se", "?", "O", "nennt", "sie", "die", "Mu\u00b7se", "der", "Tra\u00b7gik", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "NN", "$.", "NE", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.26": {"text": "Und wer seelenverwandt, tr\u00f6ste sich dieses Gesichts!", "tokens": ["Und", "wer", "see\u00b7len\u00b7ver\u00b7wandt", ",", "tr\u00f6s\u00b7te", "sich", "die\u00b7ses", "Ge\u00b7sichts", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "$,", "VVFIN", "PRF", "PDAT", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Dies jungfr\u00e4uliche Haupt, in des bitteren Todes Umnachtung", "tokens": ["Dies", "jung\u00b7fr\u00e4u\u00b7li\u00b7che", "Haupt", ",", "in", "des", "bit\u00b7te\u00b7ren", "To\u00b7des", "Um\u00b7nach\u00b7tung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Duldend geneigt, die stolz schwellende Braue, der Mund,", "tokens": ["Dul\u00b7dend", "ge\u00b7neigt", ",", "die", "stolz", "schwel\u00b7len\u00b7de", "Brau\u00b7e", ",", "der", "Mund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PRELS", "ADJD", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Nie von niedrigen Worten entweiht, von stummer Verachtung", "tokens": ["Nie", "von", "nied\u00b7ri\u00b7gen", "Wor\u00b7ten", "ent\u00b7weiht", ",", "von", "stum\u00b7mer", "Ver\u00b7ach\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Leise ger\u00fcmpft, noch jetzt, da er das Leben verhaucht \u2013", "tokens": ["Lei\u00b7se", "ge\u00b7r\u00fcmpft", ",", "noch", "jetzt", ",", "da", "er", "das", "Le\u00b7ben", "ver\u00b7haucht", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "wo