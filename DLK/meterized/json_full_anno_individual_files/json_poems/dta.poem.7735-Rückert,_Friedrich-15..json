{"dta.poem.7735": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Fried' ist sprachverwandt wol mit der Freiheit auch;", "tokens": ["Der", "Fried'", "ist", "sprach\u00b7ver\u00b7wandt", "wol", "mit", "der", "Frei\u00b7heit", "auch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVFIN", "ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus Blut des Freiheitkampfs erbl\u00fcht des Friedens Strauch.", "tokens": ["Aus", "Blut", "des", "Frei\u00b7heit\u00b7kampfs", "er\u00b7bl\u00fcht", "des", "Frie\u00b7dens", "Strauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Freiheit macht dich frei, o Mensch, von der Natur,", "tokens": ["Die", "Frei\u00b7heit", "macht", "dich", "frei", ",", "o", "Mensch", ",", "von", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "FM", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch von der ew'gen nicht, von deiner eignen nur.", "tokens": ["Doch", "von", "der", "ew'\u00b7gen", "nicht", ",", "von", "dei\u00b7ner", "eig\u00b7nen", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "PTKNEG", "$,", "APPR", "PPOSAT", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gar mit der Freiheit nicht ist die Natur in Streit,", "tokens": ["Gar", "mit", "der", "Frei\u00b7heit", "nicht", "ist", "die", "Na\u00b7tur", "in", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKNEG", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur Du Entzweiter hast die Himmlischen entzweit.", "tokens": ["Nur", "Du", "Ent\u00b7zwei\u00b7ter", "hast", "die", "Himm\u00b7li\u00b7schen", "ent\u00b7zweit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur du Vers\u00f6hnter kannst die Himmlischen vers\u00f6hnen,", "tokens": ["Nur", "du", "Ver\u00b7s\u00f6hn\u00b7ter", "kannst", "die", "Himm\u00b7li\u00b7schen", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn Freiheit und Natur du neu verm\u00e4hlst im Sch\u00f6nen.", "tokens": ["Wenn", "Frei\u00b7heit", "und", "Na\u00b7tur", "du", "neu", "ver\u00b7m\u00e4hlst", "im", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}