{"textgrid.poem.52595": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "6. (Faust)", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum umarmst du nicht das schlanke M\u00e4dchen?", "tokens": ["Wa\u00b7rum", "um\u00b7armst", "du", "nicht", "das", "schlan\u00b7ke", "M\u00e4d\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie steht vor dir mit hei\u00dfem Blick \u2013", "tokens": ["Sie", "steht", "vor", "dir", "mit", "hei\u00b7\u00dfem", "Blick", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So k\u00fchn und sch\u00f6n, und du, du liebst das M\u00e4dchen,", "tokens": ["So", "k\u00fchn", "und", "sch\u00f6n", ",", "und", "du", ",", "du", "liebst", "das", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "KON", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und du, Verzagter, bebst zur\u00fcck?", "tokens": ["Und", "du", ",", "Ver\u00b7zag\u00b7ter", ",", "bebst", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie tr\u00e4gt dein Bildni\u00df in dem weichen Herzen,", "tokens": ["Sie", "tr\u00e4gt", "d