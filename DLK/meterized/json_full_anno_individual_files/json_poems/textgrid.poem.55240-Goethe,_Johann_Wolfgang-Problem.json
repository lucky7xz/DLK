{"textgrid.poem.55240": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Problem", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum ist alles so r\u00e4tselhaft?", "tokens": ["Wa\u00b7rum", "ist", "al\u00b7les", "so", "r\u00e4t\u00b7sel\u00b7haft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hier ist das Wollen, hier ist die Kraft;", "tokens": ["Hier", "ist", "das", "Wol\u00b7len", ",", "hier", "ist", "die", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Das Wollen will, die Kraft ist bereit,", "tokens": ["Das", "Wol\u00b7len", "will", ",", "die", "Kraft", "ist", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und daneben die sch\u00f6ne lange Zeit.", "tokens": ["Und", "da\u00b7ne\u00b7ben", "die", "sch\u00f6\u00b7ne", "lan\u00b7ge", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So seht doch hin, wo die gute Welt", "tokens": ["So", "seht", "doch", "hin", ",", "wo", "die", "gu\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Zusammenh\u00e4lt!", "tokens": ["Zu\u00b7sam\u00b7men\u00b7h\u00e4lt", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Seht hin, wo sie auseinanderf\u00e4llt!", "tokens": ["Seht", "hin", ",", "wo", "sie", "aus\u00b7ein\u00b7an\u00b7der\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Warum ist alles so r\u00e4tselhaft?", "tokens": ["Wa\u00b7rum", "ist", "al\u00b7les", "so", "r\u00e4t\u00b7sel\u00b7haft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hier ist das Wollen, hier ist die Kraft;", "tokens": ["Hier", "ist", "das", "Wol\u00b7len", ",", "hier", "ist", "die", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Das Wollen will, die Kraft ist bereit,", "tokens": ["Das", "Wol\u00b7len", "will", ",", "die", "Kraft", "ist", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und daneben die sch\u00f6ne lange Zeit.", "tokens": ["Und", "da\u00b7ne\u00b7ben", "die", "sch\u00f6\u00b7ne", "lan\u00b7ge", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "ADJA", "A