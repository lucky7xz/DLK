{"textgrid.poem.62852": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des gro\u00dfen Zoroasters Ruhm", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des gro\u00dfen Zoroasters Ruhm", "tokens": ["Des", "gro\u00b7\u00dfen", "Zo\u00b7roas\u00b7ters", "Ruhm"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "War durch ganz Orient verbreitet.", "tokens": ["War", "durch", "ganz", "O\u00b7rient", "ver\u00b7brei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADV", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von Oromaz ins Heiligthum", "tokens": ["Von", "O\u00b7ro\u00b7maz", "ins", "Hei\u00b7lig\u00b7thum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "APPRART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Der himmlischen Magie geleitet,", "tokens": ["Der", "himm\u00b7li\u00b7schen", "Ma\u00b7gie", "ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Trat er auf Hermes lichte Spur", "tokens": ["Trat", "er", "auf", "Her\u00b7mes", "lich\u00b7te", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und fand der plastischen Natur", "tokens": ["Und", "fand", "der", "plas\u00b7ti\u00b7schen", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Geheime Werkstatt aufgeriegelt,", "tokens": ["Ge\u00b7hei\u00b7me", "Werk\u00b7statt", "auf\u00b7ge\u00b7rie\u00b7gelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und las mit eines Sehers Blick", "tokens": ["Und", "las", "mit", "ei\u00b7nes", "Se\u00b7hers", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Nachwelt mystisches Geschick", "tokens": ["Der", "Nach\u00b7welt", "mys\u00b7ti\u00b7sches", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In der Gestirne Lauf entsiegelt.", "tokens": ["In", "der", "Ge\u00b7stir\u00b7ne", "Lauf", "ent\u00b7sie\u00b7gelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Von Vorwitz und von Muth befl\u00fcgelt,", "tokens": ["Von", "Vor\u00b7witz", "und", "von", "Muth", "be\u00b7fl\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Kam einst ein Prinz vom Indusstrand", "tokens": ["Kam", "einst", "ein", "Prinz", "vom", "In\u00b7dus\u00b7strand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Nach Persien, die Wunderlehren", "tokens": ["Nach", "Per\u00b7si\u00b7en", ",", "die", "Wun\u00b7der\u00b7leh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Des Philosophen anzuh\u00f6ren.", "tokens": ["Des", "Phi\u00b7lo\u00b7so\u00b7phen", "an\u00b7zu\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Er trat an seines F\u00fchrers Hand", "tokens": ["Er", "trat", "an", "sei\u00b7nes", "F\u00fch\u00b7rers", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Mit raschen Schritten auf die Br\u00fccke", "tokens": ["Mit", "ra\u00b7schen", "Schrit\u00b7ten", "auf", "die", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Des Geisterreichs. Die Scheidewand", "tokens": ["Des", "Geis\u00b7ter\u00b7reichs", ".", "Die", "Schei\u00b7de\u00b7wand"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Der K\u00f6rperwelt zog sich zur\u00fccke,", "tokens": ["Der", "K\u00f6r\u00b7per\u00b7welt", "zog", "sich", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und manches neue Sylphenland", "tokens": ["Und", "man\u00b7ches", "neu\u00b7e", "Syl\u00b7phen\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Lag aufgedeckt vor seinem Blicke.", "tokens": ["Lag", "auf\u00b7ge\u00b7deckt", "vor", "sei\u00b7nem", "Bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Erfahrung macht den Sch\u00fcler k\u00fchn;", "tokens": ["Er\u00b7fah\u00b7rung", "macht", "den", "Sch\u00fc\u00b7ler", "k\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Er wollte stets noch h\u00f6her steigen", "tokens": ["Er", "woll\u00b7te", "stets", "noch", "h\u00f6\u00b7her", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und bat im Heldentaumel ihn,", "tokens": ["Und", "bat", "im", "Hel\u00b7den\u00b7tau\u00b7mel", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Das Bild des Todes ihm zu zeigen.", "tokens": ["Das", "Bild", "des", "To\u00b7des", "ihm", "zu", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Der Weise ziehet einen Kreis", "tokens": ["Der", "Wei\u00b7se", "zie\u00b7het", "ei\u00b7nen", "Kreis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und schl\u00e4gt mit seinem goldnen Stecken", "tokens": ["Und", "schl\u00e4gt", "mit", "sei\u00b7nem", "gold\u00b7nen", "Ste\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Dreymal den Grund. Auf sein Gehei\u00df", "tokens": ["Drey\u00b7mal", "den", "Grund", ".", "Auf", "sein", "Ge\u00b7hei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "Erscheinet der Monarch der Schrecken.", "tokens": ["Er\u00b7schei\u00b7net", "der", "Mon\u00b7arch", "der", "Schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Gott! rief der Prinz in kalten Schwei\u00df", "tokens": ["Gott", "!", "rief", "der", "Prinz", "in", "kal\u00b7ten", "Schwei\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Getaucht, was seh ich? la\u00df mich fliehen!", "tokens": ["Ge\u00b7taucht", ",", "was", "seh", "ich", "?", "la\u00df", "mich", "flie\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWS", "VVFIN", "PPER", "$.", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Ha! welch ein scheu\u00dfliches Phantom,", "tokens": ["Ha", "!", "welch", "ein", "scheu\u00df\u00b7li\u00b7ches", "Phan\u00b7tom", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAT", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.32": {"text": "Aus dessen Auge, wie ein Strom,", "tokens": ["Aus", "des\u00b7sen", "Au\u00b7ge", ",", "wie", "ein", "Strom", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Des Orkus rothe Blitze spr\u00fchen!", "tokens": ["Des", "Or\u00b7kus", "ro\u00b7the", "Blit\u00b7ze", "spr\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Mit Schlangen ist sein Haupt geschm\u00fcckt", "tokens": ["Mit", "Schlan\u00b7gen", "ist", "sein", "Haupt", "ge\u00b7schm\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Und \u2013 seine Faust, o la\u00df mich fliehen!", "tokens": ["Und", "\u2013", "sei\u00b7ne", "Faust", ",", "o", "la\u00df", "mich", "flie\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PPOSAT", "NN", "$,", "FM", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "H\u00e4lt einen Dolch auf mich gez\u00fcckt.", "tokens": ["H\u00e4lt", "ei\u00b7nen", "Dolch", "auf", "mich", "ge\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Mein Sohn, versetzt der graue Weise", "tokens": ["Mein", "Sohn", ",", "ver\u00b7setzt", "der", "grau\u00b7e", "Wei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Und nahet l\u00e4chelnd sich dem Kreise,", "tokens": ["Und", "na\u00b7het", "l\u00e4\u00b7chelnd", "sich", "dem", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Ich sehe die Harpye nicht,", "tokens": ["Ich", "se\u00b7he", "die", "Har\u00b7pye", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.40": {"text": "Vor welcher deine Seele bebet;", "tokens": ["Vor", "wel\u00b7cher", "dei\u00b7ne", "See\u00b7le", "be\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Ein Engel ists, der vor mir schwebet,", "tokens": ["Ein", "En\u00b7gel", "ists", ",", "der", "vor", "mir", "schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Geh\u00fcllt in heitres Sonnenlicht:", "tokens": ["Ge\u00b7h\u00fcllt", "in", "heit\u00b7res", "Son\u00b7nen\u00b7licht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Sein Scheitel ist mit Mohn umkr\u00e4nzet,", "tokens": ["Sein", "Schei\u00b7tel", "ist", "mit", "Mohn", "um\u00b7kr\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Und wie ein Demantzepter gl\u00e4nzet", "tokens": ["Und", "wie", "ein", "De\u00b7mant\u00b7zep\u00b7ter", "gl\u00e4n\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "In seiner ausgestreckten Hand", "tokens": ["In", "sei\u00b7ner", "aus\u00b7ge\u00b7streck\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Der Schl\u00fcssel zu der Himmelspforte.", "tokens": ["Der", "Schl\u00fcs\u00b7sel", "zu", "der", "Him\u00b7mel\u00b7spfor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Itzt sprach er drey geheime Worte", "tokens": ["Itzt", "sprach", "er", "drey", "ge\u00b7hei\u00b7me", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Und das erhabne Bild verschwand.", "tokens": ["Und", "das", "er\u00b7hab\u00b7ne", "Bild", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Der J\u00fcngling weint am Hals des Alten:", "tokens": ["Der", "J\u00fcng\u00b7ling", "weint", "am", "Hals", "des", "Al\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Du siehst, fuhr dieser fort, mein Sohn,", "tokens": ["Du", "siehst", ",", "fuhr", "die\u00b7ser", "fort", ",", "mein", "Sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PDS", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Der Tod ist ein Cham\u00e4leon,", "tokens": ["Der", "Tod", "ist", "ein", "Cha\u00b7m\u00e4\u00b7le\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Er borget immer die Gestalten", "tokens": ["Er", "bor\u00b7get", "im\u00b7mer", "die", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Der Seelen, denen er sich weist.", "tokens": ["Der", "See\u00b7len", ",", "de\u00b7nen", "er", "sich", "weist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und so, Geliebte, wird dein Geist", "tokens": ["Und", "so", ",", "Ge\u00b7lieb\u00b7te", ",", "wird", "dein", "Geist"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihm der Tugend Bild erblicken,", "tokens": ["In", "ihm", "der", "Tu\u00b7gend", "Bild", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das ich mit t\u00e4uschendem Entz\u00fccken", "tokens": ["Das", "ich", "mit", "t\u00e4u\u00b7schen\u00b7dem", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schon oft statt deines Bilds gegr\u00fc\u00dft.", "tokens": ["Schon", "oft", "statt", "dei\u00b7nes", "Bilds", "ge\u00b7gr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Entferne, Gott, die gro\u00dfe Scene,", "tokens": ["Ent\u00b7fer\u00b7ne", ",", "Gott", ",", "die", "gro\u00b7\u00dfe", "Sce\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis mich ein Aschenkrug verschlie\u00dft,", "tokens": ["Bis", "mich", "ein", "A\u00b7schen\u00b7krug", "ver\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und meiner Freundin stille Thr\u00e4ne", "tokens": ["Und", "mei\u00b7ner", "Freun\u00b7din", "stil\u00b7le", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf meinen Staub geflossen ist.", "tokens": ["Auf", "mei\u00b7nen", "Staub", "ge\u00b7flos\u00b7sen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Des gro\u00dfen Zoroasters Ruhm", "tokens": ["Des", "gro\u00b7\u00dfen", "Zo\u00b7roas\u00b7ters", "Ruhm"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "War durch ganz Orient verbreitet.", "tokens": ["War", "durch", "ganz", "O\u00b7rient", "ver\u00b7brei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADV", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von Oromaz ins Heiligthum", "tokens": ["Von", "O\u00b7ro\u00b7maz", "ins", "Hei\u00b7lig\u00b7thum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "APPRART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Der himmlischen Magie geleitet,", "tokens": ["Der", "himm\u00b7li\u00b7schen", "Ma\u00b7gie", "ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Trat er auf Hermes lichte Spur", "tokens": ["Trat", "er", "auf", "Her\u00b7mes", "lich\u00b7te", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und fand der plastischen Natur", "tokens": ["Und", "fand", "der", "plas\u00b7ti\u00b7schen", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Geheime Werkstatt aufgeriegelt,", "tokens": ["Ge\u00b7hei\u00b7me", "Werk\u00b7statt", "auf\u00b7ge\u00b7rie\u00b7gelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und las mit eines Sehers Blick", "tokens": ["Und", "las", "mit", "ei\u00b7nes", "Se\u00b7hers", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Nachwelt mystisc