{"textgrid.poem.55014": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Die Freuden", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es flattert um die Quelle", "tokens": ["Es", "flat\u00b7tert", "um", "die", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die wechselnde Libelle,", "tokens": ["Die", "wech\u00b7seln\u00b7de", "Li\u00b7bel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mich freut sie lange schon;", "tokens": ["Mich", "freut", "sie", "lan\u00b7ge", "schon", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bald dunkel und bald helle,", "tokens": ["Bald", "dun\u00b7kel", "und", "bald", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie der Cham\u00e4leon,", "tokens": ["Wie", "der", "Cha\u00b7m\u00e4\u00b7le\u00b7on", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Bald rot, bald blau,", "tokens": ["Bald", "rot", ",", "bald", "blau", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Bald blau, bald gr\u00fcn;", "tokens": ["Bald", "blau", ",", "bald", "gr\u00fcn", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "O da\u00df ich in der N\u00e4he", "tokens": ["O", "da\u00df", "ich", "in", "der", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.9": {"text": "Doch ihre Farben s\u00e4he!", "tokens": ["Doch", "ih\u00b7re", "Far\u00b7ben", "s\u00e4\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie schwirrt und schwebet, rastet nie!", "tokens": ["Sie", "schwirrt", "und", "schwe\u00b7bet", ",", "ras\u00b7tet", "nie", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch still, sie setzt sich an die Weiden.", "tokens": ["Doch", "still", ",", "sie", "setzt", "sich", "an", "die", "Wei\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da hab ich sie! Da hab ich sie!", "tokens": ["Da", "hab", "ich", "sie", "!", "Da", "hab", "ich", "sie", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$.", "ADV", "VAFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nun betracht ich sie genau", "tokens": ["Und", "nun", "be\u00b7tracht", "ich", "sie", "ge\u00b7nau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und seh ein traurig dunkles Blau \u2013", "tokens": ["Und", "seh", "ein", "trau\u00b7rig", "dunk\u00b7les", "Blau", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So geht es dir, Zergliedrer deiner Freuden!", "tokens": ["So", "geht", "es", "dir", ",", "Zer\u00b7glied\u00b7rer", "dei\u00b7ner", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es flattert um die Quelle", "tokens": ["Es", "flat\u00b7tert", "um", "die", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die wechselnde Libelle,", "tokens": ["Die", "wech\u00b7seln\u00b7de", "Li\u00b7bel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mich freut sie lange schon;", "tokens": ["Mich", "freut", "sie", "lan\u00b7ge", "schon", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bald dunkel und bald helle,", "tokens": ["Bald", "dun\u00b7kel", "und", "bald", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie der Cham\u00e4leon,",