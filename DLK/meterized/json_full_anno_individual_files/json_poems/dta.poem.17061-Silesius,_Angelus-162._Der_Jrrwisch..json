{"dta.poem.17061": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "162.  Der Jrrwisch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wer ohne Liebe laufft/ komt nicht ins Himmelreich:", "tokens": ["Wer", "oh\u00b7ne", "Lie\u00b7be", "laufft", "/", "komt", "nicht", "ins", "Him\u00b7mel\u00b7reich", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVFIN", "$(", "VVFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er springt bald hin bald her/ ist einem Jrrwisch gleich.", "tokens": ["Er", "springt", "bald", "hin", "bald", "her", "/", "ist", "ei\u00b7nem", "Jrr\u00b7wisch", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "PTKVZ", "$(", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}