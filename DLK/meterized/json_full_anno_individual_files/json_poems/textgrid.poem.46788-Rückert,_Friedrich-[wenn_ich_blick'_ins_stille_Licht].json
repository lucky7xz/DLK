{"textgrid.poem.46788": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wenn ich blick' ins stille Licht]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich blick' ins stille Licht", "tokens": ["Wenn", "ich", "blick'", "ins", "stil\u00b7le", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in Sinnen mich verlor,", "tokens": ["Und", "in", "Sin\u00b7nen", "mich", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Scheint es sich zu spalten,", "tokens": ["Scheint", "es", "sich", "zu", "spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Licht tritt aus dem Licht hervor,", "tokens": ["Licht", "tritt", "aus", "dem", "Licht", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Welchs ich soll, ich wei\u00df es nicht,", "tokens": ["Welchs", "ich", "soll", ",", "ich", "wei\u00df", "es", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcr das wahre halten.", "tokens": ["F\u00fcr", "das", "wah\u00b7re", "hal\u00b7ten", "."], "token_info": ["word