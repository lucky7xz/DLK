{"dta.poem.20676": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Lobgesang  \n nach t\u00f6dtlichem Schmerz unter meinen Kindern  \n gesungen  \n am 6ten December 1789.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lobet den Sch\u00f6pfer, der Himmel und Erde gegr\u00fcndet,                 ", "tokens": ["Lo\u00b7bet", "den", "Sch\u00f6p\u00b7fer", ",", "der", "Him\u00b7mel", "und", "Er\u00b7de", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "Der uns wohlth\u00e4tig die Fackel des Tages entz\u00fcndet,", "tokens": ["Der", "uns", "wohlt\u00b7h\u00e4\u00b7tig", "die", "Fa\u00b7ckel", "des", "Ta\u00b7ges", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der f\u00fcr die Nacht flimmernde Sterne gemacht,", "tokens": ["Der", "f\u00fcr", "die", "Nacht", "flim\u00b7mern\u00b7de", "Ster\u00b7ne", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Lieblich den Monden ger\u00fcndet.", "tokens": ["Lieb\u00b7lich", "den", "Mon\u00b7den", "ge\u00b7r\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Lobet den Helfer, den m\u00e4chtigen Retter aus N\u00f6then,", "tokens": ["Lo\u00b7bet", "den", "Hel\u00b7fer", ",", "den", "m\u00e4ch\u00b7ti\u00b7gen", "Ret\u00b7ter", "aus", "N\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "Feurige Pfeile des Todes, die wollten mich t\u00f6dten;", "tokens": ["Feu\u00b7ri\u00b7ge", "Pfei\u00b7le", "des", "To\u00b7des", ",", "die", "woll\u00b7ten", "mich", "t\u00f6d\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "PRELS", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.3": {"text": "Aber ich blieb, habe die Rosen schon lieb,", "tokens": ["A\u00b7ber", "ich", "blieb", ",", "ha\u00b7be", "die", "Ro\u00b7sen", "schon", "lieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die sich mir k\u00fcnftig noch r\u00f6then.", "tokens": ["Die", "sich", "mir", "k\u00fcnf\u00b7tig", "noch", "r\u00f6\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PPER", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Danket dem gro\u00dfen allg\u00fctigen Vater mein Leben,", "tokens": ["Dan\u00b7ket", "dem", "gro\u00b7\u00dfen", "all\u00b7g\u00fc\u00b7ti\u00b7gen", "Va\u00b7ter", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "Da\u00df ich nun wieder dies Auge kann freudig erheben;", "tokens": ["Da\u00df", "ich", "nun", "wie\u00b7der", "dies", "Au\u00b7ge", "kann", "freu\u00b7dig", "er\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PDS", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Feyert ein Fest, da\u00df er im Hause mich l\u00e4\u00dft,", "tokens": ["Fe\u00b7yert", "ein", "Fest", ",", "da\u00df", "er", "im", "Hau\u00b7se", "mich", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Mir durch den K\u00f6nig gegeben.", "tokens": ["Mir", "durch", "den", "K\u00f6\u00b7nig", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Preiset den Herrscher, der Str\u00f6me mit Ufern um-", "tokens": ["Prei\u00b7set", "den", "Herr\u00b7scher", ",", "der", "Str\u00f6\u00b7me", "mit", "U\u00b7fern", "um"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "APPR", "NN", "TRUNC"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "schr\u00e4nket,", "tokens": ["schr\u00e4n\u00b7ket", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der auch wie B\u00e4che die Herzen der K\u00f6nige lenket;", "tokens": ["Der", "auch", "wie", "B\u00e4\u00b7che", "die", "Her\u00b7zen", "der", "K\u00f6\u00b7ni\u00b7ge", "len\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KOKOM", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Preiset ihn laut, da\u00df er mir Gaben vertraut,", "tokens": ["Prei\u00b7set", "ihn", "laut", ",", "da\u00df", "er", "mir", "Ga\u00b7ben", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$,", "KOUS", "PPER", "PPER", "NN", "ADJD", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Da\u00df er mir Freunde geschenket.", "tokens": ["Da\u00df", "er", "mir", "Freun\u00b7de", "ge\u00b7schen\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Meine Gef\u00fchle des Dankes die sollen ihm gl\u00fchen,", "tokens": ["Mei\u00b7ne", "Ge\u00b7f\u00fch\u00b7le", "des", "Dan\u00b7kes", "die", "sol\u00b7len", "ihm", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ART", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "Bis mir die lobende Seele vom Munde wird fliehen", "tokens": ["Bis", "mir", "die", "lo\u00b7ben\u00b7de", "See\u00b7le", "vom", "Mun\u00b7de", "wird", "flie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "VAFIN", "VVINF"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ueber den Mond, wo sie Gefilde bewohnt", "tokens": ["Ue\u00b7ber", "den", "Mond", ",", "wo", "sie", "Ge\u00b7fil\u00b7de", "be\u00b7wohnt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PPER", "NN", "VVPP"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Ewig geschaffen zum Bl\u00fchen.", "tokens": ["E\u00b7wig", "ge\u00b7schaf\u00b7fen", "zum", "Bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}