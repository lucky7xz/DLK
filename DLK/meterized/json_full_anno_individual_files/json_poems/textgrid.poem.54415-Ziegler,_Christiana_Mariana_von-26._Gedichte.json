{"textgrid.poem.54415": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "26. Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rast wie ihr wollt, ihr unversch\u00e4mten Neider!", "tokens": ["Rast", "wie", "ihr", "wollt", ",", "ihr", "un\u00b7ver\u00b7sch\u00e4m\u00b7ten", "Nei\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "VMFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich klag und zittre nicht,", "tokens": ["Ich", "klag", "und", "zitt\u00b7re", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf! zeiget eure Schw\u00e4che!", "tokens": ["Auf", "!", "zei\u00b7get", "eu\u00b7re", "Schw\u00e4\u00b7che", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn mir zu viel geschicht,", "tokens": ["Wenn", "mir", "zu", "viel", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "PIS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mu\u00df ich der Thorheit lachen.", "tokens": ["Mu\u00df", "ich", "der", "Thor\u00b7heit", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr m\u00f6get nur aus mir sehr wenig machen.", "tokens": ["Ihr", "m\u00f6\u00b7get", "nur", "aus", "mir", "sehr", "we\u00b7nig", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ihr werdet, doch vielleicht zu sp\u00e4te, sehn,", "tokens": ["Ihr", "wer\u00b7det", ",", "doch", "viel\u00b7leicht", "zu", "sp\u00e4\u00b7te", ",", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADV", "ADV", "PTKZU", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df ich auf edle Art mich an euch r\u00e4che.", "tokens": ["Da\u00df", "ich", "auf", "ed\u00b7le", "Art", "mich", "an", "euch", "r\u00e4\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wie wollt ihr da bestehn,", "tokens": ["Wie", "wollt", "ihr", "da", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Wenn man einst wird von meiner Gro\u00dfmuth sprechen:", "tokens": ["Wenn", "man", "einst", "wird", "von", "mei\u00b7ner", "Gro\u00df\u00b7muth", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Sie steiget doch, wenn man sie gleich will schw\u00e4chen.", "tokens": ["Sie", "stei\u00b7get", "doch", ",", "wenn", "man", "sie", "gleich", "will", "schw\u00e4\u00b7chen", "."], "token_info": ["w