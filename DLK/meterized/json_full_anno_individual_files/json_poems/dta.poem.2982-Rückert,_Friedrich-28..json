{"dta.poem.2982": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Glaubenseiferer, gesendet aus dem Westen,", "tokens": ["Die", "Glau\u00b7ben\u00b7sei\u00b7fe\u00b7rer", ",", "ge\u00b7sen\u00b7det", "aus", "dem", "Wes\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um zu ersch\u00fcttern hier uralte Glaubensvesten;", "tokens": ["Um", "zu", "er\u00b7sch\u00fct\u00b7tern", "hier", "ur\u00b7al\u00b7te", "Glau\u00b7bens\u00b7ves\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gesegnet sei der Bau, der neue, den sie gr\u00fcnden,", "tokens": ["Ge\u00b7seg\u00b7net", "sei", "der", "Bau", ",", "der", "neu\u00b7e", ",", "den", "sie", "gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Lehre, die sie auf den Stra\u00dfen laut verk\u00fcnden!", "tokens": ["Die", "Leh\u00b7re", ",", "die", "sie", "auf", "den", "Stra\u00b7\u00dfen", "laut", "ver\u00b7k\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nichts Neues sagen sie den eingeweihten Brahmen,", "tokens": ["Nichts", "Neu\u00b7es", "sa\u00b7gen", "sie", "den", "ein\u00b7ge\u00b7weih\u00b7ten", "Brah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die aus der V\u00e4ter Mund ein Gleiches l\u00e4ngst vernahmen;", "tokens": ["Die", "aus", "der", "V\u00e4\u00b7ter", "Mund", "ein", "Glei\u00b7ches", "l\u00e4ngst", "ver\u00b7nah\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was jeder Vater sagt ins Ohr dem Sohne nur,", "tokens": ["Was", "je\u00b7der", "Va\u00b7ter", "sagt", "ins", "Ohr", "dem", "Soh\u00b7ne", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann diesem umgethan wird der Einweihung Schnur:", "tokens": ["Wann", "die\u00b7sem", "um\u00b7ge\u00b7than", "wird", "der", "Ein\u00b7wei\u00b7hung", "Schnur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "VVPP", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Mein Sohn, es ist ein Gott, ein einz'ger Gott allein,", "tokens": ["Mein", "Sohn", ",", "es", "ist", "ein", "Gott", ",", "ein", "einz'\u00b7ger", "Gott", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alle G\u00f6tter sind ein Bild nur und ein Schein.", "tokens": ["Und", "al\u00b7le", "G\u00f6t\u00b7ter", "sind", "ein", "Bild", "nur", "und", "ein", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ART", "NN", "ADV", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Denselben einen Gott sollst du im Stillen ehren,", "tokens": ["Den\u00b7sel\u00b7ben", "ei\u00b7nen", "Gott", "sollst", "du", "im", "Stil\u00b7len", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ART", "NN", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch das Geheimni\u00df nie ans Licht des Tages kehren.", "tokens": ["Doch", "das", "Ge\u00b7heim\u00b7ni\u00df", "nie", "ans", "Licht", "des", "Ta\u00b7ges", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Des Volkes Aug' ist f\u00fcr das reine Licht nicht reif,", "tokens": ["Des", "Vol\u00b7kes", "Aug'", "ist", "f\u00fcr", "das", "rei\u00b7ne", "Licht", "nicht", "reif", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und freut der T\u00e4uschung sich am bunten Farbenstreif.", "tokens": ["Und", "freut", "der", "T\u00e4u\u00b7schung", "sich", "am", "bun\u00b7ten", "Far\u00b7ben\u00b7streif", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Das ist das Licht, das wir im Innern allzeit hatten,", "tokens": ["Das", "ist", "das", "Licht", ",", "das", "wir", "im", "In\u00b7nern", "all\u00b7zeit", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das wirft nach au\u00dfenhin die euch verha\u00dften Schatten.", "tokens": ["Das", "wirft", "nach", "au\u00b7\u00dfen\u00b7hin", "die", "euch", "ver\u00b7ha\u00df\u00b7ten", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADV", "ART", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Licht und Geheimnis wollt ihr kehren nun heraus;", "tokens": ["Licht", "und", "Ge\u00b7heim\u00b7nis", "wollt", "ihr", "keh\u00b7ren", "nun", "he\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und geht nicht aus das Licht, geht das Geheimnis aus.", "tokens": ["Und", "geht", "nicht", "aus", "das", "Licht", ",", "geht", "das", "Ge\u00b7heim\u00b7nis", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}