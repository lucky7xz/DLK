{"textgrid.poem.35085": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Kindschaft", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war ein Kind, als hilflos ich gelegen", "tokens": ["Ich", "war", "ein", "Kind", ",", "als", "hil\u00b7flos", "ich", "ge\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KOUS", "ADJD", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Arm der Liebe, die mich einst gebar,", "tokens": ["Im", "Arm", "der", "Lie\u00b7be", ",", "die", "mich", "einst", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und diese Kindheit wurde mir zum Segen,", "tokens": ["Und", "die\u00b7se", "Kind\u00b7heit", "wur\u00b7de", "mir", "zum", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als au\u00dfer ihr mir nichts zum Segen war.", "tokens": ["Als", "au\u00b7\u00dfer", "ihr", "mir", "nichts", "zum", "Se\u00b7gen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "PPER", "PIS", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich blieb ein Kind und ruhte wohlgebettet", "tokens": ["Ich", "blieb", "ein", "Kind", "und", "ruh\u00b7te", "wohl\u00b7ge\u00b7bet\u00b7tet"], "token_info": ["word"