{"textgrid.poem.32440": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "119. Auf die Genesung einer Buhlerin", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Tode wurde j\u00fcngst vom Pluto anbefohlen,", "tokens": ["Dem", "To\u00b7de", "wur\u00b7de", "j\u00fcngst", "vom", "Plu\u00b7to", "an\u00b7be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Lais unsrer Stadt nach jener Welt zu holen.", "tokens": ["Die", "Lais", "uns\u00b7rer", "Stadt", "nach", "je\u00b7ner", "Welt", "zu", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie war so alt doch nicht, und reizte manchen noch,", "tokens": ["Sie", "war", "so", "alt", "doch", "nicht", ",", "und", "reiz\u00b7te", "man\u00b7chen", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADV", "PTKNEG", "$,", "KON", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch Willigkeit und Scherz in ihr gem\u00e4chlich Joch.", "tokens": ["Durch", "Wil\u00b7lig\u00b7keit", "und", "Scherz", "in", "ihr", "ge\u00b7m\u00e4ch\u00b7lich", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-