{"dta.poem.4386": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Adam,  \n wie er im Paradiese alle Thiere benennet,  \n in einem  \n unvergleichlichen Kupfer-St\u00fcck  \n  von Hrn. Ridinger vorgestellet,  \n und folgendergestalt bewundert.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer konnt' uns herrlicher, als Moses, das Paradies,", "tokens": ["Wer", "konnt'", "uns", "herr\u00b7li\u00b7cher", ",", "als", "Mo\u00b7ses", ",", "das", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADJA", "$,", "KOUS", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "mit Worten, schildern?", "tokens": ["mit", "Wor\u00b7ten", ",", "schil\u00b7dern", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wer zeiget solches herrlicher, als ", "tokens": ["Wer", "zei\u00b7get", "sol\u00b7ches", "herr\u00b7li\u00b7cher", ",", "als"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "PIAT", "ADJA", "$,", "KOUS"], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}, "line.4": {"text": "in Bildern?", "tokens": ["in", "Bil\u00b7dern", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Absonderlich stellt uns sein Geist vom Thier-Heer und der", "tokens": ["Ab\u00b7son\u00b7der\u00b7lich", "stellt", "uns", "sein", "Geist", "vom", "Thier\u00b7Heer", "und", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "KON", "ART"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "V\u00f6gel Chor,", "tokens": ["V\u00f6\u00b7gel", "Chor", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Auf diesem Blatt, nicht nur die C\u00f6rper, auch ihre Geister,", "tokens": ["Auf", "die\u00b7sem", "Blatt", ",", "nicht", "nur", "die", "C\u00f6r\u00b7per", ",", "auch", "ih\u00b7re", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PTKNEG", "ADV", "ART", "NN", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "lebend vor.", "tokens": ["le\u00b7bend", "vor", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Welch ein verwunderlich Gew\u00fchl ganz unterschiedner", "tokens": ["Welch", "ein", "ver\u00b7wun\u00b7der\u00b7lich", "Ge\u00b7w\u00fchl", "ganz", "un\u00b7ter\u00b7schied\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJD", "NN", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Creaturen!", "tokens": ["Crea\u00b7tu\u00b7ren", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Welch ein Gemisch von Huld und Schrecken! Welch eine", "tokens": ["Welch", "ein", "Ge\u00b7misch", "von", "Huld", "und", "Schre\u00b7cken", "!", "Welch", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ART", "NN", "APPR", "NN", "KON", "NN", "$.", "PIAT", "ART"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "holde Harmonie,", "tokens": ["hol\u00b7de", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Auch in so gar verschiednen Sinnen und Eigenschaften,", "tokens": ["Auch", "in", "so", "gar", "ver\u00b7schied\u00b7nen", "Sin\u00b7nen", "und", "Ei\u00b7gen\u00b7schaf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "zeigen sie!", "tokens": ["zei\u00b7gen", "sie", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Man sieht in ihrer aller Blicken, von inniger Vergn\u00fcgung,", "tokens": ["Man", "sieht", "in", "ih\u00b7rer", "al\u00b7ler", "Bli\u00b7cken", ",", "von", "in\u00b7ni\u00b7ger", "Ver\u00b7gn\u00fc\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "PIAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Spuren", "tokens": ["Spu\u00b7ren"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Und, einen s\u00fcssen Trieb zur Eintracht, in so verschiedlichen", "tokens": ["Und", ",", "ei\u00b7nen", "s\u00fcs\u00b7sen", "Trieb", "zur", "Ein\u00b7tracht", ",", "in", "so", "ver\u00b7schied\u00b7li\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$,", "APPR", "ADV", "VVINF"], "meter": "-+-+-+--+-+-+--", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Naturen.", "tokens": ["Na\u00b7tu\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Allein, in welcher edlen Stellung, in welcher sch\u00f6nen", "tokens": ["Al\u00b7lein", ",", "in", "wel\u00b7cher", "ed\u00b7len", "Stel\u00b7lung", ",", "in", "wel\u00b7cher", "sch\u00f6\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "PWAT", "ADJA", "NN", "$,", "APPR", "PWAT", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Symmetrie,", "tokens": ["Sym\u00b7me\u00b7trie", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Erscheinet, fast als wenn er gl\u00e4nzte, des Menschen C\u00f6rpers", "tokens": ["Er\u00b7schei\u00b7net", ",", "fast", "als", "wenn", "er", "gl\u00e4nz\u00b7te", ",", "des", "Men\u00b7schen", "C\u00f6r\u00b7pers"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "KOKOM", "KOUS", "PPER", "VVFIN", "$,", "ART", "NN", "NE"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Wunder-Bau,", "tokens": ["Wun\u00b7der\u00b7Bau", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "In dessen Gliedern ich, erstaunt, ein Meister-St\u00fcck der", "tokens": ["In", "des\u00b7sen", "Glie\u00b7dern", "ich", ",", "er\u00b7staunt", ",", "ein", "Meis\u00b7ter\u00b7St\u00fcck", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "PPER", "$,", "VVPP", "$,", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sch\u00f6pfung schau?", "tokens": ["Sch\u00f6p\u00b7fung", "schau", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}