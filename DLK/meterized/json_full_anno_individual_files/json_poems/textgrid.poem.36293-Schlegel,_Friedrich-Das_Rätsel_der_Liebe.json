{"textgrid.poem.36293": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das R\u00e4tsel der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob jugendlich der Dichter seine Trauer,", "tokens": ["Ob", "ju\u00b7gend\u00b7lich", "der", "Dich\u00b7ter", "sei\u00b7ne", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Herzen hingegossen, dar uns stellte,", "tokens": ["Von", "Her\u00b7zen", "hin\u00b7ge\u00b7gos\u00b7sen", ",", "dar", "uns", "stell\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der alte Meister sie mit stiller K\u00e4lte", "tokens": ["Der", "al\u00b7te", "Meis\u00b7ter", "sie", "mit", "stil\u00b7ler", "K\u00e4l\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Steine ausgepr\u00e4gt zu ew'ger Dauer;", "tokens": ["Im", "Stei\u00b7ne", "aus\u00b7ge\u00b7pr\u00e4gt", "zu", "ew'\u00b7ger", "Dau\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Feuer ist's, hier gl\u00fchend warm, dort lauer,", "tokens": ["Ein", "Feu\u00b7er", "ist's", ",", "hier", "gl\u00fc\u00b7hend", "warm", ",", "dort", "lau\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "wo