{"textgrid.poem.60140": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erheb auf mich dein Angesicht,", "genre": "verse", "period": "N.A.", "pub_year": 1756, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erheb auf mich dein Angesicht,", "tokens": ["Er\u00b7heb", "auf", "mich", "dein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und la\u00df mich deine G\u00fcte schmecken", "tokens": ["Und", "la\u00df", "mich", "dei\u00b7ne", "G\u00fc\u00b7te", "schme\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gott, der mich schuf! Es mag auch Dunkel oder Licht,", "tokens": ["Gott", ",", "der", "mich", "schuf", "!", "Es", "mag", "auch", "Dun\u00b7kel", "o\u00b7der", "Licht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "$.", "PPER", "VMFIN", "ADV", "ADJD", "KON", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Vor meinem Auge dich verdecken;", "tokens": ["Vor", "mei\u00b7nem", "Au\u00b7ge", "dich", "ver\u00b7de\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Herr! es mag ein Feuer-Meer", "tokens": ["O", "Herr", "!", "es", "mag", "ein", "Feu\u00b7er\u00b7Meer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PPER", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In tausend Str\u00f6men dich umgeben;", "tokens": ["In", "tau\u00b7send", "Str\u00f6\u00b7men", "dich", "um\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verkleide dich im Sturm, und lasse rings umher", "tokens": ["Ver\u00b7klei\u00b7de", "dich", "im", "Sturm", ",", "und", "las\u00b7se", "rings", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "KON", "VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Welt vor deinem Wetter beben;", "tokens": ["Die", "Welt", "vor", "dei\u00b7nem", "Wet\u00b7ter", "be\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df deinen Blick, voll Gottes Macht,", "tokens": ["La\u00df", "dei\u00b7nen", "Blick", ",", "voll", "Got\u00b7tes", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "ADJD", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Berg, die Felsen niederblitzen;", "tokens": ["Den", "Berg", ",", "die", "Fel\u00b7sen", "nie\u00b7der\u00b7blit\u00b7zen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verh\u00fclle deine Stirn mit Zorn und lasse Nacht,", "tokens": ["Ver\u00b7h\u00fcl\u00b7le", "dei\u00b7ne", "Stirn", "mit", "Zorn", "und", "las\u00b7se", "Nach