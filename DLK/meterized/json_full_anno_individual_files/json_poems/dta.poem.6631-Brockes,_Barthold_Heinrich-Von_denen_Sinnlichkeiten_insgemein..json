{"dta.poem.6631": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Rose, wenn ihr auf das neu", "tokens": ["Die", "Ro\u00b7se", ",", "wenn", "ihr", "auf", "das", "neu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der kluge Phrygier die Stimme lieh\u2019,", "tokens": ["Der", "klu\u00b7ge", "Phry\u00b7gier", "die", "Stim\u00b7me", "lieh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Spr\u00e4ch eben: da\u00df der Lentz, verliebt in sie,", "tokens": ["Spr\u00e4ch", "e\u00b7ben", ":", "da\u00df", "der", "Lentz", ",", "ver\u00b7liebt", "in", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "KOUS", "ART", "NN", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nur blo\u00df darum erschienen sey,", "tokens": ["Nur", "blo\u00df", "da\u00b7rum", "er\u00b7schie\u00b7nen", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PAV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den lieblichen Geruch, die Farben, die sie schm\u00fccken", "tokens": ["Den", "lieb\u00b7li\u00b7chen", "Ge\u00b7ruch", ",", "die", "Far\u00b7ben", ",", "die", "sie", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Voll holder Gluht, zu schmecken, zu erblicken.", "tokens": ["Voll", "hol\u00b7der", "Gluht", ",", "zu", "schme\u00b7cken", ",", "zu", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Aus ihrem Thron, von Dornen ausgespannt,", "tokens": ["Aus", "ih\u00b7rem", "Thron", ",", "von", "Dor\u00b7nen", "aus\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "W\u00e4r sie in eignem Glantz und holdem Schmuck entbrannt.", "tokens": ["W\u00e4r", "sie", "in", "eig\u00b7nem", "Glantz", "und", "hol\u00b7dem", "Schmuck", "ent\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Blumen-K\u00f6nigin, die so viel Lieblichkeit", "tokens": ["Die", "Blu\u00b7men\u00b7K\u00f6\u00b7ni\u00b7gin", ",", "die", "so", "viel", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In ihren eignen Wesen sp\u00fcrt,", "tokens": ["In", "ih\u00b7ren", "eig\u00b7nen", "We\u00b7sen", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sagt\u2019 ohne Zweiffel selbst von ihr:", "tokens": ["Sagt'", "oh\u00b7ne", "Zweif\u00b7fel", "selbst", "von", "ihr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADV", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sie sey der sch\u00f6nen Zeit,", "tokens": ["Sie", "sey", "der", "sch\u00f6\u00b7nen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Der angenehmen Tage, Zier,", "tokens": ["Der", "an\u00b7ge\u00b7neh\u00b7men", "Ta\u00b7ge", ",", "Zier", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df nebst den G\u00f6tterchen der Lieb\u2019 auch sie regier\u2019", "tokens": ["Da\u00df", "nebst", "den", "G\u00f6t\u00b7ter\u00b7chen", "der", "Lieb'", "auch", "sie", "re\u00b7gier'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und da\u00df die Zephir\u2019 ihrentwegen", "tokens": ["Und", "da\u00df", "die", "Ze\u00b7phir'", "ih\u00b7rent\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Allein, die lauen Fl\u00fcgel regen.", "tokens": ["Al\u00b7lein", ",", "die", "lau\u00b7en", "Fl\u00fc\u00b7gel", "re\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "ADJA", "NN", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}