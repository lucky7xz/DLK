{"textgrid.poem.56069": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Sonnenuhr", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selten reicht ein Schauer feuchter F\u00e4ule", "tokens": ["Sel\u00b7ten", "reicht", "ein", "Schau\u00b7er", "feuch\u00b7ter", "F\u00e4u\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "aus dem Gartenschatten, wo einander", "tokens": ["aus", "dem", "Gar\u00b7ten\u00b7schat\u00b7ten", ",", "wo", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PRF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Tropfen fallen h\u00f6ren und ein Wander-", "tokens": ["Trop\u00b7fen", "fal\u00b7len", "h\u00f6\u00b7ren", "und", "ein", "Wan\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "VVINF", "KON", "ART", "TRUNC"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "vogel lautet, zu der S\u00e4ule,", "tokens": ["vo\u00b7gel", "lau\u00b7tet", ",", "zu", "der", "S\u00e4u\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "die in Majoran und Koriander", "tokens": ["die", "in", "Ma\u00b7jo\u00b7ran", "und", "Ko\u00b7ri\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NE", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "steht und Sommerstunden zeigt;", "tokens": ["steht", "und", "Som\u00b7mer\u00b7stun\u00b7den", "zeigt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "nur sobald die Dame (der ein Diener", "tokens": ["nur", "so\u00b7bald", "die", "Da\u00b7me", "(", "der", "ein", "Die\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$(", "ART", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "nachfolgt) in dem hellen Florentiner", "tokens": ["nach\u00b7folgt", ")", "in", "dem", "hel\u00b7len", "Flo\u00b7ren\u00b7ti\u00b7ner"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$(", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00fcber ihren Rand sich neigt,", "tokens": ["\u00fc\u00b7ber", "ih\u00b7ren", "Rand", "sich", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wird sie schattig und verschweigt \u2013.", "tokens": ["wird", "sie", "schat\u00b7tig", "und", "ver\u00b7schweigt", "\u2013", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "VVPP", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Oder wenn ein sommerlicher Regen", "tokens": ["O\u00b7der", "wenn", "ein", "som\u00b7mer\u00b7li\u00b7cher", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "aufkommt aus dem wogenden Bewegen", "tokens": ["auf\u00b7kommt", "aus", "dem", "wo\u00b7gen\u00b7den", "Be\u00b7we\u00b7gen"], "token_info": ["word", "word", "wor