{"textgrid.poem.37092": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Tod des hochw\u00fcrdigen", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bang, wie dein Gesang o Philomele,", "tokens": ["Bang", ",", "wie", "dein", "Ge\u00b7sang", "o", "Phi\u00b7lo\u00b7me\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPOSAT", "NN", "FM", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Str\u00f6me meines Liedes Trauerton!", "tokens": ["Str\u00f6\u00b7me", "mei\u00b7nes", "Lie\u00b7des", "Trau\u00b7er\u00b7ton", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00dcberw\u00f6lkt ist meine ganze Seele,", "tokens": ["\u00dc\u00b7berw\u00b7\u00f6lkt", "ist", "mei\u00b7ne", "gan\u00b7ze", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ruh und Freude sind von mir entflohn!", "tokens": ["Ruh", "und", "Freu\u00b7de", "sind", "von", "mir", "ent\u00b7flohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Nimmer werd ich meinen Lehrer schauen,", "tokens": ["Nim\u00b7mer", "werd", "ich", "mei\u00b7nen", "Leh\u00b7rer", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Bis mein Aug im Todesschlummer bricht;", "tokens": ["Bis", "mein", "Aug", "im", "To\u00b7des\u00b7schlum\u00b7mer", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Bis sein Geist, in jenen Himmelsauen,", "tokens": ["Bis", "sein", "Geist", ",", "in", "je\u00b7nen", "Him\u00b7mel\u00b7sau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Mir die Palmenkrone flicht.", "tokens": ["Mir", "die", "Pal\u00b7men\u00b7kro\u00b7ne", "flicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Weitentfernt von seinem Leichenh\u00fcgel,", "tokens": ["Wei\u00b7tent\u00b7fernt", "von", "sei\u00b7nem", "Lei\u00b7chen\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Traur ich einsam hier am Leinestrand!", "tokens": ["Traur", "ich", "ein\u00b7sam", "hier", "am", "Lei\u00b7nes\u00b7trand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wehe meine Klag', auf schnellem Fl\u00fcgel,", "tokens": ["We\u00b7he", "mei\u00b7ne", "Klag'", ",", "auf", "schnel\u00b7lem", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zephyr, in mein m\u00fctterliches Land!", "tokens": ["Ze\u00b7phyr", ",", "in", "mein", "m\u00fct\u00b7ter\u00b7li\u00b7ches", "Land", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ihn, der hohe Tugendthaten \u00fcbte,", "tokens": ["Ihn", ",", "der", "ho\u00b7he", "Tu\u00b7gendt\u00b7ha\u00b7ten", "\u00fcb\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Seinen Nebenmenschen Beyspiel gab,", "tokens": ["Sei\u00b7nen", "Ne\u00b7ben\u00b7men\u00b7schen", "Bey\u00b7spiel", "gab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ihn, den meine ganze Seele liebte,", "tokens": ["Ihn", ",", "den", "mei\u00b7ne", "gan\u00b7ze", "See\u00b7le", "lieb\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ach, verschlang das fr\u00fche Grab.", "tokens": ["Ach", ",", "ver\u00b7schlang", "das", "fr\u00fc\u00b7he", "Grab", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Reines Herzens war er, reiner Sitte,", "tokens": ["Rei\u00b7nes", "Her\u00b7zens", "war", "er", ",", "rei\u00b7ner", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00dcbte manche hohe Christenthat,", "tokens": ["\u00dcb\u00b7te", "man\u00b7che", "ho\u00b7he", "Chris\u00b7ten\u00b7that", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gab dem Armen Brodt in seine H\u00fctte,", "tokens": ["Gab", "dem", "Ar\u00b7men", "Brodt", "in", "sei\u00b7ne", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Streute Blumen auf des Pilgers Pfad.", "tokens": ["Streu\u00b7te", "Blu\u00b7men", "auf", "des", "Pil\u00b7gers", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Dem Verla\u00dfnen l\u00e4chelt', und dem Waisen", "tokens": ["Dem", "Ver\u00b7la\u00df\u00b7nen", "l\u00e4\u00b7chelt'", ",", "und", "dem", "Wai\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Stets des edeln Mannes Vaterblick,", "tokens": ["Stets", "des", "e\u00b7deln", "Man\u00b7nes", "Va\u00b7ter\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Glitt die Unschuld aus der Tugend Gleisen,", "tokens": ["Glitt", "die", "Un\u00b7schuld", "aus", "der", "Tu\u00b7gend", "Glei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "F\u00fchrt er sie darauf zur\u00fcck.", "tokens": ["F\u00fchrt", "er", "sie", "da\u00b7rauf", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PAV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Zur Beseligung wurd er geboren!", "tokens": ["Zur", "Be\u00b7se\u00b7li\u00b7gung", "wurd", "er", "ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Einen Pflegevater, einen Freund", "tokens": ["Ei\u00b7nen", "Pfle\u00b7ge\u00b7va\u00b7ter", ",", "ei\u00b7nen", "Freund"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Haben an ihm tausende verloren,", "tokens": ["Ha\u00b7ben", "an", "ihm", "tau\u00b7sen\u00b7de", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADJA", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ihr Auge tr\u00fcbet sich, und weint.", "tokens": ["Und", "ihr", "Au\u00b7ge", "tr\u00fc\u00b7bet", "sich", ",", "und", "weint", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "$,", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "O ihr Pfleges\u00f6hn', und Pfleget\u00f6chter,", "tokens": ["O", "ihr", "Pfle\u00b7ge\u00b7s\u00f6hn'", ",", "und", "Pfle\u00b7ge\u00b7t\u00f6ch\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Er verdients, da\u00df eure Z\u00e4hre rinnt!", "tokens": ["Er", "ver\u00b7dients", ",", "da\u00df", "eu\u00b7re", "Z\u00e4h\u00b7re", "rinnt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Aber keine Thr\u00e4nen sind gerechter,", "tokens": ["A\u00b7ber", "kei\u00b7ne", "Thr\u00e4\u00b7nen", "sind", "ge\u00b7rech\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Als die meinigen es sind.", "tokens": ["Als", "die", "mei\u00b7ni\u00b7gen", "es", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPOSS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach, er war der F\u00fchrer meiner Jugend,", "tokens": ["Ach", ",", "er", "war", "der", "F\u00fch\u00b7rer", "mei\u00b7ner", "Ju\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gl\u00e4nzte mir mit seinem Beyspiel vor,", "tokens": ["Gl\u00e4nz\u00b7te", "mir", "mit", "sei\u00b7nem", "Bey\u00b7spiel", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Predigte mir hohe Christentugend,", "tokens": ["Pre\u00b7dig\u00b7te", "mir", "ho\u00b7he", "Chris\u00b7ten\u00b7tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und mein Geist hub sich durch ihn empor.", "tokens": ["Und", "mein", "Geist", "hub", "sich", "durch", "ihn", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Nimmer werd' ich dieses Manns verge\u00dfen,", "tokens": ["Nim\u00b7mer", "werd'", "ich", "die\u00b7ses", "Manns", "ver\u00b7ge\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Immer Folger seiner Tugend seyn,", "tokens": ["Im\u00b7mer", "Fol\u00b7ger", "sei\u00b7ner", "Tu\u00b7gend", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Immer, unter schauernden Cypre\u00dfen", "tokens": ["Im\u00b7mer", ",", "un\u00b7ter", "schau\u00b7ern\u00b7den", "Cyp\u00b7re\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Seinem Grabe Thr\u00e4nen weyhn.", "tokens": ["Sei\u00b7nem", "Gra\u00b7be", "Thr\u00e4\u00b7nen", "weyhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wehmuthsvoll werd ich gen Himmel blicken,", "tokens": ["Weh\u00b7muths\u00b7voll", "werd", "ich", "gen", "Him\u00b7mel", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo mein Freund, im Schoo\u00dfe Gottes, wohnt,", "tokens": ["Wo", "mein", "Freund", ",", "im", "Schoo\u00b7\u00dfe", "Got\u00b7tes", ",", "wohnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "APPRART", "NN", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo ihm Wonn', und dauerndes Entz\u00fccken", "tokens": ["Wo", "ihm", "Wonn'", ",", "und", "dau\u00b7ern\u00b7des", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "NN", "$,", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seines Pilgerlebens Treue lohnt.", "tokens": ["Sei\u00b7nes", "Pil\u00b7ger\u00b7le\u00b7bens", "Treu\u00b7e", "lohnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Unter Engeln, und an Jesus Throne,", "tokens": ["Un\u00b7ter", "En\u00b7geln", ",", "und", "an", "Je\u00b7sus", "Thro\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Werd' ich ihn, in jenen Sternenh\u00f6hn,", "tokens": ["Werd'", "ich", "ihn", ",", "in", "je\u00b7nen", "Ster\u00b7nen\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Hellberkr\u00e4nzt mit einer Siegerkrone,", "tokens": ["Hell\u00b7ber\u00b7kr\u00e4nzt", "mit", "ei\u00b7ner", "Sie\u00b7ger\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Mit des Geistes Augen, sehn.", "tokens": ["Mit", "des", "Geis\u00b7tes", "Au\u00b7gen", ",", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Bang, wie dein Gesang o Philomele,", "tokens": ["Bang", ",", "wie", "dein", "Ge\u00b7sang", "o", "Phi\u00b7lo\u00b7me\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPOSAT", "NN", "FM", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Str\u00f6me meines Liedes Trauerton!", "tokens": ["Str\u00f6\u00b7me", "mei\u00b7nes", "Lie\u00b7des", "Trau\u00b7er\u00b7ton", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00dcberw\u00f6lkt ist meine ganze Seele,", "tokens": ["\u00dc\u00b7berw\u00b7\u00f6lkt", "ist", "mei\u00b7ne", "gan\u00b7ze", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ruh und Freude sind von mir entflohn!", "tokens": ["Ruh", "und", "Freu\u00b7de", "sind", "von", "mir", "ent\u00b7flohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Nimmer werd ich meinen Lehrer schauen,", "tokens": ["Nim\u00b7mer", "werd", "ich", "mei\u00b7nen", "Leh\u00b7rer", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Bis mein Aug im Todesschlummer bricht;", "tokens": ["Bis", "mein", "Aug", "im", "To\u00b7des\u00b7schlum\u00b7mer", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Bis sein Geist, in jenen Himmelsauen,", "tokens": ["Bis", "sein", "Geist", ",", "in", "je\u00b7nen", "Him\u00b7mel\u00b7sau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Mir die Palmenkrone flicht.", "tokens": ["Mir", "die", "Pal\u00b7men\u00b7kro\u00b7ne", "flicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Weitentfernt von seinem Leichenh\u00fcgel,", "tokens": ["Wei\u00b7tent\u00b7fernt", "von", "sei\u00b7nem", "Lei\u00b7chen\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Traur ich einsam hier am Leinestrand!", "tokens": ["Traur", "ich", "ein\u00b7sam", "hier", "am", "Lei\u00b7nes\u00b7trand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.pent