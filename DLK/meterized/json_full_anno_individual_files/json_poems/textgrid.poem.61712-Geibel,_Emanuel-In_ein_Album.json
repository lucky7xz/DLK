{"textgrid.poem.61712": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "In ein Album", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Buch des Lebens liest sich nur ein einzig Mal;", "tokens": ["Das", "Buch", "des", "Le\u00b7bens", "liest", "sich", "nur", "ein", "ein\u00b7zig", "Mal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PRF", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du kannst darin nicht bl\u00e4ttern, wie's dir wohlgef\u00e4llt,", "tokens": ["Du", "kannst", "da\u00b7rin", "nicht", "bl\u00e4t\u00b7tern", ",", "wie's", "dir", "wohl\u00b7ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "PTKNEG", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Noch bei der Stelle weilen, die dich fesselte;", "tokens": ["Noch", "bei", "der", "Stel\u00b7le", "wei\u00b7len", ",", "die", "dich", "fes\u00b7sel\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.4": {"text": "Denn unerbittlich wenden sich die Bl\u00e4tter um.", "tokens": ["Denn", "un\u00b7er\u00b7bitt\u00b7lich", "wen\u00b7den", "sich", "die", "Bl\u00e4t\u00b7ter", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zum Abschnitt \u00bbLieben\u00ab kehrten wir zur\u00fcck, wie gern!", "tokens": ["Zum", "Ab\u00b7schnitt", "\u00bb", "Lie\u00b7ben", "\u00ab", "kehr\u00b7ten", "wir", "zu\u00b7r\u00fcck", ",", "wie", "gern", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "ADJA", "$(", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sind schon auf der Seite, wo es Sterben hei\u00dft.", "tokens": ["Und", "sind", "schon", "auf", "der", "Sei\u00b7te", ",", "wo", "es", "Ster\u00b7ben", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Buch des Lebens liest sich nur ein einzig Mal;", "tokens": ["Das", "Buch", "des", "Le\u00b7bens", "liest", "sich", "nur", "ein", "ein\u00b7zig", "Mal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PRF", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du kannst darin nicht bl\u00e4ttern, wie's dir wohlgef\u00e4llt,", "tokens": ["Du", "kannst", "da\u00b7rin", "nicht", "bl\u00e4t\u00b7tern", ",", "wie's", "dir", "wohl\u00b7ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "PTKNEG", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Noch bei der Stelle weilen, die dich fesselte;", 