{"dta.poem.19451": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die todten Freunde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Das Boot st\u00f6\u00dft ab von den Leuchten des Gestads.", "tokens": ["Das", "Boot", "st\u00f6\u00dft", "ab", "von", "den", "Leuch\u00b7ten", "des", "Ge\u00b7stads", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durch f\u00f6hnige Wellen dreht sich der Schwung des Rads.", "tokens": ["Durch", "f\u00f6h\u00b7ni\u00b7ge", "Wel\u00b7len", "dreht", "sich", "der", "Schwung", "des", "Rads", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Schwarz qualmt des Rohres Rauch ... Heut hab' ich schlecht,", "tokens": ["Schwarz", "qualmt", "des", "Roh\u00b7res", "Rauch", "...", "Heut", "hab'", "ich", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$(", "ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das hei\u00dft mit lauter jungem Volk gezecht \u2014", "tokens": ["Das", "hei\u00dft", "mit", "lau\u00b7ter", "jun\u00b7gem", "Volk", "ge\u00b7zecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du, der gest\u00fcrzt ist mit zerschossener Stirn,", "tokens": ["Du", ",", "der", "ge\u00b7st\u00fcrzt", "ist", "mit", "zer\u00b7schos\u00b7se\u00b7ner", "Stirn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "VVPP", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und du, verschwunden auf einer Gletscherfirn,", "tokens": ["Und", "du", ",", "ver\u00b7schwun\u00b7den", "auf", "ei\u00b7ner", "Glet\u00b7scher\u00b7firn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und du, verlodert wie schw\u00fcler Blitzesschein,", "tokens": ["Und", "du", ",", "ver\u00b7lo\u00b7dert", "wie", "schw\u00fc\u00b7ler", "Blit\u00b7zes\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Meine todten Freunde, saget, gedenkt ihr mein?", "tokens": ["Mei\u00b7ne", "tod\u00b7ten", "Freun\u00b7de", ",", "sa\u00b7get", ",", "ge\u00b7denkt", "ihr", "mein", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Wogen zischen um Boot und R\u00e4derschlag,", "tokens": ["Wo\u00b7gen", "zi\u00b7schen", "um", "Boot", "und", "R\u00e4\u00b7der\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dazwischen jubelt ein dumpfes Zechgelag,", "tokens": ["Da\u00b7zwi\u00b7schen", "ju\u00b7belt", "ein", "dum\u00b7pfes", "Zech\u00b7ge\u00b7lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In den Fluthen braust ein sturmged\u00e4mpfter Chor,", "tokens": ["In", "den", "Flut\u00b7hen", "braust", "ein", "sturm\u00b7ge\u00b7d\u00e4mpf\u00b7ter", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Becher l\u00e4uten aus tiefer Nacht empor.", "tokens": ["Be\u00b7cher", "l\u00e4u\u00b7ten", "aus", "tie\u00b7fer", "Nacht", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}}}}