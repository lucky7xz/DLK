{"textgrid.poem.38223": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Lebewohl", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Morgen mu\u00df ich weg von hier,", "tokens": ["Mor\u00b7gen", "mu\u00df", "ich", "weg", "von", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "APPR", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mu\u00df Abschied nehmen;", "tokens": ["Und", "mu\u00df", "Ab\u00b7schied", "neh\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "O du allerh\u00f6chste Zier,", "tokens": ["O", "du", "al\u00b7ler\u00b7h\u00f6chs\u00b7te", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Scheiden das bringt Gr\u00e4men.", "tokens": ["Schei\u00b7den", "das", "bringt", "Gr\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "VVFIN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Da ich dich so treu geliebt,", "tokens": ["Da", "ich", "dich", "so", "treu", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ueber alle Maa\u00dfen,", "tokens": ["Ue\u00b7ber", "al\u00b7le", "Maa\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Soll