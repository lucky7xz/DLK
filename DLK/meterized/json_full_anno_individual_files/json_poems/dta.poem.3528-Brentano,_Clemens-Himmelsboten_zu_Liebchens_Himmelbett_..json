{"dta.poem.3528": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Himmelsboten zu Liebchens Himmelbett .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mondschein, der ist schon verblichen,               ", "tokens": ["Der", "Mond\u00b7schein", ",", "der", "ist", "schon", "ver\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die finstre Nacht ist hingeschlichen;", "tokens": ["Die", "finst\u00b7re", "Nacht", "ist", "hin\u00b7ge\u00b7schli\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Steh auf du edle Morgenr\u00f6th',", "tokens": ["Steh", "auf", "du", "ed\u00b7le", "Mor\u00b7gen\u00b7r\u00f6\u00b7th'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zu dir all mein Vertrauen steht.", "tokens": ["Zu", "dir", "all", "mein", "Ver\u00b7trau\u00b7en", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ph\u00f6bus ihr Vorbott wohlgeziert,", "tokens": ["Ph\u00f6\u00b7bus", "ihr", "Vor\u00b7bott", "wohl\u00b7ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Hat schon den Wagen angeschirrt;", "tokens": ["Hat", "schon", "den", "Wa\u00b7gen", "an\u00b7ge\u00b7schirrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sonnenro\u00df sind vorgespannt,", "tokens": ["Die", "Son\u00b7nen\u00b7ro\u00df", "sind", "vor\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Z\u00fcgel ruht in seiner Hand.", "tokens": ["Z\u00fc\u00b7gel", "ruht", "in", "sei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr Vorbott der Don Lucifer,", "tokens": ["Ihr", "Vor\u00b7bott", "der", "Don", "Lu\u00b7ci\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schwebt allbereits am Himmel her,", "tokens": ["Schwebt", "all\u00b7be\u00b7reits", "am", "Him\u00b7mel", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hat die Wolken aufgeschlossen,", "tokens": ["Er", "hat", "die", "Wol\u00b7ken", "auf\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Erd mit seinem Thau begossen.", "tokens": ["Die", "Erd", "mit", "sei\u00b7nem", "Thau", "be\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O fahrt vor ihr Schlafk\u00e4mmerlein,", "tokens": ["O", "fahrt", "vor", "ihr", "Schlaf\u00b7k\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Weckt leis die s\u00fc\u00dfe Liebste mein;", "tokens": ["Weckt", "leis", "die", "s\u00fc\u00b7\u00dfe", "Liebs\u00b7te", "mein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verk\u00fcndet ihr, was ich euch sag,", "tokens": ["Ver\u00b7k\u00fcn\u00b7det", "ihr", ",", "was", "ich", "euch", "sag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Dienst, mein Gru\u00df, ein guten Tag.", "tokens": ["Mein", "Dienst", ",", "mein", "Gru\u00df", ",", "ein", "gu\u00b7ten", "Tag", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch m\u00fc\u00dft ihr sie fein z\u00fcchtig wecken,", "tokens": ["Doch", "m\u00fc\u00dft", "ihr", "sie", "fein", "z\u00fcch\u00b7tig", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dabei mein heimliche Lieb entdecken;", "tokens": ["Da\u00b7bei", "mein", "heim\u00b7li\u00b7che", "Lieb", "ent\u00b7de\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sollt sagen, wie ihr Diener wacht,", "tokens": ["Sollt", "sa\u00b7gen", ",", "wie", "ihr", "Die\u00b7ner", "wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So kummervoll die ganze Nacht.", "tokens": ["So", "kum\u00b7mer\u00b7voll", "die", "gan\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Schaut an f\u00fcr mich die gelbe Haar,", "tokens": ["Schaut", "an", "f\u00fcr", "mich", "die", "gel\u00b7be", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr H\u00e4lslein blank, ihr Aeuglein klar;", "tokens": ["Ihr", "H\u00e4ls\u00b7lein", "blank", ",", "ihr", "A\u00b7e\u00b7u\u00b7glein", "klar", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "K\u00fc\u00dft ihr f\u00fcr mich den rothen Mund,", "tokens": ["K\u00fc\u00dft", "ihr", "f\u00fcr", "mich", "den", "ro\u00b7then", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn sie's leid't die Br\u00fcstlein rund.", "tokens": ["Und", "wenn", "sie's", "leid't", "die", "Br\u00fcst\u00b7lein", "rund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}