{"dta.poem.9761": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines gehangenen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die parcen haben mir das leben nicht genommen/", "tokens": ["Die", "par\u00b7cen", "ha\u00b7ben", "mir", "das", "le\u00b7ben", "nicht", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PDS", "VVFIN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sonsten spinnerin der lebens-faden sind:", "tokens": ["Die", "sons\u00b7ten", "spin\u00b7ne\u00b7rin", "der", "le\u00b7bens\u00b7fa\u00b7den", "sind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich bin durch einen mensch und strang um selbes kommen;", "tokens": ["Ich", "bin", "durch", "ei\u00b7nen", "mensch", "und", "strang", "um", "sel\u00b7bes", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "KON", "VVFIN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach da\u00df bey menschen man so grosse strenge findt!", "tokens": ["Ach", "da\u00df", "bey", "men\u00b7schen", "man", "so", "gros\u00b7se", "stren\u00b7ge", "findt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "APPR", "ADJA", "PIS", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}