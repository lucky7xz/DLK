{"textgrid.poem.65193": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Welle gleich, die fern vom Meeresstrande", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Welle gleich, die fern vom Meeresstrande", "tokens": ["Der", "Wel\u00b7le", "gleich", ",", "die", "fern", "vom", "Mee\u00b7res\u00b7stran\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Spurlos im weiten Ozean verschwimmt;", "tokens": ["Spur\u00b7los", "im", "wei\u00b7ten", "O\u00b7ze\u00b7an", "ver\u00b7schwimmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Der Flamme \u00e4hnlich, die nach kurzem Brande", "tokens": ["Der", "Flam\u00b7me", "\u00e4hn\u00b7lich", ",", "die", "nach", "kur\u00b7zem", "Bran\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu einem toten Aschenrest verglimmt;", "tokens": ["Zu", "ei\u00b7nem", "to\u00b7ten", "A\u00b7schen\u00b7rest", "ver\u00b7glimmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Schatten nur in t\u00e4uschendem Gewande,", "tokens": ["Ein", "Schat\u00b7ten", "nur", "in", "t\u00e4u\u00b7schen\u00b7dem", "Ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der, kaum erschienen, auch schon Abschied nimmt;", "tokens": ["Der", ",", "kaum", "er\u00b7schie\u00b7nen", ",", "auch", "schon", "Ab\u00b7schied", "nimmt", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "VVINF", "$,", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dem Walten blinder Kr\u00e4fte unterth\u00e4nig, \u2013", "tokens": ["Dem", "Wal\u00b7ten", "blin\u00b7der", "Kr\u00e4f\u00b7te", "un\u00b7ter\u00b7th\u00e4\u00b7nig", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADJD", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Menschenleben, ach, es ist so wenig!", "tokens": ["Ein", "Men\u00b7schen\u00b7le\u00b7ben", ",", "ach", ",", "es", "ist", "so", "we\u00b7nig", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ITJ", "$,", "PPER", "VAFIN", "ADV", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Allein in dieser armen Spanne Zeit,", "tokens": ["Al\u00b7lein", "in", "die\u00b7ser", "ar\u00b7men", "Span\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die uns, den Bildern eines Traums, gelassen,", "tokens": ["Die", "uns", ",", "den", "Bil\u00b7dern", "ei\u00b7nes", "Traums", ",", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "ART", "NN", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Welch ein Gedr\u00e4ng' von Schmerz und Seligkeit!", "tokens": ["Welch", "ein", "Ge\u00b7dr\u00e4ng'", "von", "Schmerz", "und", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Welch ein Gewog' von Lieben und von Hassen!", "tokens": ["Welch", "ein", "Ge\u00b7wo\u00b7g'", "von", "Lie\u00b7ben", "und", "von", "Has\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "APPR", "ADJA", "KON", "APPR", "NE", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ob nichtig auch dies Sein, das Herz ist weit", "tokens": ["Ob", "nich\u00b7tig", "auch", "dies", "Sein", ",", "das", "Herz", "ist", "weit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "PDS", "PPOSAT", "$,", "ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und k\u00fchn genug Unendliches zu fassen", "tokens": ["Und", "k\u00fchn", "ge\u00b7nug", "Un\u00b7end\u00b7li\u00b7ches", "zu", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "ADJA", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Im Los, das ihm f\u00fcr fl\u00fccht'ge Stunden fiel \u2013", "tokens": ["Im", "Los", ",", "das", "ihm", "f\u00fcr", "fl\u00fccht'\u00b7ge", "Stun\u00b7den", "fiel", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"te