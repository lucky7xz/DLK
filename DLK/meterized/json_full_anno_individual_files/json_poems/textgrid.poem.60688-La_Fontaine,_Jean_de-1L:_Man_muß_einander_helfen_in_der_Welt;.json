{"textgrid.poem.60688": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Man mu\u00df einander helfen in der Welt;", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man mu\u00df einander helfen in der Welt;", "tokens": ["Man", "mu\u00df", "ein\u00b7an\u00b7der", "hel\u00b7fen", "in", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn, stirbt dein Nachbar, kann es leicht geschehen,", "tokens": ["Denn", ",", "stirbt", "dein", "Nach\u00b7bar", ",", "kann", "es", "leicht", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df seine Last auf deinen R\u00fccken f\u00e4llt.", "tokens": ["Da\u00df", "sei\u00b7ne", "Last", "auf", "dei\u00b7nen", "R\u00fc\u00b7cken", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Esel mu\u00dfte einst mit einem Pferde gehen,", "tokens": ["Ein", "E\u00b7sel", "mu\u00df\u00b7te", "einst", "mit", "ei\u00b7nem", "Pfer\u00b7de", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das nichts als nur sein leichtes Zaumzeug trug;", "tokens": ["Das", "nichts", "als", "nur", "sein", "leich\u00b7tes", "Zaum\u00b7zeug", "trug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "KOKOM", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Esel aber war bepackt genug.", "tokens": ["Der", "E\u00b7sel", "a\u00b7ber", "war", "be\u00b7packt", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wandte er sich an das Pferd mit Flehen:", "tokens": ["So", "wand\u00b7te", "er", "sich", "an", "das", "Pferd", "mit", "Fle\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbhilf mir ein wenig, oder ich mu\u00df sterben!", "tokens": ["\u00bb", "hilf", "mir", "ein", "we\u00b7nig", ",", "o\u00b7der", "ich", "mu\u00df", "ster\u00b7ben", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "ART", "PIS", "$,", "KON", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was ich erbitte, ist f\u00fcr dich nicht viel,", "tokens": ["Was", "ich", "er\u00b7bit\u00b7te", ",", "ist", "f\u00fcr", "dich", "nicht", "viel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "APPR", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die H\u00e4lfte meiner Last ist dir nur Spiel,", "tokens": ["Die", "H\u00e4lf\u00b7te", "mei\u00b7ner", "Last", "ist", "dir", "nur", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und gro\u00dfen Dank wirst du erwerben.\u00ab", "tokens": ["Und", "gro\u00b7\u00dfen", "Dank", "wirst", "du", "er\u00b7wer\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Das Pferd schlug's ab und wollte nichts mehr h\u00f6ren.", "tokens": ["Das", "Pferd", "schlug's", "ab", "und", "woll\u00b7te", "nichts", "mehr", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Doch als der Esel fiel, was half ihm da sein Wehren?", "tokens": ["Doch", "als", "der", "E\u00b7sel", "fiel", ",", "was", "half", "ihm", "da", "sein", "Weh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es mu\u00dfte mit des Esels voller Last sich plagen", "tokens": ["Es", "mu\u00df\u00b7te", "mit", "des", "E\u00b7sels", "vol\u00b7ler", "Last", "sich", "pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "ADJA", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}