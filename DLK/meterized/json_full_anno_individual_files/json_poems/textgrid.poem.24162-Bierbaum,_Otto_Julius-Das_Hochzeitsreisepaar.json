{"textgrid.poem.24162": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Das Hochzeitsreisepaar", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Eheringe gl\u00e4nzen", "tokens": ["Die", "E\u00b7he\u00b7rin\u00b7ge", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit feuerigem Schein,", "tokens": ["Mit", "feu\u00b7e\u00b7ri\u00b7gem", "Schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist kein einziger Kritzer dran;", "tokens": ["Es", "ist", "kein", "ein\u00b7zi\u00b7ger", "Krit\u00b7zer", "dran", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie sagt: \u00bbMein Herz! Mein Schatz!\u00ab \u2013 Man kann", "tokens": ["Sie", "sagt", ":", "\u00bb", "Mein", "Herz", "!", "Mein", "Schatz", "!", "\u00ab", "\u2013", "Man", "kann"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "$.", "$(", "$(", "PIS", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gar nicht verliebter sein.", "tokens": ["Gar", "nicht", "ver\u00b7lieb\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auf seine Schulter legt sie", "tokens": ["Auf", "sei\u00b7ne", "Schul\u00b7ter", "legt", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Kopf mit Lindigkeit;", "tokens": ["Den", "Kopf", "mit", "Lin\u00b7dig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach k\u00f6nnte man k\u00fcssen im Coup\u00e9!", "tokens": ["Ach", "k\u00f6nn\u00b7te", "man", "k\u00fcs\u00b7sen", "im", "Coup\u00e9", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VMFIN", "PIS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Indem ich aus dem Fenster seh", "tokens": ["In\u00b7dem", "ich", "aus", "dem", "Fens\u00b7ter", "seh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schaff ich Gelegenheit.", "tokens": ["Schaff", "ich", "Ge\u00b7le\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Sie essen und sie trinken;", "tokens": ["Sie", "es\u00b7sen", "und", "sie", "trin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie f\u00fcttert sie ihn zart!", "tokens": ["Wie", "f\u00fct\u00b7tert", "sie", "ihn", "zart", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer Augen hat zu sehn, der sieht:", "tokens": ["Wer", "Au\u00b7gen", "hat", "zu", "sehn", ",", "der", "sieht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "PTKZU", "VVINF", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Liebe st\u00e4rkt den Appetit.", "tokens": ["Die", "Lie\u00b7be", "st\u00e4rkt", "den", "Ap\u00b7pe\u00b7tit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann wischt sie ihm den Bart.", "tokens": ["Dann", "wischt", "sie", "ihm", "den", "Bart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zu sagen hat sie ihm sehr viel,", "tokens": ["Zu", "sa\u00b7gen", "hat", "sie", "ihm", "sehr", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "PPER", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Thut h\u00f6chst geheimnisvoll.", "tokens": ["Thut", "h\u00f6chst", "ge\u00b7heim\u00b7nis\u00b7voll", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie tuschelts leise ihm ins Ohr", "tokens": ["Sie", "tu\u00b7schelts", "lei\u00b7se", "ihm", "ins", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00e4lt auch noch die Hand davor,", "tokens": ["Und", "h\u00e4lt", "auch", "noch", "die", "Hand", "da\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weils niemand h\u00f6ren soll.", "tokens": ["Weils", "nie\u00b7mand", "h\u00f6\u00b7ren", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es mu\u00df nicht von fataler Art,", "tokens": ["Es", "mu\u00df", "nicht", "von", "fa\u00b7ta\u00b7ler", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was sie ihm k\u00fcndet, sein;", "tokens": ["Was", "sie", "ihm", "k\u00fcn\u00b7det", ",", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$,", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Gegenteil, mir scheint, es thut", "tokens": ["Im", "Ge\u00b7gen\u00b7teil", ",", "mir", "scheint", ",", "es", "thut"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Braven wunders\u00fc\u00dfe gut", "tokens": ["Dem", "Bra\u00b7ven", "wun\u00b7der\u00b7s\u00fc\u00b7\u00dfe", "gut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und geht ihm lieblich ein.", "tokens": ["Und", "geht", "ihm", "lieb\u00b7lich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie Butter in der Sonne gl\u00e4nzt,", "tokens": ["Wie", "But\u00b7ter", "in", "der", "Son\u00b7ne", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So gl\u00e4nzt sein Angesicht:", "tokens": ["So", "gl\u00e4nzt", "sein", "An\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Zweifel, er ist sehr begl\u00fcckt.", "tokens": ["Kein", "Zwei\u00b7fel", ",", "er", "ist", "sehr", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Gott, wie er sie an sich dr\u00fcckt!", "tokens": ["Mein", "Gott", ",", "wie", "er", "sie", "an", "sich", "dr\u00fcckt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "PPER", "PPER", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Unmensch, zerbrich sie nicht!", "tokens": ["Un\u00b7mensch", ",", "zer\u00b7brich", "sie", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Im ganzen mu\u00df ich sagen: mir", "tokens": ["Im", "gan\u00b7zen", "mu\u00df", "ich", "sa\u00b7gen", ":", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "ADJA", "VMFIN", "PPER", "VVINF", "$.", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Scheints etwas deplaziert,", "tokens": ["Scheints", "et\u00b7was", "de\u00b7pla\u00b7ziert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da\u00df man mir einfach vis-\u00e0-vis", "tokens": ["Da\u00df", "man", "mir", "ein\u00b7fach", "vis\u00b7\u00e0\u00b7vis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "ADV", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ungemeine Sympathie", "tokens": ["So", "un\u00b7ge\u00b7mei\u00b7ne", "Sym\u00b7pa\u00b7thie"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ganz offen produziert.", "tokens": ["Ganz", "of\u00b7fen", "pro\u00b7du\u00b7ziert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Mir scheint, es w\u00e4re angebracht,", "tokens": ["Mir", "scheint", ",", "es", "w\u00e4\u00b7re", "an\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcrs stille K\u00e4mmerlein", "tokens": ["F\u00fcrs", "stil\u00b7le", "K\u00e4m\u00b7mer\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu sparen diese Z\u00e4rtlichkeit.", "tokens": ["Zu", "spa\u00b7ren", "die\u00b7se", "Z\u00e4rt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie gehn entschieden etwas weit!", "tokens": ["Sie", "gehn", "ent\u00b7schie\u00b7den", "et\u00b7was", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch will ich nicht so sein:", "tokens": ["Doch", "will", "ich", "nicht", "so", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Stunden wehn, die Tage gehn,", "tokens": ["Die", "Stun\u00b7den", "wehn", ",", "die", "Ta\u00b7ge", "gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der kritzerlose Ring,", "tokens": ["Der", "krit\u00b7zer\u00b7lo\u00b7se", "Ring", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie bald wird er zerschunden sein!", "tokens": ["Wie", "bald", "wird", "er", "zer\u00b7schun\u00b7den", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Und viel vergeht mit seinem Schein,", "tokens": ["Und", "viel", "ver\u00b7geht", "mit", "sei\u00b7nem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du sehr verliebtes Ding!", "tokens": ["Du", "sehr", "ver\u00b7lieb\u00b7tes", "Ding", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Denn eine Hochzeitsreise ist", "tokens": ["Denn", "ei\u00b7ne", "Hoch\u00b7zeits\u00b7rei\u00b7se", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ehe wirklich nicht.", "tokens": ["Die", "E\u00b7he", "wirk\u00b7lich", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wird der Anschlu\u00df oft verpa\u00dft,", "tokens": ["Da", "wird", "der", "An\u00b7schlu\u00df", "oft", "ver\u00b7pa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und manche \u00fcberschwere Last", "tokens": ["Und", "man\u00b7che", "\u00fc\u00b7bersc\u00b7hwe\u00b7re", "Last"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Macht, da\u00df die Achse bricht.", "tokens": ["Macht", ",", "da\u00df", "die", "A\u00b7chse", "bricht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Drum, junge Frau und junger Mann,", "tokens": ["Drum", ",", "jun\u00b7ge", "Frau", "und", "jun\u00b7ger", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dr\u00fcckt, k\u00fc\u00dft euch ohne Zwang!", "tokens": ["Dr\u00fcckt", ",", "k\u00fc\u00dft", "euch", "oh\u00b7ne", "Zwang", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gehs euch so gut, wie mir es geht,", "tokens": ["Gehs", "euch", "so", "gut", ",", "wie", "mir", "es", "geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis da\u00df der Wagen stille steht,", "tokens": ["Bis", "da\u00df", "der", "Wa\u00b7gen", "stil\u00b7le", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die ganze Ehe lang.", "tokens": ["Die", "gan\u00b7ze", "E\u00b7he", "lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die Eheringe gl\u00e4nzen", "tokens": ["Die", "E\u00b7he\u00b7rin\u00b7ge", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit feuerigem Schein,", "tokens": ["Mit", "feu\u00b7e\u00b7ri\u00b7gem", "Schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist kein einziger Kritzer dran;", "tokens": ["Es", "ist", "kein", "ein\u00b7zi\u00b7ger", "Krit\u00b7zer", "dran", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie sagt: \u00bbMein Herz! Mein Schatz!\u00ab \u2013 Man kann", "tokens": ["Sie", "sagt", ":", "\u00bb", "Mein", "Herz", "!", "Mein", "Schatz", "!", "\u00ab", "\u2013", "Man", "kann"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "$.", "$(", "$(", "PIS", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gar nicht verliebter sein.", "tokens": ["Gar", "nicht", "ver\u00b7lieb\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Auf seine Schulter legt sie", "tokens": ["Auf", "sei\u00b7ne", "Schul\u00b7ter", "legt", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Kopf mit Lindigkeit;", "tokens": ["Den", "Kopf", "mit", "Lin\u00b7dig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach k\u00f6nnte man k\u00fcssen im Coup\u00e9!", "tokens": ["Ach", "k\u00f6nn\u00b7te", "man", "k\u00fcs\u00b7sen", "im", "Coup\u00e9", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VMFIN", "PIS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Indem ich aus dem Fenster seh", "tokens": ["In\u00b7dem", "ich", "aus", "dem", "Fens\u00b7ter", "seh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schaff ich Gelegenheit.", "tokens": ["Schaff", "ich", "Ge\u00b7le\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.14": {"line.1": {"text": "Sie essen und sie trinken;", "tokens": ["Sie", "es\u00b7sen", "und", "sie", "trin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie f\u00fcttert sie ihn zart!", "tokens": ["Wie", "f\u00fct\u00b7tert", "sie", "ihn", "zart", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer Augen hat zu sehn, der sieht:", "tokens": ["Wer", "Au\u00b7gen", "hat", "zu", "sehn", ",", "der", "sieht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "PTKZU", "VVINF", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Liebe st\u00e4rkt den Appetit.", "tokens": ["Die", "Lie\u00b7be", "st\u00e4rkt", "den", "Ap\u00b7pe\u00b7tit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann wischt sie ihm den Bart.", "tokens": ["Dann", "wischt", "sie", "ihm", "den", "Bart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Zu sagen hat sie ihm sehr viel,", "tokens": ["Zu", "sa\u00b7gen", "hat", "sie", "ihm", "sehr", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "PPER", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Thut h\u00f6chst geheimnisvoll.", "tokens": ["Thut", "h\u00f6chst", "ge\u00b7heim\