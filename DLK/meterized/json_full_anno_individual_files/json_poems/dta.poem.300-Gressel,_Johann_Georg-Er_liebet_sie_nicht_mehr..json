{"dta.poem.300": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er liebet sie nicht mehr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Hertz bezwinge dich", "tokens": ["Mein", "Hertz", "be\u00b7zwin\u00b7ge", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Liebe zu vertreiben/", "tokens": ["Die", "Lie\u00b7be", "zu", "ver\u00b7trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So dir ", "tokens": ["So", "dir"], "token_info": ["word", "word"], "pos": ["ADV", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Als ihre Artigkeit mich hat entz\u00fcndt gemacht/", "tokens": ["Als", "ih\u00b7re", "Ar\u00b7tig\u00b7keit", "mich", "hat", "ent\u00b7z\u00fcndt", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und la\u00df mich frey von ihrem Brande bleiben.", "tokens": ["Und", "la\u00df", "mich", "frey", "von", "ih\u00b7rem", "Bran\u00b7de", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mein Hertz bezwinge dich", "tokens": ["Mein", "Hertz", "be\u00b7zwin\u00b7ge", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Liebe zu vertreiben.", "tokens": ["Die", "Lie\u00b7be", "zu", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dein Feuer brennt umsonst", "tokens": ["Dein", "Feu\u00b7er", "brennt", "um\u00b7sonst"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Verl\u00f6sche deine Flammen/", "tokens": ["Ver\u00b7l\u00f6\u00b7sche", "dei\u00b7ne", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und la\u00df die Liebes-Gluth in dir verborgen seyn/", "tokens": ["Und", "la\u00df", "die", "Lie\u00b7bes\u00b7Gluth", "in", "dir", "ver\u00b7bor\u00b7gen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "APPR", "PPER", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bis da\u00df sie mit der Zeit geht von sich selber ein/", "tokens": ["Bis", "da\u00df", "sie", "mit", "der", "Zeit", "geht", "von", "sich", "sel\u00b7ber", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "APPR", "PRF", "ADV", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn du sie wirst als ungereimt verdammen.", "tokens": ["Wenn", "du", "sie", "wirst", "als", "un\u00b7ge\u00b7reimt", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "KOKOM", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dein Feuer brennt umsonst/", "tokens": ["Dein", "Feu\u00b7er", "brennt", "um\u00b7sonst", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Verl\u00f6sche deine Flammen.", "tokens": ["Ver\u00b7l\u00f6\u00b7sche", "dei\u00b7ne", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich will/ und kan dir nicht", "tokens": ["Ich", "will", "/", "und", "kan", "dir", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$(", "KON", "VMFIN", "PPER", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zu deinem Zwecke dienen/", "tokens": ["Zu", "dei\u00b7nem", "Zwe\u00b7cke", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich zieh den Freund anjetzt/ der Lust im Lieben f\u00fcr/", "tokens": ["Ich", "zieh", "den", "Freund", "an\u00b7jetzt", "/", "der", "Lust", "im", "Lie\u00b7ben", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$(", "ART", "NN", "APPRART", "ADJA", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil er mit ihr verm\u00e4hlt/ so weis ich/ da\u00df allhier", "tokens": ["Weil", "er", "mit", "ihr", "ver\u00b7m\u00e4hlt", "/", "so", "weis", "ich", "/", "da\u00df", "all\u00b7hier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "VVPP", "$(", "ADV", "PTKVZ", "PPER", "$(", "KOUS", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kein Oehl-Blatt wird vor meine Liebe gr\u00fcnen.", "tokens": ["Kein", "O\u00b7ehl\u00b7Blatt", "wird", "vor", "mei\u00b7ne", "Lie\u00b7be", "gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Ich will/ und kan dir nicht", "tokens": ["Ich", "will", "/", "und", "kan", "dir", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$(", "KON", "VMFIN", "PPER", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zu deinem Zwecke dienen.", "tokens": ["Zu", "dei\u00b7nem", "Zwe\u00b7cke", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Drum auf mein Hertz und Geist", "tokens": ["Drum", "auf", "mein", "Hertz", "und", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Last die Gedancken fahren/", "tokens": ["Last", "die", "Ge\u00b7dan\u00b7cken", "fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Als solte ihre Brust noch zu bewegen seyn/", "tokens": ["Als", "sol\u00b7te", "ih\u00b7re", "Brust", "noch", "zu", "be\u00b7we\u00b7gen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schlagt alles in den Wind/ und reist die Hoffnung ein/", "tokens": ["Schlagt", "al\u00b7les", "in", "den", "Wind", "/", "und", "reist", "die", "Hoff\u00b7nung", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$(", "KON", "VVFIN", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie mu\u00df sich nun mit ihrem Eh-Schatz paaren.", "tokens": ["Sie", "mu\u00df", "sich", "nun", "mit", "ih\u00b7rem", "Eh\u00b7Schatz", "paa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Drum auf mein Hertz und Geist", "tokens": ["Drum", "auf", "mein", "Hertz", "und", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "La\u00dft die Gedancken fahren.", "tokens": ["La\u00dft", "die", "Ge\u00b7dan\u00b7cken", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}