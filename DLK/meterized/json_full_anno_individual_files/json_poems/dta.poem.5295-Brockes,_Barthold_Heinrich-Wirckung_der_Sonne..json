{"dta.poem.5295": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wirckung der Sonne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was entsteht nicht durch die Sonne", "tokens": ["Was", "ent\u00b7steht", "nicht", "durch", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uberall f\u00fcr Nutz und Wonne!", "tokens": ["U\u00b7be\u00b7rall", "f\u00fcr", "Nutz", "und", "Won\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese Licht- und Lebens-Quelle", "tokens": ["Die\u00b7se", "Licht", "und", "Le\u00b7bens\u00b7Quel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "TRUNC", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Machet nicht nur jede Stelle", "tokens": ["Ma\u00b7chet", "nicht", "nur", "je\u00b7de", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In der Luft, und auf der Erden,", "tokens": ["In", "der", "Luft", ",", "und", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch so gar in kalter Fluth,", "tokens": ["Auch", "so", "gar", "in", "kal\u00b7ter", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lieblich, lustig, hell und licht;", "tokens": ["Lieb\u00b7lich", ",", "lus\u00b7tig", ",", "hell", "und", "licht", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Es wird, von der reinen Glut,", "tokens": ["Es", "wird", ",", "von", "der", "rei\u00b7nen", "Glut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Durch das sinnliche Gesicht,", "tokens": ["Durch", "das", "sinn\u00b7li\u00b7che", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Selbst in meiner Seelen helle.", "tokens": ["Selbst", "in", "mei\u00b7ner", "See\u00b7len", "hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Sie bestrebt sich, trotz der Erden,", "tokens": ["Sie", "be\u00b7strebt", "sich", ",", "trotz", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Fruchtbar und geschm\u00fcckt zu erden.", "tokens": ["Frucht\u00b7bar", "und", "ge\u00b7schm\u00fcckt", "zu", "er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Es entsteht in meiner Brust", "tokens": ["Es", "ent\u00b7steht", "in", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Gleichsam eine Fr\u00fchlings-Lust,", "tokens": ["Gleich\u00b7sam", "ei\u00b7ne", "Fr\u00fch\u00b7lings\u00b7Lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Eine rege Heiterkeit,", "tokens": ["Ei\u00b7ne", "re\u00b7ge", "Hei\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Eine geistge Lentzen-Zeit.", "tokens": ["Ei\u00b7ne", "geist\u00b7ge", "Lent\u00b7zen\u00b7Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Dadurch, da\u00df sie die\u00df erblickt", "tokens": ["Da\u00b7durch", ",", "da\u00df", "sie", "die\u00df", "er\u00b7blickt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "$,", "KOUS", "PPER", "PDS", "VVPP"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Wird die Seele selbst geschm\u00fcckt.", "tokens": ["Wird", "die", "See\u00b7le", "selbst", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Hofnung ist ihr sch\u00f6nes Gr\u00fcn", "tokens": ["Hof\u00b7nung", "ist", "ihr", "sch\u00f6\u00b7nes", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Und es sind die Lust und Freude", "tokens": ["Und", "es", "sind", "die", "Lust", "und", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Ob dem sch\u00f6nen welt-Geb\u00e4ude", "tokens": ["Ob", "dem", "sch\u00f6\u00b7nen", "welt\u00b7Ge\u00b7b\u00e4u\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Blumen, welche in ihr bl\u00fchn.", "tokens": ["Blu\u00b7men", ",", "wel\u00b7che", "in", "ihr", "bl\u00fchn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Wenn ich nun, dadurch ger\u00fchret,", "tokens": ["Wenn", "ich", "nun", ",", "da\u00b7durch", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Das, was sie dadurch versp\u00fchret,", "tokens": ["Das", ",", "was", "sie", "da\u00b7durch", "ver\u00b7sp\u00fch\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "PAV", "VVFIN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.25": {"text": "Durch den Danck zum Sch\u00f6pfer richte;", "tokens": ["Durch", "den", "Danck", "zum", "Sch\u00f6p\u00b7fer", "rich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Sind es die verlangten Fr\u00fcchte", "tokens": ["Sind", "es", "die", "ver\u00b7lang\u00b7ten", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Welche GOTT, aus Lieb\u2019 allein,", "tokens": ["Wel\u00b7che", "GoTT", ",", "aus", "Lieb'", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "APPR", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Lieblich und gef\u00e4llig seyn.", "tokens": ["Lieb\u00b7lich", "und", "ge\u00b7f\u00e4l\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}