"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich meinem Blick?", "tokens": ["Dich", "mei\u00b7nem", "Blick", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.17": {"line.1": {"text": "Wie meine Doris, wenn sie fliehet,", "tokens": ["Wie", "mei\u00b7ne", "Do\u00b7ris", ",", "wenn", "sie", "flie\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NE", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihren Blick", "tokens": ["Und", "ih\u00b7ren", "Blick"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das Sommerh\u00fctchen mir entziehet.", "tokens": ["Das", "Som\u00b7mer\u00b7h\u00fct\u00b7chen", "mir", "ent\u00b7zie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O komm zur\u00fcck,", "tokens": ["O", "komm", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.18": {"line.1": {"text": "Und leucht' ihr! \u2013 Ah sie k\u00f6mmt! Entr\u00fccket", "tokens": ["Und", "leucht'", "ihr", "!", "\u2013", "Ah", "sie", "k\u00f6mmt", "!", "Ent\u00b7r\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "$.", "$(", "NN", "PPER", "VVFIN", "$.", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich dein Gesicht", "tokens": ["Sich", "dein", "Ge\u00b7