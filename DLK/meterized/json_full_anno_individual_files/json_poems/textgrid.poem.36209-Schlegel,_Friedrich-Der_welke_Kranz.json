{"textgrid.poem.36209": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der welke Kranz", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war noch Mai, da hast du sie gebrochen,", "tokens": ["Es", "war", "noch", "Mai", ",", "da", "hast", "du", "sie", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Blumen ausgesprochen, selber Bl\u00fcte,", "tokens": ["In", "Blu\u00b7men", "aus\u00b7ge\u00b7spro\u00b7chen", ",", "sel\u00b7ber", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIZU", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was bl\u00fchend im Gem\u00fcte schon sich regte,", "tokens": ["Was", "bl\u00fc\u00b7hend", "im", "Ge\u00b7m\u00fc\u00b7te", "schon", "sich", "reg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPRART", "NN", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und heilig sich bewegte,", "tokens": ["Und", "hei\u00b7lig", "sich", "be\u00b7weg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was kindlich ach! der Freund so gerne hegte,", "tokens": ["Was", "kind\u00b7lich", "ach", "!", "der", "Freund", "so", "ger\u00b7ne", "heg\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ITJ", "$.", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn sie ihr Herzchen legte an das seine,", "tokens": ["Wenn", "sie", "ihr", "Herz\u00b7chen", "leg\u00b7te", "an", "das", "sei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wo ich nun ewig weine.", "tokens": ["Wo", "ich", "nun", "e\u00b7wig", "wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Veilchen sandte mir das Kind zum Zeichen,", "tokens": ["Die", "Veil\u00b7chen", "sand\u00b7te", "mir", "das", "Kind", "zum", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die so mein Herz erweichen, da\u00df die Augen", "tokens": ["Die", "so", "mein", "Herz", "er\u00b7wei\u00b7chen", ",", "da\u00df", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Schmerz, den sie nun saugen, nie vollenden,", "tokens": ["Den", "Schmerz", ",", "den", "sie", "nun", "sau\u00b7gen", ",", "nie", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich oft noch zu ihr wenden,", "tokens": ["Sich", "oft", "noch", "zu", "ihr", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und finden welk den Kranz dann in den H\u00e4nden.", "tokens": ["Und", "fin\u00b7den", "welk", "den", "Kranz", "dann", "in", "den", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie der, hat sie zu enden fr\u00fch erkoren,", "tokens": ["Wie", "der", ",", "hat", "sie", "zu", "en\u00b7den", "fr\u00fch", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "$,", "VAFIN", "PPER", "PTKZU", "VVINF", "ADJD", "VVINF", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Sich unbewu\u00dft verloren.", "tokens": ["Sich", "un\u00b7be\u00b7wu\u00dft", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nimm hin die hohe k\u00f6stlich liebe Gabe,", "tokens": ["Nimm", "hin", "die", "ho\u00b7he", "k\u00f6st\u00b7lich", "lie\u00b7be", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das einz'ge, was ich habe von der Teuern,", "tokens": ["Das", "einz'\u00b7ge", ",", "was", "ich", "ha\u00b7be", "von", "der", "Teu\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PWS", "PPER", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Bild mir zu erneuern, wenn in Tr\u00e4nen,", "tokens": ["Ihr", "Bild", "mir", "zu", "er\u00b7neu\u00b7ern", ",", "wenn", "in", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$,", "KOUS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Tode zu das Sehnen", "tokens": ["Dem", "To\u00b7de", "zu", "das", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So gern entflieht der Erde eitlem W\u00e4hnen.", "tokens": ["So", "gern", "ent\u00b7flieht", "der", "Er\u00b7de", "eit\u00b7lem", "W\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch erst la\u00df mich in Tr\u00e4nen ganz versenken", "tokens": ["Doch", "erst", "la\u00df", "mich", "in", "Tr\u00e4\u00b7nen", "ganz", "ver\u00b7sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVIMP", "PPER", "APPR", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das s\u00fc\u00dfe Angedenken!", "tokens": ["Das", "s\u00fc\u00b7\u00dfe", "An\u00b7ge\u00b7den\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Uns, die in Lust des Todes Leben fanden,", "tokens": ["Uns", ",", "die", "in", "Lust", "des", "To\u00b7des", "Le\u00b7ben", "fan\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "K\u00fchn die Natur verstanden in den Flammen,", "tokens": ["K\u00fchn", "die", "Na\u00b7tur", "ver\u00b7stan\u00b7den", "in", "den", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Wo Lieb' und Schmerz zusammen uns verbunden;", "tokens": ["Wo", "Lieb'", "und", "Schmerz", "zu\u00b7sam\u00b7men", "uns", "ver\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Uns sei die Stirn umwunden,", "tokens": ["Uns", "sei", "die", "Stirn", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vom Zeichen, dessen Sinn wir l\u00e4ngst gefunden.", "tokens": ["Vom", "Zei\u00b7chen", ",", "des\u00b7sen", "Sinn", "wir", "l\u00e4ngst", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Denn spro\u00dften aus den Wunden oft nicht Rosen,", "tokens": ["Denn", "spro\u00df\u00b7ten", "aus", "den", "Wun\u00b7den", "oft", "nicht", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADV", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Uns schmerzlich liebzukosen?", "tokens": ["Uns", "schmerz\u00b7lich", "lieb\u00b7zu\u00b7ko\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "La\u00df denn des M\u00e4dchens Schatten uns umschweben,", "tokens": ["La\u00df", "denn", "des", "M\u00e4d\u00b7chens", "Schat\u00b7ten", "uns", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Wehmut hingegeben,", "tokens": ["Der", "Weh\u00b7mut", "hin\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis wir im Tode Eins noch inn'ger leben,", "tokens": ["Bis", "wir", "im", "To\u00b7de", "Eins", "noch", "inn'\u00b7ger", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dann dies tiefe Streben ganz vereinet,", "tokens": ["Und", "dann", "dies", "tie\u00b7fe", "Stre\u00b7ben", "ganz", "ver\u00b7ei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDS", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das l\u00e4chelnd sich beweinet.", "tokens": ["Das", "l\u00e4\u00b7chelnd", "sich", "be\u00b7wei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos