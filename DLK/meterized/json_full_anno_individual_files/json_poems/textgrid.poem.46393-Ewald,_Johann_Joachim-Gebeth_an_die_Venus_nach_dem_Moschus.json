{"textgrid.poem.46393": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Gebeth an die Venus nach dem Moschus", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O leite sch\u00f6nste Venus,", "tokens": ["O", "lei\u00b7te", "sch\u00f6ns\u00b7te", "Ve\u00b7nus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.2": {"text": "Die du am Himmel wachest,", "tokens": ["Die", "du", "am", "Him\u00b7mel", "wa\u00b7chest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Schritte deines Sch\u00e4fers,", "tokens": ["Die", "Schrit\u00b7te", "dei\u00b7nes", "Sch\u00e4\u00b7fers", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der itzt Dorinden suchet!", "tokens": ["Der", "itzt", "Do\u00b7rin\u00b7den", "su\u00b7chet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich geh nicht aus zu rauben,", "tokens": ["Ich", "geh", "nicht", "aus", "zu", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und denke nicht zu t\u00f6dten,", "tokens": ["Und", "den\u00b7ke", "nicht", "zu", "t\u00f6d\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mich treibt allein die Liebe,", "tokens": ["Mich", "treibt", "al\u00b7lein", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auch du suchst was du liebest.", "tokens": ["Auch", "du", "suchst", "was", "du", "lie\u00b7best", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PWS", "PPER", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "O leite sch\u00f6nste Venus,", "tokens": ["O", "lei\u00b7te", "sch\u00f6ns\u00b7te", "Ve\u00b7nus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.2": {"text": "Die du am Himmel wachest,", "tokens": ["Die", "du", "am", "Him\u00b7mel", "wa\u00b7chest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Schritte deines Sch\u00e4fers,", "tokens": ["Die", "Schrit\u00b7te", "dei\u00b7nes", "Sch\u00e4\u00b7fers", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der itzt Dorinden suchet!", "tokens": ["Der", "itzt", "Do\u00b7rin\u00b7den", "su\u00b7chet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich geh nicht aus zu rauben,", "tokens": ["Ich", "geh", "nicht", "aus", "zu", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "