{"textgrid.poem.51235": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "In fremden Landen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An des Heimatflusses Borden,", "tokens": ["An", "des", "Hei\u00b7mat\u00b7flus\u00b7ses", "Bor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Linden \u00fcberhangen,", "tokens": ["Wo", "die", "Lin\u00b7den", "\u00fc\u00b7berh\u00b7an\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin ich manches Mal gegangen,", "tokens": ["Bin", "ich", "man\u00b7ches", "Mal", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die Erde jung geworden", "tokens": ["Wenn", "die", "Er\u00b7de", "jung", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und den Fr\u00fchlingsmantel wob,", "tokens": ["Und", "den", "Fr\u00fch\u00b7lings\u00b7man\u00b7tel", "wob", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn die Wasser voller klangen", "tokens": ["Wenn", "die", "Was\u00b7ser", "vol\u00b7ler", "klan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und bis vor die F\u00fc\u00dfe drangen,", "tokens": ["Und", "bis", "vor", "die", "F\u00fc\u00b7\u00dfe", "dran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Da\u00df der Pfad sich schwellend hob.", "tokens": ["Da\u00df", "der", "Pfad", "sich", "schwel\u00b7lend", "hob", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn die Welle singend flieht,", "tokens": ["Wenn", "die", "Wel\u00b7le", "sin\u00b7gend", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist's, als h\u00f6re man Geschichten,", "tokens": ["Ist's", ",", "als", "h\u00f6\u00b7re", "man", "Ge\u00b7schich\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "VVFIN", "PIS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was im Oberland geschieht,", "tokens": ["Was", "im", "O\u00b7ber\u00b7land", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weit ins Niederland berichten;", "tokens": ["Weit", "ins", "Nie\u00b7der\u00b7land", "be\u00b7rich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und so man stromaufw\u00e4rts sieht,", "tokens": ["Und", "so", "man", "strom\u00b7auf\u00b7w\u00e4rts", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Will es scheinen, da\u00df das ganze", "tokens": ["Will", "es", "schei\u00b7nen", ",", "da\u00df", "das", "gan\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "$,", "KOUS", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Innre Land im Firnenglanze", "tokens": ["Inn\u00b7re", "Land", "im", "Fir\u00b7nen\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf der Flut herunterzieht.", "tokens": ["Auf", "der", "Flut", "her\u00b7un\u00b7ter\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ausgespannte Netze schimmern", "tokens": ["Aus\u00b7ge\u00b7spann\u00b7te", "Net\u00b7ze", "schim\u00b7mern"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwischen bl\u00fctenwei\u00dfen B\u00e4umen,", "tokens": ["Zwi\u00b7schen", "bl\u00fc\u00b7ten\u00b7wei\u00b7\u00dfen", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Perlend in der Sonne flimmern", "tokens": ["Per\u00b7lend", "in", "der", "Son\u00b7ne", "flim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie von feuchten Wassersch\u00e4umen;", "tokens": ["Sie", "von", "feuch\u00b7ten", "Was\u00b7ser\u00b7sch\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ein Kn\u00e4blein schl\u00e4ft im Kahn,", "tokens": ["Und", "ein", "Kn\u00e4\u00b7blein", "schl\u00e4ft", "im", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Schaukelnd sich in jungen Tr\u00e4umen;", "tokens": ["Schau\u00b7kelnd", "sich", "in", "jun\u00b7gen", "Tr\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ohne Hast und ohne S\u00e4umen", "tokens": ["Oh\u00b7ne", "Hast", "und", "oh\u00b7ne", "S\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schafft der Vater nebenan.", "tokens": ["Schafft", "der", "Va\u00b7ter", "ne\u00b7be\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja, mit ruhig festem Schritte", "tokens": ["Ja", ",", "mit", "ru\u00b7hig", "fes\u00b7tem", "Schrit\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreiten dort die M\u00e4nner hin;", "tokens": ["Schrei\u00b7ten", "dort", "die", "M\u00e4n\u00b7ner", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlicht bescheiden ist die Sitte,", "tokens": ["Schlicht", "be\u00b7schei\u00b7den", "ist", "die", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ernst bewegt der freie Sinn.", "tokens": ["Ernst", "be\u00b7wegt", "der", "frei\u00b7e", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und in ihrer sichern Mitte", "tokens": ["Und", "in", "ih\u00b7rer", "si\u00b7chern", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wuchsen Recht und Freiheit auf;", "tokens": ["Wuch\u00b7sen", "Recht", "und", "Frei\u00b7heit", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Das Gesetz schirmt Haus und H\u00fctte,", "tokens": ["Das", "Ge\u00b7setz", "schirmt", "Haus", "und", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Jeden Herd ein B\u00fcchsenlauf.", "tokens": ["Je\u00b7den", "Herd", "ein", "B\u00fcch\u00b7sen\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hier, an diesem fremden Strand,", "tokens": ["Hier", ",", "an", "die\u00b7sem", "frem\u00b7den", "Strand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wachsen Weine stark und s\u00fc\u00df,", "tokens": ["Wach\u00b7sen", "Wei\u00b7ne", "stark", "und", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es gleicht das \u00fcppige Land", "tokens": ["Und", "es", "gleicht", "das", "\u00fcp\u00b7pi\u00b7ge", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wohl auch einem Paradies;", "tokens": ["Wohl", "auch", "ei\u00b7nem", "Pa\u00b7ra\u00b7dies", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber dumpf und ungewi\u00df", "tokens": ["A\u00b7ber", "dumpf", "und", "un\u00b7ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind die Herzen und die Blicke,", "tokens": ["Sind", "die", "Her\u00b7zen", "und", "die", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und verworrene Geschicke", "tokens": ["Und", "ver\u00b7wor\u00b7re\u00b7ne", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Walten in der Finsternis.", "tokens": ["Wal\u00b7ten", "in", "der", "Fins\u00b7ter\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "An des Heimatflusses Borden,", "tokens": ["An", "des", "Hei\u00b7mat\u00b7flus\u00b7ses", "Bor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Linden \u00fcberhangen,", "tokens": ["Wo", "die", "Lin\u00b7den", "\u00fc\u00b7berh\u00b7an\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin ich manches Mal gegangen,", "tokens": ["Bin", "ich", "man\u00b7ches", "Mal", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die Erde jung geworden", "tokens": ["Wenn", "die", "Er\u00b7de", "jung", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und den Fr\u00fchlingsmantel wob,", "tokens": ["Und", "den", "Fr\u00fch\u00b7lings\u00b7man\u00b7tel", "wob", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn die Wasser voller klangen", "tokens": ["Wenn", "die", "Was\u00b7ser", "vol\u00b7ler", "klan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und bis vor die F\u00fc\u00dfe drangen,", "tokens": ["Und", "bis", "vor", "die", "F\u00fc\u00b7\u00dfe", "dran\u00b7gen", ","], "token_info":