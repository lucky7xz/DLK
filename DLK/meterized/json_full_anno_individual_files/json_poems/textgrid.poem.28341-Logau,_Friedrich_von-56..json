{"textgrid.poem.28341": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bey Hofe lernt man mercken, da\u00df die die besten seyn,", "tokens": ["Bey", "Ho\u00b7fe", "lernt", "man", "mer\u00b7cken", ",", "da\u00df", "die", "die", "bes\u00b7ten", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "VVINF", "$,", "KOUS", "ART", "ART", "ADJA", "VAINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die sonst nichts thun noch k\u00fcnnen, als schlucken au\u00df und ein;", "tokens": ["Die", "sonst", "nichts", "thun", "noch", "k\u00fcn\u00b7nen", ",", "als", "schlu\u00b7cken", "au\u00df", "und", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "VVINF", "ADV", "VVINF", "$,", "KOUS", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Vieh, das man bald soll schlachten, das pflegt und h\u00e4lt man wol;", "tokens": ["Vieh", ",", "das", "man", "bald", "soll", "schlach\u00b7ten", ",", "das", "pflegt", "und", "h\u00e4lt", "man", "wol", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIS", "ADV", "VMFIN", "VVINF", "$,", "PDS", "VVFIN", "KON", "VVFIN", "PIS", "ADV", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Man mag ihn lassen prassen, der endlich darben soll.", "tokens": ["Man", "mag", "ihn", "las\u00b7sen", "pras\u00b7sen", ",", "der", "end\u00b7li