{"dta.poem.10212": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "( Der alte General a. D. )", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Ungl\u00fcck dich und Schuld, zwei schwarze Rosse,", "tokens": ["Wenn", "Un\u00b7gl\u00fcck", "dich", "und", "Schuld", ",", "zwei", "schwar\u00b7ze", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "KON", "NN", "$,", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An ihren M\u00e4hnen durch das Leben schleifen,", "tokens": ["An", "ih\u00b7ren", "M\u00e4h\u00b7nen", "durch", "das", "Le\u00b7ben", "schlei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch Berg und Thal, im Schmutz der Gassengosse,", "tokens": ["Durch", "Berg", "und", "Thal", ",", "im", "Schmutz", "der", "Gas\u00b7sen\u00b7gos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du l\u00f6st dich nimmermehr aus ihren Schweifen.", "tokens": ["Du", "l\u00f6st", "dich", "nim\u00b7mer\u00b7mehr", "aus", "ih\u00b7ren", "Schwei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie rei\u00dfen dich, o ausgelass\u2019ne Posse,", "tokens": ["Sie", "rei\u00b7\u00dfen", "dich", ",", "o", "aus\u00b7ge\u00b7lass'\u00b7ne", "Pos\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Voraus nur deines Blutes Purpurstreifen,", "tokens": ["Vo\u00b7raus", "nur", "dei\u00b7nes", "Blu\u00b7tes", "Pur\u00b7pur\u00b7strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und hinterdrein noch schwirren die Geschosse", "tokens": ["Und", "hin\u00b7ter\u00b7drein", "noch", "schwir\u00b7ren", "die", "Ge\u00b7schos\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der lieben Menschen: Lachen, Spott und Keifen.", "tokens": ["Der", "lie\u00b7ben", "Men\u00b7schen", ":", "La\u00b7chen", ",", "Spott", "und", "Kei\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}