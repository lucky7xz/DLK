{"dta.poem.14004": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Elfe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Bleib' bei uns! wir haben den Tanzplan im Thal", "tokens": ["Bleib'", "bei", "uns", "!", "wir", "ha\u00b7ben", "den", "Tanz\u00b7plan", "im", "Thal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "$.", "PPER", "VAFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Bedeckt mit Mondesglanze,", "tokens": ["Be\u00b7deckt", "mit", "Mon\u00b7des\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Johannisw\u00fcrmchen erleuchten den Saal,", "tokens": ["Jo\u00b7han\u00b7nis\u00b7w\u00fcrm\u00b7chen", "er\u00b7leuch\u00b7ten", "den", "Saal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Die Heimchen spielen zum Tanze.", "tokens": ["Die", "Heim\u00b7chen", "spie\u00b7len", "zum", "Tan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}