{"dta.poem.23711": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An den Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Tod, was wilst du bei den Br&#252;dern?", "tokens": ["Tod", ",", "was", "wilst", "du", "bei", "den", "Br", "&#252;", "dern", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "$(", "PDS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Komst du her mit uns zu trinken?", "tokens": ["Komst", "du", "her", "mit", "uns", "zu", "trin\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Geh, hier ist f\u00fcr dich kein Rheinwein!", "tokens": ["Geh", ",", "hier", "ist", "f\u00fcr", "dich", "kein", "Rhein\u00b7wein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VAFIN", "APPR", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trink aus Heidelberger F\u00e4ssern;", "tokens": ["Trink", "aus", "Hei\u00b7del\u00b7ber\u00b7ger", "F\u00e4s\u00b7sern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn der Durst, in deinen Knochen,", "tokens": ["Denn", "der", "Durst", ",", "in", "dei\u00b7nen", "Kno\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist mit Maa\u00dfen nicht zu l\u00f6schen!", "tokens": ["Ist", "mit", "Maa\u00b7\u00dfen", "nicht", "zu", "l\u00f6\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Geh, du m\u00f6chtest statt der Br\u00fcder,", "tokens": ["Geh", ",", "du", "m\u00f6ch\u00b7test", "statt", "der", "Br\u00fc\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle R\u00f6mer ledig trinken!", "tokens": ["Al\u00b7le", "R\u00f6\u00b7mer", "le\u00b7dig", "trin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Geh, und la\u00df die Br\u00fcder trinken!", "tokens": ["Geh", ",", "und", "la\u00df", "die", "Br\u00fc\u00b7der", "trin\u00b7ken", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "VVIMP", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Denn du wilst sie doch nicht holen?", "tokens": ["Denn", "du", "wilst", "sie", "doch", "nicht", "ho\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Nein, du holst ia nur die Alten;", "tokens": ["Nein", ",", "du", "holst", "i\u00b7a", "nur", "die", "Al\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Denn was soll das Reich der Todten", "tokens": ["Denn", "was", "soll", "das", "Reich", "der", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VMFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Mit den Schatten, die noch trinken?", "tokens": ["Mit", "den", "Schat\u00b7ten", ",", "die", "noch", "trin\u00b7ken", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Du verschonst die muntre Jugend.", "tokens": ["Du", "ver\u00b7schonst", "die", "mun\u00b7tre", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Tod, weil du der Jugend schonest,", "tokens": ["Tod", ",", "weil", "du", "der", "Ju\u00b7gend", "scho\u00b7nest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Solst du doch den Rheinwein schmekken.", "tokens": ["Solst", "du", "doch", "den", "Rhein\u00b7wein", "schmek\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Sieh, dort steht ein voller R\u00f6mer,", "tokens": ["Sieh", ",", "dort", "steht", "ein", "vol\u00b7ler", "R\u00f6\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Hol ihn mit den d\u00fcrren H\u00e4nden!", "tokens": ["Hol", "ihn", "mit", "den", "d\u00fcr\u00b7ren", "H\u00e4n\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Kanst du wol den R\u00f6mer halten?", "tokens": ["Kanst", "du", "wol", "den", "R\u00f6\u00b7mer", "hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Trinke, wenn die Br\u00fcder trinken;", "tokens": ["Trin\u00b7ke", ",", "wenn", "die", "Br\u00fc\u00b7der", "trin\u00b7ken", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Aber ruf erst mit den Br\u00fcdern:", "tokens": ["A\u00b7ber", "ruf", "erst", "mit", "den", "Br\u00fc\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Auf, es leben alle M\u00e4dchen!", "tokens": ["Auf", ",", "es", "le\u00b7ben", "al\u00b7le", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Und wenn dir der Rheinwein schmekket,", "tokens": ["Und", "wenn", "dir", "der", "Rhein\u00b7wein", "schmek\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.24": {"text": "O so iauchze mit den Br\u00fcdern,", "tokens": ["O", "so", "i\u00b7a\u00b7uch\u00b7ze", "mit", "den", "Br\u00fc\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}