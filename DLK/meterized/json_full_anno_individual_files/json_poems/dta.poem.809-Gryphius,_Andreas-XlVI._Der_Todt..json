{"dta.poem.809": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlVI.  \n Der Todt.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Was hilfft die gantze Welt! Mensch! deine St&#363;de schlegt! ", "tokens": ["Was", "hilfft", "die", "gant\u00b7ze", "Welt", "!", "Mensch", "!", "dei\u00b7ne", "St", "&#363;", "de", "schlegt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$.", "NN", "$.", "PPOSAT", "NN", "$(", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zwar ehr al\u00df du vermeynt! doch wer mu\u00df nicht erblei-", "tokens": ["Zwar", "ehr", "al\u00df", "du", "ver\u00b7meynt", "!", "doch", "wer", "mu\u00df", "nicht", "er\u00b7blei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOUS", "PPER", "VVPP", "$.", "KON", "PWS", "VMFIN", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "chen? ", "tokens": ["chen", "?"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Nun wird die sch\u00f6nheit rauch; nun mu\u00df die Tugend weichen/", "tokens": ["Nun", "wird", "die", "sch\u00f6n\u00b7heit", "rauch", ";", "nun", "mu\u00df", "die", "Tu\u00b7gend", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$.", "ADV", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun ist dein Adel dunst/ die st\u00e4rcke wird bewegt!", "tokens": ["Nun", "ist", "dein", "A\u00b7del", "dunst", "/", "die", "st\u00e4r\u00b7cke", "wird", "be\u00b7wegt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVFIN", "$(", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hier f\u00e4llt auff eine Baar der Hutt vnd Krone tr\u00e4gt", "tokens": ["Hier", "f\u00e4llt", "auff", "ei\u00b7ne", "Baar", "der", "Hutt", "vnd", "Kro\u00b7ne", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier feilt die grosse kunst/ kein Tagus sch\u00fctzt die reichen.", "tokens": ["Hier", "feilt", "die", "gros\u00b7se", "kunst", "/", "kein", "Ta\u00b7gus", "sch\u00fctzt", "die", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "PIAT", "NN", "VVFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man siht kein Alter an/ die gantz verstellte Leichen", "tokens": ["Man", "siht", "kein", "Al\u00b7ter", "an", "/", "die", "gantz", "ver\u00b7stell\u00b7te", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PIAT", "NN", "PTKVZ", "$(", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "(o Freunde! gutte nacht!) wird in den staub gelegt", "tokens": ["(", "o", "Freun\u00b7de", "!", "gut\u00b7te", "nacht", "!", ")", "wird", "in", "den", "staub", "ge\u00b7legt"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "FM", "NN", "$.", "ADJA", "NN", "$.", "$(", "VAFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du scheidest! gantz allein! von hier! wohin! so schnelle!", "tokens": ["Du", "schei\u00b7dest", "!", "gantz", "al\u00b7lein", "!", "von", "hier", "!", "wo\u00b7hin", "!", "so", "schnel\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "ADV", "$.", "APPR", "ADV", "$.", "PWAV", "$.", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Di\u00df ist de\u00df Himmels bahn! die \u00f6ffnet dir die helle!", "tokens": ["Di\u00df", "ist", "de\u00df", "Him\u00b7mels", "bahn", "!", "die", "\u00f6ff\u00b7net", "dir", "die", "hel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVFIN", "$.", "ART", "VVFIN", "PPER", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nach dem der strenge Printz sein ernstes Vrtheil hegt.", "tokens": ["Nach", "dem", "der", "stren\u00b7ge", "Printz", "sein", "erns\u00b7tes", "Vrtheil", "hegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nichts bringst du auf die welt/ nichts kanst du mit bekom\u0303en:", "tokens": ["Nichts", "bringst", "du", "auf", "die", "welt", "/", "nichts", "kanst", "du", "mit", "be\u00b7kom\u0303en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "PIS", "VMFIN", "PPER", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der einig\u2019 Augenblick hat/ was man hat/ genommen.", "tokens": ["Der", "ei\u00b7nig'", "Au\u00b7gen\u00b7blick", "hat", "/", "was", "man", "hat", "/", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "$(", "PWS", "PIS", "VAFIN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch zeucht dein werck dir nach! Mensch! deine Stunde", "tokens": ["Doch", "zeucht", "dein", "werck", "dir", "nach", "!", "Mensch", "!", "dei\u00b7ne", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$.", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "schlegt. ", "tokens": ["schlegt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}}}}