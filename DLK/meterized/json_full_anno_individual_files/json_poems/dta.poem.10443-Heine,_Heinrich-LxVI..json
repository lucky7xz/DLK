{"dta.poem.10443": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Die alten, b\u00f6sen Lieder,", "tokens": ["Die", "al\u00b7ten", ",", "b\u00f6\u00b7sen", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Tr\u00e4ume schlimm und arg,", "tokens": ["Die", "Tr\u00e4u\u00b7me", "schlimm", "und", "arg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die la\u00dft uns jetzt begraben,", "tokens": ["Die", "la\u00dft", "uns", "jetzt", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Holt einen gro\u00dfen Sarg.", "tokens": ["Holt", "ei\u00b7nen", "gro\u00b7\u00dfen", "Sarg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hinein leg' ich gar manches,", "tokens": ["Hin\u00b7ein", "leg'", "ich", "gar", "man\u00b7ches", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch sag' ich noch nicht was;", "tokens": ["Doch", "sag'", "ich", "noch", "nicht", "was", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "PWS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Sarg mu\u00df seyn noch gr\u00f6\u00dfer", "tokens": ["Der", "Sarg", "mu\u00df", "seyn", "noch", "gr\u00f6\u00b7\u00dfer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie's Heidelberger Fa\u00df.", "tokens": ["Wie's", "Hei\u00b7del\u00b7ber\u00b7ger", "Fa\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und holt eine Todtenbahre,", "tokens": ["Und", "holt", "ei\u00b7ne", "Tod\u00b7ten\u00b7bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von Brettern fest und dick;", "tokens": ["Von", "Bret\u00b7tern", "fest", "und", "dick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch mu\u00df sie seyn noch l\u00e4nger", "tokens": ["Auch", "mu\u00df", "sie", "seyn", "noch", "l\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als wie zu Mainz die Br\u00fcck'.", "tokens": ["Als", "wie", "zu", "Mainz", "die", "Br\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "NE", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und holt mir auch zw\u00f6lf Riesen,", "tokens": ["Und", "holt", "mir", "auch", "zw\u00f6lf", "Rie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die m\u00fcssen noch st\u00e4rker seyn", "tokens": ["Die", "m\u00fcs\u00b7sen", "noch", "st\u00e4r\u00b7ker", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "ADJD", "VAINF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als wie der starke Christoph", "tokens": ["Als", "wie", "der", "star\u00b7ke", "Chris\u00b7toph"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Im Dom zu C\u00f6ln am Rhein.", "tokens": ["Im", "Dom", "zu", "C\u00f6ln", "am", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "APPRART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die sollen den Sarg forttragen,", "tokens": ["Die", "sol\u00b7len", "den", "Sarg", "fort\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und senken in's Meer hinab;", "tokens": ["Und", "sen\u00b7ken", "in's", "Meer", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn solchem gro\u00dfen Sarge", "tokens": ["Denn", "sol\u00b7chem", "gro\u00b7\u00dfen", "Sar\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geb\u00fchrt ein gro\u00dfes Grab.", "tokens": ["Ge\u00b7b\u00fchrt", "ein", "gro\u00b7\u00dfes", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wi\u00dft Ihr warum der Sarg wohl", "tokens": ["Wi\u00dft", "Ihr", "wa\u00b7rum", "der", "Sarg", "wohl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PWAV", "ART", "NN", "ADV"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "So gro\u00df und schwer mag seyn?", "tokens": ["So", "gro\u00df", "und", "schwer", "mag", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich legt' auch meine Liebe", "tokens": ["Ich", "legt'", "auch", "mei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und meinen Schmerz hinein.", "tokens": ["Und", "mei\u00b7nen", "Schmerz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}