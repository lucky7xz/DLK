{"dta.poem.8614": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Reise-Empfindung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah in bleicher Silbertracht", "tokens": ["Ich", "sah", "in", "blei\u00b7cher", "Sil\u00b7ber\u00b7tracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Birkenst\u00e4mme prangen,", "tokens": ["Die", "Bir\u00b7ken\u00b7st\u00e4m\u00b7me", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als w\u00e4re d'ran aus heller Nacht", "tokens": ["Als", "w\u00e4\u00b7re", "d'\u00b7ran", "aus", "hel\u00b7ler", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Mondlicht blieben hangen;", "tokens": ["Das", "Mond\u00b7licht", "blie\u00b7ben", "han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und in dem zarten Birkenhain", "tokens": ["Und", "in", "dem", "zar\u00b7ten", "Bir\u00b7ken\u00b7hain"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah ich ein H\u00e4uschen blinken,", "tokens": ["Sah", "ich", "ein", "H\u00e4usc\u00b7hen", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das hob gleich an, zu sich hinein", "tokens": ["Das", "hob", "gleich", "an", ",", "zu", "sich", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "PTKVZ", "$,", "APPR", "PRF", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Holdfreundlich mich zu winken.", "tokens": ["Hold\u00b7freund\u00b7lich", "mich", "zu", "win\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie da im rothen Morgenstrahl", "tokens": ["Wie", "da", "im", "ro\u00b7then", "Mor\u00b7gen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fensterlein ergl\u00e4nzten;", "tokens": ["Die", "Fens\u00b7ter\u00b7lein", "er\u00b7gl\u00e4nz\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wie so freudig Berg und Thal", "tokens": ["Und", "wie", "so", "freu\u00b7dig", "Berg", "und", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Rosen sich bekr\u00e4nzten!", "tokens": ["Mit", "Ro\u00b7sen", "sich", "be\u00b7kr\u00e4nz\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Rebe auf zum Fenster klomm", "tokens": ["Die", "Re\u00b7be", "auf", "zum", "Fens\u00b7ter", "klomm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihren goldnen Trauben;", "tokens": ["Mit", "ih\u00b7ren", "gold\u00b7nen", "Trau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Unschuld sa\u00df am Dache fromm", "tokens": ["Die", "Un\u00b7schuld", "sa\u00df", "am", "Da\u00b7che", "fromm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In stillen wei\u00dfen Tauben.", "tokens": ["In", "stil\u00b7len", "wei\u00b7\u00dfen", "Tau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Lerche sang und schwand dahin", "tokens": ["Die", "Ler\u00b7che", "sang", "und", "schwand", "da\u00b7hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf morgenfrohen Schwingen,", "tokens": ["Auf", "mor\u00b7gen\u00b7fro\u00b7hen", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So da\u00df der blaue Himmel schien", "tokens": ["So", "da\u00df", "der", "blau\u00b7e", "Him\u00b7mel", "schien"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins Thal herabzusingen. \u2014", "tokens": ["Ins", "Thal", "her\u00b7ab\u00b7zu\u00b7sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da meint' ich schon, das Fenster soll", "tokens": ["Da", "meint'", "ich", "schon", ",", "das", "Fens\u00b7ter", "soll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich freundlich mir erschlie\u00dfen,", "tokens": ["Sich", "freund\u00b7lich", "mir", "er\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und aus dem Rahmen liebevoll", "tokens": ["Und", "aus", "dem", "Rah\u00b7men", "lie\u00b7be\u00b7voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Theure mich begr\u00fc\u00dfen.", "tokens": ["Die", "Theu\u00b7re", "mich", "be\u00b7gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du seligste der Fantasei'n!", "tokens": ["Du", "se\u00b7ligs\u00b7te", "der", "Fan\u00b7ta\u00b7sei'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Ach, w\u00e4r' es mir beschieden,", "tokens": ["Ach", ",", "w\u00e4r'", "es", "mir", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit ihr zu leben hier allein", "tokens": ["Mit", "ihr", "zu", "le\u00b7ben", "hier", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im s\u00fc\u00dfen Waldesfrieden!", "tokens": ["Im", "s\u00fc\u00b7\u00dfen", "Wal\u00b7des\u00b7frie\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Mit ihr im linden Fr\u00fchlingshauch", "tokens": ["Mit", "ihr", "im", "lin\u00b7den", "Fr\u00fch\u00b7lings\u00b7hauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch diesen Hain zu wallen,", "tokens": ["Durch", "die\u00b7sen", "Hain", "zu", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu lauschen hier im Bl\u00fcthenstrauch", "tokens": ["Zu", "lau\u00b7schen", "hier", "im", "Bl\u00fc\u00b7then\u00b7strauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Lied der Nachtigallen.", "tokens": ["Dem", "Lied", "der", "Nach\u00b7ti\u00b7gal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Mit ihr zu schau'n im Herbsteswehn", "tokens": ["Mit", "ihr", "zu", "schau'n", "im", "Herbs\u00b7tes\u00b7wehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die welken Bl\u00e4tter fliegen,", "tokens": ["Die", "wel\u00b7ken", "Bl\u00e4t\u00b7ter", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Umrauscht vom schmerzlichen Vergehn,", "tokens": ["Um\u00b7rauscht", "vom", "schmerz\u00b7li\u00b7chen", "Ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich fest an sie zu schmiegen.", "tokens": ["Mich", "fest", "an", "sie", "zu", "schmie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wenn dann in rauher Winterzeit", "tokens": ["Wenn", "dann", "in", "rau\u00b7her", "Win\u00b7ter\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Lied mein Liebchen s\u00e4nge,", "tokens": ["Ein", "Lied", "mein", "Lieb\u00b7chen", "s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und aller Himmel Seligkeit", "tokens": ["Und", "al\u00b7ler", "Him\u00b7mel", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir in die Stube dr\u00e4nge! \u2014", "tokens": ["Mir", "in", "die", "Stu\u00b7be", "dr\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ich wagt' es mich zu regen kaum", "tokens": ["Ich", "wagt'", "es", "mich", "zu", "re\u00b7gen", "kaum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meinem stillen Sinnen,", "tokens": ["In", "mei\u00b7nem", "stil\u00b7len", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Besorgt, das H\u00e4uschen m\u00f6cht', ein Traum,", "tokens": ["Be\u00b7sorgt", ",", "das", "H\u00e4usc\u00b7hen", "m\u00f6cht'", ",", "ein", "Traum", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "VMFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor meinem Blick zerrinnen.", "tokens": ["Vor", "mei\u00b7nem", "Blick", "zer\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Doch, sieh, da \u00f6ffnet sich die Th\u00fcr,", "tokens": ["Doch", ",", "sieh", ",", "da", "\u00f6ff\u00b7net", "sich", "die", "Th\u00fcr", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "$,", "ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Zauber war geschwunden,", "tokens": ["Der", "Zau\u00b7ber", "war", "ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es trat ein J\u00e4gersmann herf\u00fcr", "tokens": ["Es", "trat", "ein", "J\u00e4\u00b7gers\u00b7mann", "her\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit nachgesprengten Hunden.", "tokens": ["Mit", "nach\u00b7ge\u00b7spreng\u00b7ten", "Hun\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Er gr\u00fc\u00dfte mich mit raschem Blick", "tokens": ["Er", "gr\u00fc\u00df\u00b7te", "mich", "mit", "ra\u00b7schem", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und streift' waldein gar heiter,", "tokens": ["Und", "streift'", "wal\u00b7dein", "gar", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich gab ihm seinen Gru\u00df zur\u00fcck,", "tokens": ["Ich", "gab", "ihm", "sei\u00b7nen", "Gru\u00df", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und traurig ging ich weiter.", "tokens": ["Und", "trau\u00b7rig", "ging", "ich", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}