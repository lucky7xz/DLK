{"textgrid.poem.47615": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[da\u00df sich in Oasen-Gr\u00fcne]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df sich in Oasen-Gr\u00fcne", "tokens": ["Da\u00df", "sich", "in", "O\u00b7a\u00b7sen\u00b7Gr\u00fc\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine Quelle Weins erg\u00f6sse,", "tokens": ["Ei\u00b7ne", "Quel\u00b7le", "Weins", "er\u00b7g\u00f6s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deren unbekanntes Eden", "tokens": ["De\u00b7ren", "un\u00b7be\u00b7kann\u00b7tes", "E\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine W\u00fcstenei umschl\u00f6sse!", "tokens": ["Ei\u00b7ne", "W\u00fcs\u00b7te\u00b7nei", "um\u00b7schl\u00f6s\u00b7se", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hin zu dieser Quelle z\u00f6g' ich,", "tokens": ["Hin", "zu", "die\u00b7ser", "Quel\u00b7le", "z\u00f6g'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bauet' eine kleine H\u00fctte,", "tokens": ["Bau\u00b7et'", "ei\u00b7ne", "klei\u00b7ne", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-