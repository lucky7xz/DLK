{"dta.poem.8626": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Herbstklage .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Holder Lenz, du bist dahin!", "tokens": ["Hol\u00b7der", "Lenz", ",", "du", "bist", "da\u00b7hin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nirgends, nirgends darfst du bleiben!", "tokens": ["Nir\u00b7gends", ",", "nir\u00b7gends", "darfst", "du", "blei\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich sah dein frohes Bl\u00fch'n,", "tokens": ["Wo", "ich", "sah", "dein", "fro\u00b7hes", "Bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Rauscht des Herbstes banges Treiben.", "tokens": ["Rauscht", "des", "Herbs\u00b7tes", "ban\u00b7ges", "Trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie der Wind so traurig fuhr", "tokens": ["Wie", "der", "Wind", "so", "trau\u00b7rig", "fuhr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch den Strauch, als ob er weine;", "tokens": ["Durch", "den", "Strauch", ",", "als", "ob", "er", "wei\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sterbeseufzer der Natur", "tokens": ["Ster\u00b7be\u00b7seuf\u00b7zer", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schauern durch die welken Haine.", "tokens": ["Schau\u00b7ern", "durch", "die", "wel\u00b7ken", "Hai\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wieder ist, wie bald! wie bald!", "tokens": ["Wie\u00b7der", "ist", ",", "wie", "bald", "!", "wie", "bald", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "ADV", "$.", "PWAV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir ein Jahr dahingeschwunden!", "tokens": ["Mir", "ein", "Jahr", "da\u00b7hin\u00b7ge\u00b7schwun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fragend rauscht es aus dem Wald:", "tokens": ["Fra\u00b7gend", "rauscht", "es", "aus", "dem", "Wald", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hast du sie noch nicht gefunden? \u2014", "tokens": ["Hast", "du", "sie", "noch", "nicht", "ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}