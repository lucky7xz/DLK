{"textgrid.poem.64076": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Dora's Abendlied", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still tritt der Mond in weiter Himmelsferne", "tokens": ["Still", "tritt", "der", "Mond", "in", "wei\u00b7ter", "Him\u00b7mels\u00b7fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus des Gew\u00f6lkes n\u00e4chtlich grauem Flor,", "tokens": ["Aus", "des", "Ge\u00b7w\u00f6l\u00b7kes", "n\u00e4cht\u00b7lich", "grau\u00b7em", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In goldner Reinheit schimmernd jetzt hervor,", "tokens": ["In", "gold\u00b7ner", "Rein\u00b7heit", "schim\u00b7mernd", "jetzt", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Umgeben von dem hellen Chor der Sterne;", "tokens": ["Um\u00b7ge\u00b7ben", "von", "dem", "hel\u00b7len", "Chor", "der", "Ster\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihn, den ich mir zum Freunde auserkohr,", "tokens": ["Ihn", ",", "den", "ich", "mir", "zum", "Freun\u00b7de", "au\u00b7ser\u00b7kohr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ihn, dem ich klagte, was ich l\u00e4ngst verlohr,", "tokens": ["Ihn", ",", "dem", "ich", "klag\u00b7te", ",", "was", "ich", "l\u00e4ngst", "ver\u00b7lohr", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Begr\u00fcsst mein Blick in stiller Nacht so gerne.", "tokens": ["Be\u00b7gr\u00fcs\u00b7st", "mein", "Blick", "in", "stil\u00b7ler", "Nacht", "so", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Er leuchtet freundlich mir statt aller Kerzen,", "tokens": ["Er", "leuch\u00b7tet", "freund\u00b7lich", "mir", "statt", "al\u00b7ler", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Strahlt leisen Trost in die beklommne Brust,", "tokens": ["Strahlt", "lei\u00b7sen", "Trost", "in", "die", "be\u00b7klomm\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und schenkt in Thr\u00e4nen mir der Wehmuth Lust.", "tokens": ["Und", "schenkt", "in", "Thr\u00e4\u00b7nen", "mir", "der", "Weh\u00b7muth", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wer nimmt des Kummers Last von meinem Herzen,", "tokens": ["Wer", "nimmt", "des", "Kum\u00b7mers", "Last", "von", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wer hat um ihren Umfang je gewusst? \u2013", "tokens": ["Wer", "hat", "um", "ih\u00b7ren", "Um\u00b7fang", "je", "ge\u00b7wusst", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ach tief verschlossen in der wunden Brust", "tokens": ["Ach", "tief", "ver\u00b7schlos\u00b7sen", "in", "der", "wun\u00b7den", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADJD", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ist all' mein Weh \u2013 sind alle meine Schmerzen.", "tokens": ["Ist", "all'", "mein", "Weh", "\u2013", "sind", "al\u00b7le", "mei\u00b7ne", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "$(", "VAFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du, den ich l\u00e4ngst nicht mehr zu nennen wage,", "tokens": ["Du", ",", "den", "ich", "l\u00e4ngst", "nicht", "mehr", "zu", "nen\u00b7nen", "wa\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und dessen Bild mich dennoch stets umschwebt!", "tokens": ["Und", "des\u00b7sen", "Bild", "mich", "den\u00b7noch", "stets", "um\u00b7schwebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du, der im Innern meines Herzens lebt,", "tokens": ["Du", ",", "der", "im", "In\u00b7nern", "mei\u00b7nes", "Her\u00b7zens", "lebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo ich nur Dich, und Schmerz und Sehnsucht trage,", "tokens": ["Wo", "ich", "nur", "Dich", ",", "und", "Schmerz", "und", "Sehn\u00b7sucht", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPER", "$,", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O wenn Dein Blick hinauf zum Himmel strebt", "tokens": ["O", "wenn", "Dein", "Blick", "hin\u00b7auf", "zum", "Him\u00b7mel", "strebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und holde Tr\u00e4ume Dir der Mondschein webt,", "tokens": ["Und", "hol\u00b7de", "Tr\u00e4u\u00b7me", "Dir", "der", "Mond\u00b7schein", "webt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So denk' auch Du an unsres Gl\u00fcckes Tage.", "tokens": ["So", "denk'", "auch", "Du", "an", "uns\u00b7res", "Gl\u00fc\u00b7ckes", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie sind dahin \u2013 in weite Ferne bannte,", "tokens": ["Sie", "sind", "da\u00b7hin", "\u2013", "in", "wei\u00b7te", "Fer\u00b7ne", "bann\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "$(", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Dir getrennt, mich grausam mein Geschick.", "tokens": ["Von", "Dir", "ge\u00b7trennt", ",", "mich", "grau\u00b7sam", "mein", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erloschen ist in Thr\u00e4nen nun der Blick,", "tokens": ["Er\u00b7lo\u00b7schen", "ist", "in", "Thr\u00e4\u00b7nen", "nun", "der", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In dem sonst Muth und Hoffnung lodernd brannte.", "tokens": ["In", "dem", "sonst", "Muth", "und", "Hoff\u00b7nung", "lo\u00b7dernd", "brann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "NN", "KON", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der ersten Liebe nahmenloses Gl\u00fcck", "tokens": ["Der", "ers\u00b7ten", "Lie\u00b7be", "nah\u00b7men\u00b7lo\u00b7ses", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Rief meines Schicksals Stimme ernst zur\u00fcck,", "tokens": ["Rief", "mei\u00b7nes", "Schick\u00b7sals", "Stim\u00b7me", "ernst", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Eh' ich des Lebens vollen Werth erkannte.", "tokens": ["Eh'", "ich", "des", "Le\u00b7bens", "vol\u00b7len", "Werth", "er\u00b7kann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Seitdem verh\u00fcllt mit ihrem schwarzen Schleier", "tokens": ["Seit\u00b7dem", "ver\u00b7h\u00fcllt", "mit", "ih\u00b7rem", "schwar\u00b7zen", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Schwermuth mir die weite offne Welt;", "tokens": ["Die", "Schwer\u00b7muth", "mir", "die", "wei\u00b7te", "off\u00b7ne", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Himmels hehres, sternbes\u00e4etes Zelt,", "tokens": ["Des", "Him\u00b7mels", "heh\u00b7res", ",", "stern\u00b7be\u00b7s\u00e4e\u00b7tes", "Zelt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Mondes Glanz, der oft in stiller Feier", "tokens": ["Des", "Mon\u00b7des", "Glanz", ",", "der", "oft", "in", "stil\u00b7ler", "Fei\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der N\u00e4chte \u00f6des Dunkel mir erhellt,", "tokens": ["Der", "N\u00e4ch\u00b7te", "\u00f6\u00b7des", "Dun\u00b7kel", "mir", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und ahnungsvoll die bange Brust mir schwellt,", "tokens": ["Und", "ah\u00b7nungs\u00b7voll", "die", "ban\u00b7ge", "Brust", "mir", "schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er\u00f6ffnet nur mein Herz der Wehmuth freier.", "tokens": ["Er\u00b7\u00f6ff\u00b7net", "nur", "mein", "Herz", "der", "Weh\u00b7muth", "frei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ist mir auf ewig jenes Gl\u00fcck verschwunden?", "tokens": ["Ist", "mir", "auf", "e\u00b7wig", "je\u00b7nes", "Gl\u00fcck", "ver\u00b7schwun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJD", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist schmerzliches Entbehren nur mein Loos?", "tokens": ["Ist", "schmerz\u00b7li\u00b7ches", "Ent\u00b7beh\u00b7ren", "nur", "mein", "Loos", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wird allein des Grabes finstrer Schooss", "tokens": ["Und", "wird", "al\u00b7lein", "des", "Gra\u00b7bes", "finst\u00b7rer", "Scho\u00b7oss"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich sch\u00fctzen vor des Leidens bangen Stunden,", "tokens": ["Mich", "sch\u00fct\u00b7zen", "vor", "des", "Lei\u00b7dens", "ban\u00b7gen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So reisse schnell mich von dem Leben los,", "tokens": ["So", "reis\u00b7se", "schnell", "mich", "von", "dem", "Le\u00b7ben", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Willkommner Tod, denn in der Erde Schooss", "tokens": ["Will\u00b7komm\u00b7ner", "Tod", ",", "denn", "in", "der", "Er\u00b7de", "Scho\u00b7oss"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Verbluten sanft des Herzens tiefe Wunden.", "tokens": ["Ver\u00b7blu\u00b7ten", "sanft", "des", "Her\u00b7zens", "tie\u00b7fe", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Still tritt der Mond in weiter Himmelsferne", "tokens": ["Still", "tritt", "der", "Mond", "in", "wei\u00b7ter", "Him\u00b7mels\u00b7fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus des Gew\u00f6lkes n\u00e4chtlich grauem Flor,", "tokens": ["Aus", "des", "Ge\u00b7w\u00f6l\u00b7kes", "n\u00e4cht\u00b7lich", "grau\u00b7em", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In goldner Reinheit schimmernd jetzt hervor,", "tokens": ["In", "gold\u00b7ner", "Rein\u00b7heit", "schim\u00b7mernd", "jetzt", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Umgeben von dem hellen Chor der Sterne;", "tokens": ["Um\u00b7ge\u00b7ben", "von", "dem", "hel\u00b7len", "Chor", "der", "Ster\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihn, den ich mir zum Freunde auserkohr,", "tokens": ["Ihn", ",", "den", "ich", "mir", "zum", "Freun\u00b7de", "au\u00b7ser\u00b7kohr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ihn, dem ich klagte, was ich l\u00e