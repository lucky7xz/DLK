{"dta.poem.17993": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "236.  Nichts h\u00f6hers ist als GOttes Sohn  \n seyn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gottes Sohn ist GOtt/ mit Gott regiert auf einem", "tokens": ["Got\u00b7tes", "Sohn", "ist", "Gott", "/", "mit", "Gott", "re\u00b7giert", "auf", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VAFIN", "NN", "$(", "APPR", "NN", "VVFIN", "APPR", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Thron/", "tokens": ["Thron", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Nichts h\u00f6hers ist als ich/ wenn ich bin dieser Sohn.", "tokens": ["Nichts", "h\u00f6\u00b7hers", "ist", "als", "ich", "/", "wenn", "ich", "bin", "die\u00b7ser", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VAFIN", "KOUS", "PPER", "$(", "KOUS", "PPER", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}