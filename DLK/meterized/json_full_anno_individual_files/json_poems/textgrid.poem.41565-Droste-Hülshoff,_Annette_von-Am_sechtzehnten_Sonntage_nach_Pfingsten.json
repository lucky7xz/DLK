{"textgrid.poem.41565": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am sechtzehnten Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer nur vertraut auf Gottes Macht", "tokens": ["Wer", "nur", "ver\u00b7traut", "auf", "Got\u00b7tes", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADJD", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In allen seinen N\u00f6ten,", "tokens": ["In", "al\u00b7len", "sei\u00b7nen", "N\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den hat kein Feind zum Fall gebracht,", "tokens": ["Den", "hat", "kein", "Feind", "zum", "Fall", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PIAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den kann kein \u00dcbel t\u00f6ten;", "tokens": ["Den", "kann", "kein", "\u00dc\u00b7bel", "t\u00f6\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wo die Angst ihn \u00fcberf\u00e4llt,", "tokens": ["Und", "wo", "die", "Angst", "ihn", "\u00fc\u00b7berf\u00b7\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da wird der allerst\u00e4rkste Held", "tokens": ["Da", "wird", "der", "al\u00b7ler\u00b7st\u00e4rks\u00b7te", "Held"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "An seine Seite treten.", "tokens": ["An", "sei\u00b7ne", "Sei\u00b7te", "tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der wird mit seinem scharfen Speer", "tokens": ["Der", "wird", "mit", "sei\u00b7nem", "schar\u00b7fen", "Speer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Gegner ihm zerst\u00e4uben,", "tokens": ["Die", "Geg\u00b7ner", "ihm", "zer\u00b7st\u00e4u\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und von dem allergr\u00f6\u00dften Heer", "tokens": ["Und", "von", "dem", "al\u00b7ler\u00b7gr\u00f6\u00df\u00b7ten", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Huf wird \u00fcbrigbleiben;", "tokens": ["Kein", "Huf", "wird", "\u00fcb\u00b7rig\u00b7blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sei's \u00e4u\u00dfrer oder innrer Feind,", "tokens": ["Sei's", "\u00e4u\u00df\u00b7rer", "o\u00b7der", "inn\u00b7rer", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn nur der rechte Held erscheint,", "tokens": ["Wenn", "nur", "der", "rech\u00b7te", "Held", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der kann ihm Grenzen schreiben.", "tokens": ["Der", "kann", "ihm", "Gren\u00b7zen", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er ist der allerbeste Herr,", "tokens": ["Er", "ist", "der", "al\u00b7ler\u00b7bes\u00b7te", "Herr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den einer mag erlangen,", "tokens": ["Den", "ei\u00b7ner", "mag", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gl\u00fcckselig lebt der Fr\u00f6ner, der", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", "lebt", "der", "Fr\u00f6\u00b7ner", ",", "der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "PRELS"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "In seinem Dienst gefangen.", "tokens": ["In", "sei\u00b7nem", "Dienst", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So s\u00fc\u00df ist seine Sklaverei,", "tokens": ["So", "s\u00fc\u00df", "ist", "sei\u00b7ne", "Skla\u00b7ve\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df jeder, sei er noch so frei,", "tokens": ["Da\u00df", "je\u00b7der", ",", "sei", "er", "noch", "so", "frei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mag tragen drum Verlangen.", "tokens": ["Mag", "tra\u00b7gen", "drum", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PAV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Des Hungers Qual, der Bl\u00f6\u00dfe Schmach,", "tokens": ["Des", "Hun\u00b7gers", "Qual", ",", "der", "Bl\u00f6\u00b7\u00dfe", "Schmach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die wei\u00df er zu vergelten;", "tokens": ["Die", "wei\u00df", "er", "zu", "ver\u00b7gel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es durft' ihn noch bis diesen Tag", "tokens": ["Es", "durft'", "ihn", "noch", "bis", "die\u00b7sen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht einer treulos schelten.", "tokens": ["Nicht", "ei\u00b7ner", "treu\u00b7los", "schel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er zahlt mit wucherndem Gewinst", "tokens": ["Er", "zahlt", "mit", "wu\u00b7chern\u00b7dem", "Ge\u00b7winst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An alle, die in seinen Dienst", "tokens": ["An", "al\u00b7le", ",", "die", "in", "sei\u00b7nen", "Dienst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr Gut und Leben stellten.", "tokens": ["Ihr", "Gut", "und", "Le\u00b7ben", "stell\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und aller St\u00e4rke Talisman,", "tokens": ["Und", "al\u00b7ler", "St\u00e4r\u00b7ke", "Ta\u00b7lis\u00b7man", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den h\u00e4lt er in der Rechten;", "tokens": ["Den", "h\u00e4lt", "er", "in", "der", "Rech\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Selbst aus den sch\u00e4rfsten Dornen kann", "tokens": ["Selbst", "aus", "den", "sch\u00e4rfs\u00b7ten", "Dor\u00b7nen", "kann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er Rosenkr\u00e4nze flechten.", "tokens": ["Er", "Ro\u00b7sen\u00b7kr\u00e4n\u00b7ze", "flech\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er zeigt im wilden Kampfrevier", "tokens": ["Er", "zeigt", "im", "wil\u00b7den", "Kampf\u00b7re\u00b7vier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die echte Aaronsschlange dir,", "tokens": ["Die", "ech\u00b7te", "Aa\u00b7rons\u00b7schlan\u00b7ge", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mu\u00dft du mit Vipern fechten.", "tokens": ["Mu\u00dft", "du", "mit", "Vi\u00b7pern", "fech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und r\u00fcttelt sich der grimmste Feind:", "tokens": ["Und", "r\u00fct\u00b7telt", "sich", "der", "grimms\u00b7te", "Feind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da lehrt er dich ein Zeichen,", "tokens": ["Da", "lehrt", "er", "dich", "ein", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vor dem, so schlimm er es auch meint,", "tokens": ["Vor", "dem", ",", "so", "schlimm", "er", "es", "auch", "meint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "ADV", "ADJD", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df schnell der Drache weichen.", "tokens": ["Mu\u00df", "schnell", "der", "Dra\u00b7che", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur sei es von bereiter Hand", "tokens": ["Nur", "sei", "es", "von", "be\u00b7rei\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit rechtem Glauben angewandt,", "tokens": ["Mit", "rech\u00b7tem", "Glau\u00b7ben", "an\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sonst mag es nimmer reichen.", "tokens": ["Sonst", "mag", "es", "nim\u00b7mer", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wem schwach der Glaube und Vertraun,", "tokens": ["Wem", "schwach", "der", "Glau\u00b7be", "und", "Ver\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob ihn die Sehnsucht treibe,", "tokens": ["Ob", "ihn", "die", "Sehn\u00b7sucht", "trei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der darf doch noch von ferne schaun,", "tokens": ["Der", "darf", "doch", "noch", "von", "fer\u00b7ne", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er im Nachtrab bleibe;", "tokens": ["Da\u00df", "er", "im", "Nach\u00b7trab", "blei\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf dem erquickend in der Glut", "tokens": ["Auf", "dem", "er\u00b7qui\u00b7ckend", "in", "der", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Helden milder Schatten ruht", "tokens": ["Des", "Hel\u00b7den", "mil\u00b7der", "Schat\u00b7ten", "ruht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie m\u00e4cht'gen Schildes Scheibe.", "tokens": ["Wie", "m\u00e4cht'\u00b7gen", "Schil\u00b7des", "Schei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Doch wem der Glaube echt und klar,", "tokens": ["Doch", "wem", "der", "Glau\u00b7be", "echt", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den kann kein Leid bezwingen,", "tokens": ["Den", "kann", "kein", "Leid", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der mag wohl aller G\u00fcter bar", "tokens": ["Der", "mag", "wohl", "al\u00b7ler", "G\u00fc\u00b7ter", "bar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch wie ein Vogel singen:", "tokens": ["Noch", "wie", "ein", "Vo\u00b7gel", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbschaut doch die Lilien in dem Feld", "tokens": ["\u00bb", "schaut", "doch", "die", "Li\u00b7li\u00b7en", "in", "dem", "Feld"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wie sind sie frisch und wohlbestellt,", "tokens": ["Wie", "sind", "sie", "frisch", "und", "wohl\u00b7be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie gr\u00fcn und guter Dingen!", "tokens": ["Wie", "gr\u00fcn", "und", "gu\u00b7ter", "Din\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sie haben nicht des Webens acht", "tokens": ["Sie", "ha\u00b7ben", "nicht", "des", "We\u00b7bens", "acht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "NN", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sind so reich gezieret,", "tokens": ["Und", "sind", "so", "reich", "ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Salomo in sei