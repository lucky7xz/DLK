ben", "stell\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Und aller St\u00e4rke Talisman,", "tokens": ["Und", "al\u00b7ler", "St\u00e4r\u00b7ke", "Ta\u00b7lis\u00b7man", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den h\u00e4lt er in der Rechten;", "tokens": ["Den", "h\u00e4lt", "er", "in", "der", "Rech\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Selbst aus den sch\u00e4rfsten Dornen kann", "tokens": ["Selbst", "aus", "den", "sch\u00e4rfs\u00b7ten", "Dor\u00b7nen", "kann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er Rosenkr\u00e4nze flechten.", "tokens": ["Er", "Ro\u00b7sen\u00b7kr\u00e4n\u00b7ze", "flech\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er zeigt im wilden Kampfrevier", "tokens": ["Er", "zeigt", "im", "wil\u00b7den", "Kampf\u00b7re\u00b7vier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die echte Aaronsschlange dir,", "tokens": ["Die", "ech\u00b7te", "Aa\u00b7rons\u00b7schlan\u00b7ge", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mu\u00dft du mit Vipern fecht