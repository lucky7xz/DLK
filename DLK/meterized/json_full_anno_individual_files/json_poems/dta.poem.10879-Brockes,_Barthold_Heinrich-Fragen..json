{"dta.poem.10879": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fragen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr wen bebl\u00fcmen sich die Felder?", "tokens": ["F\u00fcr", "wen", "be\u00b7bl\u00fc\u00b7men", "sich", "die", "Fel\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr wen belauben sich die W\u00e4lder?", "tokens": ["F\u00fcr", "wen", "be\u00b7lau\u00b7ben", "sich", "die", "W\u00e4l\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr wen sprie\u00dft, durch der Sonnen Stral,", "tokens": ["F\u00fcr", "wen", "sprie\u00dft", ",", "durch", "der", "Son\u00b7nen", "Stral", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VVFIN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Kr\u00e4uter Menge sonder Zahl?", "tokens": ["Der", "Kr\u00e4u\u00b7ter", "Men\u00b7ge", "son\u00b7der", "Zahl", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr wen h\u00f6rt man der V\u00f6gel Singen", "tokens": ["F\u00fcr", "wen", "h\u00f6rt", "man", "der", "V\u00f6\u00b7gel", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWS", "VVFIN", "PIS", "ART", "NN", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "So lieblich und so s\u00fc\u00df erklingen?", "tokens": ["So", "lieb\u00b7lich", "und", "so", "s\u00fc\u00df", "er\u00b7klin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr wen erf\u00fcllt die laue Luft,", "tokens": ["F\u00fcr", "wen", "er\u00b7f\u00fcllt", "die", "lau\u00b7e", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus Blumen, solch ein Balsam-Duft?", "tokens": ["Aus", "Blu\u00b7men", ",", "solch", "ein", "Bal\u00b7sam\u00b7Duft", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "F\u00fcr wen sieht man, durch tausend R\u00f6hren,", "tokens": ["F\u00fcr", "wen", "sieht", "man", ",", "durch", "tau\u00b7send", "R\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VVFIN", "PIS", "$,", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sich Gras in Milch und Fleisch verkehren?", "tokens": ["Sich", "Gras", "in", "Milch", "und", "Fleisch", "ver\u00b7keh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "F\u00fcr wen tr\u00e4gt, sonder unsre M\u00fch,", "tokens": ["F\u00fcr", "wen", "tr\u00e4gt", ",", "son\u00b7der", "uns\u00b7re", "M\u00fch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VVFIN", "$,", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Zur Kleidung, Woll und Haar das Vieh?", "tokens": ["Zur", "Klei\u00b7dung", ",", "Woll", "und", "Haar", "das", "Vieh", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "F\u00fcr wen entspriessen so viel Fr\u00fcchte?", "tokens": ["F\u00fcr", "wen", "ent\u00b7spries\u00b7sen", "so", "viel", "Fr\u00fcch\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "So viel und mancherley Gerichte?", "tokens": ["So", "viel", "und", "man\u00b7cher\u00b7ley", "Ge\u00b7rich\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und wem zu gut w\u00e4chst Holz und Stein?", "tokens": ["Und", "wem", "zu", "gut", "w\u00e4chst", "Holz", "und", "Stein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKA", "ADJD", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "F\u00fcr dich, o Mensch, nur blo\u00df allein;", "tokens": ["F\u00fcr", "dich", ",", "o", "Mensch", ",", "nur", "blo\u00df", "al\u00b7lein", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$,", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und kannst so unempfindlich seyn?", "tokens": ["Und", "kannst", "so", "un\u00b7emp\u00b7find\u00b7lich", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Von wem entstehn denn alle Gaben,", "tokens": ["Von", "wem", "ent\u00b7stehn", "denn", "al\u00b7le", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVINF", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Die dich ern\u00e4hren, kleiden, laben?", "tokens": ["Die", "dich", "er\u00b7n\u00e4h\u00b7ren", ",", "klei\u00b7den", ",", "la\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$,", "ADJA", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Von Gott, dem Sch\u00f6pfer, blo\u00df allein,", "tokens": ["Von", "Gott", ",", "dem", "Sch\u00f6p\u00b7fer", ",", "blo\u00df", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und kannst so gar undankbar seyn!", "tokens": ["Und", "kannst", "so", "gar", "un\u00b7dank\u00b7bar", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}