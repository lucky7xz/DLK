{"textgrid.poem.36417": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit sch\u00f6nen Kindern will wohl jeder scherzen.", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit sch\u00f6nen Kindern will wohl jeder scherzen.", "tokens": ["Mit", "sch\u00f6\u00b7nen", "Kin\u00b7dern", "will", "wohl", "je\u00b7der", "scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch wollt ihr ihr euren Scherz zu ernsthaft geben,", "tokens": ["Doch", "wollt", "ihr", "ihr", "eu\u00b7ren", "Scherz", "zu", "ernst\u00b7haft", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "PPOSAT", "NN", "PTKA", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So wird die leichte Sch\u00f6nheit leicht entschweben,", "tokens": ["So", "wird", "die", "leich\u00b7te", "Sch\u00f6n\u00b7heit", "leicht", "ent\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ihr der guten Kinder Gunst verscherzen", "tokens": ["Und", "ihr", "der", "gu\u00b7ten", "Kin\u00b7der", "Gunst", "ver\u00b7scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "W\u00fcnscht ihr der h\u00f6chsten Freude s\u00fc\u00dfe Schmerzen,", "tokens": ["W\u00fcnscht", "ihr", "der", "h\u00f6chs\u00b7ten", "Freu\u00b7de", "s\u00fc\u00b7\u00dfe", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So m\u00fc\u00dft ihr nie ges\u00e4ttigt strebend leben,", "tokens": ["So", "m\u00fc\u00dft", "ihr", "nie", "ge\u00b7s\u00e4t\u00b7tigt", "stre\u00b7bend", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVPP", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Gabe hingegeben neu erheben,", "tokens": ["Die", "Ga\u00b7be", "hin\u00b7ge\u00b7ge\u00b7ben", "neu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und kindlich t\u00e4ndeln mit den zarten Herzen.", "tokens": ["Und", "kind\u00b7lich", "t\u00e4n\u00b7deln", "mit", "den", "zar\u00b7ten", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Scherzen mit dem Scherz ist was sie suchen.", "tokens": ["Das", "Scher\u00b7zen", "mit", "dem", "Scherz", "ist", "was", "sie", "su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "PWS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man darf es ihnen wahrlich nicht verdenken;", "tokens": ["Man", "darf", "es", "ih\u00b7nen", "wahr\u00b7lich", "nicht", "v