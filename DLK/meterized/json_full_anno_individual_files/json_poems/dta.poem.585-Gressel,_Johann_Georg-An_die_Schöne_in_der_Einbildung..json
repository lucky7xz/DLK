{"dta.poem.585": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die Sch\u00f6ne in der Einbildung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Gewi\u00df wird Lethens Trunck bey dir die Ursach seyn/", "tokens": ["Ge\u00b7wi\u00df", "wird", "Le\u00b7thens", "Trunck", "bey", "dir", "die", "Ur\u00b7sach", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "NN", "APPR", "PPER", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df deiner Jahre Zahl dir schon entfallen ist;", "tokens": ["Da\u00df", "dei\u00b7ner", "Jah\u00b7re", "Zahl", "dir", "schon", "ent\u00b7fal\u00b7len", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn wenn ich drey\u00dfig z\u00e4hl\u2019 so sagest du/ ach Nein!", "tokens": ["Denn", "wenn", "ich", "drey\u00b7\u00dfig", "z\u00e4hl'", "so", "sa\u00b7gest", "du", "/", "ach", "Nein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "ADV", "VVFIN", "PPER", "$(", "XY", "PTKANT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da mu\u00df erst achtzehn stehn/ wo er schon drey\u00dfig liest/", "tokens": ["Da", "mu\u00df", "erst", "acht\u00b7zehn", "stehn", "/", "wo", "er", "schon", "drey\u00b7\u00dfig", "liest", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "VVINF", "VVINF", "$(", "PWAV", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Seht doch an die Rosen-Wangen", "tokens": ["Seht", "doch", "an", "die", "Ro\u00b7sen\u00b7Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie sie prangen/", "tokens": ["Wie", "sie", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Kan wol ein garstig Bild von so viel drey\u00dfig Jahren", "tokens": ["Kan", "wol", "ein", "gars\u00b7tig", "Bild", "von", "so", "viel", "drey\u00b7\u00dfig", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "ADJD", "NN", "APPR", "ADV", "ADV", "CARD", "NN"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Sich mit der Anmuth paaren/", "tokens": ["Sich", "mit", "der", "An\u00b7muth", "paa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "So mein Gesichte zeigt?", "tokens": ["So", "mein", "Ge\u00b7sich\u00b7te", "zeigt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Nein eure Meynung treugt.", "tokens": ["Nein", "eu\u00b7re", "Mey\u00b7nung", "treugt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}