{"dta.poem.7366": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Stiftung Cappenbergs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mond mit seinem blassen Finger", "tokens": ["Der", "Mond", "mit", "sei\u00b7nem", "blas\u00b7sen", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Langt leise durch den Mauerspalt,", "tokens": ["Langt", "lei\u00b7se", "durch", "den", "Mau\u00b7ers\u00b7palt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und koset, streifend l\u00e4ngs dem Zwinger,", "tokens": ["Und", "ko\u00b7set", ",", "strei\u00b7fend", "l\u00e4ngs", "dem", "Zwin\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Norbertus' Stirne feucht und kalt.", "tokens": ["Nor\u00b7ber\u00b7tus'", "Stir\u00b7ne", "feucht", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Der lehnt an br\u00f6ckelndem Gestein,", "tokens": ["Der", "lehnt", "an", "br\u00f6\u00b7ckeln\u00b7dem", "Ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Salpeterflocken seine Daunen,", "tokens": ["Sal\u00b7pe\u00b7ter\u00b7flo\u00b7cken", "sei\u00b7ne", "Dau\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "An seinem Ohre Heimchen raunen,", "tokens": ["An", "sei\u00b7nem", "Oh\u00b7re", "Heim\u00b7chen", "rau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und wimmelnd rennt das Tausendbein.", "tokens": ["Und", "wim\u00b7melnd", "rennt", "das", "Tau\u00b7send\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und \u00fcber'm Haupte f\u00fchlt er's beben,", "tokens": ["Und", "\u00fc\u00b7ber'm", "Haup\u00b7te", "f\u00fchlt", "er's", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da geht es hoch, da zecht es frisch,", "tokens": ["Da", "geht", "es", "hoch", ",", "da", "zecht", "es", "frisch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Pulsen sch\u00e4umend pocht das Leben,", "tokens": ["In", "Pul\u00b7sen", "sch\u00e4u\u00b7mend", "pocht", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Humpen tanzen auf dem Tisch.", "tokens": ["Die", "Hum\u00b7pen", "tan\u00b7zen", "auf", "dem", "Tisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Graf von Arnsberg giebt ein Fest,", "tokens": ["Der", "Graf", "von", "Arns\u00b7berg", "giebt", "ein", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Schwiegersohn der graue Schw\u00e4her;", "tokens": ["Dem", "Schwie\u00b7ger\u00b7sohn", "der", "grau\u00b7e", "Schw\u00e4\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So mehr er trinkt so wird er z\u00e4her,", "tokens": ["So", "mehr", "er", "trinkt", "so", "wird", "er", "z\u00e4\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So wirrer steht sein Lockennest.", "tokens": ["So", "wir\u00b7rer", "steht", "sein", "Lo\u00b7cken\u00b7nest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gern hat sein Kind er dem Dynasten,", "tokens": ["Gern", "hat", "sein", "Kind", "er", "dem", "Dy\u00b7nas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem reichen Cappenberg vertraut,", "tokens": ["Dem", "rei\u00b7chen", "Cap\u00b7pen\u00b7berg", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun tr\u00e4gt sein Anker Doppellasten!", "tokens": ["Nun", "tr\u00e4gt", "sein", "An\u00b7ker", "Dop\u00b7pel\u00b7las\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und seinen Feinden hat's gegraut.", "tokens": ["Und", "sei\u00b7nen", "Fein\u00b7den", "hat's", "ge\u00b7graut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da k\u00f6mmt auf seinem Eselein", "tokens": ["Da", "k\u00f6mmt", "auf", "sei\u00b7nem", "E\u00b7sel\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Norbert, und macht den Sohn zum Pfaffen;", "tokens": ["Nor\u00b7bert", ",", "und", "macht", "den", "Sohn", "zum", "Pfaf\u00b7fen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Allein er wu\u00dfte Rath zu schaffen,", "tokens": ["Al\u00b7lein", "er", "wu\u00df\u00b7te", "Rath", "zu", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er pferchte den Apostel ein.", "tokens": ["Er", "pferch\u00b7te", "den", "A\u00b7pos\u00b7tel", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie, keine Enkel soll er wiegen?", "tokens": ["Wie", ",", "kei\u00b7ne", "En\u00b7kel", "soll", "er", "wie\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PIAT", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Soll in des Eidams Hora gehn,", "tokens": ["Soll", "in", "des", "Ei\u00b7dams", "Ho\u00b7ra", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "NE", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sehn sein Kind am Boden liegen", "tokens": ["Und", "sehn", "sein", "Kind", "am", "Bo\u00b7den", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Paternosterkugeln drehn?", "tokens": ["Und", "Pa\u00b7ter\u00b7nos\u00b7ter\u00b7ku\u00b7geln", "drehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nein, heute ist der Tag wo mu\u00df,", "tokens": ["Nein", ",", "heu\u00b7te", "ist", "der", "Tag", "wo", "mu\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "ART", "NN", "PWAV", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo wird die Sache sich erled'gen,", "tokens": ["Wo", "wird", "die", "Sa\u00b7che", "sich", "er\u00b7le\u00b7d'\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und sollt' er mit dem Schwerte pred'gen,", "tokens": ["Und", "sollt'", "er", "mit", "dem", "Schwer\u00b7te", "pre\u00b7d'\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ein umgekehrter Carolus.", "tokens": ["Ein", "um\u00b7ge\u00b7kehr\u00b7ter", "Ca\u00b7ro\u00b7lus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und \u201eGottfried\u201c, spricht er, \u201eJunge, Ritter,", "tokens": ["Und", "\u201e", "Gott\u00b7fried", "\u201c", ",", "spricht", "er", ",", "\u201e", "Jun\u00b7ge", ",", "Rit\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "$(", "NE", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So sieh doch einmal in die H\u00f6h!", "tokens": ["So", "sieh", "doch", "ein\u00b7mal", "in", "die", "H\u00f6h", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du schaust ja in den Wein so bitter", "tokens": ["Du", "schaust", "ja", "in", "den", "Wein", "so", "bit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Requiem und Kyrie.", "tokens": ["Wie", "Re\u00b7qui\u00b7em", "und", "Ky\u00b7rie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Was spinnst du an dem alten Werg?", "tokens": ["Was", "spinnst", "du", "an", "dem", "al\u00b7ten", "Werg", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df die Kaputze grauen S\u00fcndern,", "tokens": ["La\u00df", "die", "Ka\u00b7put\u00b7ze", "grau\u00b7en", "S\u00fcn\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und deine Burg die la\u00df den Kindern,", "tokens": ["Und", "dei\u00b7ne", "Burg", "die", "la\u00df", "den", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "VVIMP", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dein sch\u00f6nes festes Cappenberg!\u201c", "tokens": ["Dein", "sch\u00f6\u00b7nes", "fes\u00b7tes", "Cap\u00b7pen\u00b7berg", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und drunten in dem feuchten Thurme", "tokens": ["Und", "drun\u00b7ten", "in", "dem", "feuch\u00b7ten", "Thur\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Heil'ge fl\u00fcstert: \u201eGro\u00dfer Gott,", "tokens": ["Der", "Heil'\u00b7ge", "fl\u00fcs\u00b7tert", ":", "\u201e", "Gro\u00b7\u00dfer", "Gott", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allgegenw\u00e4rt'ger du im Wurme", "tokens": ["All\u00b7ge\u00b7gen\u00b7w\u00e4rt'\u00b7ger", "du", "im", "Wur\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als in der Krone blankem Spott,", "tokens": ["Als", "in", "der", "Kro\u00b7ne", "blan\u00b7kem", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie gr\u00f6\u00dfer deine Allmacht zeigt", "tokens": ["Wie", "gr\u00f6\u00b7\u00dfer", "dei\u00b7ne", "All\u00b7macht", "zeigt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein F\u00fc\u00dfchen, das lebendig zittert,", "tokens": ["Sein", "F\u00fc\u00df\u00b7chen", ",", "das", "le\u00b7ben\u00b7dig", "zit\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als eine Mauer die verwittert,", "tokens": ["Als", "ei\u00b7ne", "Mau\u00b7er", "die", "ver\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und ob ein Babel dr\u00fcber steigt!\u201c", "tokens": ["Und", "ob", "ein", "Ba\u00b7bel", "dr\u00fc\u00b7ber", "steigt", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PAV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u201eja\u201c spricht der Graf, den Humpen schwenkend:", "tokens": ["\u201e", "ja", "\u201c", "spricht", "der", "Graf", ",", "den", "Hum\u00b7pen", "schwen\u00b7kend", ":"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$(", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ew\u00e4r Norbert hier, dein Eselmann,", "tokens": ["\u201e", "w\u00e4r", "Nor\u00b7bert", "hier", ",", "dein", "E\u00b7sel\u00b7mann", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "NE", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich lie\u00df ihm f\u00fcllen, dein gedenkend,", "tokens": ["Ich", "lie\u00df", "ihm", "f\u00fcl\u00b7len", ",", "dein", "ge\u00b7den\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und trinken m\u00f6cht er was er kann;", "tokens": ["Und", "trin\u00b7ken", "m\u00f6cht", "er", "was", "er", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch da ihm Pech und Schwefel gl\u00fcht,", "tokens": ["Doch", "da", "ihm", "Pech", "und", "Schwe\u00b7fel", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was andern Sch\u00e4chern mild und s\u00fc\u00dfe,", "tokens": ["Was", "an\u00b7dern", "Sch\u00e4\u00b7chern", "mild", "und", "s\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So bleibt er besser im Verlie\u00dfe,", "tokens": ["So", "bleibt", "er", "bes\u00b7ser", "im", "Ver\u00b7lie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein wohlkasteiter Eremit.\u201c", "tokens": ["Ein", "wohl\u00b7kas\u00b7tei\u00b7ter", "E\u00b7re\u00b7mit", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und drunten spricht's mit mildem Tone:", "tokens": ["Und", "drun\u00b7ten", "spricht's", "mit", "mil\u00b7dem", "To\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edu der, des Himmels h\u00f6chste Zier,", "tokens": ["\u201e", "du", "der", ",", "des", "Him\u00b7mels", "h\u00f6chs\u00b7te", "Zier", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ART", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gezogen bist zur Dornenkrone", "tokens": ["Ge\u00b7zo\u00b7gen", "bist", "zur", "Dor\u00b7nen\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf einem still dem\u00fcth'gen Thier,", "tokens": ["Auf", "ei\u00b7nem", "still", "de\u00b7m\u00fcth'\u00b7gen", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du, der des Mondes Lieblichkeit", "tokens": ["Du", ",", "der", "des", "Mon\u00b7des", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In meinen Kerker lie\u00dfest rinnen,", "tokens": ["In", "mei\u00b7nen", "Ker\u00b7ker", "lie\u00b7\u00dfest", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gez\u00e4hmt mir die vertrauten Spinnen,", "tokens": ["Ge\u00b7z\u00e4hmt", "mir", "die", "ver\u00b7trau\u00b7ten", "Spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du, Milder, seyst gebenedeit!\u201c", "tokens": ["Du", ",", "Mil\u00b7der", ",", "seyst", "ge\u00b7be\u00b7ne\u00b7deit", "!", "\u201c"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und Gottfried, k\u00e4mpfend mit den Thr\u00e4nen,", "tokens": ["Und", "Gott\u00b7fried", ",", "k\u00e4mp\u00b7fend", "mit", "den", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ergreift den Humpen, noch gef\u00fcllt,", "tokens": ["Er\u00b7greift", "den", "Hum\u00b7pen", ",", "noch", "ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor seinem Ohr ein leises St\u00f6hnen,", "tokens": ["Vor", "sei\u00b7nem", "Ohr", "ein", "lei\u00b7ses", "St\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor seinem Aug' ein bleiches Bild.", "tokens": ["Vor", "sei\u00b7nem", "Aug'", "ein", "blei\u00b7ches", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O, dringen m\u00f6cht er durch den Stein,", "tokens": ["O", ",", "drin\u00b7gen", "m\u00f6cht", "er", "durch", "den", "Stein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVINF", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo seine s\u00fcnd'gen F\u00fc\u00dfe stehen,", "tokens": ["Wo", "sei\u00b7ne", "s\u00fcn\u00b7d'\u00b7gen", "F\u00fc\u00b7\u00dfe", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "O, einmal, einmal m\u00f6cht' er sehen", "tokens": ["O", ",", "ein\u00b7mal", ",", "ein\u00b7mal", "m\u00f6cht'", "er", "se\u00b7hen"], "token_info": ["word", "punct", "word", "punct", "w