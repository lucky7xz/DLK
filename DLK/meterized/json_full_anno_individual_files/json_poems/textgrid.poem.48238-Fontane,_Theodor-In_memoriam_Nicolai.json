{"textgrid.poem.48238": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "In memoriam Nicolai", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verha\u00dft ist mir alle Philisterei,", "tokens": ["Ver\u00b7ha\u00dft", "ist", "mir", "al\u00b7le", "Phi\u00b7lis\u00b7te\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wei\u00df mich auch leidlich davon frei,", "tokens": ["Wei\u00df", "mich", "auch", "leid\u00b7lich", "da\u00b7von", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur den unbedingten Begeisterungsschritt", "tokens": ["Nur", "den", "un\u00b7be\u00b7ding\u00b7ten", "Be\u00b7geis\u00b7te\u00b7rungs\u00b7schritt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "In Sachen der Kunst, den mach' ich nicht mit, \u2013", "tokens": ["In", "Sa\u00b7chen", "der", "Kunst", ",", "den", "mach'", "ich", "nicht", "mit", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "ART", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hab' ich's zu kalt oder hab' ich's zu hei\u00df,", "tokens": ["Hab'", "ich's", "zu", "kalt", "o\u00b7der", "hab'", "ich's", "zu", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKA", "ADJD", "KON", "VAFIN", "PIS", "PTKA", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "So f\u00fchl ich: auch Kunst hat ihren Preis.", "tokens": ["So", "f\u00fchl", "ich", ":", "auch", "Kunst", "hat", "ih\u00b7ren", "Preis", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Italien ... das Auge wird mir hell..", "tokens": ["I\u00b7ta\u00b7li\u00b7en", "...", "das", "Au\u00b7ge", "wird", "mir", "hell", ".."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Bellin, Giorgione, Raffael,", "tokens": ["Bel\u00b7lin", ",", "Gi\u00b7or\u00b7gio\u00b7ne", ",", "Raf\u00b7fael", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,"], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.9": {"text": "Aber wenn ich durch schreckensvolle N\u00e4chte", "tokens": ["A\u00b7ber", "wenn", "ich", "durch", "schre\u00b7ckens\u00b7vol\u00b7le", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.10": {"text": "Gek\u00e4mpft mit dem Heerwurm h\u00f6llischer M\u00e4chte,", "tokens": ["Ge\u00b7k\u00e4mpft", "mit", "dem", "Heer\u00b7wurm", "h\u00f6l\u00b7li\u00b7scher", "M\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Ka