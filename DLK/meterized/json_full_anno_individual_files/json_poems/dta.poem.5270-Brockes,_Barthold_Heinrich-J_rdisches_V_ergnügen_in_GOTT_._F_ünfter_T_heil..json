{"dta.poem.5270": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, w\u00fcrde doch in uns, durch bunter Blumen-Pracht,", "tokens": ["Ach", ",", "w\u00fcr\u00b7de", "doch", "in", "uns", ",", "durch", "bun\u00b7ter", "Blu\u00b7men\u00b7Pracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "ADV", "APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die als ein Feuer gl\u00fcht, ein Feuer angefacht", "tokens": ["Die", "als", "ein", "Feu\u00b7er", "gl\u00fcht", ",", "ein", "Feu\u00b7er", "an\u00b7ge\u00b7facht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "KOUS", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Andacht und von Lust. M\u00f6cht uns der Rose Schein", "tokens": ["Von", "An\u00b7dacht", "und", "von", "Lust", ".", "M\u00f6cht", "uns", "der", "Ro\u00b7se", "Schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$.", "VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zur reinen Liebes-Gluht ein sch\u00f6ner Zunder seyn!", "tokens": ["Zur", "rei\u00b7nen", "Lie\u00b7bes\u00b7Gluht", "ein", "sch\u00f6\u00b7ner", "Zun\u00b7der", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Himmlischen Sapphirs gestirntes blaues Prangen", "tokens": ["Des", "Himm\u00b7li\u00b7schen", "Sap\u00b7phirs", "ge\u00b7stirn\u00b7tes", "blau\u00b7es", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "ADJA", "ADJA", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Erreg\u2019 in unsrer Brust ein feuriges Verlangen,", "tokens": ["Er\u00b7reg'", "in", "uns\u00b7rer", "Brust", "ein", "feu\u00b7ri\u00b7ges", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nach jener Himmels-Lust in den gestirnten H\u00f6hen", "tokens": ["Nach", "je\u00b7ner", "Him\u00b7mels\u00b7Lust", "in", "den", "ge\u00b7stirn\u00b7ten", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit gl\u00e4ubiger Begier zu sehen!", "tokens": ["Mit", "gl\u00e4u\u00b7bi\u00b7ger", "Be\u00b7gier", "zu", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der liebliche Geruch der Bl\u00fchte", "tokens": ["Der", "lieb\u00b7li\u00b7che", "Ge\u00b7ruch", "der", "Bl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Erreg\u2019, in s\u00fcsser Lust, die Sehnsucht im Gem\u00fcthe,", "tokens": ["Er\u00b7reg'", ",", "in", "s\u00fcs\u00b7ser", "Lust", ",", "die", "Sehn\u00b7sucht", "im", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dem Sch\u00f6pfer Himmels und der Erden,", "tokens": ["Dem", "Sch\u00f6p\u00b7fer", "Him\u00b7mels", "und", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ein lieblicher Geruch zu werden!", "tokens": ["Ein", "lieb\u00b7li\u00b7cher", "Ge\u00b7ruch", "zu", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Auf eine solche s\u00fcsse Weise", "tokens": ["Auf", "ei\u00b7ne", "sol\u00b7che", "s\u00fcs\u00b7se", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Scheint, da\u00df, zu seinem ew\u2019gen Preise,", "tokens": ["Scheint", ",", "da\u00df", ",", "zu", "sei\u00b7nem", "ew'\u00b7gen", "Prei\u00b7se", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Der Sch\u00f6pfer seinen Zweck in Schaffung dieser Welt,", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "sei\u00b7nen", "Zweck", "in", "Schaf\u00b7fung", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So viel an uns, am w\u00fcrdigsten erh\u00e4lt.", "tokens": ["So", "viel", "an", "uns", ",", "am", "w\u00fcr\u00b7digs\u00b7ten", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "$,", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}