{"textgrid.poem.59908": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll mich bekehren, weil ich nicht", "tokens": ["Soll", "mich", "be\u00b7keh\u00b7ren", ",", "weil", "ich", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PRF", "VVINF", "$,", "KOUS", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im richtigen Geleise bin,", "tokens": ["Im", "rich\u00b7ti\u00b7gen", "Ge\u00b7lei\u00b7se", "bin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Derweil ich g\u00e4nzlich festgebannt.", "tokens": ["Der\u00b7weil", "ich", "g\u00e4nz\u00b7lich", "fest\u00b7ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In deinem Zauberkreise bin.", "tokens": ["In", "dei\u00b7nem", "Zau\u00b7ber\u00b7krei\u00b7se", "bin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie zeigen mir den Himmelsweg", "tokens": ["Sie", "zei\u00b7gen", "mir", "den", "Him\u00b7mels\u00b7weg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und warnen mich vor falscher Bahn,", "tokens": ["Und", "war\u00b7nen", "mich", "vor", "fal\u00b7scher", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Derweilen ich zum Paradies", "tokens": ["Der\u00b7wei\u00b7len", "ich", "zum", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4ngst fertig mit der Reise bin.", "tokens": ["L\u00e4ngst", "fer\u00b7tig", "mit", "der", "Rei\u00b7se", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie preisen ihren Himmel hoch", "tokens": ["Sie", "prei\u00b7sen", "ih\u00b7ren", "Him\u00b7mel", "hoch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und machen viel Geschrei davon,", "tokens": ["Und", "ma\u00b7chen", "viel", "Ge\u00b7schrei", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Derweilen ich im h\u00f6chsten Gl\u00fcck", "tokens": ["Der\u00b7wei\u00b7len", "ich", "im", "h\u00f6chs\u00b7ten", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verschwiegen ganz und leise bin.", "tokens": ["Ver\u00b7schwie\u00b7gen", "ganz", "und", "lei\u00b7se", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Nachtigall ist S\u00fcnderin", "tokens": ["Die", "Nach\u00b7ti\u00b7ga