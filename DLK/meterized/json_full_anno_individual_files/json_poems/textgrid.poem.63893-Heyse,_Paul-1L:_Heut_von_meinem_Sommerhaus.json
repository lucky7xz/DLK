{"textgrid.poem.63893": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heut von meinem Sommerhaus", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut von meinem Sommerhaus", "tokens": ["Heut", "von", "mei\u00b7nem", "Som\u00b7mer\u00b7haus"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4gt mich \u00fcber Tal und H\u00fcgel", "tokens": ["Tr\u00e4gt", "mich", "\u00fc\u00b7ber", "Tal", "und", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dein Holstenhaus hinaus", "tokens": ["In", "dein", "Hols\u00b7ten\u00b7haus", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Phantasie auf raschem Fl\u00fcgel.", "tokens": ["Phan\u00b7ta\u00b7sie", "auf", "ra\u00b7schem", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "In dein Zimmer f\u00fchrt sie mich,", "tokens": ["In", "dein", "Zim\u00b7mer", "f\u00fchrt", "sie", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo vor kurzen Jahr' und Tagen", "tokens": ["Wo", "vor", "kur\u00b7zen", "Jahr'", "und", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wir am Fenster abendlich", "tokens": ["Wir", "am", "Fens\u00b7ter", "a\u00b7bend\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Trauter Wechselrede pflagen.", "tokens": ["Trau\u00b7ter", "Wech\u00b7sel\u00b7re\u00b7de", "pfla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Vor uns Feld und Waldesaun,", "tokens": ["Vor", "uns", "Feld", "und", "Wal\u00b7de\u00b7saun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Drauf des Herbstes Schimmer ruhte,", "tokens": ["Drauf", "des", "Herbs\u00b7tes", "Schim\u00b7mer", "ruh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df uns Alternden im Schaun", "tokens": ["Da\u00df", "uns", "Al\u00b7tern\u00b7den", "im", "Schaun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eichendorffisch ward zumute:", "tokens": ["Ei\u00b7chen\u00b7dorf\u00b7fisch", "ward", "zu\u00b7mu\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "$."], "meter": "+--++-+-", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Gleich als h\u00e4tten ausgespannt", "tokens": ["Gleich", "als", "h\u00e4t\u00b7ten", "aus\u00b7ge\u00b7spannt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Seelen weit die Schwingen,", "tokens": ["Uns\u00b7re", "See\u00b7len", "weit", "die", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcbers abendstille Land", "tokens": ["\u00dc\u00b7bers", "a\u00b7bend\u00b7stil\u00b7le", "Land"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Friedlich uns \u00bbnach Haus\u00ab zu bringen.", "tokens": ["Fried\u00b7lich", "uns", "\u00bb", "nach", "Haus", "\u00ab", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "$(", "APPR", "NN", "$(", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da auf einmal h\u00f6rt' ich dich", "tokens": ["Da", "auf", "ein\u00b7mal", "h\u00f6rt'", "ich", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADV", "VVFIN", "PPER", "PRF"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Halb wie zu dir selber sprechen:", "tokens": ["Halb", "wie", "zu", "dir", "sel\u00b7ber", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Herbst ist da. Es melden sich", "tokens": ["Herbst", "ist", "da", ".", "Es", "mel\u00b7den", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "$.", "PPER", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon die fr\u00f6stelnden Gebrechen.", "tokens": ["Schon", "die", "fr\u00f6s\u00b7teln\u00b7den", "Ge\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Fr\u00fchreif fiel mir auf das Haupt,", "tokens": ["Fr\u00fch\u00b7reif", "fiel", "mir", "auf", "das", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenig blieb mir noch des Holden;", "tokens": ["We\u00b7nig", "blieb", "mir", "noch", "des", "Hol\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch, solang man liebt und glaubt,", "tokens": ["Doch", ",", "so\u00b7lang", "man", "liebt", "und", "glaubt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll man sich den Tag vergolden. \u2013", "tokens": ["Soll", "man", "sich", "den", "Tag", "ver\u00b7gol\u00b7den", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PIS", "PRF", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sieh, da war dein junges Kind", "tokens": ["Sieh", ",", "da", "war", "dein", "jun\u00b7ges", "Kind"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns verstohlen nachgegangen,", "tokens": ["Uns", "ver\u00b7stoh\u00b7len", "nach\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hielt mit schlanken \u00c4rmchen lind", "tokens": ["Hielt", "mit", "schlan\u00b7ken", "\u00c4rm\u00b7chen", "lind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihres Vaters Hals umfangen.", "tokens": ["Ih\u00b7res", "Va\u00b7ters", "Hals", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und ich sprach: Wem frisch und rot", "tokens": ["Und", "ich", "sprach", ":", "Wem", "frisch", "und", "rot"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "PWS", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Solche Sommerfr\u00fcchte reifen,", "tokens": ["Sol\u00b7che", "Som\u00b7mer\u00b7fr\u00fcch\u00b7te", "rei\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem wird noch des Winters Not", "tokens": ["Dem", "wird", "noch", "des", "Win\u00b7ters", "Not"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nicht so bald ans Herze greifen,", "tokens": ["Nicht", "so", "bald", "ans", "Her\u00b7ze", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und er l\u00e4\u00dft die Siebzig nahn,", "tokens": ["Und", "er", "l\u00e4\u00dft", "die", "Sieb\u00b7zig", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "CARD", "ADJA", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht geb\u00fcckt auf die Postille:", "tokens": ["Nicht", "ge\u00b7b\u00fcckt", "auf", "die", "Pos\u00b7til\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aufrecht, wie wir stets ihn sahn,", "tokens": ["Auf\u00b7recht", ",", "wie", "wir", "stets", "ihn", "sahn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandelt er in Lebensf\u00fclle.", "tokens": ["Wan\u00b7delt", "er", "in", "Le\u00b7bens\u00b7f\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wie ein Fruchtbaum herbstbereift", "tokens": ["Wie", "ein", "Frucht\u00b7baum", "herbst\u00b7be\u00b7reift"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fcnt er auf des Lebens Gipfel,", "tokens": ["Gr\u00fcnt", "er", "auf", "des", "Le\u00b7bens", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Ernten manche reift", "tokens": ["Und", "der", "Ern\u00b7ten", "man\u00b7che", "reift"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonnig noch in seinem Wipfel. \u2013", "tokens": ["Son\u00b7nig", "noch", "in", "sei\u00b7nem", "Wip\u00b7fel", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wohl propheten\u00e4ugig sah", "tokens": ["Wohl", "pro\u00b7phe\u00b7te\u00b7n\u00e4u\u00b7gig", "sah"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Damals ich in Lebensweiten.", "tokens": ["Da\u00b7mals", "ich", "in", "Le\u00b7bens\u00b7wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh, nun sind die Siebzig da,", "tokens": ["Sieh", ",", "nun", "sind", "die", "Sieb\u00b7zig", "da", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VAFIN", "ART", "NE", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und du stehst noch wie vorzeiten.", "tokens": ["Und", "du", "stehst", "noch", "wie", "vor\u00b7zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "KOKOM", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Deiner Tage Kampf und Schmerz", "tokens": ["Dei\u00b7ner", "Ta\u00b7ge", "Kampf", "und", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast du mild verkl\u00e4rt im Singen,", "tokens": ["Hast", "du", "mild", "ver\u00b7kl\u00e4rt", "im", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ein rechtes Menschenherz,", "tokens": ["Denn", "ein", "rech\u00b7tes", "Men\u00b7schen\u00b7herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00dft du, ist nicht umzubringen.", "tokens": ["Wei\u00dft", "du", ",", "ist", "nicht", "um\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VAFIN", "PTKNEG", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Schenkst dem Volke Jahr um Jahr", "tokens": ["Schenkst", "dem", "Vol\u00b7ke", "Jahr", "um", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Goldner Fr\u00fcchte reichen Segen,", "tokens": ["Gold\u00b7ner", "Fr\u00fcch\u00b7te", "rei\u00b7chen", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem nun schon die Enkelschar", "tokens": ["Dem", "nun", "schon", "die", "En\u00b7kel\u00b7schar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleich den V\u00e4tern harrt entgegen.", "tokens": ["Gleich", "den", "V\u00e4\u00b7tern", "harrt", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Und so woll'n wir's, alter Freund,", "tokens": ["Und", "so", "woll'n", "wir's", ",", "al\u00b7ter", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIS", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch ein Weilchen weitertreiben,", "tokens": ["Noch", "ein", "Weil\u00b7chen", "wei\u00b7ter\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn der Herbst das Laub auch br\u00e4unt,", "tokens": ["Wenn", "der", "Herbst", "das", "Laub", "auch", "br\u00e4unt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eingedenk des Sommers bleiben.", "tokens": ["Ein\u00b7ge\u00b7denk", "des", "Som\u00b7mers", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "W\u00e4hrend auf Parnassesh\u00f6hn", "tokens": ["W\u00e4h\u00b7rend", "auf", "Par\u00b7nas\u00b7ses\u00b7h\u00f6hn"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aberwitz'ge Knaben l\u00e4rmen:", "tokens": ["A\u00b7ber\u00b7witz'\u00b7ge", "Kna\u00b7ben", "l\u00e4r\u00b7men", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbsch\u00f6n ist h\u00e4\u00dflich, H\u00e4\u00dflich sch\u00f6n!\u00ab", "tokens": ["\u00bb", "sch\u00f