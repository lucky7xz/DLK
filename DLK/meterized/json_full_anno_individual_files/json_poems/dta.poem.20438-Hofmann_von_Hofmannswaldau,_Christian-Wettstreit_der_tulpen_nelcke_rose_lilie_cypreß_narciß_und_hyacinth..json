{"dta.poem.20438": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Wettstreit der tulpen/ nelcke/ rose/  \n lilie/ cypre\u00df/ narci\u00df und  \n hyacinth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch irrlicht/ th\u00f6richte! f\u00fchrt euch auff diesen wahn/", "tokens": ["Welch", "irr\u00b7licht", "/", "th\u00f6\u00b7rich\u00b7te", "!", "f\u00fchrt", "euch", "auff", "die\u00b7sen", "wahn", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "VVFIN", "$.", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$("], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df ihr um vorder recht aus \u00fcbermuth wolt gleichen?", "tokens": ["Da\u00df", "ihr", "um", "vor\u00b7der", "recht", "aus", "\u00fc\u00b7ber\u00b7muth", "wolt", "glei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "ADJD", "APPR", "NE", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es darff kein Paris euch den g\u00f6ldnen apffel reichen/", "tokens": ["Es", "darff", "kein", "Pa\u00b7ris", "euch", "den", "g\u00f6ld\u00b7nen", "apf\u00b7fel", "rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NE", "PPER", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn alle welt beut mir ja kron und inful an.", "tokens": ["Denn", "al\u00b7le", "welt", "beut", "mir", "ja", "kron", "und", "in\u00b7ful", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "ADV", "NN", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die muschel tochter r\u00e4umt f\u00fcr meinen schnee den plan:", "tokens": ["Die", "mu\u00b7schel", "toch\u00b7ter", "r\u00e4umt", "f\u00fcr", "mei\u00b7nen", "schnee", "den", "plan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des purpur schneckens blut mu\u00df meiner bl\u00fcte weichen/", "tokens": ["Des", "pur\u00b7pur", "schne\u00b7ckens", "blut", "mu\u00df", "mei\u00b7ner", "bl\u00fc\u00b7te", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VMFIN", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und f\u00fcr den z\u00e4serlein das sch\u00f6nste gold erbleichen.", "tokens": ["Und", "f\u00fcr", "den", "z\u00e4\u00b7ser\u00b7lein", "das", "sch\u00f6ns\u00b7te", "gold", "er\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kein k\u00f6stlich balsam ie so lieblich riechen kan.", "tokens": ["Kein", "k\u00f6st\u00b7lich", "bal\u00b7sam", "ie", "so", "lieb\u00b7lich", "rie\u00b7chen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "ADJD", "ADV", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Durch mich wird der Galen noch nicht so gro\u00df geacht;", "tokens": ["Durch", "mich", "wird", "der", "Ga\u00b7len", "noch", "nicht", "so", "gro\u00df", "ge\u00b7acht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ART", "NN", "ADV", "PTKNEG", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Die ewigkeit hat mich l\u00e4ngst in ihr buch geschrieben/", "tokens": ["Die", "e\u00b7wig\u00b7keit", "hat", "mich", "l\u00e4ngst", "in", "ihr", "buch", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Weiln ieden monat ich verneue meine pracht.", "tokens": ["Weiln", "ie\u00b7den", "mo\u00b7nat", "ich", "ver\u00b7neu\u00b7e", "mei\u00b7ne", "pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Darein die Nymphen mit den G\u00f6ttern sich verlieben/", "tokens": ["Da\u00b7rein", "die", "Nym\u00b7phen", "mit", "den", "G\u00f6t\u00b7tern", "sich", "ver\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "ART", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zu dem/ so spricht auch euch noch die geburt den hohn/", "tokens": ["Zu", "dem", "/", "so", "spricht", "auch", "euch", "noch", "die", "ge\u00b7burt", "den", "hohn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "ADV", "VVFIN", "ADV", "PPER", "ADV", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Von Cypris kam der thron/ der scepter von Adon.", "tokens": ["Von", "Cyp\u00b7ris", "kam", "der", "thron", "/", "der", "scep\u00b7ter", "von", "A\u00b7don", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "$(", "ART", "ADJA", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}