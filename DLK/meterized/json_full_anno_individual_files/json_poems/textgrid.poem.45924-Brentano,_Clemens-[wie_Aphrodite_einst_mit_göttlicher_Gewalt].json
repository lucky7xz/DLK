{"textgrid.poem.45924": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[wie Aphrodite einst mit g\u00f6ttlicher Gewalt]", "genre": "verse", "period": "N.A.", "pub_year": 1808, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Aphrodite einst mit g\u00f6ttlicher Gewalt", "tokens": ["Wie", "A\u00b7phro\u00b7di\u00b7te", "einst", "mit", "g\u00f6tt\u00b7li\u00b7cher", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Galatheas kalten Marmorbusen", "tokens": ["In", "Ga\u00b7la\u00b7the\u00b7as", "kal\u00b7ten", "Mar\u00b7mor\u00b7bu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Lebens holde Flamme senkte, und", "tokens": ["Des", "Le\u00b7bens", "hol\u00b7de", "Flam\u00b7me", "senk\u00b7te", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Liebliche das Licht des Tages gr\u00fc\u00dfte;", "tokens": ["Die", "Lieb\u00b7li\u00b7che", "das", "Licht", "des", "Ta\u00b7ges", "gr\u00fc\u00df\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So wei\u00dft ", "tokens": ["So", "wei\u00dft"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Vom ird'schen, engen, uns empor zu heben.", "tokens": ["Vom", "ird'\u00b7schen", ",", "en\u00b7gen", ",", "uns", "em\u00b7por", "zu", "he\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "$,", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Gegenwart, die \u00e4ngstliche, entflieht,", "tokens": ["Die", "Ge\u00b7gen\u00b7wart", ",", "die", "\u00e4ngst\u00b7li\u00b7che", ",", "ent\u00b7flieht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Von der Vergangenheit seh' ich den Schleier fallen,", "tokens": ["Von", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "seh'", "ich", "den", "Schlei\u00b7er", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Die heil'ge Vorwelt zeigt sich mir enth\u00fcllt,", "tokens": ["Die", "heil'\u00b7ge", "Vor\u00b7welt", "zeigt", "sich", "mir", "ent\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und magische Gestalten seh' ich wallen,", "tokens": ["Und", "ma\u00b7gi\u00b7sche", "Ge\u00b7stal\u00b7ten", "seh'", "ich", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wie Isis ernst, und Psyche sch\u00f6n und mild. \u2013", "tokens": ["Wie", "I\u00b7sis", "ernst", ",", "und", "Psy\u00b7che", "sch\u00f6n", "und", "mild", ".", "\u2