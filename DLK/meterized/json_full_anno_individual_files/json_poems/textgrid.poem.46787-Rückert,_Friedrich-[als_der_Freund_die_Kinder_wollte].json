", "tokens": ["Ei\u00b7ne", "Ta\u00b7greis'", "hin", "gen", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist's geschenkt Gro\u00dfmutter worden,", "tokens": ["Ist's", "ge\u00b7schenkt", "Gro\u00df\u00b7mut\u00b7ter", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die so sehr den Enkel liebt,", "tokens": ["Die", "so", "sehr", "den", "En\u00b7kel", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie nie das Bild hergiebt.", "tokens": ["Da\u00df", "sie", "nie", "das", "Bild", "her\u00b7giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Und sie m\u00f6g' es nie auch geben,", "tokens": ["Und", "sie", "m\u00f6g'", "es", "nie", "auch", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sondern mir bleib' er am Leben,", "tokens": ["Son\u00b7dern", "mir", "bleib'", "er", "am", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Weil ich, wenn er wollt' entfliehn,", "tokens": ["Weil", "ich", ",", "wenn", "er", "wollt'", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "punct"