{"textgrid.poem.39032": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vom Wege verirrt,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Wege verirrt,", "tokens": ["Vom", "We\u00b7ge", "ver\u00b7irrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Vom Sturm bedr\u00e4ngt,", "tokens": ["Vom", "Sturm", "be\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Vom Regen durchn\u00e4\u00dft,", "tokens": ["Vom", "Re\u00b7gen", "durc\u00b7hn\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Such ich hier Schutz", "tokens": ["Such", "ich", "hier", "Schutz"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPER", "ADV", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In dieser l\u00e4ndlichen Behausung.", "tokens": ["In", "die\u00b7ser", "l\u00e4nd\u00b7li\u00b7chen", "Be\u00b7hau\u00b7sung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Man versorgt Diener und Pferde,", "tokens": ["Man", "ver\u00b7sorgt", "Die\u00b7ner", "und", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Erquickt den M\u00fcden", "tokens": ["Er\u00b7quickt", "den", "M\u00fc\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["V