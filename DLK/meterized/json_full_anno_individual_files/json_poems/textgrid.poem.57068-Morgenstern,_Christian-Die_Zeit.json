{"textgrid.poem.57068": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gibt ein sehr probates Mittel,", "tokens": ["Es", "gibt", "ein", "sehr", "pro\u00b7ba\u00b7tes", "Mit\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die Zeit zu halten am Schlawittel:", "tokens": ["die", "Zeit", "zu", "hal\u00b7ten", "am", "Schla\u00b7wit\u00b7tel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Man nimmt die Taschenuhr zur Hand", "tokens": ["Man", "nimmt", "die", "Ta\u00b7sche\u00b7nuhr", "zur", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und folgt dem Zeiger unverwandt,", "tokens": ["und", "folgt", "dem", "Zei\u00b7ger", "un\u00b7ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie geht so langsam dann, so brav", "tokens": ["Sie", "geht", "so", "lang\u00b7sam", "dann", ",", "so", "brav"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ADV", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "als wie ein wohlgezogen Schaf,", "tokens": ["als", "wie", "ein", "wohl\u00b7g