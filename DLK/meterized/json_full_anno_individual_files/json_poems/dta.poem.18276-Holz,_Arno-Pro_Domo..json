{"dta.poem.18276": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Pro Domo.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Die Juden schachern und die Pfaffen schwindeln!", "tokens": ["Die", "Ju\u00b7den", "scha\u00b7chern", "und", "die", "Pfaf\u00b7fen", "schwin\u00b7deln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Freund erschie\u00dft man im Duell", "tokens": ["Den", "Freund", "er\u00b7schie\u00dft", "man", "im", "Du\u00b7ell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sucht die Liebe im Bordell.", "tokens": ["Und", "sucht", "die", "Lie\u00b7be", "im", "Bor\u00b7dell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die deutsche Sprache wird gef\u00e4lscht,", "tokens": ["Die", "deut\u00b7sche", "Spra\u00b7che", "wird", "ge\u00b7f\u00e4lscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gekauder- und solongew\u00e4lscht", "tokens": ["Ge\u00b7kau\u00b7der", "und", "so\u00b7lon\u00b7ge\u00b7w\u00e4lscht"], "token_info": ["word", "word", "word"], "pos": ["TRUNC", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und w\u00e4ssrig thront auf dem Parna\u00df", "tokens": ["Und", "w\u00e4ss\u00b7rig", "thront", "auf", "dem", "Par\u00b7na\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Drum schimpft nur weidlich: \u201ePamphletist\u201c,", "tokens": ["Drum", "schimpft", "nur", "weid\u00b7lich", ":", "\u201e", "Pam\u00b7ph\u00b7le\u00b7tist", "\u201c", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ADJD", "$.", "$(", "NN", "$(", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ich bin nur Stimmungspessimist!", "tokens": ["Ich", "bin", "nur", "Stim\u00b7mungs\u00b7pes\u00b7si\u00b7mist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}