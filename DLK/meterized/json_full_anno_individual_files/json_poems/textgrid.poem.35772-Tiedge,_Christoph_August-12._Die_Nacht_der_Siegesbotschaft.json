 ["NN", "ADJD", "$.", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wer heilig treu am Glauben hielt, der hebe,", "tokens": ["Wer", "hei\u00b7lig", "treu", "am", "Glau\u00b7ben", "hielt", ",", "der", "he\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADJD", "APPRART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit mir empor sein freies Haupt, und lebe!", "tokens": ["Mit", "mir", "em\u00b7por", "sein", "frei\u00b7es", "Haupt", ",", "und", "le\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "PPOSAT", "ADJA", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Das Leben war, wie ausgel\u00f6schte Gluten,", "tokens": ["Das", "Le\u00b7ben", "war", ",", "wie", "aus\u00b7ge\u00b7l\u00f6schte", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie ausgesto\u00dfen aus dem Sonnenraum,", "tokens": ["Wie", "aus\u00b7ge\u00b7sto\u00b7\u00dfen", "aus", "dem", "Son\u00b7nen\u00b7raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hinabgesunken in des Orkus Fluten,", "tokens": ["Hin\u00b7ab\u00b7ge\u00b7sun\u00b7ken", "in", "des", "Or\u00b7kus", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Un