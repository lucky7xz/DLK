{"textgrid.poem.35772": {"metadata": {"author": {"name": "Tiedge, Christoph August", "birth": "N.A.", "death": "N.A."}, "title": "12. Die Nacht der Siegesbotschaft", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erhelle dich, du meine dunkle Halle,", "tokens": ["Er\u00b7hel\u00b7le", "dich", ",", "du", "mei\u00b7ne", "dunk\u00b7le", "Hal\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erf\u00fclle dich mit Siegesherrlichkeit!", "tokens": ["Er\u00b7f\u00fcl\u00b7le", "dich", "mit", "Sie\u00b7ges\u00b7herr\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Triumphgesang! ein Welttriumph erschalle!", "tokens": ["Tri\u00b7umph\u00b7ge\u00b7sang", "!", "ein", "Welt\u00b7tri\u00b7umph", "er\u00b7schal\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verk\u00fcnde laut: erf\u00fcllet ist die Zeit.", "tokens": ["Ver\u00b7k\u00fcn\u00b7de", "laut", ":", "er\u00b7f\u00fcl\u00b7let", "ist", "die", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$.", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wer heilig treu am Glauben hielt, der hebe,", "tokens": ["Wer", "hei\u00b7lig", "treu", "am", "Glau\u00b7ben", "hielt", ",", "der", "he\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADJD", "APPRART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit mir empor sein freies Haupt, und lebe!", "tokens": ["Mit", "mir", "em\u00b7por", "sein", "frei\u00b7es", "Haupt", ",", "und", "le\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "PPOSAT", "ADJA", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Leben war, wie ausgel\u00f6schte Gluten,", "tokens": ["Das", "Le\u00b7ben", "war", ",", "wie", "aus\u00b7ge\u00b7l\u00f6schte", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie ausgesto\u00dfen aus dem Sonnenraum,", "tokens": ["Wie", "aus\u00b7ge\u00b7sto\u00b7\u00dfen", "aus", "dem", "Son\u00b7nen\u00b7raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hinabgesunken in des Orkus Fluten,", "tokens": ["Hin\u00b7ab\u00b7ge\u00b7sun\u00b7ken", "in", "des", "Or\u00b7kus", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und oben schwamm des Daseins \u00f6der Traum.", "tokens": ["Und", "o\u00b7ben", "schwamm", "des", "Da\u00b7seins", "\u00f6\u00b7der", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Todes Stachel ist hinweggerissen,", "tokens": ["Des", "To\u00b7des", "Sta\u00b7chel", "ist", "hin\u00b7weg\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der H\u00f6lle Sieg bedeckt mit Finsternissen!", "tokens": ["Der", "H\u00f6l\u00b7le", "Sieg", "be\u00b7deckt", "mit", "Fins\u00b7ter\u00b7nis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Vaterland ist ledig seiner Ketten;", "tokens": ["Das", "Va\u00b7ter\u00b7land", "ist", "le\u00b7dig", "sei\u00b7ner", "Ket\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wahrheit darf sich ihrem Altar nah'n;", "tokens": ["Die", "Wahr\u00b7heit", "darf", "sich", "ih\u00b7rem", "Al\u00b7tar", "nah'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Recht ist frei, die Seelen zu erretten,", "tokens": ["Das", "Recht", "ist", "frei", ",", "die", "See\u00b7len", "zu", "er\u00b7ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die tief verzweifelnd seinen Stern nicht sah'n.", "tokens": ["Die", "tief", "ver\u00b7zwei\u00b7felnd", "sei\u00b7nen", "Stern", "nicht", "sah'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJD", "VVPP", "PPOSAT", "NN", "PTKNEG", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den hehren Stern, der Gottes Reich verk\u00fcndet,", "tokens": ["Den", "heh\u00b7ren", "Stern", ",", "der", "Got\u00b7tes", "Reich", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und jede Brust, die an ihn glaubt, entz\u00fcndet.", "tokens": ["Und", "je\u00b7de", "Brust", ",", "die", "an", "ihn", "glaubt", ",", "ent\u00b7z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bist du es, Wahrheit, die mich aus der langen,", "tokens": ["Bist", "du", "es", ",", "Wahr\u00b7heit", ",", "die", "mich", "aus", "der", "lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "NN", "$,", "PRELS", "PRF", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verstummten Nacht in diese Strahlen hub?", "tokens": ["Ver\u00b7stumm\u00b7ten", "Nacht", "in", "die\u00b7se", "Strah\u00b7len", "hub", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist wirklich abgewischt von meinen Wangen", "tokens": ["Ist", "wirk\u00b7lich", "ab\u00b7ge\u00b7wischt", "von", "mei\u00b7nen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Thr\u00e4ne, die so tiefe Furchen grub?", "tokens": ["Die", "Thr\u00e4\u00b7ne", ",", "die", "so", "tie\u00b7fe", "Fur\u00b7chen", "grub", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Darf sich das Herz dies Hochgef\u00fchl erlauben?", "tokens": ["Darf", "sich", "das", "Herz", "dies", "Hoch\u00b7ge\u00b7f\u00fchl", "er\u00b7lau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "PDS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich zitt're noch, kaum glaub' ich meinem Glauben.", "tokens": ["Ich", "zitt'\u00b7re", "noch", ",", "kaum", "glaub'", "ich", "mei\u00b7nem", "Glau\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Noch lag vor meinem Blick ein dunkles Walten,", "tokens": ["Noch", "lag", "vor", "mei\u00b7nem", "Blick", "ein", "dunk\u00b7les", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von halb verh\u00fcllten Sternen still umkreist;", "tokens": ["Von", "halb", "ver\u00b7h\u00fcll\u00b7ten", "Ster\u00b7nen", "still", "um\u00b7kreist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und von der Kraft des Glaubens fest gehalten,", "tokens": ["Und", "von", "der", "Kraft", "des", "Glau\u00b7bens", "fest", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Besuchte voll Erinnerung mein Geist", "tokens": ["Be\u00b7such\u00b7te", "voll", "E\u00b7rin\u00b7ne\u00b7rung", "mein", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den Tempelraum, wo meine G\u00f6tter schwanden,", "tokens": ["Den", "Tem\u00b7pel\u00b7raum", ",", "wo", "mei\u00b7ne", "G\u00f6t\u00b7ter", "schwan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Stelle, wo mein Vaterland gestanden.", "tokens": ["Die", "Stel\u00b7le", ",", "wo", "mein", "Va\u00b7ter\u00b7land", "ge\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Es war der Mitternacht geweihte Stunde;", "tokens": ["Es", "war", "der", "Mit\u00b7ter\u00b7nacht", "ge\u00b7weih\u00b7te", "Stun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie hatte heilig schweigend sich geweiht:", "tokens": ["Sie", "hat\u00b7te", "hei\u00b7lig", "schwei\u00b7gend", "sich", "ge\u00b7weiht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da kam, wie Lichtaufgang, die frohe Kunde", "tokens": ["Da", "kam", ",", "wie", "Lich\u00b7tauf\u00b7gang", ",", "die", "fro\u00b7he", "Kun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des gro\u00dfen Siegs, in meine Dunkelheit,", "tokens": ["Des", "gro\u00b7\u00dfen", "Siegs", ",", "in", "mei\u00b7ne", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So festlich hell, wie Engel sich gestalten;", "tokens": ["So", "fest\u00b7lich", "hell", ",", "wie", "En\u00b7gel", "sich", "ge\u00b7stal\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,", "PWAV", "NE", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie sprach zu mir: Dein Glaub' hat Wort gehalten.", "tokens": ["Sie", "sprach", "zu", "mir", ":", "Dein", "Glaub'", "hat", "Wort", "ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$.", "PPOSAT", "NN", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Triumph! der Sieg des Rechtes ist errungen.", "tokens": ["Tri\u00b7umph", "!", "der", "Sieg", "des", "Rech\u00b7tes", "ist", "er\u00b7run\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr V\u00f6lker nichts, o nichts mehr von Verlust!", "tokens": ["Ihr", "V\u00f6l\u00b7ker", "nichts", ",", "o", "nichts", "mehr", "von", "Ver\u00b7lust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIS", "$,", "FM", "PIS", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Werft hinter euch die Schmacherinnerungen,", "tokens": ["Werft", "hin\u00b7ter", "euch", "die", "Schma\u00b7che\u00b7rin\u00b7ne\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Gottgef\u00fchl erf\u00fclle jede Brust!", "tokens": ["Ein", "Gott\u00b7ge\u00b7f\u00fchl", "er\u00b7f\u00fcl\u00b7le", "je\u00b7de", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "F\u00fchlt, was die Zeit in ihrem Scho\u00df bewahrte,", "tokens": ["F\u00fchlt", ",", "was", "die", "Zeit", "in", "ih\u00b7rem", "Scho\u00df", "be\u00b7wahr\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie gro\u00df und herrlich Gott sich offenbarte.", "tokens": ["Wie", "gro\u00df", "und", "herr\u00b7lich", "Gott", "sich", "of\u00b7fen\u00b7bar\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Sei jedes Thal zu einem Gotteshause,", "tokens": ["Sei", "je\u00b7des", "Thal", "zu", "ei\u00b7nem", "Got\u00b7tes\u00b7hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Altar jeder H\u00fcgel eingeweiht!", "tokens": ["Zum", "Al\u00b7tar", "je\u00b7der", "H\u00fc\u00b7gel", "ein\u00b7ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom Aufgang bis zum Niedergange brause", "tokens": ["Vom", "Auf\u00b7gang", "bis", "zum", "Nie\u00b7der\u00b7gan\u00b7ge", "brau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Lobgesang: Erf\u00fcllet ist die Zeit!", "tokens": ["Der", "Lob\u00b7ge\u00b7sang", ":", "Er\u00b7f\u00fcl\u00b7let", "ist", "die", "Zeit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Feind des Rechts, des Friedens ist vernichtet,", "tokens": ["Der", "Feind", "des", "Rechts", ",", "des", "Frie\u00b7dens", "ist", "ver\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nicht Menschenweish