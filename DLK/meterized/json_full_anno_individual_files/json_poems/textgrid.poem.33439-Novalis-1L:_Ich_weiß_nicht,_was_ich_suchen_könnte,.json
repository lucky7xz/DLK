{"textgrid.poem.33439": {"metadata": {"author": {"name": "Novalis", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wei\u00df nicht, was ich suchen k\u00f6nnte,", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, was ich suchen k\u00f6nnte,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "ich", "su\u00b7chen", "k\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00e4r jenes liebe Wesen mein,", "tokens": ["W\u00e4r", "je\u00b7nes", "lie\u00b7be", "We\u00b7sen", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn er mich seine Freude nennte,", "tokens": ["Wenn", "er", "mich", "sei\u00b7ne", "Freu\u00b7de", "nenn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bei mir w\u00e4r', als w\u00e4r' ich sein.", "tokens": ["Und", "bei", "mir", "w\u00e4r'", ",", "als", "w\u00e4r'", "ich", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So Viele gehn umher und suchen", "tokens": ["So", "Vie\u00b7le", "gehn", "um\u00b7her", "und", "su\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "PTKVZ", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit wild verzerrtem Angesicht,", "tokens": ["Mit", "wild", "ver\u00b7zerr\u00b7tem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie hei\u00dfen immer sich die Klugen,", "tokens": ["Sie", "hei\u00b7\u00dfen", "im\u00b7mer", "sich", "die", "Klu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und kennen diesen Schatz doch nicht.", "tokens": ["Und", "ken\u00b7nen", "die\u00b7sen", "Schatz", "doch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Eine denkt, er hat's ergriffen,", "tokens": ["Der", "Ei\u00b7ne", "denkt", ",", "er", "hat's", "er\u00b7grif\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und was er hat, ist nichts als Gold;", "tokens": ["Und", "was", "er", "hat", ",", "ist", "nichts", "als", "Gold", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VAFIN", "$,", "VAFIN", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der will die ganze Welt umschiffen,", "tokens": ["Der", "will", "die", "gan\u00b7ze", "Welt", "um\u00b7schif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts als ein Nahme wird sein Sold.", "tokens": ["Nichts", "als", "ein", "Nah\u00b7me", "wird", "sein", "Sold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der l\u00e4uft nach einem Siegerkranze", "tokens": ["Der", "l\u00e4uft", "nach", "ei\u00b7nem", "Sie\u00b7ger\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Der nach einem Lorbeerzweig,", "tokens": ["Und", "Der", "nach", "ei\u00b7nem", "Lor\u00b7beer\u00b7zweig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und so wird von verschiednem Glanze", "tokens": ["Und", "so", "wird", "von", "ver\u00b7schied\u00b7nem", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Get\u00e4uscht ein jeder, keiner reich.", "tokens": ["Ge\u00b7t\u00e4uscht", "ein", "je\u00b7der", ",", "kei\u00b7ner", "reich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "PIS", "$,", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hat er sich euch nicht kund gegeben?", "tokens": ["Hat", "er", "sich", "euch", "nicht", "kund", "ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "PPER", "PTKNEG", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verga\u00dft ihr, wer f\u00fcr euch erblich?", "tokens": ["Ver\u00b7ga\u00dft", "ihr", ",", "wer", "f\u00fcr", "euch", "er\u00b7blich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer uns zu Lieb' aus diesem Leben", "tokens": ["Wer", "uns", "zu", "Lieb'", "aus", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In bittrer Qual verachtet wich?", "tokens": ["In", "bit\u00b7trer", "Qual", "ver\u00b7ach\u00b7tet", "wich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Habt ihr von ihm denn nichts gelesen,", "tokens": ["Habt", "ihr", "von", "ihm", "denn", "nichts", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein armes Wort von ihm geh\u00f6rt?", "tokens": ["Kein", "ar\u00b7mes", "Wort", "von", "ihm", "ge\u00b7h\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie himmlisch gut er uns gewesen,", "tokens": ["Wie", "himm\u00b7lisch", "gut", "er", "uns", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "PPER", "PPER", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und welches Gut er uns bescheert?", "tokens": ["Und", "wel\u00b7ches", "Gut", "er", "uns", "be\u00b7scheert", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie er vom Himmel hergekommen,", "tokens": ["Wie", "er", "vom", "Him\u00b7mel", "her\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der sch\u00f6nsten Mutter hohes Kind?", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "Mut\u00b7ter", "ho\u00b7hes", "Kind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Welch Wort die Welt von ihm vernommen,", "tokens": ["Welch", "Wort", "die", "Welt", "von", "ihm", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie viel durch ihn genesen sind?", "tokens": ["Wie", "viel", "durch", "ihn", "ge\u00b7ne\u00b7sen", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie er von Liebe nur beweget", "tokens": ["Wie", "er", "von", "Lie\u00b7be", "nur", "be\u00b7we\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich ganz uns hingegeben hat,", "tokens": ["Sich", "ganz", "uns", "hin\u00b7ge\u00b7ge\u00b7ben", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in die Erde sich geleget", "tokens": ["Und", "in", "die", "Er\u00b7de", "sich", "ge\u00b7le\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Grundstein einer Gottesstaat?", "tokens": ["Zum", "Grund\u00b7stein", "ei\u00b7ner", "Got\u00b7tes\u00b7staat", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Kann diese Bothschaft euch nicht r\u00fchren,", "tokens": ["Kann", "die\u00b7se", "Both\u00b7schaft", "euch", "nicht", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist so ein Mensch euch nicht genug,", "tokens": ["Ist", "so", "ein", "Mensch", "euch", "nicht", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00f6ffnet ihr nicht eure Th\u00fcren", "tokens": ["Und", "\u00f6ff\u00b7net", "ihr", "nicht", "eu\u00b7re", "Th\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem, der den Abgrund zu euch schlug?", "tokens": ["Dem", ",", "der", "den", "Ab\u00b7grund", "zu", "euch", "schlug", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "La\u00dft ihr nicht alles willig fahren,", "tokens": ["La\u00dft", "ihr", "nicht", "al\u00b7les", "wil\u00b7lig", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Thut gern auf jeden Wunsch Verzicht,", "tokens": ["Thut", "gern", "auf", "je\u00b7den", "Wunsch", "Ver\u00b7zicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wollt euer Herz nur ihm bewahren,", "tokens": ["Wollt", "eu\u00b7er", "Herz", "nur", "ihm", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn er euch seine Huld verspricht?", "tokens": ["Wenn", "er", "euch", "sei\u00b7ne", "Huld", "ver\u00b7spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Nimm du mich hin, du Held der Liebe", "tokens": ["Nimm", "du", "mich", "hin", ",", "du", "Held", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PRF", "PTKVZ", "$,", "PPER", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist mein Leben, meine Welt,", "tokens": ["Du", "bist", "mein", "Le\u00b7ben", ",", "mei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn nichts vom Irdischen mir bliebe,", "tokens": ["Wenn", "nichts", "vom", "Ir\u00b7di\u00b7schen", "mir", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wei\u00df ich, wer mich schadlos h\u00e4lt.", "tokens": ["So", "wei\u00df", "ich", ",", "wer", "mich", "schad\u00b7los", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Du giebst mir meine Lieben wieder,", "tokens": ["Du", "giebst", "mir", "mei\u00b7ne", "Lie\u00b7ben", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du bleibst in Ewigkeit mir treu,", "tokens": ["Du", "bleibst", "in", "E\u00b7wig\u00b7keit", "mir", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Anbetend sinkt der Himmel nieder,", "tokens": ["An\u00b7be\u00b7tend", "sinkt", "der", "Him\u00b7mel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und dennoch wohnest du mir bei.", "tokens": ["Und", "den\u00b7noch", "woh\u00b7nest", "du", "mir", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Ich wei\u00df nicht, was ich suchen k\u00f6nnte,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "ich", "su\u00b7chen", "k\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00e4r jenes liebe Wesen mein,", "tokens": ["W\u00e4r", "je\u00b7nes", "lie\u00b7be", "We\u00b7sen", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn er mich seine Freude nennte,", "tokens": ["Wenn", "er", "mich", "sei\u00b7ne", "Freu\u00b7de", "nenn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bei mir w\u00e4r', als w\u00e4r' ich sein.", "tokens": ["Und", "bei", "mir", "w\u00e4r'", ",", "als", "w\u00e4r'", "ich", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "So Viele gehn umher und suchen", "tokens": ["So", "Vie\u00b7le", "gehn", "um\u00b7her", "und", "su\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "PTKVZ", "KON", "VVINF"]