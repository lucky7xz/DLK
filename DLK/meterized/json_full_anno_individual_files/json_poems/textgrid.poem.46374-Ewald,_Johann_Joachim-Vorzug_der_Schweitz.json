{"textgrid.poem.46374": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Vorzug der Schweitz", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein junger Z\u00fcrcher kam", "tokens": ["Ein", "jun\u00b7ger", "Z\u00fcr\u00b7cher", "kam"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Voll Stolz und Politik j\u00fcngsthin nach Amsterdam,", "tokens": ["Voll", "Stolz", "und", "Po\u00b7li\u00b7tik", "j\u00fcng\u00b7sthin", "nach", "A\u00b7mster\u00b7dam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sahe K\u00e4se zubereiten.", "tokens": ["Und", "sa\u00b7he", "K\u00e4\u00b7se", "zu\u00b7be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er kostet' ihn mit eklem Zahn und rief: was will man lange streiten,", "tokens": ["Er", "kos\u00b7tet'", "ihn", "mit", "ek\u00b7lem", "Zahn", "und", "rief", ":", "was", "will", "man", "lan\u00b7ge", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "KON", "VVFIN", "$.", "PWS", "VMFIN", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.5": {"text": "Da\u00df unser Land an Witz und Wissenschaft nicht jedes Land besiege!", "tokens": ["Da\u00df", "un\u00b7ser", "Land", "an", "Witz", "und", "Wis\u00b7sen\u00b7schaft",