{"textgrid.poem.39217": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bringt Wein mir her, rotleuchtenden Wein,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bringt Wein mir her, rotleuchtenden Wein,", "tokens": ["Bringt", "Wein", "mir", "her", ",", "rot\u00b7leuch\u00b7ten\u00b7den", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Stimmt an die weichste Musik!", "tokens": ["Stimmt", "an", "die", "weichs\u00b7te", "Mu\u00b7sik", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mein tr\u00e4ges Herze will trunken sein,", "tokens": ["Mein", "tr\u00e4\u00b7ges", "Her\u00b7ze", "will", "trun\u00b7ken", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Denn es denkt nicht gerne zur\u00fcck,", "tokens": ["Denn", "es", "denkt", "nicht", "ger\u00b7ne", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Nicht gerne zur\u00fcck an den besseren Tag,", "tokens": ["Nicht", "ger\u00b7ne", "zu\u00b7r\u00fcck", "an", "den", "bes\u00b7se\u00b7ren", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "An das frische Blut, an den volleren Schlag,", "tokens": ["An", "das", "fri\u00b7sche", "Blut", ",", "an", "den", "vol\u00b7le\u00b7ren", "Schlag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Nicht gerne zur\u00fcck, nein, nein!", "tokens": ["Nicht", "ger\u00b7ne", "zu\u00b7r\u00fcck", ",", "nein", ",", "nein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKVZ", "$,", "PTKANT", "$,", "PTKANT", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Bringt Wein!", "tokens": ["Bringt", "Wein", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Mein sch\u00f6nes Lieb, schneebusiger Schwan,", "tokens": ["Mein", "sch\u00f6\u00b7nes", "Lieb", ",", "schnee\u00b7bu\u00b7si\u00b7ger", "Schwan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "O k\u00fcsse mich stets aufs neu',", "tokens": ["O", "k\u00fcs\u00b7se", "mich", "stets", "aufs", "neu'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df ich alles, was Du mir angetan,", "tokens": ["Da\u00df", "ich", "al\u00b7les", ",", "was", "Du", "mir", "an\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "$,", "PWS", "PPER", "PPER", "VVPP", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Vergesse und selig sei,", "tokens": ["Ver\u00b7ges\u00b7se", "und", "se\u00b7lig", "sei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Vergesse, da\u00df ich einmal war jung,", "tokens": ["Ver\u00b7ges\u00b7se", ",", "da\u00df", "ich", "ein\u00b7mal", "war", "jung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+--++--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Voll Tat und frischer Begeisterung;", "tokens": ["Voll", "Tat", "und", "fri\u00b7scher", "Be\u00b7geis\u00b7te\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Gib lodernde K\u00fcsse, mein Lieb!", "tokens": ["Gib", "lo\u00b7dern\u00b7de", "K\u00fcs\u00b7se", ",", "mein", "Lieb", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "O! Gib!", "tokens": ["O", "!", "Gib", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Auf Blumen und Seide la\u00dft mich ruhn!", "tokens": ["Auf", "Blu\u00b7men", "und", "Sei\u00b7de", "la\u00dft", "mich", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bringt Wein und Musik stimmt a