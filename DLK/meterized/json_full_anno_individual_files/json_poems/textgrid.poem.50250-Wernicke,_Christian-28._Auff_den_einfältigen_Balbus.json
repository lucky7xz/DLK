{"textgrid.poem.50250": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "28. Auff den einf\u00e4ltigen Balbus", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der gute ", "tokens": ["Der", "gu\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Dass auch ein hesslich Weib den Mann zum Hahnrey macht;", "tokens": ["Dass", "auch", "ein", "hess\u00b7lich", "Weib", "den", "Mann", "zum", "Hahn\u00b7rey", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJD", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Hauss und Hoff, weil er dem Herrn die Wahrheit saget;", "tokens": ["Von", "Hauss", "und", "Hoff", ",", "weil", "er", "dem", "Herrn", "die", "Wahr\u00b7heit", "sa\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von Gott und seinem Wort', und doch an Gott nicht gl\u00e4ubt;", "tokens": ["Von", "Gott", "und", "sei\u00b7nem", "Wort'", ",", "und", "doch", "an", "Gott", "nicht", "gl\u00e4ubt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "PPOSAT", "NN", "$,", "KON", "ADV", "APPR", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dass dessen Freund er sey, den er zu f\u00e4llen suchet;", "tokens": ["Dass", "des\u00b7sen", "Freund", "er", "sey", ",", "den", "er", "zu", "f\u00e4l\u00b7len", "su\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELAT", "NN", "PPER", "VAFIN", "$,", "PRELS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vor hundert Thaler kaufft, die mancher Graf verspricht;", "tokens": ["Vor", "hun\u00b7dert", "Tha\u00b7ler", "kaufft", ",", "die", "man\u00b7cher", "Graf", "ver\u00b7spricht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der vor den Leuten sie fast auf den H\u00e4nden tr\u00e4get;", "tokens": ["Der", "vor", "den", "Leu\u00b7ten", "sie", "fast", "auf", "den", "H\u00e4n\u00b7den", "tr\u00e4\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit Gold gesticket tr\u00e4gt, offt Durst und Hunger leidt;", "tokens": ["Mit", "Gold", "ge\u00b7sti\u00b7cket", "tr\u00e4gt", ",", "offt", "Durst", "und", "Hun\u00b7ger", "leidt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VVFIN", "$,", "ADV", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dass mancher Findling wird vor rechten Erb erkennet,", "tokens": ["Dass", "man\u00b7cher", "Find\u00b7ling", "wird", "vor", "rech\u00b7ten", "Erb", "er\u00b7ken\u00b7net", ","], "token_inf