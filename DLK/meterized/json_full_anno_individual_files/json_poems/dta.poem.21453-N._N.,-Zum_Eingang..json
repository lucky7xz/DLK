{"dta.poem.21453": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Zum Eingang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Priester sei der Dichter immerfort,", "tokens": ["Ein", "Pries\u00b7ter", "sei", "der", "Dich\u00b7ter", "im\u00b7mer\u00b7fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er wahre treu der Dichtung heiligen Hort.", "tokens": ["Er", "wah\u00b7re", "treu", "der", "Dich\u00b7tung", "hei\u00b7li\u00b7gen", "Hort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Im Rausch des Wahnsinns geb\u2019 er flammend kund:", "tokens": ["Im", "Rausch", "des", "Wahn\u00b7sinns", "geb'", "er", "flam\u00b7mend", "kund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Offenbarung aus der Gottheit Mund.", "tokens": ["Die", "Of\u00b7fen\u00b7ba\u00b7rung", "aus", "der", "Got\u00b7theit", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ganz poch\u2019 in seiner Brust der Menschheit Herz,", "tokens": ["Ganz", "poch'", "in", "sei\u00b7ner", "Brust", "der", "Menschheit", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Ganz str\u00f6m\u2019 er aus der Menschheit Lust und Schmerz.", "tokens": ["Ganz", "str\u00f6m'", "er", "aus", "der", "Menschheit", "Lust", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Er sei Prophet, der in der tiefsten Nacht", "tokens": ["Er", "sei", "Pro\u00b7phet", ",", "der", "in", "der", "tiefs\u00b7ten", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Sehnsucht nach dem Lichte neu entfacht.", "tokens": ["Die", "Sehn\u00b7sucht", "nach", "dem", "Lich\u00b7te", "neu", "ent\u00b7facht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Er sei ein K\u00f6nig in der Sch\u00f6nheit Reich,", "tokens": ["Er", "sei", "ein", "K\u00f6\u00b7nig", "in", "der", "Sch\u00f6n\u00b7heit", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der s\u00fcndigen Menschheit Heiland allzugleich.", "tokens": ["Der", "s\u00fcn\u00b7di\u00b7gen", "Menschheit", "Hei\u00b7land", "all\u00b7zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Er sei die Flamme, die da ewig brennt,", "tokens": ["Er", "sei", "die", "Flam\u00b7me", ",", "die", "da", "e\u00b7wig", "brennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die Sonne an der V\u00f6lker Firmament,", "tokens": ["Die", "Son\u00b7ne", "an", "der", "V\u00f6l\u00b7ker", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Nicht eher dann verklingt des Sanges Schall,", "tokens": ["Nicht", "e\u00b7her", "dann", "ver\u00b7klingt", "des", "San\u00b7ges", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Bis da\u00df der letzte Mensch erstarb im All.", "tokens": ["Bis", "da\u00df", "der", "letz\u00b7te", "Mensch", "er\u00b7starb", "im", "All", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}