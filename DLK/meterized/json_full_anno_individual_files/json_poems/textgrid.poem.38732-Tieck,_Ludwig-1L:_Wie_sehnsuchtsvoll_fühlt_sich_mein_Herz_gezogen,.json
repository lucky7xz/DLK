{"textgrid.poem.38732": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie sehnsuchtsvoll f\u00fchlt sich mein Herz gezogen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sehnsuchtsvoll f\u00fchlt sich mein Herz gezogen,", "tokens": ["Wie", "sehn\u00b7suchts\u00b7voll", "f\u00fchlt", "sich", "mein", "Herz", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PRF", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem frischen gr\u00fcnen Walde zugelenket,", "tokens": ["Dem", "fri\u00b7schen", "gr\u00fc\u00b7nen", "Wal\u00b7de", "zu\u00b7ge\u00b7len\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von B\u00e4chen wird das neue Gras getr\u00e4nket,", "tokens": ["Von", "B\u00e4\u00b7chen", "wird", "das", "neu\u00b7e", "Gras", "ge\u00b7tr\u00e4n\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Blumen schauen sich in klaren Wogen.", "tokens": ["Die", "Blu\u00b7men", "schau\u00b7en", "sich", "in", "kla\u00b7ren", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein blau Krystall erscheint der Himmelsbogen,", "tokens": ["Ein", "blau", "Krys\u00b7tall", "er\u00b7scheint", "der", "Him\u00b7mels\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur bl\u00fchenden Erde liebend hergesenket,", "tokens": ["Zur", "bl\u00fc\u00b7hen\u00b7den", "Er\u00b7de", "lie\u00b7bend", "her\u00b7ge\u00b7sen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Sonne zeigt, da\u00df sie der Welt gedenket,", "tokens": ["Die", "Son\u00b7ne", "zeigt", ",", "da\u00df", "sie", "der", "Welt", "ge\u00b7den\u00b7ket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie hat die Blumen k\u00fcssend aufgesogen.", "tokens": ["Sie", "hat", "die", "Blu\u00b7men", "k\u00fcs\u00b7send", "auf\u00b7ge\u00b7so\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Pflanzen gl\u00e4nzen, Wasserwogen lachen,", "tokens": ["Die", "Pflan\u00b7zen", "gl\u00e4n\u00b7zen", ",", "Was\u00b7ser\u00b7wo\u00b7gen", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die muntern Thiere regen sich in Spr\u00fcngen,", "tokens": ["Die", "mun\u00b7tern", "Thie\u00b7re", "re\u00b7gen", "sich", "in", "Spr\u00fcn\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["AR