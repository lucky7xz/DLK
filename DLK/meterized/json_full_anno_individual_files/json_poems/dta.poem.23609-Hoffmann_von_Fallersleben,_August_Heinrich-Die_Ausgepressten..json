{"dta.poem.23609": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Ausgepressten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, aus dem Leben wird verschwinden", "tokens": ["Ach", ",", "aus", "dem", "Le\u00b7ben", "wird", "ver\u00b7schwin\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "ART", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Geistes und des Herzens Saat!", "tokens": ["Des", "Geis\u00b7tes", "und", "des", "Her\u00b7zens", "Saat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ist doch k\u00fcnftig noch zu finden", "tokens": ["Wo", "ist", "doch", "k\u00fcnf\u00b7tig", "noch", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "ADJD", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lebend'ges Wort und frische That?", "tokens": ["Le\u00b7ben\u00b7d'\u00b7ges", "Wort", "und", "fri\u00b7sche", "That", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wie's Korn der M\u00fcller auf die M\u00fchle,", "tokens": ["Wie's", "Korn", "der", "M\u00fcl\u00b7ler", "auf", "die", "M\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So schicken wir zur Presse hin", "tokens": ["So", "schi\u00b7cken", "wir", "zur", "Pres\u00b7se", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Vorrath frischester Gef\u00fchle", "tokens": ["Den", "Vor\u00b7rath", "fri\u00b7sches\u00b7ter", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und neuesten Ideengewinn.", "tokens": ["Und", "neu\u00b7es\u00b7ten", "I\u00b7deen\u00b7ge\u00b7winn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wenn uns so das Tagsinteresse", "tokens": ["Und", "wenn", "uns", "so", "das", "Tag\u00b7sin\u00b7ter\u00b7es\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Tagt\u00e4glich an die Presse weist,", "tokens": ["Tag\u00b7t\u00e4g\u00b7lich", "an", "die", "Pres\u00b7se", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sehn wir auf Leipzigs B\u00fcchermesse", "tokens": ["Sehn", "wir", "auf", "Leip\u00b7zigs", "B\u00fc\u00b7cher\u00b7mes\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bald nur noch Deutschlands Herz und Geist.", "tokens": ["Bald", "nur", "noch", "Deutschlands", "Herz", "und", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "KON", "NN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}}}}