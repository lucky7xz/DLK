{"dta.poem.9077": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mitten auf dem Platz,", "tokens": ["Mit\u00b7ten", "auf", "dem", "Platz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "wo die Kinder l\u00e4rmen,", "tokens": ["wo", "die", "Kin\u00b7der", "l\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "bleib ich stehn.", "tokens": ["bleib", "ich", "stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Jungens,", "tokens": ["Jun\u00b7gens", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "die sich um eine Murmel zanken,", "tokens": ["die", "sich", "um", "ei\u00b7ne", "Mur\u00b7mel", "zan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "ein kleines M\u00e4dchen, das Reifen spielt. . . .", "tokens": ["ein", "klei\u00b7nes", "M\u00e4d\u00b7chen", ",", "das", "Rei\u00b7fen", "spielt", ".", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$.", "$.", "$.", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Herr Gott, Fr\u00fchling!", "tokens": ["Herr", "Gott", ",", "Fr\u00fch\u00b7ling", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Und nichts, nichts hab ich gesehn!", "tokens": ["Und", "nichts", ",", "nichts", "hab", "ich", "ge\u00b7sehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PIS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Aus allen B\u00fcschen", "tokens": ["Aus", "al\u00b7len", "B\u00fc\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "brechen ja schon die Knospen!", "tokens": ["bre\u00b7chen", "ja", "schon", "die", "Knos\u00b7pen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}