,"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die haben numehr, o mein Heil,", "tokens": ["Die", "ha\u00b7ben", "nu\u00b7mehr", ",", "o", "mein", "Heil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Durchaus kein Theil", "tokens": ["Durc\u00b7haus", "kein", "Theil"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "An mir zu finden.", "tokens": ["An", "mir", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Mein Glaube l\u00e4sset nicht von dir,", "tokens": ["Mein", "Glau\u00b7be", "l\u00e4s\u00b7set", "nicht", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nur tilge mir", "tokens": ["Nur", "til\u00b7ge", "mir"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Die\u00df letzte Leiden:", "tokens": ["Die\u00df", "letz\u00b7te", "Lei\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Vnd la\u00df mich ewig weder Noht", "tokens": ["Vnd", "la\u00df", "mich", "e\u00b7wig", "we\u00b7der", "Noht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line