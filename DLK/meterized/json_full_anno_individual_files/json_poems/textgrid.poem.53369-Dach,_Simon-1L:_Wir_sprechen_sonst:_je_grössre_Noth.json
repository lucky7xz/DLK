{"textgrid.poem.53369": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir sprechen sonst: je gr\u00f6ssre Noth", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.71", "sv:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sprechen sonst: je gr\u00f6ssre Noth", "tokens": ["Wir", "spre\u00b7chen", "sonst", ":", "je", "gr\u00f6ss\u00b7re", "Noth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Je n\u00e4her Gott.", "tokens": ["Je", "n\u00e4\u00b7her", "Gott", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Herr, meine Sorgen", "tokens": ["Herr", ",", "mei\u00b7ne", "Sor\u00b7gen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Vnd Plagen, die ich jetzt empfind'", "tokens": ["Vnd", "Pla\u00b7gen", ",", "die", "ich", "jetzt", "emp\u00b7find'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Hertzen, sind", "tokens": ["Im", "Hert\u00b7zen", ",", "sind"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPRART", "NN", "$,", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Dir unverborgen.", "tokens": ["Dir", "un\u00b7ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Die Lebens-Kr\u00e4fft' entgehen mir", "tokens": ["Die", "Le\u00b7bens\u00b7Kr\u00e4fft'", "ent\u00b7ge\u00b7hen", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So da\u00df ich schier", "tokens": ["So", "da\u00df", "ich", "schier"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Empfinde Grawen,", "tokens": ["Emp\u00b7fin\u00b7de", "Gra\u00b7wen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Wie g\u00e4ntzlich mir die Arm' und Bein", "tokens": ["Wie", "g\u00e4ntz\u00b7lich", "mir", "die", "Arm'", "und", "Bein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Entfleischet seyn,", "tokens": ["Ent\u00b7flei\u00b7schet", "seyn", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Mehr anzuschawen.", "tokens": ["Mehr", "an\u00b7zu\u00b7scha\u00b7wen", "."], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVIZU", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Die Zung' hat keine Sprache mehr,", "tokens": ["Die", "Zung'", "hat", "kei\u00b7ne", "Spra\u00b7che", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch mein Geh\u00f6r", "tokens": ["Auch", "mein", "Ge\u00b7h\u00f6r"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Beginnt zu schwinden,", "tokens": ["Be\u00b7ginnt", "zu", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ich wei\u00df durch meiner Augen Licht", "tokens": ["Ich", "wei\u00df", "durch", "mei\u00b7ner", "Au\u00b7gen", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sonne nicht", "tokens": ["Die", "Son\u00b7ne", "nicht"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mehr zu empfinden,", "tokens": ["Mehr", "zu", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "F\u00fcr allen schweben immerdar", "tokens": ["F\u00fcr", "al\u00b7len", "schwe\u00b7ben", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die jungen Jahr'", "tokens": ["Die", "jun\u00b7gen", "Jahr'"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "In meinem Hertzen,", "tokens": ["In", "mei\u00b7nem", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "So da\u00df bey mir der S\u00fcnden Gifft", "tokens": ["So", "da\u00df", "bey", "mir", "der", "S\u00fcn\u00b7den", "Gifft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPR", "PPER", "ART", "NN", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.11": {"text": "Weit \u00fcbertrifft", "tokens": ["Weit", "\u00fc\u00b7bert\u00b7rifft"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Die Leibes-Schmertzen.", "tokens": ["Die", "Lei\u00b7bes\u00b7Schmert\u00b7zen", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ich winsle wie ein Kranich thut", "tokens": ["Ich", "wins\u00b7le", "wie", "ein", "Kra\u00b7nich", "thut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr schwerem Muth,", "tokens": ["F\u00fcr", "schwe\u00b7rem", "Muth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mu\u00df kl\u00e4glich girren", "tokens": ["Mu\u00df", "kl\u00e4g\u00b7lich", "gir\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADJD", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wie Tauben, die verwittibt seyn,", "tokens": ["Wie", "Tau\u00b7ben", ",", "die", "ver\u00b7wit\u00b7tibt", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Wald' allein", "tokens": ["Im", "Wald'", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Vnd fl\u00fcchtig irren,", "tokens": ["Vnd", "fl\u00fcch\u00b7tig", "ir\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Setz mich in dieser Angst, o Gott,", "tokens": ["Setz", "mich", "in", "die\u00b7ser", "Angst", ",", "o", "Gott", ","], "token_info": ["word", "word", "word",