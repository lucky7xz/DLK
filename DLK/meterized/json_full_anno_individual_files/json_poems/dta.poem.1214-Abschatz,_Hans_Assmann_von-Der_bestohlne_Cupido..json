{"dta.poem.1214": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der bestohlne Cupido.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Es fand auff einen Tag das sch\u00f6ne Sch\u00e4ffer-Kind/", "tokens": ["Es", "fand", "auff", "ei\u00b7nen", "Tag", "das", "sch\u00f6\u00b7ne", "Sch\u00e4f\u00b7fer\u00b7Kind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das meinen freyen Sinn mit tausend F\u00e4sseln bindt/", "tokens": ["Das", "mei\u00b7nen", "frey\u00b7en", "Sinn", "mit", "tau\u00b7send", "F\u00e4s\u00b7seln", "bindt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "APPR", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Venus zarten Sohn ins gr\u00fcne Gra\u00df gestreckt", "tokens": ["Der", "Ve\u00b7nus", "zar\u00b7ten", "Sohn", "ins", "gr\u00fc\u00b7ne", "Gra\u00df", "ge\u00b7streckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Rosen/ Lilien und N\u00e4geln \u00fcberdeckt.", "tokens": ["Mit", "Ro\u00b7sen", "/", "Li\u00b7li\u00b7en", "und", "N\u00e4\u00b7geln", "\u00fc\u00b7berd\u00b7eckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "NE", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er hatte Bogen/ Pfeil und K\u00f6cher weg gethan/", "tokens": ["Er", "hat\u00b7te", "Bo\u00b7gen", "/", "Pfeil", "und", "K\u00f6\u00b7cher", "weg", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$(", "NN", "KON", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hing seiner Ruhe nach; Schaut/ was Cordilla kan!", "tokens": ["Hing", "sei\u00b7ner", "Ru\u00b7he", "nach", ";", "Schaut", "/", "was", "Cor\u00b7dil\u00b7la", "kan", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "VVFIN", "$(", "PWS", "NE", "VMFIN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Sie schleicht sich unvermerckt mit leisen Schritten hin/", "tokens": ["Sie", "schleicht", "sich", "un\u00b7ver\u00b7merckt", "mit", "lei\u00b7sen", "Schrit\u00b7ten", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nimmt Pfeil und Bogen weg/ verwundet meinen Sinn", "tokens": ["Nimmt", "Pfeil", "und", "Bo\u00b7gen", "weg", "/", "ver\u00b7wun\u00b7det", "mei\u00b7nen", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN", "PTKVZ", "$(", "VVPP", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und tausend andre noch; doch soll mir solche Pein", "tokens": ["Und", "tau\u00b7send", "and\u00b7re", "noch", ";", "doch", "soll", "mir", "sol\u00b7che", "Pein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "PIS", "ADV", "$.", "ADV", "VMFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von ihrer sch\u00f6nen Hand gar lieb zu leiden seyn/", "tokens": ["Von", "ih\u00b7rer", "sch\u00f6\u00b7nen", "Hand", "gar", "lieb", "zu", "lei\u00b7den", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn sie nur stille steht/ und nicht zu ihrer Flucht", "tokens": ["Wenn", "sie", "nur", "stil\u00b7le", "steht", "/", "und", "nicht", "zu", "ih\u00b7rer", "Flucht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "VVFIN", "$(", "KON", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch seines Fl\u00fcgelwercks sich zu bedienen sucht.", "tokens": ["Auch", "sei\u00b7nes", "Fl\u00fc\u00b7gel\u00b7wercks", "sich", "zu", "be\u00b7die\u00b7nen", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}