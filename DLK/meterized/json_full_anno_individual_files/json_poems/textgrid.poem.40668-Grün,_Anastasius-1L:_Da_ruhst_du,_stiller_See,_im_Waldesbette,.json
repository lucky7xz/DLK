{"textgrid.poem.40668": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da ruhst du, stiller See, im Waldesbette,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da ruhst du, stiller See, im Waldesbette,", "tokens": ["Da", "ruhst", "du", ",", "stil\u00b7ler", "See", ",", "im", "Wal\u00b7des\u00b7bet\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Engherzig, selbstisch, unserm Weh verschlossen! \u2013", "tokens": ["Eng\u00b7her\u00b7zig", ",", "selbs\u00b7tisch", ",", "un\u00b7serm", "Weh", "ver\u00b7schlos\u00b7sen", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbweit \u00fcbers Land war einst mein Born ergossen,", "tokens": ["\u00bb", "weit", "\u00fc\u00b7bers", "Land", "war", "einst", "mein", "Born", "er\u00b7gos\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "APPRART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Jed' irdisch Leiden spiegelnd um die Wette.", "tokens": ["Jed'", "ir\u00b7disch", "Lei\u00b7den", "spie\u00b7gelnd", "um", "die", "Wet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"t