{"dta.poem.10194": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "( Im Bivouak. )", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Das Feuer knistert und die Becher klirren,", "tokens": ["Das", "Feu\u00b7er", "knis\u00b7tert", "und", "die", "Be\u00b7cher", "klir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df in die Arme sank der Nacht die Welt.", "tokens": ["La\u00df", "in", "die", "Ar\u00b7me", "sank", "der", "Nacht", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gedanken, ohne Steg und Steuer, irren,", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "oh\u00b7ne", "Steg", "und", "Steu\u00b7er", ",", "ir\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "KOUI", "NN", "KON", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis in der Palmenbucht der Anker f\u00e4llt.", "tokens": ["Bis", "in", "der", "Pal\u00b7men\u00b7bucht", "der", "An\u00b7ker", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Manch Wort und Witz, die hin und gegen schwirren,", "tokens": ["Manch", "Wort", "und", "Witz", ",", "die", "hin", "und", "ge\u00b7gen", "schwir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$,", "PRELS", "ADV", "KON", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verweht der Wind, begr\u00e4bt das stille Feld.", "tokens": ["Ver\u00b7weht", "der", "Wind", ",", "be\u00b7gr\u00e4bt", "das", "stil\u00b7le", "Feld", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein letzter Trunk, und schon in Traumeswirren", "tokens": ["Ein", "letz\u00b7ter", "Trunk", ",", "und", "schon", "in", "Trau\u00b7mes\u00b7wir\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verliert sich ferner Postenruf in\u2019s Zelt.", "tokens": ["Ver\u00b7liert", "sich", "fer\u00b7ner", "Pos\u00b7ten\u00b7ruf", "in's", "Zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}