eter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich bin, so wohl wie du, (man kans ja kl\u00e4rlich lesen,", "tokens": ["Ich", "bin", ",", "so", "wohl", "wie", "du", ",", "(", "man", "kans", "ja", "kl\u00e4r\u00b7lich", "le\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADV", "ADV", "KOKOM", "PPER", "$,", "$(", "PIS", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wenn man die Ode sieht) kein Stoicker gewesen.", "tokens": ["Wenn", "man", "die", "O\u00b7de", "sieht", ")", "kein", "Stoi\u00b7cker", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$(", "PIAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Was aber scherzest du, als h\u00e4tt ich in dem Brief,", "tokens": ["Was", "a\u00b7ber", "scher\u00b7zest", "du", ",", "als", "h\u00e4tt", "ich", "in", "dem", "Brief", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "$,", "KOKOM", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der vor nicht langer Zeit von mir nach Hamburg lief,", "tokens": ["Der", "vor", "nicht", "lan\u00b7ger", "Zeit", "von", "mir", "nach", "Ham\u00b7burg", "lief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PTKNEG", "ADJA", "NN", "APPR", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Geschrieben und gesetzt, als ob 