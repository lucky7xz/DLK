"], "pos": ["PWAV", "ADJD", "PPER", "PPOSAT", "NN", "VAFIN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Was du davon erzehlst: Was Mund und Seele sagt,", "tokens": ["Was", "du", "da\u00b7von", "er\u00b7zehlst", ":", "Was", "Mund", "und", "See\u00b7le", "sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PAV", "VVFIN", "$.", "PWS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Da\u00df ich mich in den Berg und auf die Fahrt gewagt.", "tokens": ["Da\u00df", "ich", "mich", "in", "den", "Berg", "und", "auf", "die", "Fahrt", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Ich w\u00fcnsche: Lebe wohl! Ein g\u00fctig Angedenken", "tokens": ["Ich", "w\u00fcn\u00b7sche", ":", "Le\u00b7be", "wohl", "!", "Ein", "g\u00fc\u00b7tig", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "VVIMP", "ADV", "$.", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Wolst du,", "tokens": ["Wolst", "du", ","], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.48": {"text": "der treusten Freundin schenken.", "tokens": ["der", "treus\u00b7ten", "Freun\u00b7din", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}