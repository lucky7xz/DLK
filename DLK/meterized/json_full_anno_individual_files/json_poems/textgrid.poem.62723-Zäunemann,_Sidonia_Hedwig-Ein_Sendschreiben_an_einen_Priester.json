{"textgrid.poem.62723": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Ein Sendschreiben an einen Priester", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gelehrt-Geehrtester!", "tokens": ["Ge\u00b7lehr\u00b7tGe\u00b7ehr\u00b7tes\u00b7ter", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "ich danke deiner Hand,", "tokens": ["ich", "dan\u00b7ke", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor den galanten Brief, den du mir zugesandt.", "tokens": ["Vor", "den", "ga\u00b7lan\u00b7ten", "Brief", ",", "den", "du", "mir", "zu\u00b7ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er ist mir angenehm, er ist mir lieb gewesen,", "tokens": ["Er", "ist", "mir", "an\u00b7ge\u00b7nehm", ",", "er", "ist", "mir", "lieb", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "PPER", "VAFIN", "PPER", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich habe deine Schrift Vergn\u00fcgungs-voll gelesen.", "tokens": ["Ich", "ha\u00b7be", "dei\u00b7ne", "Schrift", "Ver\u00b7gn\u00fc\u00b7gungs\u00b7voll", "ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ihr Dichter! kommt und seht, wie ", "tokens": ["Ihr", "Dich\u00b7ter", "!", "kommt", "und", "seht", ",", "wie"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "KON", "VVFIN", "$,", "PWAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie sch\u00f6n, wie rein und hoch er doch die Dichtkunst treibt!", "tokens": ["Wie", "sch\u00f6n", ",", "wie", "rein", "und", "hoch", "er", "doch", "die", "Dicht\u00b7kunst", "treibt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "KON", "ADJD", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Einfall ist galant, die Schreibart ist sehr sch\u00f6ne,", "tokens": ["Der", "Ein\u00b7fall", "ist", "ga\u00b7lant", ",", "die", "Schrei\u00b7bart", "ist", "sehr", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gieb mir hierbey nicht schuld, als ob die Heucheley", "tokens": ["Gieb", "mir", "hier\u00b7bey", "nicht", "schuld", ",", "als", "ob", "die", "Heu\u00b7che\u00b7ley"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "PTKNEG", "ADJD", "$,", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mir Hand und Kiel gef\u00fchrt, und meine Schwester sey;", "tokens": ["Mir", "Hand", "und", "Kiel", "ge\u00b7f\u00fchrt", ",", "und", "mei\u00b7ne", "Schwes\u00b7ter", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NE", "VVPP", "$,", "KON", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "O nein! ich brauch sie nicht. Dein Blat kan kl\u00e4rlich zeigen,", "tokens": ["O", "nein", "!", "ich", "brauch", "sie", "nicht", ".", "Dein", "Blat", "kan", "kl\u00e4r\u00b7lich", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie sch\u00f6n die Dichterkunst durch dich auch m\u00fcsse steigen.", "tokens": ["Wie", "sch\u00f6n", "die", "Dich\u00b7ter\u00b7kunst", "durch", "dich", "auch", "m\u00fcs\u00b7se", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dein Kiel lobt mich zu hoch; er schmeichelt mir zu viel,", "tokens": ["Dein", "Kiel", "lobt", "mich", "zu", "hoch", ";", "er", "schmei\u00b7chelt", "mir", "zu", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKA", "ADJD", "$.", "PPER", "VVFIN", "PPER", "PTKA", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nein! meine Poesie, mein Laut-und Saytenspiel", "tokens": ["Nein", "!", "mei\u00b7ne", "Poe\u00b7sie", ",", "mein", "Laut\u00b7\u00b7und", "Say\u00b7ten\u00b7spiel"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Verdient noch nicht den Ruhm, den du mir hast gegeben,", "tokens": ["Ver\u00b7di\u00b7ent", "noch", "nicht", "den", "Ruhm", ",", "den", "du", "mir", "hast", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "PTKNEG", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "O! meine Leyer wird noch nie an Sternen schweben.", "tokens": ["O", "!", "mei\u00b7ne", "Le\u00b7yer", "wird", "noch", "nie", "an", "Ster\u00b7nen", "schwe\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da er mich durch die Hand im Brief so hoch gepreist,", "tokens": ["Da", "er", "mich", "durch", "die", "Hand", "im", "Brief", "so", "hoch", "ge\u00b7preist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "APPRART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Meinung wohl gehabt, den Hochmuth anzuz\u00fcnden.", "tokens": ["Die", "Mei\u00b7nung", "wohl", "ge\u00b7habt", ",", "den", "Hoch\u00b7muth", "an\u00b7zu\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAPP", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie? solte ich durch dich die Bahn zum Denken finden?", "tokens": ["Wie", "?", "sol\u00b7te", "ich", "durch", "dich", "die", "Bahn", "zum", "Den\u00b7ken", "fin\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "PPER", "APPR", "PPER", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "O nein! ich bilde mir auf meinen Kiel nichts ein:", "tokens": ["O", "nein", "!", "ich", "bil\u00b7de", "mir", "auf", "mei\u00b7nen", "Kiel", "nichts", "ein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Drum wird auch deine Schrift nicht so verm\u00f6gend seyn,", "tokens": ["Drum", "wird", "auch", "dei\u00b7ne", "Schrift", "nicht", "so", "ver\u00b7m\u00f6\u00b7gend", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df sie in meiner Brust den Ehrgeitz k\u00f6nt erwecken.", "tokens": ["Da\u00df", "sie", "in", "mei\u00b7ner", "Brust", "den", "Ehr\u00b7geitz", "k\u00f6nt", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ach! leider! brachte mich die Feuers-Glut in Schrecken;", "tokens": ["Ach", "!", "lei\u00b7der", "!", "brach\u00b7te", "mich", "die", "Feu\u00b7er\u00b7sGlut", "in", "Schre\u00b7cken", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "$.", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ach! da\u00df mir diese Noth der H\u00e4user rothes Grab,", "tokens": ["Ach", "!", "da\u00df", "mir", "die\u00b7se", "Noth", "der", "H\u00e4u\u00b7ser", "ro\u00b7thes", "Grab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "PDAT", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So viel betr\u00fcbten Stoff zur neuen Dichtkunst gab!", "tokens": ["So", "viel", "be\u00b7tr\u00fcb\u00b7ten", "Stoff", "zur", "neu\u00b7en", "Dicht\u00b7kunst", "gab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich bin, so wohl wie du, (man kans ja kl\u00e4rlich lesen,", "tokens": ["Ich", "bin", ",", "so", "wohl", "wie", "du", ",", "(", "man", "kans", "ja", "kl\u00e4r\u00b7lich", "le\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADV", "ADV", "KOKOM", "PPER", "$,", "$(", "PIS", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wenn man die Ode sieht) kein Stoicker gewesen.", "tokens": ["Wenn", "man", "die", "O\u00b7de", "sieht", ")", "kein", "Stoi\u00b7cker", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$(", "PIAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Was aber scherzest du, als h\u00e4tt ich in dem Brief,", "tokens": ["Was", "a\u00b7ber", "scher\u00b7zest", "du", ",", "als", "h\u00e4tt", "ich", "in", "dem", "Brief", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "$,", "KOKOM", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der vor nicht langer Zeit von mir nach Hamburg lief,", "tokens": ["Der", "vor", "nicht", "lan\u00b7ger", "Zeit", "von", "mir", "nach", "Ham\u00b7burg", "lief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PTKNEG", "ADJA", "NN", "APPR", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Geschrieben und gesetzt, als ob ich mein Geschlechte", "tokens": ["Ge\u00b7schrie\u00b7ben", "und", "ge\u00b7setzt", ",", "als", "ob", "ich", "mein", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVPP", "$,", "KOKOM", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Zun Engeln zehlete, und um die Menschheit br\u00e4chte?", "tokens": ["Zun", "En\u00b7geln", "zeh\u00b7le\u00b7te", ",", "und", "um", "die", "Menschheit", "br\u00e4ch\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Den Anfang meiner Schrift hab ich mit Vorbedacht,", "tokens": ["Den", "An\u00b7fang", "mei\u00b7ner", "Schrift", "hab", "ich", "mit", "Vor\u00b7be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Satyrisch aufgesetzt, und scoptisch vorgebracht,", "tokens": ["Sa\u00b7ty\u00b7risch", "auf\u00b7ge\u00b7setzt", ",", "und", "scop\u00b7tisch", "vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ich habe die geh\u00f6hnt, die \u00f6fters disputiret:", "tokens": ["Ich", "ha\u00b7be", "die", "ge\u00b7h\u00f6hnt", ",", "die", "\u00f6f\u00b7ters", "dis\u00b7pu\u00b7ti\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "VVPP", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Ob uns ein menschlich Thun und menschlich Wesen zieret?", "tokens": ["Ob", "uns", "ein", "menschlich", "Thun", "und", "menschlich", "We\u00b7sen", "zie\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "NN", "KON", "ADJD", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.36": {"text": "Diesweil uns nun Verstand, Ged\u00e4chtni\u00df und Vernunft", "tokens": ["Dies\u00b7weil", "uns", "nun", "Ver\u00b7stand", ",", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "und", "Ver\u00b7nunft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Gleichwie die M\u00e4nner ziert; so mu\u00df ja unsre Zunft", "tokens": ["Gleich\u00b7wie", "die", "M\u00e4n\u00b7ner", "ziert", ";", "so", "mu\u00df", "ja", "uns\u00b7re", "Zunft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$.", "ADV", "VMFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Auch gleichfals menschlich seyn. Denn weil wir diese Gaben,", "tokens": ["Auch", "gleich\u00b7fals", "menschlich", "seyn", ".", "Denn", "weil", "wir", "die\u00b7se", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAINF", "$.", "KON", "KOUS", "PPER", "PDAT", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Die Menschen eigen sind, so gut wie M\u00e4nner haben;", "tokens": ["Die", "Men\u00b7schen", "ei\u00b7gen", "sind", ",", "so", "gut", "wie", "M\u00e4n\u00b7ner", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "$,", "ADV", "ADJD", "KOKOM", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So folgt ja klar genug, da\u00df wir auch Menschen sind.", "tokens": ["So", "folgt", "ja", "klar", "ge\u00b7nug", ",", "da\u00df", "wir", "auch", "Men\u00b7schen", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "ADV", "$,", "KOUS", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Nimm, was jetzt deine Hand von meiner Arbeit sindt,", "tokens": ["Nimm", ",", "was", "jetzt", "dei\u00b7ne", "Hand", "von", "mei\u00b7ner", "Ar\u00b7beit", "sindt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und lies es liebreich durch. Ich m\u00f6chte heimlich h\u00f6ren,", "tokens": ["Und", "lies", "es", "lieb\u00b7reich", "durch", ".", "Ich", "m\u00f6ch\u00b7te", "heim\u00b7lich", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Wie hoch du meine That wirst loben und verehren,", "tokens": ["Wie", "hoch", "du", "mei\u00b7ne", "That", "wirst", "lo\u00b7ben", "und", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPOSAT", "NN", "VAFIN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Was du davon erzehlst: Was Mund und Seele sagt,", "tokens": ["Was", "du", "da\u00b7von", "er\u00b7zehlst", ":", "Was", "Mund", "und", "See\u00b7le", "sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PAV", "VVFIN", "$.", "PWS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Da\u00df ich mich in den Berg und auf die Fahrt gewagt.", "tokens": ["Da\u00df", "ich", "mich", "in", "den", "Berg", "und", "auf", "die", "Fahrt", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Ich w\u00fcnsche: Lebe wohl! Ein g\u00fctig Angedenken", "tokens": ["Ich", "w\u00fcn\u00b7sche", ":", "Le\u00b7be", "wohl", "!", "Ein", "g\u00fc\u00b7tig", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "VVIMP", "ADV", "$.", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Wolst du,", "tokens": ["Wolst", "du", ","], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.48": {"text": "der treusten Freundin schenken.", "tokens": ["der", "treus\u00b7ten", "Freun\u00b7din", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gelehrt-Geehrtester!", "tokens": ["Ge\u00b7lehr\u00b7tGe\u00b7ehr\u00b7tes\u00b7ter", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "ich danke deiner Hand,", "tokens": ["ich", "dan\u00b7ke", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor den galanten Brief, den du mir zugesandt.", "tokens": ["Vor", "den", "ga\u00b7lan\u00b7ten", "Brief", ",", "den", "du", "mir", "zu\u00b7ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er ist mir angenehm, er ist mir lieb gewesen,", "tokens": ["Er", "ist", "mir", "an\u00b7ge\u00b7nehm", ",", "er", "ist", "mir", "lieb", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "PPER", "VAFIN", "PPER", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich habe deine Schrift Vergn\u00fcgungs-voll gelesen.", "tokens": ["Ich", "ha\u00b7be", "dei\u00b7ne", "Schrift", "Ver\u00b7gn\u00fc\u00b7gungs\u00b7voll", "ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ihr Dichter! kommt und seht, wie ", "tokens": ["Ihr", "Dich\u00b7ter", "!", "kommt", "und", "seht", ",", "wie"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "KON", "VVFIN", "$,", "PWAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie sch\u00f6n, wie rein und hoch er doch die Dichtkunst treibt!", "tokens": ["Wie", "sch\u00f6n", ",", "wie", "rein", "und", "hoch", "er", "doch", "die", "Dicht\u00b7kunst", "treibt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "KON", "ADJD", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Einfall ist galant, die Schreibart ist sehr sch\u00f6ne,", "tokens": ["Der", "Ein\u00b7fall", "ist", "ga\u00b7lant", ",", "die", "Schrei\u00b7bart", "ist", "sehr", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gieb mir hierbey nicht schuld, als ob die Heucheley", "tokens": ["Gieb", "mir", "hier\u00b7bey", "nicht", "schuld", ",", "als", "ob", "die", "Heu\u00b7che\u00b7ley"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "PTKNEG", "ADJD", "$,", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mir Hand und Kiel gef\u00fchrt, und meine Schwester sey;", "tokens": ["Mir", "Hand", "und", "Kiel", "ge\u00b7f\u00fchrt", ",", "und", "mei\u00b7ne", "Schwes\u00b7ter", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NE", "VVPP", "$,", "KON", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "O nein! ich brauch sie nicht. Dein Blat kan kl\u00e4rlich zeigen,", "tokens": ["O", "nein", "!", "ich", "brauch", "sie", "nicht", ".", "Dein", "Blat", "kan", "kl\u00e4r\u00b7lich", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie sch\u00f6n die Dichterkunst durch dich auch m\u00fcsse steigen.", "tokens": ["Wie", "sch\u00f6n", "die", "Dich\u00b7ter\u00b7kunst", "durch", "dich", "auch", "m\u00fcs\u00b7se", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dein Kiel lobt mich zu hoch; er schmeichelt mir zu viel,", "tokens": ["Dein", "Kiel", "lobt", "mich", "zu", "hoch", ";", "er", "schmei\u00b7chelt", "mir", "zu", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKA", "ADJD", "$.", "PPER", "VVFIN", "PPER", "PTKA", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nein! meine Poesie, mein Laut-und Saytenspiel", "tokens": ["Nein", "!", "mei\u00b7ne", "Poe\u00b7sie", ",", "mein", "Laut\u00b7\u00b7und", "Say\u00b7ten\u00b7spiel"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Verdient noch nicht den Ruhm, den du mir hast gegeben,", "tokens": ["Ver\u00b7di\u00b7ent", "noch", "nicht", "den", "Ruhm", ",", "den", "du", "mir", "hast", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "PTKNEG", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "O! meine Leyer wird noch nie an Sternen schweben.", "tokens": ["O", "!", "mei\u00b7ne", "Le\u00b7yer", "wird", "noch", "nie", "an", "Ster\u00b7nen", "schwe\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da er mich durch die Hand im Brief so hoch gepreist,", "tokens": ["Da", "er", "mich", "durch", "die", "Hand", "im", "Brief", "so", "hoch", "ge\u00b7preist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "APPRART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Meinung wohl gehabt, den Hochmuth anzuz\u00fcnden.", "tokens": ["Die", "Mei\u00b7nung", "wohl", "ge\u00b7habt", ",", "den", "Hoch\u00b7muth", "an\u00b7zu\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAPP", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie? solte ich durch dich die Bahn zum Denken finden?", "tokens": ["Wie", "?", "sol\u00b7te", "ich", "durch", "dich", "die", "Bahn", "zum", "Den\u00b7ken", "fin\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "PPER", "APPR", "PPER", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "O nein! ich bilde mir auf meinen Kiel nichts ein:", "tokens": ["O", "nein", "!", "ich", "bil\u00b7de", "mir", "auf", "mei\u00b7nen", "Kiel", "nichts", "ein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Drum wird auch deine Schrift nicht so verm\u00f6gend seyn,", "tokens": ["Drum", "wird", "auch", "dei\u00b7ne", "Schrift", "nicht", "so", "ver\u00b7m\u00f6\u00b7gend", "seyn", ","], "toke