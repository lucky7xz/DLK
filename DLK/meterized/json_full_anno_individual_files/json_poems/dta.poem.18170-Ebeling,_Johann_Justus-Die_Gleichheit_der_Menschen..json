{"dta.poem.18170": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Gleichheit der Menschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "GoTT hat uns alle gleich gemacht,", "tokens": ["GoTT", "hat", "uns", "al\u00b7le", "gleich", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": " da wir an diese Welt gebracht;", "tokens": ["da", "wir", "an", "die\u00b7se", "Welt", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus einen Staub belebter Erden,", "tokens": ["Aus", "ei\u00b7nen", "Staub", "be\u00b7leb\u00b7ter", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df jeder Mensch gebohren wer-", "tokens": ["Mu\u00df", "je\u00b7der", "Mensch", "ge\u00b7boh\u00b7ren", "wer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIAT", "NN", "VVPP", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "den.", "tokens": ["den", "."], "token_info": ["word", "punct"], "pos": ["ART", "$."], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Wir sind auch all auf einer Welt,", "tokens": ["Wir", "sind", "auch", "all", "auf", "ei\u00b7ner", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als Wandersleute aufgestellt;", "tokens": ["Als", "Wan\u00b7ders\u00b7leu\u00b7te", "auf\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wir gehen nach gesetzter Weise,", "tokens": ["Wir", "ge\u00b7hen", "nach", "ge\u00b7setz\u00b7ter", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nur einen Weg der Todes Reise", "tokens": ["Nur", "ei\u00b7nen", "Weg", "der", "To\u00b7des", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wir haben all an einen Heil", "tokens": ["Wir", "ha\u00b7ben", "all", "an", "ei\u00b7nen", "Heil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ja! auch durch wahren Glauben Theil,", "tokens": ["Ja", "!", "auch", "durch", "wah\u00b7ren", "Glau\u00b7ben", "Theil", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und man kann nur bei Eitelkeiten,", "tokens": ["Und", "man", "kann", "nur", "bei", "Ei\u00b7tel\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Um eingebildtes Vorrecht streiten,", "tokens": ["Um", "ein\u00b7ge\u00b7bild\u00b7tes", "Vor\u00b7recht", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und dies ist nichts, drum nehmt in acht,", "tokens": ["Und", "dies", "ist", "nichts", ",", "drum", "nehmt", "in", "acht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PIS", "$,", "PAV", "VVFIN", "APPR", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Gott hat uns alle gleich gemacht.", "tokens": ["Gott", "hat", "uns", "al\u00b7le", "gleich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}