{"dta.poem.3363": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Hymne an die Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Liebe, Bund der Herzen,", "tokens": ["O", "Lie\u00b7be", ",", "Bund", "der", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Geister Symphonie,", "tokens": ["Der", "Geis\u00b7ter", "Sym\u00b7pho\u00b7nie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kelch wollustreicher Schmerzen,", "tokens": ["Kelch", "wol\u00b7lust\u00b7rei\u00b7cher", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Quell hoher Energie,", "tokens": ["Quell", "ho\u00b7her", "E\u00b7ner\u00b7gie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Palladium der Tugend,", "tokens": ["Pal\u00b7la\u00b7di\u00b7um", "der", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Religion der Jugend,", "tokens": ["Re\u00b7li\u00b7gi\u00b7on", "der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Medeenbad des Alters \u2014", "tokens": ["Me\u00b7de\u00b7en\u00b7bad", "des", "Al\u00b7ters"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Verlass, verlass uns nie.", "tokens": ["Ver\u00b7lass", ",", "ver\u00b7lass", "uns", "nie", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Erleucht' uns, Glanz aus Eden,", "tokens": ["Er\u00b7leucht'", "uns", ",", "Glanz", "aus", "E\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Erde W\u00fcsteneyn!", "tokens": ["Der", "Er\u00b7de", "W\u00fcs\u00b7te\u00b7neyn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schlicht' ernst und mild die Fehden,", "tokens": ["Schlicht'", "ernst", "und", "mild", "die", "Feh\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die unser Ich entzweyn!", "tokens": ["Die", "un\u00b7ser", "Ich", "ent\u00b7zweyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wann auf des Lebens Wege", "tokens": ["Wann", "auf", "des", "Le\u00b7bens", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wir schleichen matt und tr\u00e4ge,", "tokens": ["Wir", "schlei\u00b7chen", "matt", "und", "tr\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So st\u00e4rke du die Matten", "tokens": ["So", "st\u00e4r\u00b7ke", "du", "die", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit deinem Labewein.", "tokens": ["Mit", "dei\u00b7nem", "La\u00b7be\u00b7wein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Entr\u00e4thsle uns die Ziffern", "tokens": ["Ent\u00b7r\u00e4ths\u00b7le", "uns", "die", "Zif\u00b7fern"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der aufgerollten Flur,", "tokens": ["Der", "auf\u00b7ge\u00b7roll\u00b7ten", "Flur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Himmels goldne Chiffern,", "tokens": ["Des", "Him\u00b7mels", "gold\u00b7ne", "Chif\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Sanskrit der Natur.", "tokens": ["Das", "Sans\u00b7krit", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.5": {"text": "In Formen, Farben, T\u00f6nen,", "tokens": ["In", "For\u00b7men", ",", "Far\u00b7ben", ",", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Guten, Wahren, Sch\u00f6nen,", "tokens": ["Im", "Gu\u00b7ten", ",", "Wah\u00b7ren", ",", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Enth\u00fcll' uns leis' und liebend", "tokens": ["Ent\u00b7h\u00fcll'", "uns", "leis'", "und", "lie\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Demiurgos Spur.", "tokens": ["Des", "De\u00b7miur\u00b7gos", "Spur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Gib Hochsinn, zu verschm\u00e4hen,", "tokens": ["Gib", "Hoch\u00b7sinn", ",", "zu", "ver\u00b7schm\u00e4\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was unsre W\u00fcrd' entweiht;", "tokens": ["Was", "uns\u00b7re", "W\u00fcrd'", "ent\u00b7weiht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gib Weisheit, auszus\u00e4en,", "tokens": ["Gib", "Weis\u00b7heit", ",", "aus\u00b7zu\u00b7s\u00e4\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was k\u00f6rnerschwer gedeiht;", "tokens": ["Was", "k\u00f6r\u00b7ner\u00b7schwer", "ge\u00b7deiht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gib Freudigkeit zu wandeln,", "tokens": ["Gib", "Freu\u00b7dig\u00b7keit", "zu", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu dulden und zu handeln,", "tokens": ["Zu", "dul\u00b7den", "und", "zu", "han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie es der Gott im Busen,", "tokens": ["Wie", "es", "der", "Gott", "im", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie es die Pflicht gebeut!", "tokens": ["Wie", "es", "die", "Pflicht", "ge\u00b7beut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wann Gewitterodem", "tokens": ["Und", "wann", "Ge\u00b7wit\u00b7te\u00b7ro\u00b7dem"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die bange Brust beklemmt;", "tokens": ["Die", "ban\u00b7ge", "Brust", "be\u00b7klemmt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann schwerer Schwefelbrodem", "tokens": ["Wann", "schwe\u00b7rer", "Schwe\u00b7fel\u00b7bro\u00b7dem"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Wandrer Athem hemmt;", "tokens": ["Der", "Wand\u00b7rer", "A\u00b7them", "hemmt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wann rings die Festen wanken,", "tokens": ["Wann", "rings", "die", "Fes\u00b7ten", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Erde S\u00e4ulen schwanken,", "tokens": ["Der", "Er\u00b7de", "S\u00e4u\u00b7len", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So lass uns ruhig l\u00e4cheln", "tokens": ["So", "lass", "uns", "ru\u00b7hig", "l\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auf deinen Stab gestemmt.", "tokens": ["Auf", "dei\u00b7nen", "Stab", "ge\u00b7stemmt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und wann wir einst vom Gipfel", "tokens": ["Und", "wann", "wir", "einst", "vom", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Wallfahrt niederschaun", "tokens": ["Der", "Wall\u00b7fahrt", "nie\u00b7der\u00b7schaun"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf abgebl\u00fchte Wipfel,", "tokens": ["Auf", "ab\u00b7ge\u00b7bl\u00fch\u00b7te", "Wip\u00b7fel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und tiefverschneyte Au'n;", "tokens": ["Und", "tief\u00b7ver\u00b7schney\u00b7te", "Au'n", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wann rings auf Schutt und Tr\u00fcmmern", "tokens": ["Wann", "rings", "auf", "Schutt", "und", "Tr\u00fcm\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Geliebte Schatten wimmern,", "tokens": ["Ge\u00b7lieb\u00b7te", "Schat\u00b7ten", "wim\u00b7mern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Geheimes Graun uns sch\u00fcttelt,", "tokens": ["Ge\u00b7hei\u00b7mes", "Graun", "uns", "sch\u00fct\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So banne du das Graun.", "tokens": ["So", "ban\u00b7ne", "du", "das", "Graun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und wann wir nun am Rande", "tokens": ["Und", "wann", "wir", "nun", "am", "Ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des \u00f6den Diesseits stehn,", "tokens": ["Des", "\u00f6\u00b7den", "Dies\u00b7seits", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und zu dem stillen Lande", "tokens": ["Und", "zu", "dem", "stil\u00b7len", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Hin\u00fcber sehnend sehn;", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "seh\u00b7nend", "sehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So l\u00e4chl' uns, Glanz aus Eden,", "tokens": ["So", "l\u00e4chl'", "uns", ",", "Glanz", "aus", "E\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und durch des Hades \u00d6den", "tokens": ["Und", "durch", "des", "Ha\u00b7des", "\u00d6\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Lass sel'ger Ahndung trunken", "tokens": ["Lass", "sel'\u00b7ger", "Ahn\u00b7dung", "trun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In jene Fern' uns gehn;", "tokens": ["In", "je\u00b7ne", "Fern'", "uns", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}