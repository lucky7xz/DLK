{"dta.poem.2315": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Aerzte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch den Anblikk holder Nimfen,", "tokens": ["Durch", "den", "An\u00b7blikk", "hol\u00b7der", "Nim\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die W\u00fcrkung sanfter H\u00e4nde,", "tokens": ["Durch", "die", "W\u00fcr\u00b7kung", "sanf\u00b7ter", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Frischer Wangen, schwarzer Augen", "tokens": ["Fri\u00b7scher", "Wan\u00b7gen", ",", "schwar\u00b7zer", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Senken sich in Geist und Glieder", "tokens": ["Sen\u00b7ken", "sich", "in", "Geist", "und", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Neue Kr\u00e4fte, neues Leben.", "tokens": ["Neu\u00b7e", "Kr\u00e4f\u00b7te", ",", "neu\u00b7es", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn ich, voll von Schlafsucht, liege,", "tokens": ["Wenn", "ich", ",", "voll", "von", "Schlaf\u00b7sucht", ",", "lie\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Darf mich nur Dorinde k\u00fctzeln,", "tokens": ["Darf", "mich", "nur", "Do\u00b7rin\u00b7de", "k\u00fct\u00b7zeln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Pl\u00f6tzlich h\u00f6r\u2019 ich auf zu schlafen.", "tokens": ["Pl\u00f6tz\u00b7lich", "h\u00f6r'", "ich", "auf", "zu", "schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn mir Kopf und Waugen schmerzen,", "tokens": ["Wenn", "mir", "Kopf", "und", "Wau\u00b7gen", "schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Darf sie sie nur einmal streicheln,", "tokens": ["Darf", "sie", "sie", "nur", "ein\u00b7mal", "strei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Pl\u00f6tzlich weichen alle Schmerzen.", "tokens": ["Pl\u00f6tz\u00b7lich", "wei\u00b7chen", "al\u00b7le", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Neulich raubte mir ein Fieber", "tokens": ["Neu\u00b7lich", "raub\u00b7te", "mir", "ein", "Fie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Kraft und Lust aus allen Nerven,", "tokens": ["Kraft", "und", "Lust", "aus", "al\u00b7len", "Ner\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und ich fieng schon an zu sterben;", "tokens": ["Und", "ich", "fi\u00b7eng", "schon", "an", "zu", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Aber Doris, meine Taube,", "tokens": ["A\u00b7ber", "Do\u00b7ris", ",", "mei\u00b7ne", "Tau\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Strich, mit sanften Liebesh\u00e4nden,", "tokens": ["Strich", ",", "mit", "sanf\u00b7ten", "Lie\u00b7bes\u00b7h\u00e4n\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Alle halberstorb\u2019ne Glieder,", "tokens": ["Al\u00b7le", "hal\u00b7ber\u00b7stor\u00b7b'\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Und, indem ich sterben wolte,", "tokens": ["Und", ",", "in\u00b7dem", "ich", "ster\u00b7ben", "wol\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "K\u00fc\u00dfte sie zum Abschiedsseegen", "tokens": ["K\u00fc\u00df\u00b7te", "sie", "zum", "Ab\u00b7schieds\u00b7see\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Noch einmal die blassen Lippen,", "tokens": ["Noch", "ein\u00b7mal", "die", "blas\u00b7sen", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Pl\u00f6tzlich h\u00f6rt\u2019 ich auf zu sterben.", "tokens": ["Pl\u00f6tz\u00b7lich", "h\u00f6rt'", "ich", "auf", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Pl\u00f6tzlich flohen Brand und Fieber,", "tokens": ["Pl\u00f6tz\u00b7lich", "flo\u00b7hen", "Brand", "und", "Fie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Pl\u00f6tzlich ward ich froh und munter.", "tokens": ["Pl\u00f6tz\u00b7lich", "ward", "ich", "froh", "und", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Zwanzig Stunden nach dem Kusse", "tokens": ["Zwan\u00b7zig", "Stun\u00b7den", "nach", "dem", "Kus\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "F\u00fchlt\u2019 ich schon in allen Gliedern", "tokens": ["F\u00fchlt'", "ich", "schon", "in", "al\u00b7len", "Glie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Neue Kr\u00e4fte, neues Leben;", "tokens": ["Neu\u00b7e", "Kr\u00e4f\u00b7te", ",", "neu\u00b7es", "Le\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Und nach zwanzig andern Stunden", "tokens": ["Und", "nach", "zwan\u00b7zig", "an\u00b7dern", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Hatt\u2019 ich mir, mit neuen Kr\u00e4ften,", "tokens": ["Hatt'", "ich", "mir", ",", "mit", "neu\u00b7en", "Kr\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Schon die Lippen roth gek\u00fcsset.", "tokens": ["Schon", "die", "Lip\u00b7pen", "roth", "ge\u00b7k\u00fcs\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Doris! dein Genesungsmittel", "tokens": ["Do\u00b7ris", "!", "dein", "Ge\u00b7ne\u00b7sungs\u00b7mit\u00b7tel"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "PPOSAT", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.31": {"text": "Hat den Beifall aller Aerzte;", "tokens": ["Hat", "den", "Bei\u00b7fall", "al\u00b7ler", "A\u00b7erz\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.32": {"text": "Aber lehr es keinen Aerzten,", "tokens": ["A\u00b7ber", "lehr", "es", "kei\u00b7nen", "A\u00b7erz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.33": {"text": "Spar es nur f\u00fcr meine Fieber,", "tokens": ["Spar", "es", "nur", "f\u00fcr", "mei\u00b7ne", "Fie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Und verschreib es keinen andern.", "tokens": ["Und", "ver\u00b7schreib", "es", "kei\u00b7nen", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Deine Schwestern kanst du\u2019s lehren.", "tokens": ["Dei\u00b7ne", "Schwes\u00b7tern", "kanst", "du's", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}