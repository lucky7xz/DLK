{"dta.poem.8125": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott sey, ja dem Sohn sey, der zu Gott geht, Anbetung!", "tokens": ["Gott", "sey", ",", "ja", "dem", "Sohn", "sey", ",", "der", "zu", "Gott", "geht", ",", "An\u00b7be\u00b7tung", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ADV", "ART", "NN", "VAFIN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Werft die Krone, werft, Engel, auch ihr", "tokens": ["Werft", "die", "Kro\u00b7ne", ",", "werft", ",", "En\u00b7gel", ",", "auch", "ihr"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "$,", "NE", "$,", "ADV", "PPOSAT"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "In Triumphgange die Palme,", "tokens": ["In", "Tri\u00b7umph\u00b7gan\u00b7ge", "die", "Pal\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df der Herr sie euch gab, nieder am Thron!", "tokens": ["Da\u00df", "der", "Herr", "sie", "euch", "gab", ",", "nie\u00b7der", "am", "Thron", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PPER", "VVFIN", "$,", "PTKVZ", "APPRART", "NN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}}}}}