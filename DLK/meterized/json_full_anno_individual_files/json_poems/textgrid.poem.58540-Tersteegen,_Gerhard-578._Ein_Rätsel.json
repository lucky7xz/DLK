{"textgrid.poem.58540": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "578. Ein R\u00e4tsel", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah verschiedne Kinder saugen;", "tokens": ["Ich", "sah", "ver\u00b7schied\u00b7ne", "Kin\u00b7der", "sau\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Eins brauchte Mund und Ohr'n und Augen,", "tokens": ["Eins", "brauch\u00b7te", "Mund", "und", "Ohr'n", "und", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es fragt' und wu\u00dfte wunderklug", "tokens": ["Es", "fragt'", "und", "wu\u00df\u00b7te", "wun\u00b7der\u00b7klug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Mutter, Brust und Milch zu schw\u00e4tzen,", "tokens": ["Von", "Mut\u00b7ter", ",", "Brust", "und", "Milch", "zu", "schw\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und bei dem kindlichen Erg\u00f6tzen", "tokens": ["Und", "bei", "dem", "kind\u00b7li\u00b7chen", "Er\u00b7g\u00f6t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sog's dann und wann, doch nur nicht g'nug.", "to