{"textgrid.poem.42862": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Komm, sage mir, was du f\u00fcr Sorgen hast", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es zwitschert eine Lerche im Kamin,", "tokens": ["Es", "zwit\u00b7schert", "ei\u00b7ne", "Ler\u00b7che", "im", "Ka\u00b7min", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn du sie h\u00f6rst.", "tokens": ["Wenn", "du", "sie", "h\u00f6rst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein jeder Schutzmann in Berlin", "tokens": ["Ein", "je\u00b7der", "Schutz\u00b7mann", "in", "Ber\u00b7lin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "APPR", "NE"], "meter": "-+-+-+++", "measure": "unknown.measure.penta"}, "line.4": {"text": "Verhaftet dich, wenn du ihn st\u00f6rst.", "tokens": ["Ver\u00b7haf\u00b7tet", "dich", ",", "wenn", "du", "ihn", "st\u00f6rst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Faltenwurfe einer Decke", "tokens": ["Im", "Fal\u00b7ten\u00b7wur\u00b7fe", "ei\u00b7ner", "De\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Klagt ein Gesicht,", "tokens": ["Klagt", "ein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "++-+", "measure": "iambic.di"}, "line.3": {"text": "Wenn du es siehst.", "tokens": ["Wenn", "du", "es", "siehst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Der Posten im Gef\u00e4ngnis schie\u00dft,", "tokens": ["Der", "Pos\u00b7ten", "im", "Ge\u00b7f\u00e4ng\u00b7nis", "schie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn du als kleiner Str\u00e4fling ihm entfliehst.", "tokens": ["Wenn", "du", "als", "klei\u00b7ner", "Str\u00e4f\u00b7ling", "ihm", "ent\u00b7fliehst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich t\u00e4t es nicht.", "tokens": ["Ich", "t\u00e4t", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "In eines Holzes Duft", "tokens": ["In", "ei\u00b7nes", "Hol\u00b7zes", "Duft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Lebt fernes Land.", "tokens": ["Lebt", "fer\u00b7nes", "Land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Gebirge schreiten durch die blaue Luft.", "tokens": ["Ge\u00b7bir\u00b7ge", "schrei\u00b7ten", "durch", "die", "blau\u00b7e", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "