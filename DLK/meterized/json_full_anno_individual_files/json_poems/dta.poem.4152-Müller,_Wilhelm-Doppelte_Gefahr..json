{"dta.poem.4152": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Doppelte Gefahr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo soll ich schiffen hin?", "tokens": ["Wo", "soll", "ich", "schif\u00b7fen", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein Rachen ist gar kleine,", "tokens": ["Mein", "Ra\u00b7chen", "ist", "gar", "klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gar sch\u00fcchtern ist mein Sinn.", "tokens": ["Gar", "sch\u00fcch\u00b7tern", "ist", "mein", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Im hohen Meere drau\u00dfen,", "tokens": ["Im", "ho\u00b7hen", "Mee\u00b7re", "drau\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da sind die Wogen gro\u00df,", "tokens": ["Da", "sind", "die", "Wo\u00b7gen", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da l\u00e4\u00dft aus Ost und Westen", "tokens": ["Da", "l\u00e4\u00dft", "aus", "Ost", "und", "Wes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Himmel die St\u00fcrme los.", "tokens": ["Der", "Him\u00b7mel", "die", "St\u00fcr\u00b7me", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Da jagen die Korsaren", "tokens": ["Da", "ja\u00b7gen", "die", "Kor\u00b7sa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach jungem Christenblut,", "tokens": ["Nach", "jun\u00b7gem", "Chris\u00b7ten\u00b7blut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da singen die Sirenen", "tokens": ["Da", "sin\u00b7gen", "die", "Si\u00b7re\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und locken hinab in die Fluth.", "tokens": ["Und", "lo\u00b7cken", "hin\u00b7ab", "in", "die", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Am Ufer sitzt ein M\u00e4dchen,", "tokens": ["Am", "U\u00b7fer", "sitzt", "ein", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die hat ein Augenpaar,", "tokens": ["Die", "hat", "ein", "Au\u00b7gen\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das droht mit Feuerflammen", "tokens": ["Das", "droht", "mit", "Feu\u00b7er\u00b7flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir t\u00f6dtliche Gefahr.", "tokens": ["Mir", "t\u00f6dt\u00b7li\u00b7che", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie strickt an einem Netze,", "tokens": ["Sie", "strickt", "an", "ei\u00b7nem", "Net\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Will drin mich fangen ein,", "tokens": ["Will", "drin", "mich", "fan\u00b7gen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Haar hat lange Flechten,", "tokens": ["Ihr", "Haar", "hat", "lan\u00b7ge", "Flech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dran soll ich gebunden sein.", "tokens": ["Dran", "soll", "ich", "ge\u00b7bun\u00b7den", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Du liebliche Sirene,", "tokens": ["Du", "lieb\u00b7li\u00b7che", "Si\u00b7re\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sirene von dem Strand,", "tokens": ["Si\u00b7re\u00b7ne", "von", "dem", "Strand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df deine Stimme t\u00f6nen", "tokens": ["La\u00df", "dei\u00b7ne", "Stim\u00b7me", "t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hell \u00fcber Meer und Land.", "tokens": ["Hell", "\u00fc\u00b7ber", "Meer", "und", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Tief unten in den Fluthen", "tokens": ["Tief", "un\u00b7ten", "in", "den", "Flut\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ist ein goldnes Haus,", "tokens": ["Da", "ist", "ein", "gold\u00b7nes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ruhen die Ertrunknen", "tokens": ["Da", "ru\u00b7hen", "die", "Er\u00b7trunk\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In weichen Armen aus.", "tokens": ["In", "wei\u00b7chen", "Ar\u00b7men", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "In diesem Liebesmeere", "tokens": ["In", "die\u00b7sem", "Lie\u00b7bes\u00b7mee\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo wird die Ruhst\u00e4tt' sein?", "tokens": ["Wo", "wird", "die", "Ruh\u00b7st\u00e4tt'", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entweder an deinem Herzen,", "tokens": ["Ent\u00b7we\u00b7der", "an", "dei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ach! oder im Grabe mein.", "tokens": ["Ach", "!", "o\u00b7der", "im", "Gra\u00b7be", "mein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "APPRART", "NN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}