{"dta.poem.12092": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Schenk' ist solch ein Ort, wo dir nichts wird geschenkt,", "tokens": ["Die", "Schenk'", "ist", "solch", "ein", "Ort", ",", "wo", "dir", "nichts", "wird", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "ART", "NN", "$,", "PWAV", "PPER", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was man einschenkt, wird dir theuer eingetr\u00e4nkt.", "tokens": ["Und", "was", "man", "ein\u00b7schenkt", ",", "wird", "dir", "theu\u00b7er", "ein\u00b7ge\u00b7tr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "VVPP", "$,", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In eine solche trat ich neulich auf dem Lande,", "tokens": ["In", "ei\u00b7ne", "sol\u00b7che", "trat", "ich", "neu\u00b7lich", "auf", "dem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fand ihr Inneres in recht idyllischem Stande.", "tokens": ["Und", "fand", "ihr", "In\u00b7ne\u00b7res", "in", "recht", "i\u00b7dyl\u00b7li\u00b7schem", "Stan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein Fenster offen hier, ein Fenster offen dort,", "tokens": ["Ein", "Fens\u00b7ter", "of\u00b7fen", "hier", ",", "ein", "Fens\u00b7ter", "of\u00b7fen", "dort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "$,", "ART", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Mahlzeit aufgetischt an dem und jenem Ort.", "tokens": ["Und", "Mahl\u00b7zeit", "auf\u00b7ge\u00b7tischt", "an", "dem", "und", "je\u00b7nem", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "APPR", "ART", "KON", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zum einen Fenster stieg herein mit mancher Henne", "tokens": ["Zum", "ei\u00b7nen", "Fens\u00b7ter", "stieg", "her\u00b7ein", "mit", "man\u00b7cher", "Hen\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ART", "NN", "VVFIN", "PTKVZ", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Hahn, und pickte stolz die K\u00f6rner von der Tenne.", "tokens": ["Der", "Hahn", ",", "und", "pick\u00b7te", "stolz", "die", "K\u00f6r\u00b7ner", "von", "der", "Ten\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Zum andern flog herein paarweise Taub' und Tauber,", "tokens": ["Zum", "an\u00b7dern", "flog", "her\u00b7ein", "paar\u00b7wei\u00b7se", "Taub'", "und", "Tau\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PTKVZ", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die lasen das Gesims von allen Kr\u00fcmchen sauber.", "tokens": ["Die", "la\u00b7sen", "das", "Ge\u00b7sims", "von", "al\u00b7len", "Kr\u00fcm\u00b7chen", "sau\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch unter Fittigen der eingeladnen Gro\u00dfen", "tokens": ["Doch", "un\u00b7ter", "Fit\u00b7ti\u00b7gen", "der", "ein\u00b7ge\u00b7lad\u00b7nen", "Gro\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lief mit manch Kleineres, vom Menschen sonst versto\u00dfen:", "tokens": ["Lief", "mit", "manch", "Klei\u00b7ne\u00b7res", ",", "vom", "Men\u00b7schen", "sonst", "ver\u00b7sto\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$,", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der Sperling und der Fink, die Ammer und die Meise,", "tokens": ["Der", "Sper\u00b7ling", "und", "der", "Fink", ",", "die", "Am\u00b7mer", "und", "die", "Mei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein jedes haschte flink auch einen Mundvoll Speise.", "tokens": ["Ein", "je\u00b7des", "haschte", "flink", "auch", "ei\u00b7nen", "Mund\u00b7voll", "Spei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Mag Hahn und Taube nun mit Kron' und Haube pralen,", "tokens": ["Mag", "Hahn", "und", "Tau\u00b7be", "nun", "mit", "Kron'", "und", "Hau\u00b7be", "pra\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "KON", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie m\u00fcssen theur das Mahl mit ihrem Leben zahlen.", "tokens": ["Sie", "m\u00fcs\u00b7sen", "theur", "das", "Mahl", "mit", "ih\u00b7rem", "Le\u00b7ben", "zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Sie werden von dem Wirth wie jeder Gast gerupft,", "tokens": ["Sie", "wer\u00b7den", "von", "dem", "Wirth", "wie", "je\u00b7der", "Gast", "ge\u00b7rupft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "KOKOM", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nur die Bettler sind mit heiler Haut entschlupft.", "tokens": ["Und", "nur", "die", "Bett\u00b7ler", "sind", "mit", "hei\u00b7ler", "Haut", "ent\u00b7schlupft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}