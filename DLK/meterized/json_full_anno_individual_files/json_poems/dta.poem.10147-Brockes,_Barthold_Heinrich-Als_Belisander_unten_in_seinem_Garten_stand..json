{"dta.poem.10147": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Als Belisander unten in seinem Garten  \n stand.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da ich dort die holde H\u00f6he,", "tokens": ["Da", "ich", "dort", "die", "hol\u00b7de", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Und, wenn ich das Haupt nur drehe,", "tokens": ["Und", ",", "wenn", "ich", "das", "Haupt", "nur", "dre\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Hier die sch\u00f6ne Fl\u00e4che sehe,", "tokens": ["Hier", "die", "sch\u00f6\u00b7ne", "Fl\u00e4\u00b7che", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der Sch\u00f6pfer mir geg\u00f6nnt;", "tokens": ["Die", "der", "Sch\u00f6p\u00b7fer", "mir", "ge\u00b7g\u00f6nnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So erfreut sich mein Gem\u00fcthe,", "tokens": ["So", "er\u00b7freut", "sich", "mein", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Preis\u2019t die Allmacht, r\u00fchmt die G\u00fcte:", "tokens": ["Preis't", "die", "All\u00b7macht", ",", "r\u00fchmt", "die", "G\u00fc\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ie mehr es sie erkennt,", "tokens": ["Und", "ie", "mehr", "es", "sie", "er\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "W\u00fcnschet es, mit froher Seelen,", "tokens": ["W\u00fcn\u00b7schet", "es", ",", "mit", "fro\u00b7her", "See\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "In der reichen Gaben Pracht,", "tokens": ["In", "der", "rei\u00b7chen", "Ga\u00b7ben", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Von des grossen Gebers Macht", "tokens": ["Von", "des", "gros\u00b7sen", "Ge\u00b7bers", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Vieles w\u00fcrdig zu erzehlen.", "tokens": ["Vie\u00b7les", "w\u00fcr\u00b7dig", "zu", "er\u00b7zeh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}