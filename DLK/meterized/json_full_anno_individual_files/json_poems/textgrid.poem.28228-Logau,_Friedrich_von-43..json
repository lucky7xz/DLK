{"textgrid.poem.28228": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der an Jahren schwer gleich tr\u00e4get,", "tokens": ["Der", "an", "Jah\u00b7ren", "schwer", "gleich", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Viel an Kr\u00e4fften abgeleget,", "tokens": ["Viel", "an", "Kr\u00e4ff\u00b7ten", "ab\u00b7ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann er nur ist frisch von Rathe,", "tokens": ["Wann", "er", "nur", "ist", "frisch", "von", "Ra\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist noch doch ein gut Soldate.", "tokens": ["Ist", "noch", "doch", "ein", "gut", "Sol\u00b7da\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer nichts mehr vermag von Thaten,", "tokens": ["Wer", "nichts", "mehr", "ver\u00b7mag", "von", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ob er viel vermag im rathen,", "tokens": ["Ob", "er", "viel", "ver\u00b7mag", "im", "ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mag nur bey den Buhlereyen", "tokens": ["Mag", "nur", "bey", "den", "Buh\u00b7le\u00b7re\u00b7yen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Dieses Nahmens sich verzeihen.", "tokens": ["Die\u00b7ses", "Nah\u00b7mens", "sich", "ver\u00b7zei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Eigner Mut und fremder Degen", "tokens": ["Eig\u00b7ner", "Mut", "und", "frem\u00b7der", "De\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "K\u00fcnnen zwar noch Ruhm erregen;", "tokens": ["K\u00fcn\u00b7nen", "zwar", "noch", "Ruhm", "er\u00b7re\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Aber mit geborgtem Leibe", "tokens": ["A\u00b7ber", "mit", "ge\u00b7borg\u00b7tem", "Lei\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "F\u00fchlt man nicht das s\u00fc\u00df am Weibe.", "tokens": ["F\u00fchlt", "man", "nicht", "das", "s\u00fc\u00df", "am", "Wei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "ART", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1":