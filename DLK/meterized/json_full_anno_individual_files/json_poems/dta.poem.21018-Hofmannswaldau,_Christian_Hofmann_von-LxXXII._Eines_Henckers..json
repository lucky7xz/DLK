{"dta.poem.21018": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxXXII.  \n Eines Henckers.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Die Marter und der Tod erworben mir das Brod/", "tokens": ["Die", "Mar\u00b7ter", "und", "der", "Tod", "er\u00b7wor\u00b7ben", "mir", "das", "Brod", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Handwerck war der Mord/ mein Leben war", "tokens": ["Mein", "Hand\u00b7werck", "war", "der", "Mord", "/", "mein", "Le\u00b7ben", "war"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$(", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der Tod.", "tokens": ["der", "Tod", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Und welcher was ich war nicht g\u00e4ntzlich kan verste-", "tokens": ["Und", "wel\u00b7cher", "was", "ich", "war", "nicht", "g\u00e4ntz\u00b7lich", "kan", "vers\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAT", "PWS", "PPER", "VAFIN", "PTKNEG", "ADJD", "VMFIN", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "hen/ (gehen.", "tokens": ["hen", "/", "(", "ge\u00b7hen", "."], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["VVFIN", "$(", "$(", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Der mag nach mehr Bericht an Rad und Galgen", "tokens": ["Der", "mag", "nach", "mehr", "Be\u00b7richt", "an", "Rad", "und", "Gal\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "PIAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}