{"dta.poem.11753": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu geben Gr\u00f6stes gern mag Gro\u00dfmuth sich bequemen,", "tokens": ["Zu", "ge\u00b7ben", "Gr\u00f6s\u00b7tes", "gern", "mag", "Gro\u00df\u00b7muth", "sich", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "NN", "ADV", "VMFIN", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Doch ungern l\u00e4\u00dft sie sich das Allerkleinste nehmen.", "tokens": ["Doch", "un\u00b7gern", "l\u00e4\u00dft", "sie", "sich", "das", "Al\u00b7ler\u00b7kleins\u00b7te", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dem Geber gibt man nur, vorm Nehmer nimmt mans fort;", "tokens": ["Dem", "Ge\u00b7ber", "gibt", "man", "nur", ",", "vorm", "Neh\u00b7mer", "nimmt", "mans", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "$,", "APPRART", "NN", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Willst du ein Gut, so gib daf\u00fcr ein gutes Wort.", "tokens": ["Willst", "du", "ein", "Gut", ",", "so", "gib", "da\u00b7f\u00fcr", "ein", "gu\u00b7tes", "Wort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,", "ADV", "VVIMP", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Man gibt ein gutes Wort, um etwas zu erlangen,", "tokens": ["Man", "gibt", "ein", "gu\u00b7tes", "Wort", ",", "um", "et\u00b7was", "zu", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUI", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dann ein zweites noch als Dank, wenn mans empfangen.", "tokens": ["Und", "dann", "ein", "zwei\u00b7tes", "noch", "als", "Dank", ",", "wenn", "mans", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "ADV", "KOUS", "NN", "$,", "KOUS", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Dank f\u00fcr eine Gab' ist selber eine Gabe,", "tokens": ["Der", "Dank", "f\u00fcr", "ei\u00b7ne", "Gab'", "ist", "sel\u00b7ber", "ei\u00b7ne", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Willkommen dem, der reich schon ist an andrer Habe.", "tokens": ["Will\u00b7kom\u00b7men", "dem", ",", "der", "reich", "schon", "ist", "an", "an\u00b7drer", "Ha\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "ADJD", "ADV", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}