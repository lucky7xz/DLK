{"dta.poem.9957": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin keine katze nicht/ ob ich schon den katzen gleiche/", "tokens": ["Ich", "bin", "kei\u00b7ne", "kat\u00b7ze", "nicht", "/", "ob", "ich", "schon", "den", "kat\u00b7zen", "glei\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "VVFIN", "PTKNEG", "$(", "KOUS", "PPER", "ADV", "ART", "ADJA", "ADJA", "$("], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und wie diese um den brey/ so um m\u00e4u\u00df und ratzen schleiche.", "tokens": ["Und", "wie", "die\u00b7se", "um", "den", "brey", "/", "so", "um", "m\u00e4u\u00df", "und", "rat\u00b7zen", "schlei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PDS", "APPR", "ART", "NN", "$(", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Kein betr\u00fcger bin ich auch/ ob ich listig schon wie schlangen/", "tokens": ["Kein", "be\u00b7tr\u00fc\u00b7ger", "bin", "ich", "auch", "/", "ob", "ich", "lis\u00b7tig", "schon", "wie", "schlan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADV", "$(", "KOUS", "PPER", "ADJD", "ADV", "KOKOM", "VVINF", "$("], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Un\u0303 kein m\u00f6rder ob ich gleich mir zur nahrung was mu\u00df fange\u0303.", "tokens": ["U\u00f1", "kein", "m\u00f6r\u00b7der", "ob", "ich", "gleich", "mir", "zur", "nah\u00b7rung", "was", "mu\u00df", "fang\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "KOUS", "PPER", "ADV", "PPER", "APPRART", "NN", "PWS", "VMFIN", "VVFIN", "$."], "meter": "--+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "(zu/", "tokens": ["(", "zu", "/"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "PTKZU", "$("], "meter": "+", "measure": "single.up"}}}}}