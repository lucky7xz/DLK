{"textgrid.poem.63864": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Prolog", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum willst du mit Ghaselen", "tokens": ["Wa\u00b7rum", "willst", "du", "mit", "Gha\u00b7se\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die geduld'ge Muse qu\u00e4len?", "tokens": ["Die", "ge\u00b7duld'\u00b7ge", "Mu\u00b7se", "qu\u00e4\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieder dichte, drin sich zwanglos", "tokens": ["Lie\u00b7der", "dich\u00b7te", ",", "drin", "sich", "zwang\u00b7los"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ADV", "PRF", "ADJD"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sinn und Reim von selbst verm\u00e4hlen,", "tokens": ["Sinn", "und", "Reim", "von", "selbst", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie sie hold von M\u00e4dchenlippen", "tokens": ["Wie", "sie", "hold", "von", "M\u00e4d\u00b7chen\u00b7lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "T\u00f6nen und aus Vogelkehlen! \u2013", "tokens": ["T\u00f6\u00b7nen", "und", "aus", "Vo\u00b7gel\u00b7keh\u00b7len", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Also werden meine Feinde,", "tokens": ["Al\u00b7so", "wer\u00b7den", "mei\u00b7ne", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Werden meine Freunde schm\u00e4hlen.", "tokens": ["Wer\u00b7den", "mei\u00b7ne", "Freun\u00b7de", "schm\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Doch bedeckt: nicht steht's dem Dichter", "tokens": ["Doch", "be\u00b7deckt", ":", "nicht", "steht's", "dem", "Dich\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "$.", "PTKNEG", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Frei, sich seine Form zu w\u00e4hlen;", "tokens": ["Frei", ",", "sich", "sei\u00b7ne", "Form", "zu", "w\u00e4h\u00b7len", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRF", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Er gehorcht geheimnisvollen", "tokens": ["Er", "ge\u00b7horcht", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Seines Genius Befehlen.", "tokens": ["Sei\u00b7nes", "Ge\u00b7nius", "Be\u00b7feh\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.13": {"text": "Platen wagte, Schiras' G\u00e4rten", "tokens": ["Pla\u00b7ten", "wag\u00b7te", ",", "Schi\u00b7ras'", "G\u00e4r\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$,", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Sehr ausgiebig zu bestehlen,", "tokens": ["Sehr", "aus\u00b7gie\u00b7big", "zu", "be\u00b7steh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "(ganze hundertsechsundf\u00fcnzig", "tokens": ["(", "gan\u00b7ze", "hun\u00b7dert\u00b7sech\u00b7sund\u00b7f\u00fcn\u00b7zig"], "token_info": ["punct", "word", "word"], "pos": ["$(", "ADJA", "CARD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Hafislieder konnt' ich z\u00e4hlen!)", "tokens": ["Ha\u00b7fis\u00b7lie\u00b7der", "konnt'", "ich", "z\u00e4h\u00b7len", "!", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und er wu\u00dfte viel vom Schenken", "tokens": ["Und", "er", "wu\u00df\u00b7te", "viel", "vom", "Schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Und vom Liebchen zu erz\u00e4hlen,", "tokens": ["Und", "vom", "Lieb\u00b7chen", "zu", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Im Gedicht; denn ach, im Leben", "tokens": ["Im", "Ge\u00b7dicht", ";", "denn", "ach", ",", "im", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$.", "KON", "XY", "$,", "APPRART", "NN"], "meter": "+-+-+-+-