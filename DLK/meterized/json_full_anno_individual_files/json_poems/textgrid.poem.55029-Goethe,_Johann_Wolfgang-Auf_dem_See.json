{"textgrid.poem.55029": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem See", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und frische Nahrung, neues Blut", "tokens": ["Und", "fri\u00b7sche", "Nah\u00b7rung", ",", "neu\u00b7es", "Blut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Saug ich aus freier Welt;", "tokens": ["Saug", "ich", "aus", "frei\u00b7er", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie ist Natur so hold und gut,", "tokens": ["Wie", "ist", "Na\u00b7tur", "so", "hold", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die mich am Busen h\u00e4lt!", "tokens": ["Die", "mich", "am", "Bu\u00b7sen", "h\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Welle wieget unsern Kahn", "tokens": ["Die", "Wel\u00b7le", "wie\u00b7get", "un\u00b7sern", "Kahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Rudertakt hinauf,", "tokens": ["Im", "Ru\u00b7der\u00b7takt", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Berge, wolkig himmelan,", "tokens": ["Und", "Ber\u00b7ge", ",", "wol\u00b7kig", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Begegnen unserm Lauf.", "tokens": ["Be\u00b7geg\u00b7nen", "un\u00b7serm", "Lauf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Aug, mein Aug, was sinkst du nieder?", "tokens": ["Aug", ",", "mein", "Aug", ",", "was", "sinkst", "du", "nie\u00b7der", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Goldne Tr\u00e4ume, kommt ihr wieder?", "tokens": ["Gold\u00b7ne", "Tr\u00e4u\u00b7me", ",", "kommt", "ihr", "wie\u00b7der", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weg, du Traum! so gold du bist;", "tokens": ["Weg", ",", "du", "Traum", "!", "so", "gold", "du", "bist", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "$.", "ADV", "ADJD", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier auch Lieb und Leben ist.", "tokens": ["Hier", "auch", "Lieb", "und", "Le\u00b7ben", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf der Welle blinken", "tokens": ["Auf", "