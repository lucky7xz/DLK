{"textgrid.poem.33520": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Spaziergang", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Abend kommt mit Mondschein und seidner Dunkelheit.", "tokens": ["Der", "A\u00b7bend", "kommt", "mit", "Mond\u00b7schein", "und", "seid\u00b7ner", "Dun\u00b7kel\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Wege werden m\u00fcde. Die enge Welt wird weit.", "tokens": ["Die", "We\u00b7ge", "wer\u00b7den", "m\u00fc\u00b7de", ".", "Die", "en\u00b7ge", "Welt", "wird", "weit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Opiumwinde gehen feldein und feldhinaus.", "tokens": ["O\u00b7pium\u00b7win\u00b7de", "ge\u00b7hen", "feld\u00b7ein", "und", "feld\u00b7hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ich breite meine Augen wie Silberfl\u00fcgel aus.", "tokens": ["Ich", "brei\u00b7te", "mei\u00b7ne", "Au\u00b7gen", "wie", "Sil\u00b7ber\u00b7fl\u00fc\u00b7gel", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KOKOM", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Mir ist, als 