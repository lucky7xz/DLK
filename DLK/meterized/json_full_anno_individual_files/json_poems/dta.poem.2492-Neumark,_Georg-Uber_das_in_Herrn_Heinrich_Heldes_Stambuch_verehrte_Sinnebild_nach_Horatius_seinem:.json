{"dta.poem.2492": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Uber das in Herrn Heinrich Heldes  \n Stambuch verehrte Sinnebild/ nach  \n Horatius seinem:", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Wer sich Apollo dir und deinen K\u00fcnsten giebet/", "tokens": ["Wer", "sich", "A\u00b7pol\u00b7lo", "dir", "und", "dei\u00b7nen", "K\u00fcns\u00b7ten", "gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "NE", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der wird nicht nur allein von Menschen hier beliebet/", "tokens": ["Der", "wird", "nicht", "nur", "al\u00b7lein", "von", "Men\u00b7schen", "hier", "be\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADV", "ADV", "APPR", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "(jmfall mans recht versteht) Er schwingt sich auch empor", "tokens": ["(", "jm\u00b7fall", "mans", "recht", "ver\u00b7steht", ")", "Er", "schwingt", "sich", "auch", "em\u00b7por"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "PIS", "ADJD", "VVFIN", "$(", "PPER", "VVFIN", "PRF", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch H\u00fclffe des Ger\u00fcchts bi\u00df an das Sternen Chor;", "tokens": ["Durch", "H\u00fclf\u00b7fe", "des", "Ge\u00b7r\u00fcchts", "bi\u00df", "an", "das", "Ster\u00b7nen", "Chor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sein Rahme wird ber\u00fchmt sein kluges H\u00e4upt das strahlet", "tokens": ["Sein", "Rah\u00b7me", "wird", "be\u00b7r\u00fchmt", "sein", "klu\u00b7ges", "H\u00e4upt", "das", "strah\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "PPOSAT", "ADJA", "NN", "PDS", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit einer Lorberkrohn;; In Summa was sonst prahlet/", "tokens": ["Mit", "ei\u00b7ner", "Lor\u00b7ber\u00b7krohn", ";", ";", "In", "Sum\u00b7ma", "was", "sonst", "prah\u00b7let", "/"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$.", "APPR", "NN", "PWS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Mit G\u00fctern dieser Welt/ mu\u00df mit der Zeit vergehn/", "tokens": ["Mit", "G\u00fc\u00b7tern", "die\u00b7ser", "Welt", "/", "mu\u00df", "mit", "der", "Zeit", "ver\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDAT", "NN", "$(", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nicht aber ein Poet/ der der bleibt ewig stehn.", "tokens": ["Nicht", "a\u00b7ber", "ein", "Po\u00b7et", "/", "der", "der", "bleibt", "e\u00b7wig", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$(", "ART", "ART", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}