{"textgrid.poem.41840": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Seligkeit", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zufrieden nicht mit Gut und Gl\u00fcck,", "tokens": ["Zu\u00b7frie\u00b7den", "nicht", "mit", "Gut", "und", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gebannt in enge Lebenssph\u00e4ren,", "tokens": ["Ge\u00b7bannt", "in", "en\u00b7ge", "Le\u00b7bens\u00b7sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erhebst Du weiter Wunsch und Blick,", "tokens": ["Er\u00b7hebst", "Du", "wei\u00b7ter", "Wunsch", "und", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und willst noch Seligkeit begehren.", "tokens": ["Und", "willst", "noch", "Se\u00b7lig\u00b7keit", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wei\u00dft Du auch ", "tokens": ["Und", "wei\u00dft", "Du", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und wei\u00dft Du auch wie sie errungen? \u2013", "tokens": ["Und", "wei\u00dft", "Du", "auch", "wie", "sie", "er\u00b7run\u00b7gen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "KOKOM", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Lichtblick nur auf Raum und Zeit,", "tokens": ["Ein", "Licht\u00b7blick", "nur", "auf", "Raum", "und", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der ihre Schranken \u00fcbersprungen!", "tokens": ["Der", "ih\u00b7re", "Schran\u00b7ken", "\u00fc\u00b7bers\u00b7prun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn Du im br\u00fcnstigen Gebet", "tokens": ["Wenn", "Du", "im", "br\u00fcns\u00b7ti\u00b7gen", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Zum Throne Gottes Dich erhoben", "tokens": ["Zum", "Thro\u00b7ne", "Got\u00b7tes", "Dich", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und die Gewi\u00dfheit vor Dir steht:", "tokens": ["Und", "die", "Ge\u00b7wi\u00df\u00b7heit", "vor", "Dir", "steht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Geist ist selbst ein Strahl von oben.", "tokens": ["Dein", "Geist", "ist", "selbst", "ein", "Strahl", "von", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Strahl, ein Teil von Gottes Licht,", "tokens": ["Ein", "Strahl", ",", "ein", "Teil", "von", "Got\u00b7tes", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Betraut mit einer hohen Sendung \u2013", "tokens": ["Be\u00b7traut", "mit", "ei\u00b7ner", "ho\u00b7hen", "Sen\u00b7dung", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und eine innre Stimme spricht:", "tokens": ["Und", "ei\u00b7ne", "inn\u00b7re", "Stim\u00b7me", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist erkoren zur Vollendung \u2013", "tokens": ["Du", "bist", "er\u00b7ko\u00b7ren", "zur", "Vol\u00b7len\u00b7dung", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPRART", "NN", "$("], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Wenn dann Dich das Gef\u00fchl beseelt", "tokens": ["Wenn", "dann", "Dich", "das", "Ge\u00b7f\u00fchl", "be\u00b7seelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "In dieses Daseins Wechselleben:", "tokens": ["In", "die\u00b7ses", "Da\u00b7seins", "Wech\u00b7sel\u00b7le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die ganze Menschheit ist erw\u00e4hlt,", "tokens": ["Die", "gan\u00b7ze", "Menschheit", "ist", "er\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Um nach Vervollkommung zu streben \u2013", "tokens": ["Um", "nach", "Ver\u00b7voll\u00b7kom\u00b7mung", "zu", "stre\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Und wenn im Tempel der Natur", "tokens": ["Und", "wenn", "im", "Tem\u00b7pel", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Abendrot, beim Sternenreigen,", "tokens": ["Im", "A\u00b7ben\u00b7drot", ",", "beim", "Ster\u00b7nen\u00b7rei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Sonnenglanz der Blumenflur", "tokens": ["Im", "Son\u00b7nen\u00b7glanz", "der", "Blu\u00b7men\u00b7flur"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich Bilder des Vollkommen zeigen;", "tokens": ["Sich", "Bil\u00b7der", "des", "Voll\u00b7kom\u00b7men", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dann sinkt von Dir ein jedes Leid", "tokens": ["Dann", "sinkt", "von", "Dir", "ein", "je\u00b7des", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und jeder Zweifel ist zerronnen,", "tokens": ["Und", "je\u00b7der", "Zwei\u00b7fel", "ist", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ein Moment der Seligkeit", "tokens": ["Und", "ein", "Mo\u00b7ment", "der", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hast Du hienieden schon gewonnen.", "tokens": ["Hast", "Du", "hien\u00b7ie\u00b7den", "schon", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und wenn in sch\u00f6ner Harmonie", "tokens": ["Und", "wenn", "in", "sch\u00f6\u00b7ner", "Har\u00b7mo\u00b7nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Herz ein andres Herz begegnet,", "tokens": ["Dein", "Herz", "ein", "and\u00b7res", "Herz", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und zweier Seelen Sympathie,", "tokens": ["Und", "zwei\u00b7er", "See\u00b7len", "Sym\u00b7pa\u00b7thie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lieb und Begeistrung zwiefach segnet.", "tokens": ["Lieb", "und", "Be\u00b7geis\u00b7trung", "zwie\u00b7fach", "seg\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.9": {"line.1": {"text": "Dann bist Du selig erdentr\u00fcckt,", "tokens": ["Dann", "bist", "Du", "se\u00b7lig", "er\u00b7den\u00b7tr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fchlst Dich von Himmelslust umfangen,", "tokens": ["F\u00fchlst", "Dich", "von", "Him\u00b7mels\u00b7lust", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ahnst begl\u00fcckend und begl\u00fcckt,", "tokens": ["Und", "ahnst", "be\u00b7gl\u00fc\u00b7ckend", "und", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df zur Vollendung zu gelangen.", "tokens": ["Da\u00df", "zur", "Vol\u00b7len\u00b7dung", "zu", "ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Und wenn ein Werk der Hand, dem Geist", "tokens": ["Und", "wenn", "ein", "Werk", "der", "Hand", ",", "dem", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gelungen ist durch Flei\u00df und M\u00fchen,", "tokens": ["Ge\u00b7lun\u00b7gen", "ist", "durch", "Flei\u00df", "und", "M\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Aug' ein dunkler Vorhang rei\u00dft", "tokens": ["Dem", "Aug'", "ein", "dunk\u00b7ler", "Vor\u00b7hang", "rei\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und neue Lande vor Dir bl\u00fchen!", "tokens": ["Und", "neu\u00b7e", "Lan\u00b7de", "vor", "Dir", "bl\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und mitten drinn in Kampf und Not", "tokens": ["Und", "mit\u00b7ten", "drinn", "in", "Kampf", "und", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch f\u00fcr den Gott im Busen streiten,", "tokens": ["Doch", "f\u00fcr", "den", "Gott", "im", "Bu\u00b7sen", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und hinzunehmen Schmach und Tod,", "tokens": ["Und", "hin\u00b7zu\u00b7neh\u00b7men", "Schmach", "und", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": 