": ["Sinkt", "Raum", "und", "Zeit", "von", "uns", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wirst Du Vollkommnes rings erkennen!", "tokens": ["Wirst", "Du", "Voll\u00b7komm\u00b7nes", "rings", "er\u00b7ken\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Zufrieden nicht mit Gut und Gl\u00fcck,", "tokens": ["Zu\u00b7frie\u00b7den", "nicht", "mit", "Gut", "und", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gebannt in enge Lebenssph\u00e4ren,", "tokens": ["Ge\u00b7bannt", "in", "en\u00b7ge", "Le\u00b7bens\u00b7sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erhebst Du weiter Wunsch und Blick,", "tokens": ["Er\u00b7hebst", "Du", "wei\u00b7ter", "Wunsch", "und", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und willst noch Seligkeit begehren.", "tokens": ["Und", "willst", "noch", "Se\u00b7lig\u00b7keit", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Und wei\u00dft Du auch ", "tokens": ["Und", "wei\u00dft", "Du", "auch"], "token_info":