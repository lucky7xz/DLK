{"textgrid.poem.65922": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Hellas", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Hellas ward die Gottheit hold gestaltet;", "tokens": ["In", "Hel\u00b7las", "ward", "die", "Got\u00b7theit", "hold", "ge\u00b7stal\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Meer und Inseln sehne dich hinaus!", "tokens": ["Auf", "Meer", "und", "In\u00b7seln", "seh\u00b7ne", "dich", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nie fa\u00dft dich dort vor Menschen alter Graus:", "tokens": ["Nie", "fa\u00dft", "dich", "dort", "vor", "Men\u00b7schen", "al\u00b7ter", "Graus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie haben sich zur Sonne stolz entfaltet.", "tokens": ["Sie", "ha\u00b7ben", "sich", "zur", "Son\u00b7ne", "stolz", "ent\u00b7fal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gesang, der treu \u2013 durch die Besinnung \u2013 schaltet,", "tokens": ["Ge\u00b7sang", ",", "der", "treu", "\u2013", "durch", "die", "Be\u00b7sin\u00b7nung", "\u2013", "schal\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "$(", "APPR", "ART", "NN", "$(", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geleite dich um dein vertrautes Haus!", "tokens": ["Ge\u00b7lei\u00b7te", "dich", "um", "dein", "ver\u00b7trau\u00b7tes", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gem\u00fct, bald wird dir wohl bei Windgebraus:", "tokens": ["Ge\u00b7m\u00fct", ",", "bald", "wird", "dir", "wohl", "bei", "Wind\u00b7ge\u00b7braus", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du sp\u00fcrst, wie Herzensblut im Grauen waltet.", "tokens": ["Du", "sp\u00fcrst", ",", "wie", "Her\u00b7zens\u00b7blut", "im", "Grau\u00b7en", "wal\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Krieger tritt gefa\u00dft zum sch\u00f6nen Weibe:", "tokens": ["Der", "Krie\u00b7ger", "tritt", "ge\u00b7fa\u00dft", "zum", "sch\u00f6\u00b7nen", "Wei\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Leidenschaften w\u00e4gt ein gro\u00dfes Ma\u00df,", "tokens": ["Die", "Lei\u00b7den\u00b7schaf\u00b7ten", "w\u00e4gt", "ein", "gro\u00b7\u00dfes", "Ma\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df Anmut ernstge\u00fcbten Leibern bleibe!", "tokens": ["Da\u00df", "An\u00b7mut", "ernst\u00b7ge\u00b7\u00fcb\u00b7ten", "Lei\u00b7bern", "blei\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bei Spielen, die kein k\u00fchnes Lied verga\u00df,", "tokens": ["Bei", "Spie\u00b7len", ",", "die", "kein", "k\u00fch\u00b7nes", "Lied", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erblickst du J\u00fcnglinge mit heller Scheibe:", "tokens": ["Er\u00b7blickst", "du", "J\u00fcng\u00b7lin\u00b7ge", "mit", "hel\u00b7ler", "Schei\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie adeln Wettstreit, als gegl\u00fcckten Spa\u00df.", "tokens": ["Sie", "a\u00b7deln", "Wett\u00b7streit", ",", "als", "ge\u00b7gl\u00fcck\u00b7ten", "Spa\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "In Hellas ward die Gottheit hold gestaltet;", "tokens": ["In", "Hel\u00b7las", "ward", "die", "Got\u00b7theit", "hold", "ge\u00b7stal\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "A