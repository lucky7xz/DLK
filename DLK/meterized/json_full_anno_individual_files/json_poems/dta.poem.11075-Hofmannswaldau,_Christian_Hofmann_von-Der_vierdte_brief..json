ord", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "APPR", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.85": {"text": "Die nach-welt (wieviel dich auch tugend-strahlen kr\u00f6nen:)", "tokens": ["Die", "nach\u00b7welt", "(", "wie\u00b7viel", "dich", "auch", "tu\u00b7gen\u00b7dstrah\u00b7len", "kr\u00f6\u00b7nen", ":)"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "emoticon"], "pos": ["ART", "NN", "$(", "VVFIN", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.86": {"text": "Wird doch was tadelhafft, nur mercken auf von dir.", "tokens": ["Wird", "doch", "was", "ta\u00b7del\u00b7hafft", ",", "nur", "mer\u00b7cken", "auf", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PWS", "VVFIN", "$,", "ADV", "VVINF", "APPR", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.87": {"text": "Gedenckst du gleich dein thun vielf\u00e4rbig zu besch\u00f6nen;", "tokens": ["Ge\u00b7denckst", "du", "gleich", "dein", "thun", "viel\u00b7f\u00e4r\u00b7big", "zu", "be\u00b7sch\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "VVINF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.88": {"text": "Zeuch lastern goldst\u00fcck an, sie blicken dennoch f\u00fcr.", "tokens": ["Zeuch", "las\u00b7tern", "gold\u00b7st\u00fcck", "an", ",", "sie", "bli\u00b7cken", "den\u00b7noch", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.89": {"text": "Ich bitte thr\u00e4nend dich, leg\u2019 alles auf die wage,", "tokens": ["Ich", "bit\u00b7te", "thr\u00e4\u00b7nend", "dich", ",", "leg'", "al\u00b7les", "auf", "die", "wa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "PPER", "$,", "VVFIN", "PIS", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.90": {"text": "Was vor verlust folgt nicht der hand-voll schn\u00f6den lust?", "tokens": ["Was", "vor", "ver\u00b7lust", "folgt", "nicht", "der", "han\u00b7dvoll", "schn\u00f6\u00b7den", "lust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVFIN", "PTKNEG", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.91": {"text": "Nicht glaube, da\u00df die magd zu dir mehr liebe trage,", "tokens": ["Nicht", "glau\u00b7be", ",", "da\u00df", "die", "magd", "zu", "dir", "mehr", "lie\u00b7be", "tra\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "KOUS", "ART", "NN", "APPR", "PPER", "ADV", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.92": {"text": "In huren steckt mehr brunst, mehr treu in keuscher brust.", "tokens": ["In", "hu\u00b7ren", "steckt", "mehr", "brunst", ",", "mehr", "treu", "in", "keu\u00b7scher", "brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIAT", "NN", "$,", "ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.93": {"text": "Zur witwe machst du mich zwar, aber dich zum knech