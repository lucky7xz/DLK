word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VVFIN", "PPER", "ADV", "$.", "PPER", "VAFIN", "ART", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.98": {"text": "Ich will die b\u00fcrde nur einpacken zu der flucht,", "tokens": ["Ich", "will", "die", "b\u00fcr\u00b7de", "nur", "ein\u00b7pa\u00b7cken", "zu", "der", "flucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "VVINF", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.99": {"text": "Wer geile m\u00e4gde liebt, ist seines weibes hasser;", "tokens": ["Wer", "gei\u00b7le", "m\u00e4g\u00b7de", "liebt", ",", "ist", "sei\u00b7nes", "wei\u00b7bes", "has\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$,", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.100": {"text": "Der aber liebet recht, der keusche seelen sucht.", "tokens": ["Der", "a\u00b7ber", "lie\u00b7bet", "recht", ",", "der", "keu\u00b7sche", "see\u00b7len", "sucht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADJD", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}