{"dta.poem.11075": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der vierdte brief.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die sollte wohl nicht mehr erschrecken f\u00fcr gewittern,", "tokens": ["Die", "soll\u00b7te", "wohl", "nicht", "mehr", "er\u00b7schre\u00b7cken", "f\u00fcr", "ge\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "PTKNEG", "ADV", "VVFIN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der so viel jahre schon der blitz ums haupt gespielt;", "tokens": ["Der", "so", "viel", "jah\u00b7re", "schon", "der", "blitz", "ums", "haupt", "ge\u00b7spielt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "ADV", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was f\u00fcr ein felsern hertz sollt\u2019 aber nicht ersch\u00fcttern,", "tokens": ["Was", "f\u00fcr", "ein", "fel\u00b7sern", "hertz", "sollt'", "a\u00b7ber", "nicht", "er\u00b7sch\u00fct\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo man den erden-grund selbst mit sich brechen f\u00fchlt?", "tokens": ["Wo", "man", "den", "er\u00b7den\u00b7grund", "selbst", "mit", "sich", "bre\u00b7chen", "f\u00fchlt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "ADV", "APPR", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der grund-stein unsers heyls und hoffnung geht zu grunde,", "tokens": ["Der", "grun\u00b7dstein", "un\u00b7sers", "heyls", "und", "hoff\u00b7nung", "geht", "zu", "grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn er zerrei\u00dft der eh ihr unzertrennlich band.", "tokens": ["Denn", "er", "zer\u00b7rei\u00dft", "der", "eh", "ihr", "un\u00b7zer\u00b7trenn\u00b7lich", "band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die ohnmacht will mir zu, das wort zerbricht im munde,", "tokens": ["Die", "ohn\u00b7macht", "will", "mir", "zu", ",", "das", "wort", "zer\u00b7bricht", "im", "mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VMFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "VVFIN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es f\u00e4llt die feder mir schon neunmal aus der hand.", "tokens": ["Es", "f\u00e4llt", "die", "fe\u00b7der", "mir", "schon", "neun\u00b7mal", "aus", "der", "hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was l\u00e4\u00dft vor antwort sich auf scheide-briefe schreiben,", "tokens": ["Was", "l\u00e4\u00dft", "vor", "ant\u00b7wort", "sich", "auf", "schei\u00b7de\u00b7brie\u00b7fe", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "PRF", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wodurch mein eh-herr mir mehr als den hals abspricht?", "tokens": ["Wo\u00b7durch", "mein", "eh\u00b7herr", "mir", "mehr", "als", "den", "hals", "ab\u00b7spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "PIAT", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Ach m\u00f6cht er einen dolch mir eh durchs hertze treiben,", "tokens": ["Ach", "m\u00f6cht", "er", "ei\u00b7nen", "dolch", "mir", "eh", "durchs", "hert\u00b7ze", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "PPER", "KOUS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Eh er das b\u00fcndnis trennt, das sonst der tod nur bricht.", "tokens": ["Eh", "er", "das", "b\u00fcnd\u00b7nis", "trennt", ",", "das", "sonst", "der", "tod", "nur", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Denn, himmel! wer will sich ihn doch bereden lassen,", "tokens": ["Denn", ",", "him\u00b7mel", "!", "wer", "will", "sich", "ihn", "doch", "be\u00b7re\u00b7den", "las\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$.", "PWS", "VMFIN", "PRF", "PPER", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Zwey frauen h\u00e4tten wohl in einem bette raum?", "tokens": ["Zwey", "frau\u00b7en", "h\u00e4t\u00b7ten", "wohl", "in", "ei\u00b7nem", "bet\u00b7te", "raum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Zertheiltes lieben ist nur ein verbl\u00fchmtes hassen,", "tokens": ["Zer\u00b7theil\u00b7tes", "lie\u00b7ben", "ist", "nur", "ein", "ver\u00b7bl\u00fchm\u00b7tes", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ein \u00fcberzuckert gifft, und ein bezaubert traum.", "tokens": ["Ein", "\u00fc\u00b7ber\u00b7zu\u00b7ckert", "gifft", ",", "und", "ein", "be\u00b7zau\u00b7bert", "traum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,", "KON", "ART", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Gott schuff nur eine Ev\u2019 aus Adams seiner riebe,", "tokens": ["Gott", "schuff", "nur", "ei\u00b7ne", "Ev'", "aus", "A\u00b7dams", "sei\u00b7ner", "rie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "APPR", "NE", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die grosse sonne giebt zwey monden nicht ihr licht;", "tokens": ["Die", "gros\u00b7se", "son\u00b7ne", "giebt", "zwey", "mon\u00b7den", "nicht", "ihr", "licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "CARD", "NN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein geist beseelt den leib, ein weib des eh-manns liebe,", "tokens": ["Ein", "geist", "be\u00b7seelt", "den", "leib", ",", "ein", "weib", "des", "eh\u00b7manns", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und unser trau-ring hat zwey mittel-puncte nicht.", "tokens": ["Und", "un\u00b7ser", "trau\u00b7ring", "hat", "zwey", "mit\u00b7tel\u00b7punc\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "CARD", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie manchen heyraths-schlu\u00df zerreist itzt brunst und reue,", "tokens": ["Wie", "man\u00b7chen", "hey\u00b7raths\u00b7schlu\u00df", "zer\u00b7reist", "itzt", "brunst", "und", "reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Nun man sie aufs papier, nicht mehr ins hertze schreibt;", "tokens": ["Nun", "man", "sie", "aufs", "pa\u00b7pier", ",", "nicht", "mehr", "ins", "hert\u00b7ze", "schreibt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PPER", "APPRART", "NN", "$,", "PTKNEG", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Jhr siegel ist itzt wachs, vor wars ertz-feste treue,", "tokens": ["Ihr", "sie\u00b7gel", "ist", "itzt", "wachs", ",", "vor", "wars", "ertz\u00b7fes\u00b7te", "treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJA", "$,", "APPR", "VAFIN", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Weil keiner l\u00e4nger nicht als wachs best\u00e4ndig bleibt.", "tokens": ["Weil", "kei\u00b7ner", "l\u00e4n\u00b7ger", "nicht", "als", "wachs", "be\u00b7st\u00e4n\u00b7dig", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "PTKNEG", "KOUS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Einf\u00e4lt\u2019ge, die ihr traut auf eurer m\u00e4nner eide,", "tokens": ["Ein\u00b7f\u00e4lt'\u00b7ge", ",", "die", "ihr", "traut", "auf", "eu\u00b7rer", "m\u00e4n\u00b7ner", "ei\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sie haben euch nicht mehr, als k\u00e4fer blumen, lieb,", "tokens": ["Sie", "ha\u00b7ben", "euch", "nicht", "mehr", ",", "als", "k\u00e4\u00b7fer", "blu\u00b7men", ",", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "ADV", "$,", "KOUS", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sie sind ein seiden-wurm, der anfangs zwar spinnt seide,", "tokens": ["Sie", "sind", "ein", "sei\u00b7den\u00b7wurm", ",", "der", "an\u00b7fangs", "zwar", "spinnt", "sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "In kurtzem aber wird ein he\u00dflich molcken-dieb.", "tokens": ["In", "kurt\u00b7zem", "a\u00b7ber", "wird", "ein", "he\u00df\u00b7lich", "mol\u00b7cken\u00b7dieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADV", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der glatten worte kost, mit welcher sie euch speisen,", "tokens": ["Der", "glat\u00b7ten", "wor\u00b7te", "kost", ",", "mit", "wel\u00b7cher", "sie", "euch", "spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "APPR", "PRELS", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ist Mithridatens tisch, der nie von gifft ist leer;", "tokens": ["Ist", "Mith\u00b7ri\u00b7da\u00b7tens", "tisch", ",", "der", "nie", "von", "gifft", "ist", "leer", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "PRELS", "ADV", "APPR", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Jhr treu-seyn bricht wie ei\u00df, das ihr erkaufft vor eisen,", "tokens": ["Ihr", "treu\u00b7seyn", "bricht", "wie", "ei\u00df", ",", "das", "ihr", "er\u00b7kaufft", "vor", "ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "ADJD", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Denn heucheley wei\u00df wind zu machen centner-schwer.", "tokens": ["Denn", "heu\u00b7che\u00b7ley", "wei\u00df", "wind", "zu", "ma\u00b7chen", "cent\u00b7ner\u00b7schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "PTKZU", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Doch w\u00e4re dieses leid noch endlich zu verschmertzen;", "tokens": ["Doch", "w\u00e4\u00b7re", "die\u00b7ses", "leid", "noch", "end\u00b7lich", "zu", "ver\u00b7schmert\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PDAT", "ADJD", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Da\u00df aber leider sie die schuld uns messen bey,", "tokens": ["Da\u00df", "a\u00b7ber", "lei\u00b7der", "sie", "die", "schuld", "uns", "mes\u00b7sen", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PPER", "ART", "ADJD", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Mit ihrem lastermahl uns unser antlitz schw\u00e4rtzen,", "tokens": ["Mit", "ih\u00b7rem", "las\u00b7ter\u00b7mahl", "uns", "un\u00b7ser", "ant\u00b7litz", "schw\u00e4rt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ist unertr\u00e4glich leid, zweyfache tyranney.", "tokens": ["Ist", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "leid", ",", "zwey\u00b7fa\u00b7che", "ty\u00b7ran\u00b7ney", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Da\u00df ich der liebe gluth mit keuschheit-schnee gek\u00fchlet,", "tokens": ["Da\u00df", "ich", "der", "lie\u00b7be", "gluth", "mit", "keuschheit\u00b7schnee", "ge\u00b7k\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Hei\u00dft itzt, ich h\u00e4tte gar ihr feuer ausgelescht,", "tokens": ["Hei\u00dft", "itzt", ",", "ich", "h\u00e4t\u00b7te", "gar", "ihr", "feu\u00b7er", "aus\u00b7ge\u00b7lescht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Da man mehr wohlthun doch von sanfftem brande f\u00fchlet,", "tokens": ["Da", "man", "mehr", "wohl\u00b7thun", "doch", "von", "sanff\u00b7tem", "bran\u00b7de", "f\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Das \u00f6l mehr zunder n\u00e4hrt, als sch\u00e4umend seiffe g\u00e4scht.", "tokens": ["Das", "\u00f6l", "mehr", "zun\u00b7der", "n\u00e4hrt", ",", "als", "sch\u00e4u\u00b7mend", "seif\u00b7fe", "g\u00e4scht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "$,", "KOUS", "ADJD", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die \u00fcbermaase pre\u00dft aus pomerantzen gallen,", "tokens": ["Die", "\u00fc\u00b7ber\u00b7maa\u00b7se", "pre\u00dft", "aus", "po\u00b7me\u00b7rant\u00b7zen", "gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ja milckt, statt s\u00fcsser milch, aus schlaffen eytern blut,", "tokens": ["Ja", "milckt", ",", "statt", "s\u00fcs\u00b7ser", "milch", ",", "aus", "schlaf\u00b7fen", "ey\u00b7tern", "blut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVPP", "$,", "KOUI", "ADJA", "NN", "$,", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ein hefftig loder-licht mu\u00df bald in staub zerfallen,", "tokens": ["Ein", "heff\u00b7tig", "lo\u00b7der\u00b7licht", "mu\u00df", "bald", "in", "staub", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "VMFIN", "ADV", "APPR", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und allzugrosse brunst ist nicht im lieben gut.", "tokens": ["Und", "all\u00b7zu\u00b7gros\u00b7se", "brunst", "ist", "nicht", "im", "lie\u00b7ben", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PTKNEG", "APPRART", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Die pflantzen unsrer eh sind zeugen meiner liebe,", "tokens": ["Die", "pflant\u00b7zen", "uns\u00b7rer", "eh", "sind", "zeu\u00b7gen", "mei\u00b7ner", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "VVFIN", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Allein der eckel ist der wollust mi\u00dfgeburth.", "tokens": ["Al\u00b7lein", "der", "ec\u00b7kel", "ist", "der", "wol\u00b7lust", "mi\u00df\u00b7ge\u00b7burth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Betr\u00e4nckten lippen sind die kl\u00e4rsten brunnen tr\u00fcbe,", "tokens": ["Be\u00b7tr\u00e4nck\u00b7ten", "lip\u00b7pen", "sind", "die", "kl\u00e4rs\u00b7ten", "brun\u00b7nen", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "F\u00fcr fremdes wasser st\u00f6\u00dft man eignen nectar fort.", "tokens": ["F\u00fcr", "frem\u00b7des", "was\u00b7ser", "st\u00f6\u00dft", "man", "eig\u00b7nen", "nec\u00b7tar", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PIS", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Die \u00fcppigkeit verschm\u00e4ht des ehweibs zucker-k\u00fcsse,", "tokens": ["Die", "\u00fcp\u00b7pig\u00b7keit", "ver\u00b7schm\u00e4ht", "des", "eh\u00b7weibs", "zu\u