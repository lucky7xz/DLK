], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Das aber ist bey Teutsch- und Christen unerh\u00f6ret,", "tokens": ["Das", "a\u00b7ber", "ist", "bey", "Teut\u00b7sch", "und", "Chris\u00b7ten", "un\u00b7er\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "APPR", "TRUNC", "KON", "NN", "ADJD", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.68": {"text": "Da\u00df man ihm einen balg legt als gemahlin bey.", "tokens": ["Da\u00df", "man", "ihm", "ei\u00b7nen", "balg", "legt", "als", "ge\u00b7mah\u00b7lin", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ART", "NN", "VVFIN", "KOKOM", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Ach! m\u00f6chte diese nacht mir vor zu grabe leuchten,", "tokens": ["Ach", "!", "m\u00f6ch\u00b7te", "die\u00b7se", "nacht", "mir", "vor", "zu", "gra\u00b7be", "leuch\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PDAT", "NN", "PPER", "APPR", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Eh man ihr morgen steckt die hochzeit-fackeln an!", "tokens": ["Eh", "man", "ihr", "mor\u00b7gen", "steckt", "die", "hoch\u00b7zeit\u00b7fa\u00b7ckeln", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.71": {"text": "Werd\u2019 ich mit thr\u00e4nen wohl genung die wangen feuchten,", "tokens": ["Werd'", "ich", "mit", "thr\u00e4\u00b7nen", "wohl", "ge\u00b7nung", "die", "wan\u00b7gen", "feuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Wo ich die greuel-eh ja noch erleben kan?", "tokens": ["Wo", "ich", "die", "greu\u00b7el\u00b7eh", "ja", "noch", "er\u00b7le\u00b7ben", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Gott schick es, wie er will, doch soll kein mensch erleben,", "tokens": ["Gott", "schick", "es", ",", "wie", "er", "will", ",", "doch", "soll", "kein", "mensch", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$,", "ADV", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Da\u00df ich und meine magd solln neben-buhler seyn;", "tokens": ["Da\u00df", "ich", "und", "mei\u00b7ne", "magd", "solln", "ne\u00b7ben\u00b7buh\u00b7ler", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PPOSAT", "NN", "VMFIN", "NE", "VAINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.75": {"text": "Wer einem g\u00f6tzen hat gold und gestalt gegeben,", "tokens": ["Wer", "ei\u00b7nem", "g\u00f6t\u00b7zen", "hat", "gold", "und", "ge\u00b7stalt", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.76": {"text": "Wird schwerlich aufs altar ihm glimmend weyrauch streun.", "tokens": ["Wird", "schwer\u00b7lich", "aufs", "al\u00b7tar", "ihm", "glim\u00b7mend", "wey\u00b7rauch", "streun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPRART", "NN", "PPER", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Du aber opfferst ihr, durchlauchster, seel und hertzen,", "tokens": ["Du", "a\u00b7ber", "opf\u00b7ferst", "ihr", ",", "durch\u00b7lauchs\u00b7ter", ",", "seel", "und", "hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "ADJD", "$,", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Hebst staub und koth ans brett, der dich selbst fleckicht macht.", "tokens": ["Hebst", "staub", "und", "koth", "ans", "brett", ",", "der", "dich", "selbst", "flec\u00b7kicht", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPRART", "ADJD", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Verh\u00fcll\u2019n die wolcken doch die g\u00f6ldnen sonnen-kertzen,", "tokens": ["Ver\u00b7h\u00fcll'n", "die", "wol\u00b7cken", "doch", "die", "g\u00f6ld\u00b7nen", "son\u00b7nen\u00b7kert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Die sie aus sc