{"dta.poem.15936": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "141.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist eben Welt, Welt \u00fcberall; sie kennen", "tokens": ["Die", "Welt", "ist", "e\u00b7ben", "Welt", ",", "Welt", "\u00fc\u00b7be\u00b7rall", ";", "sie", "ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "$,", "NN", "ADV", "$.", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu lernen, m\u00f6cht' ich sie nun f\u00fcrder nicht durchrennen.", "tokens": ["Zu", "ler\u00b7nen", ",", "m\u00f6cht'", "ich", "sie", "nun", "f\u00fcr\u00b7der", "nicht", "durch\u00b7ren\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VMFIN", "PPER", "PPER", "ADV", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was an ihr ist, hab' ich erkannt an einem Ende,", "tokens": ["Was", "an", "ihr", "ist", ",", "hab'", "ich", "er\u00b7kannt", "an", "ei\u00b7nem", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VAFIN", "$,", "VAFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mehr erkennt' ich nicht, wenn ich am andern st\u00e4nde.", "tokens": ["Und", "mehr", "er\u00b7kennt'", "ich", "nicht", ",", "wenn", "ich", "am", "an\u00b7dern", "st\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punc