{"textgrid.poem.25744": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht Flaccus Lied bedurfte des M\u00e4cens,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht Flaccus Lied bedurfte des M\u00e4cens,", "tokens": ["Nicht", "Flac\u00b7cus", "Lied", "be\u00b7durf\u00b7te", "des", "M\u00e4\u00b7cens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch Flaccus selbst bedurfte sein vielleicht.", "tokens": ["Doch", "Flac\u00b7cus", "selbst", "be\u00b7durf\u00b7te", "sein", "viel\u00b7leicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADV", "VVFIN", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wird bis zum Tag' des gro\u00dfen Wiedersehns", "tokens": ["Wird", "bis", "zum", "Tag'", "des", "gro\u00b7\u00dfen", "Wie\u00b7der\u00b7sehns"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht t\u00e4glich noch durch ", "tokens": ["Nicht", "t\u00e4g\u00b7lich", "noch", "durch"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "ADV", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Indessen ", "tokens": ["In\u00b7des\u00b7sen"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Trotz allem Schutz' der ", "tokens": ["Trotz", "al\u00b7lem", "Schutz'", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIS", "NN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Die Motte schon ein Seculum beschleicht?", "tokens": ["Die", "Mot\u00b7te", "schon", "ein", "Se\u00b7cu\u00b7lum", "be\u00b7schleicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wenn ein M\u00e4cen mein Lied vom Untergang'", "tokens": ["Wenn", "ein", "M\u00e4\u00b7cen", "mein", "Lied", "vom", "Un\u00b7ter\u00b7gang'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Nicht retten kann: wozu ihn sonst, wozu?", "tokens": ["Nicht", "ret\u00b7ten", "kann", ":", "wo\u00b7zu", "ihn", "sonst", ",", "wo\u00b7zu", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$.", "PWAV", "PPER", "ADV", "$,", "PWAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie schwankend wird des Dichters fester Gang", "tokens": ["Wie", "schwan\u00b7kend", "wird", "des", "Dich\u00b7ters", "fes\u00b7ter", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "In eines ", "tokens": ["In", "ei\u00b7nes"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Wie gl\u00fccklich bin ich, da\u00df ich keines", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "bin", "ich", ",", "da\u00df", "ich", "kei\u00b7nes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "M\u00e4cens bedarf! Bed\u00fcrft' ich Eines:", "tokens": ["M\u00e4\u00b7cens", "be\u00b7darf", "!", "Be\u00b7d\u00fcrft'", "ich", "Ei\u00b7nes", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "So w\u00fcrd' es, Freund, kein anderer \u2013 als du!", "tokens": ["So", "w\u00fcrd'", "es", ",", "Freund", ",", "kein", "an\u00b7de\u00b7rer", "\u2013", "als", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "$,", "PIAT", "ADJA", "$(", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Sollt' ich dem Mann' nicht meine Lieder weihn,", "tokens": ["Sollt'", "ich", "dem", "Mann'", "nicht", "mei\u00b7ne", "Lie\u00b7der", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Der selbst der Freud' und Weisheit Lieder singt?", "tokens": ["Der", "selbst", "der", "Freud'", "und", "Weis\u00b7heit", "Lie\u00b7der", "singt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "KON", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Sollt' ich nicht dem mein K\u00f6rnchen Weihrauch streun,", "tokens": ["Sollt'", "ich", "nicht", "dem", "mein", "K\u00f6rn\u00b7chen", "Weih\u00b7rauch", "streun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ART", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.18": {"text": "Der jeglichem Verdienst' so gern ein Opfer bringt?", "tokens": ["Der", "jeg\u00b7li\u00b7chem", "Ver\u00b7dienst'", "so", "gern", "ein", "Op\u00b7fer", "bringt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Bei dem mich sch\u00e4men einer Bitte", "tokens": ["Bei", "dem", "mich", "sch\u00e4\u00b7men", "ei\u00b7ner", "Bit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Um Milch und Brod und eine H\u00fctte,", "tokens": ["Um", "Milch", "und", "Brod", "und", "ei\u00b7ne", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Um dessen Gunst ein Thor vergebens ringt?", "tokens": ["Um", "des\u00b7sen", "Gunst", "ein", "Thor", "ver\u00b7ge\u00b7bens", "ringt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Du hast den Muth, am Hofe selbst zu seyn,", "tokens": ["Du", "hast", "den", "Muth", ",", "am", "Ho\u00b7fe", "selbst", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPRART", "NN", "ADV", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Was mancher nur auf seinem Zimmer ist,", "tokens": ["Was", "man\u00b7cher", "nur", "auf", "sei\u00b7nem", "Zim\u00b7mer", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Der, hier nicht karg mit s\u00fc\u00dfen Schmeichelein,", "tokens": ["Der", ",", "hier", "nicht", "karg", "mit", "s\u00fc\u00b7\u00dfen", "Schmei\u00b7che\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "PTKNEG", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Den ahnenlosen Mann bei Rittern bald vergi\u00dft;", "tokens": ["Den", "ah\u00b7nen\u00b7lo\u00b7sen", "Mann", "bei", "Rit\u00b7tern", "bald", "ver\u00b7gi\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Du aber hast den Freund, der \u00e4rmer", "tokens": ["Du", "a\u00b7ber", "hast", "den", "Freund", ",", "der", "\u00e4r\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "An Gl\u00fcck, als Flaccus war, noch w\u00e4rmer", "tokens": ["An", "Gl\u00fcck", ",", "als", "Flac\u00b7cus", "war", ",", "noch", "w\u00e4r\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUS", "NE", "VAFIN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Im Angesicht' des ganzen Hofs gek\u00fc\u00dft.", "tokens": ["Im", "An\u00b7ge\u00b7sicht'", "des", "gan\u00b7zen", "Hofs", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Wenn an dem Wahlspruch': Immer selbst sich gleich!", "tokens": ["Wenn", "an", "dem", "Wahl\u00b7spruch'", ":", "Im\u00b7mer", "selbst", "sich", "gleich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$.", "ADV", "ADV", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Ein Sterblicher das N\u00e4herrecht besitzt:", "tokens": ["Ein", "Sterb\u00b7li\u00b7cher", "das", "N\u00e4\u00b7her\u00b7recht", "be\u00b7sitzt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "So bist es du! Nur f\u00fcr die Tugend weich,", "tokens": ["So", "bist", "es", "du", "!", "Nur", "f\u00fcr", "die", "Tu\u00b7gend", "weich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$.", "ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Hat nie ein Pfeil des Gl\u00fccks noch deine Brust geritzt;", "tokens": ["Hat", "nie", "ein", "Pfeil", "des", "Gl\u00fccks", "noch", "dei\u00b7ne", "Brust", "ge\u00b7ritzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Selbst, wenn's zum F\u00fcrstenstuhl' dich f\u00fchret,", "tokens": ["Selbst", ",", "wenn's", "zum", "F\u00fcrs\u00b7ten\u00b7stuhl'", "dich", "f\u00fch\u00b7ret", ","], "token_info": ["word", "punct", "word