{"textgrid.poem.61395": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Die T\u00fcrkenkugel", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der H\u00f6h' am Felsenkirchlein,", "tokens": ["Auf", "der", "H\u00f6h'", "am", "Fel\u00b7sen\u00b7kirch\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Rings vom T\u00fcrkenheer umschlossen,", "tokens": ["Rings", "vom", "T\u00fcr\u00b7ken\u00b7heer", "um\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Liegt ein H\u00e4uflein tapfrer Griechen", "tokens": ["Liegt", "ein", "H\u00e4uf\u00b7lein", "tapf\u00b7rer", "Grie\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von des Bozzaris Genossen.", "tokens": ["Von", "des", "Boz\u00b7za\u00b7ris", "Ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Achtmal hat die Schar dort oben", "tokens": ["Acht\u00b7mal", "hat", "die", "Schar", "dort", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon begr\u00fc\u00dft den Strahl der Sonnen;", "tokens": ["Schon", "be\u00b7gr\u00fc\u00dft", "den", "Strahl", "der", "Son\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Achtmal schon ergimmten Mutes", "tokens": ["Acht\u00b7mal", "schon", "er\u00b7gimm\u00b7ten", "Mu\u00b7tes"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat der Feind den Sturm begonnen.", "tokens": ["Hat", "der", "Feind", "den", "Sturm", "be\u00b7gon\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch vergeblich in den Schluchten", "tokens": ["Doch", "ver\u00b7geb\u00b7lich", "in", "den", "Schluch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4uft' er Tote nur zu Toten,", "tokens": ["H\u00e4uft'", "er", "To\u00b7te", "nur", "zu", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der Fels ist schroff, und sicher", "tokens": ["Denn", "der", "Fels", "ist", "schroff", ",", "und", "si\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$,", "KON", "ADJD"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Trifft das Blei der Sulioten.", "tokens": ["Trifft", "das", "Blei", "der", "Su\u00b7li\u00b7o\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum von fern aus Feuerschl\u00fcnden", "tokens": ["Drum", "von", "fern", "aus", "Feu\u00b7er\u00b7schl\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will er nun Verderben senden;", "tokens": ["Will", "er", "nun", "Ver\u00b7der\u00b7ben", "sen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kugeln \u00fcber Kugeln wirft er", "tokens": ["Ku\u00b7geln", "\u00fc\u00b7ber", "Ku\u00b7geln", "wirft", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach den steilen Felsenw\u00e4nden.", "tokens": ["Nach", "den", "stei\u00b7len", "Fel\u00b7sen\u00b7w\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber mag sein gl\u00fchend Eisen", "tokens": ["A\u00b7ber", "mag", "sein", "gl\u00fc\u00b7hend", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seltnes Opfer nur erreichen:", "tokens": ["Selt\u00b7nes", "Op\u00b7fer", "nur", "er\u00b7rei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schon beginnt ein andrer W\u00fcrger", "tokens": ["Schon", "be\u00b7ginnt", "ein", "an\u00b7drer", "W\u00fcr\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Droben durch die Schar zu schleichen.", "tokens": ["Dro\u00b7ben", "durch", "die", "Schar", "zu", "schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Grauser als von Feindeswaffen", "tokens": ["Grau\u00b7ser", "als", "von", "Fein\u00b7des\u00b7waf\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist der Tod von Durstesqualen;", "tokens": ["Ist", "der", "Tod", "von", "Durs\u00b7tes\u00b7qua\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keinen Brunnen hat der Felsen,", "tokens": ["Kei\u00b7nen", "Brun\u00b7nen", "hat", "der", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und geleert sind Schl\u00e4uch' und Schalen.", "tokens": ["Und", "ge\u00b7leert", "sind", "Schl\u00e4uch'", "und", "Scha\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "NN", "KON", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Und der Himmel blau und ehern", "tokens": ["Und", "der", "Him\u00b7mel", "blau", "und", "e\u00b7hern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaut herab mit Feueraugen;", "tokens": ["Schaut", "her\u00b7ab", "mit", "Feu\u00b7er\u00b7au\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, nicht reicht's, da\u00df von den Halmen", "tokens": ["Ach", ",", "nicht", "reicht's", ",", "da\u00df", "von", "den", "Hal\u00b7men"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PTKNEG", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie den Tau der Fr\u00fche saugen.", "tokens": ["Sie", "den", "Tau", "der", "Fr\u00fc\u00b7he", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Bleich, mit hohlen Wangen, schwanken", "tokens": ["Bleich", ",", "mit", "hoh\u00b7len", "Wan\u00b7gen", ",", "schwan\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um das Kirchlein die Gestalten:", "tokens": ["Um", "das", "Kirch\u00b7lein", "die", "Ge\u00b7stal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kaum vermag der Arm entkr\u00e4ftet", "tokens": ["Kaum", "ver\u00b7mag", "der", "Arm", "ent\u00b7kr\u00e4f\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch das lange Rohr zu halten.", "tokens": ["Noch", "das", "lan\u00b7ge", "Rohr", "zu", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Dorrend klebt die Zung' am Gaumen,", "tokens": ["Dor\u00b7rend", "klebt", "die", "Zung'", "am", "Gau\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fieberglut durchrast die Glieder;", "tokens": ["Fie\u00b7berg\u00b7lut", "durc\u00b7hrast", "die", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Not des neunten Abends", "tokens": ["In", "der", "Not", "des", "neun\u00b7ten", "A\u00b7bends"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werfen sie sich flehend nieder:", "tokens": ["Wer\u00b7fen", "sie", "sich", "fle\u00b7hend", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbder du Mosis Stab gesegnet,", "tokens": ["\u00bb", "der", "du", "Mo\u00b7sis", "Stab", "ge\u00b7seg\u00b7net", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NE", "NE", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df er Wasser schuf dem Volke,", "tokens": ["Da\u00df", "er", "Was\u00b7ser", "schuf", "dem", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der du auf Elias' Rufen", "tokens": ["Der", "du", "auf", "E\u00b7li\u00b7as'", "Ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NE", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Kamst in schatt'ger Regenwolke,", "tokens": ["Kamst", "in", "schatt'\u00b7ger", "Re\u00b7gen\u00b7wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Herr, erbarm', erbarm' dich unser!", "tokens": ["Herr", ",", "er\u00b7barm'", ",", "er\u00b7barm'", "dich", "un\u00b7ser", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh, wir sind wie trockne Scherben, \u2013", "tokens": ["Sieh", ",", "wir", "sind", "wie", "trock\u00b7ne", "Scher\u00b7ben", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "KOKOM", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von des Feindes Schwert errettet,", "tokens": ["Von", "des", "Fein\u00b7des", "Schwert", "er\u00b7ret\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df uns nicht im Durst verderben!\u00ab", "tokens": ["La\u00df", "uns", "nicht", "im", "Durst", "ver\u00b7der\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und noch hallt es: \u00bbHerr, erbarm' dich!\u00ab", "tokens": ["Und", "noch", "hallt", "es", ":", "\u00bb", "Herr", ",", "er\u00b7barm'", "dich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "$(", "NN", "$,", "VVFIN", "PPER", "$.", "$("], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Da in rotgew\u00f6lbtem Bogen", "tokens": ["Da", "in", "rot\u00b7ge\u00b7w\u00f6lb\u00b7tem", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem T\u00fcrkenlager sausend", "tokens": ["Aus", "dem", "T\u00fcr\u00b7ken\u00b7la\u00b7ger", "sau\u00b7send"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt ein Feuerball geflogen.", "tokens": ["Kommt", "ein", "Feu\u00b7er\u00b7ball", "ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Dr\u00f6hnend schl\u00e4gt er in die Klippe,", "tokens": ["Dr\u00f6h\u00b7nend", "schl\u00e4gt", "er", "in", "die", "Klip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bohrt sich w\u00fchlend tief und tiefer, \u2013", "tokens": ["Bohrt", "sich", "w\u00fch\u00b7lend", "tief", "und", "tie\u00b7fer", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ADJD", "KON", "ADJD", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Horch, da zischt es leis, und silbern", "tokens": ["Horch", ",", "da", "zischt", "es", "leis", ",", "und", "sil\u00b7bern"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zuckt es auf im Felsgeschiefer:", "tokens": ["Zuckt", "es", "auf", "im", "Fels\u00b7ge\u00b7schie\u00b7fer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Und es blinkt und rinnt und rieselt,", "tokens": ["Und", "es", "blinkt", "und", "rinnt", "und", "rie\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit Brausen dann geschossen", "tokens": ["Und", "mit", "Brau\u00b7sen", "dann", "ge\u00b7schos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Well' auf Welle kommt das Wasser,", "tokens": ["Well'", "auf", "Wel\u00b7le", "kommt", "das", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem das Erz die Bahn erschlossen.", "tokens": ["Dem", "das", "Erz", "die", "Bahn", "er\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "O wie lieblich rauscht der Sprudel", "tokens": ["O", "wie", "lieb\u00b7lich", "rauscht", "der", "Spru\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In das Ohr der Kriegsgef\u00e4hrten!", "tokens": ["In", "das", "Ohr", "der", "Kriegs\u00b7ge\u00b7f\u00e4hr\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O wie schl\u00fcrfen sie mit Wonnen", "tokens": ["O", "wie", "schl\u00fcr\u00b7fen", "sie", "mit", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem Na\u00df, dem langentbehrten!", "tokens": ["Von", "dem", "Na\u00df", ",", "dem", "lan\u00b7gent\u00b7behr\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Aber dann zum frommen Danke", "tokens": ["A\u00b7ber", "dann", "zum", "from\u00b7men", "Dan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Siehst du sie die H\u00e4nde falten:", "tokens": ["Siehst", "du", "sie", "die", "H\u00e4n\u00b7de", "fal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbsei gepriesen, Herr der Gnaden!", "tokens": ["\u00bb", "sei", "ge\u00b7prie\u00b7sen", ",", "Herr", "der", "Gna\u00b7den", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "VVPP", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wundervoll ist all dein Walten.", "tokens": ["Wun\u00b7der\u00b7voll", "ist", "all", "dein", "Wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Durch die Hand des grimmsten Feindes", "tokens": ["Durch", "die", "Hand", "des", "grimms\u00b7ten", "Fein\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00dft du Trost und Heil zu geben;", "tokens": ["Wei\u00dft", "du", "Trost", "und", "Heil", "zu", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tod gedacht' er uns zu senden,", "tokens": ["Tod", "ge\u00b7dacht'", "er", "uns", "zu", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch du wandtest Tod in Leben!\u00ab", "tokens": ["Doch", "du", "wand\u00b7test", "Tod", "in", "Le\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Auf der H\u00f6h' am Felsenkirchlein,", "tokens": ["Auf", "der", "H\u00f6h'", "am", "Fel\u00b7sen\u00b7kirch\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Rings vom T\u00fcrkenheer umschlossen,", "tokens": ["Rings", "vom", "T\u00fcr\u00b7ken\u00b7heer", "um\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Liegt ein H\u00e4uflein tapfrer Griechen", "tokens": ["Liegt", "ein", "H\u00e4uf\u00b7lein", "tapf\u00b7rer", "Grie\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von des Bozzaris Genossen.", "tokens": ["Von", "des", "Boz\u00b7za\u00b7ris", "Ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Achtmal hat die Schar dort oben", "tokens": ["Acht\u00b7mal", "hat", "die", "Schar", "dort", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon begr\u00fc\u00dft den Strahl der Sonnen;", "tokens": ["Schon", "be\u00b7gr\u00fc\u00dft", "den", "Strahl", "der", "Son\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Achtmal schon ergimmten Mutes", "tokens": ["Acht\u00b7mal", "schon", "er\u00b7gimm\u00b7ten", "Mu\u00b7tes"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat der Feind den Sturm begonnen.", "tokens": ["Hat", "der", "Feind", "den", "Sturm", "be\u00b7gon\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Doch vergeblich in den Schluchten", "tokens": ["Doch", "ver\u00b7geb\u00b7lich", "in", "den", "Schluch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4uft' er Tote nur zu Toten,", "tokens": ["H\u00e4uft'", "er", "To\u00b7te", "nur", "zu", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der Fels ist schroff, und sicher", "tokens": ["Denn", "der", "Fels", "ist", "schroff", ",", "und", "si\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$,", "KON", "ADJD"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Trifft das Blei der Sulioten.", "tokens": ["Trifft", "das", "Blei", "der", "Su\u00b7li\u00b7o\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Drum von fern aus Feuerschl\u00fcnden", "tokens": ["Drum", "von", "fern", "aus", "Feu\u00b7er\u00b7schl\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will er nun Verderben senden;", "tokens": ["Will", "er", "nun", "Ver\u00b7der\u00b7ben", "sen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kugeln \u00fcber Kugeln wirft er", "tokens": ["Ku\u00b7geln", "\u00fc\u00b7ber", "Ku\u00b7geln", "wirft", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach den steilen Felsenw\u00e4nden.", "tokens": ["Nach", "den", "stei\u00b7len", "Fel\u00b7sen\u00b7w\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Aber mag sein gl\u00fchend Eisen", "tokens": ["A\u00b7ber", "mag", "sein", "gl\u00fc\u00b7hend", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seltnes Opfer nur erreichen:", "tokens": ["Selt\u00b7nes", "Op\u00b7fer", "nur", "er\u00b7rei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schon beginnt ein andrer W\u00fcrger", "tokens": ["Schon", "be\u00b7ginnt", "ein", "an\u00b7drer", "W\u00fcr\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Droben durch die Schar zu schleichen.", "tokens": ["Dro\u00b7ben", "durch", "die", "Schar", "zu", "schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Grauser als von Feindeswaffen", "tokens": ["Grau\u00b7ser", "als", "von", "Fein\u00b7des\u00b7waf\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist der Tod von Durstesqualen;", "tokens": ["Ist", "der", "Tod", "von", "Durs\u00b7tes\u00b7qua\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keinen Brunnen hat der Felsen,", "tokens": ["Kei\u00b7nen", "Brun\u00b7nen", "hat", "der", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und geleert sind Schl\u00e4uch' und Schalen.", "tokens": ["Und", "ge\u00b7leert", "sind", "Schl\u00e4uch'", "und", "Scha\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "NN", "KON", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.24": {"line.1": {"text": "Und der Himmel blau und ehern", "tokens": ["Und", "der", "Him\u00b7mel", "blau", "und", "e\u00b7hern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaut herab mit Feueraugen;", "tokens": ["Schaut", "her\u00b7ab", "mit", "Feu\u00b7er\u00b7au\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, nicht reicht's, da\u00df von den Halmen", "tokens": ["Ach", ",", "nicht", "reicht's", ",", "da\u00df", "von", "den", "Hal\u00b7men"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PTKNEG", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie den Tau der Fr\u00fche saugen.", "tokens": ["Sie", "den", "Tau", "der", "Fr\u00fc\u00b7he", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "Bleich, mit hohlen Wangen, schwanken", "tokens": ["Bleich", ",", "mit", "hoh\u00b7len", "Wan\u00b7gen", ",", "schwan\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um das Kirchlein die Gestalten:", "tokens": ["Um", "das", "Kirch\u00b7lein", "die", "Ge\u00b7stal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kaum vermag der Arm entkr\u00e4ftet", "tokens": ["Kaum", "ver\u00b7mag", "der", "Arm", "ent\u00b7kr\u00e4f\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch das lange Rohr zu halten.", "tokens": ["Noch", "das", "lan\u00b7ge", "Rohr", "zu", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Dorrend klebt die Zung' am Gaumen,", "tokens": ["Dor\u00b7rend", "klebt", "die", "Zung'", "am", "Gau\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fieberglut durchrast die Glieder;", "tokens": ["Fie\u00b7berg\u00b7lut", "durc\u00b7hrast", "die", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Not des neunten Abends", "tokens": ["In", "der", "Not", "des", "neun\u00b7ten", "A\u00b7bends"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werfen sie sich flehend nieder:", "tokens": ["Wer\u00b7fen", "sie", "sich", "fle\u00b7hend", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.27": {"line.1": {"text": "\u00bbder du Mosis Stab gesegnet,", "tokens": ["\u00bb", "der", "du", "Mo\u00b7sis", "Stab", "ge\u00b7seg\u00b7net", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NE", "NE", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df er Wasser schuf dem Volke,", "tokens": ["Da\u00df", "er", "Was\u00b7ser", "schuf", "dem", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der du auf Elias' Rufen", "tokens": ["Der", "du", "auf", "E\u00b7li\u00b7as'", "Ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NE", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Kamst in schatt'ger Regenwolke,", "tokens": ["Kamst", "in", "schatt'\u00b7ger", "Re\u00b7gen\u00b7wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.28": {"line.1": {"text": "Herr, erbarm', erbarm' dich unser!", "tokens": ["Herr", ",", "er\u00b7barm'", ",", "er\u00b7barm'", "dich", "un\u00b7ser", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh, wir sind wie trockne Scherben, \u2013", "tokens": ["Sieh", ",", "wir", "sind", "wie", "trock\u00b7ne", "Scher\u00b7ben", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "KOKOM", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von des Feindes Schwert errettet,", "tokens": ["Von", "des", "Fein\u00b7des", "Schwert", "er\u00b7ret\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df uns nicht im Durst verderben!\u00ab", "tokens": ["La\u00df", "uns", "nicht", "im", "Durst", "ver\u00b7der\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.29": {"line.1": {"text": "Und noch hallt es: \u00bbHerr, erbarm' dich!\u00ab", "tokens": ["Und", "noch", "hallt", "es", ":", "\u00bb", "Herr", ",", "er\u00b7barm'", "dich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "$(", "NN", "$,", "VVFIN", "PPER", "$.", "$("], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Da in rotgew\u00f6lbtem Bogen", "tokens": ["Da", "in", "rot\u00b7ge\u00b7w\u00f6lb\u00b7tem", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem T\u00fcrkenlager sausend", "tokens": ["Aus", "dem", "T\u00fcr\u00b7ken\u00b7la\u00b7ger", "sau\u00b7send"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt ein Feuerball geflogen.", "tokens": ["Kommt", "ein", "Feu\u00b7er\u00b7ball", "ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.30": {"line.1": {"text": "Dr\u00f6hnend schl\u00e4gt er in die Klippe,", "tokens": ["Dr\u00f6h\u00b7nend", "schl\u00e4gt", "er", "in", "die", "Klip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bohrt sich w\u00fchlend tief und tiefer, \u2013", "tokens": ["Bohrt", "sich", "w\u00fch\u00b7lend", "tief", "und", "tie\u00b7fer", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ADJD", "KON", "ADJD", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Horch, da zischt es leis, und silbern", "tokens": ["Horch", ",", "da", "zischt", "es", "leis", ",", "und", "sil\u00b7bern"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zuckt es auf im Felsgeschiefer:", "tokens": ["Zuckt", "es", "auf", "im", "Fels\u00b7ge\u00b7schie\u00b7fer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.31": {"line.1": {"text": "Und es blinkt und rinnt und rieselt,", "tokens": ["Und", "es", "blinkt", "und", "rinnt", "und", "rie\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit Brausen dann geschossen", "tokens": ["Und", "mit", "Brau\u00b7sen", "dann", "ge\u00b7schos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Well' auf Welle kommt das Wasser,", "tokens": ["Well'", "auf", "Wel\u00b7le", "kommt", "das", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem das Erz die Bahn erschlossen.", "tokens": ["Dem", "das", "Erz", "die", "Bahn", "er\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.32": {"line.1": {"text": "O wie lieblich rauscht der Sprudel", "tokens": ["O", "wie", "lieb\u00b7lich", "rauscht", "der", "Spru\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In das Ohr der Kriegsgef\u00e4hrten!", "tokens": ["In", "das", "Ohr", "der", "Kriegs\u00b7ge\u00b7f\u00e4hr\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O wie schl\u00fcrfen sie mit Wonnen", "tokens": ["O", "wie", "schl\u00fcr\u00b7fen", "sie", "mit", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem Na\u00df, dem langentbehrten!", "tokens": ["Von", "dem", "Na\u00df", ",", "dem", "lan\u00b7gent\u00b7behr\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.33": {"line.1": {"text": "Aber dann zum frommen Danke", "tokens": ["A\u00b7ber", "dann", "zum", "from\u00b7men", "Dan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Siehst du sie die H\u00e4nde falten:", "tokens": ["Siehst", "du", "sie", "die", "H\u00e4n\u00b7de", "fal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbsei gepriesen, Herr der Gnaden!", "tokens": ["\u00bb", "sei", "ge\u00b7prie\u00b7sen", ",", "Herr", "der", "Gna\u00b7den", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "VVPP", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wundervoll ist all dein Walten.", "tokens": ["Wun\u00b7der\u00b7voll", "ist", "all", "dein", "Wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.34": {"line.1": {"text": "Durch die Hand des grimmsten Feindes", "tokens": ["Durch", "die", "Hand", "des", "grimms\u00b7ten", "Fein\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00dft du Trost und Heil zu geben;", "tokens": ["Wei\u00dft", "du", "Trost", "und", "Heil", "zu", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tod gedacht' er uns zu senden,", "tokens": ["Tod", "ge\u00b7dacht'", "er", "uns", "zu", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch du wandtest Tod in Leben!\u00ab", "tokens": ["Doch", "du", "wand\u00b7test", "Tod", "in", "Le\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}