{"textgrid.poem.65077": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Warum ich herzenskrank und wund", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum ich herzenskrank und wund", "tokens": ["Wa\u00b7rum", "ich", "her\u00b7zen\u00b7skrank", "und", "wund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus deinen Armen mich gerissen?", "tokens": ["Aus", "dei\u00b7nen", "Ar\u00b7men", "mich", "ge\u00b7ris\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wohlan! so magst du nun den Grund", "tokens": ["Wo\u00b7hlan", "!", "so", "magst", "du", "nun", "den", "Grund"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "ADV", "VMFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des schmerzlichen Beginnens wissen.", "tokens": ["Des", "schmerz\u00b7li\u00b7chen", "Be\u00b7gin\u00b7nens", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was aus dem stillen Paradies", "tokens": ["Was", "aus", "dem", "stil\u00b7len", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem lang ersehnten, mich getrieben,", "tokens": ["Dem", "lang", "er\u00b7sehn\u00b7ten", ",", "mich", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was deine N\u00e4h' mich meiden hie\u00df,", "tokens": ["Was", "dei\u00b7ne", "N\u00e4h'", "mich", "mei\u00b7den", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es war ein unerme\u00dflich Lieben.", "tokens": ["Es", "war", "ein", "un\u00b7er\u00b7me\u00df\u00b7lich", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dir selber war's noch nicht bewu\u00dft,", "tokens": ["Dir", "sel\u00b7ber", "wa\u00b7r's", "noch", "nicht", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du wagtest nicht dir's zu gestehen,", "tokens": ["Du", "wag\u00b7test", "nicht", "dir's", "zu", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df die Empfindung deiner Brust", "tokens": ["Da\u00df", "die", "Emp\u00b7fin\u00b7dung", "dei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Schwinden, ach! und im Vergehen.", "tokens": ["Im", "Schwin\u00b7den", ",", "ach", "!", "und", "im", "Ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ITJ", "$.", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gleich blieb sich deines Auges Gru\u00df,", "tokens": ["Gleich", "blieb", "sich", "dei\u00b7nes", "Au\u00b7ges", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gleich der Lippe z\u00e4rtlich Flehen,", "tokens": ["Und", "gleich", "der", "Lip\u00b7pe", "z\u00e4rt\u00b7lich", "Fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch, wehe mir! durch deinen Ku\u00df", "tokens": ["Doch", ",", "we\u00b7he", "mir", "!", "durch", "dei\u00b7nen", "Ku\u00df"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "VVFIN", "PPER", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchlt' ich den Hauch des Himmels wehen!", "tokens": ["F\u00fchlt'", "ich", "den", "Hauch", "des", "Him\u00b7mels", "we\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie aus dem Lied, das auf der Flur", "tokens": ["Wie", "aus", "dem", "Lied", ",", "das", "auf", "der", "Flur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wanderv\u00f6gel scheidend singen,", "tokens": ["Die", "Wan\u00b7der\u00b7v\u00f6\u00b7gel", "schei\u00b7dend", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So h\u00f6rte ich durch deinen Schwur", "tokens": ["So", "h\u00f6r\u00b7te", "ich", "durch", "dei\u00b7nen", "Schwur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Schmerzenston des Abschieds klingen.", "tokens": ["Den", "Schmer\u00b7zen\u00b7ston", "des", "Ab\u00b7schieds", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und in der Rose duft'gem Schoo\u00df", "tokens": ["Und", "in", "der", "Ro\u00b7se", "duft'\u00b7gem", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah ich den Wurm des Todesnagen \u2013", "tokens": ["Sah", "ich", "den", "Wurm", "des", "To\u00b7des\u00b7na\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da ri\u00df ich mich entschlossen los,", "tokens": ["Da", "ri\u00df", "ich", "mich", "ent\u00b7schlos\u00b7sen", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um nicht die letzte Schmach zu tragen.", "tokens": ["Um", "nicht", "die", "letz\u00b7te", "Schmach", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKNEG", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Liebe, die uns einst vereint,", "tokens": ["Der", "Lie\u00b7be", ",", "die", "uns", "einst", "ver\u00b7eint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wollt' ich ein sch\u00f6ner Loos erwerben,", "tokens": ["Wollt'", "ich", "ein", "sch\u00f6\u00b7ner", "Loos", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als unbetrauert, unbeweint,", "tokens": ["Als", "un\u00b7be\u00b7trau\u00b7ert", ",", "un\u00b7be\u00b7weint", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verwelkt und matt dahin zu sterben.", "tokens": ["Ver\u00b7welkt", "und", "matt", "da\u00b7hin", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Einpr\u00e4gen wollt' ich ihre Spur", "tokens": ["Ein\u00b7pr\u00e4\u00b7gen", "wollt'", "ich", "ih\u00b7re", "Spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf immerdar in dein Ged\u00e4chtni\u00df,", "tokens": ["Auf", "im\u00b7mer\u00b7dar", "in", "dein", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", ","], "t