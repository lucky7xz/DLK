{"textgrid.poem.41473": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Tag der Freude", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ergebet euch mit freiem Herzen", "tokens": ["Er\u00b7ge\u00b7bet", "euch", "mit", "frei\u00b7em", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der jugendlichen Fr\u00f6hlichkeit:", "tokens": ["Der", "ju\u00b7gend\u00b7li\u00b7chen", "Fr\u00f6h\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verschiebet nicht das s\u00fc\u00dfe Scherzen,", "tokens": ["Ver\u00b7schie\u00b7bet", "nicht", "das", "s\u00fc\u00b7\u00dfe", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Freunde, bis ihr \u00e4lter seid.", "tokens": ["Ihr", "Freun\u00b7de", ",", "bis", "ihr", "\u00e4l\u00b7ter", "seid", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Euch lockt die Regung holder Triebe;", "tokens": ["Euch", "lockt", "die", "Re\u00b7gung", "hol\u00b7der", "Trie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die\u00df soll ein Tag der Wollust sein:", "tokens": ["Die\u00df", "soll", "ein", "Tag", "der", "Wol\u00b7lust", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf! ladet hier den Gott der Liebe,", "tokens": ["Auf", "!", "la\u00b7det", "hier", "den", "Gott", "der", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf! ladet hier die Freuden ein.", "tokens": ["Auf", "!", "la\u00b7det", "hier", "die", "Freu\u00b7den", "ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Umkr\u00e4nzt mit Rosen eure Scheitel", "tokens": ["Um\u00b7kr\u00e4nzt", "mit", "Ro\u00b7sen", "eu\u00b7re", "Schei\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "(noch stehen euch die Rosen gut)", "tokens": ["(", "noch", "ste\u00b7hen", "euch", "die", "Ro\u00b7sen", "gut", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nennet kein Vergn\u00fcgen eitel,", "tokens": ["Und", "nen\u00b7net", "kein", "Ver\u00b7gn\u00fc\u00b7gen", "ei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Wein und Liebe Vorschub thut.", "tokens": ["Dem", "Wein", "und", "Lie\u00b7be", "Vor\u00b7schub", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was kann das Todtenreich gestatten?", "tokens": ["Was", "kann", "das", "Tod\u00b7ten\u00b7reich", "ge\u00b7stat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nein! lebend mu\u00df man fr\u00f6hlich sein.", "tokens": ["Nein", "!", "le\u00b7bend", "mu\u00df", "man", "fr\u00f6h\u00b7lich", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADJD", "VMFIN", "PIS", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dort herzen wir nur kalte Schatten:", "tokens": ["Dort", "her\u00b7zen", "wir", "nur", "kal\u00b7te", "Schat\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dort trinkt man Wasser, und nicht Wein.", "tokens": ["Dort", "trinkt", "man", "Was\u00b7ser", ",", "und", "nicht", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$,", "KON", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Seht! Phyllis kommt: O neues Gl\u00fccke!", "tokens": ["Seht", "!", "Phyl\u00b7lis", "kommt", ":", "O", "neu\u00b7es", "Gl\u00fc\u00b7cke", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NE", "VVFIN", "$.", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf! Liebe, zeige deine Kunst,", "tokens": ["Auf", "!", "Lie\u00b7be", ",", "zei\u00b7ge", "dei\u00b7ne", "Kunst", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bereichre hier die sch\u00f6nsten Blicke", "tokens": ["Be\u00b7reich\u00b7re", "hier", "die", "sch\u00f6ns\u00b7ten", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Sehnsucht und mit Gegengunst.", "tokens": ["Mit", "Sehn\u00b7sucht", "und", "mit", "Ge\u00b7gen\u00b7gunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Phyllis! glaube meiner Lehre:", "tokens": ["O", "Phyl\u00b7lis", "!", "glau\u00b7be", "mei\u00b7ner", "Leh\u00b7re", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Herz mu\u00df unempfindlich sein.", "tokens": ["Kein", "Herz", "mu\u00df", "un\u00b7emp\u00b7find\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Spr\u00f6digkeit bringt etwas Ehre;", "tokens": ["Die", "Spr\u00f6\u00b7dig\u00b7keit", "bringt", "et\u00b7was", "Eh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch kann die Liebe mehr erfreun.", "tokens": ["Doch", "kann", "die", "Lie\u00b7be", "mehr", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Macht gereizter Z\u00e4rtlichkeiten,", "tokens": ["Die", "Macht", "ge\u00b7reiz\u00b7ter", "Z\u00e4rt\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Liebe schmeichelnde Gewalt,", "tokens": ["Der", "Lie\u00b7be", "schmei\u00b7cheln\u00b7de", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die werden doch dein Herz erbeuten;", "tokens": ["Die", "wer\u00b7den", "doch", "dein", "Herz", "er\u00b7beu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und du ergibst dich nicht zu bald.", "tokens": ["Und", "du", "er\u00b7gibst", "dich", "nicht", "zu", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKA", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir wollen heute dir vor allen", "tokens": ["Wir", "wol\u00b7len", "heu\u00b7te", "dir", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Lieder und die W\u00fcnsche weihn.", "tokens": ["Die", "Lie\u00b7der", "und", "die", "W\u00fcn\u00b7sche", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O k\u00f6nnten K\u00fcsse dir gefallen", "tokens": ["O", "k\u00f6nn\u00b7ten", "K\u00fcs\u00b7se", "dir", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und deiner Lippen w\u00fcrdig sein!", "tokens": ["Und", "dei\u00b7ner", "Lip\u00b7pen", "w\u00fcr\u00b7dig", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Wein, den ich dir \u00fcberreiche,", "tokens": ["Der", "Wein", ",", "den", "ich", "dir", "\u00fc\u00b7berr\u00b7ei\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist nicht vom herben Alter schwer.", "tokens": ["Ist", "nicht", "vom", "her\u00b7ben", "Al\u00b7ter", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPRART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch, da\u00df ich dich mit ihm vergleiche,", "tokens": ["Doch", ",", "da\u00df", "ich", "dich", "mit", "ihm", "ver\u00b7glei\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sei jung und feurig, so wie er.", "tokens": ["Sei", "jung", "und", "feu\u00b7rig", ",", "so", "wie", "er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "ADJD", "$,", "ADV", "KOKOM", "PPER", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "So kann man dich vollkommen nennen:", "tokens": ["So", "kann", "man", "dich", "voll\u00b7kom\u00b7men", "nen\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So darf die Jugend uns erfreun,", "tokens": ["So", "darf", "die", "Ju\u00b7gend", "uns", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ich der Liebe selbst bekennen:", "tokens": ["Und", "ich", "der", "Lie\u00b7be", "selbst", "be\u00b7ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf Phyllis K\u00fcsse schmeckt der Wein.", "tokens": ["Auf", "Phyl\u00b7lis", "K\u00fcs\u00b7se", "schmeckt", "der", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ergebet euch mit freiem Herzen", "tokens": ["Er\u00b7ge\u00b7bet", "euch", "mit", "frei\u00b7em", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der jugendlichen Fr\u00f6hlichkeit:", "tokens": ["Der", "ju\u00b7gend\u00b7li\u00b7chen", "Fr\u00f6h\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "