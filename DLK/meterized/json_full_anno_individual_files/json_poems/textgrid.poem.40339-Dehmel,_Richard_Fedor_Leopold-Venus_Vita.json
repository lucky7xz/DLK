{"textgrid.poem.40339": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Venus Vita", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und nun ein Feldweg, und um Morgengrauen;", "tokens": ["Und", "nun", "ein", "Feld\u00b7weg", ",", "und", "um", "Mor\u00b7gen\u00b7grau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "KON", "APPR", "NN", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "die kahlen B\u00e4ume stehen da wie tot,", "tokens": ["die", "kah\u00b7len", "B\u00e4u\u00b7me", "ste\u00b7hen", "da", "wie", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ich aber wandre, ohne aufzuschauen.", "tokens": ["ich", "a\u00b7ber", "wand\u00b7re", ",", "oh\u00b7ne", "auf\u00b7zu\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "KOUI", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fchle eine Furcht; und Regen droht.", "tokens": ["Ich", "f\u00fch\u00b7le", "ei\u00b7ne", "Furcht", ";", "und", "Re\u00b7gen", "droht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich h\u00f6re den ged\u00fcngten Acker schweigen;", "tokens": ["Ich", "h\u00f6\u00b7re", "den", "ge\u00b7d\u00fcng\u00b7ten", "A\u00b7cker", "schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und heute wird kein Morgenrot.", "tokens": ["und", "heu\u00b7te", "wird", "kein", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Stra\u00dfe teilt sich. In den schwarzen Zweigen", "tokens": ["Die", "Stra\u00b7\u00dfe", "teilt", "sich", ".", "In", "den", "schwar\u00b7zen", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sagt keine Tafel mir die rechte Spur:", "tokens": ["sagt", "kei\u00b7ne", "Ta\u00b7fel", "mir", "die", "rech\u00b7te", "Spur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "soll ich hinunter, soll ich steigen?", "tokens": ["soll", "ich", "hin\u00b7un\u00b7ter", ",", "soll", "ich", "stei\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKVZ", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da deucht mir, in der tiefen Flur", "tokens": ["Da", "deucht", "mir", ",", "in", "der", "tie\u00b7fen", "Flur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "rief mich mein Name, aus ersticktem Munde.", "tokens": ["rief", "mich", "mein", "Na\u00b7me", ",", "aus", "er\u00b7stick\u00b7tem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich horche; Nichts. Im Osten nur", "tokens": ["Ich", "hor\u00b7che", ";", "Nichts", ".", "Im", "Os\u00b7ten", "nur"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PIS", "$.", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "enttaucht ein Licht dem fernen blassen Grunde.", "tokens": ["ent\u00b7taucht", "ein", "Licht", "dem", "fer\u00b7nen", "blas\u00b7sen", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ist kein Stern, es schimmert warm und traut,", "tokens": ["Es", "ist", "kein", "Stern", ",", "es", "schim\u00b7mert", "warm", "und", "traut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PPER", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "mir d\u00e4mmert eine l\u00e4ngst vergangne Stunde,", "tokens": ["mir", "d\u00e4m\u00b7mert", "ei\u00b7ne", "l\u00e4ngst", "ver\u00b7gang\u00b7ne", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "und wieder h\u00f6r ich fern und laut", "tokens": ["und", "wie\u00b7der", "h\u00f6r", "ich", "fern", "und", "laut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die bange Stimme meinen Namen rufen;", "tokens": ["die", "ban\u00b7ge", "Stim\u00b7me", "mei\u00b7nen", "Na\u00b7men", "ru\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und mir graut.", "tokens": ["und", "mir", "graut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Mir scheinen pl\u00f6tzlich diese Ackerhufen", "tokens": ["Mir", "schei\u00b7nen", "pl\u00f6tz\u00b7lich", "die\u00b7se", "A\u00b7cker\u00b7hu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "bekannt; ich bin so wandermatt.", "tokens": ["be\u00b7kannt", ";", "ich", "bin", "so", "wan\u00b7der\u00b7matt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PPER", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dieser Pfad, und diese Wurzelstufen?", "tokens": ["Und", "die\u00b7ser", "Pfad", ",", "und", "die\u00b7se", "Wur\u00b7zel\u00b7stu\u00b7fen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "KON", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Hinab! \u2013 Schon wird der Abhang glatt;", "tokens": ["Hin\u00b7ab", "!", "\u2013", "Schon", "wird", "der", "Ab\u00b7hang", "glatt", ";"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "$(", "ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "auf einmal, wie von einem Kinderwagen,", "tokens": ["auf", "ein\u00b7mal", ",", "wie", "von", "ei\u00b7nem", "Kin\u00b7der\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "springt mir ein Rad", "tokens": ["springt", "mir", "ein", "Rad"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "unter den F\u00fc\u00dfen auf. Ich seh es jagen,", "tokens": ["un\u00b7ter", "den", "F\u00fc\u00b7\u00dfen", "auf", ".", "Ich", "seh", "es", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "es springt und rollt den Kiesweg vor mir her,", "tokens": ["es", "springt", "und", "rollt", "den", "Kies\u00b7weg", "vor", "mir", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "seh's Funken schlagen;", "tokens": ["seh's", "Fun\u00b7ken", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "mein Schreck, mein Zittern wird Begehr,", "tokens": ["mein", "Schreck", ",", "mein", "Zit\u00b7tern", "wird", "Be\u00b7gehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ich mu\u00df ihm nach, es haben! Bis zur Kehle", "tokens": ["ich", "mu\u00df", "ihm", "nach", ",", "es", "ha\u00b7ben", "!", "Bis", "zur", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKVZ", "$,", "PPER", "VAFIN", "$.", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "h\u00e4mmert mein Herz, das Rad rennt immer mehr,", "tokens": ["h\u00e4m\u00b7mert", "mein", "Herz", ",", "das", "Rad", "rennt", "im\u00b7mer", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.11": {"line.1": {"text": "und immer ruft mich klagend jene Seele", "tokens": ["und", "im\u00b7mer", "ruft", "mich", "kla\u00b7gend", "je\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVPP", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und winkt das Licht,", "tokens": ["und", "winkt", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "das Rad \u2013 halt! \u2013 Jetzt \u2013: ich greife \u2013 fehle \u2013:", "tokens": ["das", "Rad", "\u2013", "halt", "!", "\u2013", "Jetzt", "\u2013", ":", "ich", "grei\u00b7fe", "\u2013", "feh\u00b7le", "\u2013", ":"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "$.", "$(", "ADV", "$(", "$.", "PPER", "VVFIN", "$(", "VVFIN", "$(", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "es ist ein Lichtrad! halt! nach, eh's zerbricht!", "tokens": ["es", "ist", "ein", "Licht\u00b7rad", "!", "halt", "!", "nach", ",", "eh's", "zer\u00b7bricht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "VVFIN", "$.", "APPR", "$,", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich fass'es, st\u00fcrze \u2013 wach'ich? \u2013 meine matten", "tokens": ["Ich", "fass'\u00b7es", ",", "st\u00fcr\u00b7ze", "\u2013", "wach'\u00b7ich", "?", "\u2013", "mei\u00b7ne", "mat\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "ADJA", "$,", "VVFIN", "$(", "ADJD", "$.", "$(", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Finger umklammern es \u2013 \u2013 Nein \u2013 nicht:", "tokens": ["Fin\u00b7ger", "um\u00b7klam\u00b7mern", "es", "\u2013", "\u2013", "Nein", "\u2013", "nicht", ":"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$(", "$(", "PTKANT", "$(", "PTKNEG", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.13": {"line.1": {"text": "in meiner Hand zerrann es wie ein Schatten.", "tokens": ["in", "mei\u00b7ner", "Hand", "zer\u00b7rann", "es", "wie", "ein", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . . . 