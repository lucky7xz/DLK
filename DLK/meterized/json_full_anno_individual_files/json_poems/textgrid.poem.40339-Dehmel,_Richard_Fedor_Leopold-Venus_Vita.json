}, "stanza.16": {"line.1": {"text": "Ihr selbst, ihr verf\u00fchrerischen Sterne,", "tokens": ["Ihr", "selbst", ",", "ihr", "ver\u00b7f\u00fch\u00b7re\u00b7ri\u00b7schen", "Ster\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "wozu sch\u00fcrt ihr meine Seelennot?", "tokens": ["wo\u00b7zu", "sch\u00fcrt", "ihr", "mei\u00b7ne", "See\u00b7len\u00b7not", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Eisig haucht die glei\u00dfnerische Ferne:", "tokens": ["Ei\u00b7sig", "haucht", "die", "glei\u00df\u00b7ne\u00b7ri\u00b7sche", "Fer\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "ewig lebt allein der Tod.", "tokens": ["e\u00b7wig", "lebt", "al\u00b7lein", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Sei's denn! Umso unfa\u00dfbarer, freier,", "tokens": ["Sei's", "denn", "!", "Um\u00b7so", "un\u00b7fa\u00df\u00b7ba\u00b7rer", ",", "frei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "KON", "$.", "ADV", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "umso weiter, unbegrenzter", "tokens": ["um\u00b7so", "wei\u00b7ter", ",", "un\u00b7be\u00b7grenz\u00b7ter"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "ADV", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"te