{"textgrid.poem.36288": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Viele", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergebens wollt ihr, da\u00df mit eurem Sinne", "tokens": ["Ver\u00b7ge\u00b7bens", "wollt", "ihr", ",", "da\u00df", "mit", "eu\u00b7rem", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Dichter eins und gleich sei, der verachtet,", "tokens": ["Der", "Dich\u00b7ter", "eins", "und", "gleich", "sei", ",", "der", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "KON", "ADV", "VAFIN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Was tr\u00fcbe euren tr\u00e4gen Geist umnachtet,", "tokens": ["Was", "tr\u00fc\u00b7be", "eu\u00b7ren", "tr\u00e4\u00b7gen", "Geist", "um\u00b7nach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Besorgt, wie das Verborgne er gewinne;", "tokens": ["Be\u00b7sorgt", ",", "wie", "das", "Ver\u00b7borg\u00b7ne", "er", "ge\u00b7win\u00b7ne", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Dichter, der, wie fern die Zeit entrinne,", "tokens": ["Der", "Dich\u00b7ter", ",", "der", ",", "wie", "fern", "die", "Zeit", "ent\u00b7rin\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergangenheit als Gegenwart betrachtet,", "tokens": ["Ver\u00b7gan\u00b7gen\u00b7heit", "als", "Ge\u00b7gen\u00b7wart", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und w\u00e4hrend ihr nach Sterblichem nur trachtet,", "tokens": ["Und", "w\u00e4h\u00b7rend", "ihr", "nach", "Sterb\u00b7li\u00b7chem", "nur", "trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Unsterblich hier schon wird der Zukunft inne.", "tokens": ["U\u00b7nsterb\u00b7lich", "hier", "schon", "wird", "der", "Zu\u00b7kunft", "in\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.3": {"line.1": {"text": "Als noch die Flammen str\u00f6mten, Felsen klungen,", "tokens": ["Als", "noch", "die", "Flam\u00b7men", "str\u00f6m\u00b7ten", ",", "Fel\u00b7sen", "klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die alte Riesenzeit der jungen Erde", "tokens": ["Die", "al\u00b7te", "Rie\u00b7sen\u00b7zeit", "der", "jun\u00b7gen", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "w