{"textgrid.poem.31245": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Itzt in Teutschland reymen leider", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Itzt in Teutschland reymen leider", "tokens": ["Itzt", "in", "Teutschland", "rey\u00b7men", "lei\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "s\u00e4lbst die Sch\u00fchster und die Schneider/", "tokens": ["s\u00e4lbst", "die", "Sch\u00fchs\u00b7ter", "und", "die", "Schnei\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NE", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und nicht einer ahnt zur Frist/", "tokens": ["und", "nicht", "ei\u00b7ner", "ahnt", "zur", "Frist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PIS", "VVFIN", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wa\u00df er for ein Delpel ist.", "tokens": ["wa\u00df", "er", "for", "ein", "Del\u00b7pel", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Al\u00df ein Kerle von ", "tokens": ["Al\u00df", "ein", "Ker\u00b7le", "von"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "sage ich darzu bloh\u00df ", "tokens": ["sa\u00b7ge", "ich", "dar\u00b7zu", "bloh\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV", "ADV"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "ihr k\u00f6nt mich alle mit einander!", "tokens": ["ihr", "k\u00f6nt", "mich", "al\u00b7le", "mit", "ein\u00b7an\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "APPR", "PRF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "die fast an die Sterne sto\u00dfen/", "tokens": ["die", "fast", "an", "die", "Ster\u00b7ne", "sto\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "weil sein schlipffrichtes Gedicht", "tokens": ["weil", "sein", "schlipf\u00b7frich\u00b7tes", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "krumm wie ein ", "tokens": ["krumm", "wie", "ein"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Jeden Morgen schl\u00fckkt das Schwein", "tokens": ["Je\u00b7den", "Mor\u00b7gen", "schl\u00fckkt", "das", "Schwein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "sihben Bisem-Kugeln ein \u2013", "tokens": ["sih\u00b7ben", "Bi\u00b7sem\u00b7Ku\u00b7geln", "ein", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "dein Sing-Sang bleibt for mich/ Gevatter/", "tokens": ["dein", "Sing\u00b7Sang", "bleibt", "for", "mich", "/", "Ge\u00b7vat\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$(", "NE", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "dr\u00fcmb doch bloh\u00df leeres Gantz-Geschnatter!", "tokens": ["dr\u00fcmb", "doch", "bloh\u00df", "lee\u00b7res", "Gant\u00b7zGe\u00b7schnat\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der bald greise ", "tokens": ["Der", "bald", "grei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "brillt noch \u00fcmmer wie ", "tokens": ["brillt", "noch", "\u00fcm\u00b7mer", "wie"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "KOKOM"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "durchau\u00df nur die ", "tokens": ["durch\u00b7au\u00df", "nur", "die"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Noch st\u00e4hts mu\u00dft ich/ sorbald sie sangen/", "tokens": ["Noch", "st\u00e4hts", "mu\u00dft", "ich", "/", "sor\u00b7bald", "sie", "san\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VMFIN", "PPER", "$(", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "f\u00fcr langer Weile Fliegen fangen.", "tokens": ["f\u00fcr", "lan\u00b7ger", "Wei\u00b7le", "Flie\u00b7gen", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "S\u00e4lbst die gantz und durchau\u00df Alten/", "tokens": ["S\u00e4lbst", "die", "gantz", "und", "durch\u00b7au\u00df", "Al\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "KON", "ADV", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sollte mans for m\u00fcglich halten?", "tokens": ["soll\u00b7te", "mans", "for", "m\u00fcg\u00b7lich", "hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gr\u00e4ulich dropfft au\u00df ihrem Stei\u00df", "tokens": ["Gr\u00e4u\u00b7lich", "dropfft", "au\u00df", "ih\u00b7rem", "Stei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mehl-Tau/ Molch- und Drachen-Schwei\u00df.", "tokens": ["Mehl\u00b7Tau", "/", "Molch", "und", "Dra\u00b7chen\u00b7Schwei\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "TRUNC", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwartz verh\u00fcllt sich mein Gesicht/", "tokens": ["Schwartz", "ver\u00b7h\u00fcllt", "sich", "mein", "Ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "ohn Empfindung b\u00fcn ich nicht.", "tokens": ["ohn", "Emp\u00b7fin\u00b7dung", "b\u00fcn", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Am mehrsten aber mich verschr\u00f6kken", "tokens": ["Am", "mehrs\u00b7ten", "a\u00b7ber", "mich", "ver\u00b7schr\u00f6k\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "KON", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "die M\u00e4ntscher mit den Fischbein-R\u00f6kken!", "tokens": ["die", "M\u00e4nt\u00b7scher", "mit", "den", "Fischbein\u00b7R\u00f6k\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Durch so w\u00fchtende ", "tokens": ["Durch", "so", "w\u00fch\u00b7ten\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "kam schon ", "tokens": ["kam", "schon"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "denn er dachte sich darbey/", "tokens": ["denn", "er", "dach\u00b7te", "sich", "dar\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PAV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "so klingt Uhl- und Hunds-Geschrey.", "tokens": ["so", "klingt", "Uhl", "und", "Hunds\u00b7Ge\u00b7schrey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "TRUNC", "KON", "NN", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Wo solch eine Urschel b\u00f6llt/", "tokens": ["Wo", "solch", "ei\u00b7ne", "Ur\u00b7schel", "b\u00f6llt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "nimbt ", "tokens": ["nimbt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "denn saurer sind for keinen Gaumen", "tokens": ["denn", "sau\u00b7rer", "sind", "for", "kei\u00b7nen", "Gau\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "nicht ein mahl alte Schrumppel-Pflaumen.", "tokens": ["nicht", "ein", "mahl", "al\u00b7te", "Schrump\u00b7pel\u00b7Pflau\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "H\u00f6chst belihbte ", "tokens": ["H\u00f6chst", "be\u00b7lihb\u00b7te"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "nein/ wa\u00df seyd ihr bloh\u00df for Biester!", "tokens": ["nein", "/", "wa\u00df", "seyd", "ihr", "bloh\u00df", "for", "Bies\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PWAV", "VAFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dihses mu\u00df ich durchau\u00df schreyn/", "tokens": ["Dih\u00b7ses", "mu\u00df", "ich", "durch\u00b7au\u00df", "schreyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "denn ich kan nicht forchtsahm seyn:", "tokens": ["denn", "ich", "kan", "nicht", "forcht\u00b7sahm", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKNEG", "VVFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "al\u00df das Prett f\u00fcr eurem Plikk;", "tokens": ["al\u00df", "das", "Prett", "f\u00fcr", "eu\u00b7rem", "Plikk", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "ich k\u00fcnt for euer ", "tokens": ["ich", "k\u00fcnt", "for", "eu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "euch mit Vergn\u00fcgen ", "tokens": ["euch", "mit", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Euch \u00fcmbkr\u00e4chtzen mahl die Raben/", "tokens": ["Euch", "\u00fcm\u00b7bkr\u00e4cht\u00b7zen", "mahl", "die", "Ra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "mich wird man in Marmol graben/", "tokens": ["mich", "wird", "man", "in", "Mar\u00b7mol", "gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "NE", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "weil mein Ver\u00df itzt/ wie mir d\u00e4ucht/", "tokens": ["weil", "mein", "Ver\u00df", "itzt", "/", "wie", "mir", "d\u00e4ucht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "$(", "PWAV", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "nicht mehr nach der Lampe r\u00e4ucht.", "tokens": ["nicht", "mehr", "nach", "der", "Lam\u00b7pe", "r\u00e4ucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dr\u00fcmb so lohb ich mit Geschrey", "tokens": ["Dr\u00fcmb", "so", "lohb", "ich", "mit", "Ge\u00b7schrey"], "token_info": ["wor