{"textgrid.poem.26689": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Winternacht", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ade! Die liebe Herrgottssonne gehet,", "tokens": ["A\u00b7de", "!", "Die", "lie\u00b7be", "Herr\u00b7gotts\u00b7son\u00b7ne", "ge\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Grad \u00fcber tritt der Mond!", "tokens": ["Grad", "\u00fc\u00b7ber", "tritt", "der", "Mond", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ade! Mit schwarzem Rabenfl\u00fcgel wehet", "tokens": ["A\u00b7de", "!", "Mit", "schwar\u00b7zem", "Ra\u00b7ben\u00b7fl\u00fc\u00b7gel", "we\u00b7het"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die stumme Nacht ums Erdenrund.", "tokens": ["Die", "stum\u00b7me", "Nacht", "ums", "Er\u00b7den\u00b7rund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nichts h\u00f6r ich mehr durchs winternde Gefilde", "tokens": ["Nichts", "h\u00f6r", "ich", "mehr", "durchs", "win\u00b7tern\u00b7de", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als tief im Felsenloch", "tokens": ["Als", "tief", "im", "Fel\u00b7sen\u00b7loch"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Murmelquell, und aus dem Wald das wilde", "tokens": ["Die", "Mur\u00b7mel\u00b7quell", ",", "und", "aus", "dem", "Wald", "das", "wil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "APPR", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geheul des Uhus h\u00f6r ich noch.", "tokens": ["Ge\u00b7heul", "des", "U\u00b7hus", "h\u00f6r", "ich", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Wasserbette ruhen alle Fische,", "tokens": ["Im", "Was\u00b7ser\u00b7bet\u00b7te", "ru\u00b7hen", "al\u00b7le", "Fi\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Schnecke kriecht ins Dach,", "tokens": ["Die", "Schne\u00b7cke", "kriecht", "ins", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das H\u00fcndchen schlummert sicher unterm Tische,", "tokens": ["Das", "H\u00fcnd\u00b7chen", "schlum\u00b7mert", "si\u00b7cher", "un\u00b7term", "Ti\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Weibchen nickt im Schlafgemach.", "tokens": ["Mein", "Weib\u00b7chen", "nickt", "im", "Schlaf\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Euch Br\u00fcderchen von meinen Bubentagen,", "tokens": ["Euch", "Br\u00fc\u00b7der\u00b7chen", "von", "mei\u00b7nen", "Bu\u00b7ben\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein herzliches Willkomm!", "tokens": ["Mein", "herz\u00b7li\u00b7ches", "Will\u00b7komm", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr sitzt vielleicht mit traulichem Behagen", "tokens": ["Ihr", "sitzt", "viel\u00b7leicht", "mit", "trau\u00b7li\u00b7chem", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um einen teutschen Krug herum.", "tokens": ["Um", "ei\u00b7nen", "teut\u00b7schen", "Krug", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Im hochgef\u00fcllten Deckelglase malet", "tokens": ["Im", "hoch\u00b7ge\u00b7f\u00fcll\u00b7ten", "De\u00b7ckel\u00b7gla\u00b7se", "ma\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich purpurfarb die Welt,", "tokens": ["Sich", "pur\u00b7purf\u00b7arb", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und aus dem goldnen Traubenschaume strahlet", "tokens": ["Und", "aus", "dem", "gold\u00b7nen", "Trau\u00b7ben\u00b7schau\u00b7me", "strah\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vergn\u00fcgen, das kein Neid verg\u00e4llt.", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", ",", "das", "kein", "Neid", "ver\u00b7g\u00e4llt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Im Hintergrund vergangner Jahre findet", "tokens": ["Im", "Hin\u00b7ter\u00b7grund", "ver\u00b7gang\u00b7ner", "Jah\u00b7re", "fin\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur Rosen euer Blick,", "tokens": ["Nur", "Ro\u00b7sen", "eu\u00b7er", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Leicht, wie die blaue Knasterwolke, schwindet", "tokens": ["Leicht", ",", "wie", "die", "blau\u00b7e", "Knas\u00b7ter\u00b7wol\u00b7ke", ",", "schwin\u00b7det"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der tr\u00fcbe Gram von euch zur\u00fcck.", "tokens": ["Der", "tr\u00fc\u00b7be", "Gram", "von", "euch", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Vom Schaukelgaul bis gar zum Doktorhute", "tokens": ["Vom", "Schau\u00b7kel\u00b7gaul", "bis", "gar", "zum", "Dok\u00b7tor\u00b7hu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "St\u00f6rt ihr im Zeitbuch um", "tokens": ["St\u00f6rt", "ihr", "im", "Zeit\u00b7buch", "um"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und z\u00e4hlt nunmehr mit federleichtem Mute", "tokens": ["Und", "z\u00e4hlt", "nun\u00b7mehr", "mit", "fe\u00b7der\u00b7leich\u00b7tem", "Mu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schwei\u00dftropfen im Gymnasium.", "tokens": ["Schwei\u00df\u00b7trop\u00b7fen", "im", "Gym\u00b7na\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "++---+-+", "measure": "unknown.measure.tetra"}}, "stanza.8": {"line.1": {"text": "Wie manchen Fluch \u2013 noch m\u00f6gen unterm Boden", "tokens": ["Wie", "man\u00b7chen", "Fluch", "\u2013", "noch", "m\u00f6\u00b7gen", "un\u00b7term", "Bo\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "$(", "ADV", "VMFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich seine Knochen drehn \u2013", "tokens": ["Sich", "sei\u00b7ne", "Kno\u00b7chen", "drehn", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Terenz erpre\u00dft, trotz Herrn Minellis Noten,", "tokens": ["Te\u00b7renz", "er\u00b7pre\u00dft", ",", "trotz", "Herrn", "Mi\u00b7nel\u00b7lis", "No\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "APPR", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie manch verzogen Maul gesehn,", "tokens": ["Wie", "manch", "ver\u00b7zo\u00b7gen", "Maul", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wie ungest\u00fcm dem grimmen Landexamen", "tokens": ["Wie", "un\u00b7ge\u00b7st\u00fcm", "dem", "grim\u00b7men", "Lan\u00b7de\u00b7xa\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Buben Herz geklopft;", "tokens": ["Des", "Bu\u00b7ben", "Herz", "ge\u00b7klopft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie ihm, sprach itzt der Rektor seinen Namen,", "tokens": ["Wie", "ihm", ",", "sprach", "itzt", "der", "Rek\u00b7tor", "sei\u00b7nen", "Na\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der helle Schwei\u00df aufs Buch getropft. \u2013", "tokens": ["Der", "hel\u00b7le", "Schwei\u00df", "aufs", "Buch", "ge\u00b7tropft", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wohl redt man auch von einer \u2013 e \u2013 gewissen \u2013", "tokens": ["Wohl", "redt", "man", "auch", "von", "ei\u00b7ner", "\u2013", "e", "\u2013", "ge\u00b7wis\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "ART", "$(", "NE", "$(", "VAPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sich als ", "tokens": ["Die", "sich", "als"], "token_info": ["word", "word", "word"], "pos": ["ART", "PRF", "KOUS"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und mancher will der Lecker ba\u00df nun wissen,", "tokens": ["Und", "man\u00b7cher", "will", "der", "Le\u00b7cker", "ba\u00df", "nun", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was doch ihr ", "tokens": ["Was", "doch", "ihr"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.11": {"line.1": {"text": "Nun liegt dies all im Nebel hinterm R\u00fccken,", "tokens": ["Nun", "liegt", "dies", "all", "im", "Ne\u00b7bel", "hin\u00b7term", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "PIAT", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Bube hei\u00dft nun Mann,", "tokens": ["Und", "Bu\u00b7be", "hei\u00dft", "nun", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Friedrich schweigt der weiseren Per\u00fccken,", "tokens": ["Und", "Fried\u00b7rich", "schweigt", "der", "wei\u00b7se\u00b7ren", "Pe\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was einst der kleine Fritz getan \u2013", "tokens": ["Was", "einst", "der", "klei\u00b7ne", "Fritz", "ge\u00b7tan", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NE", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Man ist \u2013 Potz gar! \u2013 zum Doktor ausgesprochen,", "tokens": ["Man", "ist", "\u2013", "Potz", "gar", "!", "\u2013", "zum", "Dok\u00b7tor", "aus\u00b7ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$(", "NE", "ADV", "$.", "$(", "APPRART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohl gar \u2013 beim Regiment!", "tokens": ["Wohl", "gar", "\u2013", "beim", "Re\u00b7gi\u00b7ment", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hat vielleicht \u2013 doch nicht zu fr\u00fch, gerochen,", "tokens": ["Und", "hat", "viel\u00b7leicht", "\u2013", "doch", "nicht", "zu", "fr\u00fch", ",", "ge\u00b7ro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "$(", "ADV", "PTKNEG", "PTKA", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df Plane \u2013 Seifenblasen sind.", "tokens": ["Da\u00df", "Pla\u00b7ne", "\u2013", "Sei\u00b7fen\u00b7bla\u00b7sen", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NE", "$(", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Hauch immer zu \u2013 und la\u00df die Blasen springen;", "tokens": ["Hauch", "im\u00b7mer", "zu", "\u2013", "und", "la\u00df", "die", "Bla\u00b7sen", "sprin\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKZU", "$(", "KON", "VVIMP", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bleibt nur dies Herz noch ganz!", "tokens": ["Bleibt", "nur", "dies", "Herz", "noch", "ganz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDS", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und bleibt mir nur \u2013 errungen mit Ges\u00e4ngen \u2013", "tokens": ["Und", "bleibt", "mir", "nur", "\u2013", "er\u00b7run\u00b7gen", "mit", "Ge\u00b7s\u00e4n\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$(", "VVPP", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Lohn ein teutscher Lorbeerkranz.", "tokens": ["Zum", "Lohn", "ein", "teut\u00b7scher", "Lor\u00b7beer\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Ade! Die liebe Herrgottssonne gehet,", "tokens": ["A\u00b7de", "!", "Die", "lie\u00b7be", "Herr\u00b7gotts\u00b7son\u00b7ne", "ge\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Grad \u00fcber tritt der Mond!", "tokens": ["Grad", "\u00fc\u00b7ber", "tritt", "der", "Mond", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ade! Mit schwarzem Rabenfl\u00fcgel wehet", "tokens": ["A\u00b7de", "!", "Mit", "schwar\u00b7zem", "Ra\u00b7ben\u00b7fl\u00fc\u00b7gel", "we\u00b7het"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die stumme Nacht ums Erdenrund.", "tokens": ["Die", "stum\u00b7me", "Nacht", "ums", "Er\u00b7den\u00b7rund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Nichts h\u00f6r ich mehr durchs winternde Gefilde", "tokens": ["Nichts", "h\u00f6r", "ich", "mehr", "durchs", "win\u00b7tern\u00b7de", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als tief im Felsenloch", "tokens": ["Als", "tief", "im", "Fel\u00b7sen\u00b7loch"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Murmelquell, und aus dem Wald das wilde", "tokens": ["Die", "Mur\u00b7mel\u00b7quell", ",", "und", "aus", "dem", "Wald", "das", "wil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "APPR", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geheul des Uhus h\u00f6r ich noch.", "tokens": ["Ge\u00b7heul", "des", "U\u00b7hus", "h\u00f6r", "ich", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Im Wasserbette ruhen alle Fische,", "tokens": ["Im", "Was\u00b7ser\u00b7bet\u00b7te", "ru\u00b7hen", "al\u00b7le", "Fi\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Schnecke kriecht ins Dach,", "tokens": ["Die", "Schne\u00b7cke", "kriecht", "ins", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das H\u00fcndchen schlummert sicher unterm Tische,", "tokens": ["Das", "H\u00fcnd\u00b7chen", "schlum\u00b7mert", "si\u00b7cher", "un\u00b7term", "Ti\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Weibchen nickt im Schlafgemach.", "tokens": ["Mein", "Weib\u00b7chen", "nickt", "im", "Schlaf\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Euch Br\u00fcderchen von meinen Bubentagen,", "tokens": ["Euch", "Br\u00fc\u00b7der\u00b7chen", "von", "mei\u00b7nen", "Bu\u00b7ben\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein herzliches Willkomm!", "tokens": ["Mein", "herz\u00b7li\u00b7ches", "Will\u00b7komm", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr sitzt vielleicht mit traulichem Behagen", "tokens": ["Ihr", "sitzt", "viel\u00b7leicht", "mit", "trau\u00b7li\u00b7chem", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um einen teutschen Krug herum.", "tokens": ["Um", "ei\u00b7nen", "teut\u00b7schen", "Krug", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Im hochgef\u00fcllten Deckelglase malet", "tokens": ["Im", "hoch\u00b7ge\u00b7f\u00fcll\u00b7ten", "De\u00b7ckel\u00b7gla\u00b7se", "ma\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich purpurfarb die Welt,", "tokens": ["Sich", "pur\u00b7purf\u00b7arb", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und aus dem goldnen Traubenschaume strahlet", "tokens": ["Und", "aus", "dem", "gold\u00b7nen", "Trau\u00b7ben\u00b7schau\u00b7me", "strah\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vergn\u00fcgen, das kein Neid verg\u00e4llt.", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", ",", "das", "kein", "Neid", "ver\u00b7g\u00e4llt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Im Hintergrund vergangner Jahre findet", "tokens": ["Im", "Hin\u00b7ter\u00b7grund", "ver\u00b7gang\u00b7ner", "Jah\u00b7re", "fin\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur Rosen euer Blick,", "tokens": ["Nur", "Ro\u00b7sen", "eu\u00b7er", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Leicht, wie die blaue Knasterwolke, schwindet", "tokens": ["Leicht", ",", "wie", "die", "blau\u00b7e", "Knas\u00b7ter\u00b7wol\u00b7ke", ",", "schwin\u00b7det"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der tr\u00fcbe Gram von euch zur\u00fcck.", "tokens": ["Der", "tr\u00fc\u00b7be", "Gram", "von", "euch", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Vom Schaukelgaul bis gar zum Doktorhute", "tokens": ["Vom", "Schau\u00b7kel\u00b7gaul", "bis", "gar", "zum", "Dok\u00b7tor\u00b7hu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "St\u00f6rt ihr im Zeitbuch um", "tokens": ["St\u00f6rt", "ihr", "im", "Zeit\u00b7buch", "um"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und z\u00e4hlt nunmehr mit federleichtem Mute", "tokens": ["Und", "z\u00e4hlt", "nun\u00b7mehr", "mit", "fe\u00b7der\u00b7leich\u00b7tem", "Mu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schwei\u00dftropfen im Gymnasium.", "tokens": ["Schwei\u00df\u00b7trop\u00b7fen", "im", "Gym\u00b7na\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "++---+-+", "measure": "unknown.measure.tetra"}}, "stanza.21": {"line.1": {"text": "Wie manchen Fluch \u2013 noch m\u00f6gen unterm Boden", "tokens": ["Wie", "man\u00b7chen", "Fluch", "\u2013", "noch", "m\u00f6\u00b7gen", "un\u00b7term", "Bo\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "$(", "ADV", "VMFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich seine Knochen drehn \u2013", "tokens": ["Sich", "sei\u00b7ne", "Kno\u00b7chen", "drehn", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Terenz erpre\u00dft, trotz Herrn Minellis Noten,", "tokens": ["Te\u00b7renz", "er\u00b7pre\u00dft", ",", "trotz", "Herrn", "Mi\u00b7nel\u00b7lis", "No\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "APPR", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie manch verzogen Maul gesehn,", "tokens": ["Wie", "manch", "ver\u00b7zo\u00b7gen", "Maul", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.22": {"line.1": {"text": "Wie ungest\u00fcm dem grimmen Landexamen", "tokens": ["Wie", "un\u00b7ge\u00b7st\u00fcm", "dem", "grim\u00b7men", "Lan\u00b7de\u00b7xa\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Buben Herz geklopft;", "tokens": ["Des", "Bu\u00b7ben", "Herz", "ge\u00b7klopft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie ihm, sprach itzt der Rektor seinen Namen,", "tokens": ["Wie", "ihm", ",", "sprach", "itzt", "der", "Rek\u00b7tor", "sei\u00b7nen", "Na\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der helle Schwei\u00df aufs Buch getropft. \u2013", "tokens": ["Der", "hel\u00b7le", "Schwei\u00df", "aufs", "Buch", "ge\u00b7tropft", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.23": {"line.1": {"text": "Wohl redt man auch von einer \u2013 e \u2013 gewissen \u2013", "tokens": ["Wohl", "redt", "man", "auch", "von", "ei\u00b7ner", "\u2013", "e", "\u2013", "ge\u00b7wis\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "ART", "$(", "NE", "$(", "VAPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sich als ", "tokens": ["Die", "sich", "als"], "token_info": ["word", "word", "word"], "pos": ["ART", "PRF", "KOUS"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und mancher will der Lecker ba\u00df nun wissen,", "tokens": ["Und", "man\u00b7cher", "will", "der", "Le\u00b7cker", "ba\u00df", "nun", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was doch ihr ", "tokens": ["Was", "doch", "ihr"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.24": {"line.1": {"text": "Nun liegt dies all im Nebel hinterm R\u00fccken,", "tokens": ["Nun", "liegt", "dies", "all", "im", "Ne\u00b7bel", "hin\u00b7term", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "PIAT", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Bube hei\u00dft nun Mann,", "tokens": ["Und", "Bu\u00b7be", "hei\u00dft", "nun", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Friedrich schweigt der weiseren Per\u00fccken,", "tokens": ["Und", "Fried\u00b7rich", "schweigt", "der", "wei\u00b7se\u00b7ren", "Pe\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was einst der kleine Fritz getan \u2013", "tokens": ["Was", "einst", "der", "klei\u00b7ne", "Fritz", "ge\u00b7tan", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NE", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.25": {"line.1": {"text": "Man ist \u2013 Potz gar! \u2013 zum Doktor ausgesprochen,", "tokens": ["Man", "ist", "\u2013", "Potz", "gar", "!", "\u2013", "zum", "Dok\u00b7tor", "aus\u00b7ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$(", "NE", "ADV", "$.", "$(", "APPRART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohl gar \u2013 beim Regiment!", "tokens": ["Wohl", "gar", "\u2013", "beim", "Re\u00b7gi\u00b7ment", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hat vielleicht \u2013 doch nicht zu fr\u00fch, gerochen,", "tokens": ["Und", "hat", "viel\u00b7leicht", "\u2013", "doch", "nicht", "zu", "fr\u00fch", ",", "ge\u00b7ro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "$(", "ADV", "PTKNEG", "PTKA", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df Plane \u2013 Seifenblasen sind.", "tokens": ["Da\u00df", "Pla\u00b7ne", "\u2013", "Sei\u00b7fen\u00b7bla\u00b7sen", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NE", "$(", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.26": {"line.1": {"text": "Hauch immer zu \u2013 und la\u00df die Blasen springen;", "tokens": ["Hauch", "im\u00b7mer", "zu", "\u2013", "und", "la\u00df", "die", "Bla\u00b7sen", "sprin\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKZU", "$(", "KON", "VVIMP", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bleibt nur dies Herz noch ganz!", "tokens": ["Bleibt", "nur", "dies", "Herz", "noch", "ganz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDS", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und bleibt mir nur \u2013 errungen mit Ges\u00e4ngen \u2013", "tokens": ["Und", "bleibt", "mir", "nur", "\u2013", "er\u00b7run\u00b7gen", "mit", "Ge\u00b7s\u00e4n\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$(", "VVPP", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Lohn ein teutscher Lorbeerkranz.", "tokens": ["Zum", "Lohn", "ein", "teut\u00b7scher", "Lor\u00b7beer\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}