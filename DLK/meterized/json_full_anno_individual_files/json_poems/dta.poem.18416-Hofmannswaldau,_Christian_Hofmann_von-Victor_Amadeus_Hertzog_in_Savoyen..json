{"dta.poem.18416": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Victor Amadeus/ Hertzog in Savoyen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das gl\u00fccke wird dich bald an deinen feinden r\u00e4chen/", "tokens": ["Das", "gl\u00fc\u00b7cke", "wird", "dich", "bald", "an", "dei\u00b7nen", "fein\u00b7den", "r\u00e4\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indem dein helden-muth noch unbezwungen ist/", "tokens": ["In\u00b7dem", "dein", "hel\u00b7den\u00b7muth", "noch", "un\u00b7be\u00b7zwun\u00b7gen", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die nachwelt selber wird verwundernd dieses sprechen/", "tokens": ["Die", "nach\u00b7welt", "sel\u00b7ber", "wird", "ver\u00b7wun\u00b7dernd", "die\u00b7ses", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VAFIN", "VVPP", "PDAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie da\u00df du durch verlust noch gr\u00f6sser worden bist.", "tokens": ["Wie", "da\u00df", "du", "durch", "ver\u00b7lust", "noch", "gr\u00f6s\u00b7ser", "wor\u00b7den", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "APPR", "VVFIN", "ADV", "ADJD", "VAPP", "VAFIN", "$."], "