{"dta.poem.8165": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Wohl rief ich sanft dich an mein Herz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Doch blieben meine Arme leer;", "tokens": ["Doch", "blie\u00b7ben", "mei\u00b7ne", "Ar\u00b7me", "leer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Stimme Zauber, der du sonst", "tokens": ["Der", "Stim\u00b7me", "Zau\u00b7ber", ",", "der", "du", "sonst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nie widerstandest, galt nicht mehr.", "tokens": ["Nie", "wi\u00b7der\u00b7stan\u00b7dest", ",", "galt", "nicht", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was jetzt dein Leben f\u00fcllen wird,", "tokens": ["Was", "jetzt", "dein", "Le\u00b7ben", "f\u00fcl\u00b7len", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohin du gehst, wohin du irrst,", "tokens": ["Wo\u00b7hin", "du", "gehst", ",", "wo\u00b7hin", "du", "irrst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df es nicht; ich wei\u00df allein,", "tokens": ["Ich", "wei\u00df", "es", "nicht", ";", "ich", "wei\u00df", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df du mir nie mehr l\u00e4cheln wirst.", "tokens": ["Da\u00df", "du", "mir", "nie", "mehr", "l\u00e4\u00b7cheln", "wirst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch kommt erst jene stille Zeit,", "tokens": ["Doch", "kommt", "erst", "je\u00b7ne", "stil\u00b7le", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo uns das Leben l\u00e4\u00dft allein,", "tokens": ["Wo", "uns", "das", "Le\u00b7ben", "l\u00e4\u00dft", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann wird, wie in der Jugend einst,", "tokens": ["Dann", "wird", ",", "wie", "in", "der", "Ju\u00b7gend", "einst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur meine Liebe bei dir sein.", "tokens": ["Nur", "mei\u00b7ne", "Lie\u00b7be", "bei", "dir", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann wird, was jetzt geschehen mag,", "tokens": ["Dann", "wird", ",", "was", "jetzt", "ge\u00b7sche\u00b7hen", "mag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Schatten dir vor\u00fcbergehn,", "tokens": ["Wie", "Schat\u00b7ten", "dir", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nur die Zeit, die nun dahin,", "tokens": ["Und", "nur", "die", "Zeit", ",", "die", "nun", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die uns geh\u00f6rte, wird bestehn.", "tokens": ["Die", "uns", "ge\u00b7h\u00f6r\u00b7te", ",", "wird", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wenn dein letztes Kissen einst", "tokens": ["Und", "wenn", "dein", "letz\u00b7tes", "Kis\u00b7sen", "einst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begl\u00e4nzt ein Abendsonnenstrahl,", "tokens": ["Be\u00b7gl\u00e4nzt", "ein", "A\u00b7bend\u00b7son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist die Sonne jenes Tages,", "tokens": ["Es", "ist", "die", "Son\u00b7ne", "je\u00b7nes", "Ta\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da ich dich k\u00fc\u00dfte zum ersten Mal.", "tokens": ["Da", "ich", "dich", "k\u00fc\u00df\u00b7te", "zum", "ers\u00b7ten", "Mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}