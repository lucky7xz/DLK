{"dta.poem.21263": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Lied der Nymphe Persante.  \n Den 24 September 1760.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20476-3", "language": ["de:0.99"], "booktitle": "Ramler, Karl Wilhelm: Oden. Berlin, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Er siegt! mein Perseus siegt! &#8212; Ihr\nFreudenz&#228;hren,", "tokens": ["Er", "siegt", "!", "mein", "Per\u00b7seus", "siegt", "!", "&#8212;", "Ihr", "Freu\u00b7denz", "&#228;", "hren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "XML_entity", "word", "word", "XML_entity", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "$.", "$(", "PPOSAT", "NN", "$(", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erstickt nicht meinen Lobgesang! \u2014", "tokens": ["Er\u00b7stickt", "nicht", "mei\u00b7nen", "Lob\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Fluten meines Stroms, erz\u00e4hlt in allen", "tokens": ["O", "Flu\u00b7ten", "mei\u00b7nes", "Stroms", ",", "er\u00b7z\u00e4hlt", "in", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Drachen Untergang!", "tokens": ["Des", "Dra\u00b7chen", "Un\u00b7ter\u00b7gang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hier, wo der Belt, mein Kolberg zu", "tokens": ["Hier", ",", "wo", "der", "Belt", ",", "mein", "Kol\u00b7berg", "zu"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "$,", "PPOSAT", "NN", "PTKZU"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mit D\u00fcnen sein Gestad\u2019 umzieht,", "tokens": ["Mit", "D\u00fc\u00b7nen", "sein", "Ge\u00b7stad'", "um\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sass ich, und sang entz\u00fcckt den horchenden", "tokens": ["Sass", "ich", ",", "und", "sang", "ent\u00b7z\u00fcckt", "den", "hor\u00b7chen\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von meinem Freund\u2019 ein Lied.", "tokens": ["Von", "mei\u00b7nem", "Freund'", "ein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201eer schlug das Raubthier j\u00fcngst, das", "tokens": ["\u201e", "er", "schlug", "das", "Raubt\u00b7hier", "j\u00fcngst", ",", "das"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "ADV", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201eriph\u00e4us auf mich ausgespien,", "tokens": ["\u201e", "ri\u00b7ph\u00e4us", "auf", "mich", "aus\u00b7ge\u00b7spi\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201eals ich, verlassen von den G\u00f6ttern, seine", "tokens": ["\u201e", "als", "ich", ",", "ver\u00b7las\u00b7sen", "von", "den", "G\u00f6t\u00b7tern", ",", "sei\u00b7ne"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "KOUS", "PPER", "$,", "VVPP", "APPR", "ART", "NN", "$,", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u201eunwiederbringlich schien. \u2014", "tokens": ["\u201e", "un\u00b7wie\u00b7der\u00b7bring\u00b7lich", "schien", "."], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich sprachs: als ich urpl\u00f6tzlich einen", "tokens": ["Ich", "sprachs", ":", "als", "ich", "ur\u00b7pl\u00f6tz\u00b7lich", "ei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "PPER", "ADJD", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus blauer Tiefe steigen sah", "tokens": ["Aus", "blau\u00b7er", "Tie\u00b7fe", "stei\u00b7gen", "sah"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit sunfzig aufgerissnen feuerspeynden", "tokens": ["Mit", "sunf\u00b7zig", "auf\u00b7ge\u00b7riss\u00b7nen", "feu\u00b7er\u00b7speyn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ohnm\u00e4chtig lag ich da.", "tokens": ["Ohn\u00b7m\u00e4ch\u00b7tig", "lag", "ich", "da", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mein Perseus flog in diesem Augen-", "tokens": ["Mein", "Per\u00b7seus", "flog", "in", "die\u00b7sem", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "VVFIN", "APPR", "PDAT", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Herab von seiner Warte, schwang", "tokens": ["Her\u00b7ab", "von", "sei\u00b7ner", "War\u00b7te", ",", "schwang"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein glorreich Eisen, hielt den Tod im", "tokens": ["Sein", "glor\u00b7reich", "Ei\u00b7sen", ",", "hielt", "den", "Tod", "im"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "VVFIN", "ART", "NN", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dreymal neun Tage lang.", "tokens": ["Drey\u00b7mal", "neun", "Ta\u00b7ge", "lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Ha! welche Flammenstr\u00f6me schoss die", "tokens": ["Ha", "!", "wel\u00b7che", "Flam\u00b7men\u00b7str\u00f6\u00b7me", "schoss", "die"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAT", "NN", "VVFIN", "ART"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nach seinem Leben! \u2014 Endlich fand", "tokens": ["Nach", "sei\u00b7nem", "Le\u00b7ben", "!", "End\u00b7lich", "fand"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$(", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Flehn der G\u00f6tter Ohr: und Waffen", "tokens": ["Mein", "Flehn", "der", "G\u00f6t\u00b7ter", "Ohr", ":", "und", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "NN", "$.", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da, wo mein Gastfreund stand.", "tokens": ["Da", ",", "wo", "mein", "Gast\u00b7freund", "stand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}