{"dta.poem.16138": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "192.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Ungl\u00fcck in der Welt such', als du kanst, zu lindern,", "tokens": ["Das", "Un\u00b7gl\u00fcck", "in", "der", "Welt", "such'", ",", "als", "du", "kanst", ",", "zu", "lin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VMFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soweit umher du reichst, zu mildern und zu mindern.", "tokens": ["So\u00b7weit", "um\u00b7her", "du", "reichst", ",", "zu", "mil\u00b7dern", "und", "zu", "min\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "PPER", "VVFIN", "$,", "APPR", "ADJA", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Warum? schon weil es dich im eignen Gl\u00fcck wird hindern.", "tokens": ["Wa\u00b7rum", "?", "schon", "weil", "es", "dich", "im", "eig\u00b7nen", "Gl\u00fcck", "wird", "hin\u00b7dern", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "ADV", "KOUS", "PPER", "PRF", "APPRART", "ADJA", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch reichest du nicht weit mit deinem schwachen Trost;", "tokens": ["Doch", "rei\u00b7chest", "du", "nicht", "weit", "mit", "dei\u00b7nem", "schwa\u00b7chen", "Trost", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Mund drei Spannen stirbt dein warmer Hauch im Frost.", "tokens": ["Vom", "Mund", "drei", "Span\u00b7nen", "stirbt", "dein", "war\u00b7mer", "Hauch", "im", "Frost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "CARD", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was bleibt dir da zum Trost, als da\u00df, was Ungl\u00fcck scheint,", "tokens": ["Was", "bleibt", "dir", "da", "zum", "Trost", ",", "als", "da\u00df", ",", "was", "Un\u00b7gl\u00fcck", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "KOKOM", "KOUS", "$,", "PWS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dem, der Aller Gl\u00fcck will, anders ist gemeint;", "tokens": ["Von", "dem", ",", "der", "Al\u00b7ler", "Gl\u00fcck", "will", ",", "an\u00b7ders", "ist", "ge\u00b7meint", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "ART", "ADJA", "NN", "VMFIN", "$,", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und wer die Gabe nur, wie sie gemeint ist, nimmt,", "tokens": ["Und", "wer", "die", "Ga\u00b7be", "nur", ",", "wie", "sie", "ge\u00b7meint", "ist", ",", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den f\u00f6rdert sie dazu, wozu sie war bestimmt.", "tokens": ["Den", "f\u00f6r\u00b7dert", "sie", "da\u00b7zu", ",", "wo\u00b7zu", "sie", "war", "be\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PAV", "$,", "PWAV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Nicht heben kann dein Blick den schwarzen Trauerschleier,", "tokens": ["Nicht", "he\u00b7ben", "kann", "dein", "Blick", "den", "schwar\u00b7zen", "Trau\u00b7er\u00b7schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darunter s\u00e4hst du sonst das wei\u00dfe Kleid der Feier.", "tokens": ["Da\u00b7run\u00b7ter", "s\u00e4hst", "du", "sonst", "das", "wei\u00b7\u00dfe", "Kleid", "der", "Fei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}