{"dta.poem.21740": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "M\u00fcd und matt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Thr&#228;nen,", "tokens": ["O", "Thr", "&#228;", "nen", ","], "token_info": ["word", "word", "XML_entity", "word", "punct"], "pos": ["NE", "NN", "$(", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Die ihr mich \u00fcberschwemmt;", "tokens": ["Die", "ihr", "mich", "\u00fc\u00b7bersc\u00b7hwemmt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Sehnen,", "tokens": ["O", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Das meine Brust beklemmt;", "tokens": ["Das", "mei\u00b7ne", "Brust", "be\u00b7klemmt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O Schmachten,", "tokens": ["O", "Schmach\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Davon mein Herz erschwillt;", "tokens": ["Da\u00b7von", "mein", "Herz", "er\u00b7schwillt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O himmelstrebend Trachten. . . .", "tokens": ["O", "him\u00b7mel\u00b7stre\u00b7bend", "Trach\u00b7ten", ".", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["NE", "VVPP", "NN", "$.", "$.", "$.", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wann, wann wirst du gestillt!", "tokens": ["Wann", ",", "wann", "wirst", "du", "ge\u00b7stillt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mich lasten", "tokens": ["Mich", "las\u00b7ten"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Der Erde Quaal und Lust.", "tokens": ["Der", "Er\u00b7de", "Qua\u00b7al", "und", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Zu rasten,", "tokens": ["Zu", "ras\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Gen\u00fcgt der m\u00fcden Brust.", "tokens": ["Ge\u00b7n\u00fcgt", "der", "m\u00fc\u00b7den", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nur Jammer", "tokens": ["Nur", "Jam\u00b7mer"], "token_info": ["word", "word"], "pos": ["ADV", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Daucht mir die Pracht der Welt.", "tokens": ["Daucht", "mir", "die", "Pracht", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O dunkle lezte Kammer,", "tokens": ["O", "dunk\u00b7le", "lez\u00b7te", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du bist's, die mir gef\u00e4llt.", "tokens": ["Du", "bist's", ",", "die", "mir", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich lechze,", "tokens": ["Ich", "lech\u00b7ze", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Und meine Kraft wird schwach.", "tokens": ["Und", "mei\u00b7ne", "Kraft", "wird", "schwach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich \u00e4chze", "tokens": ["Ich", "\u00e4ch\u00b7ze"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Nach der Erl\u00f6sung Tag.", "tokens": ["Nach", "der", "Er\u00b7l\u00f6\u00b7sung", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie lange", "tokens": ["Wie", "lan\u00b7ge"], "token_info": ["word", "word"], "pos": ["PWAV", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Soll ich mich qu\u00e4len noch!", "tokens": ["Soll", "ich", "mich", "qu\u00e4\u00b7len", "noch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie lange und wie bange", "tokens": ["Wie", "lan\u00b7ge", "und", "wie", "ban\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "KON", "PWAV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ziehn an dem herben Joch!", "tokens": ["Ziehn", "an", "dem", "her\u00b7ben", "Joch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Bande", "tokens": ["Die", "Ban\u00b7de"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Der Erde pressen mich.", "tokens": ["Der", "Er\u00b7de", "pres\u00b7sen", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Rande", "tokens": ["Am", "Ran\u00b7de"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "L\u00f6st jede Fessel sich.", "tokens": ["L\u00f6st", "je\u00b7de", "Fes\u00b7sel", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PRF", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "Am Rande", "tokens": ["Am", "Ran\u00b7de"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Schwank' ich; und schau' hinab,", "tokens": ["Schwank'", "ich", ";", "und", "schau'", "hin\u00b7ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und ledig aller Bande,", "tokens": ["Und", "le\u00b7dig", "al\u00b7ler", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sink' ich ins d\u00fcstre Grab!", "tokens": ["Sink'", "ich", "ins", "d\u00fcst\u00b7re", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}