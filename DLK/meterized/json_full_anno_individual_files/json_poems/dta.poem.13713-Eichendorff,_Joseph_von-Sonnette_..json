{"dta.poem.13713": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So la\u00df' Dein' Stimme hell im Wald erscheinen!", "tokens": ["So", "la\u00df'", "Dein'", "Stim\u00b7me", "hell", "im", "Wald", "er\u00b7schei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPOSAT", "NN", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Waldhorn fromm wird auf und nieder wehen,", "tokens": ["Das", "Wald\u00b7horn", "fromm", "wird", "auf", "und", "nie\u00b7der", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "APPR", "KON", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Wasser geh'n und einsam Rehe weiden.", "tokens": ["Die", "Was\u00b7ser", "geh'n", "und", "ein\u00b7sam", "Re\u00b7he", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}