{"textgrid.poem.26229": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Daheim, wo die Schneeflocken fliegen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Daheim liegt jetzt Eis auf der Stra\u00dfe,", "tokens": ["Da\u00b7heim", "liegt", "jetzt", "Eis", "auf", "der", "Stra\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Krieger kauern im Schnee.", "tokens": ["Die", "Krie\u00b7ger", "kau\u00b7ern", "im", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hier steht die Rose im Grase", "tokens": ["Hier", "steht", "die", "Ro\u00b7se", "im", "Gra\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie tut ihre Sch\u00f6nheit mir weh.", "tokens": ["Wie", "tut", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "mir", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Ich mag keine Rose hier sehen,", "tokens": ["Ich", "mag", "kei\u00b7ne", "Ro\u00b7se", "hier", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Daheim lauern Winter und Not.", "tokens": ["Da\u00b7hei