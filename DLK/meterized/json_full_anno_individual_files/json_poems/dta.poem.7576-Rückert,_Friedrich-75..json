{"dta.poem.7576": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "75.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Reich ist wol der Gehalt, allein die Form ist steif;", "tokens": ["Reich", "ist", "wol", "der", "Ge\u00b7halt", ",", "al\u00b7lein", "die", "Form", "ist", "steif", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Weich war die gr\u00fcne Saat, hier sind die Aehren reif. \u2014", "tokens": ["Weich", "war", "die", "gr\u00fc\u00b7ne", "Saat", ",", "hier", "sind", "die", "A\u00b7eh\u00b7ren", "reif", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "ADJD", "$.", "$("], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Drei S\u00e4le schritt ich durch, gebaut im Haus der Zeit,", "tokens": ["Drei", "S\u00e4\u00b7le", "schritt", "ich", "durch", ",", "ge\u00b7baut", "im", "Haus", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "VVPP", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr Zukunft, Gegenwart, und f\u00fcr Vergangenheit.", "tokens": ["F\u00fcr", "Zu\u00b7kunft", ",", "Ge\u00b7gen\u00b7wart", ",", "und", "f\u00fcr", "Ver\u00b7gan\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Im Saal der Zukunft sah ich farbige Tapeten", "tokens": ["Im", "Saal", "der", "Zu\u00b7kunft", "sah", "ich", "far\u00b7bi\u00b7ge", "Ta\u00b7pe\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Bildern, die heraus ins Leben wollten treten.", "tokens": ["Mit", "Bil\u00b7dern", ",", "die", "he\u00b7raus", "ins", "Le\u00b7ben", "woll\u00b7ten", "tre\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Im Saal der Gegenwart sah ich nach allen Seiten", "tokens": ["Im", "Saal", "der", "Ge\u00b7gen\u00b7wart", "sah", "ich", "nach", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die schon ins Leben eingetretnen Bilder schreiten.", "tokens": ["Die", "schon", "ins", "Le\u00b7ben", "ein\u00b7ge\u00b7tret\u00b7nen", "Bil\u00b7der", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Im der Vergangenheit geweihten Saale nun", "tokens": ["Im", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "ge\u00b7weih\u00b7ten", "Saa\u00b7le", "nun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ART", "NN", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sah ich zu Stein erstarrt die Lebensformen ruhn.", "tokens": ["Sah", "ich", "zu", "Stein", "er\u00b7starrt", "die", "Le\u00b7bens\u00b7for\u00b7men", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich sprach: Die Malerei malt uns der Zukunft Flor,", "tokens": ["Ich", "sprach", ":", "Die", "Ma\u00b7le\u00b7rei", "malt", "uns", "der", "Zu\u00b7kunft", "Flor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die Bildhauerei stellt das Vergangne vor.", "tokens": ["Und", "die", "Bild\u00b7hau\u00b7e\u00b7rei", "stellt", "das", "Ver\u00b7gang\u00b7ne", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "--++-+-+-+-+", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Es ist wol Poesie, die zwischen beiden Sf\u00e4ren", "tokens": ["Es", "ist", "wol", "Poe\u00b7sie", ",", "die", "zwi\u00b7schen", "bei\u00b7den", "Sf\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "PRELS", "APPR", "PIAT", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Uns die Gestalten soll der Gegenwart erkl\u00e4ren.", "tokens": ["Uns", "die", "Ge\u00b7stal\u00b7ten", "soll", "der", "Ge\u00b7gen\u00b7wart", "er\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Die ew'ge Gegenwart, was ist sie? die Natur:", "tokens": ["Die", "ew'\u00b7ge", "Ge\u00b7gen\u00b7wart", ",", "was", "ist", "sie", "?", "die", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWS", "VAFIN", "PPER", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Schein Vergangenheit, ein Schein die Zukunft nur.", "tokens": ["Ein", "Schein", "Ver\u00b7gan\u00b7gen\u00b7heit", ",", "ein", "Schein", "die", "Zu\u00b7kunft", "nur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Von hier und dort der Schein schwebt um die Wirklichkeit,", "tokens": ["Von", "hier", "und", "dort", "der", "Schein", "schwebt", "um", "die", "Wirk\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und immer tauscht den Platz Zukunft-Vergangenheit.", "tokens": ["Und", "im\u00b7mer", "tauscht", "den", "Platz", "Zu\u00b7kunft\u00b7Ver\u00b7gan\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Das Gegenw\u00e4rt'ge wird in Zukunft seyn vergangen,", "tokens": ["Das", "Ge\u00b7gen\u00b7w\u00e4rt'\u00b7ge", "wird", "in", "Zu\u00b7kunft", "seyn", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und das Vergangne hat als K\u00fcnft'ges angefangen.", "tokens": ["Und", "das", "Ver\u00b7gang\u00b7ne", "hat", "als", "K\u00fcnft'\u00b7ges", "an\u00b7ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Eh das Vergangne war, war es als Zukunft schon;", "tokens": ["Eh", "das", "Ver\u00b7gang\u00b7ne", "war", ",", "war", "es", "als", "Zu\u00b7kunft", "schon", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "$,", "VAFIN", "PPER", "KOUS", "NN", "ADV", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Und Alles bleibt im Jetzt, wann Einst und Einst entflohn.", "tokens": ["Und", "Al\u00b7les", "bleibt", "im", "Jetzt", ",", "wann", "Einst", "und", "Einst", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPRART", "ADV", "$,", "PWAV", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}