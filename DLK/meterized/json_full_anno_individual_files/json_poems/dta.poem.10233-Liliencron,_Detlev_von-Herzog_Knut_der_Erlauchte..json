{"dta.poem.10233": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Herzog Knut der Erlauchte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nig Niels, der Alte, wei\u00dfb&#228;rtig und kahl,             ", "tokens": ["K\u00f6\u00b7nig", "Niels", ",", "der", "Al\u00b7te", ",", "wei\u00dfb", "&#228;", "rtig", "und", "kahl", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "$,", "KOUS", "$(", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Aus schwarzem Himmel schie\u00dfen fahl", "tokens": ["Aus", "schwar\u00b7zem", "Him\u00b7mel", "schie\u00b7\u00dfen", "fahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Niels Sohn, K\u00f6nig Magnus von Westgothland,", "tokens": ["Niels", "Sohn", ",", "K\u00f6\u00b7nig", "Mag\u00b7nus", "von", "West\u00b7got\u00b7hland", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "NE", "APPR", "NE", "$,"], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der L\u00f6we Sturm kam hergerannt", "tokens": ["Der", "L\u00f6\u00b7we", "Sturm", "kam", "her\u00b7ge\u00b7rannt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Bl\u00fcmchen f\u00e4llt aus dem Blitzestrau\u00df", "tokens": ["Ein", "Bl\u00fcm\u00b7chen", "f\u00e4llt", "aus", "dem", "Blit\u00b7zes\u00b7trau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Regen gie\u00dft in Tonnen aus", "tokens": ["Der", "Re\u00b7gen", "gie\u00dft", "in", "Ton\u00b7nen", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "K\u00f6nig Niels schlug mit der Faust auf den Tisch,", "tokens": ["K\u00f6\u00b7nig", "Niels", "schlug", "mit", "der", "Faust", "auf", "den", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "\u201ewann endlich zappelt Knut, der Fisch,", "tokens": ["\u201e", "wann", "end\u00b7lich", "zap\u00b7pelt", "Knut", ",", "der", "Fisch", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADV", "VVFIN", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "K\u00f6nig Magnus, ich sehe Walhalla geschm\u00fcckt,", "tokens": ["K\u00f6\u00b7nig", "Mag\u00b7nus", ",", "ich", "se\u00b7he", "Wal\u00b7hal\u00b7la", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VVFIN", "NE", "VVPP", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wenn ich gestorben, dann stehst du geb\u00fcckt", "tokens": ["Wenn", "ich", "ge\u00b7stor\u00b7ben", ",", "dann", "stehst", "du", "ge\u00b7b\u00fcckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "$,", "ADV", "VVFIN", "PPER", "VVPP"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Nicht l\u00e4nger h\u00e4lst du sein Recht in Bann,", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "h\u00e4lst", "du", "sein", "Recht", "in", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und schaut dich kaum vom Sattel an,", "tokens": ["Und", "schaut", "dich", "kaum", "vom", "Sat\u00b7tel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "K\u00f6nig Magnus scho\u00df einen Blick so wild,", "tokens": ["K\u00f6\u00b7nig", "Mag\u00b7nus", "scho\u00df", "ei\u00b7nen", "Blick", "so", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Von den W\u00e4nden st\u00fcrzen Helm und Schild", "tokens": ["Von", "den", "W\u00e4n\u00b7den", "st\u00fcr\u00b7zen", "Helm", "und", "Schild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}