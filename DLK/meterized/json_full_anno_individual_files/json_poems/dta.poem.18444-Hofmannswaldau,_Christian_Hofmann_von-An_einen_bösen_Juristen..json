{"dta.poem.18444": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An einen b\u00f6sen Juristen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df du sehr wenig seyst im rechten unterwiesen/", "tokens": ["Da\u00df", "du", "sehr", "we\u00b7nig", "seyst", "im", "rech\u00b7ten", "un\u00b7ter\u00b7wie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VAFIN", "APPRART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bewei\u00df ich dir aus diesen:", "tokens": ["Be\u00b7wei\u00df", "ich", "dir", "aus", "die\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "PDAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das recht hat dreyerley: Leb\u2019 in der erbarkeit/", "tokens": ["Das", "recht", "hat", "drey\u00b7er\u00b7ley", ":", "Leb'", "in", "der", "er\u00b7bar\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "CARD", "$.", "VVIMP", "APPR", "ART", "NN", "$("], "meter": "-+-+--+-+---", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Thu keinem menschen leidt", "tokens": ["Thu", "kei\u00b7nem", "men\u00b7schen", "leidt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gib jedem was ihm soll; Nun ist dein gantzes leben", "tokens": ["Gib", "je\u00b7dem", "was", "ihm", "soll", ";", "Nun", "ist", "dein", "gant\u00b7zes", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PIAT", "PWS", "PPER", "VMFIN", "$.", "ADV", "VAFIN", "PPOSAT", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der \u00fcppigkeit ergeben/", "tokens": ["Der", "\u00fcp\u00b7pig\u00b7keit", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wer kan vor deinem maul an ehren sicher seyn/", "tokens": ["Wer", "kan", "vor", "dei\u00b7nem", "maul", "an", "eh\u00b7ren", "si\u00b7cher", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "PPOSAT", "NN", "APPR", "VVFIN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die schmach ist dir gemein/", "tokens": ["Die", "schmach", "ist", "dir", "ge\u00b7mein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wem zahlstu/ was dn solt/ bewei\u00df ich nicht aus diesen", "tokens": ["Wem", "zahl\u00b7stu", "/", "was", "dn", "solt", "/", "be\u00b7wei\u00df", "ich", "nicht", "aus", "die\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$(", "PWS", "VVINF", "VMFIN", "$(", "VVFIN", "PPER", "PTKNEG", "APPR", "PDAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie schlecht du unterwiesen?", "tokens": ["Wie", "schlecht", "du", "un\u00b7ter\u00b7wie\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}