{"dta.poem.15710": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von Aberglauben ist Unglauben stets begleitet,", "tokens": ["Von", "A\u00b7berg\u00b7lau\u00b7ben", "ist", "Un\u00b7glau\u00b7ben", "stets", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Aberglauben hat zum Glauben oft geleitet.", "tokens": ["Und", "A\u00b7berg\u00b7lau\u00b7ben", "hat", "zum", "Glau\u00b7ben", "oft", "ge\u00b7lei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So im Unglauben ist der Glaube schon enthalten;", "tokens": ["So", "im", "Un\u00b7glau\u00b7ben", "ist", "der", "Glau\u00b7be", "schon", "ent\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch Gottes Kraft geweckt, wird er sich draus entfalten.", "tokens": ["Durch", "Got\u00b7tes", "Kraft", "ge\u00b7weckt", ",", "wird", "er", "sich", "draus", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$,", "VAFIN", "PPER", "PRF", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}