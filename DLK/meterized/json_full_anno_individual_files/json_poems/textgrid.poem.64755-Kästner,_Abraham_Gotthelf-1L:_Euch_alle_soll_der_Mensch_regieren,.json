{"textgrid.poem.64755": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Euch alle soll der Mensch regieren,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Euch alle soll der Mensch regieren,", "tokens": ["Euch", "al\u00b7le", "soll", "der", "Mensch", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach einst ", "tokens": ["Sprach", "einst"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der Mensch, das Werk von meiner Hand.", "tokens": ["Der", "Mensch", ",", "das", "Werk", "von", "mei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie? lie\u00dfen sie sich trotzig h\u00f6ren,", "tokens": ["Wie", "?", "lie\u00b7\u00dfen", "sie", "sich", "trot\u00b7zig", "h\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gebeut was Gr\u00f6\u00dfers uns zu ehren,", "tokens": ["Ge\u00b7beut", "was", "Gr\u00f6\u00b7\u00dfers", "uns", "zu", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Mensch wird nicht von uns erkannt.", "tokens": ["Der", "Mensch", "wird", "nicht", "von", "uns", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die\u00df Wesen ohne Kraft und Waffen,", "tokens": ["Die\u00df", "We\u00b7sen", "oh\u00b7ne", "Kraft", "und", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die\u00df hast du uns zum Herrn erschaffen?", "tokens": ["Die\u00df", "hast", "du", "uns", "zum", "Herrn", "er\u00b7schaf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So fragt der Leue schon ergrimmt:", "tokens": ["So", "fragt", "der", "Leu\u00b7e", "schon", "er\u00b7grimmt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann will ich seine Herrschaft schauen,", "tokens": ["Dann", "will", "ich", "sei\u00b7ne", "Herr\u00b7schaft", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn er, zerfleischt von meinen Klauen,", "tokens": ["Wenn", "er", ",", "zer\u00b7fleischt", "von", "mei\u00b7nen", "Klau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In seinem Blute vor mir schwimmt.", "tokens": ["In", "sei\u00b7nem", "Blu\u00b7te", "vor", "mir", "schwimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich? sprach der Adler, soll ihm n\u00fctzen?", "tokens": ["Ich", "?", "sprach", "der", "Ad\u00b7ler", ",", "soll", "ihm", "n\u00fct\u00b7zen", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "VVFIN", "ART", "NN", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Gemsen nie erstiegnen Spitzen", "tokens": ["Auf", "Gem\u00b7sen", "nie", "er\u00b7sti\u00b7eg\u00b7nen", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wird ihm mein Wohnhaus wohl entdeckt?", "tokens": ["Wird", "ihm", "mein", "Wohn\u00b7haus", "wohl", "ent\u00b7deckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wodurch denn will er mich bezwingen?", "tokens": ["Wo\u00b7durch", "denn", "will", "er", "mich", "be\u00b7zwin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der k\u00fchne Flug von meinen Schwingen", "tokens": ["Der", "k\u00fch\u00b7ne", "Flug", "von", "mei\u00b7nen", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Geht h\u00f6her, als sein Blick sich streckt.", "tokens": ["Geht", "h\u00f6\u00b7her", ",", "als", "sein", "Blick", "sich", "streckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mir, schlo\u00df der Wallfisch, zu gebieten?", "tokens": ["Mir", ",", "schlo\u00df", "der", "Wall\u00b7fisch", ",", "zu", "ge\u00b7bie\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Soll er, wo Frost und Wellen w\u00fcthen,", "tokens": ["Soll", "er", ",", "wo", "Frost", "und", "Wel\u00b7len", "w\u00fct\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Mir mehr, als beyde, furchtbar seyn?", "tokens": ["Mir", "mehr", ",", "als", "bey\u00b7de", ",", "furcht\u00b7bar", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUS", "PIS", "$,", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nein, Heerden von dergleichen Thieren", "tokens": ["Nein", ",", "Heer\u00b7den", "von", "derg\u00b7lei\u00b7chen", "Thie\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "APPR", "PIS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Will ich, mein Schwanz darf sie nur r\u00fchren,", "tokens": ["Will", "ich", ",", "mein", "Schwanz", "darf", "sie", "nur", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PPOSAT", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zerschmettert in das Meer zerstreun.", "tokens": ["Zer\u00b7schmet\u00b7tert", "in", "das", "Meer", "zer\u00b7streun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wi\u00dft, hat sie drauf der Gott belehret,", "tokens": ["Wi\u00dft", ",", "hat", "sie", "drauf", "der", "Gott", "be\u00b7leh\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "PPER", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schwach, unbefl\u00fcgelt, unbewehret", "tokens": ["Schwach", ",", "un\u00b7be\u00b7fl\u00fc\u00b7gelt", ",", "un\u00b7be\u00b7weh\u00b7ret"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist er doch m\u00e4chtiger, als ihr.", "tokens": ["Ist", "er", "doch", "m\u00e4ch\u00b7ti\u00b7ger", ",", "als", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJA", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was mehr, als St\u00e4rke, Flug und Waffen,", "tokens": ["Was", "mehr", ",", "als", "St\u00e4r\u00b7ke", ",", "Flug", "und", "Waf\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "KOUS", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wird ihm nur eine Kraft verschaffen,", "tokens": ["Wird", "ihm", "nur", "ei\u00b7ne", "Kraft", "ver\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und diese Kraft besitzt kein Thier.", "tokens": ["Und", "die\u00b7se", "Kraft", "be\u00b7sitzt", "kein", "Thier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann trotz' ein L\u00f6w' auf seine Klauen,", "tokens": ["Dann", "trotz'", "ein", "L\u00f6\u00b7w'", "auf", "sei\u00b7ne", "Klau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn er, durch sch\u00e4rfern Stahl zerhauen,", "tokens": ["Wenn", "er", ",", "durch", "sch\u00e4r\u00b7fern", "Stahl", "zer\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vor ihm in seinem Blute liegt.", "tokens": ["Vor", "ihm", "in", "sei\u00b7nem", "Blu\u00b7te", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann wird der Adler ihm enteilen,", "tokens": ["Dann", "wird", "der", "Ad\u00b7ler", "ihm", "ent\u00b7ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn durch die Luft auf schnellern Pfeilen", "tokens": ["Wenn", "durch", "die", "Luft", "auf", "schnel\u00b7lern", "Pfei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Tod unfehlbar nach ihm fliegt.", "tokens": ["Der", "Tod", "un\u00b7fehl\u00b7bar", "nach", "ihm", "fliegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du w\u00fcthest nur, um mehr zu bluten,", "tokens": ["Du", "w\u00fct\u00b7hest", "nur", ",", "um", "mehr", "zu", "blu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUI", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Belebtes Eyland kalter Fluthen,", "tokens": ["Be\u00b7leb\u00b7tes", "Ey\u00b7land", "kal\u00b7ter", "Flut\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn dich sein k\u00fchner Schu\u00df verletzt", "tokens": ["Wenn", "dich", "sein", "k\u00fch\u00b7ner", "Schu\u00df", "ver\u00b7letzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor Menschen wird der Stolz entweichen,", "tokens": ["Vor", "Men\u00b7schen", "wird", "der", "Stolz", "ent\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die selbst der Rest von ihres gleichen", "tokens": ["Die", "selbst", "der", "Rest", "von", "ih\u00b7res", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ver\u00e4chtlich, fast zu Thieren setzt.", "tokens": ["Ver\u00b7\u00e4cht\u00b7lich", ",", "fast", "zu", "Thie\u00b7ren", "setzt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Kraft, durch die ihm wird gelingen,", "tokens": ["Die", "Kraft", ",", "durch", "die", "ihm", "wird", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Luft, Erd' und Wasser zu bezwingen,", "tokens": ["Luft", ",", "Erd'", "und", "Was\u00b7ser", "zu", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die bleibt euch ewig unbekannt.", "tokens": ["Die", "bleibt", "euch", "e\u00b7wig", "un\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu bald nur wird sie euch zum Schrecken", "tokens": ["Zu", "bald", "nur", "wird", "sie", "euch", "zum", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "VAFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch ihre Wirkung sich entdecken,", "tokens": ["Durch", "ih\u00b7re", "Wir\u00b7kung", "sich", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Den Namen h\u00f6rt: sie hei\u00dft ", "tokens": ["Den", "Na\u00b7men", "h\u00f6rt", ":", "sie", "hei\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Euch alle soll der Mensch regieren,", "tokens": ["Euch", "al\u00b7le", "soll", "der", "Mensch", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach einst ", "tokens": ["Sprach", "einst"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der Mensch, das Werk von meiner Hand.", "tokens": ["Der", "Mensch", ",", "das", "Werk", "von", "mei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie? lie\u00dfen sie sich trotzig h\u00f6ren,", "tokens": ["Wie", "?", "lie\u00b7\u00dfen", "sie", "sich", "trot\u00b7zig", "h\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gebeut was Gr\u00f6\u00df