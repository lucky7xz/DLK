{"dta.poem.3192": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist zu wissen werth, was ist nicht werth zu wissen?", "tokens": ["Was", "ist", "zu", "wis\u00b7sen", "werth", ",", "was", "ist", "nicht", "werth", "zu", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKZU", "VVINF", "ADJD", "$,", "PWS", "VAFIN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Wissensw\u00fcrdigsten h\u00e4tt' ich mich gern beflissen.", "tokens": ["Des", "Wis\u00b7sens\u00b7w\u00fcr\u00b7digs\u00b7ten", "h\u00e4tt'", "ich", "mich", "gern", "be\u00b7flis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Gleichwerth ist alles wohl zu wissen, w\u00e4re nur", "tokens": ["Gleich\u00b7werth", "ist", "al\u00b7les", "wohl", "zu", "wis\u00b7sen", ",", "w\u00e4\u00b7re", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "PIS", "ADV", "PTKZU", "VVINF", "$,", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Leben lang genug, zu gehn auf jeder Spur.", "tokens": ["Das", "Le\u00b7ben", "lang", "ge\u00b7nug", ",", "zu", "gehn", "auf", "je\u00b7der", "Spur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "$,", "PTKZU", "VVINF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Darum verlier' nicht Zeit zu fragen, was nun frommt", "tokens": ["Da\u00b7rum", "ver\u00b7lier'", "nicht", "Zeit", "zu", "fra\u00b7gen", ",", "was", "nun", "frommt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PTKNEG", "NN", "PTKZU", "VVINF", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu lernen, sondern lern', was in den Wurf dir kommt.", "tokens": ["Zu", "ler\u00b7nen", ",", "son\u00b7dern", "lern'", ",", "was", "in", "den", "Wurf", "dir", "kommt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KON", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Am besten aber, was gleich frisch ist, zu verbrauchen;", "tokens": ["Am", "bes\u00b7ten", "a\u00b7ber", ",", "was", "gleich", "frisch", "ist", ",", "zu", "ver\u00b7brau\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADV", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn was du lang' aufsparst, wird \u00fcber Nacht verrauchen.", "tokens": ["Denn", "was", "du", "lang'", "auf\u00b7sparst", ",", "wird", "\u00fc\u00b7ber", "Nacht", "ver\u00b7rau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "$,", "VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}