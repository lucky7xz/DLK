{"textgrid.poem.64108": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Drusus", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drusus sah, der R\u00f6merheros,", "tokens": ["Dru\u00b7sus", "sah", ",", "der", "R\u00f6\u00b7mer\u00b7he\u00b7ros", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhmgekr\u00f6nt in zwanzig Siegen,", "tokens": ["Ruhm\u00b7ge\u00b7kr\u00f6nt", "in", "zwan\u00b7zig", "Sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00e4nzend durch die dunkeln W\u00e4lder", "tokens": ["Gl\u00e4n\u00b7zend", "durch", "die", "dun\u00b7keln", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine goldnen Adler fliegen.", "tokens": ["Sei\u00b7ne", "gold\u00b7nen", "Ad\u00b7ler", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mitten im bezwungnen Lande", "tokens": ["Mit\u00b7ten", "im", "be\u00b7zwung\u00b7nen", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lag sein wallgeschirmtes Lager,", "tokens": ["Lag", "sein", "wall\u00b7ge\u00b7schirm\u00b7tes", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie der Knoten all' der Bande,", "tokens": ["Wie", "der", "Kno\u00b7ten", "all'", "der", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die umstricken die Germanen.", "tokens": ["Die", "um\u00b7stri\u00b7cken", "die", "Ger\u00b7ma\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Schamrot starke M\u00e4nner schau'n", "tokens": ["Scham\u00b7rot", "star\u00b7ke", "M\u00e4n\u00b7ner", "schau'n"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "In das Antlitz ihrer Frau'n. \u2013", "tokens": ["In", "das", "Ant\u00b7litz", "ih\u00b7rer", "Frau'", "n.", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "abbreviation", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "An dem gr\u00fcnen Elbeufer", "tokens": ["An", "dem", "gr\u00fc\u00b7nen", "E\u00b7lbeu\u00b7fer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.12": {"text": "Rauschen ernst und doch gelinde,", "tokens": ["Rau\u00b7schen", "ernst", "und", "doch", "ge\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Rauschen wie vor Wotans Hauche", "tokens": ["Rau\u00b7schen", "wie", "vor", "Wo\u00b7tans", "Hau\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Eichen in dem Abendwinde.", "tokens": ["Ei\u00b7chen", "in", "dem", "Ab\u00b7end\u00b7win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Sieh, in Gold und Purpur schreitet", "tokens": ["Sieh", ",", "in", "Gold", "und", "Pur\u00b7pur", "schrei\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Da ein Mann mit Schwert und Zepter,", "tokens": ["Da", "ein", "Mann", "mit", "Schwert", "und", "Zep\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und so fern die Flur sich weitet,", "tokens": ["Und", "so", "fern", "die", "Flur", "sich", "wei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.18": {"text": "Wirst sein flammend R\u00f6merauge", "tokens": ["Wirst", "sein", "flam\u00b7mend", "R\u00f6\u00b7me\u00b7rau\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Ein gebietend Siegerdrohn: \u2013", "tokens": ["Ein", "ge\u00b7bie\u00b7tend", "Sie\u00b7ger\u00b7drohn", ":", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "VVPP", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Drusus ist's, der Kaisersohn.", "tokens": ["Dru\u00b7sus", "ist's", ",", "der", "Kai\u00b7ser\u00b7sohn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "In der eignen Kraft Bewu\u00dftsein,", "tokens": ["In", "der", "eig\u00b7nen", "Kraft", "Be\u00b7wu\u00df\u00b7tsein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Im Gef\u00fchl von Romas Hoheit", "tokens": ["Im", "Ge\u00b7f\u00fchl", "von", "Ro\u00b7mas", "Ho\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Spricht er: \u00bbZittre, schn\u00f6de Wildnis,", "tokens": ["Spricht", "er", ":", "\u00bb", "Zitt\u00b7re", ",", "schn\u00f6\u00b7de", "Wild\u00b7nis", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Letzte Zuflucht trotz'ger Roheit;", "tokens": ["Letz\u00b7te", "Zu\u00b7flucht", "trotz'\u00b7ger", "Ro\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Deine W\u00e4lder will ich lichten,", "tokens": ["Dei\u00b7ne", "W\u00e4l\u00b7der", "will", "ich", "lich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Deine Felsen will ich brechen,", "tokens": ["Dei\u00b7ne", "Fel\u00b7sen", "will", "ich", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Deinen Freiheitsstolz zernichten,", "tokens": ["Dei\u00b7nen", "Frei\u00b7heits\u00b7stolz", "zer\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Und, gezw\u00e4ngt in Damm und Br\u00fccken,", "tokens": ["Und", ",", "ge\u00b7zw\u00e4ngt", "in", "Damm", "und", "Br\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Spiegle der bezwungene Strom", "tokens": ["Spieg\u00b7le", "der", "be\u00b7zwun\u00b7ge\u00b7ne", "Strom"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.30": {"text": "Deine Herrschaft, ew'ges Rom.\u00ab \u2013", "tokens": ["Dei\u00b7ne", "Herr\u00b7schaft", ",", "ew'\u00b7ges", "Rom", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NE", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Horch, da rauscht es in den Fluten,", "tokens": ["Horch", ",", "da", "rauscht", "es", "in", "den", "Flu\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Horch, da bricht es in den Zweigen,", "tokens": ["Horch", ",", "da", "bricht", "es", "in", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Aus dem Flusse sieht der R\u00f6mer", "tokens": ["Aus", "dem", "Flus\u00b7se", "sieht", "der", "R\u00f6\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Eine G\u00f6tterjungfrau steigen:", "tokens": ["Ei\u00b7ne", "G\u00f6t\u00b7ter\u00b7jung\u00b7frau", "stei\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Gr\u00fcnend durch die gelben Haare", "tokens": ["Gr\u00fc\u00b7nend", "durch", "die", "gel\u00b7ben", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Windet sich der feuchte Schilfkranz,", "tokens": ["Win\u00b7det", "sich", "der", "feuch\u00b7te", "Schilf\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Riesig ragt die Wunderbare", "tokens": ["Rie\u00b7sig", "ragt", "die", "Wun\u00b7der\u00b7ba\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "In den ahnungsvollen Mondglanz,", "tokens": ["In", "den", "ah\u00b7nungs\u00b7vol\u00b7len", "Mond\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Bebend lauscht der Kaisersohn", "tokens": ["Be\u00b7bend", "lauscht", "der", "Kai\u00b7ser\u00b7sohn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Der gewalt'gen Stimme Drohn:", "tokens": ["Der", "ge\u00b7walt'\u00b7gen", "Stim\u00b7me", "Drohn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "\u00bbdrusus, Drusus, kehre heimw\u00e4rts,", "tokens": ["\u00bb", "dru\u00b7sus", ",", "Dru\u00b7sus", ",", "keh\u00b7re", "heim\u00b7w\u00e4rts", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "NE", "$,", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Fliehe, nimmersatter Streiter!", "tokens": ["Flie\u00b7he", ",", "nim\u00b7mer\u00b7sat\u00b7ter", "Strei\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Bis hierher f\u00fchrt dich dein Schicksal,", "tokens": ["Bis", "hier\u00b7her", "f\u00fchrt", "dich", "dein", "Schick\u00b7sal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PAV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.44": {"text": "Doch es f\u00fchrt dich nimmer weiter:", "tokens": ["Doch", "es", "f\u00fchrt", "dich", "nim\u00b7mer", "wei\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Ich besch\u00fctze meine Gauen!", "tokens": ["Ich", "be\u00b7sch\u00fct\u00b7ze", "mei\u00b7ne", "Gau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Aber einstens aus dem Tiber", "tokens": ["A\u00b7ber", "eins\u00b7tens", "aus", "dem", "Ti\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Tauchen keine G\u00f6tterfrauen,", "tokens": ["Tau\u00b7chen", "kei\u00b7ne", "G\u00f6t\u00b7ter\u00b7frau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Also auch zur Flucht zu scheuchen", "tokens": ["Al\u00b7so", "auch", "zur", "Flucht", "zu", "scheu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Vor dem siegentkr\u00f6nten Rom", "tokens": ["Vor", "dem", "sie\u00b7gent\u00b7kr\u00f6n\u00b7ten", "Rom"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.50": {"text": "Meiner blonden S\u00f6hne Strom.\u00ab", "tokens": ["Mei\u00b7ner", "blon\u00b7den", "S\u00f6h\u00b7ne", "Strom", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Und das Weib versinket wieder,", "tokens": ["Und", "das", "Weib", "ver\u00b7sin\u00b7ket", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Finster dr\u00e4uend mit der Rechten.", "tokens": ["Fins\u00b7ter", "dr\u00e4u\u00b7end", "mit", "der", "Rech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Und es bebt der Imperator", "tokens": ["Und", "es", "bebt", "der", "Im\u00b7pe\u00b7ra\u00b7tor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.54": {"text": "Vor den ew'gen Schicksalsm\u00e4chten.", "tokens": ["Vor", "den", "ew'\u00b7gen", "Schick\u00b7sals\u00b7m\u00e4ch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Bleich, entsetzt st\u00fcrzt er ins Lager,", "tokens": ["Bleich", ",", "ent\u00b7setzt", "st\u00fcrzt", "er", "ins", "La\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "R\u00fcckw\u00e4rts f\u00fchrt er seine Adler,", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "f\u00fchrt", "er", "sei\u00b7ne", "Ad\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Und der gro\u00dfe Schlachtenschlager, \u2013", "tokens": ["Und", "der", "gro\u00b7\u00dfe", "Schlach\u00b7ten\u00b7schla\u00b7ger", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Tot lag er am dritten Tage.", "tokens": ["Tot", "lag", "er", "am", "drit\u00b7ten", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.59": {"text": "Und es sah kein R\u00f6merheer", "tokens": ["Und", "es", "sah", "kein", "R\u00f6\u00b7mer\u00b7heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.60": {"text": "Je die Elbeufer mehr.", "tokens": ["Je", "die", "E\u00b7lbeu\u00b7fer", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Drusus sah, der R\u00f6merheros,", "tokens": ["Dru\u00b7sus", "sah", ",", "der", "R\u00f6\u00b7mer\u00b7he\u00b7ros", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhmgekr\u00f6nt in zwanzig Siegen,", "tokens": ["Ruhm\u00b7ge\u00b7kr\u00f6nt", "in", "zwan\u00b7zig", "Sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00e4nzend durch die dunkeln W\u00e4lder", "tokens": ["Gl\u00e4n\u00b7zend", "durch", "die", "dun\u00b7keln", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine goldnen Adler fliegen.", "tokens": ["Sei\u00b7ne", "gold\u00b7nen", "Ad\u00b7ler", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mitten im bezwungnen Lande", "tokens": ["Mit\u00b7ten", "im", "be\u00b7zwung\u00b7nen", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lag sein wallgeschirmtes Lager,", "tokens": ["Lag", "sein", "wall\u00b7ge\u00b7schirm\u00b7tes", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie der Knoten all' der Bande,", "tokens": ["Wie", "der", "Kno\u00b7ten", "all'", "der", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die umstricken die Germanen.", "tokens": ["Die", "um\u00b7stri\u00b7cken", "die", "Ger\u00b7ma\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Schamrot starke M\u00e4nner schau'n", "tokens": ["Scham\u00b7rot", "star\u00b7ke", "M\u00e4n\u00b7ner", "schau'n"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "In das Antlitz ihrer Frau'n. \u2013", "tokens": ["In", "das", "Ant\u00b7litz", "ih\u00b7rer", "Frau'", "n.", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "abbreviation", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "An dem gr\u00fcnen Elbeufer", "tokens": ["An", "dem", "gr\u00fc\u00b7nen", "E\u00b7lbeu\u00b7fer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.12": {"text": "Rauschen ernst und doch gelinde,", "tokens": ["Rau\u00b7schen", "ernst", "und", "doch", "ge\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Rauschen wie vor Wotans Hauche", "tokens": ["Rau\u00b7schen", "wie", "vor", "Wo\u00b7tans", "Hau\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Eichen in dem Abendwinde.", "tokens": ["Ei\u00b7chen", "in", "dem", "Ab\u00b7end\u00b7win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Sieh, in Gold und Purpur schreitet", "tokens": ["Sieh", ",", "in", "Gold", "und", "Pur\u00b7pur", "schrei\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Da ein Mann mit Schwert und Zepter,", "tokens": ["Da", "ein", "Mann", "mit", "Schwert", "und", "Zep\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und so fern die Flur sich weitet,", "tokens": ["Und", "so", "fern", "die", "Flur", "sich", "wei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.18": {"text": "Wirst sein flammend R\u00f6merauge", "tokens": ["Wirst", "sein", "flam\u00b7mend", "R\u00f6\u00b7me\u00b7rau\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Ein gebietend Siegerdrohn: \u2013", "tok