{"dta.poem.10642": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet au\u00df der Italienischen Poetin  \n  Veronica Gambara.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr sch\u00f6ne Wasserb\u00e4ch/ jhr Vfer an den Fl\u00fcssen/", "tokens": ["Ihr", "sch\u00f6\u00b7ne", "Was\u00b7ser\u00b7b\u00e4ch", "/", "jhr", "Vfer", "an", "den", "Fl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da sich des Himmels Lufft erzeigt so sch\u00f6n vnd klar", "tokens": ["Da", "sich", "des", "Him\u00b7mels", "Lufft", "er\u00b7zeigt", "so", "sch\u00f6n", "vnd", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als jrgendt/ vnd ersch\u00f6pfft an euch die Gaben gar/", "tokens": ["Als", "jr\u00b7gendt", "/", "vnd", "er\u00b7sch\u00f6pfft", "an", "euch", "die", "Ga\u00b7ben", "gar", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$(", "KON", "VVFIN", "APPR", "PPER", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ander \u00f6rter sonst fast sparsamlich geniessen/", "tokens": ["Die", "an\u00b7der", "\u00f6r\u00b7ter", "sonst", "fast", "spar\u00b7sam\u00b7lich", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wann sich di\u00df mein Sonnet so zierlich k\u00f6nte schliessen/", "tokens": ["Wann", "sich", "di\u00df", "mein", "Son\u00b7net", "so", "zier\u00b7lich", "k\u00f6n\u00b7te", "schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PDS", "PPOSAT", "NN", "ADV", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Als es von Hertzen geht/ es w\u00fcrden offenbahr", "tokens": ["Als", "es", "von", "Hert\u00b7zen", "geht", "/", "es", "w\u00fcr\u00b7den", "of\u00b7fen\u00b7bahr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$(", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch meine sch\u00f6ne Vers all ewrer Gaben schar/", "tokens": ["Durch", "mei\u00b7ne", "sch\u00f6\u00b7ne", "Vers", "all", "ew\u00b7rer", "Ga\u00b7ben", "schar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PIAT", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Man solte weit vnd breit von euch zureden wissen/", "tokens": ["Man", "sol\u00b7te", "weit", "vnd", "breit", "von", "euch", "zu\u00b7re\u00b7den", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADJD", "KON", "ADJD", "APPR", "PPER", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nun aber meine reim\u2019 vnd vngelehrte Sinnen", "tokens": ["Nun", "a\u00b7ber", "mei\u00b7ne", "reim'", "vnd", "vn\u00b7ge\u00b7lehr\u00b7te", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "VVFIN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den H\u00fcgel ewrer Ehr nicht vberstehen k\u00fcnnen/", "tokens": ["Den", "H\u00fc\u00b7gel", "ew\u00b7rer", "Ehr", "nicht", "vbers\u00b7te\u00b7hen", "k\u00fcn\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKNEG", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Erliegen sie/ weil jhr so hoch gestiegen seit/", "tokens": ["Er\u00b7lie\u00b7gen", "sie", "/", "weil", "jhr", "so", "hoch", "ge\u00b7stie\u00b7gen", "seit", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "ADJD", "VVPP", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich achte mich nicht werth mit ewrem Lob zuschertzen/", "tokens": ["Ich", "ach\u00b7te", "mich", "nicht", "werth", "mit", "ew\u00b7rem", "Lob", "zu\u00b7schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch hab ich hier viel Frewd empfangen in dem Hertzen;", "tokens": ["Doch", "hab", "ich", "hier", "viel", "Frewd", "emp\u00b7fan\u00b7gen", "in", "dem", "Hert\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PIAT", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mit dieser bin ich euch zu ehren gantz bereit.", "tokens": ["Mit", "die\u00b7ser", "bin", "ich", "euch", "zu", "eh\u00b7ren", "gantz", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VAFIN", "PPER", "PPER", "PTKZU", "VVINF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}