{"textgrid.poem.40384": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Ein Weltreisender", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann Weltreisender werden,", "tokens": ["Ich", "kann", "Welt\u00b7rei\u00b7sen\u00b7der", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VAINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "wo keine Eisenbahn geht:", "tokens": ["wo", "kei\u00b7ne", "Ei\u00b7sen\u00b7bahn", "geht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.3": {"text": "wo \u00fcberm ewigen Eismeergrab", "tokens": ["wo", "\u00fc\u00b7berm", "e\u00b7wi\u00b7gen", "Eis\u00b7meer\u00b7grab"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "die Nordlichtkrone steht,", "tokens": ["die", "Nord\u00b7licht\u00b7kro\u00b7ne", "steht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "die Krone der ganzen Erde.", "tokens": ["die", "Kro\u00b7ne", "der", "gan\u00b7zen", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Oder wo hei\u00df die Wildnis", "tokens": ["O\u00b7der", "wo", "hei\u00df", "die", "Wild\u00b7nis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "nur Gr\u00fc\u00dfe Gottes haucht,", "tokens": ["nur", "Gr\u00fc\u00b7\u00dfe", "Got\u00b7tes", "haucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und wo die liebe Seele", "tokens": ["und", "wo", "die", "lie\u00b7be", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "keinen andern Wegweiser braucht", "tokens": ["kei\u00b7nen", "an\u00b7dern", "Weg\u00b7wei\u00b7ser", "braucht"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "als Sonne, Mond und Sterne.", "tokens": ["als", "Son\u00b7ne", ",", "Mond", "und", "Ster\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und treff ich mal auf Menschen,", "tokens": ["Und", "treff", "ich", "mal", "auf", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "die sind wohl nicht wie ich;", "tokens": ["die", "sind", "wohl", "nicht", "wie", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PTKNEG", "PWAV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ihr Gott, der hei\u00dft wohl Fitzebutze,", "tokens": ["ihr", "Gott", ",", "der", "hei\u00dft", "wohl", "Fit\u00b7ze\u00b7but\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ihr H\u00e4uptling Duckedich \u2013", "tokens": ["ihr", "H\u00e4upt\u00b7ling", "Du\u00b7cke\u00b7dich", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "hurra, das gibt einen Hauptspa\u00df!", "tokens": ["hur\u00b7ra", ",", "das", "gibt", "ei\u00b7nen", "Haupt\u00b7spa\u00df", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Ich ducke sie noch ein bi\u00dfchen", "tokens": ["Ich", "du\u00b7cke", "sie", "noch", "ein", "bi\u00df\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "tiefer zum Schabernack;", "tokens": ["tie\u00b7fer", "zum", "Scha\u00b7ber\u00b7nack", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "und wollen sie's \u00fcbel nehmen,", "tokens": ["und", "wol\u00b7len", "sie's", "\u00fc\u00b7bel", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "dann los! habt Mut, ihr Pack!", "tokens": ["dann", "los", "!", "habt", "Mut", ",", "ihr", "Pack", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "VAFIN", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "hier steht ein kleiner Held!", "tokens": ["hier", "steht", "ein", "klei\u00b7ner", "Held"