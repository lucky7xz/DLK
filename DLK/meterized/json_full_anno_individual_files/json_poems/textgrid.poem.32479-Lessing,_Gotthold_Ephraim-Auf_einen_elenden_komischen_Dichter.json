{"textgrid.poem.32479": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen elenden komischen Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein elend j\u00e4mmerliches Spiel", "tokens": ["Ein", "e\u00b7lend", "j\u00e4m\u00b7mer\u00b7li\u00b7ches", "Spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schrieb Koromandels stumpfer Kiel,", "tokens": ["Schrieb", "Ko\u00b7ro\u00b7man\u00b7dels", "stump\u00b7fer", "Kiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als er in der Entz\u00fcckung dachte,", "tokens": ["Als", "er", "in", "der", "Ent\u00b7z\u00fc\u00b7ckung", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Da\u00df er wohl Plautos schamrot machte,", "tokens": ["Da\u00df", "er", "wohl", "Plau\u00b7tos", "scham\u00b7rot", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und da\u00df kein Molier", "tokens": ["Und", "da\u00df", "kein", "Mo\u00b7lier"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Ihm zu vergleichen w\u00e4r.", "tokens": ["Ihm", "zu", "ver\u00b7glei\u00b7chen", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er, der sie beide kennt,", "tokens": ["Er", ",", "der", "sie", "bei\u00b7de", "kennt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wie ich den gro\u00dfen Mogul kenne,", "tokens": ["Wie", "ich", "den", "gro\u00b7\u00dfen", "Mo\u00b7gul", "ken\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und sie zu kennen brennt,", "tokens": ["Und", "sie", "zu", "ken\u00b7nen", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "So wie ich ihn zu kennen brenne.", "tokens": ["So", "wie", "ich", "ihn", "zu", "ken\u00b7nen", "bren\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Er, der der Feinheit keuscher Ohren,", "tokens": ["Er", ",", "der", "der", "Fein\u00b7heit", "keu\u00b7scher", "Oh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Dem Witz, den Regeln, dem Verstand,", "tokens": ["Dem", "Witz", ",", "den", "Re\u00b7geln", ",", "dem", "Ver\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Den l\u00e4cherlichsten Krieg geschworen,", "tokens": ["Den", "l\u00e4\u00b7cher\u00b7lichs\u00b7ten", "Krieg", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der je im Reich der Sittenlehr entstand;", "tokens": ["Der", "je", "im", "Reich", "der", "Sit\u00b7ten\u00b7lehr", "ent\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "F\u00fcr ihn ein unentdecktes Land!", "tokens": ["F\u00fcr", "ihn", "ein", "un\u00b7ent\u00b7deck\u00b7tes", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Doch mu\u00df ich, kritisch zu verfahren,", "tokens": ["Doch", "mu\u00df", "ich", ",", "kri\u00b7tisch", "zu", "ver\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Dem Leser treulich offenbaren,", "tokens": ["Dem", "Le\u00b7ser", "treu\u00b7lich", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Da\u00df ich an seinem St\u00fccke", "tokens": ["Da\u00df", "ich", "an", "sei\u00b7nem", "St\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Auch etwas Trefflich