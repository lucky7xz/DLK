{"dta.poem.2999": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df tr\u00f6sten dich, mein Sohn, f\u00fcr eines Augs Verlust!", "tokens": ["La\u00df", "tr\u00f6s\u00b7ten", "dich", ",", "mein", "Sohn", ",", "f\u00fcr", "ei\u00b7nes", "Augs", "Ver\u00b7lust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bewahre doppelt rein den Sinn in deiner Brust!", "tokens": ["Be\u00b7wah\u00b7re", "dop\u00b7pelt", "rein", "den", "Sinn", "in", "dei\u00b7ner", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So wird der Himmel voll dir durch Ein Auge stralen,", "tokens": ["So", "wird", "der", "Him\u00b7mel", "voll", "dir", "durch", "Ein", "Au\u00b7ge", "stra\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sanft auf Seelengrund das Bild der Welt sich malen.", "tokens": ["Und", "sanft", "auf", "See\u00b7len\u00b7grund", "das", "Bild", "der", "Welt", "sich", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "ART", "NN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das ist dir besser als wenn unversehrt vom Leide,", "tokens": ["Das", "ist", "dir", "bes\u00b7ser", "als", "wenn", "un\u00b7ver\u00b7sehrt", "vom", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "KOKOM", "KOUS", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Leidenschaft getr\u00fcbt, du h\u00e4ttest alle beide.", "tokens": ["Von", "Lei\u00b7den\u00b7schaft", "ge\u00b7tr\u00fcbt", ",", "du", "h\u00e4t\u00b7test", "al\u00b7le", "bei\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "PPER", "VAFIN", "PIAT", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}