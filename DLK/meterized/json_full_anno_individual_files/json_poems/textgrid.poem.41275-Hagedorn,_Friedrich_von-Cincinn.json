{"textgrid.poem.41275": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Cincinn", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lassen sich Cincinn und seines L\u00e4chelns Kunst,", "tokens": ["Es", "las\u00b7sen", "sich", "Cin\u00b7cinn", "und", "sei\u00b7nes", "L\u00e4\u00b7chelns", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In fr\u00fcher Gegenwart, bei Hofe t\u00e4glich sehen,", "tokens": ["In", "fr\u00fc\u00b7her", "Ge\u00b7gen\u00b7wart", ",", "bei", "Ho\u00b7fe", "t\u00e4g\u00b7lich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und hieraus schlie\u00dfest du, er m\u00fcsse recht in Gunst", "tokens": ["Und", "hier\u00b7aus", "schlie\u00b7\u00dfest", "du", ",", "er", "m\u00fcs\u00b7se", "recht", "in", "Gunst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bei herrschenden Ministern stehen;", "tokens": ["Bei", "herr\u00b7schen\u00b7den", "Mi\u00b7nis\u00b7tern", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch durch sein Dasein wird uns das nicht offenbart:", "tokens": ["Doch", "durch", "sein", "Da\u00b7sein", "wird", "uns", "das", "nicht", "of\u00b7fen\u00b7bart", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VAFIN", "PPER", "PDS", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erkennt man Christen bester Art", "tokens": ["Er\u00b7kennt", "man", "Chris\u00b7ten", "bes\u00b7ter", "Art"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Allein an ihrem Kirchengehen?", "tokens": ["Al\u00b7lein", "an", "ih\u00b7rem", "Kir\u00b7chen\u00b7ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es lassen sich Cincinn und seines L\u00e4chelns Kunst,", "tokens": ["Es", "las\u00b7sen", "sich", "Cin\u00b7cinn", "und", "sei\u00b7nes", "L\u00e4\u00b7chelns", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In fr\u00fcher Gegenwart, bei Hofe t\u00e4glich sehen,", "tokens": ["In", "fr\u00fc\u00b7her", "Ge\u00b7gen\u00b7wart", ",", "bei", "Ho\u00b7fe", "t\u00e4g\u00b7lich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und hieraus schlie\u00dfest du, er m\u00fcsse recht in Gunst", "tokens": ["Und", "hier\u00b7aus", "schlie\u00b7\u00dfest", "du", ",", "er", "m\u00fcs\u00b7se", "recht", "in", "Gunst"], "to