{"dta.poem.11175": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine schrifft von mehr als einer welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jeder glaube, was er will. Andre m\u00f6gen fragen gehen:", "tokens": ["Je\u00b7der", "glau\u00b7be", ",", "was", "er", "will", ".", "And\u00b7re", "m\u00f6\u00b7gen", "fra\u00b7gen", "ge\u00b7hen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$.", "PIS", "VMFIN", "VVINF", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ob die welten im gehirn, oder in der stirne stehen?", "tokens": ["Ob", "die", "wel\u00b7ten", "im", "ge\u00b7hirn", ",", "o\u00b7der", "in", "der", "stir\u00b7ne", "ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "$,", "KON", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "--+-+-+----+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Wer die meinung wahr befindet, dieser bilde sich nur ein:", "tokens": ["Wer", "die", "mei\u00b7nung", "wahr", "be\u00b7fin\u00b7det", ",", "die\u00b7ser", "bil\u00b7de", "sich", "nur", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJD", "VVFIN", "$,", "PDS", "VVFIN", "PRF", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ey was werden tumme leute in dem hunds-stern m\u00fcssen seyn!", "tokens": ["Ey", "was", "wer\u00b7den", "tum\u00b7me", "leu\u00b7te", "in", "dem", "hunds\u00b7st