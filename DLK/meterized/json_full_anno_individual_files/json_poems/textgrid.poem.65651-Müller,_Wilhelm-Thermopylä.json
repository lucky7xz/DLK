{"textgrid.poem.65651": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Thermopyl\u00e4", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil! Heil! Nie wird Thermopyl\u00e4 den Sieg der Sklaven sehn.", "tokens": ["Heil", "!", "Heil", "!", "Nie", "wird", "Ther\u00b7mo\u00b7py\u00b7l\u00e4", "den", "Sieg", "der", "Skla\u00b7ven", "sehn", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "ADV", "VAFIN", "NE", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Heil! Ewig wird Thermopyl\u00e4, ein Hort der Freiheit, stehn.", "tokens": ["Heil", "!", "E\u00b7wig", "wird", "Ther\u00b7mo\u00b7py\u00b7l\u00e4", ",", "ein", "Hort", "der", "Frei\u00b7heit", ",", "stehn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NE", "VAFIN", "NE", "$,", "ART", "NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Da kreist er mit dem Flammenschwert als W\u00e4chter um den Pa\u00df,", "tokens": ["Da", "kreist", "er", "mit", "dem", "Flam\u00b7men\u00b7schwert", "als", "W\u00e4ch\u00b7ter", "um", "den", "Pa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "KOKOM", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Den er mit seinem Blut gefeit, der Held Leonidas,", "tokens": ["Den", "er", "mit", "sei\u00b7nem", "Blut", "ge\u00b7feit", ",", "der", "Held", "Leo\u00b7ni\u00b7das", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und hinter ihm die ganze Schaar der Treuen bis zum Tod,", "tokens": ["Und", "hin\u00b7ter", "ihm", "die", "gan\u00b7ze", "Schaar", "der", "Treu\u00b7en", "bis", "zum", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "ADJA", "NN", "ART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Mit gr\u00fcnen Kr\u00e4nzen auf dem Haupt, die Brust ganz purpurroth.", "tokens": ["Mit", "gr\u00fc\u00b7nen", "Kr\u00e4n\u00b7zen", "auf", "dem", "Haupt", ",", "die", "Brust", "ganz", "pur\u00b7pur\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Nun rottet euch zusammen nur, ihr Sklaven und ihr Herrn!", "tokens": ["Nun", "rot\u00b7tet", "euch", "zu\u00b7sam\u00b7men", "nur", ",", "ihr", "Skla\u00b7ven", "und", "ihr", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Ihr S\u00f6ldnerhorden, zieht heran, heran von nah und fern!", "tokens": ["Ihr", "S\u00f6ld\u00b7ner\u00b7hor\u00b7den", ",", "zieht", "he\u00b7ran", ",", "he\u00b7ran", "von", "nah", "und", "fern", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$,", "PTKVZ", "APPR", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Wir stehen bei Thermopyl\u00e4; wir stehen Mann f\u00fcr Mann,", "tokens": ["Wir", "ste\u00b7hen", "bei", "Ther\u00b7mo\u00b7py\u00b7l\u00e4", ";", "wir", "ste\u00b7hen", "Mann", "f\u00fcr", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$.", "PPER", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Zu zeigen euch, was Freiheit ist, was Freiheit will und kann.", "tokens": ["Zu", "zei\u00b7gen", "euch", ",", "was", "Frei\u00b7heit", "ist", ",", "was", "Frei\u00b7heit", "will", "und", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "$,", "PWS", "NN", "VAFIN", "$,", "PWS", "NN", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "Leonidas, ", "tokens": ["Leo\u00b7ni\u00b7das", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.12": {"text": "Und nun la\u00df uns im Kampf allein \u2013 wir stehn auf deinem Grab,", "tokens": ["Und", "nun", "la\u00df", "uns", "im", "Kampf", "al\u00b7lein", "\u2013", "wir", "stehn", "auf", "dei\u00b7nem", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVIMP", "PPER", "APPRART", "NN", "ADV", "$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "Da stehen wir, da fallen wir, da scharren sie uns ein;", "tokens": ["Da", "ste\u00b7hen", "wir", ",", "da", "fal\u00b7len", "wir", ",", "da", "schar\u00b7ren", "sie", "uns", "ein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Mit unsern Leichen wollen wir des Grabes Decke sein,", "tokens": ["Mit", "un\u00b7sern", "Lei\u00b7chen", "wol\u00b7len", "wir", "des", "Gra\u00b7bes", "De\u00b7cke", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "ART", "NN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.15": {"text": "Da\u00df nimmer deinen heil'gen Staub ber\u00fchr' ein Sklavenfu\u00df;", "tokens": ["Da\u00df", "nim\u00b7mer", "dei\u00b7nen", "heil'\u00b7gen", "Staub", "be\u00b7r\u00fchr'", "ein", "Skla\u00b7ven\u00b7fu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "Er trete lieber doch auf uns, wenn er hier treten mu\u00df.", "tokens": ["Er", "tre\u00b7te", "lie\u00b7ber", "doch", "auf", "uns", ",", "wenn", "er", "hier", "tre\u00b7ten", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPER", "$,", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.17": {"text": "Heil! Heil! Nie wird Thermopyl\u00e4 den Sieg der Sklaven sehn.", "tokens": ["Heil", "!", "Heil", "!", "Nie", "wird", "Ther\u00b7mo\u00b7py\u00b7l\u00e4", "den", "Sieg", "der", "Skla\u00b7ven", "sehn", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "ADV", "VAFIN", "NE", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.18": {"text": "Heil! Ewig wird Thermopyl\u00e4, ein Hort der Freiheit, stehn.", "tokens": ["Heil", "!", "E\u00b7wig", "wird", "Ther\u00b7mo\u00b7py\u00b7l\u00e4", ",", "ein", "Hort", "der", "Frei\u00b7heit", ",", "stehn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NE", "VAFIN", "NE", "$,", "ART", "NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.19": {"text": "Schon einmal sprang der T\u00fcrkenstahl an diesem Felsgestein,", "tokens": ["Schon", "ein\u00b7mal", "sprang", "der", "T\u00fcr\u00b7ken\u00b7stahl", "an", "die\u00b7sem", "Fels\u00b7ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.20": {"text": "Schon einmal sank der halbe Mond hier in den Staub hinein,", "tokens": ["Schon", "ein\u00b7mal", "sank", "der", "hal\u00b7be", "Mond", "hier", "in", "den", "Staub", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.21": {"text": "Schon manche neue Schatten auch sind \u00fcber uns zu sehn,", "tokens": ["Schon", "man\u00b7che", "neu\u00b7e", "Schat\u00b7ten", "auch", "sind", "\u00fc\u00b7ber", "uns", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "ADV", "VAFIN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.22": {"text": "Die mit der alten Heldenschaar umschweben diese H\u00f6hn.", "tokens": ["Die", "mit", "der", "al\u00b7ten", "Hel\u00b7den\u00b7schaar", "um\u00b7schwe\u00b7ben", "die\u00b7se", "H\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.23": {"text": "Wir kennen euch, wir folgen euch getreu in Sieg und Tod,", "tokens": ["Wir", "ken\u00b7nen", "euch", ",", "wir", "fol\u00b7gen", "euch", "ge\u00b7treu", "in", "Sieg", "und", "Tod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.24": {"text": "Wir f\u00e4rben unsre Brust, wie ihr, mit sch\u00f6nem Purpur roth.", "tokens": ["Wir", "f\u00e4r\u00b7ben", "uns\u00b7re", "Brust", ",", "wie", "ihr", ",", "mit", "sch\u00f6\u00b7nem", "Pur\u00b7pur", "roth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "PPER", "$,", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.25": {"text": "Heran, ihr Sklaven, nur heran! wir haben unser Mahl", "tokens": ["He\u00b7ran", ",", "ihr", "Skla\u00b7ven", ",", "nur", "he\u00b7ran", "!", "wir", "ha\u00b7ben", "un\u00b7ser", "Mahl"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKVZ", "$,", "PPOSAT", "NN", "$,", "ADV", "PTKVZ", "$.", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.26": {"text": "Genossen schon im Morgenroth, geleert ist der Pokal.", "tokens": ["Ge\u00b7nos\u00b7sen", "schon", "im", "Mor\u00b7gen\u00b7roth", ",", "ge\u00b7leert", "ist", "der", "Po\u00b7kal", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$,", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.27": {"text": "Wir kr\u00e4nzen unsre Stirn zum Fest, wir kr\u00e4nzen unser Schwert", "tokens": ["Wir", "kr\u00e4n\u00b7zen", "uns\u00b7re", "Stirn", "zum", "Fest", ",", "wir", "kr\u00e4n\u00b7zen", "un\u00b7ser", "Schwert"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.28": {"text": "Zum Siegesfest, zum Todesfest \u2013 was uns der Herr gew\u00e4hrt;", "tokens": ["Zum", "Sie\u00b7ges\u00b7fest", ",", "zum", "To\u00b7des\u00b7fest", "\u2013", "was", "uns", "der", "Herr", "ge\u00b7w\u00e4hrt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$(", "PWS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.29": {"text": "Nur sei des Todes werth der Sieg, des Sieges werth der Tod!", "tokens": ["Nur", "sei", "des", "To\u00b7des", "werth", "der", "Sieg", ",", "des", "Sie\u00b7ges", "werth", "der", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "ART", "NN", "$,", "ART", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.30": {"text": "Vor Sparta's Leichen bebte hier der medische Despot,", "tokens": ["Vor", "Spar\u00b7ta's", "Lei\u00b7chen", "beb\u00b7te", "hier", "der", "me\u00b7di\u00b7sche", "Des\u00b7pot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.31": {"text": "Und f\u00fchlte sich besiegt im Sieg, und sah es selber an", "tokens": ["Und", "f\u00fchl\u00b7te", "sich", "be\u00b7siegt", "im", "Sieg", ",", "und", "sah", "es", "sel\u00b7ber", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "VVPP", "APPRART", "NN", "$,", "KON", "VVFIN", "PPER", "ADV", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.32": {"text": "Mit finstrem Blick, was Freiheit ist, was Freiheit will und kann.", "tokens": ["Mit", "fins\u00b7trem", "Blick", ",", "was", "Frei\u00b7heit", "ist", ",", "was", "Frei\u00b7heit", "will", "und", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWS", "NN", "VAFIN", "$,", "PWS", "NN", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.33": {"text": "Heran, ihr Sklaven, nur heran! Auch ihr, ihr sollt es sehn!", "tokens": ["He\u00b7ran", ",", "ihr", "Skla\u00b7ven", ",", "nur", "he\u00b7ran", "!", "Auch", "ihr", ",", "ihr", "sollt", "es", "sehn", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PPOSAT", "NN", "$,", "ADV", "PTKVZ", "$.", "ADV", "PPER", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-