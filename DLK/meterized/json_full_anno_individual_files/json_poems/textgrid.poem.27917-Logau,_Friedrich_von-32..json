{"textgrid.poem.27917": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man sagt mir viel vom Gl\u00fcck und dessen Seligkeiten,", "tokens": ["Man", "sagt", "mir", "viel", "vom", "Gl\u00fcck", "und", "des\u00b7sen", "Se\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "KON", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und war und ist und wird doch keiner aller Zeiten,", "tokens": ["Und", "war", "und", "ist", "und", "wird", "doch", "kei\u00b7ner", "al\u00b7ler", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KON", "VAFIN", "KON", "VAFIN", "ADV", "PIS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der gl\u00fccklich sey durchau\u00df. Dann ist das Gl\u00fccke rund,", "tokens": ["Der", "gl\u00fcck\u00b7lich", "sey", "durch\u00b7au\u00df", ".", "Dann", "ist", "das", "Gl\u00fc\u00b7cke", "rund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "PTKVZ", "$.", "ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So steht es morgen nicht, als wie es heute stund.", "tokens": ["So", "steht", "es", "mor\u00b7gen", "nicht", ",", "als", "wie", "es", "heu\u00b7te", "stund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "PWAV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo Ph\u00f6nix etwa wohnt, wohnt, glaub ich, auch das Gl\u00fccke,", "tokens": ["Wo", "Ph\u00f6\u00b7nix", "et\u00b7wa", "wohnt", ",", "wohnt", ",", "glaub", "ich", ",", "auch", "das", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von dem man nach dem Ohr und nichts wei\u00df nach dem Blicke.", "tokens": ["Von", "dem", "man", "nach", "dem", "Ohr", "und", "nichts", "wei\u00df", "nach", "dem", "Bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "APPR", "ART", "NN", "KON", "PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Iedoch ich wei\u00df den Ort, wo Gl\u00fccke macht Bestand,", "tokens": ["Ie\u00b7doch", "ich", "wei\u00df", "den", "Ort", ",", "wo", "Gl\u00fc\u00b7cke", "macht", "Be\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den aber niemand kennt, bi\u00df dieser wird verbrant.", "tokens": ["Den", "a\u00b7ber", "nie\u00b7mand", "kennt", ",", "bi\u00df", "die\u00b7ser", "wird", "ver\u00b7brant", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "VVFIN", "$,", "KOUS", "PDS", "VAFIN", "VVP