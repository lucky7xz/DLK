{"dta.poem.1141": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "27. Die Sch\u00f6ne Kr\u00f6pffigte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Auff einem Stocke w\u00e4chst/ was Spinn und Biene n\u00e4hrt/ ", "tokens": ["Auff", "ei\u00b7nem", "Sto\u00b7cke", "w\u00e4chst", "/", "was", "Spinn", "und", "Bie\u00b7ne", "n\u00e4hrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$(", "PWS", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff einer Wiese sucht der Ochse frischen Rasen/", "tokens": ["Auff", "ei\u00b7ner", "Wie\u00b7se", "sucht", "der", "O\u00b7chse", "fri\u00b7schen", "Ra\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Storch die gifftge Schlang\u2019/ ein Jagt-Hund seinen Ha-", "tokens": ["Der", "Storch", "die", "gifft\u00b7ge", "Schlang'", "/", "ein", "Jag\u00b7tHund", "sei\u00b7nen", "Ha"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$(", "ART", "NN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "sen: ", "tokens": ["sen", ":"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Was der zu loben denckt/ scheint jenem tadlens werth.", "tokens": ["Was", "der", "zu", "lo\u00b7ben", "denckt", "/", "scheint", "je\u00b7nem", "tad\u00b7lens", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "PTKZU", "VVINF", "VVFIN", "$(", "VVFIN", "PDAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Drum lache/ wenn dir auch dergleichen widerf\u00e4hrt/", "tokens": ["Drum", "la\u00b7che", "/", "wenn", "dir", "auch", "derg\u00b7lei\u00b7chen", "wi\u00b7der\u00b7f\u00e4hrt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$(", "KOUS", "PPER", "ADV", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn man dein hohes Lob zu niedrig will begrasen/", "tokens": ["Wenn", "man", "dein", "ho\u00b7hes", "Lob", "zu", "nied\u00b7rig", "will", "be\u00b7gra\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "ADJA", "NN", "PTKA", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil deiner K\u00e4hle Schnee zu hoch sey auffgeblasen.", "tokens": ["Weil", "dei\u00b7ner", "K\u00e4h\u00b7le", "Schnee", "zu", "hoch", "sey", "auff\u00b7ge\u00b7bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "PTKA", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich achte dich dadurch im wenigsten beschwert.", "tokens": ["Ich", "ach\u00b7te", "dich", "da\u00b7durch", "im", "we\u00b7nigs\u00b7ten", "be\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "APPRART", "PIS", "VVPP", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.10": {"text": "Ein ander suche Gifft/ ich will dein Honig klauben:", "tokens": ["Ein", "an\u00b7der", "su\u00b7che", "Gifft", "/", "ich", "will", "dein", "Ho\u00b7nig", "klau\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$(", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie mancher Seuffzer/ wie manch s\u00fcsses Wort steckt hier!", "tokens": ["Wie", "man\u00b7cher", "Seuff\u00b7zer", "/", "wie", "manch", "s\u00fcs\u00b7ses", "Wort", "steckt", "hier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$(", "KOKOM", "PIAT", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die sch\u00f6ne Venus ziehn ein paar gekr\u00f6pffte Tauben/", "tokens": ["Die", "sch\u00f6\u00b7ne", "Ve\u00b7nus", "ziehn", "ein", "paar", "ge\u00b7kr\u00f6pff\u00b7te", "Tau\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Des vollen Halses Glantz ist ihre beste Zier.", "tokens": ["Des", "vol\u00b7len", "Hal\u00b7ses", "Glantz", "ist", "ih\u00b7re", "bes\u00b7te", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Cupido/ wo ich darff dem Augenscheine glauben/", "tokens": ["Cu\u00b7pi\u00b7do", "/", "wo", "ich", "darff", "dem", "Au\u00b7gen\u00b7schei\u00b7ne", "glau\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PWAV", "PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "+----+-+-+-+-", "measure": "dactylic.init"}, "line.15": {"text": "Hat seinen Blasebalg/ o sch\u00f6ne Kehl\u2019/ in dir.", "tokens": ["Hat", "sei\u00b7nen", "Bla\u00b7se\u00b7balg", "/", "o", "sch\u00f6\u00b7ne", "Kehl'", "/", "in", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$(", "FM", "ADJA", "NN", "$(", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}