{"textgrid.poem.31297": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willst du den allerh\u00f6chsten Zevs erh\u00f6hen,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du den allerh\u00f6chsten Zevs erh\u00f6hen,", "tokens": ["Willst", "du", "den", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Zevs", "er\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der sein allm\u00e4chtig Haupt bewegt,", "tokens": ["Der", "sein", "all\u00b7m\u00e4ch\u00b7tig", "Haupt", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und den Olymp ersch\u00fcttert? oder Athen\u00e4en,", "tokens": ["Und", "den", "O\u00b7lymp", "er\u00b7sch\u00fct\u00b7tert", "?", "o\u00b7der", "A\u00b7the\u00b7n\u00e4\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$.", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In diesem Haupt gepflegt,", "tokens": ["In", "die\u00b7sem", "Haupt", "ge\u00b7pflegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die mit best\u00e4lter Esche, nimmer m\u00fcde,", "tokens": ["Die", "mit", "be\u00b7st\u00e4l\u00b7ter", "E\u00b7sche", ",", "nim\u00b7mer", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Typhon, den Encelados", "tokens": ["Den", "Ty\u00b7phon", ",", "den", "En\u00b7ce\u00b7la\u00b7dos"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Zur\u00fcckewarf, und mit der ewigen Aegide", "tokens": ["Zu\u00b7r\u00fc\u00b7cke\u00b7warf", ",", "und", "mit", "der", "e\u00b7wi\u00b7gen", "A\u00b7e\u00b7gi\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Felsen, ihr Geschoss?", "tokens": ["Die", "Fel\u00b7sen", ",", "ihr", "Ge\u00b7schoss", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Singst du den ", "tokens": ["Singst", "du", "den"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Die Patareus dir aufgespannt?", "tokens": ["Die", "Pa\u00b7ta\u00b7reus", "dir", "auf\u00b7ge\u00b7spannt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihn? oder ", "tokens": ["Ihn", "?", "o\u00b7der"], "token_info": ["word", "punct", "word"], "pos": ["PPER", "$.", "KON"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "In k\u00f6niglicher Weisheit unterwiesen,", "tokens": ["In", "k\u00f6\u00b7nig\u00b7li\u00b7cher", "Weis\u00b7heit", "un\u00b7ter\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu Kriegestugenden erhitzt,", "tokens": ["Zu", "Krie\u00b7ges\u00b7tu\u00b7gen\u00b7den", "er\u00b7hitzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sind beide hoher Hymnen werth. \u2013 Bald singe diesen,", "tokens": ["Sind", "bei\u00b7de", "ho\u00b7her", "Hym\u00b7nen", "werth", ".", "\u2013", "Bald", "sin\u00b7ge", "die\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "ADJD", "$.", "$(", "ADV", "VVFIN", "PDAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O Muse! jenen itzt.", "tokens": ["O", "Mu\u00b7se", "!", "je\u00b7nen", "itzt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PDS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wohlan, mein Lied! spann' alle deine Segel", "tokens": ["Wo\u00b7hlan", ",", "mein", "Lied", "!", "spann'", "al\u00b7le", "dei\u00b7ne", "Se\u00b7gel"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$.", "VVFIN", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis an den Wimpel auf, und sprich:", "tokens": ["Bis", "an", "den", "Wim\u00b7pel", "auf", ",", "und", "sprich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PTKVZ", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als der Monarch, den Sprea, Viadrus und Pregel", "tokens": ["Als", "der", "Mon\u00b7arch", ",", "den", "Sprea", ",", "Viad\u00b7rus", "und", "Pre\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "NN", "$,", "NE", "KON", "NN"], "meter": "--+--++--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Anbeten, ", "tokens": ["An\u00b7be\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "Die Neid und Wahn und Hass verband,", "tokens": ["Die", "Neid", "und", "Wahn", "und", "Hass", "ver\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinem Donner nicht allgegenw\u00e4rtig allen", "tokens": ["Mit", "sei\u00b7nem", "Don\u00b7ner", "nicht", "all\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "ADJD", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ewig widerstand:", "tokens": ["Und", "e\u00b7wig", "wi\u00b7der\u00b7stand", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da brach, gen\u00e4hrt im sorgelosen Frieden,", "tokens": ["Da", "brach", ",", "ge\u00b7n\u00e4hrt", "im", "sor\u00b7ge\u00b7lo\u00b7sen", "Frie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gleich einem neuen Meteor,", "tokens": ["Gleich", "ei\u00b7nem", "neu\u00b7en", "Me\u00b7te\u00b7or", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das den Orion ausl\u00f6scht und die Tyndariden,", "tokens": ["Das", "den", "O\u00b7rion", "aus\u00b7l\u00f6scht", "und", "die", "Tyn\u00b7da\u00b7ri\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVPP", "KON", "ART", "NN", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}}, "stanza.8": {"line.1": {"text": "Als J\u00fcngling schlief er ehmals in der H\u00f6hle", "tokens": ["Als", "J\u00fcng\u00b7ling", "schlief", "er", "eh\u00b7mals", "in", "der", "H\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Anoniens, und war die Lust", "tokens": ["A\u00b7no\u00b7ni\u00b7ens", ",", "und", "war", "die", "Lust"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Musen; itzt erh\u00f6heten sie seine Seele:", "tokens": ["Der", "Mu\u00b7sen", ";", "itzt", "er\u00b7h\u00f6\u00b7he\u00b7ten", "sie", "sei\u00b7ne", "See\u00b7le", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit unbewegter Brust", "tokens": ["Mit", "un\u00b7be\u00b7weg\u00b7ter", "Brust"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Hielt er der S\u00f6hne Teuts verschworne Heere", "tokens": ["Hielt", "er", "der", "S\u00f6h\u00b7ne", "Teuts", "ver\u00b7schwor\u00b7ne", "Hee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Z\u00fcruck von unsrer Flur; (so stand", "tokens": ["Z\u00fc\u00b7ruck", "von", "uns\u00b7rer", "Flur", ";(", "so", "stand"], "token_info": ["word", "word", "word", "word", "emoticon", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$(", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Isthmische Gebirge, trennte beide Meere,", "tokens": ["Das", "Isth\u00b7mi\u00b7sche", "Ge\u00b7bir\u00b7ge", ",", "trenn\u00b7te", "bei\u00b7de", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ward zweyer V\u00f6lker Band;)", "tokens": ["Ward", "zwey\u00b7er", "V\u00f6l\u00b7ker", "Band", ";)"], "token_info": ["word", "word", "word", "word", "emoticon"], "pos": ["VAFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und pl\u00f6tzlich schlug er die bet\u00e4ubten Schaaren,", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "schlug", "er", "die", "be\u00b7t\u00e4ub\u00b7ten", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und kr\u00f6nete, diess war der Schluss", "tokens": ["Und", "kr\u00f6\u00b7ne\u00b7te", ",", "diess", "war", "der", "Schluss"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PDS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der G\u00f6tter! jene zw\u00f6lf Herkulischen Gefahren", "tokens": ["Der", "G\u00f6t\u00b7ter", "!", "je\u00b7ne", "zw\u00f6lf", "Her\u00b7ku\u00b7li\u00b7schen", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PDAT", "CARD", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.11": {"line.1": {"text": "Wagst du noch mehr zu singen? \u2013 Dass der Sieger,", "tokens": ["Wagst", "du", "noch", "mehr", "zu", "sin\u00b7gen", "?", "\u2013", "Dass", "der", "Sie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$.", "$(", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So weit er in der Feinde Land", "tokens": ["So", "weit", "er", "in", "der", "Fein\u00b7de", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit seinem Lager flog, gesegnet, seine Krieger", "tokens": ["Mit", "sei\u00b7nem", "La\u00b7ger", "flog", ",", "ge\u00b7seg\u00b7net", ",", "sei\u00b7ne", "Krie\u00b7ger"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "VVPP", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zum Wohlthun ausgefandt?", "tokens": ["Zum", "Wohl\u00b7thun", "aus\u00b7ge\u00b7fandt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iamb