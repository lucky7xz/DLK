{"textgrid.poem.62415": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Studenten-Kriegslied", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin Student gewesen,", "tokens": ["Ich", "bin", "Stu\u00b7dent", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VAPP", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Nun hei\u00df' ich Lieutenant,", "tokens": ["Nun", "hei\u00df'", "ich", "Lie\u00b7u\u00b7ten\u00b7ant", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Fahr wohl, gelahrtes Wesen,", "tokens": ["Fahr", "wohl", ",", "ge\u00b7lahr\u00b7tes", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ade, du B\u00fcchertand.", "tokens": ["A\u00b7de", ",", "du", "B\u00fc\u00b7cher\u00b7tand", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Zum K\u00f6nig will ich ziehen,", "tokens": ["Zum", "K\u00f6\u00b7nig", "will", "ich", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ins gr\u00fcne Waffenfeld,", "tokens": ["Ins", "gr\u00fc\u00b7ne", "Waf\u00b7fen\u00b7feld", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo rothe Rosen bl\u00fchen,", "tokens": ["Wo", "ro\u00b7the", "Ro\u00b7sen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da schlaf ich ohne Zelt.", "tokens": ["Da", "schlaf", "ich", "oh\u00b7ne", "Zelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ihr guten Kameraden", "tokens": ["Ihr", "gu\u00b7ten", "Ka\u00b7me\u00b7ra\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Bei B\u00fcchern und beim Mahl,", "tokens": ["Bei", "B\u00fc\u00b7chern", "und", "beim", "Mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Seid alle mitgeladen", "tokens": ["Seid", "al\u00b7le", "mit\u00b7ge\u00b7la\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VAIMP", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "In diesem gro\u00dfen Saal.", "tokens": ["In", "die\u00b7sem", "gro\u00b7\u00dfen", "Saal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Frisch auf, wem solche Stimme", "tokens": ["Frisch", "auf", ",", "wem", "sol\u00b7che", "Stim\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "PWS", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Ohr und Herzen geht!", "tokens": ["Zum", "Ohr", "und", "Her\u00b7zen", "geht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es rege sich im Grimme", "tokens": ["Es", "re\u00b7ge", "sich", "im", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun jede Fakult\u00e4t.", "tokens": ["Nun", "je\u00b7de", "Fa\u00b7kul\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die ihr euch weise Meister", "tokens": ["Die", "ihr", "euch", "wei\u00b7se", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im stolzen Wahn genannt,", "tokens": ["Im", "stol\u00b7zen", "Wahn", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Auf Regeln f\u00fcr die Geister,", "tokens": ["Auf", "Re\u00b7geln", "f\u00fcr", "die", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcr die Gedanken sannt, \u2013", "tokens": ["F\u00fcr", "die", "Ge\u00b7dan\u00b7ken", "sannt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Hier ist die hohe Schule,", "tokens": ["Hier", "ist", "die", "ho\u00b7he", "Schu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die freie K\u00fcnste lehrt,", "tokens": ["Die", "frei\u00b7e", "K\u00fcns\u00b7te", "lehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und f\u00fcr die Federspule", "tokens": ["Und", "f\u00fcr", "die", "Fe\u00b7der\u00b7spu\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Sch\u00e4rf' ich mein gutes Schwert.", "tokens": ["Sch\u00e4r\u00b7f'", "ich", "mein", "gu\u00b7tes", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr Herren Rechtsgelehrten,", "tokens": ["Ihr", "Her\u00b7ren", "Rechts\u00b7ge\u00b7lehr\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die durch den Urvertrag", "tokens": ["Die", "durch", "den", "Ur\u00b7ver\u00b7trag"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das alte Recht verkehrten,", "tokens": ["Das", "al\u00b7te", "Recht", "ver\u00b7kehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es kommt f\u00fcr euch ein Tag.", "tokens": ["Es", "kommt", "f\u00fcr", "euch", "ein", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die G\u00fcter sind verpf\u00e4ndet,", "tokens": ["Die", "G\u00fc\u00b7ter", "sind", "ver\u00b7pf\u00e4n\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Keiner missen darf,", "tokens": ["Die", "Kei\u00b7ner", "mis\u00b7sen", "darf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Freiheit ist entwendet, \u2013", "tokens": ["Die", "Frei\u00b7heit", "ist", "ent\u00b7wen\u00b7det", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Macht eure Beile scharf.", "tokens": ["Macht", "eu\u00b7re", "Bei\u00b7le", "scharf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die S\u00fcnde sollt ihr r\u00e4chen,", "tokens": ["Die", "S\u00fcn\u00b7de", "sollt", "ihr", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die durch die Wolken drang,", "tokens": ["Die", "durch", "die", "Wol\u00b7ken", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ein Urtheil ist zu sprechen", "tokens": ["Ein", "Ur\u00b7theil", "ist", "zu", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Auf Beil und Rad und Strang.", "tokens": ["Auf", "Beil", "und", "Rad", "und", "Strang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Von eures Meisters Lehren,", "tokens": ["Von", "eu\u00b7res", "Meis\u00b7ters", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Aerzte, weichet nicht,", "tokens": ["Ihr", "A\u00b7erz\u00b7te", ",", "wei\u00b7chet", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Messer hebt in Ehren,", "tokens": ["Das", "Mes\u00b7ser", "hebt", "in", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn anders Heil gebricht;", "tokens": ["Wenn", "an\u00b7ders", "Heil", "ge\u00b7bricht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So kurz ist ja das Leben,", "tokens": ["So", "kurz", "ist", "ja", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So lang und schwer die Kunst,", "tokens": ["So", "lang", "und", "schwer", "die", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dem Fl\u00fccht'gen sei gegeben", "tokens": ["Dem", "Fl\u00fccht'\u00b7gen", "sei", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Himmels reine Gunst.", "tokens": ["Des", "Him\u00b7mels", "rei\u00b7ne", "Gunst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wenn Leib und Seele leiden,", "tokens": ["Wenn", "Leib", "und", "See\u00b7le", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "In Schmerz, in Brand und Ha\u00df,", "tokens": ["In", "Schmerz", ",", "in", "Brand", "und", "Ha\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "So hilft ein k\u00fchnes Schneiden,", "tokens": ["So", "hilft", "ein", "k\u00fch\u00b7nes", "Schnei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "So hilft ein Aderla\u00df.", "tokens": ["So", "hilft", "ein", "A\u00b7der\u00b7la\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wolauf, ihr Theologen,", "tokens": ["Wo\u00b7lauf", ",", "ihr", "Theo\u00b7lo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der Herr ist nicht mehr weit,", "tokens": ["Der", "Herr", "ist", "nicht", "mehr", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So kommt nur mitgezogen", "tokens": ["So", "kommt", "nur", "mit\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Entgegen ihm im Streit.", "tokens": ["Ent\u00b7ge\u00b7gen", "ihm", "im", "Streit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hier kann man deutlich lernen", "tokens": ["Hier", "kann", "man", "deut\u00b7lich", "ler\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Zukunft zum Gericht,", "tokens": ["Die", "Zu\u00b7kunft", "zum", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn \u00fcber seinen Sternen", "tokens": ["Wenn", "\u00fc\u00b7ber", "sei\u00b7nen", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Herr das Urtheil spricht.", "tokens": ["Der", "Herr", "das", "Ur\u00b7theil", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Uns wird das Herz erledigt,", "tokens": ["Uns", "wird", "das", "Herz", "er\u00b7le\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Uns wird der Sinn erfreut,", "tokens": ["Uns", "wird", "der", "Sinn", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Wenn die Kanonenpredigt", "tokens": ["Wenn", "die", "Ka\u00b7no\u00b7nen\u00b7pre\u00b7digt"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "In alle Ohren schreit.", "tokens": ["In", "al\u00b7le", "Oh\u00b7ren", "schreit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Noch k\u00e4mpft der Leonide,", "tokens": ["Noch", "k\u00e4mpft", "der", "Leo\u00b7ni\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Noch schallt die Hermannsschlacht,", "tokens": ["Noch", "schallt", "die", "Her\u00b7manns\u00b7schlacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Fall der Winkelriede", "tokens": ["Der", "Fall", "der", "Win\u00b7kel\u00b7rie\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Uebt wieder seine Macht.", "tokens": ["Uebt", "wie\u00b7der", "sei\u00b7ne", "Macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was wir geh\u00f6rt, gelesen,", "tokens": ["Was", "wir", "ge\u00b7h\u00f6rt", ",", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word"