", "ein", "End", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NE", "$,", "$(", "PAV", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Dem Werwolf schmeichelten die F\u00e4lle,", "tokens": ["Dem", "Wer\u00b7wolf", "schmei\u00b7chel\u00b7ten", "die", "F\u00e4l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "er rollte seine Augenb\u00e4lle.", "tokens": ["er", "roll\u00b7te", "sei\u00b7ne", "Au\u00b7gen\u00b7b\u00e4l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Indessen, bat er, f\u00fcge doch", "tokens": ["In\u00b7des\u00b7sen", ",", "bat", "er", ",", "f\u00fc\u00b7ge", "doch"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "zur Einzahl auch die Mehrzahl noch!", "tokens": ["zur", "Ein\u00b7zahl", "auch", "die", "Mehr\u00b7zahl", "noch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Dorfschulmeister aber mu\u00dfte", "tokens": ["Der", "Dorf\u00b7schul\u00b7meis\u00b7ter", "a\u00b7ber", "mu\u00df\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "gestehn, da\u00df er von ihr nichts wu\u00dfte.", "tokens": ["ge\u00b7stehn", ",", "da\u00df", "er", "von", "ihr", "nichts", "wu\u00