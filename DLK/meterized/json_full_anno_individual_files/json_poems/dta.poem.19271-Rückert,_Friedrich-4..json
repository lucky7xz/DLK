{"dta.poem.19271": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welt auszusprechen, welch ein St\u00fcck der Welt es sei,", "tokens": ["Welt", "aus\u00b7zu\u00b7spre\u00b7chen", ",", "welch", "ein", "St\u00fcck", "der", "Welt", "es", "sei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVIZU", "$,", "PWAT", "ART", "NN", "ART", "NN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tief oder hoch, gro\u00df oder klein, ist einerlei", "tokens": ["Tief", "o\u00b7der", "hoch", ",", "gro\u00df", "o\u00b7der", "klein", ",", "ist", "ei\u00b7ner\u00b7lei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,", "VAFIN", "ADV"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Dem dichtrischen Beruf; es ist zu seinem Gl\u00fcck", "tokens": ["Dem", "dich\u00b7tri\u00b7schen", "Be\u00b7ruf", ";", "es", "ist", "zu", "sei\u00b7nem", "Gl\u00fcck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Die ganze gro\u00dfe Welt in jedem kleinen St\u00fcck.", "tokens": ["Die", "gan\u00b7ze", "gro\u00b7\u00dfe", "Welt", "in", "je\u00b7dem", "klei\u00b7nen", "St\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-