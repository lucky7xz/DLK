{"dta.poem.257": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Sterne an den Himmels-B\u00fchnen/", "tokens": ["Ihr", "Ster\u00b7ne", "an", "den", "Him\u00b7mels\u00b7B\u00fch\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr Lichter dieser Unter-Welt/", "tokens": ["Ihr", "Lich\u00b7ter", "die\u00b7ser", "Un\u00b7ter\u00b7Welt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Helfft mir ", "tokens": ["Helfft", "mir"], "token_info": ["word", "word"], "pos": ["NN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Erh\u00e4llt das dunckle Wolcken-Feld/", "tokens": ["Er\u00b7h\u00e4llt", "das", "dunck\u00b7le", "Wol\u00b7cken\u00b7Feld", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00dft eure Strahlen heller schimmern/", "tokens": ["La\u00dft", "eu\u00b7re", "Strah\u00b7len", "hel\u00b7ler", "schim\u00b7mern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vertreibt die schwartze Schatten-Nacht.", "tokens": ["Ver\u00b7treibt", "die", "schwart\u00b7ze", "Schat\u00b7ten\u00b7Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Seckt aus den blauen Himmels-Zimmern/", "tokens": ["Seckt", "aus", "den", "blau\u00b7en", "Him\u00b7mels\u00b7Zim\u00b7mern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Fahnen eurer Sch\u00f6nheit-Pracht.", "tokens": ["Die", "Fah\u00b7nen", "eu\u00b7rer", "Sch\u00f6n\u00b7heit\u00b7Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Diana la\u00df die tr\u00fcben Wangen", "tokens": ["Di\u00b7a\u00b7na", "la\u00df", "die", "tr\u00fc\u00b7ben", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erf\u00fcllt mit klahrem Silber seyn/", "tokens": ["Er\u00b7f\u00fcllt", "mit", "klah\u00b7rem", "Sil\u00b7ber", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df da die hellen Strahlen prangen", "tokens": ["La\u00df", "da", "die", "hel\u00b7len", "Strah\u00b7len", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was jetzt die schwartze Nacht nimmt ein/", "tokens": ["Was", "jetzt", "die", "schwart\u00b7ze", "Nacht", "nimmt", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "VVFIN", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erhell\u2019 ihr Zimmer/ und das Bette", "tokens": ["Er\u00b7hell'", "ihr", "Zim\u00b7mer", "/", "und", "das", "Bet\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$(", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich mein Engels-Kind kan sehn/", "tokens": ["Da\u00df", "ich", "mein", "En\u00b7gels\u00b7Kind", "kan", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und schauen wie an ihr so nette", "tokens": ["Und", "schau\u00b7en", "wie", "an", "ihr", "so", "net\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "APPR", "PPER", "ADV", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Die Anmuhts-vollen Glieder stehn.", "tokens": ["Die", "Anm\u00b7uhts\u00b7vol\u00b7len", "Glie\u00b7der", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Wangen s\u00fcssen ", "tokens": ["Die", "Wan\u00b7gen", "s\u00fcs\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die Lippen sind ein Zucker-Brodt/", "tokens": ["Die", "Lip\u00b7pen", "sind", "ein", "Zu\u00b7cke\u00b7rBrodt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Busen l\u00e4st in Liljen schiffen/", "tokens": ["Der", "Bu\u00b7sen", "l\u00e4st", "in", "Lil\u00b7jen", "schif\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schoo\u00df gebiehrt den s\u00fcssen Tod/", "tokens": ["Die", "Schoo\u00df", "ge\u00b7biehrt", "den", "s\u00fcs\u00b7sen", "Tod", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da geht man von Egyptens K\u00fcsten", "tokens": ["Da", "geht", "man", "von", "E\u00b7gyp\u00b7tens", "K\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NE", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Durchs rohte Meer in ", "tokens": ["Durchs", "roh\u00b7te", "Meer", "in"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und in der Oed und dunckeln W\u00fcsten", "tokens": ["Und", "in", "der", "O\u00b7ed", "und", "dun\u00b7ckeln", "W\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "ADJA", "NN"], "meter": "+--++-+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Man s\u00fcsses Manna kosten kan.", "tokens": ["Man", "s\u00fcs\u00b7ses", "Man\u00b7na", "kos\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Cupido ist da K\u00fcchenmeister", "tokens": ["Cu\u00b7pi\u00b7do", "ist", "da", "K\u00fc\u00b7chen\u00b7meis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die Anmuht tr\u00e4gt die Speise zu/", "tokens": ["Die", "An\u00b7muht", "tr\u00e4gt", "die", "Spei\u00b7se", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lust berauscht die heissen Geister", "tokens": ["Die", "Lust", "be\u00b7rauscht", "die", "heis\u00b7sen", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schafft dem Leibe s\u00fcsse Ruh.", "tokens": ["Und", "schafft", "dem", "Lei\u00b7be", "s\u00fcs\u00b7se", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Granaten/ Kirschen und ", "tokens": ["Gra\u00b7na\u00b7ten", "/", "Kir\u00b7schen", "und"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$(", "NN", "KON"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Die setzet sie dem Munde f\u00fcr/", "tokens": ["Die", "set\u00b7zet", "sie", "dem", "Mun\u00b7de", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Aepffel zur Erfrischung dienen/", "tokens": ["Die", "A\u00b7e\u00b7pf\u00b7fel", "zur", "Er\u00b7fri\u00b7schung", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Rosen bricht man mit Begier.", "tokens": ["Die", "Ro\u00b7sen", "bricht", "man", "mit", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach eilet doch ihr hellen Sternen!", "tokens": ["Ach", "ei\u00b7let", "doch", "ihr", "hel\u00b7len", "Ster\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Eilt/ f\u00f6rdert meine Liebes Lust!", "tokens": ["Eilt", "/", "f\u00f6r\u00b7dert", "mei\u00b7ne", "Lie\u00b7bes", "Lust", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geh auf du klahre Mond-Laterne/", "tokens": ["Geh", "auf", "du", "klah\u00b7re", "Mon\u00b7dLa\u00b7ter\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch weich! ", "tokens": ["Doch", "weich", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Mit solchem Silber ist versorget/", "tokens": ["Mit", "sol\u00b7chem", "Sil\u00b7ber", "ist", "ver\u00b7sor\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das seinen Glantz mir sietig weiht/", "tokens": ["Das", "sei\u00b7nen", "Glantz", "mir", "sie\u00b7tig", "weiht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da du von ", "tokens": ["Da", "du", "von"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Der seinen Uberflu\u00df dir leiht.", "tokens": ["Der", "sei\u00b7nen", "U\u00b7ber\u00b7flu\u00df", "dir", "leiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}