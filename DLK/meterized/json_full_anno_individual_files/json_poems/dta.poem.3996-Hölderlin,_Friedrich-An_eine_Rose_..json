{"dta.poem.3996": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An eine Rose .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Ewig tr\u00e4gt im Mutterschoose,", "tokens": ["E\u00b7wig", "tr\u00e4gt", "im", "Mut\u00b7ter\u00b7scho\u00b7o\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "S\u00fc\u00dfe K\u00f6nigin der Flur,", "tokens": ["S\u00fc\u00b7\u00dfe", "K\u00f6\u00b7ni\u00b7gin", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich und mich die stille, gro\u00dfe,", "tokens": ["Dich", "und", "mich", "die", "stil\u00b7le", ",", "gro\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "KON", "PPER", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Allbelebende Natur.", "tokens": ["All\u00b7be\u00b7le\u00b7ben\u00b7de", "Na\u00b7tur", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "R\u00f6schen! unser Schmuck veraltet,", "tokens": ["R\u00f6\u00b7schen", "!", "un\u00b7ser", "Schmuck", "ver\u00b7al\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sturm entbl\u00e4ttert dich und mich,", "tokens": ["Sturm", "ent\u00b7bl\u00e4t\u00b7tert", "dich", "und", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der ew'ge Keim entfaltet", "tokens": ["Doch", "der", "ew'\u00b7ge", "Keim", "ent\u00b7fal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald zu neuer Bl\u00fcthe sich.", "tokens": ["Bald", "zu", "neu\u00b7er", "Bl\u00fc\u00b7the", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}