": ["VVINF", "VMFIN", "APPR", "ART", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Daher dan kan sich der verdru\u00df", "tokens": ["Da\u00b7her", "dan", "kan", "sich", "der", "ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VMFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "erleichtern, da\u00df die finsternus", "tokens": ["er\u00b7leich\u00b7tern", ",", "da\u00df", "die", "fins\u00b7ter\u00b7nus"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVINF", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "des scheidens m\u00f6g nicht allzeit wehren,", "tokens": ["des", "schei\u00b7dens", "m\u00f6g", "nicht", "all\u00b7zeit", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wan du mich oftmal deiner schrift,", "tokens": ["wan", "du", "mich", "oft\u00b7mal", "dei\u00b7ner", "schrift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die wider des abwesens gift", "tokens": ["die", "wi\u00b7der", "des", "ab\u00b7we\u00b7sens", "gift"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "mich kan erquicken, wirst gewehren.", "tokens": ["mich", "kan", "er\u00b7qui\u00b7cken", ",", "wirst", "ge\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "i