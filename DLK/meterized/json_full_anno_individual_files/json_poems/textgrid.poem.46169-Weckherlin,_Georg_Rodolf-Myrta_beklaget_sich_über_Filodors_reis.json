{"textgrid.poem.46169": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Myrta beklaget sich \u00fcber Filodors reis", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, dises ist der b\u00f6se tag,", "tokens": ["Ach", ",", "di\u00b7ses", "ist", "der", "b\u00f6\u00b7se", "tag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "tag? nein, die t\u00f6dlich schwere plag,", "tokens": ["tag", "?", "nein", ",", "die", "t\u00f6d\u00b7lich", "schwe\u00b7re", "plag", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PTKANT", "$,", "PRELS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "was? soll ich sagen deines scheidens?", "tokens": ["was", "?", "soll", "ich", "sa\u00b7gen", "dei\u00b7nes", "schei\u00b7dens", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$.", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "nein, Filodor, es ist die nacht,", "tokens": ["nein", ",", "Fi\u00b7lo\u00b7dor", ",", "es", "ist", "die", "nacht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NE", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und unverhinderliche macht,", "tokens": ["und", "un\u00b7ver\u00b7hin\u00b7der\u00b7li\u00b7che", "macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "tag, plag, nacht, macht meines verscheidens.", "tokens": ["tag", ",", "plag", ",", "nacht", ",", "macht", "mei\u00b7nes", "ver\u00b7schei\u00b7dens", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Mit deinem scheiden scheidet sich", "tokens": ["Mit", "dei\u00b7nem", "schei\u00b7den", "schei\u00b7det", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "von mir mein herz und geist, und ich,", "tokens": ["von", "mir", "mein", "herz", "und", "geist", ",", "und", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "KON", "NN", "$,", "KON", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ich? nein, mein k\u00f6rper soll umschweben.", "tokens": ["ich", "?", "nein", ",", "mein", "k\u00f6r\u00b7per", "soll", "um\u00b7schwe\u00b7ben", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "PTKANT", "$,", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "doch kan gewi\u00df mein k\u00f6rper nicht,", "tokens": ["doch", "kan", "ge\u00b7wi\u00df", "mein", "k\u00f6r\u00b7per", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "verlierend sein herz, seel, gesicht,", "tokens": ["ver\u00b7lie\u00b7rend", "sein", "herz", ",", "seel", ",", "ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "VAINF", "PTKVZ", "$,", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "umschweben, weil er nicht kan leben.", "tokens": ["um\u00b7schwe\u00b7ben", ",", "weil", "er", "nicht", "kan", "le\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch deine bildnus und gestalt", "tokens": ["Doch", "dei\u00b7ne", "bild\u00b7nus", "und", "ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ist all der trost, den ich behalt", "tokens": ["ist", "all", "der", "trost", ",", "den", "ich", "be\u00b7halt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "in meinem schwachen haupt und herzen:", "tokens": ["in", "mei\u00b7nem", "schwa\u00b7chen", "haupt", "und", "her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wan anderst allen trost von mir", "tokens": ["wan", "an\u00b7derst", "al\u00b7len", "trost", "von", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PIAT", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "nicht senden stracks nach und mit dir", "tokens": ["nicht", "sen\u00b7den", "stracks", "nach", "und", "mit", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VAFIN", "ADV", "APPR", "KON", "APPR", "PPER"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "mein \u00fcbergro\u00dfes leid und schmerzen.", "tokens": ["mein", "\u00fc\u00b7ber\u00b7gro\u00b7\u00dfes", "leid", "und", "schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJD", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch dein vilf\u00e4ltige zusag,", "tokens": ["Doch", "dein", "vil\u00b7f\u00e4l\u00b7ti\u00b7ge", "zu\u00b7sag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "vermischet stets mit meiner klag,", "tokens": ["ver\u00b7mi\u00b7schet", "stets", "mit", "mei\u00b7ner", "klag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "uns k\u00fcrzlich wider umzufangen,", "tokens": ["uns", "k\u00fcrz\u00b7lich", "wi\u00b7der", "um\u00b7zu\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "bezaubert wunderlich in mich", "tokens": ["be\u00b7zau\u00b7bert", "wun\u00b7der\u00b7lich", "in", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die hofnung, da\u00df sie alsbald sich", "tokens": ["die", "hof\u00b7nung", ",", "da\u00df", "sie", "als\u00b7bald", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADV", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "verm\u00e4hlen darf mit dem verlangen.", "tokens": ["ver\u00b7m\u00e4h\u00b7len", "darf", "mit", "dem", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "VMFIN", "APPR", "ART", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Daher dan kan sich der verdru\u00df", "tokens": ["Da\u00b7her", "dan", "kan", "sich", "der", "ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VMFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "erleichtern, da\u00df die finsternus", "tokens": ["er\u00b7leich\u00b7tern", ",", "da\u00df", "die", "fins\u00b7ter\u00b7nus"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVINF", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "des scheidens m\u00f6g nicht allzeit wehren,", "tokens": ["des", "schei\u00b7dens", "m\u00f6g", "nicht", "all\u00b7zeit", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wan du mich oftmal deiner schrift,", "tokens": ["wan", "du", "mich", "oft\u00b7mal", "dei\u00b7ner", "schrift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die wider des abwesens gift", "tokens": ["die", "wi\u00b7der", "des", "ab\u00b7we\u00b7sens", "gift"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "mich kan erquicken, wirst gewehren.", "tokens": ["mich", "kan", "er\u00b7qui\u00b7cken", ",", "wirst", "ge\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Bis nach wunsch deine widerkunft", "tokens": ["Bis", "nach", "wunsch", "dei\u00b7ne", "wi\u00b7der\u00b7kunft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "uns beed wird wider mit vernunft", "tokens": ["uns", "beed", "wird", "wi\u00b7der", "mit", "ver\u00b7nunft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "VAFIN", "APPR", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "freud und gl\u00fcckseligkeit erlaben,", "tokens": ["freud", "und", "gl\u00fcck\u00b7se\u00b7lig\u00b7keit", "er\u00b7la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "alsdan werd durch der sonnen schein", "tokens": ["als\u00b7dan", "werd", "durch", "der", "son\u00b7nen", "schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "ich von der nacht gefreiet sein,", "tokens": ["ich", "von", "der", "nacht", "ge\u00b7frei\u00b7et", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "in die mich dises leid begraben.", "tokens": ["in", "die", "mich", "di\u00b7ses", "leid", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PDS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch solt du mich hernach nicht mehr", "tokens": ["Doch", "solt", "du", "mich", "her\u00b7nach", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "f\u00fcr deines k\u00f6nigs gunst und ehr", "tokens": ["f\u00fcr", "dei\u00b7nes", "k\u00f6\u00b7nigs", "gunst", "und", "ehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "durch reis betr\u00fcben noch verlassen,", "tokens": ["durch", "reis", "be\u00b7tr\u00fc\u00b7ben", "noch", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "dan ich will dich mit solcher brunst,", "tokens": ["dan", "ich", "will", "dich", "mit", "sol\u00b7cher", "brunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "da\u00df aller f\u00fcrsten macht umsunst", "tokens": ["da\u00df", "al\u00b7ler", "f\u00fcrs\u00b7ten", "macht", "um\u00b7sunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "zu scheiden uns sein soll, umfassen.", "tokens": ["zu", "schei\u00b7den", "uns", "sein", "soll", ",", "um\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "VAINF