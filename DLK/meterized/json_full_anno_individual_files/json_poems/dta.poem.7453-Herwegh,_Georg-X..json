{"dta.poem.7453": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "X.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Gl\u00fcck, ihr G\u00f6tter, oder nur ein Leiden,", "tokens": ["Ein", "Gl\u00fcck", ",", "ihr", "G\u00f6t\u00b7ter", ",", "o\u00b7der", "nur", "ein", "Lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein himmlisch w\u00fcrdig Leiden Eurem Sohne!", "tokens": ["Ein", "himm\u00b7lisch", "w\u00fcr\u00b7dig", "Lei\u00b7den", "Eu\u00b7rem", "Soh\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Grunde ist es doch die Dornenkrone,", "tokens": ["Im", "Grun\u00b7de", "ist", "es", "doch", "die", "Dor\u00b7nen\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um die wir Eure Lieblinge beneiden.", "tokens": ["Um", "die", "wir", "Eu\u00b7re", "Lieb\u00b7lin\u00b7ge", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich kann das Gl\u00fcck mit stummem L\u00e4cheln meiden \u2014", "tokens": ["Ich", "kann", "das", "Gl\u00fcck", "mit", "stum\u00b7mem", "L\u00e4\u00b7cheln", "mei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Naht' ich mich je, ein Sklave, seinem Throne? \u2014", "tokens": ["Naht'", "ich", "mich", "je", ",", "ein", "Skla\u00b7ve", ",", "sei\u00b7nem", "Thro\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,", "ART", "NN", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur Eines w\u00fcnsch' ich, da\u00df ich einst nicht ohne", "tokens": ["Nur", "Ei\u00b7nes", "w\u00fcn\u00b7sch'", "ich", ",", "da\u00df", "ich", "einst", "nicht", "oh\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "APPR"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Des Ungl\u00fccks Weihe m\u00f6g' von hinnen scheiden.", "tokens": ["Des", "Un\u00b7gl\u00fccks", "Wei\u00b7he", "m\u00f6g'", "von", "hin\u00b7nen", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich bin entsagend gern zur\u00fcckgeblieben,", "tokens": ["Ich", "bin", "ent\u00b7sa\u00b7gend", "gern", "zu\u00b7r\u00fcck\u00b7ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn bl\u00fchendrot das Volk sich auf den Stra\u00dfen,", "tokens": ["Wenn", "bl\u00fc\u00b7hen\u00b7drot", "das", "Volk", "sich", "auf", "den", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit seinen Dirnen sch\u00e4ckernd, umgetrieben;", "tokens": ["Mit", "sei\u00b7nen", "Dir\u00b7nen", "sch\u00e4\u00b7ckernd", ",", "um\u00b7ge\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch manch ein stilles Antlitz von den blassen,", "tokens": ["Doch", "manch", "ein", "stil\u00b7les", "Ant\u00b7litz", "von", "den", "blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War's auch nur um ein ungl\u00fcckselig Lieben,", "tokens": ["Wa\u00b7r's", "auch", "nur", "um", "ein", "un\u00b7gl\u00fcck\u00b7se\u00b7lig", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "ART", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es mu\u00dfte sich von mir beneiden lassen.", "tokens": ["Es", "mu\u00df\u00b7te", "sich", "von", "mir", "be\u00b7nei\u00b7den", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}