 {"text": "Hier nannt' er's dreifach sein.", "tokens": ["Hier", "nannt'", "er's", "drei\u00b7fach", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PAV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Er sah verz\u00fcckt den Himmel auf,", "tokens": ["Er", "sah", "ver\u00b7z\u00fcckt", "den", "Him\u00b7mel", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah Krone nah und Kranz,", "tokens": ["Sah", "Kro\u00b7ne", "nah", "und", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sterbend schwang er sich hinauf", "tokens": ["Und", "ster\u00b7bend", "schwang", "er", "sich", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu vorgeahntem Glanz.", "tokens": ["Zu", "vor\u00b7ge\u00b7ahn\u00b7tem", "Glanz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}