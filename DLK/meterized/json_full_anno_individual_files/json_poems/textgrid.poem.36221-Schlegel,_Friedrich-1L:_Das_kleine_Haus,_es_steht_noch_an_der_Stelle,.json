{"textgrid.poem.36221": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das kleine Haus, es steht noch an der Stelle,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das kleine Haus, es steht noch an der Stelle,", "tokens": ["Das", "klei\u00b7ne", "Haus", ",", "es", "steht", "noch", "an", "der", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo ich es sonst gesehn vor vielen Jahren,", "tokens": ["Wo", "ich", "es", "sonst", "ge\u00b7sehn", "vor", "vie\u00b7len", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seit ich so manches Leid und Freud' erfahren,", "tokens": ["Seit", "ich", "so", "man\u00b7ches", "Leid", "und", "Freud'", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Umhergetragen auf des Lebens Welle;", "tokens": ["Um\u00b7her\u00b7ge\u00b7tra\u00b7gen", "auf", "des", "Le\u00b7bens", "Wel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dieselben Tritt' und Weg' an selber Stelle,", "tokens": ["Die\u00b7sel\u00b7ben", "Tritt'", "und", "Weg'", "an", "sel\u00b7b