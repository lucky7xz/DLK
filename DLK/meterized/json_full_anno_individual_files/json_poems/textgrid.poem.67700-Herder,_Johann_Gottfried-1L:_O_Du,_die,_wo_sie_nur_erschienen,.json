{"textgrid.poem.67700": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Du, die, wo sie nur erschienen,", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Du, die, wo sie nur erschienen,", "tokens": ["O", "Du", ",", "die", ",", "wo", "sie", "nur", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "$,", "PWAV", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Allg\u00fct'ge Liebe ist,", "tokens": ["All\u00b7g\u00fct'\u00b7ge", "Lie\u00b7be", "ist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wei\u00df Herzen herzlich zu gewinnen", "tokens": ["Wei\u00df", "Her\u00b7zen", "herz\u00b7lich", "zu", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Allen Alles bist,", "tokens": ["Und", "Al\u00b7len", "Al\u00b7les", "bist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PIS", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die einst auch mich die Himmelslehren", "tokens": ["Die", "einst", "auch", "mich", "die", "Him\u00b7mels\u00b7leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erst selbst vom Himmel machte h\u00f6ren!", "tokens": ["Erst", "selbst", "vom", "Him\u00b7mel", "mach\u00b7te", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sind mancherlei der Menschen Gaben,", "tokens": ["Sind", "man\u00b7cher\u00b7lei", "der", "Men\u00b7schen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Womit uns Gott verband,", "tokens": ["Wo\u00b7mit", "uns", "Gott", "ver\u00b7band", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So mu\u00dftest Du die sch\u00f6nste haben,", "tokens": ["So", "mu\u00df\u00b7test", "Du", "die", "sch\u00f6ns\u00b7te", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Liebe Bruderband!", "tokens": ["Der", "Lie\u00b7be", "Bru\u00b7der\u00b7band", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auf Wahrheit und auf Tugendwegen", "tokens": ["Auf", "Wahr\u00b7heit", "und", "auf", "Tu\u00b7gend\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist sie die Krone, Licht und Segen.", "tokens": ["Ist", "sie", "die", "Kro\u00b7ne", ",", "Licht", "und", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"