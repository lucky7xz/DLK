{"dta.poem.21752": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Abschied von Ida.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dich verliehren soll ich? dir entsagen,", "tokens": ["Dich", "ver\u00b7lieh\u00b7ren", "soll", "ich", "?", "dir", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "PPER", "$.", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die ich mir aus einer Welt erkohr?", "tokens": ["Die", "ich", "mir", "aus", "ei\u00b7ner", "Welt", "er\u00b7kohr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Die in jenen ewig sch\u00f6nen Tagen", "tokens": ["Die", "in", "je\u00b7nen", "e\u00b7wig", "sch\u00f6\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PDAT", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Frey und willig sich an mich verlor?", "tokens": ["Frey", "und", "wil\u00b7lig", "sich", "an", "mich", "ver\u00b7lor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Deinem Arme soll ich mich entwinden,", "tokens": ["Dei\u00b7nem", "Ar\u00b7me", "soll", "ich", "mich", "ent\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Der aus Millionen mich umwand?", "tokens": ["Der", "aus", "Mil\u00b7lion\u00b7en", "mich", "um\u00b7wand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Deines Flammeneides dich entbinden,", "tokens": ["Dei\u00b7nes", "Flam\u00b7me\u00b7nei\u00b7des", "dich", "ent\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Der f\u00fcr Zeit und Ewigkeit dich band?", "tokens": ["Der", "f\u00fcr", "Zeit", "und", "E\u00b7wig\u00b7keit", "dich", "band", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ida, kannst du Demantketten brechen,", "tokens": ["I\u00b7da", ",", "kannst", "du", "De\u00b7mant\u00b7ket\u00b7ten", "bre\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wie dein Finger schwache F\u00e4den bricht?", "tokens": ["Wie", "dein", "Fin\u00b7ger", "schwa\u00b7che", "F\u00e4\u00b7den", "bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ida, wird sich nicht die Liebe r\u00e4chen,", "tokens": ["I\u00b7da", ",", "wird", "sich", "nicht", "die", "Lie\u00b7be", "r\u00e4\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PRF", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der rebellisch sich dein Arm entflicht?", "tokens": ["Der", "re\u00b7bel\u00b7lisch", "sich", "dein", "Arm", "ent\u00b7flicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Willst du einsam durch das Leben irren,", "tokens": ["Willst", "du", "ein\u00b7sam", "durch", "das", "Le\u00b7ben", "ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Willst du stablos seinen Sturm bestehn,", "tokens": ["Willst", "du", "stab\u00b7los", "sei\u00b7nen", "Sturm", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ungetr\u00f6stet deine Klagen girren,", "tokens": ["Un\u00b7ge\u00b7tr\u00f6s\u00b7tet", "dei\u00b7ne", "Kla\u00b7gen", "gir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Unbeklagt ins Reich der Schatten gehn?", "tokens": ["Un\u00b7be\u00b7klagt", "ins", "Reich", "der", "Schat\u00b7ten", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Oder kannst du deine Liebschaft \u00e4ndern,", "tokens": ["O\u00b7der", "kannst", "du", "dei\u00b7ne", "Lieb\u00b7schaft", "\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Leicht und luftig, wie ein Sonntagskleid?", "tokens": ["Leicht", "und", "luf\u00b7tig", ",", "wie", "ein", "Sonn\u00b7tags\u00b7kleid", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Spielen M\u00e4dchen, wie mit Flor und B\u00e4ndern,", "tokens": ["Spie\u00b7len", "M\u00e4d\u00b7chen", ",", "wie", "mit", "Flor", "und", "B\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PWAV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "So mit Treu und Schwur und Ewigkeit?", "tokens": ["So", "mit", "Treu", "und", "Schwur", "und", "E\u00b7wig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "VVFIN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Finden magst du in der Freyer Reihe", "tokens": ["Fin\u00b7den", "magst", "du", "in", "der", "Frey\u00b7er", "Rei\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Einen sch\u00f6nern, kl\u00fcgern, reichern leicht;", "tokens": ["Ei\u00b7nen", "sch\u00f6\u00b7nern", ",", "kl\u00fc\u00b7gern", ",", "rei\u00b7chern", "leicht", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Doch auch einen, Ida, dem an Treue,", "tokens": ["Doch", "auch", "ei\u00b7nen", ",", "I\u00b7da", ",", "dem", "an", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "$,", "NE", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Dem an Zartgef\u00fchl dein Liebling weicht?", "tokens": ["Dem", "an", "Zart\u00b7ge\u00b7f\u00fchl", "dein", "Lieb\u00b7ling", "weicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Hab' ich einzig nicht an dir gehangen", "tokens": ["Hab'", "ich", "ein\u00b7zig", "nicht", "an", "dir", "ge\u00b7han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJD", "PTKNEG", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit Begriff, Gef\u00fchl und Fantasey,", "tokens": ["Mit", "Be\u00b7griff", ",", "Ge\u00b7f\u00fchl", "und", "Fan\u00b7ta\u00b7sey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Mit des Herzens innigstem Verlangen,", "tokens": ["Mit", "des", "Her\u00b7zens", "in\u00b7nigs\u00b7tem", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Mit des Geistes h\u00f6chster Schw\u00e4rmerey?", "tokens": ["Mit", "des", "Geis\u00b7tes", "h\u00f6chs\u00b7ter", "Schw\u00e4r\u00b7me\u00b7rey", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Hab' ich nicht dem Sch\u00f6nen, Guten, Wahren", "tokens": ["Hab'", "ich", "nicht", "dem", "Sch\u00f6\u00b7nen", ",", "Gu\u00b7ten", ",", "Wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "PPER", "PTKNEG", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Aufgeschlossen deinen treuen Blick?", "tokens": ["Auf\u00b7ge\u00b7schlos\u00b7sen", "dei\u00b7nen", "treu\u00b7en", "Blick", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Hab' ich nicht, um dich nur, dich zu sparen,", "tokens": ["Hab'", "ich", "nicht", ",", "um", "dich", "nur", ",", "dich", "zu", "spa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "$,", "KOUI", "PRF", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Aller Ruh entsagt und jedem Gl\u00fcck?", "tokens": ["Al\u00b7ler", "Ruh", "ent\u00b7sagt", "und", "je\u00b7dem", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "KON", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Gar in dich verlohren und versunken", "tokens": ["Gar", "in", "dich", "ver\u00b7loh\u00b7ren", "und", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "VVPP", "KON", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schwand mir Aussenwelt und Zeit und Raum;", "tokens": ["Schwand", "mir", "Aus\u00b7sen\u00b7welt", "und", "Zeit", "und", "Raum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00dcberseliger Gef\u00fchle trunken,", "tokens": ["\u00dc\u00b7ber\u00b7se\u00b7li\u00b7ger", "Ge\u00b7f\u00fch\u00b7le", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Taumelt' ich umher im wachen Traum.", "tokens": ["Tau\u00b7melt'", "ich", "um\u00b7her", "im", "wa\u00b7chen", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Fr\u00fch und spat, du weisst es, nah und ferne", "tokens": ["Fr\u00fch", "und", "spat", ",", "du", "weisst", "es", ",", "nah", "und", "fer\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KON", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Galt mein Denken, Dichten, Sehnen dich.", "tokens": ["Galt", "mein", "Den\u00b7ken", ",", "Dich\u00b7ten", ",", "Seh\u00b7nen", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Auf und unter gingen Mond und Sterne,", "tokens": ["Auf", "und", "un\u00b7ter", "gin\u00b7gen", "Mond", "und", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "APPR", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Fanden voll von dir, und selig mich.", "tokens": ["Fan\u00b7den", "voll", "von", "dir", ",", "und", "se\u00b7lig", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPER", "$,", "KON", "ADJD", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Wahrlich, Ida, so von dir durchdrungen,", "tokens": ["Wahr\u00b7lich", ",", "I\u00b7da", ",", "so", "von", "dir", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "$,", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "So voll Andacht und Religion,", "tokens": ["So", "voll", "An\u00b7dacht", "und", "Re\u00b7li\u00b7gi\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Solche Opfer, solche Anbetungen", "tokens": ["Sol\u00b7che", "Op\u00b7fer", ",", "sol\u00b7che", "An\u00b7be\u00b7tun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Endlos spendend sonder Sold und Lohn,", "tokens": ["End\u00b7los", "spen\u00b7dend", "son\u00b7der", "Sold", "und", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "So verschenkt an dich, an dich vergeben,", "tokens": ["So", "ver\u00b7schenkt", "an", "dich", ",", "an", "dich", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Ach, auf Gnad' und auf Barmherzigkeit,", "tokens": ["Ach", ",", "auf", "Gnad'", "und", "auf", "Barm\u00b7her\u00b7zig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Liebt in diesem, liebt in jenem Leben,", "tokens": ["Liebt", "in", "die\u00b7sem", ",", "liebt", "in", "je\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "$,", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Liebt dich keiner mehr in Ewigkeit.", "tokens": ["Liebt", "dich", "kei\u00b7ner", "mehr", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Und, o Seligkeit von kurzer Dauer!", "tokens": ["Und", ",", "o", "Se\u00b7lig\u00b7keit", "von", "kur\u00b7zer", "Dau\u00b7er", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Du auch liebtest mich. Dein knospend Herz", "tokens": ["Du", "auch", "lieb\u00b7test", "mich", ".", "Dein", "knos\u00b7pend", "Herz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$.", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00d6ffneten des Ahnens leise S