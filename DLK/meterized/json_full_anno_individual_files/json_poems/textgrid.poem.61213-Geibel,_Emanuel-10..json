{"textgrid.poem.61213": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O h\u00e4tt' ich Drachenz\u00e4hne statt der Lieder,", "tokens": ["O", "h\u00e4tt'", "ich", "Dra\u00b7chen\u00b7z\u00e4h\u00b7ne", "statt", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df, s\u00e4t' ich sie auf diese d\u00fcrre K\u00fcste,", "tokens": ["Da\u00df", ",", "s\u00e4t'", "ich", "sie", "auf", "die\u00b7se", "d\u00fcr\u00b7re", "K\u00fcs\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "VVFIN", "PPER", "PPER", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Draus ein Geschlecht von Kriegern wachsen m\u00fc\u00dfte,", "tokens": ["Draus", "ein", "Ge\u00b7schlecht", "von", "Krie\u00b7gern", "wach\u00b7sen", "m\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Waffentanz zu r\u00fchren Eisenglieder.", "tokens": ["Im", "Waf\u00b7fen\u00b7tanz", "zu", "r\u00fch\u00b7ren", "Ei\u00b7seng\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie alle sollten Deutschlands Heerschild wieder", "tokens": ["Sie", "al\u00b7le", "soll\u00b7ten", "Deutschlands", "Heer\u00b7schild", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "VMFIN", "NE", "NN", "ADV"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erh\u00f6hn, unnahbar jedem Raubgel\u00fcste,", "tokens": ["Er\u00b7h\u00f6hn", ",", "un\u00b7nah\u00b7bar", "je\u00b7dem", "Raub\u00b7ge\u00b7l\u00fcs\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nimmer fragen nach des Kampfes R\u00fcste,", "tokens": ["Und", "nim\u00b7mer", "fra\u00b7gen", "nach", "des", "Kamp\u00b7fes", "R\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis Hauch des Siegs umspielt' ihr Helmgefieder.", "tokens": ["Bis", "Hauch", "des", "Siegs", "um\u00b7spielt'", "ihr", "Helm\u00b7ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun hab' ich Worte nur, allein wie Saaten", "tokens": ["Nun", "hab'", "ich", "Wor\u00b7te", "nur", ",", "al\u00b7lein", "wie", "Saa\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ADV", "$,", "ADV", "KOKOM", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Will ich sie streun in deutsche Seelen wacker,", "tokens": ["Will", "ich", "sie", "streun", "in", "deut\u00b7sche", "See\u00b7len", "wa\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob hier und dort mag eine Frucht geraten.", "tokens": ["Ob", "hier", "und", "dort", "mag", "ei\u00b7ne", "Frucht", "ge\u00b7ra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KON", "ADV", "VMFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch soll draus aufgehn nicht ein Zorngeflacker,", "tokens": ["Doch", "soll", "draus", "auf\u00b7gehn", "nicht", "ein", "Zorn\u00b7ge\u00b7fla\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PAV", "VVIZU", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nein, ruhig ernst ein Mut zu gro\u00dfen Taten.", "tokens": ["Nein", ",", "ru\u00b7hig", "ernst", "ein", "Mut", "zu", "gro\u00b7\u00dfen", "Ta\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du aber, Herr, bereite selbst den Acker!", "tokens": ["Du", "a\u00b7ber", ",", "Herr", ",", "be\u00b7rei\u00b7te", "selbst", "den", "A\u00b7cker", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O h\u00e4tt' ich Drachenz\u00e4hne statt der Lieder,", "tokens": ["O", "h\u00e4tt'", "ich", "Dra\u00b7chen\u00b7z\u00e4h\u00b7ne", "statt", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE",