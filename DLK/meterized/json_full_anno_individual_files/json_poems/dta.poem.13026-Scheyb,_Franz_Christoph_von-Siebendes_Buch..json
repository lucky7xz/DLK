{"dta.poem.13026": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Etwelche stunden auf, und drangen sich darnach,", "tokens": ["Et\u00b7wel\u00b7che", "stun\u00b7den", "auf", ",", "und", "dran\u00b7gen", "sich", "dar\u00b7nach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "PTKVZ", "$,", "KON", "VVFIN", "PRF", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "We\u00dfwegen das Gespr\u00e4ch sich etwas unterbrach.", "tokens": ["We\u00df\u00b7we\u00b7gen", "das", "Ge\u00b7spr\u00e4ch", "sich", "et\u00b7was", "un\u00b7ter\u00b7brach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man wollte dort und da den Innbegriff ersehen;", "tokens": ["Man", "woll\u00b7te", "dort", "und", "da", "den", "Inn\u00b7be\u00b7griff", "er\u00b7se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "KON", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "160So mu\u00dfte dieses Buch von der zu jener gehen.", "tokens": ["mu\u00df\u00b7te", "die\u00b7ses", "Buch", "von", "der", "zu", "je\u00b7ner", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "APPR", "ART", "APPR", "PDAT", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}