{"dta.poem.9396": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Als H. Bartholomaeus Heinsius zu  \n Wittenberg Magister worden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Solte die Tugend so liegen verschwiegen?", "tokens": ["Sol\u00b7te", "die", "Tu\u00b7gend", "so", "lie\u00b7gen", "ver\u00b7schwie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVFIN", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Solte die Wissen schafft Ehren-lo\u00df seyn?", "tokens": ["Sol\u00b7te", "die", "Wis\u00b7sen", "schafft", "Eh\u00b7ren\u00b7lo\u00df", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVFIN", "NN", "VAINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Solten die K\u00fcnste verdunckelt erliegen", "tokens": ["Sol\u00b7ten", "die", "K\u00fcns\u00b7te", "ver\u00b7dun\u00b7ckelt", "er\u00b7lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "VVPP", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Ohne Belohnung und W\u00fcrden? ach nein!", "tokens": ["Oh\u00b7ne", "Be\u00b7loh\u00b7nung", "und", "W\u00fcr\u00b7den", "?", "ach", "nein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "XY", "PTKANT", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Die Heliconinnen", "tokens": ["Die", "He\u00b7li\u00b7co\u00b7nin\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "erwecken die Sinnen", "tokens": ["er\u00b7we\u00b7cken", "die", "Sin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "und winden den Kr\u00e4ntz;", "tokens": ["und", "win\u00b7den", "den", "Kr\u00e4ntz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Die Hochzeit herdringet/", "tokens": ["Die", "Hoch\u00b7zeit", "her\u00b7drin\u00b7get", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "das Seitenspiel klinget/", "tokens": ["das", "Sei\u00b7ten\u00b7spiel", "klin\u00b7get", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "unsre Sophia die zieret den tantz.", "tokens": ["uns\u00b7re", "So\u00b7phia", "die", "zie\u00b7ret", "den", "tantz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Ph\u00f6bus hatt jtzund dier solches gegeben", "tokens": ["Ph\u00f6\u00b7bus", "hatt", "jt\u00b7zund", "dier", "sol\u00b7ches", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "ADV", "PIS", "VVPP"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "was du verdienet durch t\u00e4glichen flei\u00df/", "tokens": ["was", "du", "ver\u00b7die\u00b7net", "durch", "t\u00e4g\u00b7li\u00b7chen", "flei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Thoren und Bre\u00dflau gab edele Reben/", "tokens": ["Tho\u00b7ren", "und", "Bre\u00df\u00b7lau", "gab", "e\u00b7de\u00b7le", "Re\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "VVFIN", "ADJA", "NN", "$("], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Rostock die Trauben und L\u00fcbeck den Prei\u00df;", "tokens": ["Ros\u00b7tock", "die", "Trau\u00b7ben", "und", "L\u00fc\u00b7beck", "den", "Prei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "NE", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "ja Leypzig dich tr\u00e4nckte", "tokens": ["ja", "Leyp\u00b7zig", "dich", "tr\u00e4nck\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NE", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "und s\u00fc\u00dfen Most sch\u00e4uckte/", "tokens": ["und", "s\u00fc\u00b7\u00dfen", "Most", "sch\u00e4uck\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Nun folget der Wein/", "tokens": ["Nun", "fol\u00b7get", "der", "Wein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "den Wittenberg giebet/", "tokens": ["den", "Wit\u00b7ten\u00b7berg", "gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "das deine Kunst liebet/", "tokens": ["das", "dei\u00b7ne", "Kunst", "lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "sch\u00e4ncket dier/ Liebster Freund/ Ehre vollein.", "tokens": ["sch\u00e4n\u00b7cket", "dier", "/", "Liebs\u00b7ter", "Freund", "/", "Eh\u00b7re", "voll\u00b7ein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ADJA", "NN", "$(", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Pallas lieb-\u00e4ugelt und l\u00e4chlet vor freude/", "tokens": ["Pal\u00b7las", "lie\u00b7b\u00e4u\u00b7gelt", "und", "l\u00e4ch\u00b7let", "vor", "freu\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "VVFIN", "APPR", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "alle G\u00f6ttinnen erfreuen sich auch/", "tokens": ["al\u00b7le", "G\u00f6t\u00b7tin\u00b7nen", "er\u00b7freu\u00b7en", "sich", "auch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "ADV", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "unsre Sophia windt Kr\u00e4ntze mit seide/", "tokens": ["uns\u00b7re", "So\u00b7phia", "windt", "Kr\u00e4nt\u00b7ze", "mit", "sei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "APPR", "PPOSAT", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "gibet den trau-ring nach jhrem gebrauch.", "tokens": ["gi\u00b7bet", "den", "trau\u00b7ring", "nach", "jhrem", "ge\u00b7brauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Ey! lustig! jhr Br\u00fcder", "tokens": ["Ey", "!", "lus\u00b7tig", "!", "jhr", "Br\u00fc\u00b7der"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$.", "ADJD", "$.", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Dactylische Lieder", "tokens": ["Dac\u00b7ty\u00b7li\u00b7sche", "Lie\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Nun singet und klingt", "tokens": ["Nun", "sin\u00b7get", "und", "klingt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Sophia mein Leben/", "tokens": ["So\u00b7phia", "mein", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Dier will ich nachstreben;", "tokens": ["Dier", "will", "ich", "nach\u00b7stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "unsere feder gen Himmel dich schwingt.", "tokens": ["un\u00b7se\u00b7re", "fe\u00b7der", "gen", "Him\u00b7mel", "dich", "schwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Mercke Sophia/ dis sing\u2019 ich zu ehren", "tokens": ["Mer\u00b7cke", "So\u00b7phia", "/", "dis", "sing'", "ich", "zu", "eh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$(", "PDS", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "deinem Geliebten und selbsten auch dier/", "tokens": ["dei\u00b7nem", "Ge\u00b7lieb\u00b7ten", "und", "selbs\u00b7ten", "auch", "dier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "ADV", "ADV", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "unsere Muse wird solches vermehren/", "tokens": ["un\u00b7se\u00b7re", "Mu\u00b7se", "wird", "sol\u00b7ches", "ver\u00b7meh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIS", "VVINF", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "geben dein hohes Lob weiter herf\u00fcr;", "tokens": ["ge\u00b7ben", "dein", "ho\u00b7hes", "Lob", "wei\u00b7ter", "her\u00b7f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Hier will ich beschlie\u00dfen", "tokens": ["Hier", "will", "ich", "be\u00b7schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "mit Versen zu gr\u00fc\u00dfen/", "tokens": ["mit", "Ver\u00b7sen", "zu", "gr\u00fc\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Dich Edeles Bild/", "tokens": ["Dich", "E\u00b7de\u00b7les", "Bild", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "wil anderwerts singen", "tokens": ["wil", "an\u00b7der\u00b7werts", "sin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "von h\u00f6heren dingen", "tokens": ["von", "h\u00f6\u00b7he\u00b7ren", "din\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "welche dein Hertze vollk\u00f6mlich erf\u00fcllt.", "tokens": ["wel\u00b7che", "dein", "Hert\u00b7ze", "voll\u00b7k\u00f6m\u00b7lich", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPOSAT", "VVFIN", "ADJD", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}