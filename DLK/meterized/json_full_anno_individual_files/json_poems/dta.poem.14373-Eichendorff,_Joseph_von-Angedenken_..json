{"dta.poem.14373": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Angedenken .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ach ein solches Angedenken,", "tokens": ["Ach", "ein", "sol\u00b7ches", "An\u00b7ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "'s ist nur eitel Klang und Luft,", "tokens": ["'s", "ist", "nur", "ei\u00b7tel", "Klang", "und", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und kann schimmernd doch versenken", "tokens": ["Und", "kann", "schim\u00b7mernd", "doch", "ver\u00b7sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADJD", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rings in Thr\u00e4nen Thal und Kluft!", "tokens": ["Rings", "in", "Thr\u00e4\u00b7nen", "Thal", "und", "Kluft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}