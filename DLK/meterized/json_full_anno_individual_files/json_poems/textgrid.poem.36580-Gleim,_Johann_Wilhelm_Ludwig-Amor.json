{"textgrid.poem.36580": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Amor", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amor lief mit einer Fackel", "tokens": ["A\u00b7mor", "lief", "mit", "ei\u00b7ner", "Fa\u00b7ckel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf Anakreon einst zu!", "tokens": ["Auf", "A\u00b7nak\u00b7re\u00b7on", "einst", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freund, ich habe dich errettet,", "tokens": ["Freund", ",", "ich", "ha\u00b7be", "dich", "er\u00b7ret\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprach er, brennen solltest du!", "tokens": ["Sprach", "er", ",", "bren\u00b7nen", "soll\u00b7test", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VVINF", "VMFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hymen sa\u00df mit dieser Fackel", "tokens": ["Hy\u00b7men", "sa\u00df", "mit", "die\u00b7ser", "Fa\u00b7ckel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Geb\u00fcsch, und wollte dich", "tokens": ["Im", "Ge\u00b7b\u00fcsch", ",", "und", "woll\u00b7te", "dich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,",