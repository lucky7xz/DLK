{"dta.poem.11680": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Bei Gelegenheit eines Kinderspielzeugs,  \n vorstellend:  \n  Hanswurst an der Sandm\u00fchle.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Volksmittel? Ich versteh'", "tokens": ["Volks\u00b7mit\u00b7tel", "?", "Ich", "ver\u00b7steh'"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Spitzbub! Schlagt ihm den Sch\u00e4del ein!", "tokens": ["Spitz\u00b7bub", "!", "Schlagt", "ihm", "den", "Sch\u00e4\u00b7del", "ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}