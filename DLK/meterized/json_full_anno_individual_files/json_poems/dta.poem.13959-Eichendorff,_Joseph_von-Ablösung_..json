{"dta.poem.13959": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abl\u00f6sung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Verlassen nun stehen die R\u00e4ume,", "tokens": ["Ver\u00b7las\u00b7sen", "nun", "ste\u00b7hen", "die", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Es schauen und rauschen allein", "tokens": ["Es", "schau\u00b7en", "und", "rau\u00b7schen", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die gro\u00df gewordenen B\u00e4ume", "tokens": ["Die", "gro\u00df", "ge\u00b7wor\u00b7de\u00b7nen", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So ernst in die Stille herein.", "tokens": ["So", "ernst", "in", "die", "Stil\u00b7le", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}