{"textgrid.poem.38779": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[o lichtes Strahlen dieser holden Blicke!]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O lichtes Strahlen dieser holden Blicke!", "tokens": ["O", "lich\u00b7tes", "Strah\u00b7len", "die\u00b7ser", "hol\u00b7den", "Bli\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind's Himmelskr\u00e4fte, die sich aus euch gie\u00dfen?", "tokens": ["Sin\u00b7d's", "Him\u00b7mels\u00b7kr\u00e4f\u00b7te", ",", "die", "sich", "aus", "euch", "gie\u00b7\u00dfen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "L\u00e4cheln, willst du auf rothen Lippen sprie\u00dfen,", "tokens": ["L\u00e4\u00b7cheln", ",", "willst", "du", "auf", "ro\u00b7then", "Lip\u00b7pen", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ist mir, als ob ich Himmelsfrucht erblicke.", "tokens": ["Ist", "mir", ",", "als", "ob", "ich", "Him\u00b7mels\u00b7frucht", "er\u00b7bli\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOKOM", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ton! Wort! Gesang! o Wahn! o holdes Gl\u00fccke!",