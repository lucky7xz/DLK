{"dta.poem.23518": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Chatten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sitte war's in alten Tagen", "tokens": ["Sit\u00b7te", "wa\u00b7r's", "in", "al\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Bei der edlen Chattenschaar:", "tokens": ["Bei", "der", "ed\u00b7len", "Chat\u00b7ten\u00b7schaar", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis man einen Feind erschlagen,", "tokens": ["Bis", "man", "ei\u00b7nen", "Feind", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lie\u00df man wachsen Bart und Haar.", "tokens": ["Lie\u00df", "man", "wach\u00b7sen", "Bart", "und", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch noch heute giebt es Chatten,", "tokens": ["Auch", "noch", "heu\u00b7te", "giebt", "es", "Chat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mit langen B\u00e4rten gehn,", "tokens": ["Die", "mit", "lan\u00b7gen", "B\u00e4r\u00b7ten", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil sie noch das Gl\u00fcck nicht hatten,", "tokens": ["Weil", "sie", "noch", "das", "Gl\u00fcck", "nicht", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "PTKNEG", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Irgend einen Feind zu sehn. \u2014", "tokens": ["Ir\u00b7gend", "ei\u00b7nen", "Feind", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo die meisten Feinde waren,", "tokens": ["Wo", "die", "meis\u00b7ten", "Fein\u00b7de", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PIAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drang der Chatte wild hinein,", "tokens": ["Drang", "der", "Chat\u00b7te", "wild", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von des Leibes Feigheitshaaren", "tokens": ["Von", "des", "Lei\u00b7bes", "Feig\u00b7heits\u00b7haa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollt' er zeitig sich befrein.", "tokens": ["Wollt'", "er", "zei\u00b7tig", "sich", "be\u00b7fr\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir auch haben heute Chatten,", "tokens": ["Wir", "auch", "ha\u00b7ben", "heu\u00b7te", "Chat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mit langen B\u00e4rten gehn,", "tokens": ["Die", "mit", "lan\u00b7gen", "B\u00e4r\u00b7ten", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch sie wollen auch den Schatten", "tokens": ["Doch", "sie", "wol\u00b7len", "auch", "den", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eines Feindes nicht mal sehn.", "tokens": ["Ei\u00b7nes", "Fein\u00b7des", "nicht", "mal", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}