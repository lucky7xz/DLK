{"textgrid.poem.61000": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Der Zigeunerbube im Norden", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fern im S\u00fcd das sch\u00f6ne Spanien,", "tokens": ["Fern", "im", "S\u00fcd", "das", "sch\u00f6\u00b7ne", "Spa\u00b7ni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+----", "measure": "unknown.measure.tri"}, "line.2": {"text": "Spanien ist mein Heimatland,", "tokens": ["Spa\u00b7ni\u00b7en", "ist", "mein", "Hei\u00b7mat\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo die schattigen Kastanien", "tokens": ["Wo", "die", "schat\u00b7ti\u00b7gen", "Kas\u00b7ta\u00b7ni\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Rauschen an des Ebro Strand,", "tokens": ["Rau\u00b7schen", "an", "des", "Eb\u00b7ro", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo die Mandeln r\u00f6tlich bl\u00fchen,", "tokens": ["Wo", "die", "Man\u00b7deln", "r\u00f6t\u00b7lich", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo die hei\u00dfe Traube winkt,", "tokens": ["Wo", "die", "hei\u00b7\u00dfe", "Trau\u00b7be", "winkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Rosen sch\u00f6ner gl\u00fchen,", "tokens": ["Und", "die", "Ro\u00b7sen", "sch\u00f6\u00b7ner", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und das Mondlicht goldner blinkt.", "tokens": ["Und", "das", "Mond\u00b7licht", "gold\u00b7ner", "blinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und nun wandr' ich mit der Laute", "tokens": ["Und", "nun", "wandr'", "ich", "mit", "der", "Lau\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Traurig hier von Haus zu Haus,", "tokens": ["Trau\u00b7rig", "hier", "von", "Haus", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch kein helles Auge schaute", "tokens": ["Doch", "kein", "hel\u00b7les", "Au\u00b7ge", "schau\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freundlich noch nach mir heraus.", "tokens": ["Freund\u00b7lich", "noch", "nach", "mir", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sp\u00e4rlich reicht man mir die Gaben,", "tokens": ["Sp\u00e4r\u00b7lich", "reicht", "man", "mir", "die", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "M\u00fcrrisch hei\u00dfet man mich gehn;", "tokens": ["M\u00fcr\u00b7risch", "hei\u00b7\u00dfet", "man", "mich", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, den armen braunen Knaben", "tokens": ["Ach", ",", "den", "ar\u00b7men", "brau\u00b7nen", "Kna\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Will kein einziger verstehn.", "tokens": ["Will", "kein", "ein\u00b7zi\u00b7ger", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ADJA", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Dieser Nebel dr\u00fcckt mich nieder,", "tokens": ["Die\u00b7ser", "Ne\u00b7bel", "dr\u00fcckt", "mich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der die Sonne mir entfernt,", "tokens": ["Der", "die", "Son\u00b7ne", "mir", "ent\u00b7fernt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die alten lust'gen Lieder", "tokens": ["Und", "die", "al\u00b7ten", "lust'\u00b7gen", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hab' ich alle fast verlernt.", "tokens": ["Hab'", "ich", "al\u00b7le", "fast", "ver\u00b7lernt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Immer in die Melodien", "tokens": ["Im\u00b7mer", "in", "die", "Me\u00b7lo\u00b7dien"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schleicht der eine Klang sich ein:", "tokens": ["Schleicht", "der", "ei\u00b7ne", "Klang", "sich", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbin die Heimat m\u00f6cht' ich ziehen,", "tokens": ["\u00bb", "in", "die", "Hei\u00b7mat", "m\u00f6cht'", "ich", "zie\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In das Land voll Sonnenschein!\u00ab", "tokens": ["In", "das", "Land", "voll", "Son\u00b7nen\u00b7schein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Als beim letzten Erntefeste", "tokens": ["Als", "beim", "letz\u00b7ten", "Ern\u00b7te\u00b7fes\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Man den gro\u00dfen Reigen hielt,", "tokens": ["Man", "den", "gro\u00b7\u00dfen", "Rei\u00b7gen", "hielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab' ich j\u00fcngst das allerbeste", "tokens": ["Hab'", "ich", "j\u00fcngst", "das", "al\u00b7ler\u00b7bes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Lieder aufgespielt.", "tokens": ["Mei\u00b7ner", "Lie\u00b7der", "auf\u00b7ge\u00b7spielt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch wie sich die Paare schwangen", "tokens": ["Doch", "wie", "sich", "die", "Paa\u00b7re", "schwan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PRF", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "In der Abendsonne Gold,", "tokens": ["In", "der", "A\u00b7bend\u00b7son\u00b7ne", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sind auf meine dunkeln Wangen", "tokens": ["Sind", "auf", "mei\u00b7ne", "dun\u00b7keln", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hei\u00dfe Tr\u00e4nen hingerollt.", "tokens": ["Hei\u00b7\u00dfe", "Tr\u00e4\u00b7nen", "hin\u00b7ge\u00b7rollt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach, ich dachte bei dem Tanze", "tokens": ["Ach", ",", "ich", "dach\u00b7te", "bei", "dem", "Tan\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An des Vaterlandes Lust,", "tokens": ["An", "des", "Va\u00b7ter\u00b7lan\u00b7des", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo im duft'gen Mondenglanze", "tokens": ["Wo", "im", "duft'\u00b7gen", "Mon\u00b7den\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freier atmet jede Brust,", "tokens": ["Frei\u00b7er", "at\u00b7met", "je\u00b7de", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo sich bei der Zither T\u00f6nen", "tokens": ["Wo", "sich", "bei", "der", "Zi\u00b7ther", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jeder Fu\u00df befl\u00fcgelt schwingt,", "tokens": ["Je\u00b7der", "Fu\u00df", "be\u00b7fl\u00fc\u00b7gelt", "schwingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Knabe mit der Sch\u00f6nen", "tokens": ["Und", "der", "Kna\u00b7be", "mit", "der", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gl\u00fchend den Fandango schlingt.", "tokens": ["Gl\u00fc\u00b7hend", "den", "Fan\u00b7dan\u00b7go", "schlingt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Nein! Des Herzens sehnend Schlagen", "tokens": ["Nein", "!", "Des", "Her\u00b7zens", "seh\u00b7nend", "Schla\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "ART", "NN", "VVPP", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4nger halt' ich's nicht zur\u00fcck;", "tokens": ["L\u00e4n\u00b7ger", "halt'", "ich's", "nicht", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will ja jeder Lust entsagen,", "tokens": ["Will", "ja", "je\u00b7der", "Lust", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft mir nur der Heimat Gl\u00fcck!", "tokens": ["La\u00dft", "mir", "nur", "der", "Hei\u00b7mat", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fort zum S\u00fcden! Fort nach Spanien,", "tokens": ["Fort", "zum", "S\u00fc\u00b7den", "!", "Fort", "nach", "Spa\u00b7ni\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "NN", "APPR", "NE", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "In das Land voll Sonnenschein!", "tokens": ["In", "das", "Land", "voll", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"