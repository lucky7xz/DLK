{"textgrid.poem.58584": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, man laufet hin und her, meint, den Schatz also zu finden,", "tokens": ["Ach", ",", "man", "lau\u00b7fet", "hin", "und", "her", ",", "meint", ",", "den", "Schatz", "al\u00b7so", "zu", "fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,", "VVFIN", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Seel', es bleibet deine Pein,", "tokens": ["Seel'", ",", "es", "blei\u00b7bet", "dei\u00b7ne", "Pein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo du nicht willst stille sein;", "tokens": ["Wo", "du", "nicht", "willst", "stil\u00b7le", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VMFIN", "PIS", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Brich den eignen Willen nur, la\u00df die Kreatur verschwinden,", "tokens": ["Brich", "den", "eig\u00b7nen", "Wil\u00b7len", "nur", ",", "la\u00df", "die", "Kre\u00b7a\u00b7tur", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "ADV", "$,", "VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Kehr dich ein und harre da,", "tokens": ["Kehr", "dich", "ein", "und", "har\u00b7re", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "KON", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "So ist Gottes Hilfe nah!", "tokens": ["So", "ist", "Got\u00b7tes", "Hil\u00b7fe", "nah", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der Vernunft Gesch\u00e4ftigkeit und die Fl\u00fcchtigkeit der Sinnen,", "tokens": ["Der", "Ver\u00b7nunft", "Ge\u00b7sch\u00e4f\u00b7tig\u00b7keit", "und", "die", "Fl\u00fcch\u00b7tig\u00b7keit", "der", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Alle Mannigfaltigkeit", "tokens": ["Al\u00b7le", "Man\u00b7nig\u00b7fal\u00b7tig\u00b7keit"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Bringe sanft zur Einigkeit,", "tokens": ["Brin\u00b7ge", "sanft", "zur", "Ei\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Bleibe so vertraulich still, Gott ist nahe bei dir drinnen,", "tokens": ["Blei\u00b7be", "so", "ver\u00b7trau\u00b7lich", "still", ",", "Gott", "ist", "na\u00b7he", "bei", "dir", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "PTKVZ", "$,", "NN", "VAFIN", "ADJD", "APPR", "PPER", "ADV", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Stillesein und guter Mut", "tokens": ["Stil\u00b7le\u00b7sein", "und", "gu\u00b7ter", "Mut"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Mit Gott gro\u00dfe Taten tut!", "tokens": ["Mit", "Gott", "gro\u00b7\u00dfe", "Ta\u00b7ten", "tut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, man laufet hin und her, meint, den Schatz also zu finden,", "tokens": ["Ach", ",", "man", "lau\u00b7fet", "hin", "und", "her", ",", "meint", ",", "den", "Schatz", "al\u00b7so", "zu", "fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,", "VVFIN", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Seel', es bleibet deine Pein,", "tokens": ["Seel'", ",", "es", "blei\u00b7bet", "dei\u00b7ne", "Pein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo du nicht willst stille sein;", "tokens": ["Wo", "du", "nicht", "willst", "stil\u00b7le", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VMFIN", "PIS", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Brich den eignen Willen nur, la\u00df die Kreatur verschwinden,", "tokens": ["Brich", "den", "eig\u00b7nen", "Wil\u00b7len", "nur", ",", "la\u00df", "die", "Kre\u00b7a\u00b7tur", "ver\u00b7schwin\u0