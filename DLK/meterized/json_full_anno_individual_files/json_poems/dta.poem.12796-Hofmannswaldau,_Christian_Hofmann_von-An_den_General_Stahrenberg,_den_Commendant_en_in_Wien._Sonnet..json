{"dta.poem.12796": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An den General Stahrenberg, den  \n  Commendant en in Wien.  \n Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Held! den die Christenheit mit tausend palmen-zweigen,", "tokens": ["Held", "!", "den", "die", "Chris\u00b7ten\u00b7heit", "mit", "tau\u00b7send", "pal\u00b7men\u00b7zwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Als einen Hunniad und Scanderbegg verehrt!", "tokens": ["Als", "ei\u00b7nen", "Hun\u00b7ni\u00b7ad", "und", "Scan\u00b7der\u00b7begg", "ver\u00b7ehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df Wien, das edle Wien! der T\u00fcrcke nicht zust\u00f6rt:", "tokens": ["Da\u00df", "Wi\u00b7en", ",", "das", "ed\u00b7le", "Wi\u00b7en", "!", "der", "T\u00fcr\u00b7cke", "nicht", "zu\u00b7st\u00f6rt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "ART", "ADJA", "NN", "$.", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df sich der Ister nicht darff vor dem Bosphor neigen:", "tokens": ["Da\u00df", "sich", "der", "Is\u00b7ter", "nicht", "darff", "vor", "dem", "Bosp\u00b7hor", "nei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "PTKNEG", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und da\u00df die sonne noch kan ihre strahlen zeigen:", "tokens": ["Und", "da\u00df", "die", "son\u00b7ne", "noch", "kan", "ih\u00b7re", "strah\u00b7len", "zei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das dauckt man dir, o Held! der du die welt gelehrt,", "tokens": ["Das", "dauckt", "man", "dir", ",", "o", "Held", "!", "der", "du", "die", "welt", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "PPER", "$,", "FM", "NN", "$.", "ART", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was witz und tugend kan; die ihren ruhm vermehrt,", "tokens": ["Was", "witz", "und", "tu\u00b7gend", "kan", ";", "die", "ih\u00b7ren", "ruhm", "ver\u00b7mehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "NN", "VMFIN", "$.", "ART", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn ihr die fluten schon bis an die lippen steigen.", "tokens": ["Wenn", "ihr", "die", "flu\u00b7ten", "schon", "bis", "an", "die", "lip\u00b7pen", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jtzt mu\u00df der tolle schwarm geschimpfft zur\u00fccke gehn,", "tokens": ["Jtzt", "mu\u00df", "der", "tol\u00b7le", "schwarm", "ge\u00b7schimpfft", "zu\u00b7r\u00fc\u00b7cke", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "ADJD", "VVPP", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und seines monden schein mit blut verd\u00fcstert sehn:", "tokens": ["Und", "sei\u00b7nes", "mon\u00b7den", "schein", "mit", "blut", "ver\u00b7d\u00fcs\u00b7tert", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "APPR", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er mu\u00df vor Christi creutz aus furcht die fahne sencken,", "tokens": ["Er", "mu\u00df", "vor", "Chris\u00b7ti", "creutz", "aus", "furcht", "die", "fah\u00b7ne", "sen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NE", "NN", "APPR", "NN", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und wird mit h\u00f6chstem schmertz an dich und Wien gedencken.", "tokens": ["Und", "wird", "mit", "h\u00f6chs\u00b7tem", "schmertz", "an", "dich", "und", "Wi\u00b7en", "ge\u00b7den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "APPR", "PPER", "KON", "NE", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Doch gieb es willig nach, o tapffrer Stahrenberg!", "tokens": ["Doch", "gieb", "es", "wil\u00b7lig", "nach", ",", "o", "tapf\u00b7frer", "Stah\u00b7ren\u00b7berg", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "PTKVZ", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es ist nicht dein allein, es ist des H\u00f6chsten werck.", "tokens": ["Es", "ist", "nicht", "dein", "al\u00b7lein", ",", "es", "ist", "des", "H\u00f6chs\u00b7ten", "werck", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PPOSAT", "ADV", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}