{"dta.poem.20738": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An einen Ingenieur ,  \n Liebhaber der Phyllis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du kennst den Grund der Festungswerke.               ", "tokens": ["Du", "kennst", "den", "Grund", "der", "Fes\u00b7tungs\u00b7wer\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einem Blicke messest du", "tokens": ["Mit", "ei\u00b7nem", "Bli\u00b7cke", "mes\u00b7sest", "du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Schanzen und der Mauern St\u00e4rke;", "tokens": ["Der", "Schan\u00b7zen", "und", "der", "Mau\u00b7ern", "St\u00e4r\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch meine Muse ruft dir zu:", "tokens": ["Doch", "mei\u00b7ne", "Mu\u00b7se", "ruft", "dir", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wahr, als Friedrich unvergessen", "tokens": ["So", "wahr", ",", "als", "Fried\u00b7rich", "un\u00b7ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "NE", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bewundert wird in sp\u00e4ter Zeit,", "tokens": ["Be\u00b7wun\u00b7dert", "wird", "in", "sp\u00e4\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So wahr ist dies Unm\u00f6glichkeit", "tokens": ["So", "wahr", "ist", "dies", "Un\u00b7m\u00f6g\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PDS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Herzens Tiefen auszumessen.", "tokens": ["Des", "Her\u00b7zens", "Tie\u00b7fen", "aus\u00b7zu\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sei klug, bedenke dich so schlau", "tokens": ["Sei", "klug", ",", "be\u00b7den\u00b7ke", "dich", "so", "schlau"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wie einst Ulysses ist gewesen,", "tokens": ["Wie", "einst", "U\u00b7lys\u00b7ses", "ist", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NE", "VAFIN", "VAPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Nie wirst du der verschmitzten Frau", "tokens": ["Nie", "wirst", "du", "der", "ver\u00b7schmitz\u00b7ten", "Frau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Verborgenste Gedanken lesen.", "tokens": ["Ver\u00b7bor\u00b7gens\u00b7te", "Ge\u00b7dan\u00b7ken", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sie decket ihre feinste List", "tokens": ["Sie", "de\u00b7cket", "ih\u00b7re", "feins\u00b7te", "List"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Mit Blumen zu, bis du gefangen", "tokens": ["Mit", "Blu\u00b7men", "zu", ",", "bis", "du", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "PTKVZ", "$,", "KOUS", "PPER", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.15": {"text": "Gleich einem Dohnenvogel bist.", "tokens": ["Gleich", "ei\u00b7nem", "Doh\u00b7nen\u00b7vo\u00b7gel", "bist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Sie schmachtet, seufzt, netzt ihre Wangen", "tokens": ["Sie", "schmach\u00b7tet", ",", "seufzt", ",", "netzt", "ih\u00b7re", "Wan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Mit Thr\u00e4nen, die sie k\u00fcnftig weint.", "tokens": ["Mit", "Thr\u00e4\u00b7nen", ",", "die", "sie", "k\u00fcnf\u00b7tig", "weint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Sie nennt dich oft in einer Stunde", "tokens": ["Sie", "nennt", "dich", "oft", "in", "ei\u00b7ner", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Wohl tausendmal den besten Freund,", "tokens": ["Wohl", "tau\u00b7send\u00b7mal", "den", "bes\u00b7ten", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und schw\u00f6rt mit schmeichlerischem Munde", "tokens": ["Und", "schw\u00f6rt", "mit", "schmeich\u00b7le\u00b7ri\u00b7schem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Beim Grabmal ihres Vaters, bei", "tokens": ["Beim", "Grab\u00b7mal", "ih\u00b7res", "Va\u00b7ters", ",", "bei"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Den Sternen und bei allen G\u00f6ttern,", "tokens": ["Den", "Ster\u00b7nen", "und", "bei", "al\u00b7len", "G\u00f6t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Bei Sonnenschein und Donnerwettern,", "tokens": ["Bei", "Son\u00b7nen\u00b7schein", "und", "Don\u00b7ner\u00b7wet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Da\u00df ihr dein Ku\u00df noch s\u00fc\u00dfer sei,", "tokens": ["Da\u00df", "ihr", "dein", "Ku\u00df", "noch", "s\u00fc\u00b7\u00dfer", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Als S\u00fc\u00dfigkeit von jungen Bienen;", "tokens": ["Als", "S\u00fc\u00b7\u00dfig\u00b7keit", "von", "jun\u00b7gen", "Bie\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Und zaubert dich mit holden Mienen", "tokens": ["Und", "zau\u00b7bert", "dich", "mit", "hol\u00b7den", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "An ihre giftbestrichne Brust", "tokens": ["An", "ih\u00b7re", "gift\u00b7be\u00b7strich\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Und nennt dich ihre gr\u00f6\u00dfte Lust,", "tokens": ["Und", "nennt", "dich", "ih\u00b7re", "gr\u00f6\u00df\u00b7te", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Den ersten Abgott ihrer Seele,", "tokens": ["Den", "ers\u00b7ten", "Ab\u00b7gott", "ih\u00b7rer", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Den reichsten J\u00fcngling von der Welt,", "tokens": ["Den", "reichs\u00b7ten", "J\u00fcng\u00b7ling", "von", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Den Menschen, der in einer H\u00f6hle", "tokens": ["Den", "Men\u00b7schen", ",", "der", "in", "ei\u00b7ner", "H\u00f6h\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Mehr ihren Augen wohlgef\u00e4llt,", "tokens": ["Mehr", "ih\u00b7ren", "Au\u00b7gen", "wohl\u00b7ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Als Prinzen, die so fein nicht f\u00fchlen", "tokens": ["Als", "Prin\u00b7zen", ",", "die", "so", "fein", "nicht", "f\u00fch\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "PRELS", "ADV", "ADJD", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Im Prunksaal und auf goldnen St\u00fchlen", "tokens": ["Im", "Prunk\u00b7saal", "und", "auf", "gold\u00b7nen", "St\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Und einer sammtbezognen Bank.", "tokens": ["Und", "ei\u00b7ner", "sammt\u00b7be\u00b7zog\u00b7nen", "Bank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Sie stellt sich gar vor Liebe krank,", "tokens": ["Sie", "stellt", "sich", "gar", "vor", "Lie\u00b7be", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Und redet nur gebrochne T\u00f6ne.", "tokens": ["Und", "re\u00b7det", "nur", "ge\u00b7broch\u00b7ne", "T\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "O sanfter J\u00fcngling, glaub es nicht:", "tokens": ["O", "sanf\u00b7ter", "J\u00fcng\u00b7ling", ",", "glaub", "es", "nicht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Es ist die Stimme der Syrene,", "tokens": ["Es", "ist", "die", "Stim\u00b7me", "der", "Sy\u00b7re\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.40": {"text": "Die ausstudirte Worte spricht,", "tokens": ["Die", "aus\u00b7stu\u00b7dir\u00b7te", "Wor\u00b7te", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}