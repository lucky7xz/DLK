{"textgrid.poem.46085": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "22. November 1835", "genre": "verse", "period": "N.A.", "pub_year": 1835, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jesus, wie s\u00fc\u00df, wer dein gedenkt,", "tokens": ["Je\u00b7sus", ",", "wie", "s\u00fc\u00df", ",", "wer", "dein", "ge\u00b7denkt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "$,", "PWS", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Selig sein Herz in dich versenkt,", "tokens": ["Se\u00b7lig", "sein", "Herz", "in", "dich", "ver\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Doch s\u00fc\u00dfer noch als alles ist,", "tokens": ["Doch", "s\u00fc\u00b7\u00dfer", "noch", "als", "al\u00b7les", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "KOUS", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn du, o Jesu, bei mir bist.", "tokens": ["Wenn", "du", ",", "o", "Je\u00b7su", ",", "bei", "mir", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "FM", "NE", "$,", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00f6her ist kein Gedankenflug,", "tokens": ["H\u00f6\u00b7her", "ist", "kein", "Ge\u00b7dan\u00b7ken\u00b7flug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Lieblicher kein' Gesanges Zug,", "tokens": ["Lieb\u00b7li\u00b7cher", "kein'", "Ge\u00b7san\u00b7ges", "Zug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Nimmer so s\u00fc\u00df klingt Liedes Ton,", "tokens": ["Nim\u00b7mer", "so", "s\u00fc\u00df", "klingt", "Lie\u00b7des", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Als, Jesus Christus Gottes Sohn.", "tokens": ["Als", ",", "Je\u00b7sus", "Chris\u00b7tus", "Got\u00b7tes", "Sohn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "NE", "NE", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Brot, das die Reue in Tr\u00e4nen baut,", "tokens": ["Brot", ",", "das", "die", "Reu\u00b7e", "in", "Tr\u00e4\u00b7nen", "baut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wein, der dem Durste vom Felsen taut,", "tokens": ["Wein", ",", "der", "dem", "Durs\u00b7te", "vom", "Fel\u00b7sen", "taut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Suchender Liebe des Hirten Laut", "tokens": ["Su\u00b7chen\u00b7der", "Lie\u00b7be", "des", "Hir\u00b7ten", "Laut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "APPR"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Selig, die findet, selig! die Braut.", "tokens": ["Se\u00b7lig", ",", "die", "fin\u00b7det", ",", "se\u00b7lig", "!", "die", "Braut", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VVFIN", "$,", "ADJD", "$.", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Sonne der Seelen, so warm, so hell,", "tokens": ["Son\u00b7ne", "der", "See\u00b7len", ",", "so", "warm", ",", "so", "hell", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wonne der Herzen, du Lebensquell,", "tokens": ["Won\u00b7ne", "der", "Her\u00b7zen", ",", "du", "Le\u00b7bens\u00b7quell", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Labung, die \u00fcber die Ufer schwillt,", "tokens": ["La\u00b7bung", ",", "die", "\u00fc\u00b7ber", "die", "U\u00b7fer", "schwillt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Alle Begierde mit S\u00e4ttigung f\u00fcllt.", "tokens": ["Al\u00b7le", "Be\u00b7gier\u00b7de", "mit", "S\u00e4t\u00b7ti\u00b7gung", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo ist die Zunge, die Schrift, die vermag,", "tokens": ["Wo", "ist", "die", "Zun\u00b7ge", ",", "die", "Schrift", ",", "die", "ver\u00b7mag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df sie die F\u00fclle der Liebe mir sag',", "tokens": ["Da\u00df", "sie", "die", "F\u00fcl\u00b7le", "der", "Lie\u00b7be", "mir", "sag'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nur wer's erfahren, der glaubet allein,", "tokens": ["Nur", "wer's", "er\u00b7fah\u00b7ren", ",", "der", "glau\u00b7bet", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVINF", "$,", "PRELS", "VVFIN", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie s\u00fc\u00df die Liebe zu Jesu kann sein!", "tokens": ["Wie", "s\u00fc\u00df", "die", "Lie\u00b7be", "zu", "Je\u00b7su", "kann", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "NE", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Bin ich allein, allein, allein!", "tokens": ["Bin", "ich", "al\u00b7lein", ",", "al\u00b7lein", ",", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "ADV", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schlie\u00df' ich des Herzens K\u00e4mmerlein fein,", "tokens": ["Schlie\u00df'", "ich", "des", "Her\u00b7zens", "K\u00e4m\u00b7mer\u00b7lein", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Dort sei sein Bettchen so still so rein,", "tokens": ["Dort", "sei", "sein", "Bett\u00b7chen", "so", "still", "so", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dort ist er mein \u2013 ach w\u00e4r' ich auch sein!", "tokens": ["Dort", "ist", "er", "mein", "\u2013", "ach", "w\u00e4r'", "ich", "auch", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "$(", "XY", "VAFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Dort bei der Herde, dort am Altar", "tokens": ["Dort", "bei", "der", "Her\u00b7de", ",", "dort", "am", "Al\u00b7tar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPRART", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Such' ich den Hirten, der Opferlamm war", "tokens": ["Such'", "ich", "den", "Hir\u00b7ten", ",", "der", "Op\u00b7fer\u00b7lamm", "war"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "VAFIN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Such' ich sehns\u00fcchtig, geheim, offenbar", "tokens": ["Such'", "ich", "sehn\u00b7s\u00fcch\u00b7tig", ",", "ge\u00b7heim", ",", "of\u00b7fen\u00b7bar"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJD", "$,", "ADJD"], "meter": "+-++-+-+-+", "measure": "zehnsilber"}, "line.4": {"text": "Jesus, nur Jesus! Ach w\u00e4re es wahr!", "tokens": ["Je\u00b7sus", ",", "nur", "Je\u00b7sus", "!", "Ach", "w\u00e4\u00b7re", "es", "wahr", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "NE", "$.", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Mit Magdalena, grauet der Tag", "tokens": ["Mit", "Mag\u00b7da\u00b7le\u00b7na", ",", "grau\u00b7et", "der", "Tag"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "VVFIN", "ART", "NN"], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wandl' ich zum Grab, wo Jesus lag,", "tokens": ["Wandl'", "ich", "zum", "Grab", ",", "wo", "Je\u00b7sus", "lag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "$,", "PWAV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Herz wer w\u00e4lzt von dir den Stein,", "tokens": ["O", "Herz", "wer", "w\u00e4lzt", "von", "dir", "den", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PWS", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Herz wann kehrt er bei dir ein.", "tokens": ["O", "Herz", "wann", "kehrt", "er", "bei", "dir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PWAV", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Herr bleib' bei mir, der Tag sich neigt", "tokens": ["Herr", "bleib'", "bei", "mir", ",", "der", "Tag", "sich", "neigt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$,", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deinem Licht mein Herz erleucht'", "tokens": ["Mit", "dei\u00b7nem", "Licht", "mein", "Herz", "er\u00b7leucht'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bleib' bei dem Kind in banger Nacht", "tokens": ["Bleib'", "bei", "dem", "Kind", "in", "ban\u00b7ger", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis es zu deinem Licht erwacht.", "tokens": ["Bis", "es", "zu", "dei\u00b7nem", "Licht", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Mein Herz vom Licht des Herrn besucht", "tokens": ["Mein", "Herz", "vom", "Licht", "des", "Herrn", "be\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bl\u00fchet und bringt der Wahrheit Frucht", "tokens": ["Bl\u00fc\u00b7het", "und", "bringt", "der", "Wahr\u00b7heit", "Frucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Gl\u00fche in heiliger Liebe Zucht,", "tokens": ["Gl\u00fc\u00b7he", "in", "hei\u00b7li\u00b7ger", "Lie\u00b7be", "Zucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Dann stirbt die Welt, von ihm verflucht!", "tokens": ["Dann", "stirbt", "die", "Welt", ",", "von", "ihm", "ver\u00b7flucht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Jesu, wie ist dein Kleid so rot", "tokens": ["Je\u00b7su", ",", "wie", "ist", "dein", "Kleid", "so", "rot"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast mich geliebet bis zum Tod!", "tokens": ["Hast", "mich", "ge\u00b7lie\u00b7bet", "bis", "zum", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Blut f\u00fcr mich gieng ins Gericht,", "tokens": ["Dein", "Blut", "f\u00fcr", "mich", "gieng", "ins", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "So schau' ich Gottes Angesicht.", "tokens": ["So", "schau'", "ich", "Got\u00b7tes", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Jesu, wie ist dein Lieben reich", "tokens": ["Je\u00b7su", ",", "wie", "ist", "dein", "Lie\u00b7ben", "reich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PPOSAT", "ADJA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Liebe, o werd' der Liebe gleich,", "tokens": ["Lie\u00b7be", ",", "o", "werd'", "der", "Lie\u00b7be", "gleich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Jesu, du gabst dich hin f\u00fcr mich,", "tokens": ["Je\u00b7su", ",", "du", "gabst", "dich", "hin", "f\u00fcr", "mich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nimm mich, o Herr, noch zaudre ich.", "tokens": ["Nimm", "mich", ",", "o", "Herr", ",", "noch", "zaud\u00b7re", "ich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "FM", "NN", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Mit deiner Lieb' berausche mich,", "tokens": ["Mit", "dei\u00b7ner", "Lieb'", "be\u00b7rau\u00b7sche", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht mehr der Welt dann lausche ich,", "tokens": ["Nicht", "mehr", "der", "Welt", "dann", "lau\u00b7sche", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jesu, dein Kreuz schwer auf dir lag,", "tokens": ["Je\u00b7su", ",", "dein", "Kreuz", "schwer", "auf", "dir", "lag", ","], "token_info": ["word", "punct", "word"