{"textgrid.poem.67508": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Tochter Jairus'", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schaut den himmlischen Arzt! mit stillem Blicke ber\u00fchrt er", "tokens": ["Schaut", "den", "himm\u00b7li\u00b7schen", "Arzt", "!", "mit", "stil\u00b7lem", "Bli\u00b7cke", "be\u00b7r\u00fchrt", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Die erstorbene Hand, richtend die Tochter empor", "tokens": ["Die", "ers\u00b7tor\u00b7be\u00b7ne", "Hand", ",", "rich\u00b7tend", "die", "Toch\u00b7ter", "em\u00b7por"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "ART", "NN", "PTKVZ"], "meter": "-+---+---+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "In das Leben. Es weint die Mutter. Mutter, o weine", "tokens": ["In", "das", "Le\u00b7ben", ".", "Es", "weint", "die", "Mut\u00b7ter", ".", "Mut\u00b7ter", ",", "o", "wei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$.", "NN", "$,", "FM", "ADJA"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Freudenthr\u00e4nen! es lebt wieder Dein z\u00e4rtliches Kind!", "tokens": ["Freu\u00b7dent\u00b7hr\u00e4\u00b7nen", "!", "es", "lebt", "wie\u00b7der", "Dein", "z\u00e4rt\u00b7li\u00b7ches", "Kind", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Alles freut sich; nur da steht der Eine, der Doctor, sinnend;", "tokens": ["Al\u00b7les", "freut", "sich", ";", "nur", "da", "steht", "der", "Ei\u00b7ne", ",", "der", "Doc\u00b7tor", ",", "sin\u00b7nend", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "$.", "ADV", "ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Seht, er sinnet die Kunst, Todte zu wecken, sich aus.", "tokens": ["Seht", ",", "er", "sin\u00b7net", "die", "Kunst", ",", "Tod\u00b7te", "zu", "we\u00b7cken", ",", "sich", "aus", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "NN", "PTKZU", "VVINF", "$,", "PRF", "PTKVZ", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Schaut den himmlischen Arzt! mit stillem Blicke ber\u00fchrt er", "tokens": ["Schaut", "den", "himm\u00b7li\u00b7schen", "Arzt", "!", "mit", "stil\u00b7lem", "Bli\u00b7cke", "be\u00b7r\u00fchrt", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Die erstorbene Hand, richtend die Tochter empor", "tokens": ["Die", "ers\u00b7tor\u00b7be\u00b7ne", "Hand", ",", "rich\u00b7tend", "die", "Toch\u00b7ter", "em\u00b7por"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "ART", "NN", "PTKVZ"], "meter": "-+---+---+--+", "measure": "iamb