{"textgrid.poem.52187": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Thibaut von Champagne", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbheim bin ich aus dem Morgenland an Seel und Leib gesund,", "tokens": ["\u00bb", "heim", "bin", "ich", "aus", "dem", "Mor\u00b7gen\u00b7land", "an", "Seel", "und", "Leib", "ge\u00b7sund", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "VAFIN", "PPER", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Mich durstet' in der W\u00fcste Sand nach Euerm frischen Mund,", "tokens": ["Mich", "durs\u00b7tet'", "in", "der", "W\u00fcs\u00b7te", "Sand", "nach", "Eu\u00b7erm", "fri\u00b7schen", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ihr bliebet mir ein treues Weib, da steht mein Glaube fest,", "tokens": ["Ihr", "blie\u00b7bet", "mir", "ein", "treu\u00b7es", "Weib", ",", "da", "steht", "mein", "Glau\u00b7be", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Drum bring ich Euch das Sch\u00f6nste mit, was sich bescheren l\u00e4\u00dft.\u00ab", "tokens": ["Drum", "bring", "ich", "Euch", "das", "Sch\u00f6ns\u00b7te", "mit", ",", "was", "sich", "be\u00b7sche\u00b7ren", "l\u00e4\u00dft", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ART", "NN", "PTKVZ", "$,", "PRELS", "PRF", "VVINF", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Die Gr\u00e4fin wandelt auf und ab in einem sachten Schritt.", "tokens": ["Die", "Gr\u00e4\u00b7fin", "wan\u00b7delt", "auf", "und", "ab", "in", "ei\u00b7nem", "sach\u00b7ten", "Schritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Sie las den Brief und las den Brief. \u00bbWas bringt der Graf mir mit?", "tokens": ["Sie", "las", "den", "Brief", "und", "las", "den", "Brief", ".", "\u00bb", "Was", "bringt", "der", "Graf", "mir", "mit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$.", "$(", "PWS", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ist's wohl ein Span vom echten Kreuz? Den k\u00fc\u00dft ich voller Scheu!", "tokens": ["Ist's", "wohl", "ein", "Span", "vom", "ech\u00b7ten", "Kreuz", "?", "Den", "k\u00fc\u00dft", "ich", "vol\u00b7ler", "Scheu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "$.", "NE", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Ist's in dem Zwinger ein Getier? Ein Pardel oder Leu?", "tokens": ["Ist's", "in", "dem", "Zwin\u00b7ger", "ein", "Ge\u00b7tier", "?", "Ein", "Par\u00b7del", "o\u00b7der", "Leu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ART", "NN", "$.", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Ist's d\u00fcnnen Schleiers Spinneweb, das Werk der Feienhand?", "tokens": ["Ist's", "d\u00fcn\u00b7nen", "Schlei\u00b7ers", "Spin\u00b7ne\u00b7web", ",", "das", "Werk", "der", "Fei\u00b7en\u00b7hand", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Ein Perserteppich, wie der Fu\u00df noch keinen weichern fand?", "tokens": ["Ein", "Per\u00b7ser\u00b7tep\u00b7pich", ",", "wie", "der", "Fu\u00df", "noch", "kei\u00b7nen", "wei\u00b7chern", "fand", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "ADV", "PIAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ist's denn ein lichter Edelstein? Ist's ein Geschirr von Gold,", "tokens": ["Ist's", "denn", "ein", "lich\u00b7ter", "E\u00b7del\u00b7stein", "?", "Ist's", "ein", "Ge\u00b7schirr", "von", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "ADJA", "NN", "$.", "NE", "ART", "NN", "APPR", "NN", "$,"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Daraus sich feiner Rauch empor in blauen W\u00f6lklein rollt?\u00ab", "tokens": ["Da\u00b7raus", "sich", "fei\u00b7ner", "Rauch", "em\u00b7por", "in", "blau\u00b7en", "W\u00f6lk\u00b7lein", "rollt", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "PRF", "ADJA", "NN", "PTKVZ", "APPR", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Der T\u00fcrmer ruft. Das Tor erf\u00fcllt der freud'ge Pilgerzug:", "tokens": ["Der", "T\u00fcr\u00b7mer", "ruft", ".", "Das", "Tor", "er\u00b7f\u00fcllt", "der", "freu\u00b7d'\u00b7ge", "Pil\u00b7ger\u00b7zug", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Barhaupt der Graf in seinem Helm wohl hundert Rosen trug,", "tokens": ["Bar\u00b7haupt", "der", "Graf", "in", "sei\u00b7nem", "Helm", "wohl", "hun\u00b7dert", "Ro\u00b7sen", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN", "ADV", "CARD", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.3": {"text": "Auf manchem Wagen schwankte dann manch t\u00f6nernes Geschirr,", "tokens": ["Auf", "man\u00b7chem", "Wa\u00b7gen", "schwank\u00b7te", "dann", "manch", "t\u00f6\u00b7ner\u00b7nes", "Ge\u00b7schirr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Dar\u00fcber bl\u00fcht' ein Rosenhain in w\u00fcrzigem Gewirr.", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "bl\u00fcht'", "ein", "Ro\u00b7sen\u00b7hain", "in", "w\u00fcr\u00b7zi\u00b7gem", "Ge\u00b7wirr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Der Gr\u00e4fin N\u00e4schen sog den Duft, das M\u00fcndchen zeigt' Verdru\u00df,", "tokens": ["Der", "Gr\u00e4\u00b7fin", "N\u00e4\u00b7schen", "sog", "den", "Duft", ",", "das", "M\u00fcnd\u00b7chen", "zeigt'", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Dann l\u00e4chelt's zu dem leichten Hort und bietet sich dem Ku\u00df \u2013", "tokens": ["Dann", "l\u00e4\u00b7chelt's", "zu", "dem", "leich\u00b7ten", "Hort", "und", "bie\u00b7tet", "sich", "dem", "Ku\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "KON", "VVFIN", "PRF", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "\u00bbwie selig bin ich, liebe Frau, da\u00df Euch der Flor gef\u00e4llt!", "tokens": ["\u00bb", "wie", "se\u00b7lig", "bin", "ich", ",", "lie\u00b7be", "Frau", ",", "da\u00df", "Euch", "der", "Flor", "ge\u00b7f\u00e4llt", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJD", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Die Rosen von Damaskus sind die vollsten auf der Welt!", "tokens": ["Die", "Ro\u00b7sen", "von", "Da\u00b7mas\u00b7kus", "sind", "die", "volls\u00b7ten", "auf", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VAFIN", "ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.6": {"line.1": {"text": "In hundert K\u00fcbeln schleppten wir den Rosenwald an Bord,", "tokens": ["In", "hun\u00b7dert", "K\u00fc\u00b7beln", "schlepp\u00b7ten", "wir", "den", "Ro\u00b7sen\u00b7wald", "an", "Bord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Er w\u00e4r mir in der Sonnenglut verdorben und verdorrt,", "tokens": ["Er", "w\u00e4r", "mir", "in", "der", "Son\u00b7nen\u00b7glut", "ver\u00b7dor\u00b7ben", "und", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Neun Tage st\u00fcrzte Regengu\u00df, der schier das Schiff versenkt \u2013", "tokens": ["Neun", "Ta\u00b7ge", "st\u00fcrz\u00b7te", "Re\u00b7gen\u00b7gu\u00df", ",", "der", "schier", "das", "Schiff", "ver\u00b7senkt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Ich dachte nur, ich lachte nur: Wie ", "tokens": ["Ich", "dach\u00b7te", "nur", ",", "ich", "lach\u00b7te", "nur", ":", "Wie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADV", "$.", "PWAV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Entpanzert, Knappen, mir die Brust, noch bin ich erzumschient!", "tokens": ["Ent\u00b7pan\u00b7zert", ",", "Knap\u00b7pen", ",", "mir", "die", "Brust", ",", "noch", "bin", "ich", "er\u00b7zum\u00b7schient", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPER", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Ich habe meinen Himmel hier und einen dort verdient!", "tokens": ["Ich", "ha\u00b7be", "mei\u00b7nen", "Him\u00b7mel", "hier", "und", "ei\u00b7nen", "dort", "ver\u00b7dient", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "KON", "ART", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Mit Rosen will ich drum zu Tisch, mit Rosen schlummern gehn,", "tokens": ["Mit", "Ro\u00b7sen", "will", "ich", "drum", "zu", "Tisch", ",", "mit", "Ro\u00b7sen", "schlum\u00b7mern", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PAV", "APPR", "NN", "$,", "APPR", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Mit Rosen steigen in die Gruft, mit Rosen auferstehn!\u00ab", "tokens": ["Mit", "Ro\u00b7sen", "stei\u00b7gen", "in", "die", "Gruft", ",", "mit", "Ro\u00b7sen", "auf\u00b7er\u00b7stehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "APPR", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.8": {"line.1": {"text": "\u00bbheim bin ich aus dem Morgenland an Seel un