{"dta.poem.21721": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Idens Nachtgesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vernimm es, Nacht, was Ida dir vertrauet,", "tokens": ["Ver\u00b7nimm", "es", ",", "Nacht", ",", "was", "I\u00b7da", "dir", "ver\u00b7trau\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "PRELS", "NE", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die, satt des Tags, in deine Arme flieht.", "tokens": ["Die", ",", "satt", "des", "Tags", ",", "in", "dei\u00b7ne", "Ar\u00b7me", "flieht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUI", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Sterne, die ihr hold und liebend auf mich", "tokens": ["Ihr", "Ster\u00b7ne", ",", "die", "ihr", "hold", "und", "lie\u00b7bend", "auf", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "KON", "ADJD", "APPR", "PPER"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "schauet,", "tokens": ["schau\u00b7et", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Vernehmt s\u00fcsslauschend Idens Lied.", "tokens": ["Ver\u00b7nehmt", "s\u00fcss\u00b7lau\u00b7schend", "I\u00b7dens", "Lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Den ich geahnt in liebevollen Stunden,", "tokens": ["Den", "ich", "ge\u00b7ahnt", "in", "lie\u00b7be\u00b7vol\u00b7len", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem sehnsuchtkrank mein Herz entgegenschlug,", "tokens": ["Dem", "sehn\u00b7sucht\u00b7krank", "mein", "Herz", "ent\u00b7ge\u00b7gen\u00b7schlug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O Nacht, o Sterne, h\u00f6rts, ich habe ihn gefunden,", "tokens": ["O", "Nacht", ",", "o", "Ster\u00b7ne", ",", "h\u00f6rts", ",", "ich", "ha\u00b7be", "ihn", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,", "ADV", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dess Bild ich l\u00e4ngst im Busen trug.", "tokens": ["Dess", "Bild", "ich", "l\u00e4ngst", "im", "Bu\u00b7sen", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Um seine Wiege l\u00e4chelten die Musen,", "tokens": ["Um", "sei\u00b7ne", "Wie\u00b7ge", "l\u00e4\u00b7chel\u00b7ten", "die", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Urania kost' ihm auf dem keuschen Schooss,", "tokens": ["U\u00b7ra\u00b7nia", "kost'", "ihm", "auf", "dem", "keu\u00b7schen", "Scho\u00b7oss", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die Sch\u00f6nheit tr\u00e4nket' ihn an ihrem Nektarbusen,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "tr\u00e4n\u00b7ket'", "ihn", "an", "ih\u00b7rem", "Nekt\u00b7ar\u00b7bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und jede Charis zog ihn gross.", "tokens": ["Und", "je\u00b7de", "Cha\u00b7ris", "zog", "ihn", "gross", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "In seinen Augen blitzt prometisch Feuer.", "tokens": ["In", "sei\u00b7nen", "Au\u00b7gen", "blitzt", "pro\u00b7me\u00b7tisch", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gerecht entbrennt sein Herz in Lieb und Zorn.", "tokens": ["Ge\u00b7recht", "ent\u00b7brennt", "sein", "Herz", "in", "Lieb", "und", "Zorn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es l\u00fcpft dem Schmachtenden die Wahrheit ihren", "tokens": ["Es", "l\u00fcpft", "dem", "Schmach\u00b7ten\u00b7den", "die", "Wahr\u00b7heit", "ih\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schleyer;", "tokens": ["Schle\u00b7yer", ";"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Ihm sprudelt Ph\u00f6bus heil'ger Born.", "tokens": ["Ihm", "spru\u00b7delt", "Ph\u00f6\u00b7bus", "heil'\u00b7ger", "Born", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Freund, du bist mein, nicht f\u00fcr die kurze Reise,", "tokens": ["Freund", ",", "du", "bist", "mein", ",", "nicht", "f\u00fcr", "die", "kur\u00b7ze", "Rei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPOSAT", "$,", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die durch das Labyrinth des Lebens f\u00fchrt;", "tokens": ["Die", "durch", "das", "La\u00b7by\u00b7rinth", "des", "Le\u00b7bens", "f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sieh, sieh die Sph\u00e4ren dort, die ewig sch\u00f6nen", "tokens": ["Sieh", ",", "sieh", "die", "Sph\u00e4\u00b7ren", "dort", ",", "die", "e\u00b7wig", "sch\u00f6\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVIMP", "ART", "NN", "ADV", "$,", "PRELS", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kreise,", "tokens": ["Krei\u00b7se", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Wo fester unser Band sich schn\u00fcrt.", "tokens": ["Wo", "fes\u00b7ter", "un\u00b7ser", "Band", "sich", "schn\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Freund, ich bin dein, nicht f\u00fcr den Sand der", "tokens": ["Freund", ",", "ich", "bin", "dein", ",", "nicht", "f\u00fcr", "den", "Sand", "der"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPOSAT", "$,", "PTKNEG", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zeiten,", "tokens": ["Zei\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der schnellversiegend Chronos Uhr entfleusst;", "tokens": ["Der", "schnell\u00b7ver\u00b7sie\u00b7gend", "Chro\u00b7nos", "Uhr", "ent\u00b7fleusst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein f\u00fcr den Riesenstrom heilvoller Ewigkeiten,", "tokens": ["Dein", "f\u00fcr", "den", "Rie\u00b7sens\u00b7trom", "heil\u00b7vol\u00b7ler", "E\u00b7wig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Der aus des Ewgen Urne scheusst.", "tokens": ["Der", "aus", "des", "Ew\u00b7gen", "Ur\u00b7ne", "scheusst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}