{"dta.poem.20878": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Es lie\u00df sich unser Geist durch die Gedancken binden/", "tokens": ["Es", "lie\u00df", "sich", "un\u00b7ser", "Geist", "durch", "die", "Ge\u00b7dan\u00b7cken", "bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und konten nirgends recht der Warheit Sitz ergr\u00fcnden/", "tokens": ["Und", "kon\u00b7ten", "nir\u00b7gends", "recht", "der", "War\u00b7heit", "Sitz", "er\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir waren gantz best\u00fcrtzt/ es wanckte Muth und Fu\u00df/", "tokens": ["Wir", "wa\u00b7ren", "gantz", "be\u00b7st\u00fcrtzt", "/", "es", "wanck\u00b7te", "Muth", "und", "Fu\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$(", "PPER", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir sahen einen Satz den andern \u00fcberwinden/", "tokens": ["Wir", "sa\u00b7hen", "ei\u00b7nen", "Satz", "den", "an\u00b7dern", "\u00fc\u00b7berw\u00b7in\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und lenckten allbereit auf diesen letzten Schlu\u00df/", "tokens": ["Und", "lenck\u00b7ten", "all\u00b7be\u00b7reit", "auf", "die\u00b7sen", "letz\u00b7ten", "Schlu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch konten wir uns auch nicht recht vergn\u00fcget finden.", "tokens": ["Doch", "kon\u00b7ten", "wir", "uns", "auch", "nicht", "recht", "ver\u00b7gn\u00fc\u00b7get", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "PTKNEG", "ADJD", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Meinung Socrates war uns so hell und klar/", "tokens": ["Die", "Mei\u00b7nung", "So\u00b7cra\u00b7tes", "war", "uns", "so", "hell", "und", "klar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Da\u00df unser gantzer Geist damit zu frieden war/", "tokens": ["Da\u00df", "un\u00b7ser", "gant\u00b7zer", "Geist", "da\u00b7mit", "zu", "frie\u00b7den", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PAV", "APPR", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und itzt begunte er fast wiederum zu wancken;", "tokens": ["Und", "itzt", "be\u00b7gun\u00b7te", "er", "fast", "wie\u00b7de\u00b7rum", "zu", "wan\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dann weil der letzte Streit uns aus der Bahn gebracht/", "tokens": ["Dann", "weil", "der", "letz\u00b7te", "Streit", "uns", "aus", "der", "Bahn", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So hielten wir das Werck vor irrige Gedancken/", "tokens": ["So", "hiel\u00b7ten", "wir", "das", "Werck", "vor", "ir\u00b7ri\u00b7ge", "Ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und unser Urthel gantz begraben in der Nacht.", "tokens": ["Und", "un\u00b7ser", "Ur\u00b7thel", "gantz", "be\u00b7gra\u00b7ben", "in", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}