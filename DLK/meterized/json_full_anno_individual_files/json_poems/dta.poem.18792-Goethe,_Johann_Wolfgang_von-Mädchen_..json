{"dta.poem.18792": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4dchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Merke wohl, du hast uns eine", "tokens": ["Mer\u00b7ke", "wohl", ",", "du", "hast", "uns", "ei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "PPER", "VAFIN", "PPER", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jener Huris vorgeheuchelt!", "tokens": ["Je\u00b7ner", "Hu\u00b7ris", "vor\u00b7ge\u00b7heu\u00b7chelt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag schon seyn, wenn es nur keine", "tokens": ["Mag", "schon", "seyn", ",", "wenn", "es", "nur", "kei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "VAINF", "$,", "KOUS", "PPER", "ADV", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich auf dieser Erde schmeichelt.", "tokens": ["Sich", "auf", "die\u00b7ser", "Er\u00b7de", "schmei\u00b7chelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}