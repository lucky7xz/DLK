{"textgrid.poem.67309": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Flora und die Blumen", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbkinderchen des holden, s\u00fc\u00dfen Fr\u00fchlings,", "tokens": ["\u00bb", "kin\u00b7der\u00b7chen", "des", "hol\u00b7den", ",", "s\u00fc\u00b7\u00dfen", "Fr\u00fch\u00b7lings", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "H\u00f6rt, o h\u00f6rt der Mutter treue Warnung:", "tokens": ["H\u00f6rt", ",", "o", "h\u00f6rt", "der", "Mut\u00b7ter", "treu\u00b7e", "War\u00b7nung", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn ein lauer Winterwest Euch heuchelt,", "tokens": ["Wenn", "ein", "lau\u00b7er", "Win\u00b7ter\u00b7west", "Euch", "heu\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Trauet nicht dem heuchelnd-b\u00f6sen M\u00f6rder!", "tokens": ["Trau\u00b7et", "nicht", "dem", "heu\u00b7cheln\u00b7db\u00f6\u00b7sen", "M\u00f6r\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wartet, bis der goldne Vater rufet,", "tokens": ["War\u00b7tet", ",", "bis", "der", "gold\u00b7ne", "Va\u00b7ter", "ru\u00b7fet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bis die treue Mutter Euch erscheinet,", "tokens": ["Bis", "die", "treu\u00b7e", "Mut\u00b7ter", "Euch", "er\u00b7schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die Euch weckt aus Euren Winterbetten", "tokens": ["Die", "Euch", "weckt", "aus", "Eu\u00b7ren", "Win\u00b7ter\u00b7bet\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und Euch Kleider bringt und sch\u00f6ne H\u00e4ubchen.\u00ab", "tokens": ["Und", "Euch", "Klei\u00b7der", "bringt", "und", "sch\u00f6\u00b7ne", "H\u00e4ub\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "NN", "VVFIN", "KON", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Also sprach zu ihren Blumenkindern", "tokens": ["Al\u00b7so", "sprach", "zu", "ih\u00b7ren", "Blu\u00b7men\u00b7kin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Flora scheidend und ging auf zum Himmel.", "tokens": ["Flo\u00b7ra", "schei\u00b7dend", "und", "ging", "auf", "zum", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Alle Blumen sagten ihr Gehorsam", "tokens": ["Al\u00b7le", "Blu\u00b7men", "sag\u00b7ten", "ihr", "Ge\u00b7hor\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und Geduld zu, bis sie wiederk\u00e4me.", "tokens": ["Und", "Ge\u00b7duld", "zu", ",", "bis", "sie", "wie\u00b7der\u00b7k\u00e4\u00b7me", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Als sie kam, der goldne Vater Fr\u00fchling", "tokens": ["Als", "sie", "kam", ",", "der", "gold\u00b7ne", "Va\u00b7ter", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Rief die Kinder aus dem Winterschlafe,", "tokens": ["Rief", "die", "Kin\u00b7der", "aus", "dem", "Win\u00b7ter\u00b7schla\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Mutter brachte sch\u00f6ne Kleider,", "tokens": ["Und", "die", "Mut\u00b7ter", "brach\u00b7te", "sch\u00f6\u00b7ne", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lief umher und sucht' und z\u00e4hlet' alle.", "tokens": ["Lief", "um\u00b7her", "und", "sucht'", "und", "z\u00e4h\u00b7let'", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "KON", "VVFIN", "PIS", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ach, da fand sie manche sch\u00f6ne Knospe,", "tokens": ["Ach", ",", "da", "fand", "sie", "man\u00b7che", "sch\u00f6\u00b7ne", "Knos\u00b7pe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fr\u00fch hervorgelockt vom b\u00f6sen M\u00f6rder;", "tokens": ["Fr\u00fch", "her\u00b7vor\u00b7ge\u00b7lockt", "vom", "b\u00f6\u00b7sen", "M\u00f6r\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ausgetreten war sie aus der Zelle,", "tokens": ["Aus\u00b7ge\u00b7tre\u00b7ten", "war", "sie", "aus", "der", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hatt' hervorgeblickt mit ihren Aeuglein", "tokens": ["Hatt'", "her\u00b7vor\u00b7ge\u00b7blickt", "mit", "ih\u00b7ren", "A\u00b7e\u00b7u\u00b7glein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.6": {"line.1": {"text": "Und war bald erstarret, von des b\u00f6sen,", "tokens": ["Und", "war", "bald", "er\u00b7star\u00b7ret", ",", "von", "des", "b\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVFIN", "$,", "APPR", "ART", "ADJA", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Heuchelnden Verf\u00fchrers Hauch vergiftet;", "tokens": ["Heu\u00b7cheln\u00b7den", "Ver\u00b7f\u00fch\u00b7rers", "Hauch", "ver\u00b7gif\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Denn der Winterwest war Frost geworden,", "tokens": ["Denn", "der", "Win\u00b7ter\u00b7west", "war", "Frost", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "NN", "VAPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und erstarret stand das arme Bl\u00fcmchen.", "tokens": ["Und", "er\u00b7star\u00b7ret", "stand", "das", "ar\u00b7me", "Bl\u00fcm\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Traurig rief die Mutter ihrem Zephyr,", "tokens": ["Trau\u00b7rig", "rief", "die", "Mut\u00b7ter", "ih\u00b7rem", "Ze\u00b7phyr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der es brach; und sie begrub es traurig.", "tokens": ["Der", "es", "brach", ";", "und", "sie", "be\u00b7grub", "es", "trau\u00b7rig", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$.", "KON", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Seht, die ungeduldig-fr\u00fche Blume", "tokens": ["Seht", ",", "die", "un\u00b7ge\u00b7dul\u00b7dig\u00b7fr\u00fc\u00b7he", "Blu\u00b7me"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.