{"textgrid.poem.48849": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "74. An Filenen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Itzt, itzt bereu' ich erst, was ich verbrochen habe.", "tokens": ["Itzt", ",", "itzt", "be\u00b7reu'", "ich", "erst", ",", "was", "ich", "ver\u00b7bro\u00b7chen", "ha\u00b7be", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein wolgemeinter Rat, Filena, tauret mich.", "tokens": ["Dein", "wol\u00b7ge\u00b7mein\u00b7ter", "Rat", ",", "Fi\u00b7le\u00b7na", ",", "tau\u00b7ret", "mich", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "NE", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich schlug es in den Wind, wie sehr du m\u00fchtest dich,", "tokens": ["Ich", "schlug", "es", "in", "den", "Wind", ",", "wie", "sehr", "du", "m\u00fch\u00b7test", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PWAV", "ADV", "PPER", "VMFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df ich doch solte nicht so scheiden von dir abe.", "tokens": ["da\u00df", "ich", "doch", "sol\u00b7te", "nicht", "so", "schei\u00b7den", "von", "dir", "a\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "PTKNEG", "ADV", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Eia, da hab ich's nun, wormit ich mich so labe!", "tokens": ["Ei\u00b7a", ",", "da", "hab", "ich's", "nun", ",", "wor\u00b7mit", "ich", "mich", "so", "la\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VAFIN", "PIS", "ADV", "$,", "PWAV", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Kummer stellt sich ein, der Mangel findet sich,", "tokens": ["Der", "Kum\u00b7mer", "stellt", "sich", "ein", ",", "der", "Man\u00b7gel", "fin\u00b7det", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "es geht an Furcht und Not. Da steh' ich Armer, ich", "tokens": ["es", "geht", "an", "Furcht", "und", "Not", ".", "Da", "steh'", "ich", "Ar\u00b7mer", ",", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$.", "ADV", "VVFIN", "PPER", "NN", "$,", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und bin bei Leben auch schon halb in meinem Grabe.", "tokens": ["und", "bin", "bei", "Le\u00b7ben", "auch", "schon", "halb", "in", "mei\u00b7nem", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "ADV", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verzeih mir, teurer Schatz, da\u00df ich dich so verletzt.", "tokens": ["Ver\u00b7zeih", "mir", ",", "teu\u00b7rer", "Schatz", ",", "da\u00df", "ich", "dich", "so", "ver\u00b7letzt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADJA", "NN", "$,", "KOUS", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich selbsten habe mich in gr\u00f6\u00dftes Leid gesetzt.", "tokens": ["Ich", "selbs\u00b7ten", "ha\u00b7be", "mich", "in", "gr\u00f6\u00df\u00b7tes", "Leid", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und weil es ist an dem, da\u00df ich mich nur mu\u00df letzen", "tokens": ["Und", "weil", "es", "ist", "an", "dem", ",", "da\u00df", "ich", "mich", "nur", "mu\u00df", "let\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "APPR", "ART", "$,", "KOUS", "PPER", "PRF", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "mit dir durch diesen Brief, so bitt' ich, edler Schein,", "tokens": ["mit", "dir", "durch", "die\u00b7sen", "Brief", ",", "so", "bitt'", "ich", ",", "ed\u00b7ler", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PDAT", "NN", "$,", "ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "la\u00df mir di\u00df hauen tief an einen hohen Stein:", "tokens": ["la\u00df", "mir", "di\u00df", "hau\u00b7en", "tief", "an", "ei\u00b7nen", "ho\u00b7hen", "Stein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDS", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u00bbder eh starb, als er starb, der lie\u00df ihm dieses setzen.\u00ab", "tokens": ["\u00bb", "der", "eh", "starb", ",", "als", "er", "starb", ",", "der", "lie\u00df", "ihm", "die\u00b7ses", "set\u00b7zen", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "PDS", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Itzt, itzt bereu' ich erst, was ich verbrochen habe.", "tokens": ["Itzt", ",", "itzt", "be\u00b7reu'", "ich", "erst", ",", "was", "ich", "ver\u00b7bro\u00b7chen", "ha\u00b7be", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein wolgemeinter Rat, Filena, tauret mich.", "tokens": ["Dein", "wol\u00b7ge\u00b7mein\u00b7ter", "Rat", ",", "Fi\u00b7le\u00b7na", ",", "tau\u00b7ret", "mich", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "NE", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich schlug es in den Wind, wie sehr du m\u00fchtest dich,", "tokens": ["Ich", "schlug", "es", "in", "den", 