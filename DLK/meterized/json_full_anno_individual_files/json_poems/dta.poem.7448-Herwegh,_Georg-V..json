{"dta.poem.7448": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "V.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer Etwas auf dem Herzen hat, der eile,", "tokens": ["Wer", "Et\u00b7was", "auf", "dem", "Her\u00b7zen", "hat", ",", "der", "ei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "VAFIN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es noch bei Zeiten vor sein Volk zu bringen;", "tokens": ["Es", "noch", "bei", "Zei\u00b7ten", "vor", "sein", "Volk", "zu", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schon r\u00fchrt der Hader seine schwarzen Schwingen,", "tokens": ["Schon", "r\u00fchrt", "der", "Ha\u00b7der", "sei\u00b7ne", "schwar\u00b7zen", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schon liegt das Haubt des Friedens unterm Beile.", "tokens": ["Schon", "liegt", "das", "Haubt", "des", "Frie\u00b7dens", "un\u00b7term", "Bei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Henker harrt, da\u00df er's vom Rumpfe theile,", "tokens": ["Der", "Hen\u00b7ker", "harrt", ",", "da\u00df", "er's", "vom", "Rump\u00b7fe", "thei\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PIS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald wird der Blutstrahl in die L\u00fcfte dringen,", "tokens": ["Bald", "wird", "der", "Bluts\u00b7trahl", "in", "die", "L\u00fcf\u00b7te", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verharschte Wunden werden wieder springen,", "tokens": ["Ver\u00b7harschte", "Wun\u00b7den", "wer\u00b7den", "wie\u00b7der", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und fehlen wird der Arzt dann, der sie heile.", "tokens": ["Und", "feh\u00b7len", "wird", "der", "Arzt", "dann", ",", "der", "sie", "hei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Schon h\u00f6r' ich ferne die Kanonen brummen,", "tokens": ["Schon", "h\u00f6r'", "ich", "fer\u00b7ne", "die", "Ka\u00b7no\u00b7nen", "brum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die S\u00e4bel klirren und die Trommeln schallen,", "tokens": ["Die", "S\u00e4\u00b7bel", "klir\u00b7ren", "und", "die", "Trom\u00b7meln", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Vogel will im Wald sein Lied mehr summen.", "tokens": ["Kein", "Vo\u00b7gel", "will", "im", "Wald", "sein", "Lied", "mehr", "sum\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "APPRART", "NN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Noch eine Nacht \u2014 die W\u00fcrfel m\u00fcssen fallen;", "tokens": ["Noch", "ei\u00b7ne", "Nacht", "die", "W\u00fcr\u00b7fel", "m\u00fcs\u00b7sen", "fal\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$(", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-++-+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Dann gibt's ein tr\u00fcbes, trauriges Verstummen,", "tokens": ["Dann", "gibt's", "ein", "tr\u00fc\u00b7bes", ",", "trau\u00b7ri\u00b7ges", "Ver\u00b7stum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Hahnen Ruf verscheucht die Nachtigallen.", "tokens": ["Des", "Hah\u00b7nen", "Ruf", "ver\u00b7scheucht", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}