{"dta.poem.5779": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es st\u00f6sst sich alles stets und sucht sich zu versehren.", "tokens": ["Es", "st\u00f6sst", "sich", "al\u00b7les", "stets", "und", "sucht", "sich", "zu", "ver\u00b7seh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "ADV", "KON", "VVFIN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Zeit, durch eine stille Macht,", "tokens": ["Die", "Zeit", ",", "durch", "ei\u00b7ne", "stil\u00b7le", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zernagt den Stahl, der Marmor wird zerst\u00fccket,", "tokens": ["Zer\u00b7nagt", "den", "Stahl", ",", "der", "Mar\u00b7mor", "wird", "zer\u00b7st\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ohn\u2019 da\u00df es unser Aug\u2019 erblicket.", "tokens": ["Ohn'", "da\u00df", "es", "un\u00b7ser", "Aug'", "er\u00b7bli\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}