{"dta.poem.10674": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Au\u00df dem Latein Josephi Scaligeri.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Die Blumen zu dem Krantz/ den ich dir wollen senden/", "tokens": ["Die", "Blu\u00b7men", "zu", "dem", "Krantz", "/", "den", "ich", "dir", "wol\u00b7len", "sen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "PRELS", "PPER", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat Amor selbst/ mein Lieb/ gelesen in dem Feldt/", "tokens": ["Hat", "A\u00b7mor", "selbst", "/", "mein", "Lieb", "/", "ge\u00b7le\u00b7sen", "in", "dem", "Feldt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "$(", "PPOSAT", "NN", "$(", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Venus hat jhn auch gemacht mit jhren H\u00e4nden/", "tokens": ["Die", "Ve\u00b7nus", "hat", "jhn", "auch", "ge\u00b7macht", "mit", "jhren", "H\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Perlen/ Stein/ vnd Seyd/ ist au\u00df der newen Welt.", "tokens": ["Die", "Per\u00b7len", "/", "Stein", "/", "vnd", "Seyd", "/", "ist", "au\u00df", "der", "ne\u00b7wen", "Welt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "$(", "KON", "VAIMP", "$(", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-----+", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Ach dencke nicht/ da\u00df ich was bessers geben solte/", "tokens": ["Ach", "den\u00b7cke", "nicht", "/", "da\u00df", "ich", "was", "bes\u00b7sers", "ge\u00b7ben", "sol\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PTKNEG", "$(", "KOUS", "PPER", "PIS", "ADJA", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ob wol die dinge dir gantz vngemesse sein/", "tokens": ["Ob", "wol", "die", "din\u00b7ge", "dir", "gantz", "vn\u00b7ge\u00b7mes\u00b7se", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dann wann ich/ was dein Werth/ dir vbersenden wolte/", "tokens": ["Dann", "wann", "ich", "/", "was", "dein", "Werth", "/", "dir", "vber\u00b7sen\u00b7den", "wol\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPER", "$(", "PWS", "PPOSAT", "NN", "$(", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "So k\u00f6nt ich nichts verehrn/ als nur dich/ dir allein.", "tokens": ["So", "k\u00f6nt", "ich", "nichts", "ver\u00b7ehrn", "/", "als", "nur", "dich", "/", "dir", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "VVINF", "$(", "KOKOM", "ADV", "PPER", "$(", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}