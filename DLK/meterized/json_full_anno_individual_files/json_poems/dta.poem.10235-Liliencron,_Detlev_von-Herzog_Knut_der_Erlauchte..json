{"dta.poem.10235": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Herzog Knut der Erlauchte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Knuts Br\u00fcder lie\u00dfen die Hunde los,", "tokens": ["Knuts", "Br\u00fc\u00b7der", "lie\u00b7\u00dfen", "die", "Hun\u00b7de", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der B\u00fcrgerkrieg fiel \u00fcbergro\u00df", "tokens": ["Der", "B\u00fcr\u00b7ger\u00b7krieg", "fiel", "\u00fc\u00b7ber\u00b7gro\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bei Fodwig traf K\u00f6nig Magnus der Pfeil", "tokens": ["Bei", "Fod\u00b7wig", "traf", "K\u00f6\u00b7nig", "Mag\u00b7nus", "der", "Pfeil"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "NN", "NE", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "K\u00f6nig Niels hieb sich Bahn mit Schwert und Beil", "tokens": ["K\u00f6\u00b7nig", "Niels", "hieb", "sich", "Bahn", "mit", "Schwert", "und", "Beil"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "PRF", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Als in Schleswig am Ende die wilde Fahrt,", "tokens": ["Als", "in", "Schles\u00b7wig", "am", "En\u00b7de", "die", "wil\u00b7de", "Fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Sie spieen ihm auf den wei\u00dfen Bart,", "tokens": ["Sie", "spie\u00b7en", "ihm", "auf", "den", "wei\u00b7\u00dfen", "Bart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}