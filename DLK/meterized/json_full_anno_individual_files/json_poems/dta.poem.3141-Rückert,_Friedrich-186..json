{"dta.poem.3141": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "186.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu guter Nachbarschaft geh\u00f6rt nicht das allein,", "tokens": ["Zu", "gu\u00b7ter", "Nach\u00b7bar\u00b7schaft", "ge\u00b7h\u00f6rt", "nicht", "das", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PTKNEG", "PDS", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht weh zu thun, auch dem, der weh that, zu verzeihn.", "tokens": ["Nicht", "weh", "zu", "thun", ",", "auch", "dem", ",", "der", "weh", "that", ",", "zu", "ver\u00b7zeihn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PTKZU", "VVINF", "$,", "ADV", "ART", "$,", "PRELS", "ADV", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein b\u00f6ser Nachbar selbst mag nicht den guten plagen,", "tokens": ["Ein", "b\u00f6\u00b7ser", "Nach\u00b7bar", "selbst", "mag", "nicht", "den", "gu\u00b7ten", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VMFIN", "PTKNEG", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein guter aber wird den b\u00f6sen selbst ertragen.", "tokens": ["Ein", "gu\u00b7ter", "a\u00b7ber", "wird", "den", "b\u00f6\u00b7sen", "selbst", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["A