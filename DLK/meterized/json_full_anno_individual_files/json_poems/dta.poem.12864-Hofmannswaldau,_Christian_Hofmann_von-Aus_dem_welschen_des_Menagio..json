{"dta.poem.12864": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem welschen des   Menagio.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du fragest, sch\u00f6ne Magdalis!", "tokens": ["Du", "fra\u00b7gest", ",", "sch\u00f6\u00b7ne", "Mag\u00b7da\u00b7lis", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird deine glut auch lange brennen?", "tokens": ["Wird", "dei\u00b7ne", "glut", "auch", "lan\u00b7ge", "bren\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ach Schatz! wie kan ich das erkennen?", "tokens": ["Ach", "Schatz", "!", "wie", "kan", "ich", "das", "er\u00b7ken\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "PWAV", "VMFIN", "PPER", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die todes-stund\u2019 ist ungewi\u00df.", "tokens": ["Die", "to\u00b7des\u00b7stund'", "ist", "un\u00b7ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}