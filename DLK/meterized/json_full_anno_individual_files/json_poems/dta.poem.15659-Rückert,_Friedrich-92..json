{"dta.poem.15659": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "92.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die kleinen Vier, die, ungeahnet alten Weisen,", "tokens": ["Die", "klei\u00b7nen", "Vier", ",", "die", ",", "un\u00b7ge\u00b7ah\u00b7net", "al\u00b7ten", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Statt Eines, zwischen Mars und Jupiter nun kreisen;", "tokens": ["Statt", "Ei\u00b7nes", ",", "zwi\u00b7schen", "Mars", "und", "Ju\u00b7pi\u00b7ter", "nun", "krei\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "APPR", "NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie sind sie anzusehn? Ein Doppelzwillingspaar,", "tokens": ["Wie", "sind", "sie", "an\u00b7zu\u00b7sehn", "?", "Ein", "Dop\u00b7pel\u00b7zwil\u00b7lings\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVIZU", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Statt einfacher Geburt, in der Geschwister Schaar.", "tokens": ["Statt", "ein\u00b7fa\u00b7cher", "Ge\u00b7burt", ",", "in", "der", "Ge\u00b7schwis\u00b7ter", "Schaar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Selbst die Verschlungenheit von ihrer Bahn beweist,", "tokens": ["Selbst", "die", "Ver\u00b7schlun\u00b7gen\u00b7heit", "von", "ih\u00b7rer", "Bahn", "be\u00b7weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Da\u00df scheinbar vier sie sind Ein Leib mit Einem Geist.", "tokens": ["Da\u00df", "schein\u00b7bar", "vier", "sie", "sind", "Ein", "Leib", "mit", "Ei\u00b7nem", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "CARD", "PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}