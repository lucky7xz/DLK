{"textgrid.poem.48777": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2. Auf ihr Bildn\u00fc\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und darf ein frecher Kiel sich dieses unterfangen,", "tokens": ["Und", "darf", "ein", "fre\u00b7cher", "Kiel", "sich", "die\u00b7ses", "un\u00b7ter\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "PRF", "PDAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df er die ganze Zier, die an der Liebsten scheint,", "tokens": ["da\u00df", "er", "die", "gan\u00b7ze", "Zier", ",", "die", "an", "der", "Liebs\u00b7ten", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "in so enges Tun zu zeichnen ab vermeint?", "tokens": ["in", "so", "en\u00b7ges", "Tun", "zu", "zeich\u00b7nen", "ab", "ver\u00b7meint", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "PTKZU", "VVFIN", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Wahr ist es, dieses Haar, die Stirne, diese Wangen", "tokens": ["Wahr", "ist", "es", ",", "die\u00b7ses", "Haar", ",", "die", "Stir\u00b7ne", ",", "die\u00b7se", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "PDAT", "NN", "$,", "ART", "NN", "$,", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "sind denen \u00e4hnlich ganz, die an derselben prangen.", "tokens": ["sind", "de\u00b7nen", "\u00e4hn\u00b7lich", "ganz", ",", "die", "an", "der\u00b7sel\u00b7ben", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADJD", "ADV", "$,", "PRELS", "APPR", "PDAT", "VVFIN", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die Augen seh' ich da, um die ich oft geweint,", "tokens": ["Die", "Au\u00b7gen", "seh'", "ich", "da", ",", "um", "die", "ich", "oft", "ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "APPR", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und di\u00df hier ist der Mund, der meinen nennet Freund.", "tokens": ["und", "di\u00df", "hier", "ist", "der", "Mund", ",", "der", "mei\u00b7nen", "nen\u00b7net", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ganz di\u00df, das ist ganz das, nach dem ich mu\u00df verlangen.", "tokens": ["Ganz", "di\u00df", ",", "das", "ist", "ganz", "das", ",", "nach", "dem", "ich", "mu\u00df", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "$,", "PDS", "VAFIN", "ADV", "PDS", "$,", "APPR", "PRELS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Die Zucht, di\u00df freundlich Sehn, die Sitten, diese Tracht,", "tokens": ["Die", "Zucht", ",", "di\u00df", "freund\u00b7lich", "Sehn", ",", "die", "Sit\u00b7ten", ",", "die\u00b7se", "Tracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "ADJD", "VVFIN", "$,", "ART", "NN", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und Alles steht vor mir, was sie so treflich macht,", "tokens": ["und", "Al\u00b7les", "steht", "vor", "mir", ",", "was", "sie", "so", "tref\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PPER", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "nur da\u00df es sich nicht regt und nicht will Antwort geben.", "tokens": ["nur", "da\u00df", "es", "sich", "nicht", "regt", "und", "nicht", "will", "Ant\u00b7wort", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "KON", "PTKNEG", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sei drum nicht halb so stolz, du k\u00fchner Pinsel du,", "tokens": ["Sei", "drum", "nicht", "halb", "so", "stolz", ",", "du", "k\u00fch\u00b7ner", "Pin\u00b7sel", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "PTKNEG", "ADJD", "ADV", "ADJD", "$,", "PPER", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das Sch\u00f6nste, das man w\u0