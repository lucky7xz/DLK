{"textgrid.poem.67712": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An Denselben", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kann Euch in Jamben ungereimt und pr\u00e4chtig", "tokens": ["Kann", "Euch", "in", "Jam\u00b7ben", "un\u00b7ge\u00b7reimt", "und", "pr\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn heut sogar antworten nicht; ohnm\u00e4chtig", "tokens": ["Denn", "heut", "so\u00b7gar", "ant\u00b7wor\u00b7ten", "nicht", ";", "ohn\u00b7m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "ADV", "VVINF", "PTKNEG", "$.", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "M\u00f6g'n Knittelverse zur Antwort sein", "tokens": ["M\u00f6g'n", "Knit\u00b7tel\u00b7ver\u00b7se", "zur", "Ant\u00b7wort", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN", "PPOSAT"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und dem Heldens\u00e4nger wohl gedeihn!", "tokens": ["Und", "dem", "Hel\u00b7den\u00b7s\u00e4n\u00b7ger", "wohl", "ge\u00b7deihn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Zuerst kam Dein prosaisch Brieflein an.", "tokens": ["Zu\u00b7erst", "kam", "Dein", "pro\u00b7sa\u00b7isch", "Brie\u00b7flein", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verzeih, wenn Dir mein letzter Wetterhahn", "tokens": ["Ver\u00b7zeih", ",", "wenn", "Dir", "mein", "letz\u00b7ter", "Wet\u00b7ter\u00b7hahn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Hat etwas zu Leid gethan!", "tokens": ["Hat", "et\u00b7was", "zu", "Leid", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "NN", "VVPP", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "War nicht mein's Herzens Grund,", "tokens": ["War", "nicht", "mein's", "Her\u00b7zens", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "War nur ein' b\u00f6se Stund.", "tokens": ["War", "nur", "ein'", "b\u00f6\u00b7se", "Stund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Ich lieb' Dein' Seele, rein als Gold,", "tokens": ["Ich", "lieb'", "Dein'", "See\u00b7le", ",", "rein", "als", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und dau'rt mich, da\u00df Du Dich mit Briefen qu\u00e4len sollt,", "tokens": ["Und", "dau'rt", "mich", ",", "da\u00df", "Du", "Dich", "mit", "Brie\u00b7fen", "qu\u00e4\u00b7len", "sollt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die immer aufschnappen nach Himmelsluft", "tokens": ["Die", "im\u00b7mer", "auf\u00b7schnap\u00b7pen", "nach", "Him\u00b7mel\u00b7sluft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und riechen nicht des Freundes Duft.", "tokens": ["Und", "rie\u00b7chen", "nicht", "des", "Freun\u00b7des", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Woll'n uns einander helf'n und tragen", "tokens": ["Woll'n", "uns", "ein\u00b7an\u00b7der", "hel\u00b7f'n", "und", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "KON", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Und nicht die Seel' aus'm Leibe jagen.", "tokens": ["Und", "nicht", "die", "Seel'", "aus'm", "Lei\u00b7be", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Darinnen lag von M\u00e4gdleins Hand", "tokens": ["Da\u00b7rin\u00b7nen", "lag", "von", "M\u00e4gd\u00b7leins", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ein Brieflein hold und unbekannt,", "tokens": ["Ein", "Brie\u00b7flein", "hold", "und", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Kam mir wie Manna an zur Stund", "tokens": ["Kam", "mir", "wie", "Man\u00b7na", "an", "zur", "Stund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "KOKOM", "NE", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Aus der Aurora goldnem Mund.", "tokens": ["Aus", "der", "Au\u00b7ro\u00b7ra", "gold\u00b7nem", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ich streckt' meine H\u00e4nde hin zu ihr,", "tokens": ["Ich", "streckt'", "mei\u00b7ne", "H\u00e4n\u00b7de", "hin", "zu", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Wu\u00dft' nicht, zu wem, und war bei Dir.", "tokens": ["Wu\u00dft'", "nicht", ",", "zu", "wem", ",", "und", "war", "bei", "Dir", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "APPR", "PIS", "$,", "KON", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und leg' ein'n Gru\u00df in Deine Hand,", "tokens": ["Und", "leg'", "ein'n", "Gru\u00df", "in", "Dei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "So heilig, wie ins Engelsland", "tokens": ["So", "hei\u00b7lig", ",", "wie", "ins", "En\u00b7gels\u00b7land"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Einer lieben Verkl\u00e4rten zu tragen,", "tokens": ["Ei\u00b7ner", "lie\u00b7ben", "Ver\u00b7kl\u00e4r\u00b7ten", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Ihr Dank und Dank und Dank zu sagen,", "tokens": ["Ihr", "Dank", "und", "Dank", "und", "Dank", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Da\u00df sie in holder Unschuldtracht", "tokens": ["Da\u00df", "sie", "in", "hol\u00b7der", "Un\u00b7schuld\u00b7tracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Mich so \u2013 besch\u00e4mt gemacht.", "tokens": ["Mich", "so", "\u2013", "be\u00b7sch\u00e4mt", "ge\u00b7macht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Konnt' nie f\u00fcr solchen Engel schreiben", "tokens": ["Konnt'", "nie", "f\u00fcr", "sol\u00b7chen", "En\u00b7gel", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Und werd's auch lang' noch lassen bleiben,", "tokens": ["Und", "werd's", "auch", "lang'", "noch", "las\u00b7sen", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Ob denken an sie in sel'ger Stund", "tokens": ["Ob", "den\u00b7ken", "an", "sie", "in", "sel'\u00b7ger", "Stund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "An der Aurora Rosenmund,", "tokens": ["An", "der", "Au\u00b7ro\u00b7ra", "Ro\u00b7sen\u00b7mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Wenn ich sie noch einmal kann ansehn,", "tokens": ["Wenn", "ich", "sie", "noch", "ein\u00b7mal", "kann", "an\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "Und Gottes Gedanken in mir aufgehn,", "tokens": ["Und", "Got\u00b7tes", "Ge\u00b7dan\u00b7ken", "in", "mir", "auf\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.34": {"text": "Wie er sie schaffend \u2013 doch genung!", "tokens": ["Wie", "er", "sie", "schaf\u00b7fend", "\u2013", "doch", "ge\u00b7nung", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADJD", "$(", "ADV", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.35": {"text": "Ich schreib' ja L\u00e4sterung.", "tokens": ["Ich", "schreib'", "ja", "L\u00e4s\u00b7te\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.36": {"text": "Dein Versebrief ist gar nicht wahr;", "tokens": ["Dein", "Ver\u00b7se\u00b7brief", "ist", "gar", "nicht", "wahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.37": {"text": "Drum ist er auch ", "tokens": ["Drum", "ist", "er", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.38": {"text": "Und kam auf gro\u00dfen Stelzen schwer", "tokens": ["Und", "kam", "auf", "gro\u00b7\u00dfen", "Stel\u00b7zen", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Zum ", "tokens": ["Zum"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.40": {"text": "Der eben hinterm Kirchenthurm", "tokens": ["Der", "e\u00b7ben", "hin\u00b7term", "Kir\u00b7chen\u00b7thurm"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Sich f\u00fchlt' und wand, ein armer Wurm,", "tokens": ["Sich", "f\u00fchlt'", "und", "wand", ",", "ein", "ar\u00b7mer", "Wurm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "KON", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Ging nicht an ", "tokens": ["Ging", "nicht", "an"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PTKVZ"], "meter": "+-+", "measure": "trochaic.di"}, "line.43": {"text": "Die Ilm hinan! lag, Gott erbarm'!", "tokens": ["Die", "Ilm", "hi\u00b7nan", "!", "lag", ",", "Gott", "er\u00b7barm'", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "PTKVZ", "$.", "VVFIN", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Zehn Klafter tief im Erdenschoo\u00df", "tokens": ["Zehn", "Klaf\u00b7ter", "tief", "im", "Er\u00b7den\u00b7schoo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Und f\u00fchlt' keinen Quell, der von ihm flo\u00df,", "tokens": ["Und", "f\u00fchlt'", "kei\u00b7nen", "Quell", ",", "der", "von", "ihm", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.46": {"text": "F\u00fchlt' keinen Tropfen Labung, wie", "tokens": ["F\u00fchlt'", "kei\u00b7nen", "Trop\u00b7fen", "La\u00b7bung", ",", "wie"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$,", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Ihn 's Herz und nicht die Phantasie", "tokens": ["Ihn", "'s", "Herz", "und", "nicht", "die", "Phan\u00b7ta\u00b7sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPER", "NN", "KON", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.48": {"text": "Begehrt und doch nur im Wahntraum kennt,", "tokens": ["Be\u00b7gehrt", "und", "doch", "nur", "im", "Wahn\u00b7traum", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.49": {"text": "Wenn es nicht Christus' Name nennt.", "tokens": ["Wenn", "es", "nicht", "Chris\u00b7tus'", "Na\u00b7me", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Den wir denn freilich Alle nennen", "tokens": ["Den", "wir", "denn", "frei\u00b7lich", "Al\u00b7le", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Und sing'n und jubeln und doch nicht kennen,", "tokens": ["Und", "sing'n", "und", "ju\u00b7beln", "und", "doch", "nicht", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVINF", "KON", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.52": {"text": "Woll'n Teufel austreiben und Wunder thun", "tokens": ["Woll'n", "Teu\u00b7fel", "aus\u00b7trei\u00b7ben", "und", "Wun\u00b7der", "thun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVINF", "KON", "NN", "VVINF"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.53": {"text": "Und k\u00f6nnen noch in ihm nicht \u2013 ruhn.", "tokens": ["Und", "k\u00f6n\u00b7nen", "noch", "in", "ihm", "nicht", "\u2013", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "PPER", "PTKNEG", "$(", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Ja, ruhn, mein Freund, als wie ein Kind,", "tokens": ["Ja", ",", "ruhn", ",", "mein", "Freund", ",", "als", "wie", "ein", "Kind", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVINF", "$,", "PPOSAT", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Und warten, ob nicht, wohlgesinnt,", "tokens": ["Und", "war\u00b7ten", ",", "ob", "nicht", ",", "wohl\u00b7ge\u00b7sinnt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVINF", "$,", "KOUS", "PTKNEG", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Ueber aller Erdenv\u00e4ter Schaar", "tokens": ["Ue\u00b7ber", "al\u00b7ler", "Er\u00b7den\u00b7v\u00e4\u00b7ter", "Schaar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.57": {"text": "Uns unser Vater ganz und gar", "tokens": ["Uns", "un\u00b7ser", "Va\u00b7ter", "ganz", "und", "gar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Was gebe, was wir nicht \u2013 verstehn,", "tokens": ["Was", "ge\u00b7be", ",", "was", "wir", "nicht", "\u2013", "ver\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "PPER", "PTKNEG", "$(", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Weder mit Tichten noch Trachten sehn,", "tokens": ["We\u00b7der", "mit", "Tich\u00b7ten", "noch", "Trach\u00b7ten", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "NN", "VVINF", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.60": {"text": "Nicht erfliegen und nicht erzwingen,", "tokens": ["Nicht", "er\u00b7flie\u00b7gen", "und", "nicht", "er\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "KON", "PTKNEG", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.61": {"text": "Nicht ers