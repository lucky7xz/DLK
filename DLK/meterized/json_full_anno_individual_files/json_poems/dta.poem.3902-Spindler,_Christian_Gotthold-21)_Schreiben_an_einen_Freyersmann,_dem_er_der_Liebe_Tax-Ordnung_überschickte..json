{"dta.poem.3902": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "21) Schreiben an einen Freyersmann,  \n dem er der Liebe Tax-Ordnung \u00fcberschickte.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Da du itzt auf der Freyerey,", "tokens": ["Da", "du", "itzt", "auf", "der", "Frey\u00b7e\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So steht es mir auch billig frey,", "tokens": ["So", "steht", "es", "mir", "auch", "bil\u00b7lig", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir Rath und Regel mitzutheilen.", "tokens": ["Dir", "Rath", "und", "Re\u00b7gel", "mit\u00b7zu\u00b7thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nimm denn die wohlgemeinten Zeilen.", "tokens": ["Nimm", "denn", "die", "wohl\u00b7ge\u00b7mein\u00b7ten", "Zei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wei\u00df, mein Maa\u00df-Recht kommt nicht fehl,", "tokens": ["Ich", "wei\u00df", ",", "mein", "Maa\u00df\u00b7Recht", "kommt", "nicht", "fehl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hier hast du, Bruder, den Befehl,", "tokens": ["Hier", "hast", "du", ",", "Bru\u00b7der", ",", "den", "Be\u00b7fehl", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den nechst ", "tokens": ["Den", "nechst"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Und darnach, meyn ich, solst du leben.", "tokens": ["Und", "dar\u00b7nach", ",", "meyn", "ich", ",", "solst", "du", "le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "$,", "PPOSAT", "PPER", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So, wie er mir es \u00fcberschickt,", "tokens": ["So", ",", "wie", "er", "mir", "es", "\u00fc\u00b7bersc\u00b7hickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Hab ich es wieder einger\u00fcckt.", "tokens": ["Hab", "ich", "es", "wie\u00b7der", "ein\u00b7ge\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Denn dieses Ding klingt Ehrenfeste,", "tokens": ["Denn", "die\u00b7ses", "Ding", "klingt", "Eh\u00b7ren\u00b7fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Erlerne nun daraus das beste:", "tokens": ["Er\u00b7ler\u00b7ne", "nun", "da\u00b7raus", "das", "bes\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PAV", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}