{"dta.poem.19033": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "ArIA,  \n Welche nach der geschehenen Verlobung zu  \n Castell abgesungen wurde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Wie bist du so wunderbar grosser Regente,", "tokens": ["Wie", "bist", "du", "so", "wun\u00b7der\u00b7bar", "gros\u00b7ser", "Re\u00b7gen\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Himmel und Erde und alles bewegt!", "tokens": ["Der", "Him\u00b7mel", "und", "Er\u00b7de", "und", "al\u00b7les", "be\u00b7wegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "PIS", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ach wenn doch die Menschheit dein Wesen erkennte;", "tokens": ["Ach", "wenn", "doch", "die", "Menschheit", "dein", "We\u00b7sen", "er\u00b7kenn\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ADV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "So w\u00fcrde dem Sorgen das Handwerck gelegt:", "tokens": ["So", "w\u00fcr\u00b7de", "dem", "Sor\u00b7gen", "das", "Hand\u00b7werck", "ge\u00b7legt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Der Eigensinn m\u00fcste, wie andere L\u00fcste,", "tokens": ["Der", "Ei\u00b7gen\u00b7sinn", "m\u00fcs\u00b7te", ",", "wie", "an\u00b7de\u00b7re", "L\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Dem Vater im Himmel, dem Sch\u00f6pffer der Erden,", "tokens": ["Dem", "Va\u00b7ter", "im", "Him\u00b7mel", ",", "dem", "Sch\u00f6pf\u00b7fer", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Geopffert, und also geb\u00e4ndiget werden.", "tokens": ["Ge\u00b7opf\u00b7fert", ",", "und", "al\u00b7so", "ge\u00b7b\u00e4n\u00b7di\u00b7get", "wer\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "ADV", "VVPP", "VAINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Die Klugheit Ahitophels muste vernarren,", "tokens": ["Die", "Klug\u00b7heit", "A\u00b7hi\u00b7to\u00b7phels", "mus\u00b7te", "ver\u00b7nar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "So bald sie mit David dem GOttes-Mann stritt:", "tokens": ["So", "bald", "sie", "mit", "Da\u00b7vid", "dem", "Got\u00b7tes\u00b7Mann", "stritt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPR", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die Wei\u00dfheit der Menschen mu\u00df gleichfalls erstarren,", "tokens": ["Die", "Wei\u00df\u00b7heit", "der", "Men\u00b7schen", "mu\u00df", "gleich\u00b7falls", "er\u00b7star\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "So bald ein Kind GOttes den Schauplatz betritt.", "tokens": ["So", "bald", "ein", "Kind", "Got\u00b7tes", "den", "Schau\u00b7platz", "be\u00b7tritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Die g\u00f6ttliche Thoren sind weiser gebohren,", "tokens": ["Die", "g\u00f6tt\u00b7li\u00b7che", "Tho\u00b7ren", "sind", "wei\u00b7ser", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Als alle die Weisen, die unter den Sternen,", "tokens": ["Als", "al\u00b7le", "die", "Wei\u00b7sen", ",", "die", "un\u00b7ter", "den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Mit M\u00fche und Arbeit ihr Wissen erlernen.", "tokens": ["Mit", "M\u00fc\u00b7he", "und", "Ar\u00b7beit", "ihr", "Wis\u00b7sen", "er\u00b7ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Das siehet und h\u00f6ret der elende Hauffe", "tokens": ["Das", "sie\u00b7het", "und", "h\u00f6\u00b7ret", "der", "e\u00b7len\u00b7de", "Hauf\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der Klugen, die Christus zu Narren gemacht,", "tokens": ["Der", "Klu\u00b7gen", ",", "die", "Chris\u00b7tus", "zu", "Nar\u00b7ren", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "NE", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und stehet nicht still im vergeblichen Lauffe,", "tokens": ["Und", "ste\u00b7het", "nicht", "still", "im", "ver\u00b7geb\u00b7li\u00b7chen", "Lauf\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Bem\u00fcht sich hingegen bey Tag und bey Nacht,", "tokens": ["Be\u00b7m\u00fcht", "sich", "hin\u00b7ge\u00b7gen", "bey", "Tag", "und", "bey", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Vom Abend zum Morgen vermehrt sich sein Sorgen", "tokens": ["Vom", "A\u00b7bend", "zum", "Mor\u00b7gen", "ver\u00b7mehrt", "sich", "sein", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und endlich bekommt er von gestern und heute", "tokens": ["Und", "end\u00b7lich", "be\u00b7kommt", "er", "von", "ge\u00b7stern", "und", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "KON", "ADV"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Das Warten der k\u00fcnfftigen Dinge zur Beute.", "tokens": ["Das", "War\u00b7ten", "der", "k\u00fcnff\u00b7ti\u00b7gen", "Din\u00b7ge", "zur", "Beu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Die Christen sind stille und lassen den machen,", "tokens": ["Die", "Chris\u00b7ten", "sind", "stil\u00b7le", "und", "las\u00b7sen", "den", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "KON", "VVFIN", "ART", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der ihnen als Vater mit Rechte befiehlt;", "tokens": ["Der", "ih\u00b7nen", "als", "Va\u00b7ter", "mit", "Rech\u00b7te", "be\u00b7fiehlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die andere sehens, und spotten, und lachen,", "tokens": ["Die", "an\u00b7de\u00b7re", "se\u00b7hens", ",", "und", "spot\u00b7ten", ",", "und", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "$,", "KON", "VVFIN", "$,", "KON", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da\u00df GOtt mit den Seinen so wunderlich spielt.", "tokens": ["Da\u00df", "Gott", "mit", "den", "Sei\u00b7nen", "so", "wun\u00b7der\u00b7lich", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "PPOSS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Und hebt sich in seinen gemessenen Schrancken", "tokens": ["Und", "hebt", "sich", "in", "sei\u00b7nen", "ge\u00b7mes\u00b7se\u00b7nen", "Schran\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Weit \u00fcber der Menschen Vernunfft und Gedancken.", "tokens": ["Weit", "\u00fc\u00b7ber", "der", "Men\u00b7schen", "Ver\u00b7nunfft", "und", "Ge\u00b7dan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Frolocket ihr Kinder der ewigen Liebe,", "tokens": ["Fro\u00b7lo\u00b7cket", "ihr", "Kin\u00b7der", "der", "e\u00b7wi\u00b7gen", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Jhr werdet zum Wunder und Zeichen gesetzt:", "tokens": ["Ihr", "wer\u00b7det", "zum", "Wun\u00b7der", "und", "Zei\u00b7chen", "ge\u00b7setzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Vater entbrennet vor hertzlichem Triebe,", "tokens": ["Der", "Va\u00b7ter", "ent\u00b7bren\u00b7net", "vor", "hertz\u00b7li\u00b7chem", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "So bald ihr die Wangen mit Thr\u00e4nen benetzt.", "tokens": ["So", "bald", "ihr", "die", "Wan\u00b7gen", "mit", "Thr\u00e4\u00b7nen", "be\u00b7netzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Er h\u00f6rt ja im Himmel der Erden Get\u00fcmmel,", "tokens": ["Er", "h\u00f6rt", "ja", "im", "Him\u00b7mel", "der", "Er\u00b7den", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Daf\u00fcr sich die himmlisch-gesinnete Seelen,", "tokens": ["Da\u00b7f\u00fcr", "sich", "die", "himm\u00b7lischge\u00b7sin\u00b7ne\u00b7te", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die stoltze Behausung der Ruhe erw\u00e4hlen.", "tokens": ["Die", "stolt\u00b7ze", "Be\u00b7hau\u00b7sung", "der", "Ru\u00b7he", "er\u00b7w\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "So hat sich bey Christen ein jeglicher Morgen", "tokens": ["So", "hat", "sich", "bey", "Chris\u00b7ten", "ein", "jeg\u00b7li\u00b7cher", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "APPR", "NN", "ART", "PIAT", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Auf seine Bed\u00fcrffni\u00df alleine geschickt:", "tokens": ["Auf", "sei\u00b7ne", "Be\u00b7d\u00fcrf\u00b7fni\u00df", "al\u00b7lei\u00b7ne", "ge\u00b7schickt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wie kommt es denn, da\u00df man die leidige Sorgen", "tokens": ["Wie", "kommt", "es", "denn", ",", "da\u00df", "man", "die", "lei\u00b7di\u00b7ge", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Bey Kindern der Menschen so h\u00e4uffig erblickt?", "tokens": ["Bey", "Kin\u00b7dern", "der", "Men\u00b7schen", "so", "h\u00e4uf\u00b7fig", "er\u00b7blickt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Weil diese sich selber, und ", "tokens": ["Weil", "die\u00b7se", "sich", "sel\u00b7ber", ",", "und"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PDS", "PRF", "ADV", "$,", "KON"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Zu ihren ohnm\u00e4chtigen G\u00f6ttern erw\u00e4hlen,", "tokens": ["Zu", "ih\u00b7ren", "ohn\u00b7m\u00e4ch\u00b7ti\u00b7gen", "G\u00f6t\u00b7tern", "er\u00b7w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Jm ", "tokens": ["Jm"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wird alle in ihrem Gewissen verklagen,", "tokens": ["Wird", "al\u00b7le", "in", "ih\u00b7rem", "Ge\u00b7wis\u00b7sen", "ver\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die GOttes Werck ohne Verwunderung sehn:", "tokens": ["Die", "Got\u00b7tes", "Werck", "oh\u00b7ne", "Ver\u00b7wun\u00b7de\u00b7rung", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Wer h\u00e4tte die Sachen verwirreter machen,", "tokens": ["Wer", "h\u00e4t\u00b7te", "die", "Sa\u00b7chen", "ver\u00b7wir\u00b7re\u00b7ter", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und dennoch so seliglich endigen k\u00f6nnen,", "tokens": ["Und", "den\u00b7noch", "so", "se\u00b7lig\u00b7lich", "en\u00b7di\u00b7gen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Als er, den wir Vater und ", "tokens": ["Als", "er", ",", "den", "wir", "Va\u00b7ter", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "P