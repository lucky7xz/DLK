{"dta.poem.4256": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zur Erndte-Zeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Sch\u00f6pfer! ach, la\u00df doch den Segen,", "tokens": ["Mein", "Sch\u00f6p\u00b7fer", "!", "ach", ",", "la\u00df", "doch", "den", "Se\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "XY", "$,", "VVIMP", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Deine reiche Vater-Hand", "tokens": ["Den", "Dei\u00b7ne", "rei\u00b7che", "Va\u00b7ter\u00b7Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In das mir itzt geschenkte Land", "tokens": ["In", "das", "mir", "itzt", "ge\u00b7schenk\u00b7te", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So gn\u00e4diglich hat wollen legen,", "tokens": ["So", "gn\u00e4\u00b7di\u00b7glich", "hat", "wol\u00b7len", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch einen gar zu starken Regen", "tokens": ["Durch", "ei\u00b7nen", "gar", "zu", "star\u00b7ken", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht schwinden, nicht vernichtig seyn!", "tokens": ["Nicht", "schwin\u00b7den", ",", "nicht", "ver\u00b7nich\u00b7tig", "seyn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach, gieb doch warmen Sonnen-Schein!", "tokens": ["Ach", ",", "gieb", "doch", "war\u00b7men", "Son\u00b7nen\u00b7Schein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "La\u00df, in der frohen Erndte-Zeit,", "tokens": ["La\u00df", ",", "in", "der", "fro\u00b7hen", "Ernd\u00b7te\u00b7Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die reife Frucht seyn abgemeyt,", "tokens": ["Die", "rei\u00b7fe", "Frucht", "seyn", "ab\u00b7ge\u00b7meyt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und, was mein Feld bisher geziert,", "tokens": ["Und", ",", "was", "mein", "Feld", "bis\u00b7her", "ge\u00b7ziert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In meine Scheune seyn gef\u00fchrt!", "tokens": ["In", "mei\u00b7ne", "Scheu\u00b7ne", "seyn", "ge\u00b7f\u00fchrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "La\u00df mich, in ernster Fr\u00f6hlichkeit,", "tokens": ["La\u00df", "mich", ",", "in", "erns\u00b7ter", "Fr\u00f6h\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "In stiller Andacht, oft erwegen,", "tokens": ["In", "stil\u00b7ler", "An\u00b7dacht", ",", "oft", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df alles, HErr! an Deinem Segen", "tokens": ["Da\u00df", "al\u00b7les", ",", "Herr", "!", "an", "Dei\u00b7nem", "Se\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Allein gelegen,", "tokens": ["Al\u00b7lein", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Da\u00df Du allein dem Saamen Kraft,", "tokens": ["Da\u00df", "Du", "al\u00b7lein", "dem", "Saa\u00b7men", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Der Luft, dem Wasser und der Erden,", "tokens": ["Der", "Luft", ",", "dem", "Was\u00b7ser", "und", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Solch\u2019 eine Wunder-Eigenschaft,", "tokens": ["Solch'", "ei\u00b7ne", "Wun\u00b7der\u00b7Ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Da\u00df sie f\u00fcr uns so fruchtbar werden,", "tokens": ["Da\u00df", "sie", "f\u00fcr", "uns", "so", "frucht\u00b7bar", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Da\u00df wir durch sie ern\u00e4hrt, getr\u00e4nkt,", "tokens": ["Da\u00df", "wir", "durch", "sie", "er\u00b7n\u00e4hrt", ",", "ge\u00b7tr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Durch Deine weise Macht geschenkt!", "tokens": ["Durch", "Dei\u00b7ne", "wei\u00b7se", "Macht", "ge\u00b7schenkt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Da\u00df Du den warmen Sonnen-Schein,", "tokens": ["Da\u00df", "Du", "den", "war\u00b7men", "Son\u00b7nen\u00b7Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Worinn die Lebens-Kraft gesenket,", "tokens": ["Wo\u00b7rinn", "die", "Le\u00b7bens\u00b7Kraft", "ge\u00b7sen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Da\u00df, durch ihr sanft und heilsam Brennen,", "tokens": ["Da\u00df", ",", "durch", "ihr", "sanft", "und", "heil\u00b7sam", "Bren\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "PPOSAT", "ADJD", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Die Saat so n\u00fctzlich reifen k\u00f6nnen,", "tokens": ["Die", "Saat", "so", "n\u00fctz\u00b7lich", "rei\u00b7fen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "So v\u00e4terlich auf uns gelenkt!", "tokens": ["So", "v\u00e4\u00b7ter\u00b7lich", "auf", "uns", "ge\u00b7lenkt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.27": {"text": "Gieb, da\u00df man oft mit Dank gedenket:", "tokens": ["Gieb", ",", "da\u00df", "man", "oft", "mit", "Dank", "ge\u00b7den\u00b7ket", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PIS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "O Sch\u00f6pfer, es ist alles Dein!", "tokens": ["O", "Sch\u00f6p\u00b7fer", ",", "es", "ist", "al\u00b7les", "Dein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "PIS", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}