{"dta.poem.2069": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Eichhorn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schrecken uns gleich viele Thiere nebst dem Nutzen;", "tokens": ["Schre\u00b7cken", "uns", "gleich", "vie\u00b7le", "Thie\u00b7re", "nebst", "dem", "Nut\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "stellet hier", "tokens": ["stel\u00b7let", "hier"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ein uns nur vergn\u00fcgend Thierchen in dem Eichhorn uns", "tokens": ["Ein", "uns", "nur", "ver\u00b7gn\u00fc\u00b7gend", "Thier\u00b7chen", "in", "dem", "Eich\u00b7horn", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVPP", "NN", "APPR", "ART", "NN", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "sich f\u00fcr:", "tokens": ["sich", "f\u00fcr", ":"], "token_info": ["word", "word", "punct"], "pos": ["PRF", "APPR", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Alle Theile seines K\u00f6rpers sind sowohl, als andre,", "tokens": ["Al\u00b7le", "Thei\u00b7le", "sei\u00b7nes", "K\u00f6r\u00b7pers", "sind", "so\u00b7wohl", ",", "als", "and\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "VAFIN", "ADV", "$,", "KOUS", "PIS", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "zierlich;", "tokens": ["zier\u00b7lich", ";"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Doch derselben Handlungen sind so artig, so po\u00dfirlich,", "tokens": ["Doch", "der\u00b7sel\u00b7ben", "Hand\u00b7lun\u00b7gen", "sind", "so", "ar\u00b7tig", ",", "so", "po\u00df\u00b7ir\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "--+--+-+-+-+++-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Die Bewegungen so lebhaft, hurtig, und so schnell, da\u00df", "tokens": ["Die", "Be\u00b7we\u00b7gun\u00b7gen", "so", "leb\u00b7haft", ",", "hur\u00b7tig", ",", "und", "so", "schnell", ",", "da\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "ADJD", "$,", "KON", "ADV", "ADJD", "$,", "KOUS"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "man", "tokens": ["man"], "token_info": ["word"], "pos": ["PIS"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Nicht leicht ihre Spr\u00fcng\u2019 und Mienen sonder Lachen sehen", "tokens": ["Nicht", "leicht", "ih\u00b7re", "Spr\u00fcng'", "und", "Mie\u00b7nen", "son\u00b7der", "La\u00b7chen", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "PPOSAT", "NN", "KON", "NN", "KON", "NN", "VVINF"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "kann.", "tokens": ["kann", "."], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Sie bewohnen hohe B\u00e4ume, k\u00f6nnen so gewaltig sprin-", "tokens": ["Sie", "be\u00b7woh\u00b7nen", "ho\u00b7he", "B\u00e4u\u00b7me", ",", "k\u00f6n\u00b7nen", "so", "ge\u00b7wal\u00b7tig", "sprin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,", "VMFIN", "ADV", "ADJD", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "gen,", "tokens": ["gen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Und von einem Baum zum andern sich so schnell und hurtig", "tokens": ["Und", "von", "ei\u00b7nem", "Baum", "zum", "an\u00b7dern", "sich", "so", "schnell", "und", "hur\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "ADJA", "PRF", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.15": {"text": "schwingen,", "tokens": ["schwin\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Da\u00df es l\u00e4\u00dft, als wenn sie fl\u00f6gen, da die langen Schw\u00e4nze", "tokens": ["Da\u00df", "es", "l\u00e4\u00dft", ",", "als", "wenn", "sie", "fl\u00f6\u00b7gen", ",", "da", "die", "lan\u00b7gen", "Schw\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.17": {"text": "ihnen,", "tokens": ["ih\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Die so lang als wie sie selbst, gleichsam als zu Fl\u00fcgeln", "tokens": ["Die", "so", "lang", "als", "wie", "sie", "selbst", ",", "gleich\u00b7sam", "als", "zu", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "KOKOM", "PWAV", "PPER", "ADV", "$,", "ADJD", "KOKOM", "APPR", "NN"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.19": {"text": "dienen.", "tokens": ["die\u00b7nen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Meistens sind sie roth, und gleichen fast dem Fuchs an Farb\u2019", "tokens": ["Meis\u00b7tens", "sind", "sie", "roth", ",", "und", "glei\u00b7chen", "fast", "dem", "Fuchs", "an", "Fa\u00b7rb'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "KON", "ADJA", "ADV", "ART", "NE", "APPR", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.21": {"text": "und Haar:", "tokens": ["und", "Haar", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "Dennoch findet man verschiedne von verschiedner Farb\u2019,", "tokens": ["Den\u00b7noch", "fin\u00b7det", "man", "ver\u00b7schied\u00b7ne", "von", "ver\u00b7schied\u00b7ner", "Fa\u00b7rb'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.23": {"text": "und zwar", "tokens": ["und", "zwar"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.24": {"text": "Ofters grau und aschenf\u00e4rbig, auch wohl schw\u00e4rzlich,", "tokens": ["Of\u00b7ters", "grau", "und", "asc\u00b7hen\u00b7f\u00e4r\u00b7big", ",", "auch", "wohl", "schw\u00e4rz\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.25": {"text": "aber selten,", "tokens": ["a\u00b7ber", "sel\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.26": {"text": "Deren kleine B\u00e4lge denn, weil sie rarer, mehr auch", "tokens": ["De\u00b7ren", "klei\u00b7ne", "B\u00e4l\u00b7ge", "denn", ",", "weil", "sie", "ra\u00b7rer", ",", "mehr", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "ADJA", "NN", "ADV", "$,", "KOUS", "PPER", "ADJD", "$,", "ADV", "ADV"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "gelten.", "tokens": ["gel\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.28": {"text": "Dieses Thierchen l\u00e4\u00dft sich z\u00e4hmen, da es seltsame Fi-", "tokens": ["Die\u00b7ses", "Thier\u00b7chen", "l\u00e4\u00dft", "sich", "z\u00e4h\u00b7men", ",", "da", "es", "selt\u00b7sa\u00b7me", "Fi"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "PRF", "VVINF", "$,", "KOUS", "PPER", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.29": {"text": "guren", "tokens": ["gu\u00b7ren"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}, "line.30": {"text": "Und in allen Handlungen l\u00e4cherliche Posituren", "tokens": ["Und", "in", "al\u00b7len", "Hand\u00b7lun\u00b7gen", "l\u00e4\u00b7cher\u00b7li\u00b7che", "Po\u00b7si\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "ADJA", "NN"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}