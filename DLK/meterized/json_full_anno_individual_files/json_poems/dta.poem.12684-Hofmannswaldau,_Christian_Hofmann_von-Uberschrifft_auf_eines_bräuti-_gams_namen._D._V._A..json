{"dta.poem.12684": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uberschrifft auf eines br\u00e4uti-  \n gams namen.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist ein stern und berg: die nacht geh\u00f6rt zum sterne,", "tokens": ["Du", "bist", "ein", "stern", "und", "berg", ":", "die", "nacht", "ge\u00b7h\u00f6rt", "zum", "ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "KON", "NE", "$.", "ART", "NN", "VVFIN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So er recht scheinen soll. Der thal ist auch noch ferne,", "tokens": ["So", "er", "recht", "schei\u00b7nen", "soll", ".", "Der", "thal", "ist", "auch", "noch", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "VVINF", "VMFIN", "$.", "ART", "NN", "VAFIN", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo hohe berge sind. Den nacht-schein und den thal", "tokens": ["Wo", "ho\u00b7he", "ber\u00b7ge", "sind", ".", "Den", "nacht\u00b7schein", "und", "den", "thal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VAFIN", "$.", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bringt dir itzt deine braut, mein Sternberg! auf einmal.", "tokens": ["Bringt", "dir", "itzt", "dei\u00b7ne", "braut", ",", "mein", "Stern\u00b7berg", "!", "auf", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "VVFIN", "$,", "PPOSAT", "NN", "$.", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}