{"dta.poem.1039": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Der 1. Epod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "D Och lie\u00df Er seine forcht fallen/", "tokens": ["D", "Och", "lie\u00df", "Er", "sei\u00b7ne", "forcht", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da Er manche Nymfelein", "tokens": ["Da", "Er", "man\u00b7che", "Nym\u00b7fel\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den Oreadelein", "tokens": ["Mit", "den", "O\u00b7rea\u00b7de\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.4": {"text": "H\u00f6rte jhre stim erschallen.", "tokens": ["H\u00f6r\u00b7te", "jhre", "stim", "er\u00b7schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Die Najaden gleicher wei\u00df", "tokens": ["Die", "Na\u00b7ja\u00b7den", "glei\u00b7cher", "wei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "(welche mit k\u00fcnstlichem flei\u00df", "tokens": ["(", "wel\u00b7che", "mit", "k\u00fcnst\u00b7li\u00b7chem", "flei\u00df"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PRELS", "APPR", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Jhre krause haar besch\u00f6net/", "tokens": ["Ih\u00b7re", "krau\u00b7se", "haar", "be\u00b7sch\u00f6\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Vnd mit kleinoten gekr\u00f6net)", "tokens": ["Vnd", "mit", "klei\u00b7no\u00b7ten", "ge\u00b7kr\u00f6\u00b7net", ")"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "VVPP", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Schwomen in hertzlichem wohn", "tokens": ["Schwo\u00b7men", "in", "hertz\u00b7li\u00b7chem", "wohn"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.10": {"text": "Mit dem Neckar/ so sie f\u00fchrte/", "tokens": ["Mit", "dem", "Ne\u00b7ckar", "/", "so", "sie", "f\u00fchr\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "ADV", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Vnd den auch ein gr\u00fcne Cron", "tokens": ["Vnd", "den", "auch", "ein", "gr\u00fc\u00b7ne", "Cron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Vnd ein k\u00f6stliches klaid zierte/", "tokens": ["Vnd", "ein", "k\u00f6st\u00b7li\u00b7ches", "klaid", "zier\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Welcher mit klarem gesicht", "tokens": ["Wel\u00b7cher", "mit", "kla\u00b7rem", "ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "APPR", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.14": {"text": "Seinen freund also bespricht:", "tokens": ["Sei\u00b7nen", "freund", "al\u00b7so", "be\u00b7spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}