{"textgrid.poem.43121": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach wie war es leer und schaurig,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach wie war es leer und schaurig,", "tokens": ["Ach", "wie", "war", "es", "leer", "und", "schau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ich einst die Stra\u00dfen zog,", "tokens": ["Als", "ich", "einst", "die", "Stra\u00b7\u00dfen", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebensm\u00fcde, sterbenstraurig,", "tokens": ["Le\u00b7bens\u00b7m\u00fc\u00b7de", ",", "ster\u00b7ben\u00b7strau\u00b7rig", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Still mich in Dein Fenster bog.", "tokens": ["Still", "mich", "in", "Dein", "Fens\u00b7ter", "bog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als ich dann mit dumpfem Weinen", "tokens": ["Als", "ich", "dann", "mit", "dum\u00b7pfem", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Auf der Schwelle niedersank,", "tokens": ["Auf", "der", "Schwel\u00b7le", "nie\u00b7der\u00b7sank", ","], "token_info": [