{"textgrid.poem.34747": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verdro\u00dfnen Sinn im kalten Herzen hegend,", "tokens": ["Ver\u00b7dro\u00df\u00b7nen", "Sinn", "im", "kal\u00b7ten", "Her\u00b7zen", "he\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Reis ich verdrie\u00dflich durch die kalte Welt,", "tokens": ["Reis", "ich", "ver\u00b7drie\u00df\u00b7lich", "durch", "die", "kal\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu Ende geht der Herbst, ein Nebel h\u00e4lt", "tokens": ["Zu", "En\u00b7de", "geht", "der", "Herbst", ",", "ein", "Ne\u00b7bel", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Feuchteingeh\u00fcllt die abgestorbne Gegend.", "tokens": ["Feuch\u00b7tein\u00b7ge\u00b7h\u00fcllt", "die", "ab\u00b7ge\u00b7storb\u00b7ne", "Ge\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Die Winde pfeifen, hin und her bewegend", "tokens": ["Die", "Win\u00b7de", "pfei\u00b7fen", ",", "hin", "und", "her", "be\u00b7we\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "po