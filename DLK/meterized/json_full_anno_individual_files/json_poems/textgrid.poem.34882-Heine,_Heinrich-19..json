{"textgrid.poem.34882": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verschlechtert sich nicht dein Herz und dein Stil,", "tokens": ["Ver\u00b7schlech\u00b7tert", "sich", "nicht", "dein", "Herz", "und", "dein", "Stil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So magst du treiben jedwedes Spiel;", "tokens": ["So", "magst", "du", "trei\u00b7ben", "jed\u00b7we\u00b7des", "Spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mein Freund, ich werde dich nie verkennen,", "tokens": ["Mein", "Freund", ",", "ich", "wer\u00b7de", "dich", "nie", "ver\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und sollt ich dich auch Herr Hofrat nennen.", "tokens": ["Und", "sollt", "ich", "dich", "auch", "Herr", "Hof\u00b7rat", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Sie machen jetzt ein gro\u00dfes Geschrei,", "tokens": ["Sie", "ma\u00b7chen", "jetzt", "ein", "gro\u00b7\u00dfes", "Ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Von wegen deiner Verhofr\u00e4terei,", "tokens": ["Von", "we\u00b7gen", "dei\u00b7ner", "Ver\u00b7ho\u00b7fr\u00e4\u00b7te\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom Seinestrand bis an der Elbe", "tokens": ["Vom", "Sei\u00b7nes\u00b7trand", "bis", "an", "der", "El\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "APPR", "ART", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6rt ich seit Monden immer dasselbe:", "tokens": ["H\u00f6rt", "ich", "seit", "Mon\u00b7den", "im\u00b7mer", "das\u00b7sel\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "ADV", "PDAT", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Fortschrittsbeine h\u00e4tten sich", "tokens": ["Die", "Fort\u00b7schritts\u00b7bei\u00b7ne", "h\u00e4t\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In R\u00fcckschrittsbeine verwandelt \u2013 Oh, sprich,", "tokens": ["In", "R\u00fcck\u00b7schritts\u00b7bei\u00b7ne", "ver\u00b7wan\u00b7delt", "\u2013", "Oh", ",", "sprich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$(", "ITJ", "$,", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Reitest du wirklich auf schw\u00e4bischen Krebsen?", "tokens": ["Rei\u00b7test", "du", "wirk\u00b7lich", "auf", "schw\u00e4\u00b7bi\u00b7schen", "Kreb\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "\u00c4ugelst du wirklich mit f\u00fcrstlichen Kebsen?", "tokens": ["\u00c4u\u00b7gelst", "du", "wirk\u00b7lich", "mit", "f\u00fcrst\u00b7li\u00b7chen", "Keb\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Vielleicht bist du m\u00fcde und sehnst dich nach Schlaf.", "tokens": ["Viel\u00b7leicht", "bist", "du", "m\u00fc\u00b7de", "und", "sehnst", "dich", "nach", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Du hast die Nacht hindurch so brav", "tokens": ["Du", "hast", "die", "Nacht", "hin\u00b7durch", "so", "brav"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "PAV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geblasen, jetzt h\u00e4ngst du das Horn an den Nagel,", "tokens": ["Ge\u00b7bla\u00b7sen", ",", "jetzt", "h\u00e4ngst", "du", "das", "Horn", "an", "den", "Na\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NE", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Mag tuten, wer will, f\u00fcr den deutschen Janhagel!", "tokens": ["Mag", "tu\u00b7ten", ",", "wer", "will", ",", "f\u00fcr", "den", "deut\u00b7schen", "Jan\u00b7ha\u00b7gel", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "PWS", "VMFIN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du legst dich zu Bette und schlie\u00dfest zu", "tokens": ["Du", "legst", "dich", "zu", "Bet\u00b7te", "und", "schlie\u00b7\u00dfest", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "VVFIN", "APPR"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Augen, doch l\u00e4\u00dft man dich nicht in Ruh'.", "tokens": ["Die", "Au\u00b7gen", ",", "doch", "l\u00e4\u00dft", "man", "dich", "nicht", "in", "Ruh'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "PIS", "PRF", "PTKNEG", "APPR", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Vor deinem Fenster spotten die Schreier:", "tokens": ["Vor", "dei\u00b7nem", "Fens\u00b7ter", "spot\u00b7ten", "die", "Schrei\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbbrutus, du schl\u00e4fst? Wach auf, Befreier!\u00ab", "tokens": ["\u00bb", "bru\u00b7tus", ",", "du", "schl\u00e4fst", "?", "Wach", "auf", ",", "Be\u00b7frei\u00b7er", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "NE", "$,", "PPER", "VVFIN", "$.", "NN", "PTKVZ", "$,", "NN", "$.", "$("], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}}, "stanza.6": {"line.1": {"text": "Ach! so ein Schreier wei\u00df nicht, warum", "tokens": ["Ach", "!", "so", "ein", "Schrei\u00b7er", "wei\u00df", "nicht", ",", "wa\u00b7rum"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "$.", "ADV", "ART", "NN", "VVFIN", "PTKNEG", "$,", "PWAV"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der beste Nachtw\u00e4chter wird endlich stumm,", "tokens": ["Der", "bes\u00b7te", "Nacht\u00b7w\u00e4ch\u00b7ter", "wird", "end\u00b7lich", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.3": {"text": "Es ahndet nicht so ein junger Maulheld,", "tokens": ["Es", "ahn\u00b7det", "nicht", "so", "ein", "jun\u00b7ger", "Maul\u00b7held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Warum der Mensch am End' das Maul h\u00e4lt.", "tokens": ["Wa\u00b7rum", "der", "Mensch", "am", "End'", "das", "Maul", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du fragst mich, wie es uns hier ergeht?", "tokens": ["Du", "fragst", "mich", ",", "wie", "es", "uns", "hier", "er\u00b7geht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hier ist es still, kein Windchen weht,", "tokens": ["Hier", "ist", "es", "still", ",", "kein", "Wind\u00b7chen", "weht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wetterfahnen sind sehr verlegen,", "tokens": ["Die", "Wet\u00b7ter\u00b7fah\u00b7nen", "sind", "sehr", "ver\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie wissen nicht, wohin sich bewegen...", "tokens": ["Sie", "wis\u00b7sen", "nicht", ",", "wo\u00b7hin", "sich", "be\u00b7we\u00b7gen", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PRF", "VVINF", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Verschlechtert sich nicht dein Herz und dein Stil,", "tokens": ["Ver\u00b7schlech\u00b7tert", "sich", "nicht", "dein", "Herz", "und", "dein", "Stil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So magst du treiben jedwedes Spiel;", "tokens": ["So", "magst", "du", "trei\u00b7ben", "jed\u00b7we\u00b7des", "Spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mein Freund, ich werde dich nie verkennen,", "tokens": ["Mein", "Freund", ",", "ich", "wer\u00b7de", "dich", "nie", "ver\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter"