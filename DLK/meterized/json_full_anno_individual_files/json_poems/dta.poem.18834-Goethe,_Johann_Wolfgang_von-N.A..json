{"dta.poem.18834": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Bulbuls Nachtlied, durch die Schauer,", "tokens": ["Bul\u00b7buls", "Nacht\u00b7lied", ",", "durch", "die", "Schau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drang zu Allahs lichtem Throne,", "tokens": ["Drang", "zu", "Al\u00b7lahs", "lich\u00b7tem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und dem Wohlgesang zu Lohne", "tokens": ["Und", "dem", "Wohl\u00b7ge\u00b7sang", "zu", "Loh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sperrt er sie in goldnen Bauer.", "tokens": ["Sperrt", "er", "sie", "in", "gold\u00b7nen", "Bau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieser sind des Menschen Glieder.", "tokens": ["Die\u00b7ser", "sind", "des", "Men\u00b7schen", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zwar sie f\u00fchlet sich beschr\u00e4nket;", "tokens": ["Zwar", "sie", "f\u00fch\u00b7let", "sich", "be\u00b7schr\u00e4n\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch wenn sie es recht bedenket,", "tokens": ["Doch", "wenn", "sie", "es", "recht", "be\u00b7den\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Singt das Seelchen immer wieder.", "tokens": ["Singt", "das", "Seel\u00b7chen", "im\u00b7mer", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}