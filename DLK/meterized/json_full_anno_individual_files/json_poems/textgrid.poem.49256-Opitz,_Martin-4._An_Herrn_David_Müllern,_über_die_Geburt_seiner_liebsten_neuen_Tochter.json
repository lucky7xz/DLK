{"textgrid.poem.49256": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "4. An Herrn David M\u00fcllern, \u00fcber die Geburt seiner liebsten neuen Tochter", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr M\u00fcller gieng im Schertze", "tokens": ["Herr", "M\u00fcl\u00b7ler", "gieng", "im", "Schert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Wetten mit mir ein,", "tokens": ["Ein", "Wet\u00b7ten", "mit", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Trost und liebstes Hertze", "tokens": ["Sein", "Trost", "und", "liebs\u00b7tes", "Hert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Br\u00e4cht' ihm ein T\u00f6chterlein;", "tokens": ["Br\u00e4cht'", "ihm", "ein", "T\u00f6ch\u00b7ter\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Urtheil war darwider", "tokens": ["Mein", "Ur\u00b7theil", "war", "dar\u00b7wi\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und rhiet' auff einen Sohn,", "tokens": ["Und", "rhiet'", "auff", "ei\u00b7nen", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich satzt' ihm meine Lieder,", "tokens": ["Ich", "satzt'", "ihm", "mei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er mir den Xenophon.", "tokens": ["Er", "mir", "den", "Xe\u00b7no\u00b7phon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Worauff er sey gegangen,", "tokens": ["Wo\u00b7rauff", "er", "sey", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Da\u00df wei\u00df ich warlich nicht,", "tokens": ["Da\u00df", "wei\u00df", "ich", "war\u00b7lich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ich sahe dieser Wangen", "tokens": ["Ich", "sa\u00b7he", "die\u00b7ser", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Lebhasstes sch\u00f6nes Liecht", "tokens": ["Leb\u00b7hass\u00b7tes", "sch\u00f6\u00b7nes", "Liecht"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Und glaubte, da\u00df sie schreiben,", "tokens": ["Und", "glaub\u00b7te", ",", "da\u00df", "sie", "schrei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Die S\u00f6hne machen roth;", "tokens": ["Die", "S\u00f6h\u00b7ne", "ma\u00b7chen", "roth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Nun mu\u00df es dabey bleiben,", "tokens": ["Nun", "mu\u00df", "es", "da\u00b7bey", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PAV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Ein Buch ist kein Gebot.", "tokens": ["Ein", "Buch", "ist", "kein", "Ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Wiewol, was ist gefehlet,", "tokens": ["Wie\u00b7wol", ",", "was", "ist", "ge\u00b7feh\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Weil doch ein Weibesbild", "tokens": ["Weil", "doch", "ein", "Wei\u00b7bes\u00b7bild"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Wird f\u00fcr ein Mensch gezehlet,", "tokens": ["Wird", "f\u00fcr", "ein", "Mensch", "ge\u00b7zeh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Und auch nicht minder gilt?", "tokens": ["Und", "auch", "nicht", "min\u00b7der", "gilt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Wer anders schon wil sagen,", "tokens": ["Wer", "an\u00b7ders", "schon", "wil", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Der kennt sich selber nicht;", "tokens": ["Der", "kennt", "sich", "sel\u00b7ber", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Dann Menschen Menschen tragen", "tokens": ["Dann", "Men\u00b7schen", "Men\u00b7schen", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "An dieses Tageliecht.", "tokens": ["An", "die\u00b7ses", "Ta\u00b7ge\u00b7liecht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Was rucken wir den Frauen", "tokens": ["Was", "ru\u00b7cken", "wir", "den", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Di\u00df oder jenes f\u00fcr?", "tokens": ["Di\u00df", "o\u00b7der", "je\u00b7nes", "f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "KON", "PDS", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Die Blumen auff den Auen", "tokens": ["Die", "Blu\u00b7men", "auff", "den", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Sind nicht von solcher Ziehr,", "tokens": ["Sind", "nicht", "von", "sol\u00b7cher", "Ziehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Die Sonne, wann sie stralet", "tokens": ["Die", "Son\u00b7ne", ",", "wann", "sie", "stra\u00b7let"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Vom Mohrenlande her,", "tokens": ["Vom", "Moh\u00b7ren\u00b7lan\u00b7de", "her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Hat sch\u00f6ner nie gemahlet", "tokens": ["Hat", "sch\u00f6\u00b7ner", "nie", "ge\u00b7mah\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Das Land und breite Meer.", "tokens": ["Das", "Land", "und", "brei\u00b7te", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Ein Mann der mu\u00df nur reysen,", "tokens": ["Ein", "Mann", "der", "mu\u00df", "nur", "rey\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Ertragen Hitz und Frost,", "tokens": ["Er\u00b7tra\u00b7gen", "Hitz", "und", "Frost", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Mu\u00df ziehn durch Ey\u00df und Eisen", "tokens": ["Mu\u00df", "ziehn", "durch", "Ey\u00df", "und", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Bey schlechter Ruh und Kost,", "tokens": ["Bey", "schlech\u00b7ter", "Ruh", "und", "Kost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Mu\u00df bauen, Kriege f\u00fchren,", "tokens": ["Mu\u00df", "bau\u00b7en", ",", "Krie\u00b7ge", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Steigt auff ein wildes Pferd,", "tokens": ["Steigt", "auff", "ein", "wil\u00b7des", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Wird mager beym studieren,", "tokens": ["Wird", "ma\u00b7ger", "beym", "stu\u00b7die\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Stirbt offtmals durch das Schwerd.", "tokens": ["Stirbt", "offt\u00b7mals", "durch", "das", "Schwerd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "De\u00df Frauenzimmers Jugend", "tokens": ["De\u00df", "Frau\u00b7en\u00b7zim\u00b7mers", "Ju\u00b7gend"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Wird sonder Sorgen gro\u00df,", "tokens": ["Wird", "son\u00b7der", "Sor\u00b7gen", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "Erlernet Witz und Tugend", "tokens": ["Er\u00b7ler\u00b7net", "Witz", "und", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "In ihrer Mutter Scho\u00df,", "tokens": ["In", "ih\u00b7rer", "Mut\u00b7ter", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.45": {"text": "Die sie mit Lust erziehen", "tokens": ["Die", "sie", "mit", "Lust", "er\u00b7zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Bi\u00df zu derselben Zeit,", "tokens": ["Bi\u00df", "zu", "der\u00b7sel\u00b7ben", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.47": {"text": "Da sie auch lernen fliehen", "tokens": ["Da", "sie", "auch", "ler\u00b7nen", "flie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Die Last der Einsamkeit.", "tokens": ["Die", "Last", "der", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.49": {"text": "Nun wachse, liebe Kleine,", "tokens": ["Nun", "wach\u00b7se", ",", "lie\u00b7be", "Klei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Sey deiner Eltern Ziehr,", "tokens": ["Sey", "dei\u00b7ner", "El\u00b7tern", "Ziehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.51": {"text": "Brich k\u00fcnfftig mit dem Scheine", "tokens": ["Brich", "k\u00fcnff\u00b7tig", "mit", "dem", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Der edlen Zucht herf\u00fcr;", "tokens": ["Der", "ed\u00b7len", "Zucht", "her\u00b7f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.53": {"text": "Gott lasse mich erfahren", "tokens": ["Gott", "las\u00b7se", "mich", "er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.54": {"text": "Den angenehmen Tag,", "tokens": ["Den", "an\u00b7ge\u00b7neh\u00b7men", "Tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.55": {"text": "Da\u00df ich mit greisen Haaren", "tokens": ["Da\u00df", "ich", "mit", "grei\u00b7sen", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.56": {"text": "Dein Brautlied schreiben mag.", "tokens": ["Dein", "Braut\u00b7lied", "schrei\u00b7ben", "mag."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["PPOSAT", "NN", "VVFIN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Herr M\u00fcller gieng im Schertze", "tokens": ["Herr", "M\u00fcl\u00b7ler", "gieng", "im", "Schert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Wetten mit mir ein,", "tokens": ["Ein", "Wet\u00b7ten", "mit", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Trost und liebstes Hertze", "tokens": ["Sein", "Trost", "und", "liebs\u00b7tes", "Hert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Br\u00e4cht' ihm ein T\u00f6chterlein;", "tokens": ["Br\u00e4cht'", "ihm", "ein", "T\u00f6ch\u00b7ter\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Urtheil war darwider", "tokens": ["Mein", "Ur\u00b7theil", "war", "dar\u00b7wi\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und rhiet' auff einen Sohn,", "tokens": ["Und", "rhiet'", "auff", "ei\u00b7nen", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich satzt' ihm meine Lieder,", "tokens": ["Ich", "satzt'", "ihm", "mei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er mir den Xenophon.", "tokens": ["Er", "mir", "den", "Xe\u00b7no\u00b7phon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Worauff er sey gegangen,", "tokens": ["Wo\u00b7rauff", "er", "sey", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", 