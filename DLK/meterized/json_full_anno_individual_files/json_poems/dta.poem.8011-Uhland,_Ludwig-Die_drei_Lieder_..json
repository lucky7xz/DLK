{"dta.poem.8011": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die drei Lieder .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "In der hohen Hall\u2019 sa\u00df K\u00f6nig Sifrid:", "tokens": ["In", "der", "ho\u00b7hen", "Hall'", "sa\u00df", "K\u00f6\u00b7nig", "Sif\u00b7rid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "NN", "NE", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "\u201eihr Harfner! wer wei\u00df mir das sch\u00f6nste Lied?\u201c", "tokens": ["\u201e", "ihr", "Harf\u00b7ner", "!", "wer", "wei\u00df", "mir", "das", "sch\u00f6ns\u00b7te", "Lied", "?", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$.", "PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und ein J\u00fcngling trat aus der Schaar behende,", "tokens": ["Und", "ein", "J\u00fcng\u00b7ling", "trat", "aus", "der", "Schaar", "be\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADJA", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Harf\u2019 in der Hand, das Schwerdt an der Lende.", "tokens": ["Die", "Ha\u00b7rf'", "in", "der", "Hand", ",", "das", "Schwerdt", "an", "der", "Len\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201edrei Lieder wei\u00df ich; den ersten Sang,", "tokens": ["\u201e", "drei", "Lie\u00b7der", "wei\u00df", "ich", ";", "den", "ers\u00b7ten", "Sang", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "CARD", "NN", "VVFIN", "PPER", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den hast du ja wohl vergessen schon lang:", "tokens": ["Den", "hast", "du", "ja", "wohl", "ver\u00b7ges\u00b7sen", "schon", "lang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "ADV", "ADJD", "$."], "meter": "----+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Meinen Bruder hast du meuchlings erstochen!", "tokens": ["Mei\u00b7nen", "Bru\u00b7der", "hast", "du", "meuch\u00b7lings", "er\u00b7sto\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und aber: hast ihn meuchlings erstochen!", "tokens": ["Und", "a\u00b7ber", ":", "hast", "ihn", "meuch\u00b7lings", "er\u00b7sto\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Das andre Lied, das hab\u2019 ich erdacht", "tokens": ["Das", "and\u00b7re", "Lied", ",", "das", "hab'", "ich", "er\u00b7dacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PDS", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In einer finstern, st\u00fcrmischen Nacht:", "tokens": ["In", "ei\u00b7ner", "fins\u00b7tern", ",", "st\u00fcr\u00b7mi\u00b7schen", "Nacht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Mu\u00dft mit mir fechten auf Leben und Sterben!", "tokens": ["Mu\u00dft", "mit", "mir", "fech\u00b7ten", "auf", "Le\u00b7ben", "und", "Ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Und aber: mu\u00dft fechten auf Leben und Sterben!\u201c", "tokens": ["Und", "a\u00b7ber", ":", "mu\u00dft", "fech\u00b7ten", "auf", "Le\u00b7ben", "und", "Ster\u00b7ben", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$.", "VMFIN", "VVINF", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Da lehnt\u2019 er die Harfe wohl an den Tisch,", "tokens": ["Da", "lehnt'", "er", "die", "Har\u00b7fe", "wohl", "an", "den", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und sie zogen Beide die Schwerdter frisch,", "tokens": ["Und", "sie", "zo\u00b7gen", "Bei\u00b7de", "die", "Schwerd\u00b7ter", "frisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIS", "ART", "NN", "ADJD", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und fochten lange mit wildem Schalle,", "tokens": ["Und", "foch\u00b7ten", "lan\u00b7ge", "mit", "wil\u00b7dem", "Schal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bis der K\u00f6nig sank in der hohen Halle.", "tokens": ["Bis", "der", "K\u00f6\u00b7nig", "sank", "in", "der", "ho\u00b7hen", "Hal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.5": {"line.1": {"text": "\u201enun sing\u2019 ich das dritte, das sch\u00f6nste Lied,", "tokens": ["\u201e", "nun", "sing'", "ich", "das", "drit\u00b7te", ",", "das", "sch\u00f6ns\u00b7te", "Lied", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das werd\u2019 ich nimmer zu singen m\u00fcd:", "tokens": ["Das", "werd'", "ich", "nim\u00b7mer", "zu", "sin\u00b7gen", "m\u00fcd", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "K\u00f6nig Sifrid liegt in seim rothen Blute!", "tokens": ["K\u00f6\u00b7nig", "Sif\u00b7rid", "liegt", "in", "seim", "ro\u00b7then", "Blu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und aber: liegt in seim rothen Blute!\u201c", "tokens": ["Und", "a\u00b7ber", ":", "liegt", "in", "seim", "ro\u00b7then", "Blu\u00b7te", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$.", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}