{"textgrid.poem.43318": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "1L: So \u00e4ngstlich bebt auf Cremonesersaiten", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So \u00e4ngstlich bebt auf Cremonesersaiten", "tokens": ["So", "\u00e4ngst\u00b7lich", "bebt", "auf", "Cre\u00b7mo\u00b7nes\u00b7er\u00b7sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der z\u00e4rtste Silberton.", "tokens": ["Der", "z\u00e4rts\u00b7te", "Sil\u00b7ber\u00b7ton", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aglaja! \u2013 rief der Silberton.", "tokens": ["Ag\u00b7la\u00b7ja", "!", "\u2013", "rief", "der", "Sil\u00b7ber\u00b7ton", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aglaja! \u2013 half der Nachhall sanft verbreiten.", "tokens": ["Ag\u00b7la\u00b7ja", "!", "\u2013", "half", "der", "Nach\u00b7hall", "sanft", "ver\u00b7brei\u00b7ten", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Umsonst, Aglaja war entflohn.", "tokens": ["Um\u00b7sonst", ",", "Ag\u00b7la\u00b7ja", "war", "ent\u00b7flohn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbach, Pan schlich l\u00e4ngst ihr nach! der Frevler hat sie schon!", "tokens": ["\u00bb", "ach", ",", "Pan", "schlich", "l\u00e4ngst", "ihr", "nach", "!", "der", "Frev\u00b7ler", "hat", "sie", "schon", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NE", "ADJD", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach, Acidalia! blick her von deinem Thron!", "tokens": ["Ach", ",", "A\u00b7ci\u00b7da\u00b7lia", "!", "blick", "her", "von", "dei\u00b7nem", "Thron", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$.", "VVIMP", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Soll sie nach langen Ewigkeiten", "tokens": ["Soll", "sie", "nach", "lan\u00b7gen", "E\u00b7wig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nur itzt nicht l\u00e4nger uns begleiten?", "tokens": ["Nur", "itzt", "nicht", "l\u00e4n\u00b7ger", "uns", "be\u00b7glei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Zwo Grazien sind aller Welt zum Hohn,", "tokens": ["Zwo", "Gra\u00b7zi\u00b7en", "sind", "al\u00b7ler", "Welt", "zum", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und ach! die dritte hat er schon! \u2013\u00ab", "tokens": ["Und", "ach", "!", "die", "drit\u00b7te", "hat", "er", "schon", "!", "\u2013", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "XY", "$.", "ART", "ADJA", "VAFIN", "PPER", "ADV", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So klagten sie. Umsonst! Aglaja war entflohn.", "tokens": ["So", "klag\u00b7ten", "sie", ".", "Um\u00b7sonst", "!", "Ag\u00b7la\u00b7ja", "war", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "$.", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn stellten sie sich gleich, den R\u00e4uber auszusp\u00e4hen,", "tokens": ["Denn", "stell\u00b7ten", "sie", "sich", "gleich", ",", "den", "R\u00e4u\u00b7ber", "aus\u00b7zu\u00b7sp\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So zitterten sie doch f\u00fcr Furcht, ihn nur zu sehen.", "tokens": ["So", "zit\u00b7ter\u00b7ten", "sie", "doch", "f\u00fcr", "Furcht", ",", "ihn", "nur", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Itzt bog ich schlau an ihrem Hals mich langsam \u00fcber,", "tokens": ["Itzt", "bog", "ich", "schlau", "an", "ih\u00b7rem", "Hals", "mich", "lang\u00b7sam", "\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "PPER", "ADJD", "APPR", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und stahl ihr schnell ein M\u00e4ulchen ab;", "tokens": ["Und", "stahl", "ihr", "schnell", "ein", "M\u00e4ul\u00b7chen", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Itzt bog sie unvermerkt den Hals zu mir her\u00fcber,", "tokens": ["Itzt", "bog", "sie", "un\u00b7ver\u00b7merkt", "den", "Hals", "zu", "mir", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und jedes nahm den Ku\u00df auf halben Weg sich ab,", "tokens": ["Und", "je\u00b7des", "nahm", "den", "Ku\u00df", "auf", "hal\u00b7ben", "Weg", "sich", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den jedes nahm und jedes gab.", "tokens": ["Den", "je\u00b7des", "nahm", "und", "je\u00b7des", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVFIN", "KON", "PIAT", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was! rief ich, lose R\u00e4uberinnen!", "tokens": ["Was", "!", "rief", "ich", ",", "lo\u00b7se", "R\u00e4u\u00b7be\u00b7rin\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "$.", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sollte sie Aglaja seyn?", "tokens": ["Wie", "soll\u00b7te", "sie", "Ag\u00b7la\u00b7ja", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "NE", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ihr irrt euch sehr, ihr Huldg\u00f6ttinnen!", "tokens": ["Ihr", "irrt", "euch", "sehr", ",", "ihr", "Huld\u00b7g\u00f6t\u00b7tin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr Grazien ist das nicht fein!", "tokens": ["F\u00fcr", "Gra\u00b7zi\u00b7en", "ist", "das", "nicht", "fein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PDS", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gebt Chloen mir zur\u00fcck! Betrogne, sie ist mein!", "tokens": ["Gebt", "Chloen", "mir", "zu\u00b7r\u00fcck", "!", "Be\u00b7trog\u00b7ne", ",", "sie", "ist", "mein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "PPER", "PTKVZ", "$.", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Warum willst du zu Chloen eilen?", "tokens": ["Wa\u00b7rum", "willst", "du", "zu", "Chloen", "ei\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Begl\u00fcckter Sterblicher, Aglaja liebet dich.", "tokens": ["Be\u00b7gl\u00fcck\u00b7ter", "Sterb\u00b7li\u00b7cher", ",", "Ag\u00b7la\u00b7ja", "lie\u00b7bet", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NE", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "K\u00fc\u00df itzt einmal statt Chloen mich;", "tokens": ["K\u00fc\u00df", "itzt", "ein\u00b7mal", "statt", "Chloen", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "NE", "PPER", "$."], "meter": "--+-+--", "measure": "anapaest.init"}, "line.4": {"text": "W\u00fcnsch nicht dein M\u00e4dchen zu ereilen:", "tokens": ["W\u00fcnsch", "nicht", "dein", "M\u00e4d\u00b7chen", "zu", "er\u00b7ei\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich, eine G\u00f6ttinn, liebe dich.", "tokens": ["Ich", ",", "ei\u00b7ne", "G\u00f6t\u00b7tinn", ",", "lie\u00b7be", "dich", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sch\u00fcchtern sah ich die Huldg\u00f6ttinn an.", "tokens": ["Sch\u00fcch\u00b7tern", "sah", "ich", "die", "Huld\u00b7g\u00f6t\u00b7tinn", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Auf ihren Wangen sprach Entz\u00fccken,", "tokens": ["Auf", "ih\u00b7ren", "Wan\u00b7gen", "sprach", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Jugend und Gef\u00fchl aus den versch\u00e4mten Blicken.", "tokens": ["Und", "Ju\u00b7gend", "und", "Ge\u00b7f\u00fchl", "aus", "den", "ver\u00b7sch\u00e4m\u00b7ten", "Bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "O Chloe, meine Lust, mein Gl\u00fcck! \u2013", "tokens": ["O", "Chloe", ",", "mei\u00b7ne", "Lust", ",", "mein", "Gl\u00fcck", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gebt meine Chloe mir zur\u00fcck!", "tokens": ["Gebt", "mei\u00b7ne", "Chloe", "mir", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ist die\u00df Aglajens Mund und Blick?", "tokens": ["Ist", "die\u00df", "Ag\u00b7la\u00b7jens", "Mund", "und", "Blick", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "NN", "NN", "KON", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Da! nehmt die Huldg\u00f6ttinn zur\u00fcck.", "tokens": ["Da", "!", "nehmt", "die", "Huld\u00b7g\u00f6t\u00b7tinn", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "So \u00e4ngstlich bebt auf Cremonesersaiten", "tokens": ["So", "\u00e4ngst\u0