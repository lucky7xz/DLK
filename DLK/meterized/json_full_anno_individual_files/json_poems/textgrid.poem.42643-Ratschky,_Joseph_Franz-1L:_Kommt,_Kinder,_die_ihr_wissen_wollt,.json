{"textgrid.poem.42643": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kommt, Kinder, die ihr wissen wollt,", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt, Kinder, die ihr wissen wollt,", "tokens": ["Kommt", ",", "Kin\u00b7der", ",", "die", "ihr", "wis\u00b7sen", "wollt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was \u00fcber euern K\u00f6pfen rollt,", "tokens": ["Was", "\u00fc\u00b7ber", "eu\u00b7ern", "K\u00f6p\u00b7fen", "rollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie's steh' um Sonn- und Mondenlicht,", "tokens": ["Wie's", "steh'", "um", "Sonn", "und", "Mon\u00b7den\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6rt eures Pfarrers Unterricht!", "tokens": ["H\u00f6rt", "eu\u00b7res", "Pfar\u00b7rers", "Un\u00b7ter\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Erde Nachtlicht, wie bekannt,", "tokens": ["Der", "Er\u00b7de", "Nacht\u00b7licht", ",", "wie", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird ", "tokens": ["Wird"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Und was euch oftmals Kraut und Kohl", "tokens": ["Und", "was", "euch", "oft\u00b7mals", "Kraut", "und", "Kohl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Versengt, heisst Sonne, ", "tokens": ["Ver\u00b7sengt", ",", "heisst", "Son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Der Mond, ", "tokens": ["Der", "Mond", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Zum Schein den Herrn vom Hause dar:", "tokens": ["Zum", "Schein", "den", "Herrn", "vom", "Hau\u00b7se", "dar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch muss er, wie in unsrer Welt,", "tokens": ["Doch", "muss", "er", ",", "wie", "in", "uns\u00b7rer", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "PWAV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Meist thun, was seinem Weib gef\u00e4llt.", "tokens": ["Meist", "thun", ",", "was", "sei\u00b7nem", "Weib", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Kaum steigt Frau Sonn' in ihrem Lauf", "tokens": ["Kaum", "steigt", "Frau", "Sonn'", "in", "ih\u00b7rem", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am hohen Himmel stolz herauf,", "tokens": ["Am", "ho\u00b7hen", "Him\u00b7mel", "stolz", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So macht der arme Hauspatron", "tokens": ["So", "macht", "der", "ar\u00b7me", "Haus\u00b7pa\u00b7tron"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich \u00fcber Hals und Kopf davon.", "tokens": ["Sich", "\u00fc\u00b7ber", "Hals", "und", "Kopf", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn seht! wie sie einherspatziert,", "tokens": ["Denn", "seht", "!", "wie", "sie", "ein\u00b7her\u00b7spat\u00b7ziert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit goldnen Spitzen schamarrirt,", "tokens": ["Mit", "gold\u00b7nen", "Spit\u00b7zen", "scha\u00b7mar\u00b7rirt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Indess ihn, um und um befleckt,", "tokens": ["In\u00b7dess", "ihn", ",", "um", "und", "um", "be\u00b7fleckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUI", "KON", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Kleid von Flittersilber deckt.", "tokens": ["Ein", "Kleid", "von", "Flit\u00b7ter\u00b7sil\u00b7ber", "deckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie g\u00f6nnt ihm keine bessre Tracht,", "tokens": ["Sie", "g\u00f6nnt", "ihm", "kei\u00b7ne", "bess\u00b7re", "Tracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dennoch sch\u00e4mt in ihrer Pracht", "tokens": ["Und", "den\u00b7noch", "sch\u00e4mt", "in", "ih\u00b7rer", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Stolze seines Anzugs sich:", "tokens": ["Die", "Stol\u00b7ze", "sei\u00b7nes", "An\u00b7zugs", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PRF", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Diess kr\u00e4nkt den Armen bitterlich.", "tokens": ["Diess", "kr\u00e4nkt", "den", "Ar\u00b7men", "bit\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er l\u00e4sst, wenn sie sich drob entzweyn,", "tokens": ["Er", "l\u00e4sst", ",", "wenn", "sie", "sich", "drob", "ent\u00b7zweyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich oft in einen Zweykampf ein:", "tokens": ["Sich", "oft", "in", "ei\u00b7nen", "Zwey\u00b7kampf", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch geht er st\u00e4ts den Abend drauf", "tokens": ["Doch", "geht", "er", "st\u00e4ts", "den", "A\u00b7bend", "drauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "ART", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blutr\u00fcnstig und verschwollen auf.", "tokens": ["Blut\u00b7r\u00fcns\u00b7tig", "und", "ver\u00b7schwol\u00b7len", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Bey solcher Wirthschaft, d\u00e4chte man,", "tokens": ["Bey", "sol\u00b7cher", "Wirth\u00b7schaft", ",", "d\u00e4ch\u00b7te", "man", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sey's um den Nachwuchs schlecht gethan:", "tokens": ["Sey's", "um", "den", "Nach\u00b7wuchs", "schlecht", "ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Allein sie br\u00fcten, wie ihr seht,", "tokens": ["Al\u00b7lein", "sie", "br\u00fc\u00b7ten", ",", "wie", "ihr", "seht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Sternen eine Quantit\u00e4t.", "tokens": ["Von", "Ster\u00b7nen", "ei\u00b7ne", "Quan\u00b7ti\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und dieses ganze Sternenheer", "tokens": ["Und", "die\u00b7ses", "gan\u00b7ze", "Ster\u00b7nen\u00b7heer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Muss nachts Herr Mond oft kreutz und queer,", "tokens": ["Muss", "nachts", "Herr", "Mond", "oft", "kreutz", "und", "que\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NN", "ADV", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich einer alten Kindermagd,", "tokens": ["Gleich", "ei\u00b7ner", "al\u00b7ten", "Kin\u00b7der\u00b7magd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Spatzierenf\u00fchren, bis es tagt.", "tokens": ["Spat\u00b7zie\u00b7ren\u00b7f\u00fch\u00b7ren", ",", "bis", "es", "tagt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Denkt, wie ihn all das qu\u00e4len muss!", "tokens": ["Denkt", ",", "wie", "ihn", "all", "das", "qu\u00e4\u00b7len", "muss", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "PIAT", "PDS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und trotz dem st\u00e4ten Hausverdruss", "tokens": ["Und", "trotz", "dem", "st\u00e4\u00b7ten", "Haus\u00b7ver\u00b7druss"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sieht einen doch der gute Mann", "tokens": ["Sieht", "ei\u00b7nen", "doch", "der", "gu\u00b7te", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fast immer lieb und freundlich an.", "tokens": ["Fast", "im\u00b7mer", "lieb", "und", "freund\u00b7lich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Drum, liebe Christen, die ihr hier", "tokens": ["Drum", ",", "lie\u00b7be", "Chris\u00b7ten", ",", "die", "ihr", "hier"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "$,", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Versammelt seyd, denkt f\u00fcr und f\u00fcr,", "tokens": ["Ver\u00b7sam\u00b7melt", "seyd", ",", "denkt", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "VVFIN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn Zank und Hausverdruss euch qu\u00e4lt,", "tokens": ["Wenn", "Zank", "und", "Haus\u00b7ver\u00b7druss", "euch", "qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was euer Pfarrer euch erz\u00e4hlt!", "tokens": ["Was", "eu\u00b7er", "Pfar\u00b7rer", "euch", "er\u00b7z\u00e4hlt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Tragt's mit Geduld! Fiel doch dem Mond,", "tokens": ["Tragt's", "mit", "Ge\u00b7duld", "!", "Fiel", "doch", "dem", "Mond", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$.", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der hoch in Gottes L\u00fcften wohnt,", "tokens": ["Der", "hoch", "in", "Got\u00b7tes", "L\u00fcf\u00b7ten", "wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und stolz auf unsern Erdenkloss", "tokens": ["Und", "stolz", "auf", "un\u00b7sern", "Er\u00b7denk\u00b7loss"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Heruntersieht, kein besser Loos.", "tokens": ["Her\u00b7un\u00b7ter\u00b7sieht", ",", "kein", "bes\u00b7ser", "Loos", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Kommt, Kinder, die ihr wissen wollt,", "tokens": ["Kommt", ",", "Kin\u00b7der", ",", "die", "ihr", "wis\u00b7sen", "wollt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was \u00fcber euern K\u00f6pfen rollt