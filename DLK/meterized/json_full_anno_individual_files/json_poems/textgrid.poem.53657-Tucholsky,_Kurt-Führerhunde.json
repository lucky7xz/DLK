{"textgrid.poem.53657": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fchrerhunde", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kluge Hunde f\u00fchren \u00fcber die Stra\u00dfen den tastenden Blinden.", "tokens": ["Klu\u00b7ge", "Hun\u00b7de", "f\u00fch\u00b7ren", "\u00fc\u00b7ber", "die", "Stra\u00b7\u00dfen", "den", "tas\u00b7ten\u00b7den", "Blin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wittern und suchen und wissen den richtigen Weg zu finden.", "tokens": ["Wit\u00b7tern", "und", "su\u00b7chen", "und", "wis\u00b7sen", "den", "rich\u00b7ti\u00b7gen", "Weg", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "KON", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}}, "stanza.2": {"line.1": {"text": "Einmal, Blinde, haben euch andre viereinhalb Jahre gef\u00fchrt.", "tokens": ["Ein\u00b7mal", ",", "Blin\u00b7de", ",", "ha\u00b7ben", "euch", "and\u00b7re", "vier\u00b7ein\u00b7halb", "Jah\u00b7re", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VAFIN", "PPER", "PIS", "CARD", "NN", "VVPP", "$."], "meter": "+-+-+--+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Haben geknurrt und gebellt und lebende Menschen dressiert.", "tokens": ["Ha\u00b7ben", "ge\u00b7knurrt", "und", "ge\u00b7bellt", "und", "le\u00b7ben\u00b7de", "Men\u00b7schen", 