{"textgrid.poem.56334": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "4", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die \u00f6ffentliche Meinung schreit und klagt:", "tokens": ["Die", "\u00f6f\u00b7fent\u00b7li\u00b7che", "Mei\u00b7nung", "schreit", "und", "klagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Ihr habt von mir erborget eure Kraft;", "tokens": ["Ihr", "habt", "von", "mir", "er\u00b7bor\u00b7get", "eu\u00b7re", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch mich geschah, was Gro\u00dfes ihr geschafft,", "tokens": ["Durch", "mich", "ge\u00b7schah", ",", "was", "Gro\u00b7\u00dfes", "ihr", "ge\u00b7schafft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "PRELS", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch mich gelang, was siegreich ihr gewagt.", "tokens": ["Durch", "mich", "ge\u00b7lang", ",", "was", "sieg\u00b7reich", "ihr", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "PWS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und nun ich euch erh\u00f6ht, wollt ihr als Magd", "tokens": ["Und", "nun", "ich", "euch", "er\u00b7h\u00f6ht", ",", "wollt", "ihr", "als", "Magd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "PPER", "VVPP", "$,", "VMFIN", "PPER", "KOUS", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mich z\u00fcchtigen mit Ruten und mit Haft;", "tokens": ["Mich", "z\u00fcch\u00b7ti\u00b7gen", "mit", "Ru\u00b7ten", "und", "mit", "Haft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr sch\u00e4mt euch fl\u00fcchtiger Genossenschaft", "tokens": ["Ihr", "sch\u00e4mt", "euch", "fl\u00fcch\u00b7ti\u00b7ger", "Ge\u00b7nos\u00b7sen\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und habt mir, eurer Herrin, widersagt?", "tokens": ["Und", "habt", "mir", ",", "eu\u00b7rer", "Her\u00b7rin", ",", "wi\u00b7der\u00b7sagt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und doch, ihr h\u00f6rtet meine Donner rollen,", "tokens": ["Und", "doch", ",", "ihr", "h\u00f6r\u00b7tet", "mei\u00b7ne", "Don\u00b7ner", "rol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und der Kolo\u00df der Zeit war schon zerstoben,", "tokens": ["Und", "der", "Ko\u00b7lo\u00df", "der", "Zeit", "war", "schon", "zer\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN