{"textgrid.poem.62906": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als mit der Leuchte Diogen,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als mit der Leuchte Diogen,", "tokens": ["Als", "mit", "der", "Leuch\u00b7te", "Dio\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um einen Menschen auszusp\u00fcren,", "tokens": ["Um", "ei\u00b7nen", "Men\u00b7schen", "aus\u00b7zu\u00b7sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch alle Gassen von Athen", "tokens": ["Durch", "al\u00b7le", "Gas\u00b7sen", "von", "A\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR", "NE"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Umherzog, stie\u00df ihm an den Th\u00fcren", "tokens": ["Um\u00b7her\u00b7zog", ",", "stie\u00df", "ihm", "an", "den", "Th\u00fc\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des Tempels der Barmherzigkeit", "tokens": ["Des", "Tem\u00b7pels", "der", "Barm\u00b7her\u00b7zig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Ein Priester auf: Herr, eine Gabe!", "tokens": ["Ein", "Pries\u00b7ter", "auf", ":", "Herr", ",", "ei\u00b7ne", "Ga\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Rief Diogen; nur einen Deut,", "tokens": ["Rief", "Dio\u00b7gen", ";", "nur", "ei\u00b7nen", "Deut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Da\u00df ich mein schwaches Alter labe!", "tokens": ["Da\u00df", "ich", "mein", "schwa\u00b7ches", "Al\u00b7ter", "la\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mein Segen gn\u00fcge dir mein Sohn,", "tokens": ["Mein", "Se\u00b7gen", "gn\u00fc\u00b7ge", "dir", "mein", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Versetzt der Pfaff und schleicht davon.", "tokens": ["Ver\u00b7setzt", "der", "Pfaff", "und", "schleicht", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "KON", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der Pilger trat vor einen Laden", "tokens": ["Der", "Pil\u00b7ger", "trat", "vor", "ei\u00b7nen", "La\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Voll Spangen, F\u00e4chern und Pommaden,", "tokens": ["Voll", "Span\u00b7gen", ",", "F\u00e4\u00b7chern", "und", "Pom\u00b7ma\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und sprach zu einem sch\u00f6nen Weib:", "tokens": ["Und", "sprach", "zu", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Weib", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ihr kauft so viel zum Zeitvertreib,", "tokens": ["Ihr", "kauft", "so", "viel", "zum", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Madam, wollt ihr nicht auch des Armen,", "tokens": ["Ma\u00b7dam", ",", "wollt", "ihr", "nicht", "auch", "des", "Ar\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Der bald verhungert, euch erbarmen?", "tokens": ["Der", "bald", "ver\u00b7hun\u00b7gert", ",", "euch", "er\u00b7bar\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Mich jammert, Alter, deine Noth,", "tokens": ["Mich", "jam\u00b7mert", ",", "Al\u00b7ter", ",", "dei\u00b7ne", "Noth", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Da, kaufe dir ein Dreyerbrod,", "tokens": ["Da", ",", "kau\u00b7fe", "dir", "ein", "Drey\u00b7er\u00b7brod", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Sie sprachs und gab im Augenblicke", "tokens": ["Sie", "sprachs", "und", "gab", "im", "Au\u00b7gen\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Dem Kr\u00e4mer zwanzig Silberst\u00fccke", "tokens": ["Dem", "Kr\u00e4\u00b7mer", "zwan\u00b7zig", "Sil\u00b7ber\u00b7st\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "F\u00fcr ihres M\u00f6pschens Halsband hin.", "tokens": ["F\u00fcr", "ih\u00b7res", "M\u00f6p\u00b7schens", "Hals\u00b7band", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der Weise kratzt sich in den Haaren", "tokens": ["Der", "Wei\u00b7se", "kratzt", "sich", "in", "den", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und geht. Der Prinz von Salamin", "tokens": ["Und", "geht", ".", "Der", "Prinz", "von", "Sa\u00b7la\u00b7min"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Kam eben in die Stadt gefahren.", "tokens": ["Kam", "e\u00b7ben", "in", "die", "Stadt", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Diogenes lief zu ihm hin;", "tokens": ["Dio\u00b7ge\u00b7nes", "lief", "zu", "ihm", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.26": {"text": "Er hieng sich an den goldnen Wagen.", "tokens": ["Er", "hieng", "sich", "an", "den", "gold\u00b7nen", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "\u00bbhalt, Sohn der G\u00f6tter, h\u00f6re mich!\u00ab", "tokens": ["\u00bb", "halt", ",", "Sohn", "der", "G\u00f6t\u00b7ter", ",", "h\u00f6\u00b7re", "mich", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Fort, Schlingel, hie\u00df es, packe dich,", "tokens": ["Fort", ",", "Schlin\u00b7gel", ",", "hie\u00df", "es", ",", "pa\u00b7cke", "dich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Sonst la\u00df ich dich zu todte schlagen.", "tokens": ["Sonst", "la\u00df", "ich", "dich", "zu", "tod\u00b7te", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PRF", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Ein Sklave, der von ferne stand,", "tokens": ["Ein", "Skla\u00b7ve", ",", "der", "von", "fer\u00b7ne", "stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Sprang auf und ri\u00df mit edler Hitze", "tokens": ["Sprang", "auf", "und", "ri\u00df", "mit", "ed\u00b7ler", "Hit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Den Alten weg, und seine Hand", "tokens": ["Den", "Al\u00b7ten", "weg", ",", "und", "sei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Warf ihm zween Heller in die M\u00fctze.", "tokens": ["Warf", "ihm", "zween", "Hel\u00b7ler", "in", "die", "M\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Ihr G\u00f6tter! rief der weise Mann,", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "!", "rief", "der", "wei\u00b7se", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Mehr als ein K\u00f6nig geben kann,", "tokens": ["Mehr", "als", "ein", "K\u00f6\u00b7nig", "ge\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Gab dieser mir; nun sterb ich gerne.", "tokens": ["Gab", "die\u00b7ser", "mir", ";", "nun", "sterb", "ich", "ger\u00b7ne", "."], "t