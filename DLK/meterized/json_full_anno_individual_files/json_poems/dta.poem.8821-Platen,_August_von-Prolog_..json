{"dta.poem.8821": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Prolog .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Tag' um Tage so verstreichen?", "tokens": ["Tag'", "um", "Ta\u00b7ge", "so", "ver\u00b7strei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt, verliebte M\u00fcssigg\u00e4nger,", "tokens": ["Kommt", ",", "ver\u00b7lieb\u00b7te", "M\u00fcs\u00b7sig\u00b7g\u00e4n\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Trinker, kommt, die Stunden schleichen:", "tokens": ["Trin\u00b7ker", ",", "kommt", ",", "die", "Stun\u00b7den", "schlei\u00b7chen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sammelt rings euch um den S\u00e4nger,", "tokens": ["Sam\u00b7melt", "rings", "euch", "um", "den", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df er sey bey seines Gleichen!", "tokens": ["Da\u00df", "er", "sey", "bey", "sei\u00b7nes", "Glei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was Vern\u00fcnft'ge hoch verehren,", "tokens": ["Was", "Ver\u00b7n\u00fcnft'\u00b7ge", "hoch", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Taugte Jedem, der's verst\u00fcnde;", "tokens": ["Taug\u00b7te", "Je\u00b7dem", ",", "der's", "ver\u00b7st\u00fcn\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "$,", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch zu schwer sind ihre Lehren,", "tokens": ["Doch", "zu", "schwer", "sind", "ih\u00b7re", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKA", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu verborgen ihre Gr\u00fcnde:", "tokens": ["Zu", "ver\u00b7bor\u00b7gen", "ih\u00b7re", "Gr\u00fcn\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVPP", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie, die von der Tugend zehren,", "tokens": ["Sie", ",", "die", "von", "der", "Tu\u00b7gend", "zeh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lie\u00dfen \u00fcbrig uns die S\u00fcnde.", "tokens": ["Lie\u00b7\u00dfen", "\u00fcb\u00b7rig", "uns", "die", "S\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was wir f\u00fchlen, was wir denken,", "tokens": ["Was", "wir", "f\u00fch\u00b7len", ",", "was", "wir", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halten d'rum wir im Geheimen,", "tokens": ["Hal\u00b7ten", "d'\u00b7rum", "wir", "im", "Ge\u00b7hei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "PPER", "APPRART", "NN", "$,"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Denn wer m\u00f6cht' ein Korn versenken,", "tokens": ["Denn", "wer", "m\u00f6cht'", "ein", "Korn", "ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wenn's noch nicht vermag zu keimen?", "tokens": ["Wenn's", "noch", "nicht", "ver\u00b7mag", "zu", "kei\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00dft inde\u00df uns in den Schenken", "tokens": ["La\u00dft", "in\u00b7de\u00df", "uns", "in", "den", "Schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebliche Gedichte reimen!", "tokens": ["Lieb\u00b7li\u00b7che", "Ge\u00b7dich\u00b7te", "rei\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}