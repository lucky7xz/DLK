{"dta.poem.12518": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XxVIII.  \n  O   Weimar!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Immer noch trinken sie Abends den Thee, und", "tokens": ["Im\u00b7mer", "noch", "trin\u00b7ken", "sie", "A\u00b7bends", "den", "Thee", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "KON"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "plaudern zusammen", "tokens": ["plau\u00b7dern", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word"], "pos": ["VVFIN", "PTKVZ"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ueber den Strumpf, den die Hahn oder die", "tokens": ["Ue\u00b7ber", "den", "Strumpf", ",", "den", "die", "Hahn", "o\u00b7der", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "KON", "ART"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Paalzow gestrickt:", "tokens": ["Paal\u00b7zow", "ge\u00b7strickt", ":"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Doch, statt Spiritus, reicht man die abgeblasene", "tokens": ["Doch", ",", "statt", "Spi\u00b7ri\u00b7tus", ",", "reicht", "man", "die", "ab\u00b7ge\u00b7bla\u00b7se\u00b7ne"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "NE", "$,", "VVFIN", "PIS", "ART", "ADJA"], "meter": "--+--+--+-+--", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Milch jetzt,", "tokens": ["Milch", "jetzt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Die ein Gewitter vor zehn Jahren schon", "tokens": ["Die", "ein", "Ge\u00b7wit\u00b7ter", "vor", "zehn", "Jah\u00b7ren", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "CARD", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "sauer gemacht.", "tokens": ["sau\u00b7er", "ge\u00b7macht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}