{"dta.poem.15717": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht auf die eigne, nur auf seines Schiffes Noth", "tokens": ["Nicht", "auf", "die", "eig\u00b7ne", ",", "nur", "auf", "sei\u00b7nes", "Schif\u00b7fes", "Noth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "$,", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Wohlfart ist bedacht der wachsame Pilot.", "tokens": ["Und", "Wohl\u00b7fart", "ist", "be\u00b7dacht", "der", "wach\u00b7sa\u00b7me", "Pi\u00b7lot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sich selbst vergisset er, nur seines Schiffes denkend,", "tokens": ["Sich", "selbst", "ver\u00b7gis\u00b7set", "er", ",", "nur", "sei\u00b7nes", "Schif\u00b7fes", "den\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVFIN", "PPER", "$,", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das stets gef\u00e4rdete durch Klipp' und Woge lenkend.", "tokens": ["Das", "stets", "ge\u00b7f\u00e4r\u00b7de\u00b7te", "durch", "Klipp'", "und", "Wo\u00b7ge", "len\u00b7kend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zur Tiefe blicket er und schauet nach den Sternen,", "tokens": ["Zur", "Tie\u00b7fe", "bli\u00b7cket", "er", "und", "schau\u00b7et", "nach", "den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht Gottes Sch\u00f6pfermacht, nur seine Fahrt zu lernen;", "tokens": ["Nicht", "Got\u00b7tes", "Sch\u00f6p\u00b7fer\u00b7macht", ",", "nur", "sei\u00b7ne", "Fahrt", "zu", "ler\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "NN", "$,", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Merkt auf der Wolken Zug, lauscht auf der L\u00fcfte Flug,", "tokens": ["Merkt", "auf", "der", "Wol\u00b7ken", "Zug", ",", "lauscht", "auf", "der", "L\u00fcf\u00b7te", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob Stille komm' ob Sturm, ob F\u00f6rdrung ob Verzug.", "tokens": ["Ob", "Stil\u00b7le", "komm'", "ob", "Sturm", ",", "ob", "F\u00f6r\u00b7drung", "ob", "Ver\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "KOUS", "NN", "$,", "KOUS", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Entgegensteuernde begr\u00fc\u00dft er nur im Eilen,", "tokens": ["Ent\u00b7ge\u00b7gens\u00b7teu\u00b7ern\u00b7de", "be\u00b7gr\u00fc\u00dft", "er", "nur", "im", "Ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie ihm Wegkundschaft und Neuigkeit ertheilen.", "tokens": ["Da\u00df", "sie", "ihm", "Weg\u00b7kund\u00b7schaft", "und", "Neu\u00b7ig\u00b7keit", "er\u00b7thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und legt er an, so thut ers nicht, vom angenehmen", "tokens": ["Und", "legt", "er", "an", ",", "so", "thut", "ers", "nicht", ",", "vom", "an\u00b7ge\u00b7neh\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PIS", "PTKNEG", "$,", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gestad gelockt, er thuts um Wasser einzunehmen;", "tokens": ["Ge\u00b7stad", "ge\u00b7lockt", ",", "er", "thuts", "um", "Was\u00b7ser", "ein\u00b7zu\u00b7neh\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "PPER", "VVFIN", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und ist zufrieden, wenn er endlich m\u00fcd' und matt", "tokens": ["Und", "ist", "zu\u00b7frie\u00b7den", ",", "wenn", "er", "end\u00b7lich", "m\u00fcd'", "und", "matt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Lebensziel erreicht im Todeshaven hat,", "tokens": ["Das", "Le\u00b7bens\u00b7ziel", "er\u00b7reicht", "im", "To\u00b7des\u00b7ha\u00b7ven", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der \u00e4ngstliche Pilot, der Geist im lecken Schiff", "tokens": ["Der", "\u00e4ngst\u00b7li\u00b7che", "Pi\u00b7lot", ",", "der", "Geist", "im", "le\u00b7cken", "Schiff"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Leibes, seiner M\u00fchn und Sorgen Inbegriff:", "tokens": ["Des", "Lei\u00b7bes", ",", "sei\u00b7ner", "M\u00fchn", "und", "Sor\u00b7gen", "In\u00b7be\u00b7griff", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Begl\u00fcckt, wenn sorgenfrei er einst durch Aetherferne", "tokens": ["Be\u00b7gl\u00fcckt", ",", "wenn", "sor\u00b7gen\u00b7frei", "er", "einst", "durch", "A\u00b7e\u00b7ther\u00b7fer\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "NN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein unzerbrechlich Schiff lenkt, wie ihr Geist die Sterne!", "tokens": ["Ein", "un\u00b7zer\u00b7brech\u00b7lich", "Schiff", "lenkt", ",", "wie", "ihr", "Geist", "die", "Ster\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Begl\u00fcckt die Unschuld auch, die hier sanft schlummernd ruht", "tokens": ["Be\u00b7gl\u00fcckt", "die", "Un\u00b7schuld", "auch", ",", "die", "hier", "sanft", "schlum\u00b7mernd", "ruht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,", "PRELS", "ADV", "ADJD", "ADJD", "VVFIN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Nachen, den ein Schwan zieht spielend durch die Flut!", "tokens": ["Im", "Na\u00b7chen", ",", "den", "ein", "Schwan", "zieht", "spie\u00b7lend", "durch", "die", "Flut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}