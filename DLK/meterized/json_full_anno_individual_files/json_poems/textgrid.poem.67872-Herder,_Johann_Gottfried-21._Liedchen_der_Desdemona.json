{"textgrid.poem.67872": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "21. Liedchen der Desdemona", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Arm' M\u00e4dchen sa\u00df singend, am Waldbaum sa\u00df sie,", "tokens": ["Arm'", "M\u00e4d\u00b7chen", "sa\u00df", "sin\u00b7gend", ",", "am", "Wald\u00b7baum", "sa\u00df", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADJD", "$,", "APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Singt alle, mein Kr\u00e4nzel ist Weide.", "tokens": ["Singt", "al\u00b7le", ",", "mein", "Kr\u00e4n\u00b7zel", "ist", "Wei\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Hand lag am Busen ihr, 's Haupt am Knie,", "tokens": ["Die", "Hand", "lag", "am", "Bu\u00b7sen", "ihr", ",", "'s", "Haupt", "am", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PPER", "$,", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Singt Weide, gr\u00fcne Weide!", "tokens": ["Singt", "Wei\u00b7de", ",", "gr\u00fc\u00b7ne", "Wei\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Strom kalt daneben ihr, murmelt ihr Ach,", "tokens": ["Der", "Strom", "kalt", "da\u00b7ne\u00b7ben", "ihr", ",", "mur\u00b7melt", "ihr", "Ach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PAV", "PPER", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Singt Weide, gr\u00fcne Weide!", "tokens": ["Singt", "Wei\u00b7de", ",", "gr\u00fc\u00b7ne", "Wei\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und Thr\u00e4nenfluth flo\u00df ihr, die Felsen wohl brach,", "tokens": ["Und", "Thr\u00e4\u00b7nen\u00b7fluth", "flo\u00df", "ihr", ",", "die", "Fel\u00b7sen", "wohl", "brach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "' Bitt dich, mach fort, er ist den Augenblick da.", "tokens": ["'", "Bitt", "dich", ",", "mach", "fort", ",", "er", "ist", "den", "Au\u00b7gen\u00b7blick", "da", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "PPER", "$,", "VVFIN", "PTKVZ", "$,", "PPER", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Singt alle, von Weiden mein Kr\u00e4nzlein mu\u00df seyn,", "tokens": ["Singt", "al\u00b7le", ",", "von", "Wei\u00b7den", "mein", "Kr\u00e4nz\u00b7lein", "mu\u00df", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "APPR", "NN", "PPOSAT", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+--+-+++-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Komm niemand und tadl' ihn. Er gef\u00e4llt mir nun so.", "tokens": ["Komm", "nie\u00b7mand", "und", "tadl'", "ihn", ".", "Er", "ge\u00b7f\u00e4llt", "mir", "nun", "so", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KON", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nein das folgt noch nicht. Horch, was klopft?", "tokens": ["Nein", "das", "folgt", "noch", "nicht", ".", "Horch", ",", "was", "klopft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "PDS", "VVFIN", "ADV", "PTKNEG", "$.", "NE", "$,", "PWS", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "AeMILIE.", "tokens": ["A\u00b7e\u00b7MI\u00b7LIE", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "'s ist der Wind.", "tokens": ["'s", "ist", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "++-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "DeSDEM.", "tokens": ["DeS\u00b7DEM", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "Ich nannt' ihn mir treulos. Was antwort't er mir:", "tokens": ["Ich", "nannt'", "ihn", "mir", "treu\u00b7los", ".", "Was", "ant\u00b7wort't", "er", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "ADJD", "$.", "PWS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gefallen mir M\u00e4dchen, die B\u00fcbchen wohl dir \u2013 \u2013", "tokens": ["Ge\u00b7fal\u00b7len", "mir", "M\u00e4d\u00b7chen", ",", "die", "B\u00fcb\u00b7chen", "wohl", "dir", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "NN", "$,", "ART", "NN", "ADV", "PPER", "$(", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "So, nun kannst du gehen. Gute Nacht. Mir jucken meine Augen, das bedeutet Weinen.", "tokens": ["So", ",", "nun", "kannst", "du", "ge\u00b7hen", ".", "Gu\u00b7te", "Nacht", ".", "Mir", "ju\u00b7cken", "mei\u00b7ne", "Au\u00b7gen", ",", "das", "be\u00b7deu\u00b7tet", "Wei\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$.", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "NN", "$."], "meter": "--+-+-+-+-+-+-+-+-+-+-", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "AeMILIE.", "tokens": ["A\u00b7e\u00b7MI\u00b7LIE", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "O nicht doch! \u2013", "tokens": ["O", "nicht", "doch", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "PTKNEG", "ADV", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "[dESDEM:]", "tokens": ["dES\u00b7DEM", ":"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "FM.la", "$.", "$("], "meter": "+", "measure": "single.up"}}, "stanza.10": {"line.1": {"text": "Gute Nacht, gute Nacht! Mir steh der Himmel bei,", "tokens": ["Gu\u00b7te", "Nacht", ",", "gu\u00b7te", "Nacht", "!", "Mir", "steh", "der", "Him\u00b7mel", "bei", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA