{"dta.poem.4368": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gef\u00e4hrlicher Abweg der Vernunft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sahe j\u00fcngst ein weinend Kind, das halb im Grase", "tokens": ["Ich", "sa\u00b7he", "j\u00fcngst", "ein", "wei\u00b7nend", "Kind", ",", "das", "halb", "im", "Gra\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJD", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "lag, halb sa\u00df,", "tokens": ["lag", ",", "halb", "sa\u00df", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Das, mit geschwollnen Augenliedern, mit Wangen, ganz", "tokens": ["Das", ",", "mit", "ge\u00b7schwoll\u00b7nen", "Au\u00b7gen\u00b7lie\u00b7dern", ",", "mit", "Wan\u00b7gen", ",", "ganz"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PDS", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "NN", "$,", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "von Z\u00e4hren na\u00df,", "tokens": ["von", "Z\u00e4h\u00b7ren", "na\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mit Fingern seine Augen rieb, verdrie\u00dflich und erb\u00e4rmlich", "tokens": ["Mit", "Fin\u00b7gern", "sei\u00b7ne", "Au\u00b7gen", "rieb", ",", "ver\u00b7drie\u00df\u00b7lich", "und", "er\u00b7b\u00e4rm\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "flennte,", "tokens": ["flenn\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "So da\u00df man sein Geschrey kaum dulden, es aber auch nicht", "tokens": ["So", "da\u00df", "man", "sein", "Ge\u00b7schrey", "kaum", "dul\u00b7den", ",", "es", "a\u00b7ber", "auch", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PIS", "PPOSAT", "NN", "ADV", "VVINF", "$,", "PPER", "ADV", "ADV", "PTKNEG"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "schweigen k\u00f6nnte.", "tokens": ["schwei\u00b7gen", "k\u00f6nn\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Ich h\u00f6rte, wie ich es, warum es so erb\u00e4rmlich weinte, fragte,", "tokens": ["Ich", "h\u00f6r\u00b7te", ",", "wie", "ich", "es", ",", "wa\u00b7rum", "es", "so", "er\u00b7b\u00e4rm\u00b7lich", "wein\u00b7te", ",", "frag\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "PPER", "$,", "PWAV", "PPER", "ADV", "ADJD", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.10": {"text": "Da\u00df es mir, mit verzognen Lippen, und heiserm Ton, zur", "tokens": ["Da\u00df", "es", "mir", ",", "mit", "ver\u00b7zog\u00b7nen", "Lip\u00b7pen", ",", "und", "hei\u00b7serm", "Ton", ",", "zur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "$,", "APPRART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Antwort sagte:", "tokens": ["Ant\u00b7wort", "sag\u00b7te", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "\u201cich wollte starr die Sonne sehn,", "tokens": ["\u201c", "ich", "woll\u00b7te", "starr", "die", "Son\u00b7ne", "sehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "\u201edaher ist mir so weh geschehn.", "tokens": ["\u201e", "da\u00b7her", "ist", "mir", "so", "weh", "ge\u00b7schehn", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nachdem wir es nun aus der Sonnen, und in den", "tokens": ["Nach\u00b7dem", "wir", "es", "nun", "aus", "der", "Son\u00b7nen", ",", "und", "in", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "$,", "KON", "APPR", "ART"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "nah gelegnen Schatten", "tokens": ["nah", "ge\u00b7leg\u00b7nen", "Schat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Gesetzet, und es allgemach, mit vieler M\u00fch\u2019, beschwichtigt", "tokens": ["Ge\u00b7set\u00b7zet", ",", "und", "es", "all\u00b7ge\u00b7mach", ",", "mit", "vie\u00b7ler", "M\u00fch'", ",", "be\u00b7schwich\u00b7tigt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "$,", "KON", "PPER", "ADV", "$,", "APPR", "PIAT", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "hatten;", "tokens": ["hat\u00b7ten", ";"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Erwegt\u2019 ich dieses Kindes Thun, mit einem ernsten Ueber-", "tokens": ["Er\u00b7wegt'", "ich", "die\u00b7ses", "Kin\u00b7des", "Thun", ",", "mit", "ei\u00b7nem", "erns\u00b7ten", "Ue\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "NN", "$,", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "legen.", "tokens": ["le\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Ich fand in ihm ein lehrend Beyspiel, und ein Erinnrung-", "tokens": ["Ich", "fand", "in", "ihm", "ein", "leh\u00b7rend", "Bey\u00b7spiel", ",", "und", "ein", "Er\u00b7inn\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJD", "NN", "$,", "KON", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "volles Bild,", "tokens": ["vol\u00b7les", "Bild", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Mit einem \u00fcberzeugenden Beweis, zumahl f\u00fcr die, erf\u00fcllt,", "tokens": ["Mit", "ei\u00b7nem", "\u00fc\u00b7berz\u00b7eu\u00b7gen\u00b7den", "Be\u00b7weis", ",", "zu\u00b7mahl", "f\u00fcr", "die", ",", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KOUS", "APPR", "ART", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.10": {"text": "Die, wie die\u00df Kind es mit der Sonnen, mit GOtt es selbst", "tokens": ["Die", ",", "wie", "die\u00df", "Kind", "es", "mit", "der", "Son\u00b7nen", ",", "mit", "Gott", "es", "selbst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "PWAV", "PDS", "NN", "PPER", "APPR", "ART", "NN", "$,", "APPR", "NN", "PPER", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "zu machen pflegen,", "tokens": ["zu", "ma\u00b7chen", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Da sie nicht GOtt in Seinen Werken, worinn sie Jhn doch", "tokens": ["Da", "sie", "nicht", "Gott", "in", "Sei\u00b7nen", "Wer\u00b7ken", ",", "wo\u00b7rinn", "sie", "Jhn", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "APPR", "PPOSAT", "NN", "$,", "PWAV", "PPER", "PPER", "ADV"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.13": {"text": "sehen sollen,", "tokens": ["se\u00b7hen", "sol\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Nein, mit Verachtung dieses Weges, Sein Wesen Selber", "tokens": ["Nein", ",", "mit", "Ver\u00b7ach\u00b7tung", "die\u00b7ses", "We\u00b7ges", ",", "Sein", "We\u00b7sen", "Sel\u00b7ber"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "NN", "PDAT", "NN", "$,", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "sehen wollen.", "tokens": ["se\u00b7hen", "wol\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Die Sonne kann in den von ihr bestrahlten C\u00f6rpern auf", "tokens": ["Die", "Son\u00b7ne", "kann", "in", "den", "von", "ihr", "be\u00b7strahl\u00b7ten", "C\u00f6r\u00b7pern", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "ART", "APPR", "PPOSAT", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "der Erden,", "tokens": ["der", "Er\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Mit vieler Lieblichkeit und Anmuht, gesehn, genossen und", "tokens": ["Mit", "vie\u00b7ler", "Lieb\u00b7lich\u00b7keit", "und", "An\u00b7muht", ",", "ge\u00b7sehn", ",", "ge\u00b7nos\u00b7sen", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,", "VVPP", "$,", "VVPP", "KON"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "gesp\u00fchrt,", "tokens": ["ge\u00b7sp\u00fchrt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "So da\u00df uns, im Genu\u00df, der Geist zu ihren Herrlichkeiten", "tokens": ["So", "da\u00df", "uns", ",", "im", "Ge\u00b7nu\u00df", ",", "der", "Geist", "zu", "ih\u00b7ren", "Herr\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "$,", "APPRART", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "f\u00fchrt,", "tokens": ["f\u00fchrt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Nicht aber ihres Lichtes Quell\u2019, in eignem Glanz, gesehen", "tokens": ["Nicht", "a\u00b7ber", "ih\u00b7res", "Lich\u00b7tes", "Quell'", ",", "in", "eig\u00b7nem", "Glanz", ",", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PTKNEG", "ADV", "PPOSAT", "NN", "NE", "$,", "APPR", "ADJA", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "werden.", "tokens": ["wer\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "F\u00fcr unsre Augen ist allein die durch ihr Licht gewirkte", "tokens": ["F\u00fcr", "uns\u00b7re", "Au\u00b7gen", "ist", "al\u00b7lein", "die", "durch", "ihr", "Licht", "ge\u00b7wirk\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "APPR", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Pracht,", "tokens": ["Pracht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Nicht aber ihres Lichtes Abgrund, ihr flammend Meer", "tokens": ["Nicht", "a\u00b7ber", "ih\u00b7res", "Lich\u00b7tes", "Ab\u00b7grund", ",", "ihr", "flam\u00b7mend", "Meer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "PPOSAT", "ADJA", "NN", "$,", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "von Glanz, gemacht.", "tokens": ["von", "Glanz", ",", "ge\u00b7macht", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Will man die Ordnung der Natur aus Eigensinn und", "tokens": ["Will", "man", "die", "Ord\u00b7nung", "der", "Na\u00b7tur", "aus", "Ei\u00b7gen\u00b7sinn", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ART", "NN", "ART", "NN", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Stolz verkehren;", "tokens": ["Stolz", "ver\u00b7keh\u00b7ren", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "So wird die nahe Straf\u2019 im Jrrthum des Jrrthums Gr\u00f6ss\u2019", "tokens": ["So", "wird", "die", "na\u00b7he", "Straf'", "im", "Jrr\u00b7thum", "des", "Jrr\u00b7thums", "Gr\u00f6ss'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "uns deutlich lehren.", "tokens": ["uns", "deut\u00b7lich", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Wie sonst, so scheinet von der Gottheit auch hier die Sonn\u2019", "tokens": ["Wie", "sonst", ",", "so", "schei\u00b7net", "von", "der", "Got\u00b7theit", "auch", "hier", "die", "Sonn'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "ADV", "VVFIN", "APPR", "ART", "NN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "ein Schatten-Bild,", "tokens": ["ein", "Schat\u00b7ten\u00b7Bild", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Voll Anmuht, Lieblichkeit und Nutzen, doch auch mit ernster", "tokens": ["Voll", "An\u00b7muht", ",", "Lieb\u00b7lich\u00b7keit", "und", "Nut\u00b7zen", ",", "doch", "auch", "mit", "erns\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "NN", "KON", "NN", "$,", "ADV", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Lehr erf\u00fcllt.", "tokens": ["Lehr", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Es will von ihres Lichtes Glanz der undurchdringlich helle", "tokens": ["Es", "will", "von", "ih\u00b7res", "Lich\u00b7tes", "Glanz", "der", "un\u00b7durch\u00b7dring\u00b7lich", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Schein", "tokens": ["Schein"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Genossen, aber nicht erblinzt, erforschet, noch ergr\u00fcbelt", "tokens": ["Ge\u00b7nos\u00b7sen", ",", "a\u00b7ber", "nicht", "er\u00b7blinzt", ",", "er\u00b7for\u00b7schet", ",", "noch", "er\u00b7gr\u00fc\u00b7belt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "PTKNEG", "VVPP", "$,", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "seyn.", "tokens": ["seyn", "."], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Gott hat uns Seine Herrlichkeit, und Seine Weisheit, Lieb\u2019", "tokens": ["Gott", "hat", "uns", "Sei\u00b7ne", "Herr\u00b7lich\u00b7keit", ",", "und", "Sei\u00b7ne", "Weis\u00b7heit", ",", "Lieb'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "VAFIN", "PPER", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "und Macht,", "tokens": ["und", "Macht", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "In Seiner sch\u00f6n- und weisen Werke ganz unausdr\u00fccklich", "tokens": ["In", "Sei\u00b7ner", "sch\u00f6n", "und", "wei\u00b7sen", "Wer\u00b7ke", "ganz", "un\u00b7aus\u00b7dr\u00fcck\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "TRUNC", "KON", "ADJA", "NN", "ADV", "ADJD"], "meter": "-+-+-+----+-+-", "measure": "unknown.measure.penta"}, "line.12": {"text": "sch\u00f6nen Pracht", "tokens": ["sch\u00f6\u00b7nen", "Pracht"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Gezeigt, und offenbahren wollen. Wir aber wollen sie nicht", "tokens": ["Ge\u00b7zeigt", ",", "und", "of\u00b7fen\u00b7bah\u00b7ren", "wol\u00b7len", ".", "Wir", "a\u00b7ber", "wol\u00b7len", "sie", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KON", "VVINF", "VMINF", "$.", "PPER", "ADV", "VMFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+--+-+--+", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "sehn,", "tokens": ["sehn", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Und, um die Gottheit selbst zu finden, auf selbst erfundnem", "tokens": ["Und", ",", "um", "die", "Got\u00b7theit", "selbst", "zu", "fin\u00b7den", ",", "auf", "selbst", "er\u00b7fund\u00b7nem"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "KOUI", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,", "APPR", "ADV", "ADJA"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Wege gehn.", "tokens": ["We\u00b7ge", "gehn", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}