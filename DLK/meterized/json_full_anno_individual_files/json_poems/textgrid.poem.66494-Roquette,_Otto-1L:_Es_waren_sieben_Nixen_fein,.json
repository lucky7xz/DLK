{"textgrid.poem.66494": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es waren sieben Nixen fein,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es waren sieben Nixen fein,", "tokens": ["Es", "wa\u00b7ren", "sie\u00b7ben", "Ni\u00b7xen", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sa\u00dfen im schlanken Kahn,", "tokens": ["Die", "sa\u00b7\u00dfen", "im", "schlan\u00b7ken", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und ruderten im Mondenschein", "tokens": ["Und", "ru\u00b7der\u00b7ten", "im", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Auf des Sees Spiegelbahn.", "tokens": ["Auf", "des", "Sees", "Spie\u00b7gel\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Sie schlugen den Takt, und sangen dazu,", "tokens": ["Sie", "schlu\u00b7gen", "den", "Takt", ",", "und", "san\u00b7gen", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PAV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und weckten des Wiederhalls tr\u00e4umende Ruh,", "tokens": ["Und", "weck\u00b7ten", "des", "Wie\u00b7der\u00b7halls", "tr\u00e4u\u00b7men\u00b7de", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Die Nixen alle sieben.", "tokens": ["Die", "Ni\u00b7xen", "al\u00b7le", "sie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "CARD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nur das j\u00fcngste Nixlein schweigsam blieb,", "tokens": ["Nur", "das", "j\u00fcngs\u00b7te", "Nix\u00b7lein", "schweig\u00b7sam", "blieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "In des Nachens Ecke geduckt,", "tokens": ["In", "des", "Na\u00b7chens", "E\u00b7cke", "ge\u00b7duckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und zum Ufer lauscht es, bald freudig, bald tr\u00fcb,", "tokens": ["Und", "zum", "U\u00b7fer", "lauscht", "es", ",", "bald", "freu\u00b7dig", ",", "bald", "tr\u00fcb", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn der Mond durch die Zweige zuckt.", "tokens": ["Wenn", "der", "Mond", "durch", "die", "Zwei\u00b7ge", "zuckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und die andern meinten, sie sei noch ein Kind,", "tokens": ["Und", "die", "an\u00b7dern", "mein\u00b7ten", ",", "sie", "sei", "noch", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und gar nicht verst\u00e4ndig, wie Nixen sind,", "tokens": ["Und", "gar", "nicht", "ver\u00b7st\u00e4n\u00b7dig", ",", "wie", "Ni\u00b7xen", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADJD", "$,", "PWAV", "NN", "VAFIN", "$,"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Sechse von ihrer sieben.", "tokens": ["Sech\u00b7se", "von", "ih\u00b7rer", "sie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "CARD", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Und die erste sang: Was die Liebe sei,", "tokens": ["Und", "die", "ers\u00b7te", "sang", ":", "Was", "die", "Lie\u00b7be", "sei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "$.", "PWS", "ART", "NN", "VAFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bleib' ewig uns unbewu\u00dft!", "tokens": ["Bleib'", "e\u00b7wig", "uns", "un\u00b7be\u00b7wu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn das Menschenherz pocht ungetreu", "tokens": ["Denn", "das", "Men\u00b7schen\u00b7herz", "pocht", "un\u00b7ge\u00b7treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "In der falschen, betr\u00fcglichen Brust.", "tokens": ["In", "der", "fal\u00b7schen", ",", "be\u00b7tr\u00fcg\u00b7li\u00b7chen", "Brust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Die Liebe, die Liebe, die sei verbannt!", "tokens": ["Die", "Lie\u00b7be", ",", "die", "Lie\u00b7be", ",", "die", "sei", "ver\u00b7bannt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "VAFIN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Das schwuren sie fest sich in die Hand,", "tokens": ["Das", "schwu\u00b7ren", "sie", "fest", "sich", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sechse von ihrer sieben.", "tokens": ["Sech\u00b7se", "von", "ih\u00b7rer", "sie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "CARD", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Die zweite sang: Was ist ein Ku\u00df?", "tokens": ["Die", "zwei\u00b7te", "sang", ":", "Was", "ist", "ein", "Ku\u00df", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$.", "PWS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein kurzer, verg\u00e4nglicher Scherz!", "tokens": ["Ein", "kur\u00b7zer", ",", "ver\u00b7g\u00e4ng\u00b7li\u00b7cher", "Scherz", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "O kindische Lust, o schlechter Genu\u00df,", "tokens": ["O", "kin\u00b7di\u00b7sche", "Lust", ",", "o", "schlech\u00b7ter", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du bringst nur Thr\u00e4nen und Schmerz!", "tokens": ["Du", "bringst", "nur", "Thr\u00e4\u00b7nen", "und", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Das K\u00fcssen, das K\u00fcssen, das sei verbannt!", "tokens": ["Das", "K\u00fcs\u00b7sen", ",", "das", "K\u00fcs\u00b7sen", ",", "das", "sei", "ver\u00b7bannt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PDS", "VAFIN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Das schwuren sie fest sich in die Hand,", "tokens": ["Das", "schwu\u00b7ren", "sie", "fest", "sich", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sechse von ihrer sieben.", "tokens": ["Sech\u00b7se", "von", "ih\u00b7rer", "sie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "CARD", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Die dritte sang: Und wenn es vorbei,", "tokens": ["Die", "drit\u00b7te", "sang", ":", "Und", "wenn", "es", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$.", "KON", "KOUS", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Dann ringt aus des Jammers Grund", "tokens": ["Dann", "ringt", "aus", "des", "Jam\u00b7mers", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die arme Seele sich nimmer frei,", "tokens": ["Die", "ar\u00b7me", "See\u00b7le", "sich", "nim\u00b7mer", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und seufzet, und seufzet allstund!", "tokens": ["Und", "seuf\u00b7zet", ",", "und", "seuf\u00b7zet", "alls\u00b7tund", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Das Seufzen, das Seufzen, das sei verbannt!", "tokens": ["Das", "Seuf\u00b7zen", ",", "das", "Seuf\u00b7zen", ",", "das", "sei", "ver\u00b7bannt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PDS", "VAFIN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Das schwuren sie fest sich in die Hand,", "tokens": ["Das", "schwu\u00b7ren", "sie", "fest", "sich", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sechse von ihrer sieben.", "tokens": ["Sech\u00b7se", "von", "ih\u00b7rer", "sie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "CARD", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Die andern sangen jedwede noch was,", "tokens": ["Die", "an\u00b7dern", "san\u00b7gen", "jed\u00b7we\u00b7de", "noch", "was", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PIS", "ADV", "PIS", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die j\u00fcngste allein war stumm,", "tokens": ["Die", "j\u00fcngs\u00b7te", "al\u00b7lein", "war", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die schm\u00fcckt sich mit Blumen, und schaut in's Na\u00df", "tokens": ["Die", "schm\u00fcckt", "sich", "mit", "Blu\u00b7men", ",", "und", "schaut", "in's", "Na\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "NN", "$,", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der Wellen \u2013 wer wei\u00df, warum?", "tokens": ["Der", "Wel\u00b7len", "\u2013", "wer", "wei\u00df", ",", "wa\u00b7rum", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "PWS", "VVFIN", "$,", "PWAV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mit leichtem Wiegen das Schifflein glitt,", "tokens": ["Mit", "leich\u00b7tem", "Wie\u00b7gen", "das", "Schif\u00b7flein", "glitt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Wellen tanzten und sangen mit,", "tokens": ["Die", "Wel\u00b7len", "tanz\u00b7ten", "und", "san\u00b7gen", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Mit sechsen von ihrer sieben.", "tokens": ["Mit", "sech\u00b7sen", "von", "ih\u00b7rer", "sie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "APPR", "PPOSAT", "CARD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Und als sie gelangt an des Ufers Bug,", "tokens": ["Und", "als", "sie", "ge\u00b7langt", "an", "des", "U\u00b7fers", "Bug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wo im Schilfe die Erlen stehn,", "tokens": ["Wo", "im", "Schil\u00b7fe", "die", "Er\u00b7len", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die L\u00fcfte der Nacht mit leisem Zug", "tokens": ["Die", "L\u00fcf\u00b7te", "der", "Nacht", "mit", "lei\u00b7sem", "Zug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von den Bergen zu Thale wehn.", "tokens": ["Von", "den", "Ber\u00b7gen", "zu", "Tha\u00b7le", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Welch Schrecken hat euch, ihr Nixen, erfa\u00dft?", "tokens": ["Welch", "Schre\u00b7cken", "hat", "euch", ",", "ihr", "Ni\u00b7xen", ",", "er\u00b7fa\u00dft", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sie sitzen erstaunt, und machen Rast,", "tokens": ["Sie", "sit\u00b7zen", "er\u00b7staunt", ",", "und", "ma\u00b7chen", "Rast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sechse von ihrer sieben.", "tokens": ["Sech\u00b7se", "von", "ih\u00b7rer", "sie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "CARD", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Und die j\u00fcngste, mit Wasserrosen bekr\u00e4nzt,", "tokens": ["Und", "die", "j\u00fcngs\u00b7te", ",", "mit", "Was\u00b7ser\u00b7ro\u00b7sen", "be\u00b7kr\u00e4nzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Behend an das Ufer sprang,", "tokens": ["Be\u00b7hend", "an", "das", "U\u00b7fer", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der blonde Fischer mondbegl\u00e4nzt,", "tokens": ["Und", "der", "blon\u00b7de", "Fi\u00b7scher", "mond\u00b7be\u00b7gl\u00e4nzt", ","], "token_info": ["w