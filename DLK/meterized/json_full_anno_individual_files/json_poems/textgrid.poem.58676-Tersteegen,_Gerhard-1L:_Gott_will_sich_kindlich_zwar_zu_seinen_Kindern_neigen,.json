{"textgrid.poem.58676": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott will sich kindlich zwar zu seinen Kindern neigen,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott will sich kindlich zwar zu seinen Kindern neigen,", "tokens": ["Gott", "will", "sich", "kind\u00b7lich", "zwar", "zu", "sei\u00b7nen", "Kin\u00b7dern", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr reiner Grund soll ihm ein Haus und Tempel sein;", "tokens": ["Ihr", "rei\u00b7ner", "Grund", "soll", "ihm", "ein", "Haus", "und", "Tem\u00b7pel", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PPER", "ART", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch lehrt die Ehrfurcht bald sich inniglich zu beugen,", "tokens": ["Doch", "lehrt", "die", "Ehr\u00b7furcht", "bald", "sich", "in\u00b7nig\u00b7lich", "zu", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn seine Majest\u00e4t kommt in dies Haus hinein.", "tokens": ["Wenn", "sei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", "kommt", "in", "dies", "Haus", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "APPR