line.3": {"text": "Und wie die V\u00f6gel sungen", "tokens": ["Und", "wie", "die", "V\u00f6\u00b7gel", "sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im dichten Laubgewog,", "tokens": ["Im", "dich\u00b7ten", "Laub\u00b7ge\u00b7wog", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Da kam auf mich hernieder", "tokens": ["Da", "kam", "auf", "mich", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein frischer Hoffnungstraum:", "tokens": ["Ein", "fri\u00b7scher", "Hoff\u00b7nungs\u00b7traum", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Getrost! So gr\u00fcnt auch wieder", "tokens": ["Ge\u00b7trost", "!", "So", "gr\u00fcnt", "auch", "wie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dereinst des Reiches Baum.", "tokens": ["De\u00b7reinst", "des", "Rei\u00b7ches", "Baum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}