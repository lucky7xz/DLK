{"dta.poem.14227": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abschied und Wiedersehen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ein zart Geheimni\u00df webt in stillen R\u00e4umen,", "tokens": ["Ein", "zart", "Ge\u00b7heim\u00b7ni\u00df", "webt", "in", "stil\u00b7len", "R\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Erde l\u00f6st die diamant'nen Schleifen,", "tokens": ["Die", "Er\u00b7de", "l\u00f6st", "die", "di\u00b7a\u00b7mant'\u00b7nen", "Schlei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nach des Himmels s\u00fc\u00dfen Strahlen greifen", "tokens": ["Und", "nach", "des", "Him\u00b7mels", "s\u00fc\u00b7\u00dfen", "Strah\u00b7len", "grei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Blumen, die der Mutter Kleid bes\u00e4umen.", "tokens": ["Die", "Blu\u00b7men", ",", "die", "der", "Mut\u00b7ter", "Kleid", "be\u00b7s\u00e4u\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}