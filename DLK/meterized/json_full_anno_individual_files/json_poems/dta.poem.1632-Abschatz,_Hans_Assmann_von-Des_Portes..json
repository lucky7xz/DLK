{"dta.poem.1632": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Des Portes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts/ was des Himmels Zorn auff unsre Schultern legt/ ", "tokens": ["Nichts", "/", "was", "des", "Him\u00b7mels", "Zorn", "auff", "uns\u00b7re", "Schul\u00b7tern", "legt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$(", "PWS", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was unsre Z\u00e4rtligkeit mit Furcht und Schrecken tr\u00e4gt/", "tokens": ["Was", "uns\u00b7re", "Z\u00e4rt\u00b7lig\u00b7keit", "mit", "Furcht", "und", "Schre\u00b7cken", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "icht Sorge/ Leyd und Qual/ nicht Kummer/ Angst und", "tokens": ["icht", "Sor\u00b7ge", "/", "Leyd", "und", "Qual", "/", "nicht", "Kum\u00b7mer", "/", "Angst", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "NN", "$(", "NN", "KON", "NN", "$(", "PTKNEG", "NN", "$(", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "icht Armutt/ Streit und Ha\u00df/ nicht Brand noch Wassers-", "tokens": ["icht", "Ar\u00b7mutt", "/", "Streit", "und", "Ha\u00df", "/", "nicht", "Brand", "noch", "Was\u00b7ser\u00b7s"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "$(", "NN", "KON", "NN", "$(", "PTKNEG", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "icht Hitze/ K\u00e4lt und Frost/ nicht Hunger/ Mord und Tod/", "tokens": ["icht", "Hit\u00b7ze", "/", "K\u00e4lt", "und", "Frost", "/", "nicht", "Hun\u00b7ger", "/", "Mord", "und", "Tod", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$(", "NN", "KON", "NN", "$(", "PTKNEG", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "leicht sich an Grausamkeit dem schweren Joch der Ehe.", "tokens": ["leicht", "sich", "an", "Grau\u00b7sam\u00b7keit", "dem", "schwe\u00b7ren", "Joch", "der", "E\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "NN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "esetze/ welches dient zum Hencker unsrer Lust/", "tokens": ["e\u00b7set\u00b7ze", "/", "wel\u00b7ches", "dient", "zum", "Hen\u00b7cker", "uns\u00b7rer", "Lust", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PWS", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "urch dessen rauhen Zwang entsteht in unsrer Brust", "tokens": ["urch", "des\u00b7sen", "rau\u00b7hen", "Zwang", "ent\u00b7steht", "in", "uns\u00b7rer", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "erachtung/ Eyffer/ Neyd/ Verdru\u00df und Widerwillen!", "tokens": ["e\u00b7rach\u00b7tung", "/", "Eyf\u00b7fer", "/", "Neyd", "/", "Ver\u00b7dru\u00df", "und", "Wi\u00b7der\u00b7wil\u00b7len", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ef\u00e4ngnis/ welches Leib und Geist zugleich bestrickt/", "tokens": ["e\u00b7f\u00e4ng\u00b7nis", "/", "wel\u00b7ches", "Leib", "und", "Geist", "zu\u00b7gleich", "be\u00b7strickt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PWAT", "NN", "KON", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "ifft/ welches unsre Rast und Ruh zu Grabe schickt/", "tokens": ["ifft", "/", "wel\u00b7ches", "uns\u00b7re", "Rast", "und", "Ruh", "zu", "Gra\u00b7be", "schickt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PWS", "PPOSAT", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "as zu vergleichen steht den bitter-s\u00fcssen Pillen.", "tokens": ["as", "zu", "ver\u00b7glei\u00b7chen", "steht", "den", "bit\u00b7ter\u00b7s\u00fcs\u00b7sen", "Pil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKZU", "VVINF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Man sagt/ als Jupiter Prometheus k\u00fchne That", "tokens": ["Man", "sagt", "/", "als", "Ju\u00b7pi\u00b7ter", "Pro\u00b7me\u00b7theus", "k\u00fch\u00b7ne", "That"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$(", "KOUS", "NN", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur Straff in vollem Grimm und Zorn gezogen hat/", "tokens": ["Zur", "Straff", "in", "vol\u00b7lem", "Grimm", "und", "Zorn", "ge\u00b7zo\u00b7gen", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NE", "KON", "NN", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als sein verfluchter Leib den Vogeln ward zur Speise/", "tokens": ["Als", "sein", "ver\u00b7fluch\u00b7ter", "Leib", "den", "Vo\u00b7geln", "ward", "zur", "Spei\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "VAFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So habe sich noch nicht sein Eyffer abgek\u00fchlt;", "tokens": ["So", "ha\u00b7be", "sich", "noch", "nicht", "sein", "Eyf\u00b7fer", "ab\u00b7ge\u00b7k\u00fchlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "PTKNEG", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Damit man nicht wie vor mit seiner Gottheit spielt/", "tokens": ["Da\u00b7mit", "man", "nicht", "wie", "vor", "mit", "sei\u00b7ner", "Got\u00b7theit", "spielt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "KOKOM", "APPR", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Strafft er die arme Welt auff mehr denn eine Weise.", "tokens": ["Strafft", "er", "die", "ar\u00b7me", "Welt", "auff", "mehr", "denn", "ei\u00b7ne", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es wird ein Weib von ihm den Menschen zugeschickt/", "tokens": ["Es", "wird", "ein", "Weib", "von", "ihm", "den", "Men\u00b7schen", "zu\u00b7ge\u00b7schickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Lieb- und Freundligkeit aus ihren Augen blickt.", "tokens": ["Der", "Lie\u00b7b", "und", "Freund\u00b7lig\u00b7keit", "aus", "ih\u00b7ren", "Au\u00b7gen", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Alabaster Hand tr\u00e4gt alles Ubels Saamen", "tokens": ["Die", "A\u00b7la\u00b7bas\u00b7ter", "Hand", "tr\u00e4gt", "al\u00b7les", "U\u00b7bels", "Saa\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gef\u00fcllet in ein Horn/ Furcht/ Feindschafft/ Traurigkeit/", "tokens": ["Ge\u00b7f\u00fcl\u00b7let", "in", "ein", "Horn", "/", "Furcht", "/", "Feind\u00b7schafft", "/", "Trau\u00b7rig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$(", "NN", "$(", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Sorge samt dem Schmertz/ das Alter/ b\u00f6se Zeit", "tokens": ["Die", "Sor\u00b7ge", "samt", "dem", "Schmertz", "/", "das", "Al\u00b7ter", "/", "b\u00f6\u00b7se", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "ART", "NN", "$(", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und was man auff der Welt hat vor verhasste Nahmen.", "tokens": ["Und", "was", "man", "auff", "der", "Welt", "hat", "vor", "ver\u00b7hass\u00b7te", "Nah\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "APPR", "ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Aus ihrer Stirne macht die Venus Helffenbein/", "tokens": ["Aus", "ih\u00b7rer", "Stir\u00b7ne", "macht", "die", "Ve\u00b7nus", "Helf\u00b7fen\u00b7bein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Apollo fl\u00f6sset ihr beliebte Reden ein/", "tokens": ["A\u00b7pol\u00b7lo", "fl\u00f6s\u00b7set", "ihr", "be\u00b7lieb\u00b7te", "Re\u00b7den", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr Hertze st\u00e4lt Vulcan/ Mars giebet ihr zur Steuer", "tokens": ["Ihr", "Hert\u00b7ze", "st\u00e4lt", "Vul\u00b7can", "/", "Mars", "gie\u00b7bet", "ihr", "zur", "Steu\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVFIN", "NE", "$(", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den k\u00fchn- und frechen Mutt/ den man noch heute schaut.", "tokens": ["Den", "k\u00fchn", "und", "fre\u00b7chen", "Mutt", "/", "den", "man", "noch", "heu\u00b7te", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN", "$(", "ART", "PIS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Mensch erkieset sie alsbald zu seiner Braut/", "tokens": ["Der", "Mensch", "er\u00b7kie\u00b7set", "sie", "als\u00b7bald", "zu", "sei\u00b7ner", "Braut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie wird doch ihm und uns die schn\u00f6de Wahl so theuer!", "tokens": ["Wie", "wird", "doch", "ihm", "und", "uns", "die", "schn\u00f6\u00b7de", "Wahl", "so", "theu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PPER", "KON", "PPER", "ART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Hiervon soll der Tyrann/ der Ehstand/ kommen her:", "tokens": ["Hier\u00b7von", "soll", "der", "Ty\u00b7rann", "/", "der", "Eh\u00b7stand", "/", "kom\u00b7men", "her", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "NN", "$(", "ART", "NN", "$(", "VVFIN", "PTKVZ", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Zu herrschen \u00fcber uns ist einig sein Begehr/", "tokens": ["Zu", "herr\u00b7schen", "\u00fc\u00b7ber", "uns", "ist", "ei\u00b7nig", "sein", "Be\u00b7gehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PPER", "VAFIN", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Freyheit/ seinen Feind/ kan er nicht um sich leiden:", "tokens": ["Die", "Frey\u00b7heit", "/", "sei\u00b7nen", "Feind", "/", "kan", "er", "nicht", "um", "sich", "lei\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPOSAT", "NN", "$(", "VMFIN", "PPER", "PTKNEG", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sein Auge winckt/ es lacht der Mund/ uns lockt die Hand/", "tokens": ["Sein", "Au\u00b7ge", "winckt", "/", "es", "lacht", "der", "Mund", "/", "uns", "lockt", "die", "Hand", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "PPER", "VVFIN", "ART", "NN", "$(", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den/ der zu nahe traut/ umfast ein solches Band/", "tokens": ["Den", "/", "der", "zu", "na\u00b7he", "traut", "/", "um\u00b7fast", "ein", "sol\u00b7ches", "Band", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ART", "PTKA", "ADJD", "VVFIN", "$(", "VVFIN", "ART", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df ihn der blasse Tod allein davon kan scheiden.", "tokens": ["Da\u00df", "ihn", "der", "blas\u00b7se", "Tod", "al\u00b7lein", "da\u00b7von", "kan", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "PAV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "An seiner Seiten h\u00e4lt die M\u00fchsamkeit die Wacht/", "tokens": ["An", "sei\u00b7ner", "Sei\u00b7ten", "h\u00e4lt", "die", "M\u00fch\u00b7sam\u00b7keit", "die", "Wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Flei\u00df/ die Arbeit giebt auff Thun und Wincken Acht.", "tokens": ["Der", "Flei\u00df", "/", "die", "Ar\u00b7beit", "giebt", "auff", "Thun", "und", "Win\u00b7cken", "Acht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An der Trabanten statt pflegt um ihn her zu lauffen", "tokens": ["An", "der", "Tra\u00b7ban\u00b7ten", "statt", "pflegt", "um", "ihn", "her", "zu", "lauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "VVFIN", "APPR", "PPER", "APZR", "PTKZU", "VVINF"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Neyd/ Eyffer/ nebst der Furcht/ die unbekannte Pein/", "tokens": ["Neyd", "/", "Eyf\u00b7fer", "/", "nebst", "der", "Furcht", "/", "die", "un\u00b7be\u00b7kann\u00b7te", "Pein", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"