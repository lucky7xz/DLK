{"dta.poem.12129": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vergi\u00dfmeinnicht, du bl\u00fchst an fremden Baches Bord,", "tokens": ["Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", ",", "du", "bl\u00fchst", "an", "frem\u00b7den", "Ba\u00b7ches", "Bord", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fl\u00fcsterst mir auch hier: Vergi\u00dfmeinnicht! dein Wort.", "tokens": ["Und", "fl\u00fcs\u00b7terst", "mir", "auch", "hier", ":", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "!", "dein", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$.", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sag' an, Vergi\u00dfmeinnicht, durch deinen Mund wer spricht?", "tokens": ["Sag'", "an", ",", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", ",", "durch", "dei\u00b7nen", "Mund", "wer", "spricht", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Liebe, die nie dein vergi\u00dft, vergi\u00df ihr nicht!", "tokens": ["Die", "Lie\u00b7be", ",", "die", "nie", "dein", "ver\u00b7gi\u00dft", ",", "ver\u00b7gi\u00df", "ihr", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"