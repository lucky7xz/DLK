{"textgrid.poem.41181": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Holder S\u00e4nger der Nacht! Sch\u00f6n ist im bebenden", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Holder S\u00e4nger der Nacht! Sch\u00f6n ist im bebenden", "tokens": ["Hol\u00b7der", "S\u00e4n\u00b7ger", "der", "Nacht", "!", "Sch\u00f6n", "ist", "im", "be\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "$.", "NE", "VAFIN", "APPRART", "ADJA"], "meter": "+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Mondenschimmer dein Lied, wenn der gelinde West", "tokens": ["Mon\u00b7den\u00b7schim\u00b7mer", "dein", "Lied", ",", "wenn", "der", "ge\u00b7lin\u00b7de", "West"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Sich im sprossenden Wipfel", "tokens": ["Sich", "im", "spros\u00b7sen\u00b7den", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPRART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "K\u00fchler Maiengeb\u00fcsche wiegt,", "tokens": ["K\u00fch\u00b7ler", "Mai\u00b7en\u00b7ge\u00b7b\u00fc\u00b7sche", "wiegt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Wenn die Gegend umher duftender Knospendrang", "tokens": ["Wenn", "die", "Ge\u00b7gend", "um\u00b7her", "duf\u00b7ten\u00b7der", "Knos\u00b7pen\u00b7drang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PTKVZ", "ADJA", "NN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Still durchathmet, und nur, nur der entfesselte", "tokens": ["Still", "durch\u00b7ath\u00b7met", ",", "und", "nur", ",", "nur", "der", "ent\u00b7fes\u00b7sel\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "KON", "ADV", "$,", "ADV", "ART", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Fernher lispelnde Waldquell", "tokens": ["Fern\u00b7her", "lis\u00b7peln\u00b7de", "Wald\u00b7quell"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Deiner Kehle Begleiter ist.", "tokens": ["Dei\u00b7ner", "Keh\u00b7le", "Be\u00b7glei\u00b7ter", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n ist, S\u00e4nger! dein Lied. Aber wer horchet ihm?", "tokens": ["Sch\u00f6n", "ist", ",", "S\u00e4n\u00b7ger", "!", "dein", "Lied", ".", "A\u00b7ber", "wer", "hor\u00b7chet", "ihm", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "NN", "$.", "PPOSAT", "NN", "$.", "KON", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Buchen ragen um dich, ragen, und horchen nicht.", "tokens": ["Bu\u00b7chen", "ra\u00b7gen", "um", "dich", ",", "ra\u00b7gen", ",", "und", "hor\u00b7chen", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$,", "VVFIN", "$,", "KON", "VVFIN", "PTKNEG", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "H\u00fcgel steigen um dich her,", "tokens": ["H\u00fc\u00b7gel", "stei\u00b7gen", "um", "dich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Triften liegen, und horchen nicht.", "tokens": ["Trif\u00b7ten", "lie\u00b7gen", ",", "und", "hor\u00b7chen", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "VVFIN", "PTKNEG", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Taub ist alles und todt. Ungeh\u00f6rt, unbelohnt", "tokens": ["Taub", "ist", "al\u00b7les", "und", "todt", ".", "Un\u00b7ge\u00b7h\u00f6rt", ",", "un\u00b7be\u00b7lohnt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "VAFIN", "PIS", "KON", "ADJD", "$.", "NN", "$,", "ADJD"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Str\u00f6mt dein heller Gesang dennoch die N\u00e4chte durch,", "tokens": ["Str\u00f6mt", "dein", "hel\u00b7ler", "Ge\u00b7sang", "den\u00b7noch", "die", "N\u00e4ch\u00b7te", "durch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Federbarde, Verschwender", "tokens": ["Fe\u00b7der\u00b7bar\u00b7de", ",", "Ver\u00b7schwen\u00b7der"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Deiner g\u00f6ttlichen Liederkraft!", "tokens": ["Dei\u00b7ner", "g\u00f6tt\u00b7li\u00b7chen", "Lie\u00b7der\u00b7kraft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Auf, und hasse den Hain ohne Gef\u00fchl und Dank!", "tokens": ["Auf", ",", "und", "has\u00b7se", "den", "Hain", "oh\u00b7ne", "Ge\u00b7f\u00fchl", "und", "Dank", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KON", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Auf, und lenke den Flug milderen Gegenden,", "tokens": ["Auf", ",", "und", "len\u00b7ke", "den", "Flug", "mil\u00b7de\u00b7ren", "Ge\u00b7gen\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--++-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und verdienteren Zeugen", "tokens": ["Und", "ver\u00b7dien\u00b7te\u00b7ren", "Zeu\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Deiner reizenden K\u00fcnste zu!", "tokens": ["Dei\u00b7ner", "rei\u00b7zen\u00b7den", "K\u00fcns\u00b7te", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Wo manch d\u00fcrstendes Ohr, S\u00e4nger! dich ganz versteh't;", "tokens": ["Wo", "manch", "d\u00fcrs\u00b7ten\u00b7des", "Ohr", ",", "S\u00e4n\u00b7ger", "!", "dich", "ganz", "ver\u00b7steh't", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "$,", "NN", "$.", "PPER", "ADV", "VVPP", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wo manch f\u00fchlendes Herz deinem Geseufze schmilzt,", "tokens": ["Wo", "manch", "f\u00fch\u00b7len\u00b7des", "Herz", "dei\u00b7nem", "Ge\u00b7seuf\u00b7ze", "schmilzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und vom z\u00e4rtlichen Auge", "tokens": ["Und", "vom", "z\u00e4rt\u00b7li\u00b7chen", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Deinen Klagen entgegenthau't.", "tokens": ["Dei\u00b7nen", "Kla\u00b7gen", "ent\u00b7ge\u00b7gent\u00b7hau'", "t."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["PPOSAT", "NN", "VVFIN", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Oder liebst du den Hain ohne Gef\u00fchl und Dank,", "tokens": ["O\u00b7der", "liebst", "du", "den", "Hain", "oh\u00b7ne", "Ge\u00b7f\u00fchl", "und", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Willst du bleiben, so schweig', schwelge dich satt und fett", "tokens": ["Willst", "du", "blei\u00b7ben", ",", "so", "schweig'", ",", "schwel\u00b7ge", "dich", "satt", "und", "fett"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "An dem Sommergew\u00fcrme,", "tokens": ["An", "dem", "Som\u00b7mer\u00b7ge\u00b7w\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Buhl' und schn\u00e4ble die Tage durch,", "tokens": ["Buhl'", "und", "schn\u00e4b\u00b7le", "die", "Ta\u00b7ge", "durch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Und durchschlumm're die Nacht an der gef\u00e4lligsten", "tokens": ["Und", "durch\u00b7schlum\u00b7m'\u00b7re", "die", "Nacht", "an", "der", "ge\u00b7f\u00e4l\u00b7ligs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Freundinn Seite! Warum folgtest du, S\u00e4nger! nicht", "tokens": ["Freun\u00b7dinn", "Sei\u00b7te", "!", "Wa\u00b7rum", "folg\u00b7test", "du", ",", "S\u00e4n\u00b7ger", "!", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "NN", "$.", "PWAV", "VVFIN", "PPER", "$,", "NN", "$.", "PTKNEG"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Andern V\u00f6geln des \u00f6den", "tokens": ["An\u00b7dern", "V\u00f6\u00b7geln", "des", "\u00f6\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Haines ohne Gef\u00fchl und Dank? \u2013", "tokens": ["Hai\u00b7nes", "oh\u00b7ne", "Ge\u00b7f\u00fchl", "und", "Dank", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Doch du bleibest und str\u00f6m'st deine Ges\u00e4nge fort,", "tokens": ["Doch", "du", "blei\u00b7best", "und", "str\u00f6m'st", "dei\u00b7ne", "Ge\u00b7s\u00e4n\u00b7ge", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "H\u00f6r'st mein Warnen nicht an. Ha, ich verstehe dich!", "tokens": ["H\u00f6r'st", "mein", "War\u00b7nen", "nicht", "an", ".", "Ha", ",", "ich", "ver\u00b7ste\u00b7he", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "$.", "ITJ", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Zeugen sind dir entbehrlich,", "tokens": ["Zeu\u00b7gen", "sind", "dir", "ent\u00b7behr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Federbarde! Du sing'st f\u00fcr dich.", "tokens": ["Fe\u00b7der\u00b7bar\u00b7de", "!", "Du", "sing'st", "f\u00fcr", "dich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "APPR", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "O so singe denn fort, sicher der G\u00f6ttlichkeit", "tokens": ["O", "so", "sin\u00b7ge", "denn", "fort", ",", "si\u00b7cher", "der", "G\u00f6tt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "ADV", "PTKVZ", "$,", "ADJD", "ART", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Deiner Kl\u00e4nge! Geneu\u00df einsam, geneu\u00df dich selbst,", "tokens": ["Dei\u00b7ner", "Kl\u00e4n\u00b7ge", "!", "Ge\u00b7neu\u00df", "ein\u00b7sam", ",", "ge\u00b7neu\u00df", "dich", "selbst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word