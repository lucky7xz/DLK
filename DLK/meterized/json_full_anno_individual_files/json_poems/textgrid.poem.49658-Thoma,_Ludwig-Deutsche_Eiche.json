unct"], "pos": ["VVFIN", "PIS", "ADJD", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Weil die \u00c4ste viel zarter sind.", "tokens": ["Weil", "die", "\u00c4s\u00b7te", "viel", "zar\u00b7ter", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.26": {"text": "Und die Krone wiegt sich im Wind.", "tokens": ["Und", "die", "Kro\u00b7ne", "wiegt", "sich", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}