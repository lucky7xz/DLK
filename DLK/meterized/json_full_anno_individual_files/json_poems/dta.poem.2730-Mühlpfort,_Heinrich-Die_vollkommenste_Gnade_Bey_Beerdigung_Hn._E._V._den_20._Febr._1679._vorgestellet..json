{"dta.poem.2730": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die vollkommenste Gnade/  \n Bey Beerdigung Hn. E. V. den 20. Febr.  \n 1679. vorgestellet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein pr\u00e4chtig Wort: Er steht in hohen Gnaden/", "tokens": ["Es", "ist", "ein", "pr\u00e4ch\u00b7tig", "Wort", ":", "Er", "steht", "in", "ho\u00b7hen", "Gna\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist ein herrlich Ding bey F\u00fcrsten seyn gesehn.", "tokens": ["Es", "ist", "ein", "herr\u00b7lich", "Ding", "bey", "F\u00fcrs\u00b7ten", "seyn", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "APPR", "NN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es kan derjenige in nichts als Wollust baden/", "tokens": ["Es", "kan", "der\u00b7je\u00b7ni\u00b7ge", "in", "nichts", "als", "Wol\u00b7lust", "ba\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "APPR", "PIS", "KOKOM", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf den die Westen Wind erlauchter Hulden wehn.", "tokens": ["Auf", "den", "die", "Wes\u00b7ten", "Wind", "er\u00b7lauch\u00b7ter", "Hul\u00b7den", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bekr\u00f6nter H\u00e4upter Gunst ist wie der Thau auf Kr\u00e4utern/", "tokens": ["Be\u00b7kr\u00f6n\u00b7ter", "H\u00e4up\u00b7ter", "Gunst", "ist", "wie", "der", "Thau", "auf", "Kr\u00e4u\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "VAFIN", "KOKOM", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Blumen Laub und Gra\u00df fl\u00f6st neue Kr\u00e4fften ein.", "tokens": ["Der", "Blu\u00b7men", "Laub", "und", "Gra\u00df", "fl\u00f6st", "neu\u00b7e", "Kr\u00e4ff\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es wird sich dessen Ruhm und Ehre mehr erweitern/", "tokens": ["Es", "wird", "sich", "des\u00b7sen", "Ruhm", "und", "Eh\u00b7re", "mehr", "er\u00b7wei\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PRELAT", "NN", "KON", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der darff unangesagt stets bey den Hohen seyn.", "tokens": ["Der", "darff", "un\u00b7an\u00b7ge\u00b7sagt", "stets", "bey", "den", "Ho\u00b7hen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "ADV", "APPR", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er sch\u00f6pffet Pracht und Glantz von seines F\u00fcrsten Sonne/", "tokens": ["Er", "sch\u00f6pf\u00b7fet", "Pracht", "und", "Glantz", "von", "sei\u00b7nes", "F\u00fcrs\u00b7ten", "Son\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den Purpur den er ehrt/ wirckt auch sein Ehren-Kleid.", "tokens": ["Den", "Pur\u00b7pur", "den", "er", "ehrt", "/", "wirckt", "auch", "sein", "Eh\u00b7ren\u00b7Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$(", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er heist des Landes Heil/ der Unterthanen Wonne/", "tokens": ["Er", "heist", "des", "Lan\u00b7des", "Heil", "/", "der", "Un\u00b7ter\u00b7tha\u00b7nen", "Won\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es giebt durch seinen Mund des F\u00fcrsten Hertz Bescheid.", "tokens": ["Es", "giebt", "durch", "sei\u00b7nen", "Mund", "des", "F\u00fcrs\u00b7ten", "Hertz", "Be\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Thron/ vor dem er steht/ kr\u00f6nt ihn mit Lorbeer-Zweigen/", "tokens": ["Der", "Thron", "/", "vor", "dem", "er", "steht", "/", "kr\u00f6nt", "ihn", "mit", "Lor\u00b7beer\u00b7Zwei\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "PRELS", "PPER", "VVFIN", "$(", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wohin sein Fu\u00df nur tritt folgt ihm Gehorsam nach.", "tokens": ["Wo\u00b7hin", "sein", "Fu\u00df", "nur", "tritt", "folgt", "ihm", "Ge\u00b7hor\u00b7sam", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "VVFIN", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sein Wincken giebt an Tag wer reden soll und schweigen/", "tokens": ["Sein", "Win\u00b7cken", "giebt", "an", "Tag", "wer", "re\u00b7den", "soll", "und", "schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "PWS", "VVINF", "VMFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und Ehrerbietigkeit bewachet sein Gemach.", "tokens": ["Und", "Ehr\u00b7er\u00b7bie\u00b7tig\u00b7keit", "be\u00b7wa\u00b7chet", "sein", "Ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Es m\u00fcssen L\u00e4nder sich nach seinem Namen nennen/", "tokens": ["Es", "m\u00fcs\u00b7sen", "L\u00e4n\u00b7der", "sich", "nach", "sei\u00b7nem", "Na\u00b7men", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Man gr\u00e4bt sein hohes Lob den Marmel Seulen ein.", "tokens": ["Man", "gr\u00e4bt", "sein", "ho\u00b7hes", "Lob", "den", "Mar\u00b7mel", "Seu\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Es mu\u00df der gantze Hoff ihn vor das Haupt erkennen/", "tokens": ["Es", "mu\u00df", "der", "gant\u00b7ze", "Hoff", "ihn", "vor", "das", "Haupt", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und was er angeschafft/ zu thun bereitet seyn.", "tokens": ["Und", "was", "er", "an\u00b7ge\u00b7schafft", "/", "zu", "thun", "be\u00b7rei\u00b7tet", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVPP", "$(", "PTKZU", "VVINF", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So hat Tiberius durch den Sejan regiret/", "tokens": ["So", "hat", "Ti\u00b7be\u00b7ri\u00b7us", "durch", "den", "Se\u00b7jan", "re\u00b7gi\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+----+--+--+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Den er den besten Freund und Mitgeh\u00fclffen hie\u00df.", "tokens": ["Den", "er", "den", "bes\u00b7ten", "Freund", "und", "Mit\u00b7ge\u00b7h\u00fclf\u00b7fen", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So hat den Scepter auch des Nero Hand gef\u00fchret/", "tokens": ["So", "hat", "den", "Scep\u00b7ter", "auch", "des", "Ne\u00b7ro", "Hand", "ge\u00b7f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ART", "NE", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wenn ihm der Seneca die Kunst zu herrschen wie\u00df\u2019.", "tokens": ["Wenn", "ihm", "der", "Se\u00b7ne\u00b7ca", "die", "Kunst", "zu", "herr\u00b7schen", "wie\u00df'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NE", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ach aber Eitelkeit! Wer glaubet da\u00df bey Cronen/", "tokens": ["Ach", "a\u00b7ber", "Ei\u00b7tel\u00b7keit", "!", "Wer", "glau\u00b7bet", "da\u00df", "bey", "Cro\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "NN", "$.", "PWS", "VVFIN", "KOUS", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die edeln Steine nichts als schwerer Steine Last.", "tokens": ["Die", "e\u00b7deln", "Stei\u00b7ne", "nichts", "als", "schwe\u00b7rer", "Stei\u00b7ne", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIS", "KOKOM", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df in dem Purpur nur des Neides Motten wohnen/", "tokens": ["Da\u00df", "in", "dem", "Pur\u00b7pur", "nur", "des", "Nei\u00b7des", "Mot\u00b7ten", "woh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "ART", "NN", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.28": {"text": "Und da\u00df wer eifrig liebt/ viel eifriger noch ha\u00dft.", "tokens": ["Und", "da\u00df", "wer", "eif\u00b7rig", "liebt", "/", "viel", "eif\u00b7ri\u00b7ger", "noch", "ha\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PWS", "ADJD", "VVFIN", "$(", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ach wanckelbahre Gnad\u2019! Ach Gnade von Crystallen/", "tokens": ["Ach", "wan\u00b7ckel\u00b7bah\u00b7re", "Gnad'", "!", "Ach", "Gna\u00b7de", "von", "Crys\u00b7tal\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "NN", "$.", "NN", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die nur ein einzig Sto\u00df in tausend St\u00fccke bricht.", "tokens": ["Die", "nur", "ein", "ein\u00b7zig", "Sto\u00df", "in", "tau\u00b7send", "St\u00fc\u00b7cke", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJD", "NN", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ach Gl\u00e4tt-Ei\u00df da man steht und augenblicks mu\u00df fallen!", "tokens": ["Ach", "Gl\u00e4t\u00b7tEi\u00df", "da", "man", "steht", "und", "au\u00b7gen\u00b7blicks", "mu\u00df", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "KOUS", "PIS", "VVFIN", "KON", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ach in dem Finstern\u00fc\u00df ein recht verf\u00fchrisch Licht!", "tokens": ["Ach", "in", "dem", "Fins\u00b7ter\u00b7n\u00fc\u00df", "ein", "recht", "ver\u00b7f\u00fch\u00b7risch", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "APPR", "ART", "NN", "ART", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Vertumnus kan sich nicht so wunderlich verstellen/", "tokens": ["Ver\u00b7tum\u00b7nus", "kan", "sich", "nicht", "so", "wun\u00b7der\u00b7lich", "ver\u00b7stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "PTKNEG", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Es nimmt Cham\u00e4leon nicht so viel Farben an.", "tokens": ["Es", "nimmt", "Cha\u00b7m\u00e4\u00b7le\u00b7on", "nicht", "so", "viel", "Far\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "PTKNEG", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Als viel Ver\u00e4nderung und grimmen Ungl\u00fccks-F\u00e4llen/", "tokens": ["Als", "viel", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", "und", "grim\u00b7men", "Un\u00b7gl\u00fccks\u00b7F\u00e4l\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ein solcher Gnaden-Freund mu\u00df leben unterthan.", "tokens": ["Ein", "sol\u00b7cher", "Gna\u00b7den\u00b7Freund", "mu\u00df", "le\u00b7ben", "un\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VMFIN", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Bald stellt ihm der Neid mehr als verborgne Schlingen/", "tokens": ["Bald", "stellt", "ihm", "der", "Neid", "mehr", "als", "ver\u00b7borg\u00b7ne", "Schlin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PIAT", "KOKOM", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Bald hat Verleumbdung ihm das Fallbret zugericht/", "tokens": ["Bald", "hat", "Ver\u00b7leumb\u00b7dung", "ihm", "das", "Fall\u00b7bret", "zu\u00b7ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und wenn am lieblichsten des Lobes-Glocken klingen/", "tokens": ["Und", "wenn", "am", "lieb\u00b7lichs\u00b7ten", "des", "Lo\u00b7bes\u00b7Glo\u00b7cken", "klin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "ADJA", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So hat ein falscher Freund Aufflagen ausgedicht.", "tokens": ["So", "hat", "ein", "fal\u00b7scher", "Freund", "Auf\u00b7fla\u00b7gen", "aus\u00b7ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Dann brennt das Gnaden-Feur/ bey dem er sich gew\u00e4rmet", "tokens": ["Dann", "brennt", "das", "Gna\u00b7den\u00b7Feur", "/", "bey", "dem", "er", "sich", "ge\u00b7w\u00e4r\u00b7met