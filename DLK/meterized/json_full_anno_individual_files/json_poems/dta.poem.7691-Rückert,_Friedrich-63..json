{"dta.poem.7691": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du zum Ziele mich den rechten Weg willst leiten,", "tokens": ["Wenn", "du", "zum", "Zie\u00b7le", "mich", "den", "rech\u00b7ten", "Weg", "willst", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu langsam sollst du nicht, noch auch zu schnell mir schreiten.", "tokens": ["Zu", "lang\u00b7sam", "sollst", "du", "nicht", ",", "noch", "auch", "zu", "schnell", "mir", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VMFIN", "PPER", "PTKNEG", "$,", "ADV", "ADV", "PTKA", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Unterhaltung sei nicht unterwegs zuviel,", "tokens": ["Der", "Un\u00b7ter\u00b7hal\u00b7tung", "sei", "nicht", "un\u00b7ter\u00b7wegs", "zu\u00b7viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit wir nicht den Weg vergessen und das Ziel.", "tokens": ["Da\u00b7mit", "wir", "nicht", "den", "Weg", "ver\u00b7ges\u00b7sen", "und", "das", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVPP", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Kurz mache mir den Weg, und leicht und unbeschwerlich,", "tokens": ["Kurz", "ma\u00b7che", "mir", "den", "Weg", ",", "und", "leicht", "und", "un\u00b7be\u00b7schwer\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht schwerer, um dich selbst zu machen unentbehrlich.", "tokens": ["Nicht", "schwe\u00b7rer", ",", "um", "dich", "selbst", "zu", "ma\u00b7chen", "un\u00b7ent\u00b7behr\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "KOUI", "PPER", "ADV", "PTKZU", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Denn F\u00fchrer sollst du mir nicht immer wieder seyn,", "tokens": ["Denn", "F\u00fch\u00b7rer", "sollst", "du", "mir", "nicht", "im\u00b7mer", "wie\u00b7der", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "PPER", "PPER", "PTKNEG", "ADV", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ablernen will ich dir den Weg zu gehn allein.", "tokens": ["Ab\u00b7ler\u00b7nen", "will", "ich", "dir", "den", "Weg", "zu", "gehn", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "ART", "NN", "PTKZU", "VVINF", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}