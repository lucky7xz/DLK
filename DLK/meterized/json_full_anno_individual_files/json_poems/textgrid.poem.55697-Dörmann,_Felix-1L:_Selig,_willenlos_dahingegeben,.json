{"textgrid.poem.55697": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Selig, willenlos dahingegeben,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selig, willenlos dahingegeben,", "tokens": ["Se\u00b7lig", ",", "wil\u00b7len\u00b7los", "da\u00b7hin\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "PAV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ruht der schlanke Leib in meinen Armen,", "tokens": ["Ruht", "der", "schlan\u00b7ke", "Leib", "in", "mei\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die feuchten, vollen Lippen suchen", "tokens": ["Und", "die", "feuch\u00b7ten", ",", "vol\u00b7len", "Lip\u00b7pen", "su\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Leise die meinen.", "tokens": ["Lei\u00b7se", "die", "mei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber keine Liebesworte schauern", "tokens": ["A\u00b7ber", "kei\u00b7ne", "Lie\u00b7bes\u00b7wor\u00b7te", "schau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus bedr\u00e4ngtem Busen weich ans Ohr mir;", "tokens": ["Aus", "be\u00b7dr\u00e4ng\u00b7tem", "Bu\u00b7sen", "weich"