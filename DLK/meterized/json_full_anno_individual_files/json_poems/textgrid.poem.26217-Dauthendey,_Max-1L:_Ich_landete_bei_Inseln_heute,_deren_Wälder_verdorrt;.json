{"textgrid.poem.26217": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich landete bei Inseln heute, deren W\u00e4lder verdorrt;", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich landete bei Inseln heute, deren W\u00e4lder verdorrt;", "tokens": ["Ich", "lan\u00b7de\u00b7te", "bei", "In\u00b7seln", "heu\u00b7te", ",", "de\u00b7ren", "W\u00e4l\u00b7der", "ver\u00b7dorrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "$,", "PRELAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "Sonne hat die Wasser ausgesogen an jedem Ort.", "tokens": ["Son\u00b7ne", "hat", "die", "Was\u00b7ser", "aus\u00b7ge\u00b7so\u00b7gen", "an", "je\u00b7dem", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Wald\u00e4ste stehen grau und braun, blattleer im Licht.", "tokens": ["Wal\u00b7d\u00e4s\u00b7te", "ste\u00b7hen", "grau", "und", "braun", ",", "blatt\u00b7leer", "im", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "KON", "ADJD", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Vergr\u00e4mt, verarmt ist hier des Landes Angesicht.", "tokens": ["Ver\u00b7gr\u00e4mt", ",", "ver\u00b7armt", "ist", "hier", "des", "Lan\u00b7des", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und so gequ\u00e4lt wie jene Durstigen, die ohne Saft sich winden,", "tokens": ["Und", "so", "ge\u00b7qu\u00e4lt", "wie", "je\u00b7ne", "Durs\u00b7ti\u00b7gen", ",", "die", "oh\u00b7ne", "Saft", "sich", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "KOKOM", "PDAT", "NN", "$,", "PRELS", "APPR", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.6": {"text": "Kann auch mein Sinn jetzt fr\u00fch und sp\u00e4t nicht Trost mehr finden.", "tokens": ["Kann", "auch", "mein", "Sinn", "jetzt", "fr\u00fch", "und", "sp\u00e4t", "nicht", "Trost", "mehr", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "PTKNEG", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich landete bei Inseln heute, deren W\u00e4lder verdorrt;", "tokens": ["Ich", "lan\u00b7de\u00b7te", "bei", "In\u00b7seln", "heu\u00b7te", ",", "de\u00b7ren", "W\u00e4l\u00b7der", "ver\u00b7dorrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "$,", "PRELAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "Sonne hat die Wasser ausgesogen an jedem Ort.", "tokens": ["Son\u00b7ne", "hat", "die", "Was\u00b7ser", "aus\u00b7ge\u00b7so\u00b7gen", "an", "je\u00b7dem", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+