{"dta.poem.19021": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "V.   Uber sich selbst. Zu Pari\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Ich suche mich mit GOtt, dem h\u00f6chsten Gut,", "tokens": ["Ich", "su\u00b7che", "mich", "mit", "Gott", ",", "dem", "h\u00f6chs\u00b7ten", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aufs n\u00e4chste, da es seyn kan, zu verbinden;", "tokens": ["Aufs", "n\u00e4chs\u00b7te", ",", "da", "es", "seyn", "kan", ",", "zu", "ver\u00b7bin\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "KOUS", "PPER", "VAINF", "VMFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und da ich sonst auf meinem Kopff beruht,", "tokens": ["Und", "da", "ich", "sonst", "auf", "mei\u00b7nem", "Kopff", "be\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mu\u00df nun durchaus der eigne Wille schwinden.", "tokens": ["Mu\u00df", "nun", "durc\u00b7haus", "der", "eig\u00b7ne", "Wil\u00b7le", "schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mein Hertze ist dem HErren \u00fcbergeben,", "tokens": ["Mein", "Hert\u00b7ze", "ist", "dem", "Her\u00b7ren", "\u00fc\u00b7ber\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der soll hinfort in seinem Bilde leben.", "tokens": ["Der", "soll", "hin\u00b7fort", "in", "sei\u00b7nem", "Bil\u00b7de", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Tod, der mir sonst vieles Grauen macht,", "tokens": ["Der", "Tod", ",", "der", "mir", "sonst", "vie\u00b7les", "Grau\u00b7en", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "F\u00e4ngt itzo an viel besser auszusehen;", "tokens": ["F\u00e4ngt", "it\u00b7zo", "an", "viel", "bes\u00b7ser", "aus\u00b7zu\u00b7se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die so gef\u00fcrchtete und lange Nacht", "tokens": ["Die", "so", "ge\u00b7f\u00fcrch\u00b7te\u00b7te", "und", "lan\u00b7ge", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wird einmahl unversehens \u00fcbergehen;", "tokens": ["Wird", "ein\u00b7mahl", "un\u00b7ver\u00b7se\u00b7hens", "\u00fc\u00b7ber\u00b7ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Der Tag wird desto unverr\u00fcckter gl\u00e4ntzen,", "tokens": ["Der", "Tag", "wird", "des\u00b7to", "un\u00b7ver\u00b7r\u00fcck\u00b7ter", "gl\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und meinen Geist in Ewigkeit bekr\u00e4ntzen.", "tokens": ["Und", "mei\u00b7nen", "Geist", "in", "E\u00b7wig\u00b7keit", "be\u00b7kr\u00b7\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Der Heyland, der vor mich gelitten hat,", "tokens": ["Der", "Hey\u00b7land", ",", "der", "vor", "mich", "ge\u00b7lit\u00b7ten", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Bleibt blo\u00df allein die Regel meines Lebens;", "tokens": ["Bleibt", "blo\u00df", "al\u00b7lein", "die", "Re\u00b7gel", "mei\u00b7nes", "Le\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Davon zeigt Mund, und Hertz, und auch die That,", "tokens": ["Da\u00b7von", "zeigt", "Mund", ",", "und", "Hertz", ",", "und", "auch", "die", "That", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "$,", "KON", "NN", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ich m\u00fche mich nicht mehr, wie sonst, vergebens,", "tokens": ["Ich", "m\u00fc\u00b7he", "mich", "nicht", "mehr", ",", "wie", "sonst", ",", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$,", "PWAV", "ADV", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Ich w\u00fcrck in GOtt, und wei\u00df, auf Sieges-Thronen", "tokens": ["Ich", "w\u00fcrck", "in", "Gott", ",", "und", "wei\u00df", ",", "auf", "Sie\u00b7ges\u00b7Thro\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,", "KON", "VVFIN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Wird Gnaden-Lohn mein Werck in GOtt belohnen.", "tokens": ["Wird", "Gna\u00b7den\u00b7Lohn", "mein", "Werck", "in", "Gott", "be\u00b7loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}