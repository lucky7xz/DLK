{"textgrid.poem.58561": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "599. Von inwendiger Blo\u00dfheit und Gelassenheit unsrer selbst und aller Dinge", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein neues Lied ich von der Blo\u00dfheit dicht',", "tokens": ["Ein", "neu\u00b7es", "Lied", "ich", "von", "der", "Blo\u00df\u00b7heit", "dicht'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn rechte Lauterkeit, die denket nicht,", "tokens": ["Denn", "rech\u00b7te", "Lau\u00b7ter\u00b7keit", ",", "die", "den\u00b7ket", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie m\u00f6gen da Gedanken sein,", "tokens": ["Wie", "m\u00f6\u00b7gen", "da", "Ge\u00b7dan\u00b7ken", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ich verloren hab' das Mein'?", "tokens": ["Da", "ich", "ver\u00b7lo\u00b7ren", "hab'", "das", "Mein'", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin entworden.", "tokens": ["Ich", "bin", "ent\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wer wahrlich steht im Geist entbl\u00f6\u00dft,", "tokens": ["Wer", "wahr\u00b7lich", "steht", "im", "Geist", "ent\u00b7bl\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von allen Sorgen ist erl\u00f6st.", "tokens": ["Von", "al\u00b7len", "Sor\u00b7gen", "ist", "er\u00b7l\u00f6st", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl oder \u00fcbel gilt mir gleich,", "tokens": ["Wohl", "o\u00b7der", "\u00fc\u00b7bel", "gilt", "mir", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich will so gern arm sein als reich;", "tokens": ["Ich", "will", "so", "gern", "arm", "sein", "als", "reich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADJD", "VAINF", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Bildern mag ich nicht umgehn,", "tokens": ["Mit", "Bil\u00b7dern", "mag", "ich", "nicht", "um\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df meiner selbst gar ledig stehn:", "tokens": ["Mu\u00df", "mei\u00b7ner", "selbst", "gar", "le\u00b7dig", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich bin entworden.", "tokens": ["Ich", "bin", "ent\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Fragt ihr, wie ich zur Bilderfreiheit kam:", "tokens": ["Fragt", "ihr", ",", "wie", "ich", "zur", "Bil\u00b7der\u00b7frei\u00b7heit", "kam", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie ich die Einheit recht in mir vernahm", "tokens": ["Wie", "ich", "die", "Ein\u00b7heit", "recht", "in", "mir", "ver\u00b7nahm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und rechte Einheit dies einpr\u00e4gt',", "tokens": ["Und", "rech\u00b7te", "Ein\u00b7heit", "dies", "ein\u00b7pr\u00e4gt'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df mich nicht Lieb' noch Leid bewegt'.", "tokens": ["Da\u00df", "mich", "nicht", "Lieb'", "noch", "Leid", "be\u00b7wegt'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin entworden.", "tokens": ["Ich", "bin", "ent\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Wer wahrlich steht im Geist entbl\u00f6\u00dft,", "tokens": ["Wer", "wahr\u00b7lich", "steht", "im", "Geist", "ent\u00b7bl\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von allen Sorgen ist erl\u00f6st;", "tokens": ["Von", "al\u00b7len", "Sor\u00b7gen", "ist", "er\u00b7l\u00f6st", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fragt ihr, wie ich dem Geiste so entkam:", "tokens": ["Fragt", "ihr", ",", "wie", "ich", "dem", "Geis\u00b7te", "so", "ent\u00b7kam", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie ich nicht dies noch das in mir vernahm", "tokens": ["Wie", "ich", "nicht", "dies", "noch", "das", "in", "mir", "ver\u00b7nahm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PTKNEG", "PDS", "ADV", "ART", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als blo\u00dfe Gottheit ohne Grund,", "tokens": ["Als", "blo\u00b7\u00dfe", "Got\u00b7theit", "oh\u00b7ne", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da konnt' ich l\u00e4nger schweigen nicht,", "tokens": ["Da", "konnt'", "ich", "l\u00e4n\u00b7ger", "schwei\u00b7gen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich mu\u00dft' es machen kund:", "tokens": ["Ich", "mu\u00dft'", "es", "ma\u00b7chen", "kund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ich bin entworden.", "tokens": ["Ich", "bin", "ent\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Wer wahrlich steht im Geist entbl\u00f6\u00dft,", "tokens": ["Wer", "wahr\u00b7lich", "steht", "im", "Geist", "ent\u00b7bl\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von allen Sorgen ist erl\u00f6st;", "tokens": ["Von", "al\u00b7len", "Sor\u00b7gen", "ist", "er\u00b7l\u00f6st", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seitdem ich so verlor'n in 'n Abgrund kommen,", "tokens": ["Seit\u00b7dem", "ich", "so", "ver\u00b7lor'n", "in", "'n", "Ab\u00b7grund", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Kann ich auch reden nicht, ich mu\u00df verstummen,", "tokens": ["Kann", "ich", "auch", "re\u00b7den", "nicht", ",", "ich", "mu\u00df", "ver\u00b7stum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "PTKNEG", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die klare Gottheit mich", "tokens": ["Die", "kla\u00b7re", "Got\u00b7theit", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Verschlinget so in sich:", "tokens": ["Ver\u00b7schlin\u00b7get", "so", "in", "sich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich bin entworden.", "tokens": ["Ich", "bin", "ent\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Doch diese Finsternis macht tief erquickt und froh,", "tokens": ["Doch", "die\u00b7se", "Fins\u00b7ter\u00b7nis", "macht", "tief", "er\u00b7quickt", "und", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "ADJD", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nachdem ich dring' durch alles so.", "tokens": ["Nach\u00b7dem", "ich", "dring'", "durch", "al\u00b7les", "so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei meinem Ursprung werd' ich nimmer alt,", "tokens": ["Bei", "mei\u00b7nem", "Ur\u00b7sprung", "werd'", "ich", "nim\u00b7mer", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nein, wie der Adler ich verj\u00fcnge bald,", "tokens": ["Nein", ",", "wie", "der", "Ad\u00b7ler", "ich", "ver\u00b7j\u00fcn\u00b7ge", "bald", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So sind all meine Kr\u00e4ft' f\u00fcrwahr", "tokens": ["So", "sind", "all", "mei\u00b7ne", "Kr\u00e4ft'", "f\u00fcr\u00b7wahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verschlungen und erstorben gar.", "tokens": ["Ver\u00b7schlun\u00b7gen", "und", "ers\u00b7tor\u00b7ben", "gar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wer wahrlich steht im Geist entbl\u00f6\u00dft,", "tokens": ["Wer", "wahr\u00b7lich", "steht", "im", "Geist", "ent\u00b7bl\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von allen Sorgen ist erl\u00f6st;", "tokens": ["Von", "al\u00b7len", "Sor\u00b7gen", "ist", "er\u00b7l\u00f6st", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer dergestalt nur ist verschwunden", "tokens": ["Wer", "der\u00b7ge\u00b7stalt", "nur", "ist", "ver\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und innig hat die Finsternis befunden,", "tokens": ["Und", "in\u00b7nig", "hat", "die", "Fins\u00b7ter\u00b7nis", "be\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der ist ohn' allen Kummer reich f\u00fcrwahr,", "tokens": ["Der", "ist", "ohn'", "al\u00b7len", "Kum\u00b7mer", "reich", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PIAT", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Liebefeu'r mich so entz\u00fcnd't hat gar,", "tokens": ["Das", "Lie\u00b7be\u00b7feu'r", "mich", "so", "ent\u00b7z\u00fcnd't", "hat", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVPP", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df ich erstorben.", "tokens": ["Da\u00df", "ich", "ers\u00b7tor\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Wer wahrlich steht im Geist entbl\u00f6\u00dft,", "tokens": ["Wer", "wahr\u00b7lich", "steht", "im", "Geist", "ent\u00b7bl\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von allen Sorgen ist erl\u00f6st;", "tokens": ["Von", "al\u00b7len", "Sor\u00b7gen", "ist", "er\u00b7l\u00f6st", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf dieses Sterben und Entwerden", "tokens": ["Auf", "die\u00b7ses", "Ster\u00b7ben", "und", "Ent\u00b7wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "KON", "NN"