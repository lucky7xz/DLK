{"dta.poem.20652": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  den Herrn Kanonikus  \n Gleim.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie eilt, wir m\u00fcssen sie haschen                 ", "tokens": ["Sie", "eilt", ",", "wir", "m\u00fcs\u00b7sen", "sie", "ha\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "O Freund, die fliehende Zeit,", "tokens": ["O", "Freund", ",", "die", "flie\u00b7hen\u00b7de", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Komm zum befruchteten Garten,", "tokens": ["Komm", "zum", "be\u00b7fruch\u00b7te\u00b7ten", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Herbst hat Freuden f\u00fcr uns.", "tokens": ["Der", "Herbst", "hat", "Freu\u00b7den", "f\u00fcr", "uns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Brich diese lockende Aepfel:", "tokens": ["Brich", "die\u00b7se", "lo\u00b7cken\u00b7de", "A\u00b7e\u00b7pfel", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "ADJA", "NN", "$."], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.2": {"text": "Sie l\u00e4cheln unter dem Laub,", "tokens": ["Sie", "l\u00e4\u00b7cheln", "un\u00b7ter", "dem", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie Wangen bl\u00fchender M\u00e4dchen,", "tokens": ["Wie", "Wan\u00b7gen", "bl\u00fc\u00b7hen\u00b7der", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "F\u00fcr Dich gereifet, hervor.", "tokens": ["F\u00fcr", "Dich", "ge\u00b7rei\u00b7fet", ",", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Die schlanken, neidischen Aeste,", "tokens": ["Die", "schlan\u00b7ken", ",", "nei\u00b7di\u00b7schen", "A\u00b7es\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "F\u00fcr Deine Griffe zu hoch;", "tokens": ["F\u00fcr", "Dei\u00b7ne", "Grif\u00b7fe", "zu", "hoch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hilf mit dem h\u00fcpfenden Fu\u00dfe", "tokens": ["Hilf", "mit", "dem", "h\u00fcp\u00b7fen\u00b7den", "Fu\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Hand, und pfl\u00fccke die Frucht.", "tokens": ["Der", "Hand", ",", "und", "pfl\u00fc\u00b7cke", "die", "Frucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Du hast sie. Lohnender Arbeit", "tokens": ["Du", "hast", "sie", ".", "Loh\u00b7nen\u00b7der", "Ar\u00b7beit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$.", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Verrichtung nennen wir Lust.", "tokens": ["Ver\u00b7rich\u00b7tung", "nen\u00b7nen", "wir", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie viel bestrebten sich H\u00e4nde", "tokens": ["Wie", "viel", "be\u00b7streb\u00b7ten", "sich", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "PRF", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nach Deinem Herzen umsonst!", "tokens": ["Nach", "Dei\u00b7nem", "Her\u00b7zen", "um\u00b7sonst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Sieh diesen h\u00f6ckrigten Apfel:", "tokens": ["Sieh", "die\u00b7sen", "h\u00f6ck\u00b7rig\u00b7ten", "Ap\u00b7fel", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie seine Br\u00fcder gebl\u00fcht", "tokens": ["Wie", "sei\u00b7ne", "Br\u00fc\u00b7der", "ge\u00b7bl\u00fcht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VVPP"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hat er in w\u00e4hrender Bildung,", "tokens": ["Hat", "er", "in", "w\u00e4h\u00b7ren\u00b7der", "Bil\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und dennoch ward er ein Zwerg.", "tokens": ["Und", "den\u00b7noch", "ward", "er", "ein", "Zwerg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "In seinem Fleische gen\u00e4hret", "tokens": ["In", "sei\u00b7nem", "Flei\u00b7sche", "ge\u00b7n\u00e4h\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ward der fortfressende Wurm:", "tokens": ["Ward", "der", "fort\u00b7fres\u00b7sen\u00b7de", "Wurm", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So w\u00e4chst mit kommenden Tagen", "tokens": ["So", "w\u00e4chst", "mit", "kom\u00b7men\u00b7den", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im Knaben Bosheit herauf,", "tokens": ["Im", "Kna\u00b7ben", "Bos\u00b7heit", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Der nicht vom Hauche des Lebens,", "tokens": ["Der", "nicht", "vom", "Hau\u00b7che", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als ein kaum werdender Mensch,", "tokens": ["Als", "ein", "kaum", "wer\u00b7den\u00b7der", "Mensch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Zu gro\u00dfen sch\u00f6nen Gedanken", "tokens": ["Zu", "gro\u00b7\u00dfen", "sch\u00f6\u00b7nen", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Beseelt geworden, wie Du.", "tokens": ["Be\u00b7seelt", "ge\u00b7wor\u00b7den", ",", "wie", "Du", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAPP", "$,", "PWAV", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Sieh der hartsch\u00e4ligen N\u00fcsse", "tokens": ["Sieh", "der", "hart\u00b7sch\u00e4\u00b7li\u00b7gen", "N\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Herunterfallen vom Baum:", "tokens": ["Her\u00b7un\u00b7ter\u00b7fal\u00b7len", "vom", "Baum", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ihn zwingt der schlagende J\u00fcngling,", "tokens": ["Ihn", "zwingt", "der", "schla\u00b7gen\u00b7de", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sonst w\u00fcrf er keine herab.", "tokens": ["Sonst", "w\u00fcrf", "er", "kei\u00b7ne", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "So schlie\u00dft der Geizige treulich", "tokens": ["So", "schlie\u00dft", "der", "Gei\u00b7zi\u00b7ge", "treu\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ans Herz gesammeltes Gold,", "tokens": ["Ans", "Herz", "ge\u00b7sam\u00b7mel\u00b7tes", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Verschlie\u00dft die kargende Rechte", "tokens": ["Ver\u00b7schlie\u00dft", "die", "kar\u00b7gen\u00b7de", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dem Armen, welcher ihn fleht.", "tokens": ["Dem", "Ar\u00b7men", ",", "wel\u00b7cher", "ihn", "fleht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Darbt im Besitze des Reichthums,", "tokens": ["Darbt", "im", "Be\u00b7sit\u00b7ze", "des", "Reicht\u00b7hums", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Schmeckt nie den k\u00f6stlichen Wein,", "tokens": ["Schmeckt", "nie", "den", "k\u00f6st\u00b7li\u00b7chen", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und nie den s\u00fc\u00dferen Nektar", "tokens": ["Und", "nie", "den", "s\u00fc\u00b7\u00dfe\u00b7ren", "Nek\u00b7tar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Freundschaft, die er nicht f\u00fchlt.", "tokens": ["Der", "Freund\u00b7schaft", ",", "die", "er", "nicht", "f\u00fchlt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "La\u00df ihm die magere Wollust.", "tokens": ["La\u00df", "ihm", "die", "ma\u00b7ge\u00b7re", "Wol\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Er ruh auf todtem Metall:", "tokens": ["Er", "ruh", "auf", "tod\u00b7tem", "Me\u00b7tall", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wir, in der deckenden Laube,", "tokens": ["Wir", ",", "in", "der", "de\u00b7cken\u00b7den", "Lau\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.4": {"text": "Beneiden K\u00f6nige nicht.", "tokens": ["Be\u00b7nei\u00b7den", "K\u00f6\u00b7ni\u00b7ge", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "Genie\u00df mit Augen des Geizes", "tokens": ["Ge\u00b7nie\u00df", "mit", "Au\u00b7gen", "des", "Gei\u00b7zes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das bald hinsterbende Gr\u00fcn", "tokens": ["Das", "bald", "hins\u00b7ter\u00b7ben\u00b7de", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Im Garten unter den B\u00e4umen.", "tokens": ["Im", "Gar\u00b7ten", "un\u00b7ter", "den", "B\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schon macht der n\u00e4chtliche Reif", "tokens": ["Schon", "macht", "der", "n\u00e4cht\u00b7li\u00b7che", "Reif"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Die Bl\u00e4tter alle zu Kranken:", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "al\u00b7le", "zu", "Kran\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So rei\u00dft die m\u00e4chtige Zeit,", "tokens": ["So", "rei\u00dft", "die", "m\u00e4ch\u00b7ti\u00b7ge", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und ein durchdringendes Fieber", "tokens": ["Und", "ein", "durch\u00b7drin\u00b7gen\u00b7des", "Fie\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den Reiz vom Menschen dahin.", "tokens": ["Den", "Reiz", "vom", "Men\u00b7schen", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.14": {"line.1": {"text": "Sp\u00e4t in dem Sommer des Lebens", "tokens": ["Sp\u00e4t", "in", "dem", "Som\u00b7mer", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Sind wir, sie fliehen zu schnell", "tokens": ["Sind", "wir", ",", "sie", "flie\u00b7hen", "zu", "schnell"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "PPER", "VVFIN", "PTKA", "ADJD"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Die Stunden, brauche sie fr\u00f6lich,", "tokens": ["Die", "Stun\u00b7den", ",", "brau\u00b7che", "sie", "fr\u00f6\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Uns macht das Alter zu Eis.", "tokens": ["Uns", "macht", "das", "Al\u00b7ter", "zu", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}