{"dta.poem.10597": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma au\u00df dem Mureto.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn nicht die Sonne scheint/ vnd wann vns trifft der Regen/", "tokens": ["Wenn", "nicht", "die", "Son\u00b7ne", "scheint", "/", "vnd", "wann", "vns", "trifft", "der", "Re\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "VVFIN", "$(", "KON", "PWAV", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fleucht alle Fr\u00f6ligkeit/ Hertz/ Muth vnd Sinn sich legen/", "tokens": ["Fleucht", "al\u00b7le", "Fr\u00f6\u00b7lig\u00b7keit", "/", "Hertz", "/", "Muth", "vnd", "Sinn", "sich", "le\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Lieb/ sey nicht best\u00fcrtzt/ da\u00df ich solch Trawren f\u00fchr/", "tokens": ["Mein", "Lieb", "/", "sey", "nicht", "be\u00b7st\u00fcrtzt", "/", "da\u00df", "ich", "solch", "Traw\u00b7ren", "f\u00fchr", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VAFIN", "PTKNEG", "VVPP", "$(", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich regne selbst/ vnd du/