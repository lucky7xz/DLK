{"textgrid.poem.24421": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Freunde dieser Welt", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So lange was an dir/ das noch die Welt vergn\u00fcgt/", "tokens": ["So", "lan\u00b7ge", "was", "an", "dir", "/", "das", "noch", "die", "Welt", "ver\u00b7gn\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PWS", "APPR", "PPER", "$(", "PDS", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Scheint man dir gut zu seyn. Wird heute di\u00df verschwinden", "tokens": ["Scheint", "man", "dir", "gut", "zu", "seyn", ".", "Wird", "heu\u00b7te", "di\u00df", "ver\u00b7schwin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPER", "ADJD", "PTKZU", "VAINF", "$.", "VAFIN", "ADV", "PDS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wird sich morgen auch der Schein nicht weiter finden.", "tokens": ["So", "wird", "sich", "mor\u00b7gen", "auch", "der", "Schein", "nicht", "wei\u00b7ter", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "ADV", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Oft stirbet schon ein Freund/ weil er sich vor uns schmiegt.", "tokens": ["Oft", "stir\u00b7bet", "schon", "ein", "Freund", "/", "weil", "er", "sich", "vor", "uns", "schmiegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "