es", "he\u00b7be"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$.", "ADJD", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Krieg sich hin an seinen Ort;", "tokens": ["Der", "Krieg", "sich", "hin", "an", "sei\u00b7nen", "Ort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So thut ers, wie im Ocean die Ebbe.", "tokens": ["So", "thut", "ers", ",", "wie", "im", "O\u00b7cean", "die", "Eb\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PWAV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Auf dein Gebot", "tokens": ["Auf", "dein", "Ge\u00b7bot"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Fliehn Zank und Tod:", "tokens": ["Fliehn", "Zank", "und", "Tod", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der ewige Rebelle", "tokens": ["Der", "e\u00b7wi\u00b7ge", "Re\u00b7bel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.8": {"text": "Empf\u00e4ngt sie in der H\u00f6lle.", "tokens": ["Emp\u00b7f\u00e4ngt", "sie", "in", "der", "H\u00f6l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "O Gott! der du allm\u00e4chtig bist! \u2013 \u2013", "tokens": ["O", "Gott", "!", "der", "du", "all\u00b7m\u00e4ch\u00b7tig", "bist", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "NN", "$.", "ART", "PPER", "ADJD", "VAFIN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An deiner unersch\u00f6pften G\u00fcte,", "tokens": ["An", "dei\u00b7ner", "un\u00b7er\u00b7sch\u00f6pf\u00b7ten", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die meines Daseyns Ursach ist,", "tokens": ["Die", "mei\u00b7nes", "Da\u00b7seyns", "Ur\u00b7sach", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erg\u00f6tzen sich mein Herz und mein Gem\u00fcte;", "tokens": ["Er\u00b7g\u00f6t\u00b7zen", "sich", "mein", "Herz", "und", "mein", "Ge\u00b7m\u00fc\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich denke sie,", "tokens": ["Ich", "den\u00b7ke", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Denn Herr! noch nie,", "tokens": ["Denn", "Herr", "!", "noch", "nie", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "ADV", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wenn ich in Noth gesessen,", "tokens": ["Wenn", "ich", "in", "Noth", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ward ich von ihr vergessen.", "tokens": ["Ward", "ich", "von", "ihr", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sie reicht so weit die Himmel gehn,", "tokens": ["Sie", "reicht", "so", "weit", "die", "Him\u00b7mel", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist breiter als zehntausend Erden.", "tokens": ["Ist", "brei\u00b7ter", "als", "zehn\u00b7tau\u00b7send", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie hie\u00df Erzengel vor sich stehn", "tokens": ["Sie", "hie\u00df", "Er\u00b7zen\u00b7gel", "vor", "sich", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sprach; und Thier und Menschen musten werden!", "tokens": ["Sie", "sprach", ";", "und", "Thier", "und", "Men\u00b7schen", "mus\u00b7ten", "wer\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "NN", "KON", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Sternen Heer,", "tokens": ["Das", "Ster\u00b7nen", "Heer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Das tiefe Meer,", "tokens": ["Das"