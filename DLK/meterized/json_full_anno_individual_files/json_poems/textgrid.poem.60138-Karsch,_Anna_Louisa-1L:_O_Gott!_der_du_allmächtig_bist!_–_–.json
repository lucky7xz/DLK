{"textgrid.poem.60138": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Gott! der du allm\u00e4chtig bist! \u2013 \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Gott! der du allm\u00e4chtig bist! \u2013 \u2013", "tokens": ["O", "Gott", "!", "der", "du", "all\u00b7m\u00e4ch\u00b7tig", "bist", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "NN", "$.", "ART", "PPER", "ADJD", "VAFIN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An deiner unersch\u00f6pften G\u00fcte,", "tokens": ["An", "dei\u00b7ner", "un\u00b7er\u00b7sch\u00f6pf\u00b7ten", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die meines Daseyns Ursach ist,", "tokens": ["Die", "mei\u00b7nes", "Da\u00b7seyns", "Ur\u00b7sach", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erg\u00f6tzen sich mein Herz und mein Gem\u00fcte;", "tokens": ["Er\u00b7g\u00f6t\u00b7zen", "sich", "mein", "Herz", "und", "mein", "Ge\u00b7m\u00fc\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich denke sie,", "tokens": ["Ich", "den\u00b7ke", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Denn Herr! noch nie,", "tokens": ["Denn", "Herr", "!", "noch", "nie", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "ADV", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wenn ich in Noth gesessen,", "tokens": ["Wenn", "ich", "in", "Noth", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ward ich von ihr vergessen.", "tokens": ["Ward", "ich", "von", "ihr", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie reicht so weit die Himmel gehn,", "tokens": ["Sie", "reicht", "so", "weit", "die", "Him\u00b7mel", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist breiter als zehntausend Erden.", "tokens": ["Ist", "brei\u00b7ter", "als", "zehn\u00b7tau\u00b7send", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie hie\u00df Erzengel vor sich stehn", "tokens": ["Sie", "hie\u00df", "Er\u00b7zen\u00b7gel", "vor", "sich", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sprach; und Thier und Menschen musten werden!", "tokens": ["Sie", "sprach", ";", "und", "Thier", "und", "Men\u00b7schen", "mus\u00b7ten", "wer\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "NN", "KON", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Sternen Heer,", "tokens": ["Das", "Ster\u00b7nen", "Heer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Das tiefe Meer,", "tokens": ["Das", "tie\u00b7fe", "Meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Sind Werke seiner H\u00e4nde;", "tokens": ["Sind", "Wer\u00b7ke", "sei\u00b7ner", "H\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie aber ist ohn Ende.", "tokens": ["Sie", "a\u00b7ber", "ist", "ohn", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein Gott! von dieser G\u00fcte sagt", "tokens": ["Mein", "Gott", "!", "von", "die\u00b7ser", "G\u00fc\u00b7te", "sagt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir deiner Creaturen Menge;", "tokens": ["Mir", "dei\u00b7ner", "Crea\u00b7tu\u00b7ren", "Men\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Vogel nennt sie, wenn es tagt,", "tokens": ["Der", "Vo\u00b7gel", "nennt", "sie", ",", "wenn", "es", "tagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sorgt f\u00fcr ihn, er singt ihr Lobges\u00e4nge.", "tokens": ["Sie", "sorgt", "f\u00fcr", "ihn", ",", "er", "singt", "ihr", "Lob\u00b7ge\u00b7s\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Donner schillt,", "tokens": ["Der", "Don\u00b7ner", "schillt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der L\u00f6we br\u00fcllt,", "tokens": ["Der", "L\u00f6\u00b7we", "br\u00fcllt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Als deiner St\u00e4rke Zeugen;", "tokens": ["Als", "dei\u00b7ner", "St\u00e4r\u00b7ke", "Zeu\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und beyde m\u00fcssen schweigen!", "tokens": ["Und", "bey\u00b7de", "m\u00fcs\u00b7sen", "schwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der L\u00f6we mu\u00df, wenn du es willst,", "tokens": ["Der", "L\u00f6\u00b7we", "mu\u00df", ",", "wenn", "du", "es", "willst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "$,", "KOUS", "PPER", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit bl\u00f6den L\u00e4mmern freundlich spielen,", "tokens": ["Mit", "bl\u00f6\u00b7den", "L\u00e4m\u00b7mern", "freund\u00b7lich", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn du dich in Wolken h\u00fcllst", "tokens": ["Und", "wenn", "du", "dich", "in", "Wol\u00b7ken", "h\u00fcllst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So mu\u00df dein Blitz die heisse Luft nur k\u00fchlen!", "tokens": ["So", "mu\u00df", "dein", "Blitz", "die", "heis\u00b7se", "Luft", "nur", "k\u00fch\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Donners Wuth,", "tokens": ["Des", "Don\u00b7ners", "Wuth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Des Meeres Fluth,", "tokens": ["Des", "Mee\u00b7res", "Fluth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Den Sturmwind und die Stille", "tokens": ["Den", "Sturm\u00b7wind", "und", "die", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Schickt deiner Allmacht Wille!", "tokens": ["Schickt", "dei\u00b7ner", "All\u00b7macht", "Wil\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du schickst den Hunger in ein Land,", "tokens": ["Du", "schickst", "den", "Hun\u00b7ger", "in", "ein", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und strafst die s\u00fcndigen Gesch\u00f6pfe", "tokens": ["Und", "strafst", "die", "s\u00fcn\u00b7di\u00b7gen", "Ge\u00b7sch\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Ueberschwemmung und mit Brand:", "tokens": ["Mit", "Ue\u00b7ber\u00b7schwem\u00b7mung", "und", "mit", "Brand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Krieg zermalmt die Menschen wie die T\u00f6pfe.", "tokens": ["Dein", "Krieg", "zer\u00b7malmt", "die", "Men\u00b7schen", "wie", "die", "T\u00f6p\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie schreyn zu dir:", "tokens": ["Sie", "schreyn", "zu", "dir", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Herr! siehe, wir", "tokens": ["Herr", "!", "sie\u00b7he", ",", "wir"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$.", "VVIMP", "$,", "PPER"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Sind elend, und zerschlagen;", "tokens": ["Sind", "e\u00b7lend", ",", "und", "zer\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KON", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dann wendest du die Plagen:", "tokens": ["Dann", "wen\u00b7dest", "du", "die", "Pla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und deine G\u00fcte blickt herab", "tokens": ["Und", "dei\u00b7ne", "G\u00fc\u00b7te", "blickt", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf \u00f6de Felder voller Leichen;", "tokens": ["Auf", "\u00f6\u00b7de", "Fel\u00b7der", "vol\u00b7ler", "Lei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schlie\u00dft das unerf\u00fcllte Grab", "tokens": ["Sie", "schlie\u00dft", "das", "un\u00b7er\u00b7f\u00fcll\u00b7te", "Grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und giebt die Ruh verheerten K\u00f6nigreichen.", "tokens": ["Und", "giebt", "die", "Ruh", "ver\u00b7heer\u00b7ten", "K\u00f6\u00b7nig\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das trunkne Schwert", "tokens": ["Das", "trunk\u00b7ne", "Schwert"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Noch blutig, f\u00e4hrt", "tokens": ["Noch", "blu\u00b7tig", ",", "f\u00e4hrt"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "$,", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Zur\u00fcck in seine Scheide,", "tokens": ["Zu\u00b7r\u00fcck", "in", "sei\u00b7ne", "Schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Klagen werden Freude!", "tokens": ["Und", "Kla\u00b7gen", "wer\u00b7den", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du, unser Gott! noch wie zuvor,", "tokens": ["Du", ",", "un\u00b7ser", "Gott", "!", "noch", "wie", "zu\u00b7vor", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$.", "ADV", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Zeit der Kinder Jacobs, g\u00fctig:", "tokens": ["Zur", "Zeit", "der", "Kin\u00b7der", "Ja\u00b7cobs", ",", "g\u00fc\u00b7tig", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NE", "$,", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu dir schreyt unser Herz empor!", "tokens": ["Zu", "dir", "schreyt", "un\u00b7ser", "Herz", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch br\u00fcllt der Krieg, und mehr als L\u00f6wenm\u00fcthig", "tokens": ["Noch", "br\u00fcllt", "der", "Krieg", ",", "und", "mehr", "als", "L\u00f6\u00b7wen\u00b7m\u00fct\u00b7hig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "PIAT", "KOKOM", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von Waffen schwer,", "tokens": ["Von", "Waf\u00b7fen", "schwer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ziehn sie daher", "tokens": ["Ziehn", "sie", "da\u00b7her"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Die Feinde, die uns dr\u00e4uen;", "tokens": ["Die", "Fein\u00b7de", ",", "die", "uns", "dr\u00e4u\u00b7en", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und du kannst sie zerstreuen!", "tokens": ["Und", "du", "kannst", "sie", "zer\u00b7streu\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "In deine Vorsicht eingeh\u00fcllt", "tokens": ["In", "dei\u00b7ne", "Vor\u00b7sicht", "ein\u00b7ge\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr! werden wir dennoch erhalten.", "tokens": ["Herr", "!", "wer\u00b7den", "wir", "den\u00b7noch", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wenn \u00fcber uns der Donner br\u00fcllt,", "tokens": ["Wenn", "\u00fc\u00b7ber", "uns", "der", "Don\u00b7ner", "br\u00fcllt", ","], "token_info": ["word"