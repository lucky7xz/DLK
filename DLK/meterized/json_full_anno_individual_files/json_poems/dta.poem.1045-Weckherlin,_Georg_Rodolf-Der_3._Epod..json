{"dta.poem.1045": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Der 3. Epod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vmb sie seind jhre jungfrawen/", "tokens": ["Vmb", "sie", "seind", "jhre", "jung\u00b7fra\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VAFIN", "PPOSAT", "ADJA", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welcher sch\u00f6n vn\u0303 h\u00f6fllichkeit", "tokens": ["Wel\u00b7cher", "sch\u00f6n", "v\u00f1", "h\u00f6fl\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmet hinweg die freyheit", "tokens": ["Nim\u00b7met", "hin\u00b7weg", "die", "frey\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Denen/ welche sie anschawen.", "tokens": ["De\u00b7nen", "/", "wel\u00b7che", "sie", "an\u00b7scha\u00b7wen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$(", "PRELS", "PPER", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "M\u00f6niglich \u00f6fnet sein wohn/", "tokens": ["M\u00f6\u00b7nig\u00b7lich", "\u00f6f\u00b7net", "sein", "wohn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Durch zungen vnd saiten thon/", "tokens": ["Durch", "zun\u00b7gen", "vnd", "sai\u00b7ten", "thon", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Die l\u00fcft vmb vnd vmb erschallen", "tokens": ["Die", "l\u00fcft", "vmb", "vnd", "vmb", "er\u00b7schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "KON", "APPR", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ab dem gmeinen wolgefallen/", "tokens": ["Ab", "dem", "gmei\u00b7nen", "wol\u00b7ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Also das die Sehlen bald", "tokens": ["Al\u00b7so", "das", "die", "Seh\u00b7len", "bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Durch die augen oder ohren/", "tokens": ["Durch", "die", "au\u00b7gen", "o\u00b7der", "oh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "In der stim oder gestalt", "tokens": ["In", "der", "stim", "o\u00b7der", "ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "S\u00fc\u00dfwilliglich sich verlohren.", "tokens": ["S\u00fc\u00df\u00b7wil\u00b7lig\u00b7lich", "sich", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVPP", "$."], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Der Rhein mit dem Neckar fro", "tokens": ["Der", "Rhein", "mit", "dem", "Ne\u00b7ckar", "fro"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "APPR", "ART", "NN", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Besprach Sie damahls also?", "tokens": ["Be\u00b7sprach", "Sie", "da\u00b7mahls", "al\u00b7so", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}