{"textgrid.poem.63947": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus einem Teiche voller Rohr", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus einem Teiche voller Rohr", "tokens": ["Aus", "ei\u00b7nem", "Tei\u00b7che", "vol\u00b7ler", "Rohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kroch einst ein dicker Frosch hervor;", "tokens": ["Kroch", "einst", "ein", "di\u00b7cker", "Frosch", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Zeit ward ihm im Wasser lang,", "tokens": ["Die", "Zeit", "ward", "ihm", "im", "Was\u00b7ser", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er nahm zur Lust d'rum einen Gang", "tokens": ["Er", "nahm", "zur", "Lust", "d'\u00b7rum", "ei\u00b7nen", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hin nach dem n\u00e4chsten gr\u00fcnen Wald,", "tokens": ["Hin", "nach", "dem", "n\u00e4chs\u00b7ten", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem angenehmen Aufenthalt", "tokens": ["Dem", "an\u00b7ge\u00b7neh\u00b7men", "Auf\u00b7ent\u00b7halt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von manchem gro\u00df' und kleinen Thier.", "tokens": ["Von", "man\u00b7chem", "gro\u00df'", "und", "klei\u00b7nen", "Thier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da stieg er voller Ruhmbegier", "tokens": ["Da", "stieg", "er", "vol\u00b7ler", "Ruhm\u00b7be\u00b7gier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Auf einen runden Eichenklotz,", "tokens": ["Auf", "ei\u00b7nen", "run\u00b7den", "Ei\u00b7chen\u00b7klotz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sah um sich her mit edlem Trotz;", "tokens": ["Sah", "um", "sich", "her", "mit", "ed\u00b7lem", "Trotz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "APZR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und als sich auf den Blumenmatten", "tokens": ["Und", "als", "sich", "auf", "den", "Blu\u00b7men\u00b7mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Viel Thier' um ihn versammelt hatten,", "tokens": ["Viel", "Thier'", "um", "ihn", "ver\u00b7sam\u00b7melt", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Blies er die Backen auf und sprach:", "tokens": ["Blies", "er", "die", "Ba\u00b7cken", "auf", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "F\u00fchlt etwan wer ein Ungemach", "tokens": ["F\u00fchlt", "et\u00b7wan", "wer", "ein", "Un\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PWS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "An Leber, Lunge, Milz und Herzen;", "tokens": ["An", "Le\u00b7ber", ",", "Lun\u00b7ge", ",", "Milz", "und", "Her\u00b7zen", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Hat einer Pein, und gro\u00dfe Schmerzen", "tokens": ["Hat", "ei\u00b7ner", "Pein", ",", "und", "gro\u00b7\u00dfe", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Von Podagra, von Stein und Gicht;", "tokens": ["Von", "Po\u00b7da\u00b7gra", ",", "von", "Stein", "und", "Gicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Hat einer keine Oeffnung nicht;", "tokens": ["Hat", "ei\u00b7ner", "kei\u00b7ne", "Oeff\u00b7nung", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ist er von hektischer Natur;", "tokens": ["Ist", "er", "von", "hek\u00b7ti\u00b7scher", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.20": {"text": "Liegt er am Fieber, an der Ruhr,", "tokens": ["Liegt", "er", "am", "Fie\u00b7ber", ",", "an", "der", "Ruhr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "An Cachexie, Epilepsie,", "tokens": ["An", "Cach\u00b7e\u00b7xie", ",", "E\u00b7pi\u00b7le\u00b7psie", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "An Agrypnie, Hydropisie;", "tokens": ["An", "Ag\u00b7ryp\u00b7nie", ",", "Hyd\u00b7ro\u00b7pi\u00b7sie", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Hat er den Appetit verloren,", "tokens": ["Hat", "er", "den", "Ap\u00b7pe\u00b7tit", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "F\u00fchlt Sausen, Brausen in den Ohren \u2013", "tokens": ["F\u00fchlt", "Sau\u00b7sen", ",", "Brau\u00b7sen", "in", "den", "Oh\u00b7ren", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Der trete dreist zu mir heran,", "tokens": ["Der", "tre\u00b7te", "dreist", "zu", "mir", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und nehme von mir Tropfen an!", "tokens": ["Und", "neh\u00b7me", "von", "mir", "Trop\u00b7fen", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Honnette Herr'n nach Standsgeb\u00fchr,", "tokens": ["Hon\u00b7net\u00b7te", "Herr'n", "nach", "Stands\u00b7ge\u00b7b\u00fchr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Sie sehn den gr\u00f6\u00dften Doktor hier!", "tokens": ["Sie", "sehn", "den", "gr\u00f6\u00df\u00b7ten", "Dok\u00b7tor", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Ich bin die halbe Welt durchreist,", "tokens": ["Ich", "bin", "die", "hal\u00b7be", "Welt", "durc\u00b7hreist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Und meinen gro\u00dfen Namen preist", "tokens": ["Und", "mei\u00b7nen", "gro\u00b7\u00dfen", "Na\u00b7men", "preist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Paris und London, Wien und Rom,", "tokens": ["Pa\u00b7ris", "und", "Lon\u00b7don", ",", "Wi\u00b7en", "und", "Rom", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$,", "NE", "KON", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.32": {"text": "Der Rhein, der Main, der Donaustrom,", "tokens": ["Der", "Rhein", ",", "der", "Main", ",", "der", "Do\u00b7naus\u00b7trom", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Denn Alles hab' ich ausstudirt,", "tokens": ["Denn", "Al\u00b7les", "hab'", "ich", "aus\u00b7stu\u00b7dirt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Und Tausende hab' ich kurirt!", "tokens": ["Und", "Tau\u00b7sen\u00b7de", "hab'", "ich", "ku\u00b7rirt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Die Thiere glaubten ihm zum Theil,", "tokens": ["Die", "Thie\u00b7re", "glaub\u00b7ten", "ihm", "zum", "Theil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kamen schon in gro\u00dfer Eil'", "tokens": ["Und", "ka\u00b7men", "schon", "in", "gro\u00b7\u00dfer", "Eil'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von allen Ecken hergelaufen,", "tokens": ["Von", "al\u00b7len", "E\u00b7cken", "her\u00b7ge\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um Arzenei von ihm zu kaufen;", "tokens": ["Um", "Ar\u00b7ze\u00b7nei", "von", "ihm", "zu", "kau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da rief der Fuchs: Ihr armen Thoren!", "tokens": ["Da", "rief", "der", "Fuchs", ":", "Ihr", "ar\u00b7men", "Tho\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sagt, habt ihr den Verstand verloren?", "tokens": ["Sagt", ",", "habt", "ihr", "den", "Ver\u00b7stand", "ver\u00b7lo\u00b7ren", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Seht euren Doktor doch recht an,", "tokens": ["Seht", "eu\u00b7ren", "Dok\u00b7tor", "doch", "recht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er ist ja selber \u00fcbel d'ran!", "tokens": ["Er", "ist", "ja", "sel\u00b7ber", "\u00fc\u00b7bel", "d'\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Die Augen stehn ihm aus dem Kopf,", "tokens": ["Die", "Au\u00b7gen", "stehn", "ihm", "aus", "dem", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Brust kocht wie ein alter Topf,", "tokens": ["Die", "Brust", "kocht", "wie", "ein", "al\u00b7ter", "Topf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der Mund ist bla\u00df, der Fu\u00df geschwollen;", "tokens": ["Der", "Mund", "ist", "bla\u00df", ",", "der", "Fu\u00df", "ge\u00b7schwol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der dicke Bauch hervorgequollen;", "tokens": ["Der", "di\u00b7cke", "Bauch", "her\u00b7vor\u00b7ge\u00b7quol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Kann Er hievon sich nicht befrei'n,", "tokens": ["Kann", "Er", "hie\u00b7von", "sich", "nicht", "be\u00b7frei'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PAV", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wie will er And'rer Doktor seyn?", "tokens": ["Wie", "will", "er", "An\u00b7d'\u00b7rer", "Dok\u00b7tor", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJA", "NN", "VAINF", "$."], "meter": "-+----+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Aus einem Teiche voller Rohr", "tokens": ["Aus", "ei\u00b7nem", "Tei\u00b7che", "vol\u00b7ler", "Rohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"te