{"dta.poem.21322": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput VI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Doch jener erwiederte trockenen Tons,", "tokens": ["Doch", "je\u00b7ner", "er\u00b7wie\u00b7der\u00b7te", "tro\u00b7cke\u00b7nen", "Tons", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sogar ein bischen phlegmatisch:", "tokens": ["So\u00b7gar", "ein", "bi\u00b7schen", "phleg\u00b7ma\u00b7tisch", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "ADJD", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "\u201eich bitte dich, exorzire mich nicht,", "tokens": ["\u201e", "ich", "bit\u00b7te", "dich", ",", "ex\u00b7or\u00b7zi\u00b7re", "mich", "nicht", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und werde nur nicht emphatisch!", "tokens": ["Und", "wer\u00b7de", "nur", "nicht", "em\u00b7pha\u00b7tisch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201eich bin kein Gespenst der Vergangenheit,", "tokens": ["\u201e", "ich", "bin", "kein", "Ge\u00b7spenst", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Kein grabentstiegener Strohwisch,", "tokens": ["Kein", "gra\u00b7bent\u00b7stie\u00b7ge\u00b7ner", "Stroh\u00b7wisch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und von Rhetorik bin ich kein Freund,", "tokens": ["Und", "von", "Rhe\u00b7to\u00b7rik", "bin", "ich", "kein", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Bin auch nicht sehr philosophisch.", "tokens": ["Bin", "auch", "nicht", "sehr", "phi\u00b7lo\u00b7so\u00b7phisch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201eich bin von praktischer Natur,", "tokens": ["\u201e", "ich", "bin", "von", "prak\u00b7ti\u00b7scher", "Na\u00b7tur", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und immer schweigsam und ruhig.", "tokens": ["Und", "im\u00b7mer", "schweig\u00b7sam", "und", "ru\u00b7hig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch wisse: was du ersonnen im Geist\u2019,", "tokens": ["Doch", "wis\u00b7se", ":", "was", "du", "er\u00b7son\u00b7nen", "im", "Geist'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das f\u00fchr\u2019 ich aus, das thu\u2019 ich.", "tokens": ["Das", "f\u00fchr'", "ich", "aus", ",", "das", "thu'", "ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$,", "PDS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201eund gehn auch Jahre dr\u00fcber hin,", "tokens": ["\u201e", "und", "gehn", "auch", "Jah\u00b7re", "dr\u00fc\u00b7ber", "hin", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "ADV", "NN", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich raste nicht, bis ich verwandl", "tokens": ["Ich", "ras\u00b7te", "nicht", ",", "bis", "ich", "ver\u00b7wandl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "NE"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "In Wirklichkeit was du gedacht;", "tokens": ["In", "Wirk\u00b7lich\u00b7keit", "was", "du", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du denkst, und ich, ich handle.", "tokens": ["Du", "denkst", ",", "und", "ich", ",", "ich", "hand\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}