{"textgrid.poem.62423": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Auf seines Bruders Tod", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er focht in sieben Schlachten,", "tokens": ["Er", "focht", "in", "sie\u00b7ben", "Schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er war ein deutsches Blut,", "tokens": ["Er", "war", "ein", "deut\u00b7sches", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gefahr hie\u00df ihn verachten", "tokens": ["Ge\u00b7fahr", "hie\u00df", "ihn", "ver\u00b7ach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein stiller Kriegesmuth.", "tokens": ["Sein", "stil\u00b7ler", "Krie\u00b7ges\u00b7muth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das Schwert an seiner Linken,", "tokens": ["Das", "Schwert", "an", "sei\u00b7ner", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er nannt es seine Braut;", "tokens": ["Er", "nannt", "es", "sei\u00b7ne", "Braut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geneigter Blicke Winken,", "tokens": ["Ge\u00b7neig\u00b7ter", "Bli\u00b7cke", "Win\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das schien ihm kaum so traut.", "tokens": ["Das", "schien", "ihm", "kaum", "so", "traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bei Hochkirch ihn umfangen", "tokens": ["Bei", "Hoch\u00b7kirch", "ihn", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hab' ich mit Liebesgru\u00df", "tokens": ["Hab'", "ich", "mit", "Lie\u00b7bes\u00b7gru\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ahnungsvoll empfangen", "tokens": ["Und", "ah\u00b7nungs\u00b7voll", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den letzten hei\u00dfen Ku\u00df.", "tokens": ["Den", "letz\u00b7ten", "hei\u00b7\u00dfen", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es schlug die sch\u00f6ne Stunde,", "tokens": ["Es", "schlug", "die", "sch\u00f6\u00b7ne", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ward sein Busen roth,", "tokens": ["Da", "ward", "sein", "Bu\u00b7sen", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So blutet an der Wunde", "tokens": ["So", "blu\u00b7tet", "an", "der", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein edler Hirsch sich todt.", "tokens": ["Ein", "ed\u00b7ler", "Hirsch", "sich", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Tragt nach den Riesenbergen", "tokens": ["Tragt", "nach", "den", "Rie\u00b7sen\u00b7ber\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Den kranken Ritter nun,", "tokens": ["Den", "kran\u00b7ken", "Rit\u00b7ter", "nun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es darf ja nicht bei Zwergen", "tokens": ["Es", "darf", "ja", "nicht", "bei", "Zwer\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der fromme Degen ruhn.", "tokens": ["Der", "from\u00b7me", "De\u00b7gen", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der V\u00e4ter freie Erde", "tokens": ["Der", "V\u00e4\u00b7ter", "frei\u00b7e", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er sich erlesen hat,", "tokens": ["Er", "sich", "er\u00b7le\u00b7sen", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du Stadt des Hirsches werde", "tokens": ["Du", "Stadt", "des", "Hir\u00b7sches", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr ihn die Ruhestadt.", "tokens": ["F\u00fcr", "ihn", "die", "Ru\u00b7he\u00b7stadt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das schwarze Kreuz, das blaue,", "tokens": ["Das", "schwar\u00b7ze", "Kreuz", ",", "das", "blau\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "H\u00e4ngt auf dem Grabes-Baum,", "tokens": ["H\u00e4ngt", "auf", "dem", "Gra\u00b7bes\u00b7Baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df jeder Pilger schaue,", "tokens": ["Da\u00df", "je\u00b7der", "Pil\u00b7ger", "schau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer tr\u00e4umt hier seinen Traum.", "tokens": ["Wer", "tr\u00e4umt", "hier", "sei\u00b7nen", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Fahr' Bruder wohl, Gespiele", "tokens": ["Fahr'", "Bru\u00b7der", "wohl", ",", "Ge\u00b7spie\u00b7le"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "NN", "ADV", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In froher Kinderzeit,", "tokens": ["In", "fro\u00b7her", "Kin\u00b7der\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du schrittest vor zum Ziele,", "tokens": ["Du", "schrit\u00b7test", "vor", "zum", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du J\u00fcngerer, wie weit.", "tokens": ["Du", "J\u00fcn\u00b7ge\u00b7rer", ",", "wie", "weit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Hoffnung lie\u00df mich kommen,", "tokens": ["Die", "Hoff\u00b7nung", "lie\u00df", "mich", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ob ich dich lebend f\u00e4nd'?", "tokens": ["Ob", "ich", "dich", "le\u00b7bend", "f\u00e4nd'", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch, du warst aufgenommen", "tokens": ["Doch", ",", "du", "warst", "auf\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins reine Element.", "tokens": ["Ins", "rei\u00b7ne", "E\u00b7le\u00b7ment", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Zeuch hin, wo Karl der Gro\u00dfe,", "tokens": ["Zeuch", "hin", ",", "wo", "Karl", "der", "Gro\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "NE", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo Gottfried, Balduin", "tokens": ["Wo", "Gott\u00b7fried", ",", "Bal\u00b7du\u00b7in"], "token_info": ["word", "word", "punct", "word"], "pos": ["PWAV", "NE", "$,", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Siegs- und Todesloose", "tokens": ["Die", "Siegs", "und", "To\u00b7des\u00b7loo\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr Gottes Krieger ziehn.", "tokens": ["F\u00fcr", "Got\u00b7tes", "Krie\u00b7ger", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wol gr\u00f6\u00df're S\u00fcnden b\u00fc\u00dfen", "tokens": ["Wol", "gr\u00f6\u00df'\u00b7re", "S\u00fcn\u00b7den", "b\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Kann solch' ein Glaubenstod;", "tokens": ["Kann", "solch'", "ein", "Glau\u00b7ben\u00b7stod", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Vater magst du gr\u00fc\u00dfen", "tokens": ["Den", "Va\u00b7ter", "magst", "du", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], 