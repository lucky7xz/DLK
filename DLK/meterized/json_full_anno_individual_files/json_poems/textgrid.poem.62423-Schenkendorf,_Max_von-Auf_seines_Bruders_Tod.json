, "measure": "iambic.tri"}, "line.2": {"text": "Er sich erlesen hat,", "tokens": ["Er", "sich", "er\u00b7le\u00b7sen", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du Stadt des Hirsches werde", "tokens": ["Du", "Stadt", "des", "Hir\u00b7sches", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr ihn die Ruhestadt.", "tokens": ["F\u00fcr", "ihn", "die", "Ru\u00b7he\u00b7stadt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Das schwarze Kreuz, das blaue,", "tokens": ["Das", "schwar\u00b7ze", "Kreuz", ",", "das", "blau\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "H\u00e4ngt auf dem Grabes-Baum,", "tokens": ["H\u00e4ngt", "auf", "dem", "Gra\u00b7bes\u00b7Baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df jeder Pilger schaue,", "tokens": ["Da\u00df", "je\u00b7der", "Pil\u00b7ger", "schau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer tr\u00e4umt hier seinen Traum.", "tokens": ["Wer", "tr\u00e4umt", "hier", "sei\u00b7nen", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Fahr' Bruder wohl, Gespiele", "tokens": ["Fahr'", "Bru\u00b7der", "wohl", ",", "Ge\u00b7spie\u00b7le"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "NN", "ADV", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In froher Kinderzeit,", "tokens": ["In", "fro\u00b7her", "Kin\u00b7der\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du schrittest vor zum Ziele,", "tokens": ["Du", "schrit\u00b7test", "vor", "zum", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du J\u00fcngerer, wie weit.", "tokens": ["Du", "J\u00fcn\u00b7ge\u00b7rer", ",", "wie", "weit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Die Hoffnung lie\u00df mich kommen,", "tokens": ["Die", "Hoff\u00b7nung", "lie\u00df", "mich", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ob ich dich lebend f\u00e4nd'?", "tokens": ["Ob", "ich", "dich", "le\u00b7bend", "f\u00e4nd'", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch, du warst aufgenommen", "tokens": ["Doch", ",", "du", "warst", "auf\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins reine Element.", "tokens": ["Ins", "rei\u00b7ne", "E\u00b7le\u00b7ment", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Zeuch hin, wo Karl der Gro\u00dfe,", "tokens": ["Zeuch", "hin", ",", "wo", "Karl", "der", "Gro\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "NE", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo Gottfried, Balduin", "tokens": ["Wo", "Gott\u00b7fried", ",", "Bal\u00b7du\u00b7in"], "token_info": ["word", "word", "punct", "word"], "pos": ["PWAV", "NE", "$,", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Siegs- und Todesloose", "tokens": ["Die", "Siegs", "und", "To\u00b7des\u00b7loo\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr Gottes Krieger ziehn.", "tokens": ["F\u00fcr", "Got\u00b7tes", "Krie\u00b7ger", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Wol gr\u00f6\u00df're S\u00fcnden b\u00fc\u00dfen", "tokens": ["Wol", "gr\u00f6\u00df'\u00b7re", "S\u00fcn\u00b7den", "b\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Kann solch' ein Glaubenstod;", "tokens": ["Kann", "solch'", "ein", "Glau\u00b7ben\u00b7stod", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Vater magst du gr\u00fc\u00dfen", "tokens": ["Den", "Va\u00b7ter", "magst", "du", "gr\u00fc\u00b7\u00dfen"], 