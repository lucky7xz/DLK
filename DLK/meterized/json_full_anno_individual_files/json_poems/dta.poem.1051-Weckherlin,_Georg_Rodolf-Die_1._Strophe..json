{"dta.poem.1051": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die 1. Strophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich wie ein Patron/ welcher lang", "tokens": ["Gleich", "wie", "ein", "Pat\u00b7ron", "/", "wel\u00b7cher", "lang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "$(", "PRELS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein schif nach nohturft wol versehe\u0303/", "tokens": ["Sein", "schif", "nach", "noh\u00b7turft", "wol", "ver\u00b7seh\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "APPR", "VVFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Pfleget in des hafens au\u00dfgang", "tokens": ["Pfle\u00b7get", "in", "des", "ha\u00b7fens", "au\u00df\u00b7gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Erwartend guten wind zustehen/", "tokens": ["Er\u00b7war\u00b7tend", "gu\u00b7ten", "wind", "zu\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Damit Er mit behertzter hand", "tokens": ["Da\u00b7mit", "Er", "mit", "be\u00b7hertz\u00b7ter", "hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "M\u00f6ge seine segel aufziehen/", "tokens": ["M\u00f6\u00b7ge", "sei\u00b7ne", "se\u00b7gel", "auf\u00b7zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Vnd der armut b\u00e4lder entfliehen", "tokens": ["Vnd", "der", "ar\u00b7mut", "b\u00e4l\u00b7der", "ent\u00b7flie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Durch des winds gl\u00fccklichem beistand:", "tokens": ["Durch", "des", "winds", "gl\u00fcck\u00b7li\u00b7chem", "bei\u00b7stand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Also will Ich mich nicht bew\u00f6gen/", "tokens": ["Al\u00b7so", "will", "Ich", "mich", "nicht", "be\u00b7w\u00f6\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$("], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.10": {"text": "O mein Printz/ meine zuversicht/", "tokens": ["O", "mein", "Printz", "/", "mei\u00b7ne", "zu\u00b7ver\u00b7sicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Bi\u00df jhr meiner Musen verm\u00f6gen", "tokens": ["Bi\u00df", "jhr", "mei\u00b7ner", "Mu\u00b7sen", "ver\u00b7m\u00f6\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.12": {"text": "Mit verhilflichem angesicht", "tokens": ["Mit", "ver\u00b7hilf\u00b7li\u00b7chem", "an\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "Werdet eine seglung aufl\u00f6gen.", "tokens": ["Wer\u00b7det", "ei\u00b7ne", "se\u00b7glung", "auf\u00b7l\u00f6\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVIZU", "$."], "meter": "+-+----+-", "measure": "unknown.measure.tri"}}}}}