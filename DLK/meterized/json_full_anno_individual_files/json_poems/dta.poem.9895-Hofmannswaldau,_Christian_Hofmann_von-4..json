{"dta.poem.9895": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es geschieht durch meine krafft/ da\u00df ein mensch kan l\u00e4nger lebe\u0303/", "tokens": ["Es", "ge\u00b7schieht", "durch", "mei\u00b7ne", "krafft", "/", "da\u00df", "ein", "mensch", "kan", "l\u00e4n\u00b7ger", "le\u00b7b\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "KOUS", "ART", "NN", "VMFIN", "ADJD", "VVFIN", "$("], "meter": "--+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich kan allem/ auch den tod/ was nur ist und lebet/ geben.", "tokens": ["Ich", "kan", "al\u00b7lem", "/", "auch", "den", "tod", "/", "was", "nur", "ist", "und", "le\u00b7bet", "/", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "$(", "ADV", "ART", "NN", "$(", "PWS", "ADV", "VAFIN", "KON", "VVFIN", "$(", "VVINF", "$."], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da/ wenn alles mu\u00df vergehen/ werd ich bleiben nur allein/", "tokens": ["Da", "/", "wenn", "al\u00b7les", "mu\u00df", "ver\u00b7ge\u00b7hen", "/", "werd", "ich", "blei\u00b7ben", "nur", "al\u00b7lein", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PIS", "VMFIN", "VVINF", "$(", "VAFIN", "PPER", "VVFIN", "ADV", "ADV", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Meinen platz alsdenn ver\u00e4ndern/ ewig und unsterblich seyn.", "tokens": ["Mei\u00b7nen", "platz", "als\u00b7denn", "ver\u00b7\u00e4n\u00b7dern", "/", "e\u00b7wig", "und", "uns\u00b7terb\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVIMP", "ADV", "VVINF", "$(", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}