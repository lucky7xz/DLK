{"dta.poem.12037": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist die Tugend? Schrank' und Ma\u00df der Menschenkraft;", "tokens": ["Was", "ist", "die", "Tu\u00b7gend", "?", "Schrank'", "und", "Ma\u00df", "der", "Men\u00b7schen\u00b7kraft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum Menschentugend ist gleich Menschen mangelhaft.", "tokens": ["Drum", "Men\u00b7schen\u00b7tu\u00b7gend", "ist", "gleich", "Men\u00b7schen", "man\u00b7gel\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "VAFIN", "ADV", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und manches was f\u00fcr uns hier Tugend ist auf Erden,", "tokens": ["Und", "man\u00b7ches", "was", "f\u00fcr", "uns", "hier", "Tu\u00b7gend", "ist", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "APPR", "PPER", "ADV", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird keine seyn, wenn wir einst mehr als Menschen werden.", "tokens": ["Wird", "kei\u00b7ne", "seyn", ",", "wenn", "wir", "einst", "mehr", "als", "Men\u00b7schen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "VAINF", "$,", "KOUS", "PPER", "ADV", "ADV", "KOUS", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So ists auch nicht f\u00fcr die, die mehr als Menschen sind,", "tokens": ["So", "ists", "auch", "nicht", "f\u00fcr", "die", ",", "die", "mehr", "als", "Men\u00b7schen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PTKNEG", "APPR", "ART", "$,", "PRELS", "PIS", "KOKOM", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch rechnen sie dir's an als Tugend, Menschenkind!", "tokens": ["Doch", "rech\u00b7nen", "sie", "dir's", "an", "als", "Tu\u00b7gend", ",", "Men\u00b7schen\u00b7kind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "PTKVZ", "KOKOM", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}