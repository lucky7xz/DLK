{"textgrid.poem.37467": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seine Meinung ist die rechte,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seine Meinung ist die rechte,", "tokens": ["Sei\u00b7ne", "Mei\u00b7nung", "ist", "die", "rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn er spricht, m\u00fc\u00dft ihr verstummen,", "tokens": ["Wenn", "er", "spricht", ",", "m\u00fc\u00dft", "ihr", "ver\u00b7stum\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonst erkl\u00e4rt er euch f\u00fcr Schlechte,", "tokens": ["Sonst", "er\u00b7kl\u00e4rt", "er", "euch", "f\u00fcr", "Schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder nennt euch gar die Dummen.", "tokens": ["O\u00b7der", "nennt", "euch", "gar", "die", "Dum\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Leider sind dergleichen Strolche", "tokens": ["Lei\u00b7der", "sind", "derg\u00b7lei\u00b7chen", "Strol\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keine seltene Erscheinung.", "tokens": ["Kei\u00b