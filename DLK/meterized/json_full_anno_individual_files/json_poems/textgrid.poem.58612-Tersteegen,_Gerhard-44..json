{"textgrid.poem.58612": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer den Herren f\u00fcrchtet nur, seiner Stimme folget gerne", "tokens": ["Wer", "den", "Her\u00b7ren", "f\u00fcrch\u00b7tet", "nur", ",", "sei\u00b7ner", "Stim\u00b7me", "fol\u00b7get", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und im Finstern wandeln mu\u00df, denke nicht: der Herr ist ferne!", "tokens": ["Und", "im", "Fins\u00b7tern", "wan\u00b7deln", "mu\u00df", ",", "den\u00b7ke", "nicht", ":", "der", "Herr", "ist", "fer\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "VMFIN", "$,", "VVFIN", "PTKNEG", "$.", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Scheint dir gleich die Sonne nicht, so ist doch die Sonne da,", "tokens": ["Scheint", "dir", "gleich", "die", "Son\u00b7ne", "nicht", ",", "so", "ist", "doch", "die", "Son\u00b7ne", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "$,", "ADV", "VAFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Nach der Finsternis kommt Licht; suchst du Gott, so ist er nah.", "tokens": ["Nach", "der", "Fins\u00b7ter\u00b7nis", "kommt", "Licht", ";", "suchst", "du", "Gott", ",", "so", "ist", "er", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$.", "VVFIN", "PPER", "NN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Werd im Dunkeln nicht verzagt, hoffe auf des Herren Namen,", "tokens": ["Werd", "im", "Dun\u00b7keln", "nicht", "ver\u00b7zagt", ",", "hof\u00b7fe", "auf", "des", "Her\u00b7ren", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PTKNEG", "VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Denk, Jehova ist getreu, was er spricht, ist Ja und Amen,", "tokens": ["Denk", ",", "Je\u00b7ho\u00b7va", "ist", "ge\u00b7treu", ",", "was", "er", "spricht", ",", "ist", "Ja", "und", "A\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "VAFIN", "ADJD", "$,", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Lehne dich auf deinen Gott, halt ihn fest, so f\u00e4llst du nicht,", "tokens": ["Leh\u00b7ne", "dich", "auf", "dei\u00b7nen", "Gott", ",", "halt", "ihn", "fest", ",", "so", "f\u00e4llst", "du", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Glaub, du g