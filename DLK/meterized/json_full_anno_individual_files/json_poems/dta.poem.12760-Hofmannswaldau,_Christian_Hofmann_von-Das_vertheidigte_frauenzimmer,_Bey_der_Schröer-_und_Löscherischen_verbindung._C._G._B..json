{"dta.poem.12760": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das vertheidigte frauenzimmer,  \n Bey der Schr\u00f6er- und L\u00f6scherischen  \n verbindung.  \n C. G. B.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So offt als uns die lust zu fremden schrifften treibt,", "tokens": ["So", "offt", "als", "uns", "die", "lust", "zu", "frem\u00b7den", "schriff\u00b7ten", "treibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In denen man von thun der-alten weisen schreibt,", "tokens": ["In", "de\u00b7nen", "man", "von", "thun", "der\u00b7al\u00b7ten", "wei\u00b7sen", "schreibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "APPR", "VVINF", "VMFIN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So dencken wir, weil sie auf lieb und weiber schmehlen:", "tokens": ["So", "den\u00b7cken", "wir", ",", "weil", "sie", "auf", "lieb", "und", "wei\u00b7ber", "schmeh\u00b7len", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "ADJD", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es mu\u00df den weisen hier wol an verstande fehlen.", "tokens": ["Es", "mu\u00df", "den", "wei\u00b7sen", "hier", "wol", "an", "ver\u00b7stan\u00b7de", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "ADV", "ADV", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir stutzen, wann ihr spruch die liebe so veracht.", "tokens": ["Wir", "stut\u00b7zen", ",", "wann", "ihr", "spruch", "die", "lie\u00b7be", "so", "ver\u00b7acht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "ART", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und das galante volck den teuffeln \u00e4hnlich macht,", "tokens": ["Und", "das", "ga\u00b7lan\u00b7te", "volck", "den", "teuf\u00b7feln", "\u00e4hn\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wann sie den liebes-trieb ein tolles wesen nennen,", "tokens": ["Wann", "sie", "den", "lie\u00b7bes\u00b7trieb", "ein", "tol\u00b7les", "we\u00b7sen", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wann sie die weiber gar vor menschen nicht erkennen,", "tokens": ["Wann", "sie", "die", "wei\u00b7ber", "gar", "vor", "men\u00b7schen", "nicht", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Von denen sie doch selbst zur welt geboren seyn;", "tokens": ["Von", "de\u00b7nen", "sie", "doch", "selbst", "zur", "welt", "ge\u00b7bo\u00b7ren", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "APPRART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie bilden beyde sich als ungeheuer ein,", "tokens": ["Sie", "bil\u00b7den", "bey\u00b7de", "sich", "als", "un\u00b7ge\u00b7heu\u00b7er", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PRF", "KOUS", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und meynen: liebe kan nicht bey der weisheit stehen;", "tokens": ["Und", "mey\u00b7nen", ":", "lie\u00b7be", "kan", "nicht", "bey", "der", "weis\u00b7heit", "ste\u00b7hen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVFIN", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die weiber nimmer sich mit musen wohlbegehen,", "tokens": ["Die", "wei\u00b7ber", "nim\u00b7mer", "sich", "mit", "mu\u00b7sen", "wohl\u00b7be\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PRF", "APPR", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und dennoch bauen sie der liebe gar ein hau\u00df,", "tokens": ["Und", "den\u00b7noch", "bau\u00b7en", "sie", "der", "lie\u00b7be", "gar", "ein", "hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und schm\u00fccken Musen so, als wie die weiber aus.", "tokens": ["Und", "schm\u00fc\u00b7cken", "Mu\u00b7sen", "so", ",", "als", "wie", "die", "wei\u00b7ber", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "$,", "KOUS", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Warum? Jhr sanffter sinn, ihr angenehmes wesen,", "tokens": ["Wa\u00b7rum", "?", "Ihr", "sanff\u00b7ter", "sinn", ",", "ihr", "an\u00b7ge\u00b7neh\u00b7mes", "we\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Steht Musen besser an, als wann sie sich erlesen,", "tokens": ["Steht", "Mu\u00b7sen", "bes\u00b7ser", "an", ",", "als", "wann", "sie", "sich", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJD", "PTKVZ", "$,", "KOUS", "PWAV", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der m\u00e4nner allzufrech- und kriegerischen muth;", "tokens": ["Der", "m\u00e4n\u00b7ner", "all\u00b7zu\u00b7frech", "und", "krie\u00b7ge\u00b7ri\u00b7schen", "muth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ein sanfftes wesen ist allein zur weisheit gut.", "tokens": ["Ein", "sanff\u00b7tes", "we\u00b7sen", "ist", "al\u00b7lein", "zur", "weis\u00b7heit", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Gewi\u00df, wofern wir uns hier sollen recht erkl\u00e4ren:", "tokens": ["Ge\u00b7wi\u00df", ",", "wo\u00b7fern", "wir", "uns", "hier", "sol\u00b7len", "recht", "er\u00b7kl\u00e4\u00b7ren", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "PPER", "ADV", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wann keine liebe mehr und keine weiber w\u00e4ren,", "tokens": ["Wann", "kei\u00b7ne", "lie\u00b7be", "mehr", "und", "kei\u00b7ne", "wei\u00b7ber", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "VVFIN", "ADV", "KON", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wo w\u00fcrde welt und mensch in wenig jahren seyn?", "tokens": ["Wo", "w\u00fcr\u00b7de", "welt", "und", "mensch", "in", "we\u00b7nig", "jah\u00b7ren", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "KON", "ADJD", "APPR", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die liebe fl\u00f6\u00dft uns ja das leben selber ein;", "tokens": ["Die", "lie\u00b7be", "fl\u00f6\u00dft", "uns", "ja", "das", "le\u00b7ben", "sel\u00b7ber", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ADV", "PDS", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Sie ist der geist der welt; sie h\u00e4lt durch ihre flammen", "tokens": ["Sie", "ist", "der", "geist", "der", "welt", ";", "sie", "h\u00e4lt", "durch", "ih\u00b7re", "flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Den wunder-vollen bau noch immerdar zusammen;", "tokens": ["Den", "wun\u00b7der\u00b7vol\u00b7len", "bau", "noch", "im\u00b7mer\u00b7dar", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der ist ein halber mensch, der ohne liebe lebt;", "tokens": ["Der", "ist", "ein", "hal\u00b7ber", "mensch", ",", "der", "oh\u00b7ne", "lie\u00b7be", "lebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die liebe tr\u00f6stet uns, wann erd und himmel bebt;", "tokens": ["Die", "lie\u00b7be", "tr\u00f6s\u00b7tet", "uns", ",", "wann", "erd", "und", "him\u00b7mel", "bebt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "$,", "PWAV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die liebe hindert uns nicht an der weisheit sch\u00e4tzen,", "tokens": ["Die", "lie\u00b7be", "hin\u00b7dert", "uns", "nicht", "an", "der", "weis\u00b7heit", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wer an den k\u00fcnsten sich nur suchet zu ergetzen,", "tokens": ["Wer", "an", "den", "k\u00fcns\u00b7ten", "sich", "nur", "su\u00b7chet", "zu", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "PRF", "ADV", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Den reitzt ihr feuer mehr zu flei\u00df und eifer an,", "tokens": ["Den", "reitzt", "ihr", "feu\u00b7er", "mehr", "zu", "flei\u00df", "und", "ei\u00b7fer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "NN", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und leitet seinen fu\u00df auf einer freuden-bahn.", "tokens": ["Und", "lei\u00b7tet", "sei\u00b7nen", "fu\u00df", "auf", "ei\u00b7ner", "freu\u00b7den\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Di\u00df kan die liebe thun; nicht weniger verdienen", "tokens": ["Di\u00df", "kan", "die", "lie\u00b7be", "thun", ";", "nicht", "we\u00b7ni\u00b7ger", "ver\u00b7die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "VVINF", "$.", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die weiber hohes lob, indem allein mit ihnen", "tokens": ["Die", "wei\u00b7ber", "ho\u00b7hes", "lob", ",", "in\u00b7dem", "al\u00b7lein", "mit", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "KOUS", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Zugleich die liebe lebt, und auch zugleich erstirbt;", "tokens": ["Zu\u00b7gleich", "die", "lie\u00b7be", "lebt", ",", "und", "auch", "zu\u00b7gleich", "er\u00b7stirbt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "$,", "KON", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Sie sind das sch\u00f6ne volck, das alle gunst erwirbt;", "tokens": ["Sie", "sind", "das", "sch\u00f6\u00b7ne", "volck", ",", "das", "al\u00b7le", "gunst", "er\u00b7wirbt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Jhr angenehmer blick kan unsre brust erquicken,", "tokens": ["Ihr", "an\u00b7ge\u00b7neh\u00b7mer", "blick", "kan", "uns\u00b7re", "brust", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wenn uns der kummer fast die geister will ersticken;", "tokens": ["Wenn", "uns", "der", "kum\u00b7mer", "fast", "die", "geis\u00b7ter", "will", "er\u00b7sti\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Sie sind das edelste, womit die erde prangt,", "tokens": ["Sie", "sind", "das", "e\u00b7dels\u00b7te", ",", "wo\u00b7mit", "die", "er\u00b7de", "prangt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und der ist recht begl\u00fcckt, der ihre gunst erlangt.", "tokens": ["Und", "der", "ist", "recht", "be\u00b7gl\u00fcckt", ",", "der", "ih\u00b7re", "gunst", "er\u00b7langt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "ADJD", "VVPP", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wir wollen itzund nicht ihr gantzes lob bezeugen,", "tokens": ["Wir", "wol\u00b7len", "it\u00b7zund", "nicht", "ihr", "gant\u00b7zes", "lob", "be\u00b7zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Doch dieses k\u00f6nnen wir von ihnen nicht verschweigen:", "tokens": ["Doch", "die\u00b7ses", "k\u00f6n\u00b7nen", "wir", "von", "ih\u00b7nen", "nicht", "ver\u00b7schwei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PPER", "APPR", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wenn sie das alterthum aus unsern armen rei\u00dft,", "tokens": ["Wenn", "sie", "das", "al\u00b7ter\u00b7thum", "aus", "un\u00b7sern", "ar\u00b7men", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und einen weisen mann vor ihnen fliehen heist;", "tokens": ["Und", "ei\u00b7nen", "wei\u00b7sen", "mann", "vor", "ih\u00b7nen", "flie\u00b7hen", "heist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Weil sie den musen feind und unsern flei\u00df vermindern,", "tokens": ["Weil", "sie", "den", "mu\u00b7sen", "feind", "und", "un\u00b7sern", "flei\u00df", "ver\u00b7min\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ja gar durch ihren tand uns im studiren hindern;", "tokens": ["Ja", "gar", "durch", "ih\u00b7ren", "tand", "uns", "im", "stu\u00b7di\u00b7ren", "hin\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPR", "PPOSAT", "VVFIN", "PPER", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.45": {"text": "So hat ihr eitler wahn nur dieses ausgedacht,", "tokens": ["So", "hat", "ihr", "eit\u00b7ler", "wahn", "nur", "die\u00b7ses", "aus\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "PDAT", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Indem ein weib vielmehr uns alles leichter macht;", "tokens": ["In\u00b7dem", "ein", "weib", "viel\u00b7mehr", "uns", "al\u00b7les", "leich\u00b7ter", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PPER", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Sie nimmt die m\u00fch auf sich vor unser hau\u00df zu sorgen,", "tokens": ["Sie", "nimmt", "die", "m\u00fch", "auf", "sich", "vor", "un\u00b7ser", "hau\u00df", "zu", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "APPR", "PRF", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Nur da\u00df wir ungest\u00f6rt vom abend bis an morgen,", "tokens": ["Nur", "da\u00df", "wir", "un\u00b7ge\u00b7st\u00f6rt", "vom", "a\u00b7bend", "bis", "an", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "APPRART", "ADV", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Und wieder bis zur nacht bey b\u00fcchern k\u00f6nnen stehn;", "tokens": ["Und", "wie\u00b7der", "bis", "zur", "nacht", "bey", "b\u00fc\u00b7chern", "k\u00f6n\u00b7nen", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "APPRART", "NN", "APPR", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Und nicht selbst in die k\u00fcch und keller d\u00fcrffen gehn;", "tokens": ["Und", "nicht", "selbst", "in", "die", "k\u00fcch", "und", "kel\u00b7ler", "d\u00fcrf\u00b7fen", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "APPR", "ART", "ADJD", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Kommt sie bisweilen schon in unsre b\u00fccher-schrancken,", "tokens": ["Kommt", "sie", "bis\u00b7wei\u00b7len", "schon", "in", "uns\u00b7re", "b\u00fc\u00b7cher\u00b7schran\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "So st\u00f6ret sie uns doch nicht leicht in den gedancken,", "tokens": ["So", "st\u00f6\u00b7ret", "sie", "uns", "doch", "nicht", "leicht", "in", "den", "ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Ja ihre liebe steigt wol gar die leiter nauf,", "tokens": ["Ja", "ih\u00b7re", "lie\u00b7be", "steigt", "wol", "gar", "die", "lei\u00b7ter", "nauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "ADJA", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Und schl\u00e4get selber uns die besten b\u00fccher auf;", "tokens": ["Und", "schl\u00e4\u00b7get", "sel\u00b7ber", "uns", "die", "bes\u00b7ten", "b\u00fc\u00b7cher", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Wenn wir denn m\u00fcde seyn, so kan ihr h\u00e4nde-dr\u00fccken,", "tokens": ["Wenn", "wir", "denn", "m\u00fc\u00b7de", "seyn", ",", "so", "kan", "ihr", "h\u00e4n\u00b7de\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAINF", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Jhr ku\u00df, ihr freundlich thun uns ungemein erquicken.", "tokens": ["Ihr", "ku\u00df", ",", "ihr", "freund\u00b7lich", "thun", "uns", "un\u00b7ge\u00b7mein", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "ADJD", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "So mu\u00df es allezeit in diesem hause gehn,", "tokens": ["So", "mu\u00df", "es", "al\u00b7le\u00b7zeit", "in", "die\u00b7sem", "hau\u00b7se", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Wo ein verst\u00e4ndig weib und treue liebe stehn.", "tokens": ["Wo", "ein", "ver\u00b7st\u00e4n\u00b7dig", "weib", "und", "treu\u00b7e", "lie\u00b7be", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "VVFIN", "KON", "ADJA", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Drum, weisen! haltet nur mit eurem spott zur\u00fccke;", "tokens": ["Drum", ",", "wei\u00b7sen", "!", "hal\u00b7tet", "nur", "mit", "eu\u00b7rem", "spott", "zu\u00b7r\u00fc\u00b7cke", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VVFIN", "$.", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Glaubt ihr den worten nicht, so sind noch andre st\u00fccke,", "tokens": ["Glaubt", "ihr", "den", "wor\u00b7ten", "nicht", ",", "so", "sind", "noch", "and\u00b7re", "st\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,", "ADV", "VAFIN", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Damit man euren sinn gewi\u00df besiegen kan;", "tokens": ["Da\u00b7mit", "man", "eu\u00b7ren", "sinn", "ge\u00b7wi\u00df", "be\u00b7sie\u00b7gen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Seht! unsre meynung nimmt der kluge Schr\u00f6er an,", "tokens": ["Seht", "!", "uns\u00b7re", "mey\u00b7nung", "nimmt", "der", "klu\u00b7ge", "Schr\u00f6er", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.63": {"text": "Der will euch ingesamt durch sein exempel lehren,", "tokens": ["Der", "will", "euch", "in\u00b7ge\u00b7samt", "durch", "sein", "ex\u00b7em\u00b7pel", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.64": {"text": "Wie man die liebe soll und eine frau verehren;", "tokens": ["Wie", "man", "die", "lie\u00b7be", "soll", "und", "ei\u00b7ne", "frau", "ver\u00b7eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "VMFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Er geht euch an verstand und hoher weisheit f\u00fcr,", "tokens": ["Er", "geht", "euch", "an", "ver\u00b7stand", "und", "ho\u00b7her", "weis\u00b7heit", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "VVFIN", "KON", "ADJA", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Darum so glaubet ihm, nicht uns: ihm, sagen wir;", "tokens": ["Da\u00b7rum", "so", "glau\u00b7bet", "ihm", ",", "nicht", "uns", ":", "ihm", ",", "sa\u00b7gen", "wir", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "$,", "PTKNEG", "PPER", "$.", "PPER", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Er wird es allbereit aus der erfahrung wissen,", "tokens": ["Er", "wird", "es", "all\u00b7be\u00b7reit", "aus", "der", "er\u00b7fah\u00b7rung", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Indem sein eh-gemahl, da\u00df ihm der tod entrissen,", "tokens": ["In\u00b7dem", "sein", "eh\u00b7ge\u00b7mahl", ",", "da\u00df", "ihm", "der", "tod", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Jhr gantzes leben hat zu seiner lust geweiht,", "tokens": ["Ihr", "gant\u00b7zes", "le\u00b7ben", "hat", "zu", "sei\u00b7ner", "lust", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVINF", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Und ihn bey tr\u00fcber lufft durch ihre treu erfreut.", "tokens": ["Und", "ihn", "bey", "tr\u00fc\u00b7ber", "lufft", "durch", "ih\u00b7re", "treu", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Jtzt hat er wiederum ihm eine braut erkohren,", "tokens": ["Jtzt", "hat", "er", "wie\u00b7de\u00b7rum", "ihm", "ei\u00b7ne", "braut", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "ART", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Mit der die liebligkeit und sch\u00f6nheit ist geboren;", "tokens": ["Mit", "der", "die", "lieb\u00b7lig\u00b7keit", "und", "sch\u00f6n\u00b7heit", "ist", "ge\u00b7bo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "KON", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Es scheint, der himmel selbst hat sie darzu erwehlt,", "tokens": ["Es", "scheint", ",", "der", "him\u00b7mel", "selbst", "hat", "sie", "dar\u00b7zu", "er\u00b7wehlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "ADV", "VAFIN", "PPER", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Indem ein eh-gemahl dem gro\u00dfen Schr\u00f6er fehlt,", "tokens": ["In\u00b7dem", "ein", "eh\u00b7ge\u00b7mahl", "dem", "gro\u00b7\u00dfen", "Schr\u00f6er", "fehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.75": {"text": "Damit er den verlust durch diese kan ersetzen,", "tokens": ["Da\u00b7mit", "er", "den", "ver\u00b7lust", "durch", "die\u00b7se", "kan", "er\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VVFIN", "APPR", "PDS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Die auch ein weiber-feind mu\u00df gantz vollkommen sch\u00e4tzen.", "tokens": ["Die", "auch", "ein", "wei\u00b7ber\u00b7feind", "mu\u00df", "gantz", "voll\u00b7kom\u00b7men", "sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VMFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Di\u00df heisset, theurer mann! in stetem gl\u00fccke stehn:", "tokens": ["Di\u00df", "heis\u00b7set", ",", "theu\u00b7rer", "mann", "!", "in", "ste\u00b7tem", "gl\u00fc\u00b7cke", "stehn", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJD", "NN", "$.", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "N\u00e4chst lie\u00df der himmel dich bis auf den thron erh\u00f6hn,", "tokens": ["N\u00e4chst", "lie\u00df", "der", "him\u00b7mel", "dich", "bis", "auf", "den", "thron", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Jtzt will er einen schatz dir in die h\u00e4nde reichen,", "tokens": ["Jtzt", "will", "er", "ei\u00b7nen", "schatz", "dir", "in", "die", "h\u00e4n\u00b7de", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "PPER", "APPR", "ART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Vor der auch Helena mu\u00df ihre segel streichen;", "tokens": ["Vor", "der", "auch", "He\u00b7le\u00b7na", "mu\u00df", "ih\u00b7re", "se\u00b7gel", "strei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "NE", "VMFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.81": {"text": "Jhr angenehmer blick wird deinen geist erfreun,", "tokens": ["Ihr", "an\u00b7ge\u00b7neh\u00b7mer", "blick", "wird", "dei\u00b7nen", "geist", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.82": {"text": "Und alle speisen dir mit zucker \u00fcberstreun;", "tokens": ["Und", "al\u00b7le", "spei\u00b7sen", "dir", "mit", "zu\u00b7cker", "\u00fc\u00b7bers\u00b7treun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.83": {"text": "Sie wird die sorgen-last zugleiche helffen tragen,", "tokens": ["Sie", "wird", "die", "sor\u00b7gen\u00b7last", "zu\u00b7glei\u00b7che", "helf\u00b7fen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.84": {"text": "Und wann dir ihre hand die b\u00fccher aufgeschlagen,", "tokens": ["Und", "wann", "dir", "ih\u00b7re", "hand", "die", "b\u00fc\u00b7cher", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPOSAT", "NN", "ART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.85": {"text": "So wirst du deine lust auf allen bl\u00e4ttern schaun,", "tokens": ["So", "wirst", "du", "dei\u00b7ne", "lust", "auf", "al\u00b7len", "bl\u00e4t\u00b7tern", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.86": {"text": "Und deine weisheit noch viel besser auferbaun.", "tokens": ["Und", "dei\u00b7ne", "weis\u00b7heit", "noch", "viel", "bes\u00b7ser", "au\u00b7fer\u00b7baun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.87": {"text": "Nun deine Schlesier sind ungemein vergn\u00fcget,", "tokens": ["Nun", "dei\u00b7ne", "Schle\u00b7sier", "sind", "un\u00b7ge\u00b7mein", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.88": {"text": "Weil das gel\u00fccke dich mit lauter rosen wieget;", "tokens": ["Weil", "das", "ge\u00b7l\u00fc\u00b7cke", "dich", "mit", "lau\u00b7ter", "ro\u00b7sen", "wie\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.89": {"text": "Sie sehen diesen tag mit solchen augen an,", "tokens": ["Sie", "se\u00b7hen", "die\u00b7sen", "tag", "mit", "sol\u00b7chen", "au\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.90": {"text": "Daraus man ihre pflicht genugsam sp\u00fcren kan;", "tokens": ["Da\u00b7raus", "man", "ih\u00b7re", "pflicht", "ge\u00b7nug\u00b7sam", "sp\u00fc\u00b7ren", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "PPOSAT", "NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.91": {"text": "Ja ihre treue dir noch besser zu bezeigen,", "tokens": ["Ja", "ih\u00b7re", "treu\u00b7e", "dir", "noch", "bes\u00b7ser", "zu", "be\u00b7zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "ADJA", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.92": {"text": "Soll dieser seuffzer itzt bis in den himmel steigen:", "tokens": ["Soll", "die\u00b7ser", "seuff\u00b7zer", "itzt", "bis", "in", "den", "him\u00b7mel", "stei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "ADJA", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.93": {"text": "Der theure Schr\u00f6er bl\u00fch; es lebe seine braut!", "tokens": ["Der", "theu\u00b7re", "Schr\u00f6er", "bl\u00fch", ";", "es", "le\u00b7be", "sei\u00b7ne", "braut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPOSAT", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.94": {"text": "Vergn\u00fcgung sey mit ihr ihm ewig anvertraut!", "tokens": ["Ver\u00b7gn\u00fc\u00b7gung", "sey", "mit", "ihr", "ihm", "e\u00b7wig", "an\u00b7ver\u00b7traut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.95": {"text": "So wird sich Schlesien an seiner lieb ergetzen,", "tokens": ["So", "wird", "sich", "Schle\u00b7si\u00b7en", "an", "sei\u00b7ner", "lieb", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "NN", "APPR", "PPOSAT", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.96": {"text": "Und noch ein ehrenmahl dem frauenzimmer setzen.", "tokens": ["Und", "noch", "ein", "eh\u00b7ren\u00b7mahl", "dem", "frau\u00b7en\u00b7zim\u00b7mer", "set\u00b7zen", "."], "to