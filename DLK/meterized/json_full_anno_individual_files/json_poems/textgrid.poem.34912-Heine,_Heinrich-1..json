{"textgrid.poem.34912": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Goldne Menschen, Silbermenschen!", "tokens": ["Gold\u00b7ne", "Men\u00b7schen", ",", "Sil\u00b7ber\u00b7men\u00b7schen", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spricht ein Lump von einem Toman,", "tokens": ["Spricht", "ein", "Lump", "von", "ei\u00b7nem", "To\u00b7man", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist die Rede nur von Silber,", "tokens": ["Ist", "die", "Re\u00b7de", "nur", "von", "Sil\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist gemeint ein Silbertoman.", "tokens": ["Ist", "ge\u00b7meint", "ein", "Sil\u00b7ber\u00b7to\u00b7man", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch im Munde eines F\u00fcrsten,", "tokens": ["Doch", "im", "Mun\u00b7de", "ei\u00b7nes", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eines Schaches, ist ein Toman", "tokens": ["Ei\u00b7nes", "Scha\u00b7ches", ",", "ist", "ein", "To\u00b7man"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00fclden stets; ein Schach empf\u00e4ngt", "tokens": ["G\u00fcl\u00b7den", "stets", ";", "ein", "Schach", "emp\u00b7f\u00e4ngt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$.", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er gibt nur goldne Toman.", "tokens": ["Und", "er", "gibt", "nur", "gold\u00b7ne", "To\u00b7man", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Also denken brave Leute,", "tokens": ["Al\u00b7so", "den\u00b7ken", "bra\u00b7ve", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Also dachte auch Firdusi,", "tokens": ["Al\u00b7so", "dach\u00b7te", "auch", "Fir\u00b7du\u00b7si", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Der Verfasser des ber\u00fchmten", "tokens": ["Der", "Ver\u00b7fas\u00b7ser", "des", "be\u00b7r\u00fchm\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verg\u00f6tterten \u00bbSchach Nameh\u00ab.", "tokens": ["Und", "ver\u00b7g\u00f6t\u00b7ter\u00b7ten", "\u00bb", "Schach", "Na\u00b7meh", "\u00ab", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$(", "NE", "NN", "$(", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Dieses gro\u00dfe Heldenlied", "tokens": ["Die\u00b7ses", "gro\u00b7\u00dfe", "Hel\u00b7den\u00b7lied"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schrieb er auf Gehei\u00df des Schaches,", "tokens": ["Schrieb", "er", "auf", "Ge\u00b7hei\u00df", "des", "Scha\u00b7ches", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der f\u00fcr jeden seiner Verse", "tokens": ["Der", "f\u00fcr", "je\u00b7den", "sei\u00b7ner", "Ver\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Einen Toman ihm versprochen.", "tokens": ["Ei\u00b7nen", "To\u00b7man", "ihm", "ver\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Siebzehnmal die Rose bl\u00fchte,", "tokens": ["Sieb\u00b7zehn\u00b7mal", "die", "Ro\u00b7se", "bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Siebzehnmal ist sie verwelket,", "tokens": ["Sieb\u00b7zehn\u00b7mal", "ist", "sie", "ver\u00b7wel\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und die Nachtigall besang sie", "tokens": ["Und", "die", "Nach\u00b7ti\u00b7gall", "be\u00b7sang", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verstummte siebzehnmal \u2013", "tokens": ["Und", "ver\u00b7stumm\u00b7te", "sieb\u00b7zehn\u00b7mal", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Unterdessen sa\u00df der Dichter", "tokens": ["Un\u00b7ter\u00b7des\u00b7sen", "sa\u00df", "der", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An dem Webstuhl des Gedankens,", "tokens": ["An", "dem", "Web\u00b7stuhl", "des", "Ge\u00b7dan\u00b7kens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tag und Nacht, und webte emsig", "tokens": ["Tag", "und", "Nacht", ",", "und", "web\u00b7te", "em\u00b7sig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "KON", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seines Liedes Riesenteppich \u2013", "tokens": ["Sei\u00b7nes", "Lie\u00b7des", "Rie\u00b7sen\u00b7tep\u00b7pich", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Riesenteppich, wo der Dichter", "tokens": ["Rie\u00b7sen\u00b7tep\u00b7pich", ",", "wo", "der", "Dich\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wunderbar hineingewebt", "tokens": ["Wun\u00b7der\u00b7bar", "hin\u00b7ein\u00b7ge\u00b7webt"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seiner Heimat Fabelchronik,", "tokens": ["Sei\u00b7ner", "Hei\u00b7mat", "Fa\u00b7bel\u00b7chro\u00b7nik", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Farsistans uralte K\u00f6n'ge,", "tokens": ["Far\u00b7sis\u00b7tans", "ur\u00b7al\u00b7te", "K\u00f6n'\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Lieblingshelden seines Volkes,", "tokens": ["Lieb\u00b7lings\u00b7hel\u00b7den", "sei\u00b7nes", "Vol\u00b7kes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rittertaten, Avent\u00fcren,", "tokens": ["Rit\u00b7ter\u00b7ta\u00b7ten", ",", "A\u00b7ven\u00b7t\u00fc\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zauberwesen und D\u00e4monen,", "tokens": ["Zau\u00b7ber\u00b7we\u00b7sen", "und", "D\u00e4\u00b7mo\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keck umrankt von M\u00e4rchenblumen \u2013", "tokens": ["Keck", "um\u00b7rankt", "von", "M\u00e4r\u00b7chen\u00b7blu\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Alles bl\u00fchend und lebendig,", "tokens": ["Al\u00b7les", "bl\u00fc\u00b7hend", "und", "le\u00b7ben\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVPP", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Farbengl\u00e4nzend, gl\u00fchend, brennend,", "tokens": ["Far\u00b7ben\u00b7gl\u00e4n\u00b7zend", ",", "gl\u00fc\u00b7hend", ",", "bren\u00b7nend", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie himmlisch angestrahlt", "tokens": ["Und", "wie", "himm\u00b7lisch", "an\u00b7ge\u00b7strahlt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem heil'gen Lichte Irans,", "tokens": ["Von", "dem", "heil'\u00b7gen", "Lich\u00b7te", "I\u00b7rans", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Von dem g\u00f6ttlich reinen Urlicht,", "tokens": ["Von", "dem", "g\u00f6tt\u00b7lich", "rei\u00b7nen", "Ur\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dessen letzter Feuertempel,", "tokens": ["Des\u00b7sen", "letz\u00b7ter", "Feu\u00b7er\u00b7tem\u00b7pel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Trotz dem Koran und dem Mufti,", "tokens": ["Trotz", "dem", "Ko\u00b7ran", "und", "dem", "Muf\u00b7ti", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "In des Dichters Herzen flammte.", "tokens": ["In", "des", "Dich\u00b7ters", "Her\u00b7zen", "flamm\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Als vollendet war das Lied,", "tokens": ["Als", "voll\u00b7en\u00b7det", "war", "das", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcberschickte seinem G\u00f6nner", "tokens": ["\u00dc\u00b7bersc\u00b7hick\u00b7te", "sei\u00b7nem", "G\u00f6n\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Poet das Manuskript,", "tokens": ["Der", "Po\u00b7et", "das", "Ma\u00b7nus\u00b7kript", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zweimalhunderttausend Verse.", "tokens": ["Zwei\u00b7mal\u00b7hun\u00b7dert\u00b7tau\u00b7send", "Ver\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "In der Badestube war es,", "tokens": ["In", "der", "Ba\u00b7de\u00b7stu\u00b7be", "war", "es", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Badestub' zu Gasna,", "tokens": ["In", "der", "Ba\u00b7de\u00b7stub'", "zu", "Gas\u00b7na", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo des Schaches schwarze Boten", "tokens": ["Wo", "des", "Scha\u00b7ches", "schwar\u00b7ze", "Bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den Firdusi angetroffen \u2013", "tokens": ["Den", "Fir\u00b7du\u00b7si", "an\u00b7ge\u00b7trof\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.13": {"line.1": {"text": "Jeder schleppte einen Geldsack,", "tokens": ["Je\u00b7der", "schlepp\u00b7te", "ei\u00b7nen", "Geld\u00b7sack", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Den er zu des Dichters F\u00fc\u00dfen", "tokens": ["Den", "er", "zu", "des", "Dich\u00b7ters", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kniend legte, als den hohen", "tokens": ["Kniend", "leg\u00b7te", ",", "als", "den", "ho\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "KOUS", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ehrensold f\u00fcr seine Dichtung.", "tokens": ["Eh\u00b7ren\u00b7sold", "f\u00fcr", "sei\u00b7ne", "Dich\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Der Poet ri\u00df auf die S\u00e4cke", "tokens": ["Der", "Po\u00b7et", "ri\u00df", "auf", "die", "S\u00e4\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Hastig, um am lang entbehrten", "tokens": ["Has\u00b7tig", ",", "um", "am", "lang", "ent\u00b7behr\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUI", "PTKA", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Goldesanblick sich zu laben \u2013", "tokens": ["Gol\u00b7de\u00b7san\u00b7blick", "sich", "zu", "la\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da gewahrt' er mit Best\u00fcrzung,", "tokens": ["Da", "ge\u00b7wahrt'", "er", "mit", "Be\u00b7st\u00fcr\u00b7zung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Da\u00df der Inhalt dieser S\u00e4cke", "tokens": ["Da\u00df", "der", "In\u00b7halt", "die\u00b7ser", "S\u00e4\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleiches Silber, Silbertomans,", "tokens": ["Blei\u00b7ches", "Sil\u00b7ber", ",", "Sil\u00b7ber\u00b7to\u00b7mans", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zweimalhunderttausend etwa \u2013", "tokens": ["Zwei\u00b7mal\u00b7hun\u00b7dert\u00b7tau\u00b7send", "et\u00b7wa", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["CARD", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Dichter lachte bitter.", "tokens": ["Und", "der", "Dich\u00b7ter", "lach\u00b7te", "bit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Bitter lachend hat er jene", "tokens": ["Bit\u00b7ter", "la\u00b7chend", "hat", "er", "je\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "PDS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Summe abgeteilt in drei", "tokens": ["Sum\u00b7me", "ab\u00b7ge\u00b7teilt", "in", "drei"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPR", "CARD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleiche Teile, und jedwedem", "tokens": ["Glei\u00b7che", "Tei\u00b7le", ",", "und", "jed\u00b7we\u00b7dem"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von den beiden schwarzen Boten", "tokens": ["Von", "den", "bei\u00b7den", "schwar\u00b7zen", "Bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Schenkte er als Botenlohn", "tokens": ["Schenk\u00b7te", "er", "als", "Bo\u00b7ten\u00b7lohn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOUS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Solch ein Drittel, und das dritte", "tokens": ["Solch", "ein", "Drit\u00b7tel", ",", "und", "das", "drit\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "$,", "KON", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gab er einem Badeknechte,", "tokens": ["Gab", "er", "ei\u00b7nem", "Ba\u00b7de\u00b7knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der sein Bad besorgt, als Trinkgeld.", "tokens": ["Der", "sein", "Bad", "be\u00b7sorgt", ",", "als", "Trink\u00b7geld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NE", "VVPP", "$,", "KOUS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Seinen Wanderstab ergriff er", "tokens": ["Sei\u00b7nen", "Wan\u00b7der\u00b7stab", "er\u00b7griff", "er"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzo und verlie\u00df die Hauptstadt;", "tokens": ["Jet\u00b7zo", "und", "ver\u00b7lie\u00df", "die", "Haupt\u00b7stadt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "VVFIN", "ART", "NN", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Vor dem Tor hat er den Staub", "tokens": ["Vor", "dem", "Tor", "hat", "er", "den", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Abgefegt von seinen Schuhen.", "tokens": ["Ab\u00b7ge\u00b7fegt", "von", "sei\u00b7nen", "Schu\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Goldne Menschen, Silbermenschen!", "tokens": ["Gold\u00b7ne", "Men\u00b7schen", ",", "Sil\u00b7ber\u00b7men\u00b7schen", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spricht ein Lump von einem Toman,", "tokens": ["Spricht", "ein", "Lump", "von", "ei\u00b7nem", "To\u00b7man", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist die Rede nur von Silber,", "tokens": ["Ist", "die", "Re\u00b7de", "nur", "von", "Sil\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist gemeint ein Silbertoman.", "tokens": ["Ist", "ge\u00b7meint", "ein", "Sil\u00b7ber\u00b7to\u00b7man", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Doch im Munde eines F\u00fcrsten,", "tokens": ["Doch", "im", "Mun\u00b7de", "ei\u00b7nes", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eines Schaches, ist ein Toman", "tokens": ["Ei\u00b7nes", "Scha\u00b7ches", ",", "ist", "ein", "To\u00b7man"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00fclden stets; ein Schach empf\u00e4ngt", "tokens": ["G\u00fcl\u00b7den", "stets", ";", "ein", "Schach", "emp\u00b7f\u00e4ngt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$.", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er gibt nur goldne Toman.", "tokens": ["Und", "er", "gibt", "nur", "gold\u00b7ne", "To\u00b7man", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Also denken brave Leute,", "tokens": ["Al\u00b7so", "den\u00b7ken", "bra\u00b7ve", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Also dachte auch Firdusi,", "tokens": ["Al\u00b7so", "dach\u00b7te", "auch", "Fir\u00b7du\u00b7si", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Der Verfasser des ber\u00fchmten", "tokens": ["Der", "Ver\u00b7fas\u00b7ser", "des", "be\u00b7r\u00fchm\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verg\u00f6tterten \u00bbSchach Nameh\u00ab.", "tokens": ["Und", "ver\u00b7g\u00f6t\u00b7ter\u00b7ten", "\u00bb", "Schach", "Na\u00b7meh", "\u00ab", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$(", "NE", "NN", "$(", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.22": {"line.1": {"text": "Dieses gro\u00dfe Heldenlied", "tokens": ["Die\u00b7ses", "gro\u00b7\u00dfe", "Hel\u00b7den\u00b7lied"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schrieb er auf Gehei\u00df des Schaches,", "tokens": ["Schrieb", "er", "auf", "Ge\u00b7hei\u00df", "des", "Scha\u00b7ches", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der f\u00fcr jeden seiner Verse", "tokens": ["Der", "f\u00fcr", "je\u00b7den", "sei\u00b7ner", "Ver\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Einen Toman ihm versprochen.", "tokens": ["Ei\u00b7nen", "To\u00b7man", "ihm", "ver\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Siebzehnmal die Rose bl\u00fchte,", "tokens": ["Sieb\u00b7zehn\u00b7mal", "die", "Ro\u00b7se", "bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Siebzehnmal ist sie verwelket,", "tokens": ["Sieb\u00b7zehn\u00b7mal", "ist", "sie", "ver\u00b7wel\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und die Nachtigall besang sie", "tokens": ["Und", "die", "Nach\u00b7ti\u00b7gall", "be\u00b7sang", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verstummte siebzehnmal \u2013", "tokens": ["Und", "ver\u00b7stumm\u00b7te", "sieb\u00b7zehn\u00b7mal", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "Unterdessen sa\u00df der Dichter", "tokens": ["Un\u00b7ter\u00b7des\u00b7sen", "sa\u00df", "der", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An dem Webstuhl des Gedankens,", "tokens": ["An", "dem", "Web\u00b7stuhl", "des", "Ge\u00b7dan\u00b7kens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "mete