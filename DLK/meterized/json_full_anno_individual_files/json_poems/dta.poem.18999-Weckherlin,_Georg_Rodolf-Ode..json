{"dta.poem.18999": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ode.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Frisch auff/ jhr dapfere Soldaten/", "tokens": ["Frisch", "auff", "/", "jhr", "dap\u00b7fe\u00b7re", "Sol\u00b7da\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr/ die jhr noch mit Teutschem blut/", "tokens": ["Ihr", "/", "die", "jhr", "noch", "mit", "Teut\u00b7schem", "blut", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "PPER", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Jhr/ die jhr noch mit fr\u00fchem muht", "tokens": ["Ihr", "/", "die", "jhr", "noch", "mit", "fr\u00fc\u00b7hem", "muht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$(", "PRELS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Belebet/ suchet grosse thaten!", "tokens": ["Be\u00b7le\u00b7bet", "/", "su\u00b7chet", "gros\u00b7se", "tha\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jhr Landsleut/ jhr Landsknecht frisch auff/", "tokens": ["Ihr", "Lands\u00b7leut", "/", "jhr", "Lands\u00b7knecht", "frisch", "auff", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "ADJD", "APPR", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Das Land/ die Freyheit sich verlieret/", "tokens": ["Das", "Land", "/", "die", "Frey\u00b7heit", "sich", "ver\u00b7lie\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wa jhr nicht muhtig schlaget drauff/", "tokens": ["Wa", "jhr", "nicht", "muh\u00b7tig", "schla\u00b7get", "drauff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "VVFIN", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vnd vberwindend triumfieret.", "tokens": ["Vnd", "vber\u00b7win\u00b7dend", "tri\u00b7um\u00b7fie\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der ist ein Teutscher wolgeboren/", "tokens": ["Der", "ist", "ein", "Teut\u00b7scher", "wol\u00b7ge\u00b7bo\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der von betrug vnd falschheit frey/", "tokens": ["Der", "von", "be\u00b7trug", "vnd", "falschheit", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "VVFIN", "KON", "NN", "ADJD", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hat weder redlichkeit noch trew/", "tokens": ["Hat", "we\u00b7der", "red\u00b7lich\u00b7keit", "noch", "trew", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch glauben/ noch freyheit verlohren:", "tokens": ["Noch", "glau\u00b7ben", "/", "noch", "frey\u00b7heit", "ver\u00b7loh\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "ADV", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Der ist ein teutscher ehren wehrt/", "tokens": ["Der", "ist", "ein", "teut\u00b7scher", "eh\u00b7ren", "wehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der wacker/ hertzhafft/ vnverzaget/", "tokens": ["Der", "wa\u00b7cker", "/", "hertz\u00b7hafft", "/", "vn\u00b7ver\u00b7za\u00b7get", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "$(", "VVFIN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr die Freyheit mit seinem schwert", "tokens": ["F\u00fcr", "die", "Frey\u00b7heit", "mit", "sei\u00b7nem", "schwert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "In die gr\u00f6ste gefahr sich waget.", "tokens": ["In", "die", "gr\u00f6s\u00b7te", "ge\u00b7fahr", "sich", "wa\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Dan wan jhn schon die feind verwunden/", "tokens": ["Dan", "wan", "jhn", "schon", "die", "feind", "ver\u00b7wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd nemen jhm das leben hin/", "tokens": ["Vnd", "ne\u00b7men", "jhm", "das", "le\u00b7ben", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist doch ruhm vnd ehr sein gewin/", "tokens": ["Ist", "doch", "ruhm", "vnd", "ehr", "sein", "ge\u00b7win", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "KON", "NN", "PPOSAT", "NN", "$("], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Vnd Er ist gar nicht vberwunden:", "tokens": ["Vnd", "Er", "ist", "gar", "nicht", "vber\u00b7wun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ein solcher tod ist jhm nicht schwer/", "tokens": ["Ein", "sol\u00b7cher", "tod", "ist", "jhm", "nicht", "schwer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil sein gewissen jhn vers\u00fcsset;", "tokens": ["Weil", "sein", "ge\u00b7wis\u00b7sen", "jhn", "ver\u00b7s\u00fcs\u00b7set", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vnd Er erwirbet lob vnd ehr/", "tokens": ["Vnd", "Er", "er\u00b7wir\u00b7bet", "lob", "vnd", "ehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In dem er sein blut