rland verlasset;", "tokens": ["Die", "er", "dem", "Vat\u00b7ter\u00b7land", "ver\u00b7las\u00b7set", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da der hertzlo\u00df durch seine flucht", "tokens": ["Da", "der", "hertz\u00b7lo\u00df", "durch", "sei\u00b7ne", "flucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wirt gantz verachtet vnd gehasset.", "tokens": ["Wirt", "gantz", "ver\u00b7ach\u00b7tet", "vnd", "ge\u00b7has\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Also zu leben vnd zu sterben", "tokens": ["Al\u00b7so", "zu", "le\u00b7ben", "vnd", "zu", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Gilt dem redlichen Teutschen gleich:", "tokens": ["Gilt", "dem", "red\u00b7li\u00b7chen", "Teut\u00b7schen", "gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Der Tod vnd Sig seind sch\u00f6n vnd reich/", "tokens": ["Der", "Tod", "vnd", "Sig", "seind", "sch\u00f6n", "vnd", "reich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NE", "VAFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch beed kan er sein hayl erwerben:", "tokens": ["Durch", "beed", "kan", "er", "sein", "hayl", "er\u00b7wer\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hingegen fliehen allen danck", "tokens": ["Hin\u00b7ge\u00b7gen", "flie\u00b7hen", "al\u00b7len", "danck"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Die fl\u00fcchtige vnd der verr\u00e4hter/", "tokens": ["Die", "fl\u00fcch\u00b7ti\u00b7ge", "vnd", "der", "ver\u00b7r\u00e4h\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Vnd jhnen folget ein gestanck/", "tokens": ["Vnd", "jh\u00b7nen", "fol\u00b7get", "ein", "ge\u00b7stanck", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Weil sie verfluchte vbelth\u00e4ter.", "tokens": ["Weil", "sie", "ver\u00b7fluch\u00b7te", "vbelt\u00b7h\u00e4\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Wolan derhalb/ jhr wehrte Teutschen/", "tokens": ["Wo\u00b7lan", "der\u00b7halb", "/", "jhr", "wehr\u00b7te", "Teut\u00b7schen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Teutscher faust/ mit k\u00fchnem muht/", "tokens": ["Mit", "Teut\u00b7scher", "faust", "/", "mit", "k\u00fch\u00b7nem", "muht", "/"], "token_info": ["word", "word"