{"dta.poem.20955": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XiX.  \n Columbens.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Der Wind trieb meinen Leib/ die Ehre meine Sin-", "tokens": ["Der", "Wind", "trieb", "mei\u00b7nen", "Leib", "/", "die", "Eh\u00b7re", "mei\u00b7ne", "Sin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$(", "ART", "NN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "nen/", "tokens": ["nen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Des H\u00f6chsten starcke Hand begleite mein Begin\u0303en.", "tokens": ["Des", "H\u00f6chs\u00b7ten", "star\u00b7cke", "Hand", "be\u00b7glei\u00b7te", "mein", "Be\u00b7gi\u00f1en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich fand die neue Welt/ und trug nicht viel davon/", "tokens": ["Ich", "fand", "die", "neu\u00b7e", "Welt", "/", "und", "trug", "nicht", "viel", "da\u00b7von", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$(", "KON", "VVFIN", "PTKNEG", "ADV", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vor alle meine M\u00fch ist dieses Grab mein Lohn.", "tokens": ["Vor", "al\u00b7le", "mei\u00b7ne", "