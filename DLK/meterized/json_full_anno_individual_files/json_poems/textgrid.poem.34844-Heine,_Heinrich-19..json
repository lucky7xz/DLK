{"textgrid.poem.34844": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl dem, dem noch die Tugend lacht,", "tokens": ["Wohl", "dem", ",", "dem", "noch", "die", "Tu\u00b7gend", "lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weh dem, der sie verlieret!", "tokens": ["Weh", "dem", ",", "der", "sie", "ver\u00b7lie\u00b7ret", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es haben mich armen J\u00fcngling", "tokens": ["Es", "ha\u00b7ben", "mich", "ar\u00b7men", "J\u00fcng\u00b7ling"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die b\u00f6sen Gesellen verf\u00fchret.", "tokens": ["Die", "b\u00f6\u00b7sen", "Ge\u00b7sel\u00b7len", "ver\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Sie haben mich um mein Geld gebracht,", "tokens": ["Sie", "ha\u00b7ben", "mich", "um", "mein", "Geld", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit Karten und mit Kn\u00f6cheln;", "tokens": ["Mit", "Kar\u0