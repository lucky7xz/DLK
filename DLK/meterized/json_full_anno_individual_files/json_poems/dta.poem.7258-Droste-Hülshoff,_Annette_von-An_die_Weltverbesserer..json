{"dta.poem.7258": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "An die Weltverbesserer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Pochest du an \u2014 poch' nicht zu laut,", "tokens": ["Po\u00b7chest", "du", "an", "poch'", "nicht", "zu", "laut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "$(", "VVIMP", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Eh du gepr\u00fcft des Nachhalls Dauer.", "tokens": ["Eh", "du", "ge\u00b7pr\u00fcft", "des", "Nach\u00b7halls", "Dau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dr\u00fcckst du die Hand \u2014 dr\u00fcck nicht zu traut,", "tokens": ["Dr\u00fcckst", "du", "die", "Hand", "dr\u00fcck", "nicht", "zu", "traut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$(", "ADV", "PTKNEG", "PTKZU", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Eh du gefragt des Herzens Schauer.", "tokens": ["Eh", "du", "ge\u00b7fragt", "des", "Her\u00b7zens", "Schau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wirfst du den Stein \u2014 bedenke wohl,", "tokens": ["Wirfst", "du", "den", "Stein", "be\u00b7den\u00b7ke", "wohl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$(", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie weit ihn deine Hand wird treiben.", "tokens": ["Wie", "weit", "ihn", "dei\u00b7ne", "Hand", "wird", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Oft schreckt ein Echo, dumpf und hohl,", "tokens": ["Oft", "schreckt", "ein", "E\u00b7cho", ",", "dumpf", "und", "hohl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Reicht goldne Hand dir den Obol,", "tokens": ["Reicht", "gold\u00b7ne", "Hand", "dir", "den", "O\u00b7bol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Oft trifft ein Wurf des Nachbars Scheiben.", "tokens": ["Oft", "trifft", "ein", "Wurf", "des", "Nach\u00b7bars", "Schei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00f6hlen giebt es am Meeresstrand,", "tokens": ["H\u00f6h\u00b7len", "giebt", "es", "am", "Mee\u00b7res\u00b7strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Gewalt'ge Stalaktitendome,", "tokens": ["Ge\u00b7walt'\u00b7ge", "Sta\u00b7lak\u00b7ti\u00b7ten\u00b7do\u00b7me", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo bl\u00e4ulich zuckt der Fackeln Brand,", "tokens": ["Wo", "bl\u00e4u\u00b7lich", "zuckt", "der", "Fa\u00b7ckeln", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und K\u00e4hne gleiten wie Phantome.", "tokens": ["Und", "K\u00e4h\u00b7ne", "glei\u00b7ten", "wie", "Phan\u00b7to\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das Ruder schl\u00e4ft, der Schiffer legt", "tokens": ["Das", "Ru\u00b7der", "schl\u00e4ft", ",", "der", "Schif\u00b7fer", "legt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Hand dir angstvoll auf die Lippe,", "tokens": ["Die", "Hand", "dir", "angst\u00b7voll", "auf", "die", "Lip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein R\u00e4uspern nur, ein Fu\u00df geregt,", "tokens": ["Ein", "R\u00e4us\u00b7pern", "nur", ",", "ein", "Fu\u00df", "ge\u00b7regt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und donnernd \u00fcberm Haupte schl\u00e4gt", "tokens": ["Und", "don\u00b7nernd", "\u00fc\u00b7berm", "Haup\u00b7te", "schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zusammen dir die Riesenklippe.", "tokens": ["Zu\u00b7sam\u00b7men", "dir", "die", "Rie\u00b7sen\u00b7klip\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und H\u00e4nde giebts im Orient,", "tokens": ["Und", "H\u00e4n\u00b7de", "giebts", "im", "O\u00b7rient", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Schw\u00e4ne wei\u00df, mit blauen Malen,", "tokens": ["Wie", "Schw\u00e4\u00b7ne", "wei\u00df", ",", "mit", "blau\u00b7en", "Ma\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In denen zwiefach Feuer brennt,", "tokens": ["In", "de\u00b7nen", "zwie\u00b7fach", "Feu\u00b7er", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als gelt' es Liebesglut zu zahlen;", "tokens": ["Als", "gelt'", "es", "Lie\u00b7bes\u00b7glut", "zu", "zah\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein leichter Thau hat sie gen\u00e4\u00dft,", "tokens": ["Ein", "leich\u00b7ter", "Thau", "hat", "sie", "ge\u00b7n\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein leises Zittern sie umflogen,", "tokens": ["Ein", "lei\u00b7ses", "Zit\u00b7tern", "sie", "um\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie fassen krampfhaft, dr\u00fccken fest \u2014", "tokens": ["Sie", "fas\u00b7sen", "krampf\u00b7haft", ",", "dr\u00fc\u00b7cken", "fest"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hinweg, hinweg! du hast die Pest", "tokens": ["Hin\u00b7weg", ",", "hin\u00b7weg", "!", "du", "hast", "die", "Pest"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In deine Poren eingesogen!", "tokens": ["In", "dei\u00b7ne", "Po\u00b7ren", "ein\u00b7ge\u00b7so\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Auch hat ein D\u00e4mon einst gesandt", "tokens": ["Auch", "hat", "ein", "D\u00e4\u00b7mon", "einst", "ge\u00b7sandt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den gift'gen Pfeil zum Himmelsbogen;", "tokens": ["Den", "gift'\u00b7gen", "Pfeil", "zum", "Him\u00b7mels\u00b7bo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dort r\u00fchrt ihn eines Gottes Hand,", "tokens": ["Dort", "r\u00fchrt", "ihn", "ei\u00b7nes", "Got\u00b7tes", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun starrt er in den Aetherwogen.", "tokens": ["Nun", "starrt", "er", "in", "den", "A\u00b7e\u00b7ther\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und l\u00e4\u00dft der Zauber nach, dann wird", "tokens": ["Und", "l\u00e4\u00dft", "der", "Zau\u00b7ber", "nach", ",", "dann", "wird"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er niederprallen mit Geschmetter,", "tokens": ["Er", "nie\u00b7der\u00b7pral\u00b7len", "mit", "Ge\u00b7schmet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df das Gebirg' in Scherben klirrt,", "tokens": ["Da\u00df", "das", "Ge\u00b7bir\u00b7g'", "in", "Scher\u00b7ben", "klirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Und durch der Erde Adern irrt", "tokens": ["Und", "durch", "der", "Er\u00b7de", "A\u00b7dern", "irrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Fortan das Gift der H\u00f6lleng\u00f6tter.", "tokens": ["For\u00b7tan", "das", "Gift", "der", "H\u00f6l\u00b7len\u00b7g\u00f6t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Drum poche sacht, du wei\u00dft es nicht", "tokens": ["Drum", "po\u00b7che", "sacht", ",", "du", "wei\u00dft", "es", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was dir mag \u00fcberm Haupte schwanken;", "tokens": ["Was", "dir", "mag", "\u00fc\u00b7berm", "Haup\u00b7te", "schwan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Drum dr\u00fccke sacht, der Augen Licht", "tokens": ["Drum", "dr\u00fc\u00b7cke", "sacht", ",", "der", "Au\u00b7gen", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADJD", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl siehst du, doch nicht der Gedanken,", "tokens": ["Wohl", "siehst", "du", ",", "doch", "nicht", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Wirf nicht den Stein zu jener H\u00f6h'", "tokens": ["Wirf", "nicht", "den", "Stein", "zu", "je\u00b7ner", "H\u00f6h'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo dir gestaltlos Form und Wege,", "tokens": ["Wo", "dir", "ge\u00b7stalt\u00b7los", "Form", "und", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und schnelltest du ihn einmal je,", "tokens": ["Und", "schnell\u00b7test", "du", "ihn", "ein\u00b7mal", "je", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So fall auf deine Knie und fleh',", "tokens": ["So", "fall", "auf", "dei\u00b7ne", "Knie", "und", "fleh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df ihn ein Gott ber\u00fchren m\u00f6ge.", "tokens": ["Da\u00df", "ihn", "ein", "Gott", "be\u00b7r\u00fch\u00b7ren", "m\u00f6\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}