{"dta.poem.8250": {"metadata": {"author": {"name": "Beer, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Der  S chau- P latz ist eine  L andschafft bey auf-  \n gehender  S onnen.  \n  V orrede.", "genre": "Lyrik, Libretto", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-25222-5", "language": ["de:0.99"], "booktitle": "Beer, Johann: Antonius R\u00f6mischer Triumvir. Wei\u00dfenfels, 1697."}, "poem": {"stanza.1": {"line.1": {"text": "Das Gl\u00fcck kan st\u00fcrtzen und erh\u00f6hen/", "tokens": ["Das", "Gl\u00fcck", "kan", "st\u00fcrt\u00b7zen", "und", "er\u00b7h\u00f6\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nachdem sein Rad den Umlauf h\u00e4lt/", "tokens": ["Nach\u00b7dem", "sein", "Rad", "den", "Um\u00b7lauf", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sich desselben Speichen drehen/", "tokens": ["Und", "sich", "des\u00b7sel\u00b7ben", "Spei\u00b7chen", "dre\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df einer steigt/ der andre f\u00e4llt.", "tokens": ["Da\u00df", "ei\u00b7ner", "steigt", "/", "der", "and\u00b7re", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$(", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}