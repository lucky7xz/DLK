ht.", "tokens": ["Als", "w\u00e4\u00b7re", "ich", "selbst", "ein", "Wicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Nicht will ich dein Wesen verg\u00f6ttern", "tokens": ["Nicht", "will", "ich", "dein", "We\u00b7sen", "ver\u00b7g\u00f6t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Mit br\u00fcnstiger Philosophie,", "tokens": ["Mit", "br\u00fcns\u00b7ti\u00b7ger", "Phi\u00b7lo\u00b7so\u00b7phie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will nicht, wie sonst meine Vettern,", "tokens": ["Will", "nicht", ",", "wie", "sonst", "mei\u00b7ne", "Vet\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "$,", "PWAV", "ADV", "PPOSAT", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Dich halten f\u00fcr ein Genie.", "tokens": ["Dich", "hal\u00b7ten", "f\u00fcr", "ein", "Ge\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich will meine Seufzer behalten", "tokens": ["Ich", "will", "mei\u00b7ne", "Seuf\u00b7zer", "be\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "So viel auch als m\u00f6glich f\u00fcr mich,", "tokens": ["So", "viel", "auch", "als", "m\u00f6g\u00b7lich", "f\u00fcr", "m