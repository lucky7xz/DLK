{"dta.poem.142": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er verlanget zu sterben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Weich Leben! weich geschwind/ der Tod ist vor der Th\u00fcr/", "tokens": ["Weich", "Le\u00b7ben", "!", "weich", "ge\u00b7schwind", "/", "der", "Tod", "ist", "vor", "der", "Th\u00fcr", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "ADJD", "VVPP", "$(", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Komm doch! komm ", "tokens": ["Komm", "doch", "!", "komm"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "$.", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Todten-Feld/", "tokens": ["Tod\u00b7ten\u00b7Feld", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Wie leb\u2019 ich! bin ich todt! bin ich in jener Welt?", "tokens": ["Wie", "leb'", "ich", "!", "bin", "ich", "todt", "!", "bin", "ich", "in", "je\u00b7ner", "Welt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "VAFIN", "PPER", "ADJD", "$.", "VAFIN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Strahlt ", "tokens": ["Strahlt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Nein! ach nun ists geschehn/ mein letzter Tag ist hier.", "tokens": ["Nein", "!", "ach", "nun", "ists", "ge\u00b7schehn", "/", "mein", "letz\u00b7ter", "Tag", "ist", "hier", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "ADV", "VAFIN", "VVPP", "$(", "PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jhr Unbarmhertzig-seyn bringt mich an ", "tokens": ["Ihr", "Un\u00b7barm\u00b7hert\u00b7zig\u00b7seyn", "bringt", "mich", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Mein Oden h\u00f6ret auf/ mein Urtheil ist gestelt/", "tokens": ["Mein", "O\u00b7den", "h\u00f6\u00b7ret", "auf", "/", "mein", "Ur\u00b7theil", "ist", "ge\u00b7stelt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich soll und mu\u00df vergehn. Ach kehre doch zu mir.", "tokens": ["Ich", "soll", "und", "mu\u00df", "ver\u00b7gehn", ".", "Ach", "keh\u00b7re", "doch", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "VVINF", "$.", "ITJ", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Beliebte ", "tokens": ["Be\u00b7lieb\u00b7te"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Ich habe dich gek\u00fc\u00dft/ ist das des Todes werth?", "tokens": ["Ich", "ha\u00b7be", "dich", "ge\u00b7k\u00fc\u00dft", "/", "ist", "das", "des", "To\u00b7des", "werth", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$(", "VAFIN", "ART", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie/ h\u00f6rt dein Ohr denn nicht? ach Nein! nun mu\u00df ich sterben/", "tokens": ["Wie", "/", "h\u00f6rt", "dein", "Ohr", "denn", "nicht", "?", "ach", "Nein", "!", "nun", "mu\u00df", "ich", "ster\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "VVFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "$.", "XY", "PTKANT", "$.", "ADV", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jhr/ die ihr sie noch liebt/ h\u00f6rt was ich sage/ h\u00f6rt.", "tokens": ["Ihr", "/", "die", "ihr", "sie", "noch", "liebt", "/", "h\u00f6rt", "was", "ich", "sa\u00b7ge", "/", "h\u00f6rt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "PPER", "PPER", "ADV", "VVFIN", "$(", "VVFIN", "PWS", "PPER", "VVFIN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "H\u00f6rt! wenn ihr leben wolt/ so la\u00dft sie ungek\u00fcsset/", "tokens": ["H\u00f6rt", "!", "wenn", "ihr", "le\u00b7ben", "wolt", "/", "so", "la\u00dft", "sie", "un\u00b7ge\u00b7k\u00fcs\u00b7set", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "KOUS", "PPER", "VVINF", "VMFIN", "$(", "ADV", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mein Vorwitz wird anjetzt durch meinen Tod geb\u00fcsset.", "tokens": ["Mein", "Vor\u00b7witz", "wird", "an\u00b7jetzt", "durch", "mei\u00b7nen", "Tod", "ge\u00b7b\u00fcs\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}