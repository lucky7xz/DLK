{"textgrid.poem.31724": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An K\u00f6ln", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O K\u00f6ln, du gro\u00dfe Freudenstadt,", "tokens": ["O", "K\u00f6ln", ",", "du", "gro\u00b7\u00dfe", "Freu\u00b7den\u00b7stadt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was sag ich noch zu deinem Ruhme?", "tokens": ["Was", "sag", "ich", "noch", "zu", "dei\u00b7nem", "Ruh\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie du gebl\u00fcht im grauen Altertume,", "tokens": ["Wie", "du", "ge\u00b7bl\u00fcht", "im", "grau\u00b7en", "Al\u00b7ter\u00b7tu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So bl\u00fchst du noch \u2013 die sch\u00f6nste Blume,", "tokens": ["So", "bl\u00fchst", "du", "noch", "\u2013", "die", "sch\u00f6ns\u00b7te", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die je gebl\u00fchet hat!", "tokens": ["Die", "je", "ge\u00b7bl\u00fc\u00b7het", "hat", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dich preis ich, K\u00f6nigin, allein!", "tokens": ["Dich", "preis", "ich", ",", "K\u00f6\u00b7ni\u00b7gin", ",", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der hohe Dom ist deine Krone!", "tokens": ["Der", "ho\u00b7he", "Dom", "ist", "dei\u00b7ne", "Kro\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ha! wie es rauscht an deinem Uferthrone!", "tokens": ["Ha", "!", "wie", "es", "rauscht", "an", "dei\u00b7nem", "Uf\u00b7ert\u00b7hro\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die V\u00f6lker bringt dir, jeder Zone,", "tokens": ["Die", "V\u00f6l\u00b7ker", "bringt", "dir", ",", "je\u00b7der", "Zo\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der rebengr\u00fcne Rhein.", "tokens": ["Der", "re\u00b7ben\u00b7gr\u00fc\u00b7ne", "Rhein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Frohlockend gr\u00fc\u00dft dich ihr Gesang;", "tokens": ["Froh\u00b7lo\u00b7ckend", "gr\u00fc\u00dft", "dich", "ihr", "Ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und rascher schl\u00e4gt den Schaum der Wellen", "tokens": ["Und", "ra\u00b7scher", "schl\u00e4gt", "den", "Schaum", "der", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Schiffer, wenn in T\u00f6nen, wunderhellen,", "tokens": ["Der", "Schif\u00b7fer", ",", "wenn", "in", "T\u00f6\u00b7nen", ",", "wun\u00b7der\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herab von Kirchen und Kapellen", "tokens": ["Her\u00b7ab", "von", "Kir\u00b7chen", "und", "Ka\u00b7pel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erklingt der Glocken Klang;", "tokens": ["Er\u00b7klingt", "der", "Glo\u00b7cken", "Klang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn in der Abendsonne Strahl", "tokens": ["Wenn", "in", "der", "A\u00b7bend\u00b7son\u00b7ne", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die buntbemalten Fenster spr\u00fchen,", "tokens": ["Die", "bunt\u00b7be\u00b7mal\u00b7ten", "Fens\u00b7ter", "spr\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn rings die alten Gotenb\u00f6gen gl\u00fchen", "tokens": ["Wenn", "rings", "die", "al\u00b7ten", "Go\u00b7ten\u00b7b\u00f6\u00b7gen", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Laubgewinde, die erbl\u00fchen", "tokens": ["Wie", "Laub\u00b7ge\u00b7win\u00b7de", ",", "die", "er\u00b7bl\u00fc\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "NN", "$,", "PRELS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Rosen ohne Zahl.", "tokens": ["Mit", "Ro\u00b7sen", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Still schreit ich durch das graue Tor,", "tokens": ["Still", "schreit", "ich", "durch", "das", "grau\u00b7e", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dran hoch hinauf die Linden ragen;", "tokens": ["Dran", "hoch", "hin\u00b7auf", "die", "Lin\u00b7den", "ra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "ADV", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und pr\u00e4chtig steigt der Glanz aus fernen Tagen,", "tokens": ["Und", "pr\u00e4ch\u00b7tig", "steigt", "der", "Glanz", "aus", "fer\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der ganze Zauber deiner Sagen", "tokens": ["Der", "gan\u00b7ze", "Zau\u00b7ber", "dei\u00b7ner", "Sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vor meinem Geist empor!", "tokens": ["Vor", "mei\u00b7nem", "Geist", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Hier ist's, wo Agrippinens Haar", "tokens": ["Hier", "ist's", ",", "wo", "Ag\u00b7rip\u00b7pi\u00b7nens", "Haar"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich lockig um die Schl\u00e4fen dr\u00fcckte,", "tokens": ["Sich", "lo\u00b7ckig", "um", "die", "Schl\u00e4\u00b7fen", "dr\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Karl Martell vom Kapitole blickte", "tokens": ["Wo", "Karl", "Mar\u00b7tell", "vom", "Ka\u00b7pi\u00b7to\u00b7le", "blick\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "NE", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wo das Schwert, das blut'ge, z\u00fcckte", "tokens": ["Und", "wo", "das", "Schwert", ",", "das", "blut'\u00b7ge", ",", "z\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch der Normannen Schar!", "tokens": ["Durch", "der", "Nor\u00b7man\u00b7nen", "Schar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Hier rief zu deiner B\u00fcrger Krieg", "tokens": ["Hier", "rief", "zu", "dei\u00b7ner", "B\u00fcr\u00b7ger", "Krieg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Horn in schauerlichen Kl\u00e4ngen;", "tokens": ["Das", "Horn", "in", "schau\u00b7er\u00b7li\u00b7chen", "Kl\u00e4n\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hier sah man Panzer gegen Panzer dr\u00e4ngen", "tokens": ["Hier", "sah", "man", "Pan\u00b7zer", "ge\u00b7gen", "Pan\u00b7zer", "dr\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und deinen Overstolzen sprengen", "tokens": ["Und", "dei\u00b7nen", "O\u00b7ver\u00b7stol\u00b7zen", "spren\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Tode und zum Sieg!", "tokens": ["Zum", "To\u00b7de", "und", "zum", "Sieg", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Hier schuf der Maler r\u00fcst'ge Hand", "tokens": ["Hier", "schuf", "der", "Ma\u00b7ler", "r\u00fcst'\u00b7ge", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Heer von schimmernden Gestalten;", "tokens": ["Ein", "Heer", "von", "schim\u00b7mern\u00b7den", "Ge\u00b7stal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dort sah man um Mitternacht den alten", "tokens": ["Und", "dort", "sah", "man", "um", "Mit\u00b7ter\u00b7nacht", "den", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS", "APPR", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Albertus Magnus Wache halten", "tokens": ["Al\u00b7ber\u00b7tus", "Mag\u00b7nus", "Wa\u00b7che", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "NN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ob staub'gem Foliant!", "tokens": ["Ob", "staub'\u00b7gem", "Fo\u00b7li\u00b7ant", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das war vordem! Auf ihr Gebein", "tokens": ["Das", "war", "vor\u00b7dem", "!", "Auf", "ihr", "Ge\u00b7bein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Ist l\u00e4ngst der Grabesstein gesunken.", "tokens": ["Ist", "l\u00e4ngst", "der", "Gra\u00b7bess\u00b7tein", "ge\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Banner weht daran; und freudetrunken", "tokens": ["Dein", "Ban\u00b7ner", "weht", "da\u00b7ran", ";", "und", "freu\u00b7de\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PAV", "$.", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sah ich ergl\u00fchn eilf goldne Funken", "tokens": ["Sah", "ich", "er\u00b7gl\u00fchn", "eilf", "gold\u00b7ne", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und dreier Kronen Schein!", "tokens": ["Und", "drei\u00b7er", "Kro\u00b7nen", "Schein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "So hat es einst auf langer Fahrt", "tokens": ["So", "hat", "es", "einst", "auf", "lan\u00b7ger", "Fahrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gewallt von deiner Hansa Masten,", "tokens": ["Ge\u00b7wallt", "von", "dei\u00b7ner", "Han\u00b7sa", "Mas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn St\u00fcrme wild die wei\u00dfen Segel fa\u00dften", "tokens": ["Wenn", "St\u00fcr\u00b7me", "wild", "die", "wei\u00b7\u00dfen", "Se\u00b7gel", "fa\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADJD", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und drauf in Golfen kam zu rasten", "tokens": ["Und", "drauf", "in", "Gol\u00b7fen", "kam", "zu", "ras\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "APPR", "NN", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Flotte, bunt geschart.", "tokens": ["Die", "Flot\u00b7te", ",", "bunt", "ge\u00b7schart", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Es sah die Welt zu ew'gem Ruhm", "tokens": ["Es", "sah", "die", "Welt", "zu", "ew'\u00b7gem", "Ruhm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stets deine B\u00fcrger es geleiten;", "tokens": ["Stets", "dei\u00b7ne", "B\u00fcr\u00b7ger", "es", "ge\u00b7lei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Drum, wie die Jahre wild verheerend schreiten,", "tokens": ["Drum", ",", "wie", "die", "Jah\u00b7re", "wild", "ver\u00b7hee\u00b7rend", "schrei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "ART", "NN", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du stehest da, zu allen Zeiten", "tokens": ["Du", "ste\u00b7hest", "da", ",", "zu", "al\u00b7len", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein sch\u00f6nes Heiligtum:", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Hei\u00b7lig\u00b7tum", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Wo Freiheit noch die Herz