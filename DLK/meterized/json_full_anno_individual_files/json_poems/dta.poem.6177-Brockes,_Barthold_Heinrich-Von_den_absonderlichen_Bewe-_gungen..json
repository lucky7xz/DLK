{"dta.poem.6177": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den absonderlichen Bewe-  \n gungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn auch ein C\u00f6rper sich verdickt; so fliehn", "tokens": ["Wenn", "auch", "ein", "C\u00f6r\u00b7per", "sich", "ver\u00b7dickt", ";", "so", "fliehn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PRF", "VVPP", "$.", "ADV", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die kleinen fremden C\u00f6rperlein", "tokens": ["Die", "klei\u00b7nen", "frem\u00b7den", "C\u00f6r\u00b7per\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In D\u00fcnsten in die Lufft; es ziehn", "tokens": ["In", "D\u00fcns\u00b7ten", "in", "die", "Lufft", ";", "es", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$.", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dadurch, da\u00df sie nun nicht mehr gegenw\u00e4rtig seyn,", "tokens": ["Da\u00b7durch", ",", "da\u00df", "sie", "nun", "nicht", "mehr", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Oeffnungen, die blos von ihnen stammen,", "tokens": ["Die", "Oeff\u00b7nun\u00b7gen", ",", "die", "blos", "von", "ih\u00b7nen", "stam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sich wieder, wie vorhin, zusammen.", "tokens": ["Sich", "wie\u00b7der", ",", "wie", "vor\u00b7hin", ",", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ADV", "$,", "PWAV", "ADV", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}