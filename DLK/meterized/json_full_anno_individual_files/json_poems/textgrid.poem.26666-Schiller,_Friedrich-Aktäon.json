{"textgrid.poem.26666": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Akt\u00e4on", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wart! deine Frau soll dich betr\u00fcgen,", "tokens": ["Wart", "!", "dei\u00b7ne", "Frau", "soll", "dich", "be\u00b7tr\u00fc\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein andrer soll in ihren Armen liegen,", "tokens": ["Ein", "an\u00b7drer", "soll", "in", "ih\u00b7ren", "Ar\u00b7men", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und H\u00f6rner dir hervor zum Kopfe bl\u00fchn!", "tokens": ["Und", "H\u00f6r\u00b7ner", "dir", "her\u00b7vor", "zum", "Kop\u00b7fe", "bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "PTKVZ", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Entsetzlich! mich im Bad zu \u00fcberraschen", "tokens": ["Ent\u00b7setz\u00b7lich", "!", "mich", "im", "Bad", "zu", "\u00fc\u00b7berr\u00b7a\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$.", "PPER", "APPRART", "NE", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "(die Schande kann kein \u00c4therbad verwaschen)", "tokens": ["(", "die", "Schan\u00b7de", "kann", "kein", "\u00c4\u00b7ther\u00b7bad", "ver\u00b7wa\u00b7schen", ")"], "token_info": ["punct", "word", "word", "word",