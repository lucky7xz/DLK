{"textgrid.poem.53791": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Monolog mit Ch\u00f6ren", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin so menschenm\u00fcde und wie ohne Haut.", "tokens": ["Ich", "bin", "so", "men\u00b7schen\u00b7m\u00fc\u00b7de", "und", "wie", "oh\u00b7ne", "Haut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "PWAV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die andern mag ich nicht \u2013 sie tun mir wehe.", "tokens": ["Die", "an\u00b7dern", "mag", "ich", "nicht", "\u2013", "sie", "tun", "mir", "we\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "PPER", "PTKNEG", "$(", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ich nur fremde Menschen sehe,", "tokens": ["Wenn", "ich", "nur", "frem\u00b7de", "Men\u00b7schen", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "lauf ich davon \u2013 wie sind sie derb und laut!", "tokens": ["lauf", "ich", "da\u00b7von", "\u2013", "wie", "sind", "sie", "derb", "und", "laut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PAV", "$(", "KOKOM", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich bin so m\u00fcde und wie ohne Haut!", "tokens": ["Ich", "bin", "so", "m\u00fc\u00b7de", "und", "wie", "oh\u00b7ne", "Haut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "PWAV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "(chor der Arbeitslosen): Das ist ja hervorragend interessant, Herr Tiger!", "tokens": ["(", "chor", "der", "Ar\u00b7beits\u00b7lo\u00b7sen", ")", ":", "Das", "ist", "ja", "her\u00b7vor\u00b7ra\u00b7gend", "in\u00b7ter\u00b7es\u00b7sant", ",", "Herr", "Ti\u00b7ger", "!"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "$(", "$.", "PDS", "VAFIN", "ADV", "ADJD", "ADJD", "$,", "NN", "NN", "$."], "meter": "--+-+--+--+--+--+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich spinn mich selig in die Sch\u00f6nheit ein.", "tokens": ["Ich", "spinn", "mich", "se\u00b7lig", "in", "die", "Sch\u00f6n\u00b7heit", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sch\u00f6nheit ist Einsamkeit. Ein stiller Morgen", "tokens": ["Sch\u00f6n\u00b7heit", "ist", "Ein\u00b7sam\u00b7keit", ".", "Ein", "stil\u00b7ler", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "im feuchten Park, allein und ohne Sorgen,", "tokens": ["im", "feuch\u00b7ten", "Park", ",", "al\u00b7lein", "und", "oh\u00b7ne", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADV", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "durchs Blattgr\u00fcn schimmert eine Mauer, grau im Stein.", "tokens": ["durchs", "Blatt\u00b7gr\u00fcn", "schim\u00b7mert", "ei\u00b7ne", "Mau\u00b7er", ",", "grau", "im", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich spinn mich selig in die Sch\u00f6nheit ein . . .", "tokens": ["Ich", "spinn", "mich", "se\u00b7lig", "in", "die", "Sch\u00f6n\u00b7heit", "ein", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "(chor der Proletarierm\u00fctter): Wir w\u00fc\u00dften nicht, was uns mehr zu Herzen ginge, Herr Tiger!", "tokens": ["(", "chor", "der", "Pro\u00b7le\u00b7ta\u00b7ri\u00b7er\u00b7m\u00fct\u00b7ter", ")", ":", "Wir", "w\u00fc\u00df\u00b7ten", "nicht", ",", "was", "uns", "mehr", "zu", "Her\u00b7zen", "gin\u00b7ge", ",", "Herr", "Ti\u00b7ger", "!"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "$(", "$.", "PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,", "NN", "NN", "$."], "meter": "+-+-+--+--+----+-+-+--+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich dichte leis und sachte vor mich hin.", "tokens": ["Ich", "dich\u00b7te", "leis", "und", "sach\u00b7te", "vor", "mich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie fein analysier ich Seelenf\u00e4den,", "tokens": ["Wie", "fein", "a\u00b7nal\u00b7ysier", "ich", "See\u00b7len\u00b7f\u00e4\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "PPER", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "zart psychologisch schildere ich jeden", "tokens": ["zart", "psy\u00b7cho\u00b7lo\u00b7gisch", "schil\u00b7de\u00b7re", "ich", "je\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "VVFIN", "PPER", "PIAT"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "und leg in die Nuance letzten Sinn . . .", "tokens": ["und", "leg", "in", "die", "Nu\u00b7an\u00b7ce", "letz\u00b7ten", "Sinn", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "NE", "APPR", "ART", "NN", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "(chor der Tuberkul\u00f6sen): Sie glauben nicht, wie wohl Sie uns damit tun, Herr Tiger!", "tokens": ["(", "chor", "der", "Tu\u00b7ber\u00b7ku\u00b7l\u00f6\u00b7sen", ")", ":", "Sie", "glau\u00b7ben", "nicht", ",", "wie", "wohl", "Sie", "uns", "da\u00b7mit", "tun", ",", "Herr", "Ti\u00b7ger", "!"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "$(", "$.", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "ADV", "PPER", "PRF", "PAV", "VVINF", "$,", "NN", "NN", "$."], "meter": "+-+--+--+-+-+-+----+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich dichte leis und sachte vor mich hin . . .", "tokens": ["Ich", "dich\u00b7te", "leis", "und", "sach\u00b7te", "vor", "mich", "hin", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "(alle Ch\u00f6re): Wir haben keine Zeit, Nuancen zu betrachten!", "tokens": ["(", "al\u00b7le", "Ch\u00f6\u00b7re", ")", ":", "Wir", "ha\u00b7ben", "kei\u00b7ne", "Zeit", ",", "Nu\u00b7an\u00b7cen", "zu", "be\u00b7trach\u00b7ten", "!"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "$(", "$.", "PPER", "VAFIN", "PIAT", "NN", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Wir m\u00fcssen in muffigen L\u00f6chern und Gasr\u00f6hren \u00fcbernachten!", "tokens": ["Wir", "m\u00fcs\u00b7sen", "in", "muf\u00b7fi\u00b7gen", "L\u00f6\u00b7chern", "und", "Gas\u00b7r\u00f6h\u00b7ren", "\u00fc\u00b7bern\u00b7ach\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wir haben keine Lust, zu warten und immer zu warten!", "tokens": ["Wir", "ha\u00b7ben", "kei\u00b7ne", "Lust", ",", "zu", "war\u00b7ten", "und", "im\u00b7mer", "zu", "war\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PTKZU", "VVINF", "KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Unsre Not schafft erst deine Einsamkeit, deine Stille und deinen Garten!", "tokens": ["Uns\u00b7re", "Not", "schafft", "erst", "dei\u00b7ne", "Ein\u00b7sam\u00b7keit", ",", "dei\u00b7ne", "Stil\u00b7le", "und", "dei\u00b7nen", "Gar\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+--+--+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "Wir: Arbeitslose, welke M\u00fctter, Tuberkelkranke wollen heraus", "tokens": ["Wir", ":", "Ar\u00b7beits\u00b7lo\u00b7se", ",", "wel\u00b7ke", "M\u00fct\u00b7ter", ",", "Tu\u00b7ber\u00b7kel\u00b7kran\u00b7ke", "wol\u00b7len", "he\u00b7raus"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$.", "NE", "$,", "PWAT", "NN", "$,", "NN", "VMFIN", "ADV"], "meter": "-+-+-+-+-+--+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "aus euerm Dreck in unser neues Haus!", "tokens": ["aus", "eu\u00b7erm", "Dreck", "in", "un\u00b7ser", "neu\u00b7es", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wir singen auch ein Lied. Das ist nicht fein.", "tokens": ["Wir", "sin\u00b7gen", "auch", "ein", "Lied", ".", "Das", "ist", "nicht", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$.", "PDS", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Darauf kommts auch gar nicht an. Und wir stampfen es euch in die Ohren hinein:", "tokens": ["Da\u00b7rauf", "kommts", "auch", "gar", "nicht", "an", ".", "Und", "wir", "stamp\u00b7fen", "es", "euch", "in", "die", "Oh\u00b7ren", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ADV", "PTKNEG", "PTKVZ", "$.", "KON", "PPER", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+--+--+--+", "measure": "iambic.septa.relaxed"}}, "stanza.6": {"line.1": {"text": "V\u00f6lker, h\u00f6rt die Signale!", "tokens": ["V\u00f6l\u00b7ker", ",", "h\u00f6rt", "die", "Sig\u00b7na\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Auf zum letzten Gefecht!", "tokens": ["Auf", "zum", "letz\u00b7ten", "Ge\u00b7fecht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Internationale", "tokens": ["Die", "In\u00b7ter\u00b7na\u00b7ti\u00b7o\u00b7na\u00b7le"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Erk\u00e4mpft das Menschenrecht \u2013!", "tokens": ["Er\u00b7k\u00e4mpft", "das", "Men\u00b7schen\u00b7recht", "\u2013", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich bin so menschenm\u00fcde und wie ohne Haut.", "tokens": ["Ich", "bin", "so", "men\u00b7schen\u00b7m\u00fc\u00b7de", "und", "wie", "oh\u00b7ne", "Haut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "PWAV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die andern mag ich nicht \u2013 sie tun mir wehe.", "tokens": ["