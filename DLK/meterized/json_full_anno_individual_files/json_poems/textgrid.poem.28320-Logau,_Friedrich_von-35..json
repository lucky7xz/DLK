{"textgrid.poem.28320": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein M\u00e4gdlein, dem ein Traum hat etwas warm gemacht,", "tokens": ["Ein", "M\u00e4gd\u00b7lein", ",", "dem", "ein", "Traum", "hat", "et\u00b7was", "warm", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den sie auch kunte nicht bald bringen aus der acht,", "tokens": ["Den", "sie", "auch", "kun\u00b7te", "nicht", "bald", "brin\u00b7gen", "aus", "der", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VMFIN", "PTKNEG", "ADV", "VVINF", "APPR", "ART", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ging Morgens fr\u00fch hinau\u00df spatziren in das Gras,", "tokens": ["Ging", "Mor\u00b7gens", "fr\u00fch", "hin\u00b7au\u00df", "spat\u00b7zi\u00b7ren", "in", "das", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APZR", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da spritzt ihr dessen Thau hinauff auff di\u00df und das.", "tokens": ["Da", "spritzt", "ihr", "des\u00b7sen", "Thau", "hin\u00b7auff", "auff", "di\u00df", "und", "das", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "NN", "ADV", "APPR", "PDS", "KON", "PDS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie sprach: Es mag wol seyn, da\u00df Kr\u00e4uter w\u00fcrcken sehr;", "tokens": ["Sie", "sprach", ":", "Es", "mag", "wol", "seyn", ",", "da\u00df", "Kr\u00e4u\u00b7ter", "w\u00fcr\u00b7cken", "sehr", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "ADV", "VAINF", "$,", "KOUS", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ie dennoch, wie mich d\u00fcnckt, so k\u00fcnnen Wurtzeln mehr.", "tokens": ["Ie", "den\u00b7noch", ",", "wie", "mich", "d\u00fcnckt", ",", "so", "k\u00fcn\u00b7nen", "Wurt\u00b7zeln", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein M\u00e4gdlein, dem ein Traum hat etwas warm gemacht,", "tokens": ["Ein", "M\u00e4gd\u00b7lein", ",", "dem", "ein", "Traum", "hat", "et\u00b7was", "warm", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den sie auch kunte nicht bald bringen aus der acht,", "tokens": ["Den", "sie", "auch", "kun\u00b7te", "nicht", "bald", "brin\u00b7gen", "aus", "der", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VMFIN", "PTKNEG", "ADV", "VVINF", "APPR", "ART", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambi