{"textgrid.poem.38111": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Tartarf\u00fcrstin", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was wollt ihr aber h\u00f6ren,", "tokens": ["Was", "wollt", "ihr", "a\u00b7ber", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was wollt ihr, da\u00df ich sing?", "tokens": ["Was", "wollt", "ihr", ",", "da\u00df", "ich", "sing", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl von der Tartarf\u00fcrstin,", "tokens": ["Wohl", "von", "der", "Tar\u00b7tar\u00b7f\u00fcrs\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie's der zu Neumark ging.", "tokens": ["Wie's", "der", "zu", "Neu\u00b7mark", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nach Bresselau in Schlesien", "tokens": ["Nach", "Bres\u00b7se\u00b7lau", "in", "Schle\u00b7si\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein gro\u00dfe Rei\u00df sie macht,", "tokens": ["Ein", "gro\u00b7\u00dfe", "Rei\u00df", "sie", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nach Neumark kam sie gefahren", "tokens": ["Nach", "Neu\u00b7mark", "kam", "sie", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "PPER", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und blieb allda zur Nacht.", "tokens": ["Und", "blieb", "all\u00b7da", "zur", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da sprach der Wirth zum andern:", "tokens": ["Da", "sprach", "der", "Wirth", "zum", "an\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbein Heydin wohnt bey mir,", "tokens": ["\u00bb", "ein", "Hey\u00b7din", "wohnt", "bey", "mir", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat Gold, Edelsteine,", "tokens": ["Sie", "hat", "Gold", ",", "E\u00b7del\u00b7stei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die la\u00df ich nicht von hier.\u00ab", "tokens": ["Die", "la\u00df", "ich", "nicht", "von", "hier", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "VVIMP", "PPER", "PTKNEG", "APPR", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbgut Nacht, O F\u00fcrstin sch\u00f6ne,", "tokens": ["\u00bb", "gut", "Nacht", ",", "O", "F\u00fcrs\u00b7tin", "sch\u00f6\u00b7ne", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$,", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr lebt nicht bis zum Tag.\u00ab", "tokens": ["Ihr", "lebt", "nicht", "bis", "zum", "Tag", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wandte sich behende,", "tokens": ["Und", "wand\u00b7te", "sich", "be\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gab ihr den Todesschlag.", "tokens": ["Gab", "ihr", "den", "To\u00b7des\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und all ihr Hofgesinde", "tokens": ["Und", "all", "ihr", "Hof\u00b7ge\u00b7sin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In tiefem Schlaf er fand,", "tokens": ["In", "tie\u00b7fem", "Schlaf", "er", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00fcrgt sie gro\u00df und kleine", "tokens": ["Und", "w\u00fcrgt", "sie", "gro\u00df", "und", "klei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit seiner eignen Hand.", "tokens": ["Mit", "sei\u00b7ner", "eig\u00b7nen", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mit seinen eignen H\u00e4nden", "tokens": ["Mit", "sei\u00b7nen", "eig\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Begrub er sie allzumal", "tokens": ["Be\u00b7grub", "er", "sie", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gar tief in kalten Keller,", "tokens": ["Gar", "tief", "in", "kal\u00b7ten", "Kel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Gold und Gut er stahl.", "tokens": ["Ihr", "Gold", "und", "Gut", "er", "stahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er zeigte drauf den andern", "tokens": ["Er", "zeig\u00b7te", "drauf", "den", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein Hand von Blut so roth,", "tokens": ["Sein", "Hand", "von", "Blut", "so", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Gold und Edelsteinen", "tokens": ["Von", "Gold", "und", "E\u00b7del\u00b7stei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die H\u00e4lft er ihnen bot.", "tokens": ["Die", "H\u00e4lft", "er", "ih\u00b7nen", "bot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die nahmen sie so gerne", "tokens": ["Die", "nah\u00b7men", "sie", "so", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schwiegen von der That,", "tokens": ["Und", "schwie\u00b7gen", "von", "der", "That", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch was nicht fr\u00fch ger\u00e4chet,", "tokens": ["Doch", "was", "nicht", "fr\u00fch", "ge\u00b7r\u00e4\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das straft der Himmel spat.", "tokens": ["Das", "straft", "der", "Him\u00b7mel", "spat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Tartarf\u00fcrst, der h\u00f6rte", "tokens": ["Der", "Tar\u00b7tar\u00b7f\u00fcrst", ",", "der", "h\u00f6r\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Neumark ist mein Kind", "tokens": ["In", "Neu\u00b7mark", "ist", "mein", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gemordet und beraubet,", "tokens": ["Ge\u00b7mor\u00b7det", "und", "be\u00b7rau\u00b7bet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den K\u00f6rper man noch findt.", "tokens": ["Den", "K\u00f6r\u00b7per", "man", "noch", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da rief er seinen Haufen:", "tokens": ["Da", "rief", "er", "sei\u00b7nen", "Hau\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbauf nehmet Spie\u00df und Schwerd,", "tokens": ["\u00bb", "auf", "neh\u00b7met", "Spie\u00df", "und", "Schwerd", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nach Schlesien wir ziehen,", "tokens": ["Nach", "Schle\u00b7si\u00b7en", "wir", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es ist des Ziehens werth.\u00ab", "tokens": ["Es", "ist", "des", "Zie\u00b7hens", "werth", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "So kamen sie in Schaaren", "tokens": ["So", "ka\u00b7men", "sie", "in", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ins ganze Schlesier Land,", "tokens": ["Ins", "gan\u00b7ze", "Schle\u00b7sier", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sengten, brannten, stahlen,", "tokens": ["Und", "seng\u00b7ten", ",", "brann\u00b7ten", ",", "stah\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Welt ists wohlbekannt.", "tokens": ["Der", "Welt", "ists", "wohl\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der F\u00fcrstin Tod zu r\u00e4chen", "tokens": ["Der", "F\u00fcrs\u00b7tin", "Tod", "zu", "r\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bey Wahlstadt ging es tr\u00fcb,", "tokens": ["Bey", "Wahl\u00b7stadt", "ging", "es", "tr\u00fcb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zur Ehr der Heidenf\u00fcrstin", "tokens": ["Zur", "Ehr", "der", "Hei\u00b7den\u00b7f\u00fcrs\u00b7tin"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Christen Herzog blieb.", "tokens": ["Der", "Chris\u00b7ten", "Her\u00b7zog", "blieb", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "So ward am Land ger\u00e4chet", "tokens": ["So", "ward", "am", "Land", "ge\u00b7r\u00e4\u00b7chet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was Neumark hat gethan,", "tokens": ["Was", "Neu\u00b7mark", "hat", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Herr Gott mich selbst regiere", "tokens": ["Herr", "Gott", "mich", "selbst", "re\u00b7gie\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fang ich allein was an.", "tokens": ["Fang", "ich", "al\u00b7lein", "was", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PWS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Was wollt ihr aber h\u00f6ren,", "tokens": ["Was", "wollt", "ihr", "a\u00b7ber", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was wollt ihr, da\u00df ich sing?", "tokens": ["Was", "wollt", "ihr", ",", "da\u00df", "ich", "sing", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl von der Tartarf\u00fcrstin,", "tokens": ["Wohl", "von", "der", "Tar\u00b7tar\u00b7f\u00fcrs\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measur