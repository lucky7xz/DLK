{"dta.poem.18036": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Critische Betrachtungen \u00fcber des  \n Herrn von Hagedorn Ode auf  \n den Weisen.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein andrer, dessen Schrift mein wallend Hertz bewegt,", "tokens": ["Ein", "an\u00b7drer", ",", "des\u00b7sen", "Schrift", "mein", "wal\u00b7lend", "Hertz", "be\u00b7wegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELAT", "NN", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df mein Gesang sein Lob auf willgen Fl\u00fcgeln tr\u00e4gt,", "tokens": ["Da\u00df", "mein", "Ge\u00b7sang", "sein", "Lob", "auf", "will\u00b7gen", "Fl\u00fc\u00b7geln", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist jener, den ein Schwarm verbuhlter Fr\u00f6hligkeiten,", "tokens": ["Ist", "je\u00b7ner", ",", "den", "ein", "Schwarm", "ver\u00b7buhl\u00b7ter", "Fr\u00f6h\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "$,", "PRELS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Z\u00e4rtlichkeit, der Witz, und schlaue Schertz begleiten.", "tokens": ["Die", "Z\u00e4rt\u00b7lich\u00b7keit", ",", "der", "Witz", ",", "und", "schlau\u00b7e", "Schertz", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er f\u00fchrte sie zuerst bey Hamburgs Sch\u00f6nen ein;", "tokens": ["Er", "f\u00fchr\u00b7te", "sie", "zu\u00b7erst", "bey", "Ham\u00b7burgs", "Sch\u00f6\u00b7nen", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bey ihrer Ankunft floh der falschen Frommen Schein,", "tokens": ["Bey", "ih\u00b7rer", "An\u00b7kunft", "floh", "der", "fal\u00b7schen", "From\u00b7men", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Zunge Furchtsamkeit, die Plumpheit im Betragen,", "tokens": ["Der", "Zun\u00b7ge", "Furcht\u00b7sam\u00b7keit", ",", "die", "Plum\u00b7pheit", "im", "Be\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Glieder tr\u00e4ge Last, die Minen, die nichts sagen,", "tokens": ["Der", "Glie\u00b7der", "tr\u00e4\u00b7ge", "Last", ",", "die", "Mi\u00b7nen", ",", "die", "nichts", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$,", "ART", "NN", "$,", "PRELS", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das Lachen ohne Sinn, die schwartze Sudeley,", "tokens": ["Das", "La\u00b7chen", "oh\u00b7ne", "Sinn", ",", "die", "schwart\u00b7ze", "Su\u00b7de\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit der gekauften Lust, und w\u00fcsten Schwelgerey.", "tokens": ["Mit", "der", "ge\u00b7kauf\u00b7ten", "Lust", ",", "und", "w\u00fcs\u00b7ten", "Schwel\u00b7ge\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wovon er nur erz\u00e4hlt, das kriegt urpl\u00f6tzlich Sitten.", "tokens": ["Wo\u00b7von", "er", "nur", "er\u00b7z\u00e4hlt", ",", "das", "kriegt", "ur\u00b7pl\u00f6tz\u00b7lich", "Sit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "PDS", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Annehmlichkeit und Reitz w\u00e4ch\u00dft unter seinen Tritten.", "tokens": ["An\u00b7nehm\u00b7lich\u00b7keit", "und", "Reitz", "w\u00e4ch\u00dft", "un\u00b7ter", "sei\u00b7nen", "Trit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Wahrheit weiset sich in holder Zierlichkeit,", "tokens": ["Die", "Wahr\u00b7heit", "wei\u00b7set", "sich", "in", "hol\u00b7der", "Zier\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und die Natur gl\u00e4ntzt hier gantz pr\u00e4chtig ungekleidt.", "tokens": ["Und", "die", "Na\u00b7tur", "gl\u00e4ntzt", "hier", "gantz", "pr\u00e4ch\u00b7tig", "un\u00b7ge\u00b7kleidt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nat\u00fcrlichs dieser Art ist kaum genug zu sch\u00e4tzen,", "tokens": ["Na\u00b7t\u00fcr\u00b7lichs", "die\u00b7ser", "Art", "ist", "kaum", "ge\u00b7nug", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "NN", "VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und dem Erhabnen selbst nur wenig nachzusetzen.", "tokens": ["Und", "dem", "Er\u00b7hab\u00b7nen", "selbst", "nur", "we\u00b7nig", "nach\u00b7zu\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}