{"textgrid.poem.30826": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was man guten Freunden schencket, ist verwahret, nicht verschencket;", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was man guten Freunden schencket, ist verwahret, nicht verschencket;", "tokens": ["Was", "man", "gu\u00b7ten", "Freun\u00b7den", "schen\u00b7cket", ",", "ist", "ver\u00b7wah\u00b7ret", ",", "nicht", "ver\u00b7schen\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJA", "NN", "VVFIN", "$,", "VAFIN", "VVFIN", "$,", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Also saget Poscinummus, wann er was zu haben dencket;", "tokens": ["Al\u00b7so", "sa\u00b7get", "Pos\u00b7ci\u00b7num\u00b7mus", ",", "wann", "er", "was", "zu", "ha\u00b7ben", "den\u00b7cket", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "PWAV", "PPER", "PIS", "PTKZU", "VAINF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Aber wann er was soll geben, o, so r\u00fchmt er hoch das sparen,", "tokens": ["A\u00b7ber", "wann", "er", "was", "soll", "ge\u00b7ben", ",", "o", ",", "so", "r\u00fchmt", "er", "hoch", "das", "spa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PIS", "VMFIN", "VVINF", "$,", "FM", "$,", "ADV", "VVFIN", "PPER", "ADJD", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Da\u00df man etwa nicht auffs Alter Noth