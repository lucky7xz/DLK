{"textgrid.poem.43740": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das ist ein reicher Segen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist ein reicher Segen", "tokens": ["Das", "ist", "ein", "rei\u00b7cher", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In G\u00e4rten und an Wegen!", "tokens": ["In", "G\u00e4r\u00b7ten", "und", "an", "We\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die B\u00e4ume brechen fast.", "tokens": ["Die", "B\u00e4u\u00b7me", "bre\u00b7chen", "fast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie voll doch Alles hanget!", "tokens": ["Wie", "voll", "doch", "Al\u00b7les", "han\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie lieblich schwebt und pranget", "tokens": ["Wie", "lieb\u00b7lich", "schwebt", "und", "pran\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Aepfel goldne Last!", "tokens": ["Der", "A\u00b7e\u00b7pfel", "gold\u00b7ne", "Last", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Jetzt auf den Baum gestiegen!", "tokens": ["Jetzt", "auf", "den", "Baum", "ge\u00b7stie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "La\u00dft uns die Zweige biegen,", "tokens": ["La\u00dft", "uns", "die", "Zwei\u00b7ge", "bie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Jedes pfl\u00fccken kann!", "tokens": ["Da\u00df", "Je\u00b7des", "pfl\u00fc\u00b7cken", "kann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie hoch die Aepfel hangen,", "tokens": ["Wie", "hoch", "die", "A\u00b7e\u00b7pfel", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wir holen sie mit Stangen", "tokens": ["Wir", "ho\u00b7len", "sie", "mit", "Stan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Haken all' heran.", "tokens": ["Und", "Ha\u00b7ken", "all'", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und ist das Werk vollendet,", "tokens": ["Und", "ist", "das", "Werk", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So wird auch uns gespendet", "tokens": ["So", "wird", "auch", "uns", "ge\u00b7spen\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Lohn f\u00fcr unsern Flei\u00df.", "tokens": ["Ein", "Lohn", "f\u00fcr", "un\u00b7sern", "Flei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dann zieh'n wir fort und bringen", "tokens": ["Dann", "zieh'n", "wir", "fort", "und", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Aepfel heim und singen", "tokens": ["Die", "A\u00b7e\u00b7pfel", "heim", "und", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "KON", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Dem Herbste Lob und Preis.", "tokens": ["Dem", "Herbs\u00b7te", "Lob", "und", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das ist ein reicher Segen", "tokens": ["Das", "ist", "ein", "rei\u00b7cher", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In G\u00e4rten und an Wegen!", "tokens": ["In", "G\u00e4r\u00b7ten", "und", "an", "We\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die B\u00e4ume brechen fast.", "tokens": ["Die", "B\u0