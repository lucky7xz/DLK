{"textgrid.poem.41460": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der m\u00e4\u00dfige Eifer des Frontins", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frontin, der fast Aesop, an Wuchs und Bildung, glich,", "tokens": ["Fron\u00b7tin", ",", "der", "fast", "A\u00b7e\u00b7sop", ",", "an", "Wuchs", "und", "Bil\u00b7dung", ",", "glich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "NE", "$,", "APPR", "NN", "KON", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Ging lustig an den Strand, warf schnell sein Kleid von sich,", "tokens": ["Ging", "lus\u00b7tig", "an", "den", "Strand", ",", "warf", "schnell", "sein", "Kleid", "von", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,", "VVFIN", "ADJD", "PPOSAT", "NN", "APPR", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sprang muthig in den Strom, und schwamm recht meisterlich.", "tokens": ["Sprang", "mut\u00b7hig", "in", "den", "Strom", ",", "und", "schwamm", "recht", "meis\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Indessen k\u00f6mmt ein Dieb, bestiehlt den sichern Schwimmer,", "tokens": ["In\u00b7des\u00b7sen", "k\u00f6mmt", "ein", "Dieb", ",", "be\u00b7stiehlt", "den", "si\u00b7chern", "Schwim\u00b7mer", ","], "token_info": ["