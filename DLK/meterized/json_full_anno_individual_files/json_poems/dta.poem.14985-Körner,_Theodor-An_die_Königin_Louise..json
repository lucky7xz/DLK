{"dta.poem.14985": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "An die K\u00f6nigin Louise.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Es dringe m\u00e4chtig auf zu ", "tokens": ["Es", "drin\u00b7ge", "m\u00e4ch\u00b7tig", "auf", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PTKZU"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kannst wieder freundlich auf uns niedersehen,", "tokens": ["Kannst", "wie\u00b7der", "freund\u00b7lich", "auf", "uns", "nie\u00b7der\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verkl\u00e4rter Engel! l\u00e4nger weine nicht!", "tokens": ["Ver\u00b7kl\u00e4r\u00b7ter", "En\u00b7gel", "!", "l\u00e4n\u00b7ger", "wei\u00b7ne", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJD", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn Preu\u00dfens Adler soll zum Kampfe wehen,", "tokens": ["Denn", "Preu\u00b7\u00dfens", "Ad\u00b7ler", "soll", "zum", "Kamp\u00b7fe", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VMFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es dr\u00e4ngt ", "tokens": ["Es", "dr\u00e4ngt"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Und jeder w\u00e4hlt, und keinen siehst ", "tokens": ["Und", "je\u00b7der", "w\u00e4hlt", ",", "und", "kei\u00b7nen", "siehst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den freien Tod f\u00fcr ein bezwungnes Leben.", "tokens": ["Den", "frei\u00b7en", "Tod", "f\u00fcr", "ein", "be\u00b7zwung\u00b7nes", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir lagen noch in feige Schmach gebettet\u201a", "tokens": ["Wir", "la\u00b7gen", "noch", "in", "fei\u00b7ge", "Schmach", "ge\u00b7bet\u00b7tet", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da rief nach ", "tokens": ["Da", "rief", "nach"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "An die unw\u00fcrd'ge Zeit warst ", "tokens": ["An", "die", "un\u00b7w\u00fcrd'\u00b7ge", "Zeit", "warst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Zur Rache mahnte ", "tokens": ["Zur", "Ra\u00b7che", "mahn\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "So hast ", "tokens": ["So", "hast"], "token_info": ["word", "word"], "pos": ["ADV", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Jetzt sieh auf uns, sieh auf ", "tokens": ["Jetzt", "sieh", "auf", "uns", ",", "sieh", "auf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$,", "VVIMP", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie alle Herzen treu und muthig brennen! \u2014", "tokens": ["Wie", "al\u00b7le", "Her\u00b7zen", "treu", "und", "mut\u00b7hig", "bren\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADJD", "KON", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nun woll' uns auch die ", "tokens": ["Nun", "woll'", "uns", "auch", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Und wie einst alle Kr\u00e4fte zu beleben,", "tokens": ["Und", "wie", "einst", "al\u00b7le", "Kr\u00e4f\u00b7te", "zu", "be\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Heil'genbild f\u00fcr den gerechten Krieg,", "tokens": ["Ein", "Heil'\u00b7gen\u00b7bild", "f\u00fcr", "den", "ge\u00b7rech\u00b7ten", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem Heeresbanner sch\u00fctzend zugegeben,", "tokens": ["Dem", "Hee\u00b7res\u00b7ban\u00b7ner", "sch\u00fct\u00b7zend", "zu\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als Oriflamme in die L\u00fcfte stieg;", "tokens": ["Als", "O\u00b7ri\u00b7flam\u00b7me", "in", "die", "L\u00fcf\u00b7te", "stieg", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So soll dein Bild auf unsern Fahnen schweben,", "tokens": ["So", "soll", "dein", "Bild", "auf", "un\u00b7sern", "Fah\u00b7nen", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und soll uns leuchten durch die Nacht zum Sieg.", "tokens": ["Und", "soll", "uns", "leuch\u00b7ten", "durch", "die", "Nacht", "zum", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wenn wir dann dem Meuter-Heer begegnen,", "tokens": ["Und", "wenn", "wir", "dann", "dem", "Meu\u00b7ter\u00b7Heer", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir st\u00fcrzen uns voll Zuversicht hinein,", "tokens": ["Wir", "st\u00fcr\u00b7zen", "uns", "voll", "Zu\u00b7ver\u00b7sicht", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und m\u00f6gen tausend Flammenblitze regnen,", "tokens": ["Und", "m\u00f6\u00b7gen", "tau\u00b7send", "Flam\u00b7men\u00b7blit\u00b7ze", "reg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und m\u00f6gen tausend Tode uns umdr\u00e4u'n,", "tokens": ["Und", "m\u00f6\u00b7gen", "tau\u00b7send", "To\u00b7de", "uns", "um\u00b7dr\u00e4u'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "CARD", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Blick auf ", "tokens": ["Ein", "Blick", "auf"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Wir stehen fest, wir m\u00fcssen Sieger seyn! \u2014", "tokens": ["Wir", "ste\u00b7hen", "fest", ",", "wir", "m\u00fcs\u00b7sen", "Sie\u00b7ger", "seyn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VMFIN", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wer dann auch f\u00e4llt f\u00fcr Tugend, Recht und                                 Wahrheit,", "tokens": ["Wer", "dann", "auch", "f\u00e4llt", "f\u00fcr", "Tu\u00b7gend", ",", "Recht", "und", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}