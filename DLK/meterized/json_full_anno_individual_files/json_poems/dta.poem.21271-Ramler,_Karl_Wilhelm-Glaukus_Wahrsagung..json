{"dta.poem.21271": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Glaukus Wahrsagung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20476-3", "language": ["de:0.99"], "booktitle": "Ramler, Karl Wilhelm: Oden. Berlin, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Als Ludewigs Pilot mit stolzer Flotte", "tokens": ["Als", "Lu\u00b7de\u00b7wigs", "Pi\u00b7lot", "mit", "stol\u00b7zer", "Flot\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Westgalliens besch\u00e4umtes Thor", "tokens": ["West\u00b7gal\u00b7li\u00b7ens", "be\u00b7sch\u00e4um\u00b7tes", "Thor"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Verliess, hub Glaukus aus der tiefen Fel-", "tokens": ["Ver\u00b7liess", ",", "hub", "Glau\u00b7kus", "aus", "der", "tie\u00b7fen", "Fel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein blaues Haupt empor:", "tokens": ["Sein", "blau\u00b7es", "Haupt", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ungl\u00fccklicher! der schon, von Hoff-", "tokens": ["Un\u00b7gl\u00fcck\u00b7li\u00b7cher", "!", "der", "schon", ",", "von", "Hoff"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "ART", "ADV", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Oceans Gebieter ist,", "tokens": ["Des", "O\u00b7ceans", "Ge\u00b7bie\u00b7ter", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du f\u00fchrst in deinen Schiffen einen Feuer-", "tokens": ["Du", "f\u00fchrst", "in", "dei\u00b7nen", "Schif\u00b7fen", "ei\u00b7nen", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der beide Welten frisst!", "tokens": ["Der", "bei\u00b7de", "Wel\u00b7ten", "frisst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Bald nimmt der Abgrund eine My-", "tokens": ["Bald", "nimmt", "der", "Ab\u00b7grund", "ei\u00b7ne", "My"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu fr\u00fch entleibter Seelen ein;", "tokens": ["Zu", "fr\u00fch", "ent\u00b7leib\u00b7ter", "See\u00b7len", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald werdet ihr im Meer der Hayen, am", "tokens": ["Bald", "wer\u00b7det", "ihr", "im", "Meer", "der", "Ha\u00b7yen", ",", "am"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,", "APPRART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Aaren Beute seyn!", "tokens": ["Der", "Aa\u00b7ren", "Beu\u00b7te", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die G\u00f6tter, die jetzt lachend mit euch", "tokens": ["Die", "G\u00f6t\u00b7ter", ",", "die", "jetzt", "la\u00b7chend", "mit", "euch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "APPR", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Bereuen ihr geschenktes Gl\u00fcck,", "tokens": ["Be\u00b7reu\u00b7en", "ihr", "ge\u00b7schenk\u00b7tes", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verachten euren Uebermuth, und alle", "tokens": ["Ver\u00b7ach\u00b7ten", "eu\u00b7ren", "Ue\u00b7ber\u00b7muth", ",", "und", "al\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach Albion zur\u00fcck:", "tokens": ["Nach", "Al\u00b7bi\u00b7on", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Dass Albion der meerumflossnen", "tokens": ["Dass", "Al\u00b7bi\u00b7on", "der", "mee\u00b7rum\u00b7floss\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gerechte Friedensrichterinn,", "tokens": ["Ge\u00b7rech\u00b7te", "Frie\u00b7dens\u00b7rich\u00b7te\u00b7rinn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Schrecken der beraubten Oceane", "tokens": ["Das", "Schre\u00b7cken", "der", "be\u00b7raub\u00b7ten", "O\u00b7cea\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Inseln K\u00f6niginn;", "tokens": ["Der", "In\u00b7seln", "K\u00f6\u00b7ni\u00b7ginn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ihr aber, fl\u00fcchtig unter jeder Zone,", "tokens": ["Ihr", "a\u00b7ber", ",", "fl\u00fcch\u00b7tig", "un\u00b7ter", "je\u00b7der", "Zo\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So manchen schwimmenden Palast,", "tokens": ["So", "man\u00b7chen", "schwim\u00b7men\u00b7den", "Pa\u00b7last", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Port, und Meer, und Eyland, und der", "tokens": ["Und", "Port", ",", "und", "Meer", ",", "und", "Ey\u00b7land", ",", "und", "der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "$,", "KON", "NN", "$,", "KON", "NN", "$,", "KON", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durchstr\u00f6mte Flur verlasst.", "tokens": ["Durch\u00b7str\u00f6m\u00b7te", "Flur", "ver\u00b7lasst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O weiche S\u00f6hne tapfrer Franken,", "tokens": ["O", "wei\u00b7che", "S\u00f6h\u00b7ne", "tapf\u00b7rer", "Fran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Helvetien um M\u00e4nner an!", "tokens": ["Hel\u00b7ve\u00b7ti\u00b7en", "um", "M\u00e4n\u00b7ner", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O! pl\u00fcndert unbewehrte F\u00fcrstenth\u00fcmer!", "tokens": ["O", "!", "pl\u00fcn\u00b7dert", "un\u00b7be\u00b7wehr\u00b7te", "F\u00fcrs\u00b7tent\u00b7h\u00fc\u00b7mer", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Wagen, Ross und Mann", "tokens": ["Mit", "Wa\u00b7gen", ",", "Ross", "und", "Mann"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NE", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "In eurer V\u00e4ter alte Sitze! schreitet", "tokens": ["In", "eu\u00b7rer", "V\u00e4\u00b7ter", "al\u00b7te", "Sit\u00b7ze", "!", "schrei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$.", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "K\u00fchn \u00fcber den geh\u00f6rnten Rhein,", "tokens": ["K\u00fchn", "\u00fc\u00b7ber", "den", "ge\u00b7h\u00f6rn\u00b7ten", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sucht Pallas Liebling auf, der f\u00fcr sein", "tokens": ["Sucht", "Pal\u00b7las", "Lieb\u00b7ling", "auf", ",", "der", "f\u00fcr", "sein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NE", "NN", "PTKVZ", "$,", "PRELS", "APPR", "PPOSAT"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und, eurer Macht zu klein,", "tokens": ["Und", ",", "eu\u00b7rer", "Macht", "zu", "klein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und von verschwornen Barbarn \u00fcber-", "tokens": ["Und", "von", "ver\u00b7schwor\u00b7nen", "Bar\u00b7barn", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Einst wanken muss: erdr\u00fccket ihn! \u2014", "tokens": ["Einst", "wan\u00b7ken", "muss", ":", "er\u00b7dr\u00fc\u00b7cket", "ihn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$.", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr unter den verschwornen sollt, ihr un-", "tokens": ["Ihr", "un\u00b7ter", "den", "ver\u00b7schwor\u00b7nen", "sollt", ",", "ihr", "un"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPR", "ART", "VVINF", "VMFIN", "$,", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Allein mit Schande fliehn!", "tokens": ["Al\u00b7lein", "mit", "Schan\u00b7de", "fliehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Der Ort, wo sieben Krieger funfzig", "tokens": ["Der", "Ort", ",", "wo", "sie\u00b7ben", "Krie\u00b7ger", "funf\u00b7zig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "CARD", "NN", "CARD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ob ihr ihn zu vernichten sucht,", "tokens": ["Ob", "ihr", "ihn", "zu", "ver\u00b7nich\u00b7ten", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Brandmaal wird er euch, worauf in", "tokens": ["Ein", "Brand\u00b7maal", "wird", "er", "euch", ",", "wo\u00b7rauf", "in"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPER", "$,", "PWAV", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein bessrer Enkel flucht.", "tokens": ["Ein", "bess\u00b7rer", "En\u00b7kel", "flucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ob alle Reisigen aus euren Vesten,", "tokens": ["Ob", "al\u00b7le", "Rei\u00b7si\u00b7gen", "aus", "eu\u00b7ren", "Ves\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob eine neue Helene", "tokens": ["Ob", "ei\u00b7ne", "neu\u00b7e", "He\u00b7le\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Euch alle Prinzen aus Lutetiens Pa-", "tokens": ["Euch", "al\u00b7le", "Prin\u00b7zen", "aus", "Lu\u00b7te\u00b7ti\u00b7ens", "Pa"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIAT", "NN", "APPR", "NE", "TRUNC"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Zu Feldherrn sendete:", "tokens": ["Zu", "Feld\u00b7herrn", "sen\u00b7de\u00b7te", ":"], "token_info": ["word", "word", 