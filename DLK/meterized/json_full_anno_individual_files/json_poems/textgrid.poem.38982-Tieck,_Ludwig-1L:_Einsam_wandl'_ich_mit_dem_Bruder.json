{"textgrid.poem.38982": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einsam wandl' ich mit dem Bruder", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsam wandl' ich mit dem Bruder", "tokens": ["Ein\u00b7sam", "wandl'", "ich", "mit", "dem", "Bru\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter Gr\u00e4bern.", "tokens": ["Un\u00b7ter", "Gr\u00e4\u00b7bern", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Bild an Bild, und Vers an Vers gedr\u00e4ngt.", "tokens": ["Bild", "an", "Bild", ",", "und", "Vers", "an", "Vers", "ge\u00b7dr\u00e4ngt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "KON", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Rosen gl\u00fchn, und Lilien gl\u00e4nzen,", "tokens": ["Ro\u00b7sen", "gl\u00fchn", ",", "und", "Li\u00b7li\u00b7en", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Frischer gr\u00fcner Rasen,", "tokens": ["Fri\u00b7scher", "gr\u00fc\u00b7ner", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Die Gluth des Lebens mit allen Farben", "tokens": ["Die", "Gluth", "des", "Le\u00b7bens", "mit", "al\u00b7len", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Als Teppich des Todes. \u2013", "tokens": ["Als", "Tep\u00b7pich", "des", "To\u00b7des", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Solche Haushaltung f\u00fchrt nur die Liebe.", "tokens": ["Sol\u00b7che", "Haus\u00b7hal\u00b7tung", "f\u00fchrt", "nur", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Nein, hier sind die Verschiednen nicht entflohn,", "tokens": ["Nein", ",", "hier", "sind", "die", "Ver\u00b7schied\u00b7nen", "nicht", "ent\u00b7flohn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Aus Knosp' und Blum und Thau des Grafes", "tokens": ["Aus", "Knosp'", "und", "Blum", "und", "Thau", "des", "Gra\u00b7fes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NE", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Quillt L\u00e4cheln und Thr\u00e4ne noch immer hervor.", "tokens": ["Quillt", "L\u00e4\u00b7cheln", "und", "Thr\u00e4\u00b7ne", "noch", "im\u00b7mer", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Dort knien auch Kinder", "tokens": ["Dort", "kni\u00b7en", "auch", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Und heften betend Blumengewinde", "tokens": ["Und", "hef\u00b7ten", "be\u00b7tend", "Blu\u00b7men\u00b7ge\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Um die eisernen Kreuze der Eltern.", "tokens": ["Um", "die", "ei\u00b7ser\u00b7nen", "Kreu\u00b7ze", "der", "El\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.15": {"text": "Der Gatte entfernter", "tokens": ["Der", "Gat\u00b7te", "ent\u00b7fern\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APPO"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Die Eltern hier in der N\u00e4he,", "tokens": ["Die", "El\u00b7tern", "hier", "in", "der", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Bringen, wie immer die Liebe that,", "tokens": ["Brin\u00b7gen", ",", "wie", "im\u00b7mer", "die", "Lie\u00b7be", "that", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.18": {"text": "Thr\u00e4nen, Gebet und des Sommers bunter Schmuck.", "tokens": ["Thr\u00e4\u00b7nen", ",", "Ge\u00b7bet", "und", "des", "Som\u00b7mers", "bun\u00b7ter", "Schmuck", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Welche Wehmuth zittert durch mein Wesen?", "tokens": ["Wel\u00b7che", "Weh\u00b7muth", "zit\u00b7tert", "durch", "mein", "We\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Auch hier in weiter Ferne", "tokens": ["Auch", "hier", "in", "wei\u00b7ter", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Kann ich um alle die Theuern klagen,", "tokens": ["Kann", "ich", "um", "al\u00b7le", "die", "Theu\u00b7ern", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Die ich fr\u00fch und sp\u00e4t verlor.", "tokens": ["Die", "ich", "fr\u00fch", "und", "sp\u00e4t", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Mein Schmerz vermischt sich mit den Weinenden,", "tokens": ["Mein", "Schmerz", "ver\u00b7mischt", "sich", "mit", "den", "Wei\u00b7nen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.24": {"text": "In den Thr\u00e4nen mehr als in der Lust", "tokens": ["In", "den", "Thr\u00e4\u00b7nen", "mehr", "als", "in", "der", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PIAT", "KOKOM", "APPR", "ART", "NN"], "meter": "+-+-+++-+", "measure": "unknown.measure.hexa"}, "line.25": {"text": "Sind wir alle Br\u00fcder.", "tokens": ["Sind", "wir", "al\u00b7le", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Aber hier in der Halle,", "tokens": ["A\u00b7ber", "hier", "in", "der", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Im fernen, unbesuchten Winkel", "tokens": ["Im", "fer\u00b7nen", ",", "un\u00b7be\u00b7such\u00b7ten", "Win\u00b7kel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Find ich ein Blatt, von alter Hand beschrieben,", "tokens": ["Find", "ich", "ein", "Blatt", ",", "von", "al\u00b7ter", "Hand", "be\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So deutet die zitternde, ungewisse Schrift:", "tokens": ["So", "deu\u00b7tet", "die", "zit\u00b7tern\u00b7de", ",", "un\u00b7ge\u00b7wis\u00b7se", "Schrift", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "\u00bbjeder Christ, der hier mag wandeln,", "tokens": ["\u00bb", "je\u00b7der", "Christ", ",", "der", "hier", "mag", "wan\u00b7deln", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "$,", "PRELS", "ADV", "VMFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bete freundlich f\u00fcr ein Wesen,", "tokens": ["Be\u00b7te", "freund\u00b7lich", "f\u00fcr", "ein", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das im unnennbaren Jammer,", "tokens": ["Das", "im", "un\u00b7nenn\u00b7ba\u00b7ren", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das im tiefsten Schmerz vergeht,", "tokens": ["Das", "im", "tiefs\u00b7ten", "Schmerz", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Zu dem Vater, der die Liebe,", "tokens": ["Zu", "dem", "Va\u00b7ter", ",", "der", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df er tr\u00f6ste, wenn nicht helfe.\u00ab \u2013", "tokens": ["Da\u00df", "er", "tr\u00f6s\u00b7te", ",", "wenn", "nicht", "hel\u00b7fe", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PTKNEG", "VVFIN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da brachen unaufhaltsam meine Thr\u00e4nen,", "tokens": ["Da", "bra\u00b7chen", "un\u00b7auf\u00b7halt\u00b7sam", "mei\u00b7ne", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sie beteten mit Inbrunst.", "tokens": ["Und", "sie", "be\u00b7te\u00b7ten", "mit", "In\u00b7brunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Kommt ein Herr dahergegangen,", "tokens": ["Kommt", "ein", "Herr", "da\u00b7her\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht das Blatt, die Kreutz' und Blumen,", "tokens": ["Sieht", "das", "Blatt", ",", "die", "Kreutz'", "und", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Kinder, Eltern, Gatten,", "tokens": ["Und", "die", "Kin\u00b7der", ",", "El\u00b7tern", ",", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4lt wohl meinen Schmerz f\u00fcr Ingrimm,", "tokens": ["H\u00e4lt", "wohl", "mei\u00b7nen", "Schmerz", "f\u00fcr", "In\u00b7grimm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPR", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Spricht mit Afterweisheit;", "tokens": ["Spricht", "mit", "Af\u00b7ter\u00b7weis\u00b7heit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ja, es w\u00e4re nun wohl an der Zeit,", "tokens": ["Ja", ",", "es", "w\u00e4\u00b7re", "nun", "wohl", "an", "der", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Alle diese Thorheit abzuthun,", "tokens": ["Al\u00b7le", "die\u00b7se", "Thor\u00b7heit", "ab\u00b7zu\u00b7thun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Diese Blumennarrheit, diesen Aberglauben,", "tokens": ["Die\u00b7se", "Blu\u00b7men\u00b7nar\u00b7rheit", ",", "die\u00b7sen", "A\u00b7berg\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Dies Wallfahrten, Beten auf den Gr\u00e4bern,", "tokens": ["Dies", "Wall\u00b7fahr\u00b7ten", ",", "Be\u00b7ten", "auf", "den", "Gr\u00e4\u00b7bern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Sollte die Regierung hemmen. \u2013", "tokens": ["Soll\u00b7te", "die", "Re\u00b7gie\u00b7rung", "hem\u00b7men", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schmerzte mich der Arme fast noch mehr", "tokens": ["Schmerz\u00b7te", "mich", "der", "Ar\u00b7me", "fast", "noch", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Als die Schreiberin des alten Blattes:", "tokens": ["Als", "die", "Schrei\u00b7be\u00b7rin", "des", "al\u00b7ten", "Blat\u00b7tes", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Also hier auch, unter diesen Gottesbergen,", "tokens": ["Al\u00b7so", "hier", "auch", ",", "un\u00b7ter", "die\u00b7sen", "Got\u00b7tes\u00b7ber\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Wo Natur so heilge Worte rauscht,", "tokens": ["Wo", "Na\u00b7tur", "so", "heil\u00b7ge", "Wor\u00b7te", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Giebt's derlei vern\u00fcnftig Wesen,", "tokens": ["Giebt's", "der\u00b7lei", "ver\u00b7n\u00fcnf\u00b7tig", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das, so w\u00e4hnt' ich, nur daheim bei mir,", "tokens": ["Das", ",", "so", "w\u00e4hnt'", "ich", ",", "nur", "da\u00b7heim", "bei", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADV", "VVFIN", "PPER", "$,", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Auf im Sande schie\u00dft und unter