rd", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PIAT", "KOKOM", "APPR", "ART", "NN"], "meter": "+-+-+++-+", "measure": "unknown.measure.hexa"}, "line.25": {"text": "Sind wir alle Br\u00fcder.", "tokens": ["Sind", "wir", "al\u00b7le", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Aber hier in der Halle,", "tokens": ["A\u00b7ber", "hier", "in", "der", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Im fernen, unbesuchten Winkel", "tokens": ["Im", "fer\u00b7nen", ",", "un\u00b7be\u00b7such\u00b7ten", "Win\u00b7kel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Find ich ein Blatt, von alter Hand beschrieben,", "tokens": ["Find", "ich", "ein", "Blatt", ",", "von", "al\u00b7ter", "Hand", "be\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So deutet die zitternde, ungewisse Schrift:", "tokens": ["So", "deu\u00b7tet", "die", "zit\u00b7tern\u00b7de", ",", "un\u00b7ge\u00b7wis\u00b7se", "Schrift", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "\u00bbjeder Christ, der hier mag wandeln,", "tokens": ["\u00bb", "je\u00b7der", "Christ", ",", "der", "hier", "mag", "wan\u00b7deln", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "$,", "PRELS", "ADV", "VMFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bete freundlich f\u00fcr ein Wesen,", "tokens": ["Be\u00b7te", "freund\u00b7lich", "f\u00fcr", "ein", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das im unnennbaren Jammer,", "tokens": ["Das", "im", "un\u00b7nenn\u00b7ba\u00b7ren", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das im tiefsten Schmerz vergeht,", "tokens": ["Das", "im", "tiefs\u00b7ten", "Schmerz", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Zu dem Vater, der die Liebe,", "tokens": ["Zu", "dem", "Va\u00b7ter", ",", "der", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df er tr\u00f6ste, wenn nicht helfe.\u00ab \u2013", "tokens": ["Da\u00df", "er", "tr\u00f6s\u00b7te", ",", "wenn", "nicht", "hel\u00b7fe", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PTKNEG", "VVFIN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Da brachen unaufhaltsam meine Thr\u00e4nen,", "tokens": ["Da", "bra\u00b7chen", "un\u00b7auf\u00b7halt\u00b7sam", "mei\u00b7ne", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sie beteten mit Inbrunst.", "tokens": ["Und", "sie", "be\u00b7te\u00b7ten", "mit", "In\u00b7brunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Kommt ein Herr dahergegangen,", "tokens": ["Kommt", "ein", "Herr", "da\u00b7her\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht das Blatt, die Kreutz' und Blumen,", "tokens": ["Sieht", "das", "Blatt", ",", "die", "Kreutz'", "und", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Kinder, Eltern, Gatten,", "tokens": ["Und", "die", "Kin\u00b7der", ",", "El\u00b7tern", ",", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4lt wohl meinen Schmerz f\u00fcr Ingrimm,", "tokens": ["H\u00e4lt", "wohl", "mei\u00b7nen", "Schmerz", "f\u00fcr", "In\u00b7grimm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPR", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Spricht mit Afterweisheit;", "tokens": ["Spricht", "mit", "Af\u00b7ter\u00b7weis\u00b7heit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ja, es w\u00e4re nun wohl an der Zeit,", "tokens": ["Ja", ",", "es", "w\u00e4\u00b7re", "nun", "wohl", "an", "der", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Alle diese Thorheit abzuthun,", "tokens": ["Al\u00b7le", "die\u00b7se", "Thor\u00b7heit", "ab\u00b7zu\u00b7thun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Diese Blumennarrheit, diesen Aberglauben,", "tokens": ["Die\u00b7se", "Blu\u00b7men\u00b7nar\u00b7rheit", ",", "die\u00b7sen", "A\u00b7berg\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Dies Wallfahrten, Beten auf den Gr\u00e4bern,", "tokens": ["Dies", "Wall\u00b7fahr\u00b7ten", ",", "Be\u00b7ten", "auf", "den", "Gr\u00e4\u00b7bern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Sollte die Regierung hemmen. \u2013", "tokens": ["Soll\u00b7te", "die", "Re\u00b7gie\u00b7rung", "hem\u00b7men", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Schmerzte mich der Arme fast noch mehr", "tokens": ["Schmerz\u00b7te", "mich", "der", "Ar\u00b7me", "fast", "noch", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Als die Schreiberin des alten Blattes:", "tokens": ["Als", "die", "Schrei\u00b7be\u00b7rin", "des", "al\u00b7ten", "Blat\u00b7tes", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Also hier auch, unter diesen Gottesbergen,", "tokens": ["Al\u00b7so", "hier", "auch", ",", "un\u00b7ter", "die\u00b7sen", "Got\u00b7tes\u00b7ber\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Wo Natur so heilge Worte rauscht,", "tokens": ["Wo", "Na\u00b7tur", "so", "heil\u00b7ge", "Wor\u00b7te", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Giebt's derlei vern\u00fcnftig Wesen,", "tokens": ["Giebt's", "der\u00b7lei", "ver\u00b7n\u00fcnf\u00b7tig", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das, so w\u00e4hnt' ich, nur daheim bei mir,", "tokens": ["Das", ",", "so", "w\u00e4hnt'", "ich", ",", "nur", "da\u00b7heim", "bei", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADV", "VVFIN", "PPER", "$,", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Auf im Sande schie\u00dft und unter Kiefern.", "tokens": ["Auf", "im", "San\u00b7de", "schie\u00dft", "und", "un\u00b7ter", "Kie\u00b7fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "VVFIN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}