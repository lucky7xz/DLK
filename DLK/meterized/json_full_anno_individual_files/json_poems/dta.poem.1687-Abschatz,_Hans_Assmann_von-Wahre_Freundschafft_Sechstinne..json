{"dta.poem.1687": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Wahre Freundschafft/ Sechstinne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist das h\u00f6chste Gutt auff diesem Erdenkrei\u00df? ", "tokens": ["Was", "ist", "das", "h\u00f6chs\u00b7te", "Gutt", "auff", "die\u00b7sem", "Er\u00b7den\u00b7krei\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es halten ihrer viel davor den theuren Koth", "tokens": ["Es", "hal\u00b7ten", "ih\u00b7rer", "viel", "da\u00b7vor", "den", "theu\u00b7ren", "Koth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADV", "PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Erde gelbes Marck/ das man mit strenger Hand", "tokens": ["Der", "Er\u00b7de", "gel\u00b7bes", "Marck", "/", "das", "man", "mit", "stren\u00b7ger", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "PRELS", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus fest-verschlo\u00dfner Schos der gro\u00dfen Mutter reist/", "tokens": ["Aus", "fest\u00b7ver\u00b7schlo\u00df\u00b7ner", "Schos", "der", "gro\u00b7\u00dfen", "Mut\u00b7ter", "reist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie setzen gegen Gold die Seele selber auff/", "tokens": ["Sie", "set\u00b7zen", "ge\u00b7gen", "Gold", "die", "See\u00b7le", "sel\u00b7ber", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "ADV", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Im Fall sie lachet an ein Nutzen und Gewinn.", "tokens": ["Im", "Fall", "sie", "la\u00b7chet", "an", "ein", "Nut\u00b7zen", "und", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was ist auff dieser Welt der edelste Gewinn?", "tokens": ["Was", "ist", "auff", "die\u00b7ser", "Welt", "der", "e\u00b7dels\u00b7te", "Ge\u00b7winn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PDAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel sch\u00e4tzen sich vor reich/ im Fall der Riegelkrei\u00df", "tokens": ["Viel", "sch\u00e4t\u00b7zen", "sich", "vor", "reich", "/", "im", "Fall", "der", "Rie\u00b7gel\u00b7krei\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADJD", "$(", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erdienter Herren-Gunst gehoben aus dem Koth", "tokens": ["Er\u00b7dien\u00b7ter", "Her\u00b7ren\u00b7Gunst", "ge\u00b7ho\u00b7ben", "aus", "dem", "Koth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr vor verachtes Haubt/ im Fall die Gnaden-Hand", "tokens": ["Ihr", "vor", "ver\u00b7ach\u00b7tes", "Haubt", "/", "im", "Fall", "die", "Gna\u00b7den\u00b7Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN", "$(", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des F\u00fcrsten sie erh\u00f6ht und aus dem Staube reist/", "tokens": ["Des", "F\u00fcrs\u00b7ten", "sie", "er\u00b7h\u00f6ht", "und", "aus", "dem", "Stau\u00b7be", "reist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "KON", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So bl\u00e4set sich ihr Sinn aus allen Kr\u00e4fften auff.", "tokens": ["So", "bl\u00e4\u00b7set", "sich", "ihr", "Sinn", "aus", "al\u00b7len", "Kr\u00e4ff\u00b7ten", "auff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was hebet man mit Recht vor alle Gaben auff?", "tokens": ["Was", "he\u00b7bet", "man", "mit", "Recht", "vor", "al\u00b7le", "Ga\u00b7ben", "auff", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "APPR", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es nehmen ihrer viel den kurtzen Jahr-Gewinn/", "tokens": ["Es", "neh\u00b7men", "ih\u00b7rer", "viel", "den", "kurt\u00b7zen", "Jahr\u00b7Ge\u00b7winn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dadurch erweitert wird der enge Lebenskrei\u00df/", "tokens": ["Da\u00b7durch", "er\u00b7wei\u00b7tert", "wird", "der", "en\u00b7ge", "Le\u00b7bens\u00b7krei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor Ehre/ Geld und Gutt/ sie kleben an dem Koth", "tokens": ["Vor", "Eh\u00b7re", "/", "Geld", "und", "Gutt", "/", "sie", "kle\u00b7ben", "an", "dem", "Koth"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$(", "NN", "KON", "NN", "$(", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der irrdnen Sterbligkeit mit Hertze/ Mund und Hand/", "tokens": ["Der", "irrd\u00b7nen", "Ster\u00b7blig\u00b7keit", "mit", "Hert\u00b7ze", "/", "Mund", "und", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "VVFIN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bi\u00df sie zulezt davon ein traurig Ende reist.", "tokens": ["Bi\u00df", "sie", "zu\u00b7lezt", "da\u00b7von", "ein", "trau\u00b7rig", "En\u00b7de", "reist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PAV", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was ist darnach man sich mit bestem Rechte reist?", "tokens": ["Was", "ist", "dar\u00b7nach", "man", "sich", "mit", "bes\u00b7tem", "Rech\u00b7te", "reist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PAV", "PIS", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was nur den geilen Leib zur Freude muntert auff/", "tokens": ["Was", "nur", "den", "gei\u00b7len", "Leib", "zur", "Freu\u00b7de", "mun\u00b7tert", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Di\u00df acht der meiste Theil der Menschen vor Gewinn/", "tokens": ["Di\u00df", "acht", "der", "meis\u00b7te", "Theil", "der", "Men\u00b7schen", "vor", "Ge\u00b7winn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "CARD", "ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da doch so lange Zeit den rundten Jammerkrei\u00df", "tokens": ["Da", "doch", "so", "lan\u00b7ge", "Zeit", "den", "rund\u00b7ten", "Jam\u00b7mer\u00b7krei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In Ach und Weh bewohnt der schwache Bau von Koth/", "tokens": ["In", "Ach", "und", "Weh", "be\u00b7wohnt", "der", "schwa\u00b7che", "Bau", "von", "Koth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "ADJA", "NN", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verwechselt Leyd und Freud einander reicht die Hand.", "tokens": ["Ver\u00b7wech\u00b7selt", "Leyd", "und", "Freud", "ein\u00b7an\u00b7der", "reicht", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was ist zu nehmen an mit ausgestreckter Hand?", "tokens": ["Was", "ist", "zu", "neh\u00b7men", "an", "mit", "aus\u00b7ge\u00b7streck\u00b7ter", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKZU", "VVINF", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht stoltze Wissenschafft/ die von der Erde reist", "tokens": ["Nicht", "stolt\u00b7ze", "Wis\u00b7sen\u00b7schafft", "/", "die", "von", "der", "Er\u00b7de", "reist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJA", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Wind-gef\u00fcllten Sinn/ zu steigen Himmel auff/", "tokens": ["Den", "Win\u00b7dge\u00b7f\u00fcll\u00b7ten", "Sinn", "/", "zu", "stei\u00b7gen", "Him\u00b7mel", "auff", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "PTKZU", "VVINF", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die vielen zwar verheist der Ewigkeit Gewinn/", "tokens": ["Die", "vie\u00b7len", "zwar", "ver\u00b7heist", "der", "E\u00b7wig\u00b7keit", "Ge\u00b7winn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADV", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch wenn sich kehret um der Ehre falscher Krei\u00df/", "tokens": ["Doch", "wenn", "sich", "keh\u00b7ret", "um", "der", "Eh\u00b7re", "fal\u00b7scher", "Krei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wird durch der Feinde Grimm gest\u00fcrtzet in den Koth.", "tokens": ["Wird", "durch", "der", "Fein\u00b7de", "Grimm", "ge\u00b7st\u00fcrt\u00b7zet", "in", "den", "Koth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NE", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Geld/ Gnade/ Zeit und Lust und Wissen f\u00e4llt in Koth/", "tokens": ["Geld", "/", "Gna\u00b7de", "/", "Zeit", "und", "Lust", "und", "Wis\u00b7sen", "f\u00e4llt", "in", "Koth", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "KON", "NN", "VVFIN", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein treuer Jonathan des Freundes rechte Hand/", "tokens": ["Ein", "treu\u00b7er", "Jo\u00b7na\u00b7than", "des", "Freun\u00b7des", "rech\u00b7te", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der von dem Freunde sich in keinen N\u00f6then reist/", "tokens": ["Der", "von", "dem", "Freun\u00b7de", "sich", "in", "kei\u00b7nen", "N\u00f6\u00b7then", "reist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Gutt und Blutt vor ihn mit Freuden setzet auff/", "tokens": ["Der", "Gutt", "und", "Blutt", "vor", "ihn", "mit", "Freu\u00b7den", "set\u00b7zet", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "APPR", "PPER", "APPR", "NN", "VVFIN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist/ meinem Sinne nach/ der edelste Gewinn/", "tokens": ["Ist", "/", "mei\u00b7nem", "Sin\u00b7ne", "nach", "/", "der", "e\u00b7dels\u00b7te", "Ge\u00b7winn", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "PPOSAT", "NN", "APPR", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den uns gew\u00e4hren kan der Erde Kugelkrei\u00df.", "tokens": ["Den", "uns", "ge\u00b7w\u00e4h\u00b7ren", "kan", "der", "Er\u00b7de", "Ku\u00b7gel\u00b7krei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}