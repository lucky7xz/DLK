{"textgrid.poem.61304": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn von der Zeit der sinkenden C\u00e4saren", "tokens": ["Wenn", "von", "der", "Zeit", "der", "sin\u00b7ken\u00b7den", "C\u00e4\u00b7sa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich las, bevor die Stadt der Feinde Beute,", "tokens": ["Ich", "las", ",", "be\u00b7vor", "die", "Stadt", "der", "Fein\u00b7de", "Beu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Geist erw\u00e4gend, was die Welt erfreute,", "tokens": ["Im", "Geist", "er\u00b7w\u00e4\u00b7gend", ",", "was", "die", "Welt", "er\u00b7freu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und was die Welt verst\u00f6rt in jenen Jahren:", "tokens": ["Und", "was", "die", "Welt", "ver\u00b7st\u00f6rt", "in", "je\u00b7nen", "Jah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So hat's mich oft wie j\u00e4her Schreck durchfahren;", "tokens": ["So", "hat's", "mich", "oft", "wie", "j\u00e4\u00b7her", "Schreck", "durch\u00b7fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KOKOM", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir war's, als ob ein Spiegelbild des Heute", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "ob", "ein", "Spie\u00b7gel\u00b7bild", "des", "Heu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Aus der Geschichte mir entgegendr\u00e4ute", "tokens": ["Aus", "der", "Ge\u00b7schich\u00b7te", "mir", "ent\u00b7ge\u00b7gen\u00b7dr\u00e4u\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und spr\u00e4ch': Ihr seid, was jene R\u00f6mer waren.", "tokens": ["Und", "spr\u00e4ch'", ":", "Ihr", "seid", ",", "was", "je\u00b7ne", "R\u00f6\u00b7mer", "wa\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "$,", "PWS", "PDAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So lag bei hohlem Wort die Zucht im Staube,", "tokens": ["So", "lag", "bei", "hoh\u00b7lem", "Wort", "die", "Zucht", "im", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So ward der Seelen gottverla\u00dfnes Bangen", "tokens": ["So", "ward", "der", "See\u00b7len", "gott\u00b7ver\u00b7la\u00df\u00b7nes", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Heut frecher Taumel, morgen Aberglaube.", "tokens": ["Heut", "fre\u00b7cher", "Tau\u00b7mel", ",", "mor\u00b7gen", "A\u00b7berg\u00b7lau\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So hielt der Schein jedwedes Sein gefangen,", "tokens": ["So", "hielt", "der", "Schein", "jed\u00b7we\u00b7des", "Sein", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Indes vom Nord her, schon bereit zum Raube,", "tokens": ["In\u00b7des", "vom", "Nord", "her", ",", "schon", "be\u00b7reit", "zum", "Rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKVZ", "$,", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Barbarenst\u00e4mme dumpfen Schlachtruf sangen.", "tokens": ["Bar\u00b7ba\u00b7ren\u00b7st\u00e4m\u00b7me", "dum\u00b7pfen", "Schlach\u00b7truf", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn von der Zeit der sinkenden C\u00e4saren", "tokens": ["Wenn", "von", "der", "Zeit", "der", "sin\u00b7ken\u00b7den", "C\u00e4\u00b7sa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure":