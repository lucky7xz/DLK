{"dta.poem.16053": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "107.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist der Widerspruch, den Widerspr\u00fcche loben,", "tokens": ["Du", "bist", "der", "Wi\u00b7der\u00b7spruch", ",", "den", "Wi\u00b7der\u00b7spr\u00fc\u00b7che", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jeder Widerspruch ist in dir aufgehoben.", "tokens": ["Und", "je\u00b7der", "Wi\u00b7der\u00b7spruch", "ist", "in", "dir", "auf\u00b7ge\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Widerspr\u00fcch', in die sich die Vernunft verstrickt,", "tokens": ["Die", "Wi\u00b7der\u00b7spr\u00fcch'", ",", "in", "die", "sich", "die", "Ver\u00b7nunft", "ver\u00b7strickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zergehn, und sie zergeht, wo dich der Geist erblickt.", "tokens": ["Zer\u00b7gehn", ",", "und", "sie", "zer\u00b7geht", ",", "wo", "dich", "der", "Geist", "er\u00b7blickt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "PPER", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Welt ist nicht in dir, und du bist nicht in ihr;", "tokens": ["Die", "Welt", "ist", "nicht", "in", "dir", ",", "und", "du", "bist", "nicht", "in", "ihr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "APPR", "PPER", "$,", "KON", "PPER", "VAFIN", "PTKNEG", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur du bist in der Welt, die Welt ist nur in dir.", "tokens": ["Nur", "du", "bist", "in", "der", "Welt", ",", "die", "Welt", "ist", "nur", "in", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "APPR", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}