{"textgrid.poem.38290": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Gru\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir ist ein roth Gold Ringelein", "tokens": ["Mir", "ist", "ein", "roth", "Gold", "Rin\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf meinen Fu\u00df gefallen;", "tokens": ["Auf", "mei\u00b7nen", "Fu\u00df", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So darf ichs doch nicht heben auf,", "tokens": ["So", "darf", "ichs", "doch", "nicht", "he\u00b7ben", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "PTKNEG", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Leut, die sehens alle.", "tokens": ["Die", "Leut", ",", "die", "se\u00b7hens", "al\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit Lust tret ich an diesen Tanz,", "tokens": ["Mit", "Lust", "tret", "ich", "an", "die\u00b7sen", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hoff, mir wird ein sch\u00f6ner Kranz", "tokens": ["Ich", "hoff", ",", "mir", "wird", "ein", "sch\u00f6\u00b7ner", "Kranz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von einem sch\u00f6n Jungfr\u00e4uelein,", "tokens": ["Von", "ei\u00b7nem", "sch\u00f6n", "Jung\u00b7fr\u00e4u\u00b7e\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darum will ich ihr eigen seyn.", "tokens": ["Da\u00b7rum", "will", "ich", "ihr", "ei\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So tret ich hin auf einen Stein,", "tokens": ["So", "tret", "ich", "hin", "auf", "ei\u00b7nen", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gott gr\u00fc\u00df dich zart Jungfr\u00e4uelein;", "tokens": ["Gott", "gr\u00fc\u00df", "dich", "zart", "Jung\u00b7fr\u00e4u\u00b7e\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und gr\u00fc\u00df euch Gott allsammt gleich,", "tokens": ["Und", "gr\u00fc\u00df", "euch", "Gott", "all\u00b7sammt", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie seyn arm oder reich.", "tokens": ["Sie", "seyn", "arm", "o\u00b7der", "reich", "."], "token_info": ["word", "word", "word", "word", "word", 