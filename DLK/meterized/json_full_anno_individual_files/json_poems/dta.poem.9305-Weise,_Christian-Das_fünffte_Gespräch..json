{"dta.poem.9305": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das f\u00fcnffte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.57", "sv:0.42"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Seht doch den jungen sack", "tokens": ["Seht", "doch", "den", "jun\u00b7gen", "sack"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dort in dem fenster sitzen/", "tokens": ["Dort", "in", "dem", "fens\u00b7ter", "sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie hurtig und wie strack", "tokens": ["Wie", "hur\u00b7tig", "und", "wie", "strack"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "PWAV", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Kan sie das m\u00e4ulgen spitzen.", "tokens": ["Kan", "sie", "das", "m\u00e4ul\u00b7gen", "spit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie war vor wenig tagen", "tokens": ["Sie", "war", "vor", "we\u00b7nig", "ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Noch weit davon entfernt:", "tokens": ["Noch", "weit", "da\u00b7von", "ent\u00b7fernt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PAV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch itzo mu\u00df ich sagen/", "tokens": ["Doch", "it\u00b7zo", "mu\u00df", "ich", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie hat schon ansgelernt.", "tokens": ["Sie", "hat", "schon", "ans\u00b7ge\u00b7lernt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "2. Der liebste sitzt bey ihr", "tokens": ["Der", "liebs\u00b7te", "sitzt", "bey", "ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und l\u00f6ffelt auf die thauer:", "tokens": ["Und", "l\u00f6f\u00b7felt", "auf", "die", "thau\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie f\u00fcrcht sich nicht daf\u00fcr/", "tokens": ["Sie", "f\u00fcrcht", "sich", "nicht", "da\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "PAV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und h\u00e4lt wie eine mauer.", "tokens": ["Und", "h\u00e4lt", "wie", "ei\u00b7ne", "mau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo lernens denn die kinder?", "tokens": ["Wo", "ler\u00b7nens", "denn", "die", "kin\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist irgend so ein buch?", "tokens": ["Ist", "ir\u00b7gend", "so", "ein", "buch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie kans f\u00fcrwar geschwinder", "tokens": ["Sie", "kans", "f\u00fcr\u00b7war", "ge\u00b7schwin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als sonst den sontag-spruch.", "tokens": ["Als", "sonst", "den", "son\u00b7tag\u00b7spruch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "3. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Da\u00df dich mein leibgen dr\u00fccke/", "tokens": ["Da\u00df", "dich", "mein", "leib\u00b7gen", "dr\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie bringstu so geschwind", "tokens": ["Wie", "brings\u00b7tu", "so", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das leder ins geschicke?", "tokens": ["Das", "le\u00b7der", "ins", "ge\u00b7schi\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es geht so fix/ ich halte", "tokens": ["Es", "geht", "so", "fix", "/", "ich", "hal\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du hast/ wie sichs geb\u00fchrt/", "tokens": ["Du", "hast", "/", "wie", "sichs", "ge\u00b7b\u00fchrt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PWAV", "PIS", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das m\u00e4ulgen in die falte", "tokens": ["Das", "m\u00e4ul\u00b7gen", "in", "die", "fal\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit baum-\u00f6l eingeschmiert.", "tokens": ["Mit", "baum\u00b7\u00f6l", "ein\u00b7ge\u00b7schmiert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "4. Ich hab es tausendmahl", "tokens": ["Ich", "hab", "es", "tau\u00b7send\u00b7mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vor diesem f\u00fcrgenommen:", "tokens": ["Vor", "die\u00b7sem", "f\u00fcr\u00b7ge\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch bin ich allemahl", "tokens": ["Doch", "bin", "ich", "al\u00b7le\u00b7mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In das gedr\u00e4nge kommen.", "tokens": ["In", "das", "ge\u00b7dr\u00e4n\u00b7ge", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich armer st\u00fcmper dachte/", "tokens": ["Ich", "ar\u00b7mer", "st\u00fcm\u00b7per", "dach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie h\u00e4tte keine lust:", "tokens": ["Sie", "h\u00e4t\u00b7te", "kei\u00b7ne", "lust", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und sie hat still und sachte", "tokens": ["Und", "sie", "hat", "still", "und", "sach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die h\u00e4ndel schon gewust.", "tokens": ["Die", "h\u00e4n\u00b7del", "schon", "ge\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "5. Bald stie\u00df sie sich an kopff/", "tokens": ["Bald", "stie\u00df", "sie", "sich", "an", "kopff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NE", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da wars mit mir verlohren:", "tokens": ["Da", "wars", "mit", "mir", "ver\u00b7loh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bald fand ich armer tropff", "tokens": ["Bald", "fand", "ich", "ar\u00b7mer", "tropff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das m\u00e4ulgen hintern ohren:", "tokens": ["Das", "m\u00e4ul\u00b7gen", "hin\u00b7tern", "oh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da hielte sie die h\u00e4nde", "tokens": ["Da", "hiel\u00b7te", "sie", "die", "h\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bald gar die m\u00fctze f\u00fcr/", "tokens": ["Bald", "gar", "die", "m\u00fct\u00b7ze", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und allzeit an dem ende", "tokens": ["Und", "all\u00b7zeit", "an", "dem", "en\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da hatt ich nichts von ihr.", "tokens": ["Da", "hatt", "ich", "nichts", "von", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "APPR", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "6. Bi\u00dfweilen bat sie mich", "tokens": ["Bi\u00df\u00b7wei\u00b7len", "bat", "sie", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Um hundert Gottes willen;", "tokens": ["Um", "hun\u00b7dert", "Got\u00b7tes", "wil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und bald erz\u00fcrnt sie sich/", "tokens": ["Und", "bald", "er\u00b7z\u00fcrnt", "sie", "sich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da krieg ich andre pillen:", "tokens": ["Da", "krieg", "ich", "and\u00b7re", "pil\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ja in den besten freuden", "tokens": ["Ja", "in", "den", "bes\u00b7ten", "freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da h\u00f6rt die mutter schreyn/", "tokens": ["Da", "h\u00f6rt", "die", "mut\u00b7ter", "schreyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und halff die h\u00e4ndel scheiden/", "tokens": ["Und", "halff", "die", "h\u00e4n\u00b7del", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich musts gewesen seyn.", "tokens": ["Ich", "musts", "ge\u00b7we\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VAPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "7. Je schrey doch itzund auch/", "tokens": ["Je", "schrey", "doch", "it\u00b7zund", "auch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADV", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der liebste spitzt die lippen/", "tokens": ["Der", "liebs\u00b7te", "spitzt", "die", "lip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo ist nun dein gebrauch?", "tokens": ["Wo", "ist", "nun", "dein", "ge\u00b7brauch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Soll er denn immer tippen?", "tokens": ["Soll", "er", "denn", "im\u00b7mer", "tip\u00b7pen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das zucker-s\u00fcsse futter", "tokens": ["Das", "zu\u00b7cke\u00b7rs\u00fcs\u00b7se", "fut\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Genenst er gar zu viel/", "tokens": ["Ge\u00b7nenst", "er", "gar", "zu", "viel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKA", "PIS", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und ich seh keine mutter", "tokens": ["Und", "ich", "seh", "kei\u00b7ne", "mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die friede nehmen will.", "tokens": ["Die", "frie\u00b7de", "neh\u00b7men", "will", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "8. Die m\u00e4dgen haben nu", "tokens": ["Die", "m\u00e4d\u00b7gen", "ha\u00b7ben", "nu"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "VAFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gar den credit verlohren/", "tokens": ["Gar", "den", "cre\u00b7dit", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und dieses sey darzu", "tokens": ["Und", "die\u00b7ses", "sey", "dar\u00b7zu"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In specie geschworen/", "tokens": ["In", "spe\u00b7cie", "ge\u00b7schwo\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$("], "meter": "+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Wo ich ein m\u00e4dgen finde/", "tokens": ["Wo", "ich", "ein", "m\u00e4d\u00b7gen", "fin\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da will ich lose seyn:", "tokens": ["Da", "will", "ich", "lo\u00b7se", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie f\u00fcrchten sich der s\u00fcnde", "tokens": ["Sie", "f\u00fcrch\u00b7ten", "sich", "der", "s\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nur auff den blossen schein.", "tokens": ["Nur", "auff", "den", "blos\u00b7sen", "schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}