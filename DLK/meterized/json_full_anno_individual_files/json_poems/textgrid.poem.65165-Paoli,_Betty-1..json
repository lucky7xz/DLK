{"textgrid.poem.65165": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allm\u00e4chtig lebt und webt der Geist,", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "lebt", "und", "webt", "der", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "De\u00df Hauch, ein g\u00f6ttlich Offenbaren,", "tokens": ["De\u00df", "Hauch", ",", "ein", "g\u00f6tt\u00b7lich", "Of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Schmerzen, K\u00e4mpfe und Gefahren", "tokens": ["Durch", "Schmer\u00b7zen", ",", "K\u00e4mp\u00b7fe", "und", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Menschheit vorw\u00e4rts schreiten hei\u00dft,", "tokens": ["Die", "Menschheit", "vor\u00b7w\u00e4rts", "schrei\u00b7ten", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Gleichviel, auf welchem Schreckenspfad", "tokens": ["Gleich\u00b7viel", ",", "auf", "wel\u00b7chem", "Schre\u00b7cken\u00b7spfad"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie ihrem lichten Ziele naht!", "tokens": ["Sie", "ih\u