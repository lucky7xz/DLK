ct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wert im Gesange fortzuleben", "tokens": ["Wert", "im", "Ge\u00b7san\u00b7ge", "fort\u00b7zu\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Ist die Begeist'rung, die euch trieb!", "tokens": ["Ist", "die", "Be\u00b7geist'\u00b7rung", ",", "die", "euch", "trieb", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Erliegend truget euer Teil", "tokens": ["Er\u00b7lie\u00b7gend", "tru\u00b7get", "eu\u00b7er", "Teil"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ihr bei zum allgemeinen Heil!", "tokens": ["Ihr", "bei", "zum", "all\u00b7ge\u00b7mei\u00b7nen", "Heil", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Denn: will ein ewiges Gebot,", "tokens": ["Denn", ":", "will", "ein", "e\u00b7wi\u00b7ges", "Ge\u00b7bot", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Da\u00df rastlos sich die Welt erneue,", "tokens": ["Da\u00df", "rast\u00b7los", "sich", "die", "Welt", "er\u00b7neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Das Beispiel heldenm\u00