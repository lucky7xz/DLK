{"textgrid.poem.33846": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Vorw\u00e4rts", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steine, sie liegen hier,", "tokens": ["Stei\u00b7ne", ",", "sie", "lie\u00b7gen", "hier", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Liebchen, im Wege dir,", "tokens": ["Lieb\u00b7chen", ",", "im", "We\u00b7ge", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Klotzig herum!", "tokens": ["Klot\u00b7zig", "he\u00b7rum", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Gerne ja b\u00fcckt' ich mich,", "tokens": ["Ger\u00b7ne", "ja", "b\u00fcckt'", "ich", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Schaffte sie fort f\u00fcr dich,", "tokens": ["Schaff\u00b7te", "sie", "fort", "f\u00fcr", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "W\u00fcrd' ich blo\u00df krumm;", "tokens": ["W\u00fcrd'", "ich", "blo\u00df", "krumm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Aber, ich seh's genau,", "tokens": ["A\u00b7ber", ",", "ich", "seh's