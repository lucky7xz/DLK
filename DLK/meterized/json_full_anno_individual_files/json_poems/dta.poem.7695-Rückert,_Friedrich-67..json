{"dta.poem.7695": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "67.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer immer kommt zur Welt, verbraucht von ihr ein St\u00fcck,", "tokens": ["Wer", "im\u00b7mer", "kommt", "zur", "Welt", ",", "ver\u00b7braucht", "von", "ihr", "ein", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und doch wird sie davon nie minder, welch ein Gl\u00fcck.", "tokens": ["Und", "doch", "wird", "sie", "da\u00b7von", "nie", "min\u00b7der", ",", "welch", "ein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PAV", "ADV", "ADV", "$,", "PWAT", "ART", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Warum wird sie davon nie minder? weil, wer auch", "tokens": ["Wa\u00b7rum", "wird", "sie", "da\u00b7von", "nie", "min\u00b7der", "?", "weil", ",", "wer", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PAV", "ADV", "ADV", "$.", "KOUS", "$,", "PWS", "ADV"], "meter": "-----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Sie mag verbrauchen, ihr dient wieder zum Verbrauch.", "tokens": ["Sie", "mag", "ver\u00b7brau\u00b7chen", ",", "ihr", "dient", "wie\u00b7der", "zum", "Ver\u00b7brauch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos":