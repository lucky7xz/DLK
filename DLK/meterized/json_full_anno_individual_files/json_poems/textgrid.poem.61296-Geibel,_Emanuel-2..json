{"textgrid.poem.61296": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil meine Muse nicht den wilden Trieben", "tokens": ["Weil", "mei\u00b7ne", "Mu\u00b7se", "nicht", "den", "wil\u00b7den", "Trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Menge fr\u00f6nt in diesen wirren Tagen,", "tokens": ["Der", "Men\u00b7ge", "fr\u00f6nt", "in", "die\u00b7sen", "wir\u00b7ren", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So hat sie fr\u00fch gelernt dem Ruhm entsagen", "tokens": ["So", "hat", "sie", "fr\u00fch", "ge\u00b7lernt", "dem", "Ruhm", "ent\u00b7sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ist in ihrer Stille gern geblieben.", "tokens": ["Und", "ist", "in", "ih\u00b7rer", "Stil\u00b7le", "gern", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn nicht verwechseln l\u00e4\u00dft sich's nach Belieben,", "tokens": ["Denn", "nicht", "ver\u00b7wech\u00b7seln", "l\u00e4\u00dft", "sich's", "nach", "Be\u00b7lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVINF", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wof\u00fcr begeistert eine Brust geschlagen;", "tokens": ["Wo\u00b7f\u00fcr", "be\u00b7geis\u00b7tert", "ei\u00b7ne", "Brust", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und was ein Gott mich lehrt' im Herzen tragen,", "tokens": ["Und", "was", "ein", "Gott", "mich", "lehrt'", "im", "Her\u00b7zen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "PPER", "VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das kann mit meinem Herzen nur zerstieben.", "tokens": ["Das", "kann", "mit", "mei\u00b7nem", "Her\u00b7zen", "nur", "zer\u00b7stie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Behagt mein Lied euch recht, so la\u00dft mich gehen", "tokens": ["Be\u00b7hagt", "mein", "Lied", "euch", "recht", ",", "so", "la\u00dft", "mich", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "ADJD", "$,", "ADV", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und horcht den Weisen andrer, die geschwinde", "tokens": ["Und", "horcht", "den", "Wei\u00b7sen", "an\u00b7drer", ",", "die", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", 