{"textgrid.poem.39749": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Stimmt an den Sang, die gr\u00fcnen Wogen lauschen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stimmt an den Sang, die gr\u00fcnen Wogen lauschen", "tokens": ["Stimmt", "an", "den", "Sang", ",", "die", "gr\u00fc\u00b7nen", "Wo\u00b7gen", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Im alten Schwabenmeer,", "tokens": ["Im", "al\u00b7ten", "Schwa\u00b7ben\u00b7meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sobald ihr singt, beginnen sie zu rauschen", "tokens": ["So\u00b7bald", "ihr", "singt", ",", "be\u00b7gin\u00b7nen", "sie", "zu", "rau\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und h\u00fcpfen um euch her.", "tokens": ["Und", "h\u00fcp\u00b7fen", "um", "euch", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und sie durchstr\u00f6mt der Geist der fernen Zeiten,", "tokens": ["Und", "sie", "durch\u00b7str\u00f6mt", "der", "Geist", "der", "fer\u00b7nen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo rings der Strand erklang,", "tokens": ["Wo", "rings", "der", "Strand", "er\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Minne Lied zum Silberton der Saiten", "tokens": ["Der", "Min\u00b7ne", "Lied", "zum", "Sil\u00b7ber\u00b7ton", "der", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus hundert Burgen drang.", "tokens": ["Aus", "hun\u00b7dert", "Bur\u00b7gen", "drang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das Land ist stumm, das Ufer unbesungen,", "tokens": ["Das", "Land", "ist", "stumm", ",", "das", "U\u00b7fer", "un\u00b7be\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Versunken ist die Lust \u2013", "tokens": ["Ver\u00b7sun\u00b7ken", "ist", "die", "Lust", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch aus den Wassern hat sie sich geschwungen", "tokens": ["Doch", "aus", "den", "Was\u00b7sern", "hat", "sie", "sich", "ge\u00b7schwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und lebt in unsrer Brust.", "tokens": ["Und", "lebt", "in", "uns\u00b7rer", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Im leichten Haus, das auf der Woge schwimmet,", "tokens": ["Im", "leich\u00b7ten", "Haus", ",", "das", "auf", "der", "Wo\u00b7ge", "schwim\u00b7met", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da wohnt der leichte Mut,", "tokens": ["Da", "wohnt", "der", "leich\u00b7te", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wiegt sich jede Freude gro\u00df, da glimmet", "tokens": ["Da", "wiegt", "sich", "je\u00b7de", "Freu\u00b7de", "gro\u00df", ",", "da", "glim\u00b7met"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "NN", "ADJD", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch jeder Hoffnung Glut.", "tokens": ["Noch", "je\u00b7der", "Hoff\u00b7nung", "Glut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Ruderschlag verst\u00e4rkt den Schlag der Herzen,", "tokens": ["Der", "Ru\u00b7der\u00b7schlag", "ver\u00b7st\u00e4rkt", "den", "Schlag", "der", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Freundschaft und Lieb' erwacht;", "tokens": ["Freund\u00b7schaft", "und", "Lieb'", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "O blickt umher, wie k\u00fchn die Wellen scherzen,", "tokens": ["O", "blickt", "um\u00b7her", ",", "wie", "k\u00fchn", "die", "Wel\u00b7len", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "PWAV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum scherzt auch ihr und lacht!", "tokens": ["Drum", "scherzt", "auch", "ihr", "und", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der frohe Sto\u00df, der unsern Nachen treibet,", "tokens": ["Der", "fro\u00b7he", "Sto\u00df", ",", "der", "un\u00b7sern", "Na\u00b7chen", "trei\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er geht durch Berg und Thal,", "tokens": ["Er", "geht", "durch", "Berg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie fliegen hin, die Ruhe thront und bleibet", "tokens": ["Sie", "flie\u00b7gen", "hin", ",", "die", "Ru\u00b7he", "thront", "und", "blei\u00b7bet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur in des Aethers Saal.", "tokens": ["Nur", "in", "des", "A\u00b7e\u00b7thers", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Und heller gl\u00e4nzet im Vor\u00fcberschweben", "tokens": ["Und", "hel\u00b7ler", "gl\u00e4n\u00b7zet", "im", "Vor\u00b7\u00fc\u00b7bersc\u00b7hwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Thurm von Dorf und Stadt,", "tokens": ["Der", "Thurm", "von", "Dorf", "und", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Firnen gl\u00fchn, die niedern H\u00fcgel beben", "tokens": ["Die", "Fir\u00b7nen", "gl\u00fchn", ",", "die", "nie\u00b7dern", "H\u00fc\u00b7gel", "be\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Umwallt von Bl\u00fct' und Blatt.", "tokens": ["Um\u00b7wallt", "von", "Bl\u00fct'", "und", "Blatt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dort am Gestade schwingen sich die Reben \u2013", "tokens": ["Dort", "am", "Ge\u00b7sta\u00b7de", "schwin\u00b7gen", "sich", "die", "Re\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So sagt, wo habt ihr Wein?", "tokens": ["So", "sagt", ",", "wo", "habt", "ihr", "Wein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Doppelstrom durchschwimmen wir das Leben,", "tokens": ["Im", "Dop\u00b7pel\u00b7strom", "durch\u00b7schwim\u00b7men", "wir", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schenkt ein, schenkt ein, schenkt ein!", "tokens": ["Schenkt", "ein", ",", "schenkt", "ein", ",", "schenkt", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Die Wonne wacht und alle Sorgen schlafen:", "tokens": ["Die", "Won\u00b7ne", "wacht", "und", "al\u00b7le", "Sor\u00b7gen", "schla\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch ist des Gl\u00fccks zuviel;", "tokens": ["Doch", "ist", "des", "Gl\u00fccks", "zu\u00b7viel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonne sinkt, es \u00f6ffnet sich der Hafen,", "tokens": ["Die", "Son\u00b7ne", "sinkt", ",", "es", "\u00f6ff\u00b7net", "sich", "der", "Ha\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach, schon sind wir am Ziel!", "tokens": ["Ach", ",", "schon", "sind", "wir", "am", "Ziel", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Doch tragen wir die Lust des Elementes", "tokens": ["Doch", "tra\u00b7gen", "wir", "die", "Lust", "des", "E\u00b7le\u00b7men\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hinaus in Stadt und Land,", "tokens": ["Hin\u00b7aus", "in", "Stadt", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verbunden stets, denn das ist kein Getrenntes,", "tokens": ["Ver\u00b7bun\u00b7den", "stets", ",", "denn", "das", "ist", "kein", "Ge\u00b7trenn\u00b7tes", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "PDS", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was Lieb' und Lust verband!", "tokens": ["Was", "Lieb'", "und", "Lust", "ver\u00b7band", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Im Herzen lebt, von Sonnenschein umflossen,", "tokens": ["Im", "Her\u00b7zen", "lebt", ",", "von", "Son\u00b7nen\u00b7schein", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der treuen Freunde Bild,", "tokens": ["Der", "treu\u00b7en", "Freun\u00b7de", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die blaue Flut wallt ewig drum ergossen,", "tokens": ["Die", "blau\u00b7e", "Flut", "wallt", "e\u00b7wig", "drum", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Nachen wiegt es mild.", "tokens": ["Der", "Na\u00b7chen", "wiegt", "es", "mild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "So s\u00fc\u00dfe Fahrt la\u00dft uns durchs Leben tr\u00e4umen,", "tokens": ["So", "s\u00fc\u00b7\u00dfe", "Fahrt", "la\u00dft", "uns", "durchs", "Le\u00b7ben", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da lebt sich's noch so gern;", "tokens": ["Da", "lebt", "sich's", "noch", "so", "gern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn's auch st\u00fcrmt, wenn bleich die Wogen sch\u00e4umen,", "tokens": ["Und", "wenn's", "auch", "st\u00fcrmt", ",", "wenn", "bleich", "die", "Wo\u00b7gen", "sch\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "VVFIN", "$,", "KOUS", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Hafen ist nicht fern!", "tokens": ["Der", "Ha\u00b7fen", "ist", "nicht", "fern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Stimmt an den Sang, die gr\u00fcnen Wogen lauschen", "tokens": ["Stimmt", "an", "den", "Sang", ",", "die", "gr\u00fc\u00b7nen", "Wo\u00b7gen", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Im alten Schwabenmeer,", "tokens": ["Im", "al\u00b7ten", "Schwa\u00b7ben\u00b7meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sobald ihr singt, beginnen sie zu rauschen", "tokens": ["So\u00b7bald", "ihr", "singt", ",", "be\u00b7gin\u00b7nen", "sie", "zu", "rau\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und h\u00fcpfen um euch her.", "tokens": ["Und", "h\u00fcp\u00b7fen", "um", "euch", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und sie durchstr\u00f6mt der Geist der fernen Zeiten,", "tokens": ["