{"textgrid.poem.61461": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun winkt's und fl\u00fcstert's aus den B\u00e4chen,", "tokens": ["Nun", "winkt's", "und", "fl\u00fcs\u00b7tert's", "aus", "den", "B\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun duftet's aus dem Tal herauf;", "tokens": ["Nun", "duf\u00b7tet's", "aus", "dem", "Tal", "her\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In ungest\u00fcmer Sehnsucht brechen", "tokens": ["In", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Sehn\u00b7sucht", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Knospen und die Herzen auf.", "tokens": ["Die", "Knos\u00b7pen", "und", "die", "Her\u00b7zen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Des Hirsches Trott erklingt im Walde,", "tokens": ["Des", "Hir\u00b7sches", "Trott", "er\u00b7klingt", "im", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Blauen schifft der wilde Schwan,", "tokens": ["Im", "Blau\u0