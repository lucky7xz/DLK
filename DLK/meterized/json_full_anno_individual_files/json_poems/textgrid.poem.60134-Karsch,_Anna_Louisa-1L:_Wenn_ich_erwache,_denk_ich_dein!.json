{"textgrid.poem.60134": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ich erwache, denk ich dein!", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich erwache, denk ich dein!", "tokens": ["Wenn", "ich", "er\u00b7wa\u00b7che", ",", "denk", "ich", "dein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Gott! der Tag und Nacht entscheidet,", "tokens": ["Du", "Gott", "!", "der", "Tag", "und", "Nacht", "ent\u00b7schei\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und in der Nacht mit Sonnenschein", "tokens": ["Und", "in", "der", "Nacht", "mit", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den finstern Mond bekleidet.", "tokens": ["Den", "fins\u00b7tern", "Mond", "be\u00b7klei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er leuchtet k\u00f6niglich daher,", "tokens": ["Er", "leuch\u00b7tet", "k\u00f6\u00b7nig\u00b7lich", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus hoher ungeme\u00dfner Ferne,", "tokens": ["Aus", "ho\u00b7her", "un\u00b7ge\u00b7me\u00df\u00b7ner", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ungez\u00e4hlt, wie Sand am Meer,", "tokens": ["Und", "un\u00b7ge\u00b7z\u00e4hlt", ",", "wie", "Sand", "am", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stehn um ihn her die Sterne.", "tokens": ["Stehn", "um", "ihn", "her", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Welch eine Pracht verbreitet sich!", "tokens": ["Welch", "ei\u00b7ne", "Pracht", "ver\u00b7brei\u00b7tet", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Dunkelheit geschm\u00fcckt mit Lichte", "tokens": ["Die", "Dun\u00b7kel\u00b7heit", "ge\u00b7schm\u00fcckt", "mit", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sieht auf uns nieder, nennet dich", "tokens": ["Sieht", "auf", "uns", "nie\u00b7der", ",", "nen\u00b7net", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "PTKVZ", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Glanz im Angesichte.", "tokens": ["Mit", "Glanz", "im", "An\u00b7ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du Sonnensch\u00f6pfer! wie so gro\u00df", "tokens": ["Du", "Son\u00b7nen\u00b7sch\u00f6p\u00b7fer", "!", "wie", "so", "gro\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$.", "PWAV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bist du im kleinsten Stern dort oben!", "tokens": ["Bist", "du", "im", "kleins\u00b7ten", "Stern", "dort", "o\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie unaussprechlich nahmenlos!", "tokens": ["Wie", "un\u00b7aus\u00b7sprech\u00b7lich", "nah\u00b7men\u00b7los", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Morgensterne loben", "tokens": ["Die", "Mor\u00b7gens\u00b7ter\u00b7ne", "lo\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dich mit einander in ein Chor", "tokens": ["Dich", "mit", "ein\u00b7an\u00b7der", "in", "ein", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschlossen, wie zu jener Stunde,", "tokens": ["Ge\u00b7schlos\u00b7sen", ",", "wie", "zu", "je\u00b7ner", "Stun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da aus dem Chaos tief hervor", "tokens": ["Da", "aus", "dem", "Chaos", "tief", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "PTKVZ"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein Wort aus deinem Munde", "tokens": ["Ein", "Wort", "aus", "dei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Allm\u00e4chtig diese Welten rief,", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "die\u00b7se", "Wel\u00b7ten", "rief", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Firmament herum gesetzet.", "tokens": ["Am", "Fir\u00b7ma\u00b7ment", "he\u00b7rum", "ge\u00b7set\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du sprachst, das Rad der Dinge lief,", "tokens": ["Du", "sprachst", ",", "das", "Rad", "der", "Din\u00b7ge", "lief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4uft noch unverletzet.", "tokens": ["Und", "l\u00e4uft", "noch", "un\u00b7ver\u00b7let\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Noch voller Jugend gl\u00e4nzen sie", "tokens": ["Noch", "vol\u00b7ler", "Ju\u00b7gend", "gl\u00e4n\u00b7zen", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da schon Jahrtausende vergangen!", "tokens": ["Da", "schon", "Jahr\u00b7tau\u00b7sen\u00b7de", "ver\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Zeiten Wechsel raubet nie", "tokens": ["Der", "Zei\u00b7ten", "Wech\u00b7sel", "rau\u00b7bet", "nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Licht von ihren Wangen.", "tokens": ["Das", "Licht", "von", "ih\u00b7ren", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Hier aber unter ihrem Blick", "tokens": ["Hier", "a\u00b7ber", "un\u00b7ter", "ih\u00b7rem", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vergeht, verfliegt, veraltet alles.", "tokens": ["Ver\u00b7geht", ",", "ver\u00b7fliegt", ",", "ver\u00b7al\u00b7tet", "al\u00b7les", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Thronenpomp, dem Cronengl\u00fcck", "tokens": ["Dem", "Thro\u00b7nen\u00b7pomp", ",", "dem", "Cro\u00b7nen\u00b7gl\u00fcck"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Droht eine Zeit des Falles!", "tokens": ["Droht", "ei\u00b7ne", "Zeit", "des", "Fal\u00b7les", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Mensch verbl\u00fcht wie pr\u00e4chtig Gras,", "tokens": ["Der", "Mensch", "ver\u00b7bl\u00fcht", "wie", "pr\u00e4ch\u00b7tig", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KOKOM", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Ansehn wird der Zeit zum Raube.", "tokens": ["Sein", "An\u00b7sehn", "wird", "der", "Zeit", "zum", "Rau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Weise, der in Sternen las,", "tokens": ["Der", "Wei\u00b7se", ",", "der", "in", "Ster\u00b7nen", "las", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Liegt schon gestreckt im Staube!", "tokens": ["Liegt", "schon", "ge\u00b7streckt", "im", "Stau\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ich lese, grosser Sch\u00f6pfer! dich", "tokens": ["Ich", "le\u00b7se", ",", "gros\u00b7ser", "Sch\u00f6p\u00b7fer", "!", "dich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$.", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Nachts in B\u00fcchern, aufgeschlagen", "tokens": ["Das", "Nachts", "in", "B\u00fc\u00b7chern", ",", "auf\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "ADV", "APPR", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von deiner Hand. O lehre mich", "tokens": ["Von", "dei\u00b7ner", "Hand", ".", "O", "leh\u00b7re", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "NE", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach deinem Lichte fragen!", "tokens": ["Nach", "dei\u00b7nem", "Lich\u00b7te", "fra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sey meiner Seele Klarheit, du", "tokens": ["Sey", "mei\u00b7ner", "See\u00b7le", "Klar\u00b7heit", ",", "du"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Regierer der entstandnen Sterne!", "tokens": ["Re\u00b7gie\u00b7rer", "der", "ent\u00b7stand\u00b7nen", "Ster\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und blicke meinem Herzen zu,", "tokens": ["Und", "bli\u00b7cke", "mei\u00b7nem", "Her\u00b7zen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es dich kennen lerne!", "tokens": ["Da\u00df", "es", "dich", "ken\u00b7nen", "ler\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Wenn ich erwache, denk ich dein!", "tokens": ["Wenn", "ich", "er\u00b7wa\u00b7che", ",", "denk", "ich", "dein", "!"], "t