{"textgrid.poem.36845": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Viertes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Glaub' ich's, da\u00df ihr nun auch mein trinakrisch Gl\u00fcck mir beneidet?", "tokens": ["Glaub'", "ich's", ",", "da\u00df", "ihr", "nun", "auch", "mein", "tri\u00b7nak\u00b7risch", "Gl\u00fcck", "mir", "be\u00b7nei\u00b7det", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PPER", "ADV", "ADV", "PPOSAT", "ADJD", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Eifrer der Heimath, ihr seid, heilige Fr\u00f6mmler, gemeint.", "tokens": ["Eif\u00b7rer", "der", "Hei\u00b7math", ",", "ihr", "seid", ",", "hei\u00b7li\u00b7ge", "Fr\u00f6mm\u00b7ler", ",", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VAFIN", "$,", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "\u00bbuners\u00e4ttlich nach Sinnengenu\u00df, von Freude zu Freude", "tokens": ["\u00bb", "un\u00b7er\u00b7s\u00e4tt\u00b7lich", "nach", "Sin\u00b7nen\u00b7ge\u00b7nu\u00df", ",", "von", "Freu\u00b7de", "zu", "Freu\u00b7de"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "APPR", "NN", "$,", "APPR", "NN", "APPR", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Jagt er beth\u00f6rt und bedenkt nicht, da\u00df die Nemesis naht.", "tokens": ["Jagt", "er", "be\u00b7th\u00f6rt", "und", "be\u00b7denkt", "nicht", ",", "da\u00df", "die", "Ne\u00b7me\u00b7sis", "naht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Irdischem neigt sich der Sinn, der verwilderte. Bessrer Empfindung,", "tokens": ["Ir\u00b7di\u00b7schem", "neigt", "sich", "der", "Sinn", ",", "der", "ver\u00b7wil\u00b7der\u00b7te", ".", "Bess\u00b7rer", "Emp\u00b7fin\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$,", "PRELS", "VVFIN", "$.", "NN", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Frommer und reiner, verschlie\u00dft er das vergiftete Herz.", "tokens": ["From\u00b7mer", "und", "rei\u00b7ner", ",", "ver\u00b7schlie\u00dft", "er", "das", "ver\u00b7gif\u00b7te\u00b7te", "Herz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Sitte achtet er nicht noch Gesetz, nicht Glauben und Schule,", "tokens": ["Sit\u00b7te", "ach\u00b7tet", "er", "nicht", "noch", "Ge\u00b7setz", ",", "nicht", "Glau\u00b7ben", "und", "Schu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "ADV", "NN", "$,", "PTKNEG", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Den die Willk\u00fcr allein, den die Begierde beherrscht.", "tokens": ["Den", "die", "Will\u00b7k\u00fcr", "al\u00b7lein", ",", "den", "die", "Be\u00b7gier\u00b7de", "be\u00b7herrscht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "So der Heimath entflohn von dem ernsteren Gange des Lebens", "tokens": ["So", "der", "Hei\u00b7math", "ent\u00b7flohn", "von", "dem", "erns\u00b7te\u00b7ren", "Gan\u00b7ge", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Schwelgt er in Lust und Genu\u00df selbst bis an Lybiens Strand.\u00ab", "tokens": ["Schwelgt", "er", "in", "Lust", "und", "Ge\u00b7nu\u00df", "selbst", "bis", "an", "Ly\u00b7bi\u00b7ens", "Strand", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "ADV", "ADV", "APPR", "NE", "NE", "$.", "$("], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Schweigt, o Kinder des Lichts, ihr auserkohrenen L\u00e4mmer;", "tokens": ["Schweigt", ",", "o", "Kin\u00b7der", "des", "Lichts", ",", "ihr", "au\u00b7ser\u00b7koh\u00b7re\u00b7nen", "L\u00e4m\u00b7mer", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Ja, verk\u00fcnd' ich es nur, gr\u00f6\u00dfrer Entz\u00fcckungen Rausch,", "tokens": ["Ja", ",", "ver\u00b7k\u00fcnd'", "ich", "es", "nur", ",", "gr\u00f6\u00df\u00b7rer", "Ent\u00b7z\u00fc\u00b7ckun\u00b7gen", "Rausch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "PPER", "ADV", "$,", "ADJA", "NN", "NN", "$,"], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "K\u00fchnere Orgien feiert' ich nicht, seitdem mir des Lebens", "tokens": ["K\u00fch\u00b7ne\u00b7re", "Or\u00b7gi\u00b7en", "fei\u00b7ert'", "ich", "nicht", ",", "seit\u00b7dem", "mir", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+---+-+-+-+-+-+-", "measure": "dactylic.init"}, "line.14": {"text": "Sch\u00e4umender Becher den Mund freieren Geistes ber\u00fchrt.", "tokens": ["Sch\u00e4u\u00b7men\u00b7der", "Be\u00b7cher", "den", "Mund", "frei\u00b7e\u00b7ren", "Geis\u00b7tes", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Ja, gesteh' ich's euch nur, ich sch\u00e4mte mich selber der Heimath,", "tokens": ["Ja", ",", "ge\u00b7steh'", "ich's", "euch", "nur", ",", "ich", "sch\u00e4m\u00b7te", "mich", "sel\u00b7ber", "der", "Hei\u00b7math", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PIS", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "--+--+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "W\u00e4rt ihr das Aermlichste nicht, was noch die Mutter gebar,", "tokens": ["W\u00e4rt", "ihr", "das", "A\u00b7erm\u00b7lichs\u00b7te", "nicht", ",", "was", "noch", "die", "Mut\u00b7ter", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKNEG", "$,", "PRELS", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.17": {"text": "Zeugte die Stammburg einst, die zertr\u00fcmmerte, theure, die Helden,", "tokens": ["Zeug\u00b7te", "die", "Stamm\u00b7burg", "einst", ",", "die", "zer\u00b7tr\u00fcm\u00b7mer\u00b7te", ",", "theu\u00b7re", ",", "die", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Das unsterbliche Paar, staufische Friedriche nicht.", "tokens": ["Das", "uns\u00b7terb\u00b7li\u00b7che", "Paar", ",", "stau\u00b7fi\u00b7sche", "Fried\u00b7ri\u00b7che", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "PTKNEG", "$."], "meter": "--+--++-+-+-+", "measure": "anapaest.di.plus"}, "line.19": {"text": "H\u00f6r's, engbr\u00fcstig Geschlecht, ich verberge dir nichts, ich bekenne", "tokens": ["H\u00f6r's", ",", "eng\u00b7br\u00fcs\u00b7tig", "Ge\u00b7schlecht", ",", "ich", "ver\u00b7ber\u00b7ge", "dir", "nichts", ",", "ich", "be\u00b7ken\u00b7ne"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJD", "NN", "$,", "PPER", "VVFIN", "PPER", "PIS", "$,", "PPER", "VVFIN"], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Stolz und freudig, wie Zeus reich mir die Tage geschenkt.", "tokens": ["Stolz", "und", "freu\u00b7dig", ",", "wie", "Zeus", "reich", "mir", "die", "Ta\u00b7ge", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "PWAV", "NE", "ADJD", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Bald am Anapus weil' ich, es gleitet der Kahn zu der Quelle,", "tokens": ["Bald", "am", "A\u00b7na\u00b7pus", "weil'", "ich", ",", "es", "glei\u00b7tet", "der", "Kahn", "zu", "der", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Und auf dem fl\u00fcssigen Pfad schattet die Blume des Nils.", "tokens": ["Und", "auf", "dem", "fl\u00fcs\u00b7si\u00b7gen", "Pfad", "schat\u00b7tet", "die", "Blu\u00b7me", "des", "Nils", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Bald umschweben die G\u00f6ttinnen mich im seligen Enna,", "tokens": ["Bald", "um\u00b7schwe\u00b7ben", "die", "G\u00f6t\u00b7tin\u00b7nen", "mich", "im", "se\u00b7li\u00b7gen", "En\u00b7na", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Und die Stunde, da mir Helios einst sich erhob", "tokens": ["Und", "die", "Stun\u00b7de", ",", "da", "mir", "He\u00b7li\u00b7os", "einst", "sich", "er\u00b7hob"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "PPER", "NE", "ADV", "PRF", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.25": {"text": "Ueber des Aetnas Riesengebild, nicht, glaub' ich, ihr gleichet,", "tokens": ["Ue\u00b7ber", "des", "A\u00b7et\u00b7nas", "Rie\u00b7sen\u00b7ge\u00b7bild", ",", "nicht", ",", "glaub'", "ich", ",", "ihr", "glei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PTKNEG", "$,", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.26": {"text": "W\u00e4hrt es auch Ewigkeit, all euer Leben an Werth.", "tokens": ["W\u00e4hrt", "es", "auch", "E\u00b7wig\u00b7keit", ",", "all", "eu\u00b7er", "Le\u00b7ben", "an", "Werth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$,", "PIAT", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.27": {"text": "Bald in duftigen Hainen besuch ich des Akragas Tempel,", "tokens": ["Bald", "in", "duf\u00b7ti\u00b7gen", "Hai\u00b7nen", "be\u00b7such", "ich", "des", "Ak\u00b7ra\u00b7gas", "Tem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.28": {"text": "Einen ganzen Olymp birgt mir das liebliche Gr\u00fcn.", "tokens": ["Ei\u00b7nen", "gan\u00b7zen", "O\u00b7lymp", "birgt", "mir", "das", "lieb\u00b7li\u00b7che", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "Selinunts Titanenruin und der stolzen Segesta", "tokens": ["Se\u00b7lin\u00b7unts", "Ti\u00b7ta\u00b7nen\u00b7ru\u00b7in", "und", "der", "stol\u00b7zen", "Se\u00b7ge\u00b7sta"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "KON", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.30": {"text": "Troisches S\u00e4ulenhaus ladet den Gl\u00fccklichen ein.", "tokens": ["Tro\u00b7i\u00b7sches", "S\u00e4u\u00b7len\u00b7haus", "la\u00b7det", "den", "Gl\u00fcck\u00b7li\u00b7chen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.31": {"text": "Bald nach Karthagos Tr\u00fcmmern vom lilyb\u00e4ischen Strande", "tokens": ["Bald", "nach", "Kar\u00b7tha\u00b7gos", "Tr\u00fcm\u00b7mern", "vom", "li\u00b7ly\u00b7b\u00e4\u00b7i\u00b7schen", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.32": {"text": "W\u00fcnsch' ich mich \u00fcber die See, \u00fcber die lybische, weg.", "tokens": ["W\u00fcn\u00b7sch'", "ich", "mich", "\u00fc\u00b7ber", "die", "See", ",", "\u00fc\u00b7ber", "die", "ly\u00b7bi\u00b7sche", ",", "weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "$,", "PTKVZ", "$."], "meter": "+---+--++--++-+", "measure": "trochaic.septa.relaxed"}, "line.33": {"text": "Unter Marsalas Palmen und hesperidischen Reben", "tokens": ["Un\u00b7ter", "Mar\u00b7sa\u00b7las", "Pal\u00b7men", "und", "hes\u00b7pe\u00b7ri\u00b7di\u00b7schen", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "KON", "ADJA", "NN"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.34": {"text": "Wandr' ich zum heiligen Berg, h\u00f6rt es, zum Eryx hinan.", "tokens": ["Wandr'", "ich", "zum", "hei\u00b7li\u00b7gen", "Berg", ",", "h\u00f6rt", "es", ",", "zum", "E\u00b7ryx", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Schm\u00e4het ihr noch, so ruf ich dich an, o Genius: Lehre", "tokens": ["Schm\u00e4\u00b7het", "ihr", "noch", ",", "so", "ruf", "ich", "dich", "an", ",", "o", "Ge\u00b7nius", ":", "Leh\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "FM", "NN", "$.", "NN"], "meter": "+--+-+--+-+-+-", "measure": "iambic.hexa.invert"}, "line.36": {"text": "Dithyrambischen Worts stolzre Bedeutungen mich.", "tokens": ["Dit\u00b7hy\u00b7ram\u00b7bi\u00b7schen", "Worts", "stolz\u00b7re", "Be\u00b7deu\u00b7tun\u00b7gen", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "PPER", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.37": {"text": "So entstr\u00f6me die Flamme des Aetnas Grunde, so w\u00e4lze", "tokens": ["So", "ent\u00b7str\u00f6\u00b7me", "die", "Flam\u00b7me", "des", "A\u00b7et\u00b7nas", "Grun\u00b7de", ",", "so", "w\u00e4l\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "NN", "$,", "ADV", "ADJA"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.38": {"text": "Donnernd der purpurne Strom sich aus der Tiefe hervor;", "tokens": ["Don\u00b7nernd", "der", "pur\u00b7pur\u00b7ne", "Strom", "sich", "aus"