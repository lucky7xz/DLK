, "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "ART", "NN", "$,", "PPER", "APPR", "NE", "ADV", "PPER"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.44": {"text": "Und kredenze des Kelchs ewig verj\u00fcngenden Trank.", "tokens": ["Und", "kre\u00b7den\u00b7ze", "des", "Kelchs", "e\u00b7wig", "ver\u00b7j\u00fcn\u00b7gen\u00b7den", "Trank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.45": {"text": "Schon durchgl\u00fcht mich die Flamme, vernehmt's: Was ist's, wenn im Taumel", "tokens": ["Schon", "durch\u00b7gl\u00fcht", "mich", "die", "Flam\u00b7me", ",", "ver\u00b7nehmt's", ":", "Was", "ist's", ",", "wenn", "im", "Tau\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "$.", "PWS", "VAFIN", "$,", "KOUS", "APPRART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.46": {"text": "Eurer zu spotten ich mir Apotheose getr\u00e4umt!", "tokens": ["Eu\u00b7rer", "zu", "spot\u00b7ten", "ich", "mir", "A\u00b7pot\u00b7he\u00b7o\u00b7se", "ge\u00b7tr\u00e4umt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PTKZU", "VVFIN", "PPER", "PPER", "NN", "VVPP", "$."], "meter": "+--+-+--+-+--+", "measure": "iambic.hexa.invert"}}}}}