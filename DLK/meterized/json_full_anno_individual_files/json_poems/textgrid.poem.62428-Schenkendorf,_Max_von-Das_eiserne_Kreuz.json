{"textgrid.poem.62428": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Das eiserne Kreuz", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der Nogat gr\u00fcnen Wiesen", "tokens": ["Auf", "der", "No\u00b7gat", "gr\u00fc\u00b7nen", "Wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht ein Schlo\u00df in Preu\u00dfenland,", "tokens": ["Steht", "ein", "Schlo\u00df", "in", "Preu\u00b7\u00dfen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das die frommen deutschen Riesen", "tokens": ["Das", "die", "from\u00b7men", "deut\u00b7schen", "Rie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst Marienburg genannt.", "tokens": ["Einst", "Ma\u00b7ri\u00b7en\u00b7burg", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "An der Mauer ist zu schauen", "tokens": ["An", "der", "Mau\u00b7er", "ist", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bildni\u00df leuchtend gro\u00df und klar,", "tokens": ["Bild\u00b7ni\u00df", "leuch\u00b7tend", "gro\u00df", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bildni\u00df unsrer lieben Frauen,", "tokens": ["Bild\u00b7ni\u00df", "uns\u00b7rer", "lie\u00b7ben", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die den Heiland uns gebar.", "tokens": ["Die", "den", "Hei\u00b7land", "uns", "ge\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lieb' und Glaube wollten geben", "tokens": ["Lieb'", "und", "Glau\u00b7be", "woll\u00b7ten", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jener F\u00fclle milden Reiz,", "tokens": ["Je\u00b7ner", "F\u00fcl\u00b7le", "mil\u00b7den", "Reiz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den L\u00fcften sah man schweben,", "tokens": ["In", "den", "L\u00fcf\u00b7ten", "sah", "man", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Fahnen hoch das Kreuz.", "tokens": ["In", "den", "Fah\u00b7nen", "hoch", "das", "Kreuz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Heil'ges Zeichen ward erlesen", "tokens": ["Heil'\u00b7ges", "Zei\u00b7chen", "ward", "er\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fern im weisen Morgenland,", "tokens": ["Fern", "im", "wei\u00b7sen", "Mor\u00b7gen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nach seinem tiefsten Wesen", "tokens": ["Und", "nach", "sei\u00b7nem", "tiefs\u00b7ten", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ward es ", "tokens": ["Ward", "es"], "token_info": ["word", "word"], "pos": ["VAFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "Heil dir, alter Bund der Starken,", "tokens": ["Heil", "dir", ",", "al\u00b7ter", "Bund", "der", "Star\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heil euch edle deutsche Herrn,", "tokens": ["Heil", "euch", "ed\u00b7le", "deut\u00b7sche", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den frommen Christen-Marken", "tokens": ["Von", "den", "from\u00b7men", "Chris\u00b7ten\u00b7Mar\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hieltet ihr die Heiden fern.", "tokens": ["Hiel\u00b7tet", "ihr", "die", "Hei\u00b7den", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach, die Ritter sind gefallen,", "tokens": ["Ach", ",", "die", "Rit\u00b7ter", "sind", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre Tempel sind entweiht,", "tokens": ["Ih\u00b7re", "Tem\u00b7pel", "sind", "ent\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Abgebrochen ihre Hallen \u2013", "tokens": ["Ab\u00b7ge\u00b7bro\u00b7chen", "ih\u00b7re", "Hal\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den S\u00e4rgen liegt ihr Kleid.", "tokens": ["Auf", "den", "S\u00e4r\u00b7gen", "liegt", "ihr", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Immer nur das Lose, Neue", "tokens": ["Im\u00b7mer", "nur", "das", "Lo\u00b7se", ",", "Neu\u00b7e"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nahm die j\u00fcngste Zeit um Ziel,", "tokens": ["Nahm", "die", "j\u00fcngs\u00b7te", "Zeit", "um", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alte Kraft und alte Treue", "tokens": ["Al\u00b7te", "Kraft", "und", "al\u00b7te", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebten kaum im Ritterspiel.", "tokens": ["Leb\u00b7ten", "kaum", "im", "Rit\u00b7ter\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch, ein Herr, dem Alle weichen,", "tokens": ["Doch", ",", "ein", "Herr", ",", "dem", "Al\u00b7le", "wei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "$,", "PRELS", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat den Jammer fromm bedacht,", "tokens": ["Hat", "den", "Jam\u00b7mer", "fromm", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat uns unser Ordenszeichen", "tokens": ["Hat", "uns", "un\u00b7ser", "Or\u00b7dens\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der Gruft herauf gebracht.", "tokens": ["Aus", "der", "Gruft", "her\u00b7auf", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wieder schm\u00fcckt es unsre Fahnen,", "tokens": ["Wie\u00b7der", "schm\u00fcckt", "es", "uns\u00b7re", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder deckt es unsre Brust,", "tokens": ["Wie\u00b7der", "deckt", "es", "uns\u00b7re", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im Himmel noch die Ahnen", "tokens": ["Und", "im", "Him\u00b7mel", "noch", "die", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schauen es mit Heldenlust.", "tokens": ["Schau\u00b7en", "es", "mit", "Hel\u00b7den\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "War das alte Kreuz von Wollen,", "tokens": ["War", "das", "al\u00b7te", "Kreuz", "von", "Wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eisern ist das neue Bild,", "tokens": ["Ei\u00b7sern", "ist", "das", "neu\u00b7e", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Anzudeuten, was wir sollen,", "tokens": ["An\u00b7zu\u00b7deu\u00b7ten", ",", "was", "wir", "sol\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was der M\u00e4nner Herzen f\u00fcllt.", "tokens": ["Was", "der", "M\u00e4n\u00b7ner", "Her\u00b7zen", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Denn nur Eisen kann uns retten,", "tokens": ["Denn", "nur", "Ei\u00b7sen", "kann", "uns", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und erl\u00f6sen kann nur Blut,", "tokens": ["Und", "er\u00b7l\u00f6\u00b7sen", "kann", "nur", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der S\u00fcnde schweren Ketten,", "tokens": ["Von", "der", "S\u00fcn\u00b7de", "schwe\u00b7ren", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von des B\u00f6sen Uebermuth.", "tokens": ["Von", "des", "B\u00f6\u00b7sen", "Ue\u00b7ber\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Heil'ges Kreuz, ihr dunkeln Farben,", "tokens": ["Heil'\u00b7ges", "Kreuz", ",", "ihr", "dun\u00b7keln", "Far\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seid in jede Brust gepr\u00e4gt.", "tokens": ["Seid", "in", "je\u00b7de", "Brust", "ge\u00b7pr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4nnern, die im Glauben starben,", "tokens": [