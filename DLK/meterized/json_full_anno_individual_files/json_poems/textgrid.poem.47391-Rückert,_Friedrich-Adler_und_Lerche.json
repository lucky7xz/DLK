{"textgrid.poem.47391": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Adler und Lerche", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnt' ich steigen,", "tokens": ["K\u00f6nnt'", "ich", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Dem Adler gleich,", "tokens": ["Dem", "Ad\u00b7ler", "gleich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der kommenden Sonn' entgegen,", "tokens": ["Der", "kom\u00b7men\u00b7den", "Sonn'", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Brust getaucht", "tokens": ["Die", "Brust", "ge\u00b7taucht"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In Morgenrot,", "tokens": ["In", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Badend in Glanz des \u00c4thers,", "tokens": ["Ba\u00b7dend", "in", "Glanz", "des", "\u00c4\u00b7thers", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Weil in Tiefen", "tokens": ["Weil", "in", "Tie\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Die Nacht noch tr\u00e4umt,", "tokens": ["Die", "Nacht", "noch", "tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Dem erwachenden", "tokens": ["Dem", "er\u00b7wa\u00b7chen\u00b7den"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Auge der Welt", "tokens": ["Au\u00b7ge", "der", "Welt"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Den ersten Blick entsaugen!", "tokens": ["Den", "ers\u00b7ten", "Blick", "ent\u00b7sau\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Oder fliegen,", "tokens": ["O\u00b7der", "flie\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Der Lerche gleich,", "tokens": ["Der", "Ler\u00b7che", "gleich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nach, der scheidenden Sonne nach,", "tokens": ["Nach", ",", "der", "schei\u00b7den\u00b7den", "Son\u00b7ne", "nach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "\u00dcber der stillen Sch\u00f6pfung,", "tokens": ["\u00dc\u00b7ber", "der", "stil\u00b7len", "Sch\u00f6p\u00b7fung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Angegl\u00fchet", "tokens": ["An\u00b7ge\u00b7gl\u00fc\u00b7het"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Vom letzten Strahl,", "tokens": ["Vom", "letz\u00b7ten", "Strahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die Seel' im Liede verhauchend,", "tokens": ["Die", "Seel'", "im", "Lie\u00b7de", "ver\u00b7hau\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Verschwebend,", "tokens": ["Ver\u00b7schwe\u00b7bend", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Verschwirrend", "tokens": ["Ver\u00b7schwir\u00b7rend"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "In \u00c4therduft,", "tokens": ["In", "\u00c4t\u00b7her\u00b7duft", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Nie mehr wieder", "tokens": ["Nie", "mehr", "wie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Zur Erd' hernieder!", "tokens": ["Zur", "Erd'", "her\u00b7nie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Aber ach!", "tokens": ["A\u00b7ber", "ach", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Der Adler, der", "tokens": ["Der", "Ad\u00b7ler", ",", "der"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Sonn' ins Angesicht geschaut,", "tokens": ["Der", "Sonn'", "ins", "An\u00b7ge\u00b7sicht", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Senkt den Fittich", "tokens": ["Senkt", "den", "Fit\u00b7tich"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Aus Himmelsglanz,", "tokens": ["Aus", "Him\u00b7mels\u00b7glanz", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Um in dunkeler Tiefe", "tokens": ["Um", "in", "dun\u00b7ke\u00b7ler", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Nach der Beute des Tags zu sp\u00e4hn,", "tokens": ["Nach", "der", "Beu\u00b7te", "des", "Tags", "zu", "sp\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und die Lerche", "tokens": ["Und", "die", "Ler\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Aus den Wirbeln", "tokens": ["Aus", "den", "Wir\u00b7beln"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Ihres Himmelsgesanges", "tokens": ["Ih\u00b7res", "Him\u00b7mels\u00b7ge\u00b7san\u00b7ges"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.11": {"text": "Sinkt ermattet", "tokens": ["Sinkt", "er\u00b7mat\u00b7tet"], "token_info": ["word", "word"], "pos": ["VVFIN", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Zum Boden wieder,", "tokens": ["Zum", "Bo\u00b7den", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Wo sie das Nest f\u00fcr die Nacht gebaut.", "tokens": ["Wo", "sie", "das", "Nest", "f\u00fcr", "die", "Nacht", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Kann kein erdegeborner,", "tokens": ["Kann", "kein", "er\u00b7de\u00b7ge\u00b7bor\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fl\u00fcgelbegabter", "tokens": ["Fl\u00fc\u00b7gel\u00b7be\u00b7gab\u00b7ter"], "token_info": ["word"], "pos": ["NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Heldensinn,", "tokens": ["Hel\u00b7den\u00b7sinn", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "S\u00e4ngergeist", "tokens": ["S\u00e4n\u00b7ger\u00b7geist"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Den Banden der niedren Mutter", "tokens": ["Den", "Ban\u00b7den", "der", "nied\u00b7ren", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ganz entfliehn,", "tokens": ["Ganz", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Dem edlen Vater", "tokens": ["Dem", "ed\u00b7len", "Va\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Lichte zu?", "tokens": ["Lich\u00b7te", "zu", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Liebe setzte die Schwingen", "tokens": ["Lie\u00b7be", "setz\u00b7te", "die", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Der Begeisterung", "tokens": ["Der", "Be\u00b7geis\u00b7te\u00b7rung"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "An mein Herz,", "tokens": ["An", "mein", "Herz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Und es flog", "tokens": ["Und", "es", "flog"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Der Sonne zu,", "tokens": ["Der", "Son\u00b7ne", "zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Bis die Fittiche", "tokens": ["Bis", "die", "Fit\u00b7ti\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.7": {"text": "Schmolzen,", "tokens": ["Schmol\u00b7zen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Seinen H\u00f6hen", "tokens": ["Sei\u00b7nen", "H\u00f6\u00b7hen"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Entst\u00fcrzend", "tokens": ["Ent\u00b7st\u00fcr\u00b7zend"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Es ins Meer der Besch\u00e4mung sank.", "tokens": ["Es", "ins", "Meer", "der", "Be\u00b7sch\u00e4\u00b7mung", "sank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Und es klagte.", "tokens": ["Und", "es", "klag\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Doch die Liebe", "tokens": ["Doch", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Sprach, die Schwing' ihm erneuend:", "tokens": ["Sprach", ",", "die", "Schwing'", "ihm", "er\u00b7neu\u00b7end", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "\u00bbandre geb' ich", "tokens": ["\u00bb", "and\u00b7re", "geb'", "ich"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "PIS", "VVFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Dir, die schwache,", "tokens": ["Dir", ",", "die", "schwa\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Aber himmlische", "tokens": ["A\u00b7ber", "himm\u00b7li\u00b7sche"], "token_info": ["word", "word"], "pos": ["KON", "ADJA"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Freundin, nicht.", "tokens": ["Freun\u00b7din", ",", "nicht", "."], "token_info": ["word", "punct", "word", "punc