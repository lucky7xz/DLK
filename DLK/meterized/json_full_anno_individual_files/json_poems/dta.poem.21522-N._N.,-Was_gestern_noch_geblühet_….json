{"dta.poem.21522": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Was gestern noch gebl\u00fchet \u2026", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Was gestern noch gebl\u00fchet,", "tokens": ["Was", "ge\u00b7stern", "noch", "ge\u00b7bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ist heute schon verdorrt,", "tokens": ["Ist", "heu\u00b7te", "schon", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und was du j\u00fcngst mir zugeraunt,", "tokens": ["Und", "was", "du", "j\u00fcngst", "mir", "zu\u00b7ge\u00b7raunt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verklungen ist das Wort!", "tokens": ["Ver\u00b7klun\u00b7gen", "ist", "das", "Wort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verrauscht ist sie, die Stunde,", "tokens": ["Ver\u00b7rauscht", "ist", "sie", ",", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo dich mein Arm umfing \u2014", "tokens": ["Wo", "dich", "mein", "Arm", "um\u00b7fing"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo lustberauscht mein Flammenblick", "tokens": ["Wo", "lust\u00b7be\u00b7rauscht", "mein", "Flam\u00b7men\u00b7blick"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An deinem Antlitz hing!", "tokens": ["An", "dei\u00b7nem", "Ant\u00b7litz", "hing", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Herbstwind fegt die Bl\u00e4tter,", "tokens": ["Der", "Herbst\u00b7wind", "fegt", "die", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die letzten, von dem Ast \u2014", "tokens": ["Die", "letz\u00b7ten", ",", "von", "dem", "Ast"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wand\u2019re durch das \u00f6de Land", "tokens": ["Ich", "wan\u00b7d'\u00b7re", "durch", "das", "\u00f6\u00b7de", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bald hier, bald da zu Gast \u2026", "tokens": ["Bald", "hier", ",", "bald", "da", "zu", "Gast", "\u2026"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Stirne gl\u00fcht in Fieber \u2014", "tokens": ["Die", "Stir\u00b7ne", "gl\u00fcht", "in", "Fie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In Fieber bebt die Hand,", "tokens": ["In", "Fie\u00b7ber", "bebt", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wirre Wahnsinnsphantasie\u2019n", "tokens": ["Und", "wir\u00b7re", "Wahn\u00b7sinns\u00b7phan\u00b7ta\u00b7sie'n"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sind mir im Hirn entbrannt \u2026", "tokens": ["Sind", "mir", "im", "Hirn", "ent\u00b7brannt", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da\u00df ich dich lassen mu\u00dfte,", "tokens": ["Da\u00df", "ich", "dich", "las\u00b7sen", "mu\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das ficht mich gar nicht an \u2014", "tokens": ["Das", "ficht", "mich", "gar", "nicht", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ist nun einmal Menschenloos", "tokens": ["Das", "ist", "nun", "ein\u00b7mal", "Men\u00b7schen\u00b7loos"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sei nun abgethan!", "tokens": ["Das", "sei", "nun", "ab\u00b7ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Eins aber zieht mich nieder,", "tokens": ["Eins", "a\u00b7ber", "zieht", "mich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das lastet wie ein Fluch,", "tokens": ["Das", "las\u00b7tet", "wie", "ein", "Fluch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das l\u00e4hmt der Seele stolze Kraft,", "tokens": ["Das", "l\u00e4hmt", "der", "See\u00b7le", "stol\u00b7ze", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Hochgedanken Flug;", "tokens": ["Der", "Hoch\u00b7ge\u00b7dan\u00b7ken", "Flug", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das gr\u00e4bt sich in die Stirne", "tokens": ["Das", "gr\u00e4bt", "sich", "in", "die", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit tausend Furchen ein;", "tokens": ["Mit", "tau\u00b7send", "Fur\u00b7chen", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das dunkelt mir der Sonne Gold,", "tokens": ["Das", "dun\u00b7kelt", "mir", "der", "Son\u00b7ne", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das dunkelt Sternenschein;", "tokens": ["Das", "dun\u00b7kelt", "Ster\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das w\u00fchlt sich in die Brust mir", "tokens": ["Das", "w\u00fchlt", "sich", "in", "die", "Brust", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie eines Sch\u00e4chers Blick;", "tokens": ["Wie", "ei\u00b7nes", "Sch\u00e4\u00b7chers", "Blick", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das hemmt des Athems Freiheitsdrang", "tokens": ["Das", "hemmt", "des", "A\u00b7thems", "Frei\u00b7heits\u00b7drang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie eines Henkers Strick!", "tokens": ["Wie", "ei\u00b7nes", "Hen\u00b7kers", "Strick", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das grinst mich an wie eine", "tokens": ["Das", "grinst", "mich", "an", "wie", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "KOKOM", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verrenkte Bettlerfaust;", "tokens": ["Ver\u00b7renk\u00b7te", "Bett\u00b7ler\u00b7faust", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das loht in mir wie H\u00f6llenqual,", "tokens": ["Das", "loht", "in", "mir", "wie", "H\u00f6l\u00b7len\u00b7qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Herz und Hirn durchbraust \u2014", "tokens": ["Die", "Herz", "und", "Hirn", "durch\u00b7braust"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und fragt ihr: ", "tokens": ["Und", "fragt", "ihr", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Den wirren Qualenstrom?", "tokens": ["Den", "wir\u00b7ren", "Qua\u00b7len\u00b7strom", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Sehusucht, die da lechzt nach Gl\u00fcck,", "tokens": ["Die", "Se\u00b7hu\u00b7sucht", ",", "die", "da", "lechzt", "nach", "Gl\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Nach Gl\u00fcck, das nur \u2014 Phantom!", "tokens": ["Nach", "Gl\u00fcck", ",", "das", "nur", "Phan\u00b7tom", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "$(", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}