{"textgrid.poem.39189": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "Mich freut's", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was fliehst Du uns mit trotzigen Mienen,", "tokens": ["Was", "fliehst", "Du", "uns", "mit", "trot\u00b7zi\u00b7gen", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir sind so klug, wir sind so reich,", "tokens": ["Wir", "sind", "so", "klug", ",", "wir", "sind", "so", "reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es rollt die Welt auf glatten Schienen,", "tokens": ["Es", "rollt", "die", "Welt", "auf", "glat\u00b7ten", "Schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was rennst Du quer durch Sumpf und Teich? \u2013", "tokens": ["Was", "rennst", "Du", "quer", "durch", "Sumpf", "und", "Teich", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u2013 O, la\u00dft die Fragen, klug verbindlich,", "tokens": ["\u2013", "O", ",", "la\u00dft", "die", "Fra\u00b7gen", ",", "klug", "ver\u00b7bind\u00b7lich", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "VVFIN", "ART", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00dft mir mein selbstgew\u00e4hltes Kreuz,", "tokens": ["La\u00dft", "mir", "mein", "selbst\u00b7ge\u00b7w\u00e4hl\u00b7tes", "Kreuz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es bleibt Euch ewig unergr\u00fcndlich,", "tokens": ["Es", "bleibt", "Euch", "e\u00b7wig", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die einz'ge Antwort ist: Mich freut's!", "tokens": ["Die", "einz'\u00b7ge", "Ant\u00b7wort", "ist", ":", "Mich", "freut's", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mich freut's, in dieser Zeit des Alters", "tokens": ["Mich", "freut's", ",", "in", "die\u00b7ser", "Zeit", "des", "Al\u00b7ters"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu sein mitunter ganz ein Kind", "tokens": ["Zu", "sein", "mi\u00b7tun\u00b7ter", "ganz", "ein", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und zickzack, wie der Flug des Falters,", "tokens": ["Und", "zick\u00b7zack", ",", "wie", "der", "Flug", "des", "Fal\u00b7ters", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu taumeln hin in Bl\u00fctenwind.", "tokens": ["Zu", "tau\u00b7meln", "hin", "in", "Bl\u00fc\u00b7ten\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich freut's, behaglich zu verstummen,", "tokens": ["Mich", "freut's", ",", "be\u00b7hag\u00b7lich", "zu", "ver\u00b7stum\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Indes gesch\u00e4ftig summt die Stadt.", "tokens": ["In\u00b7des", "ge\u00b7sch\u00e4f\u00b7tig", "summt", "die", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mich freut's, in dieser Zeit der Summen", "tokens": ["Mich", "freut's", ",", "in", "die\u00b7ser", "Zeit", "der", "Sum\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu handeln um ein Rosenblatt.", "tokens": ["Zu", "han\u00b7deln", "um", "ein", "Ro\u00b7sen\u00b7blatt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mich freut's, gestreckt im Meer' zu liegen,", "tokens": ["Mich", "freut's", ",", "ge\u00b7streckt", "im", "Meer'", "zu", "lie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn dunkelgr\u00fcnlich ruht sein Schacht,", "tokens": ["Wenn", "dun\u00b7kel\u00b7gr\u00fcn\u00b7lich", "ruht", "sein", "Schacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lang mit flutendem Vergn\u00fcgen", "tokens": ["Und", "lang", "mit", "flu\u00b7ten\u00b7dem", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu schaun ins M\u00e4rchenaug' der Nacht.", "tokens": ["Zu", "schaun", "ins", "M\u00e4r\u00b7che\u00b7naug'", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich freut im Sturme markersch\u00fctternd", "tokens": ["Mich", "freut", "im", "Stur\u00b7me", "mar\u00b7ker\u00b7sch\u00fct\u00b7ternd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Wehruf der gepeitschten See,", "tokens": ["Der", "We\u00b7hruf", "der", "ge\u00b7peitschten", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Mich freut, aus stillen Fluten zitternd,", "tokens": ["Mich", "freut", ",", "aus", "stil\u00b7len", "Flu\u00b7ten", "zit\u00b7ternd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das Wiegenlied der Meeresfee.", "tokens": ["Das", "Wie\u00b7gen\u00b7lied", "der", "Mee\u00b7res\u00b7fee", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mich freut's, mit herzigen Kumpanen", "tokens": ["Mich", "freut's", ",", "mit", "her\u00b7zi\u00b7gen", "Kum\u00b7pa\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In goldbes\u00e4umter D\u00e4mmerung,", "tokens": ["In", "gold\u00b7be\u00b7s\u00e4um\u00b7ter", "D\u00e4m\u00b7me\u00b7rung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Auf gr\u00fcnbehangnen Burgaltanen", "tokens": ["Auf", "gr\u00fcn\u00b7be\u00b7hang\u00b7nen", "Bur\u00b7gal\u00b7ta\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu tuen einen edlen Trunk.", "tokens": ["Zu", "tu\u00b7en", "ei\u00b7nen", "ed\u00b7len", "Trunk", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich freut's, die Brust entbl\u00f6\u00dft dem Taue,", "tokens": ["Mich", "freut's", ",", "die", "Brust", "ent\u00b7bl\u00f6\u00dft", "dem", "Tau\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gebettet unter Gras und Ried,", "tokens": ["Ge\u00b7bet\u00b7tet", "un\u00b7ter", "Gras", "und", "Ried", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu schmettern weit ins Himmelblaue", "tokens": ["Zu", "schmet\u00b7tern", "weit", "ins", "Him\u00b7mel\u00b7blau\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein rechtes deutsches Heldenlied.", "tokens": ["Ein", "rech\u00b7tes", "deut\u00b7sches", "Hel\u00b7den\u00b7lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mich freut's, im Forst am Erlenteiche", "tokens": ["Mich", "freut's", ",", "im", "Forst", "am", "Er\u00b7len\u00b7tei\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu lauschen, wenn der Hirsch sich k\u00fchlt,", "tokens": ["Zu", "lau\u00b7schen", ",", "wenn", "der", "Hirsch", "sich", "k\u00fchlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn klatschend an die Binsenstr\u00e4uche", "tokens": ["Wenn", "klat\u00b7schend", "an", "die", "Bin\u00b7sen\u00b7str\u00e4u\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das gr\u00fcnliche Gewoge sp\u00fclt.", "tokens": ["Das", "gr\u00fcn\u00b7li\u00b7che", "Ge\u00b7wo\u00b7ge", "sp\u00fclt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich freut's, wenn Eure Glocken hallen", "tokens": ["Mich", "freut's", ",", "wenn", "Eu\u00b7re", "Glo\u00b7cken", "hal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und neblig wallt der Weihrauchduft,", "tokens": ["Und", "neb\u00b7lig", "wallt", "der", "Weih\u00b7rauch\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In lichter Au' aufs Knie zu fallen,", "tokens": ["In", "lich\u00b7ter", "Au'", "aufs", "Knie", "zu", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Stillbetend in die Sonntagsluft. \u2013", "tokens": ["Still\u00b7be\u00b7tend", "in", "die", "Sonn\u00b7tags\u00b7luft", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es rollt die Welt auf glatten Schienen,", "tokens": ["Es", "rollt", "die", "Welt", "auf", "glat\u00b7ten", "Schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich freut's, zu gehn durchs hohe Gras,", "tokens": ["Mich", "freut's", ",", "zu", "gehn", "durchs", "ho\u00b7he", "Gras", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PTKZU", "VVINF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bin ich Euch als Narr erschienen,", "tokens": ["Und", "bin", "ich", "Euch", "als", "Narr", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So denkt: es ist einmal sein Spa\u00df!", "tokens": ["So", "denkt", ":", "es", "ist", "ein\u00b7mal", "sein", "Spa\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr seid f\u00fcr heut die Herrn der Erden,", "tokens": ["Ihr", "seid", "f\u00fcr", "heut", "die", "Herrn", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich kann Euch nicht beneiden, seid's!", "tokens": ["Ich", "kann", "Euch", "nicht", "be\u00b7nei\u00b7den", ",", "sei\u00b7d's", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich aber m\u00f6cht' ein Dichter werden,", "tokens": ["Ich", "a\u00b7ber", "m\u00f6cht'", "ein", "Dich\u00b7ter", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und meine Antwort ist: Mich freut's!", "tokens": ["Und", "mei\u00b7ne", "Ant\u00b7wort", "ist", ":", "Mich", "freut's", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was fliehst Du uns mit trotzigen Mienen,", "tokens": ["Was", "fliehst", "Du", "uns", "mit", "trot\u00b7zi\u00b7gen", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir sind so klug, wir sind so reich,", "tokens": ["Wir", "sind", "so", "klug", ",", "wir", "sind", "so", "reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es rollt die Welt auf glatten Schienen,", "tokens": ["Es", "rollt", "die", "Welt", "auf", "glat\u00b7ten", "Schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was rennst Du quer durch Sumpf und Teich? \u2013", "tokens": ["Was", "rennst", "Du", "quer", "durch", "Sumpf", "und", "Teich", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u2013 O, la\u00dft die Fragen, klug verbindlic