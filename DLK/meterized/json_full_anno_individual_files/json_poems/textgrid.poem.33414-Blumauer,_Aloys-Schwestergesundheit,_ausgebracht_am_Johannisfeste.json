{"textgrid.poem.33414": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Schwestergesundheit, ausgebracht am Johannisfeste", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Eintracht und der Schwestern Preis,", "tokens": ["Der", "Ein\u00b7tracht", "und", "der", "Schwes\u00b7tern", "Preis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer beide zu vereinen wei\u00df,", "tokens": ["Wer", "bei\u00b7de", "zu", "ver\u00b7ei\u00b7nen", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist nicht genug zu preisen:", "tokens": ["Ist", "nicht", "ge\u00b7nug", "zu", "prei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als Bruder stets um Schwestern sein,", "tokens": ["Als", "Bru\u00b7der", "stets", "um", "Schwes\u00b7tern", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nie mit ihnen sich entzwei'n,", "tokens": ["Und", "nie", "mit", "ih\u00b7nen", "sich", "ent\u00b7zwei'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das ist der Stein der Weisen.", "tokens": ["Das", "ist", "der", "Stein", "der", "Wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Schwestern gruben zwar den Stein", "tokens": ["Die", "Schwes\u00b7tern", "gru\u00b7ben", "zwar", "den", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar tief in ihre Herzen ein,", "tokens": ["Gar", "tief", "in", "ih\u00b7re", "Her\u00b7zen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df wir ihn nicht ergr\u00fcnden;", "tokens": ["Da\u00df", "wir", "ihn", "nicht", "er\u00b7gr\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Allein das schreckt den Maurer nicht:", "tokens": ["Al\u00b7lein", "das", "schreckt", "den", "Mau\u00b7rer", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er wird bei seiner Sonne Licht", "tokens": ["Er", "wird", "bei", "sei\u00b7ner", "Son\u00b7ne", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Stein gewi\u00df noch finden.", "tokens": ["Den", "Stein", "ge\u00b7wi\u00df", "noch", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch bis der edle Diamant,", "tokens": ["Doch", "bis", "der", "ed\u00b7le", "Di\u00b7a\u00b7mant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der allen Zwist und Hader bannt,", "tokens": ["Der", "al\u00b7len", "Zwist", "und", "Ha\u00b7der", "bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch uns're Kunst erschienen,", "tokens": ["Durch", "un\u00b7s'\u00b7re", "Kunst", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Will ich, wie jeder Maurer soll,", "tokens": ["Will", "ich", ",", "wie", "je\u00b7der", "Mau\u00b7rer", "soll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "PIAT", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu unserm und der Schwestern Wohl", "tokens": ["Zu", "un\u00b7serm", "und", "der", "Schwes\u00b7tern", "Wohl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit frommen W\u00fcnschen dienen.", "tokens": ["Mit", "from\u00b7men", "W\u00fcn\u00b7schen", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn Vorwitz eine Schwester sticht", "tokens": ["Wenn", "Vor\u00b7witz", "ei\u00b7ne", "Schwes\u00b7ter", "sticht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Dingen, die uns Eid und Pflicht", "tokens": ["Nach", "Din\u00b7gen", ",", "die", "uns", "Eid", "und", "Pflicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu sagen nicht erlauben,", "tokens": ["Zu", "sa\u00b7gen", "nicht", "er\u00b7lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So gebet ihr ein X f\u00fcr U,", "tokens": ["So", "ge\u00b7bet", "ihr", "ein", "X", "f\u00fcr", "U", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "XY", "XY", "XY", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und macht ein ernst Gesicht dazu,", "tokens": ["Und", "macht", "ein", "ernst", "Ge\u00b7sicht", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und traun! sie wird es glauben.", "tokens": ["Und", "traun", "!", "sie", "wird", "es", "glau\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn Schwestern b\u00f6ser Laune sind,", "tokens": ["Wenn", "Schwes\u00b7tern", "b\u00f6\u00b7ser", "Lau\u00b7ne", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mit Gemahl und Hausgesind'", "tokens": ["Und", "mit", "Ge\u00b7mahl", "und", "Haus\u00b7ge\u00b7sind'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus langer Weile keifen,", "tokens": ["Aus", "lan\u00b7ger", "Wei\u00b7le", "kei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So m\u00fc\u00dft ihr taub und f\u00fchllos sein,", "tokens": ["So", "m\u00fc\u00dft", "ihr", "taub", "und", "f\u00fchl\u00b7los", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und euch bem\u00fch'n, den rauhen Stein", "tokens": ["Und", "euch", "be\u00b7m\u00fch'n", ",", "den", "rau\u00b7hen", "Stein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Job's Geduld zu schleifen.", "tokens": ["Mit", "Job's", "Ge\u00b7duld", "zu", "schlei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn Schwestern eifers\u00fcchtig schm\u00e4h'n,", "tokens": ["Wenn", "Schwes\u00b7tern", "ei\u00b7fer\u00b7s\u00fcch\u00b7tig", "schm\u00e4h'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und euch Gardinenpredigten", "tokens": ["Und", "euch", "Gar\u00b7di\u00b7nen\u00b7pre\u00b7dig\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "NN"], "meter": "-+-+----", "measure": "unknown.measure.di"}, "line.3": {"text": "Um eu're Ohren sausen,", "tokens": ["Um", "eu'\u00b7re", "Oh\u00b7ren", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So denket, da\u00df hier vor Gericht", "tokens": ["So", "den\u00b7ket", ",", "da\u00df", "hier", "vor", "Ge\u00b7richt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ADV", "APPR", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Ein lauer Bruder Redner spricht,", "tokens": ["Ein", "lau\u00b7er", "Bru\u00b7der", "Red\u00b7ner", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schlaft ein, und la\u00dft euch zausen.", "tokens": ["Schlaft", "ein", ",", "und", "la\u00dft", "euch", "zau\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wenn Zorn in Schwesteraugen gl\u00fcht,", "tokens": ["Wenn", "Zorn", "in", "Schwes\u00b7ter\u00b7au\u00b7gen", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus ihrem Munde Feuer spr\u00fcht,", "tokens": ["Aus", "ih\u00b7rem", "Mun\u00b7de", "Feu\u00b7er", "spr\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und euch in Schrecken setzet,", "tokens": ["Und", "euch", "in", "Schre\u00b7cken", "set\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geht wacker zu, seht euch nicht um;", "tokens": ["Geht", "wa\u00b7cker", "zu", ",", "seht", "euch", "nicht", "um", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "'s ist eitel Colophonium,", "tokens": ["'s", "ist", "ei\u00b7tel", "Co\u00b7lo\u00b7pho\u00b7ni\u00b7um", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Das euch kein Haar verletzet.", "tokens": ["Das", "euch", "kein", "Haar", "ver\u00b7let\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wenn eine Schwester seitw\u00e4rts schielt,", "tokens": ["Wenn", "ei\u00b7ne", "Schwes\u00b7ter", "seit\u00b7w\u00e4rts", "schielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Hangen und Verlangen spielt,", "tokens": ["Und", "Han\u00b7gen", "und", "Ver\u00b7lan\u00b7gen", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So nehmt geschwind die Binde,", "tokens": ["So", "nehmt", "ge\u00b7schwind", "die", "Bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und h\u00fctet euch vor'm Tageslicht;", "tokens": ["Und", "h\u00fc\u00b7tet", "euch", "vor'm", "Ta\u00b7ges\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Seh'nde geht hier lange nicht", "tokens": ["Der", "Seh'n\u00b7de", "geht", "hier", "lan\u00b7ge", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So sicher als der Blinde.", "tokens": ["So", "si\u00b7cher", "als", "der", "Blin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wenn eine Schwester zankt, da\u00df sie", "tokens": ["Wenn", "ei\u00b7ne", "Schwes\u00b7ter", "zankt", ",", "da\u00df", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht Erben kriegt, so machet nie", "tokens": ["Nicht", "Er\u00b7ben", "kriegt", ",", "so", "ma\u00b7chet", "nie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "NN", "VVFIN", "$,", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Zank das Uebel gr\u00f6sser;", "tokens": ["Durch", "Zank", "das", "Ue\u00b7bel", "gr\u00f6s\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beruhigt sie f\u00fcr diesen Fall,", "tokens": ["Be\u00b7ru\u00b7higt", "sie", "f\u00fcr", "die\u00b7sen", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und machet f\u00fcr ein andermal", "tokens": ["Und", "ma\u00b7chet", "f\u00fcr", "ein", "an\u00b7der\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Eh'standsarbeit besser.", "tokens": ["Die", "Eh'\u00b7stands\u00b7a\u00b7rbeit", "bes\u00b7ser", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wenn's eine gar zu bitter macht,", "tokens": ["Wenn's", "ei\u00b7ne", "gar", "zu", "bit\u00b7ter", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ohn' Erbarmen Tag und Nacht", "tokens": ["Und", "ohn'", "Er\u00b7bar\u00b7men", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Ha\u00df und Lieb' euch qu\u00e4let,", "tokens": ["Mit", "Ha\u00df", "und", "Lieb'", "euch", "qu\u00e4\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So duldet's ohne Ach! und Weh!", "tokens": ["So", "dul\u00b7det's", "oh\u00b7ne", "Ach", "!", 