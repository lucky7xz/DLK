{"textgrid.poem.59505": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Grabe", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erde, wie du lebest und gr\u00fcnst!", "tokens": ["Er\u00b7de", ",", "wie", "du", "le\u00b7best", "und", "gr\u00fcnst", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Hast das Grab der Liebe umsponnen", "tokens": ["Hast", "das", "Grab", "der", "Lie\u00b7be", "um\u00b7spon\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Lustig mit Blumen bunt, mit gr\u00fcnen Gr\u00e4sern,", "tokens": ["Lus\u00b7tig", "mit", "Blu\u00b7men", "bunt", ",", "mit", "gr\u00fc\u00b7nen", "Gr\u00e4\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Webest Moos um die Steine.", "tokens": ["We\u00b7best", "Moos", "um", "die", "Stei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Aber, Tr\u00e4nen, flie\u00dfet darauf;", "tokens": ["A\u00b7ber", ",", "Tr\u00e4\u00b7nen", ",", "flie\u00b7\u00dfet", "da\u00b7rauf", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "VVFIN", "PAV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Denn den Schl\u00e4fer drinnen erquicket", "tokens": ["Denn", "den", "Schl\u00e4\u00b7fer", "drin\u00b7nen", "er\u00b7quic\u00b7ket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nimmer der Blumen Duft, er h\u00f6rt nicht das L\u00fcftchen,", "tokens": ["Nim\u00b7mer", "der", "Blu\u00b7men", "Duft", ",", "er", "h\u00f6rt", "nicht", "das", "L\u00fcft\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Das sein Lager ums\u00e4uselt.", "tokens": ["Das", "sein", "La\u00b7ger", "um\u00b7s\u00e4u\u00b7selt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVPP", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Weine nicht! es wandelt der Mond", "tokens": ["Wei\u00b7ne", "nicht", "!", "es", "wan\u00b7delt", "der", "Mond"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mit den stillen Sternen vor\u00fcber,", "tokens": ["Mit", "den", "stil\u00b7len", "Ster\u00b7nen", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gl\u00e4nzt auf das goldne Moos, die tauenden Gr\u00e4ser,", "tokens": ["Gl\u00e4nzt", "auf", "das", "gold\u00b7ne", "Moos", ",", "die", "tau\u00b7en\u00b7den", "Gr\u00e4\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word",