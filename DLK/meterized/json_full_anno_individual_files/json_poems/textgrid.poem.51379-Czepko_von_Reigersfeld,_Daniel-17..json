{"textgrid.poem.51379": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann durch der Flammen Krafft der Phoenix sich gebiert,", "tokens": ["Wann", "durch", "der", "Flam\u00b7men", "Krafft", "der", "Phoe\u00b7nix", "sich", "ge\u00b7biert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "ART", "NE", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sehn wir, da\u00df die Geburt von seinem Sterben r\u00fchrt.", "tokens": ["Sehn", "wir", ",", "da\u00df", "die", "Ge\u00b7burt", "von", "sei\u00b7nem", "Ster\u00b7ben", "r\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus seinem Grabe kan sich seine Wieg erheben,", "tokens": ["Aus", "sei\u00b7nem", "Gra\u00b7be", "kan", "sich", "sei\u00b7ne", "Wieg", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus seiner Asche springt und bricht hervor das Leben.", "tokens": ["Aus", "sei\u00b7ner", "A\u00b7sche", "springt", "und", "bricht", "her\u00b7vor", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So sterb und leb ich auch. Es machen mich, o Noth,", "tokens": ["So", "sterb", "und", "leb", "ich", "auch", ".", "Es", "ma\u00b7chen", "mich", ",", "o", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dein Augen lebendig, mein liebes Feuer todt.", "tokens": ["Dein", "Au\u00b7gen", "le\u00b7ben\u00b7dig", ",", "mein", "lie\u00b7bes", "Feu\u00b7er", "todt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wann durch der Flammen Krafft der Phoenix sich gebiert,", "tokens": ["Wann", "durch", "der", "Flam\u00b7men", "Krafft", "der", "Phoe\u00b7nix", "sich", "ge\u00b7biert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "ART", "NE", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sehn wir, da\u00df die Geburt von seinem Sterben r\u00fchrt.", "tokens": ["Sehn", "wir", ",", "da\u00df", "die", "Ge\u00b7burt", "von", "sei\u00b7nem", "Ster\u00b7ben", "r\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus seinem Grabe kan sich seine Wieg erheben,", "tokens": ["Aus", "sei\u00b7nem", "Gra\u00b7b