{"textgrid.poem.62335": {"metadata": {"author": {"name": "Gessner, Salomon", "birth": "N.A.", "death": "N.A."}, "title": "Daphne. Micon", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sage mir mein Geliebter, was soll dieser kleine Altar hier? Welcher Gottheit ist er wol heilig?", "tokens": ["Sa\u00b7ge", "mir", "mein", "Ge\u00b7lieb\u00b7ter", ",", "was", "soll", "die\u00b7ser", "klei\u00b7ne", "Al\u00b7tar", "hier", "?", "Wel\u00b7cher", "Got\u00b7theit", "ist", "er", "wol", "hei\u00b7lig", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,", "PWS", "VMFIN", "PDAT", "ADJA", "NN", "ADV", "$.", "PWAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+--+-+-+-+-+--+-+-+-+--+-", "measure": "iambic.octa.plus.invert"}}, "stanza.2": {"line.1": {"text": "Noch weis ichs; die kleinsten Bl\u00fcmgen der Wiese und der Flur pflanztest du drein.", "tokens": ["Noch", "weis", "ichs", ";", "die", "kleins\u00b7ten", "Bl\u00fcm\u00b7gen", "der", "Wie\u00b7se", "und", "der", "Flur", "pflanz\u00b7test", "du", "drein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "PIS", "$.", "ART", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Ja, nun wird er uns die gute Pflege belohnen.", "tokens": ["Ja", ",", "nun", "wird", "er", "uns", "die", "gu\u00b7te", "Pfle\u00b7ge", "be\u00b7loh\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "PPER", "PRF", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}}, "stanza.4": {"line.1": {"text": "So waren die Tage unsrer Kindheit honigs\u00fcsse, da zum Spiel ich dein Mann war, du mein Weib.", "tokens": ["So", "wa\u00b7ren", "die", "Ta\u00b7ge", "uns\u00b7rer", "Kind\u00b7heit", "ho\u00b7ni\u00b7gs\u00fcs\u00b7se", ",", "da", "zum", "Spiel", "ich", "dein", "Mann", "war", ",", "du", "mein", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "APPRART", "NN", "PPER", "PPOSAT", "NN", "VAFIN", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Auch mein graues Alter wird sie nicht vergessen.", "tokens": ["Auch", "mein", "grau\u00b7es", "Al\u00b7ter", "wird", "sie", "nicht", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.6": {"line.1": {"text": "Segnen die g\u00fctigen G\u00f6tter uns, dann, mein Geliebter, war Mann und Weib nie gl\u00fccklicher als wir.", "tokens": ["Seg\u00b7nen", "die", "g\u00fc\u00b7ti\u00b7gen", "G\u00f6t\u00b7ter", "uns", ",", "dann", ",", "mein", "Ge\u00b7lieb\u00b7ter", ",", "war", "Mann", "und", "Weib", "nie", "gl\u00fcck\u00b7li\u00b7cher", "als", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PPER", "$,", "ADV", "$,", "PPOSAT", "NN", "$,", "VAFIN", "NN", "KON", "NN", "ADV", "ADJD", "KOKOM", "PP