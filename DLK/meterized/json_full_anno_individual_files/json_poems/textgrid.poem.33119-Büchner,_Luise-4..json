{"textgrid.poem.33119": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manchen Tag noch zog der wackre Reiter", "tokens": ["Man\u00b7chen", "Tag", "noch", "zog", "der", "wack\u00b7re", "Rei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Durch der Berge Labyrinth,", "tokens": ["Durch", "der", "Ber\u00b7ge", "La\u00b7by\u00b7rinth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis sie \u00f6ffnen weiter sich und weiter,", "tokens": ["Bis", "sie", "\u00f6ff\u00b7nen", "wei\u00b7ter", "sich", "und", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "PRF", "KON", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und als einst die Nacht zerrinnt,", "tokens": ["Und", "als", "einst", "die", "Nacht", "zer\u00b7rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Als die Morgenl\u00fcste frischer schwellen,", "tokens": ["Als", "die", "Mor\u00b7gen\u00b7l\u00fcs\u00b7te", "fri\u00b7scher", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schaut er in das offne Land,", "tokens": ["Schaut", "er", "in", "das", "off\u00b7ne", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die blauen, wundersamen Wellen", "tokens": ["Wo", "die", "blau\u00b7en", ",", "wun\u00b7der\u00b7sa\u00b7men", "Wel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Rollt der Leman an den Strand.", "tokens": ["Rollt", "der", "Le\u00b7man", "an", "den", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie ein Traum voll Grausen und Entz\u00fccken", "tokens": ["Wie", "ein", "Traum", "voll", "Grau\u00b7sen", "und", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hinter ihm die M\u00fch' entweicht,", "tokens": ["Hin\u00b7ter", "ihm", "die", "M\u00fch'", "ent\u00b7weicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Welt entrollt sich seinen Blicken,", "tokens": ["Ei\u00b7ne", "Welt", "ent\u00b7rollt", "sich", "sei\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Fast an Sch\u00f6nheit unerreicht:", "tokens": ["Fast", "an", "Sch\u00f6n\u00b7heit", "un\u00b7er\u00b7reicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Einen duft'gen Zauberschleier weben", "tokens": ["Ei\u00b7nen", "duft'\u00b7gen", "Zau\u00b7ber\u00b7schlei\u00b7er", "we\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lorbeer- und Kastaniengr\u00fcn,", "tokens": ["Lor\u00b7beer", "und", "Kas\u00b7ta\u00b7ni\u00b7en\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Hinter dem die blauen Fluthen schweben,", "tokens": ["Hin\u00b7ter", "dem", "die", "blau\u00b7en", "Flut\u00b7hen", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zitternd auf und niederspr\u00fch'n.", "tokens": ["Zit\u00b7ternd", "auf", "und", "nie\u00b7der\u00b7spr\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["VVPP", "PTKVZ", "KON", "XY", "XY"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hat der See den Himmel eingetrunken,", "tokens": ["Hat", "der", "See", "den", "Him\u00b7mel", "ein\u00b7ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df er strahlt in solchem Blau?", "tokens": ["Da\u00df", "er", "strahlt", "in", "sol\u00b7chem", "Blau", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu der Erde scheint der hingesunken,", "tokens": ["Zu", "der", "Er\u00b7de", "scheint", "der", "hin\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Huldigend der hohen Frau,", "tokens": ["Hul\u00b7di\u00b7gend", "der", "ho\u00b7hen", "Frau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Welche hier an Majest\u00e4t und Sch\u00f6ne", "tokens": ["Wel\u00b7che", "hier", "an", "Ma\u00b7jes\u00b7t\u00e4t", "und", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gleicht der stolzen Juno, wie", "tokens": ["Gleicht", "der", "stol\u00b7zen", "Ju\u00b7no", ",", "wie"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PWAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie, damit der h\u00f6chste Reiz sie kr\u00f6ne,", "tokens": ["Sie", ",", "da\u00b7mit", "der", "h\u00f6chs\u00b7te", "Reiz", "sie", "kr\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Sich der Anmuth G\u00fcrtel lieh.", "tokens": ["Sich", "der", "An\u00b7muth", "G\u00fcr\u00b7tel", "lieh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "So erhaben und so hold und s\u00fc\u00dfe,", "tokens": ["So", "er\u00b7ha\u00b7ben", "und", "so", "hold", "und", "s\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Den entz\u00fcckten Blick sie gr\u00fc\u00dft,", "tokens": ["Den", "ent\u00b7z\u00fcck\u00b7ten", "Blick", "sie", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rebenh\u00fcgel kr\u00e4nzen ihre F\u00fc\u00dfe", "tokens": ["Re\u00b7ben\u00b7h\u00fc\u00b7gel", "kr\u00e4n\u00b7zen", "ih\u00b7re", "F\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und das Haupt die Sterne k\u00fc\u00dft.", "tokens": ["Und", "das", "Haupt", "die", "Ster\u00b7ne", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie sie Segen giebt mit vollen H\u00e4nden,", "tokens": ["Wie", "sie", "Se\u00b7gen", "giebt", "mit", "vol\u00b7len", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zeigt des Menschen friedlich Haus,", "tokens": ["Zeigt", "des", "Men\u00b7schen", "fried\u00b7lich", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hingestreut an allen H\u00fcgelw\u00e4nden,", "tokens": ["Hin\u00b7ge\u00b7streut", "an", "al\u00b7len", "H\u00fc\u00b7gel\u00b7w\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Baumumschattet lugt's heraus. \u2013", "tokens": ["Bau\u00b7mum\u00b7schat\u00b7tet", "lugt's", "he\u00b7raus", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "NE", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Doch nicht lang mag jetzt der Ritter schauen,", "tokens": ["Doch", "nicht", "lang", "mag", "jetzt", "der", "Rit\u00b7ter", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auf springt er nach kurzer Rast.", "tokens": ["Auf", "springt", "er", "nach", "kur\u00b7zer", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Eilen mu\u00df er zu den holden Frauen,", "tokens": ["Ei\u00b7len", "mu\u00df", "er", "zu", "den", "hol\u00b7den", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Denn am Ziele ist er fast.", "tokens": ["Denn", "am", "Zie\u00b7le", "ist", "er", "fast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wie der Adler stolz sein Nest nur klebet", "tokens": ["Wie", "der", "Ad\u00b7ler", "stolz", "sein", "Nest", "nur", "kle\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "VAINF", "VVFIN", "ADV", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "An den Fels, in Tiefen nie,", "tokens": ["An", "den", "Fels", ",", "in", "Tie\u00b7fen", "nie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So auf schroffer Bergwand dort erhebet", "tokens": ["So", "auf", "schrof\u00b7fer", "Berg\u00b7wand", "dort", "er\u00b7he\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sich das Schlo\u00df von Meillerie.", "tokens": ["Sich", "das", "Schlo\u00df", "von", "Meil\u00b7le\u00b7rie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "An der Pforte fraget er beklommen", "tokens": ["An", "der", "Pfor\u00b7te", "fra\u00b7get", "er", "be\u00b7klom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nach der Dame von Blonay,", "tokens": ["Nach", "der", "Da\u00b7me", "von", "Blo\u00b7nay", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Schlecht will ihm darauf die Antwort frommen,", "tokens": ["Schlecht", "will", "ihm", "da\u00b7rauf", "die", "Ant\u00b7wort", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PAV", "ART", "NN", "ADJA", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Da\u00df sie dr\u00fcben \u00fcberm See.", "tokens": ["Da\u00df", "sie", "dr\u00fc\u00b7ben", "\u00fc\u00b7berm", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Kann er immer sich noch nicht entlasten", "tokens": ["Kann", "er", "im\u00b7mer", "sich", "noch", "nicht", "ent\u00b7las\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PRF", "ADV", "PTKNEG", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von der auferlegten Schuld?", "tokens": ["Von", "der", "auf\u00b7er\u00b7leg\u00b7ten", "Schuld", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen Kahn sieht er am Strande rasten,", "tokens": ["Ei\u00b7nen", "Kahn", "sieht", "er", "am", "Stran\u00b7de", "ras\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Springt hinein voll Ungeduld,", "tokens": ["Springt", "hin\u00b7ein", "voll", "Un\u00b7ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Fa\u00dft das Ruder an mit starken H\u00e4nden,", "tokens": ["Fa\u00dft", "das", "Ru\u00b7der", "an", "mit", "star\u00b7ken", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Theilt die klare Zauberfluth;", "tokens": ["Theilt", "die", "kla\u00b7re", "Zau\u00b7ber\u00b7fluth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und hin\u00fcber nach den Rebgel\u00e4nden", "tokens": ["Und", "hin\u00b7\u00fc\u00b7ber", "nach", "den", "Reb\u00b7ge\u00b7l\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Steuert er sein Schifflein gut.", "tokens": ["Steu\u00b7ert", "er", "sein", "Schif\u00b7flein", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Lustig regt der Brise frisches Wehen", "tokens": ["Lus\u00b7tig", "regt", "der", "Bri\u00b7se", "fri\u00b7sches", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Rings ein stolzes Wellenheer,", "tokens": ["Rings", "ein", "stol\u00b7zes", "Wel\u00b7len\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00e4nzend rieseln von den Wasserh\u00f6hen", "tokens": ["Gl\u00e4n\u00b7zend", "rie\u00b7seln", "von", "den", "Was\u00b7ser\u00b7h\u00