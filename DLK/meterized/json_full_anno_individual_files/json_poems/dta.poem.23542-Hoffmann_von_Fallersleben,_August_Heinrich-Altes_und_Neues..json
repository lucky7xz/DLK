{"dta.poem.23542": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Altes und Neues.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Tod dem Alten, Tod dem Neuen,", "tokens": ["Tod", "dem", "Al\u00b7ten", ",", "Tod", "dem", "Neu\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Allem was uns trennen mu\u00df!", "tokens": ["Al\u00b7lem", "was", "uns", "tren\u00b7nen", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprecht nicht mehr von Luthers Siege,", "tokens": ["Sprecht", "nicht", "mehr", "von", "Lu\u00b7thers", "Sie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht vom drei\u00dfigj\u00e4hr'gen Kriege", "tokens": ["Nicht", "vom", "drei\u00b7\u00dfig\u00b7j\u00e4hr'\u00b7gen", "Krie\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und westph\u00e4lschen Friedensschlu\u00df.", "tokens": ["Und", "west\u00b7ph\u00e4l\u00b7schen", "Frie\u00b7dens\u00b7schlu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Tod dem Alten, Tod dem Neuen,", "tokens": ["Tod", "dem", "Al\u00b7ten", ",", "Tod", "dem", "Neu\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00fcber nur die Menschheit weint!", "tokens": ["Dr\u00fc\u00b7ber", "nur", "die", "Menschheit", "weint", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sprecht nicht mehr von Adelsrechten", "tokens": ["Sprecht", "nicht", "mehr", "von", "A\u00b7dels\u00b7rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wie Deutsche mussten fechten", "tokens": ["Und", "wie", "Deut\u00b7sche", "muss\u00b7ten", "fech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wider Deutsche f\u00fcr den Feind.", "tokens": ["Wi\u00b7der", "Deut\u00b7sche", "f\u00fcr", "den", "Feind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Tod dem Alten, Tod dem Neuen,", "tokens": ["Tod", "dem", "Al\u00b7ten", ",", "Tod", "dem", "Neu\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was uns trennt von Recht und Pflicht!", "tokens": ["Was", "uns", "trennt", "von", "Recht", "und", "Pflicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deutschlands Alter, Deutschlands Jugend", "tokens": ["Deutschlands", "Al\u00b7ter", ",", "Deutschlands", "Ju\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sei ein fester Bund der Tugend,", "tokens": ["Sei", "ein", "fes\u00b7ter", "Bund", "der", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dran des Feindes Macht zerbricht!", "tokens": ["Dran", "des", "Fein\u00b7des", "Macht", "zer\u00b7bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Heil dem Alten, Heil dem Neuen,", "tokens": ["Heil", "dem", "Al\u00b7ten", ",", "Heil", "dem", "Neu\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was uns f\u00fchrt zu Recht und Pflicht!", "tokens": ["Was", "uns", "f\u00fchrt", "zu", "Recht", "und", "Pflicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lasst die Jungen und die Alten", "tokens": ["Lasst", "die", "Jun\u00b7gen", "und", "die", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frei auf diesem Pfade walten \u2014", "tokens": ["Frei", "auf", "die\u00b7sem", "Pfa\u00b7de", "wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ihr F\u00fcrsten, wehrt sie nicht!", "tokens": ["Und", "ihr", "F\u00fcrs\u00b7ten", ",", "wehrt", "sie", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}