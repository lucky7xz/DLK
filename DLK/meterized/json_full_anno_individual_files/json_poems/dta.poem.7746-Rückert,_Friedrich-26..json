{"dta.poem.7746": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er hat in seinem Land das Gl\u00fcckspiel untersagt,", "tokens": ["Er", "hat", "in", "sei\u00b7nem", "Land", "das", "Gl\u00fcck\u00b7spiel", "un\u00b7ter\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch das noch Niemand hat ein ernstlich Gl\u00fcck erjagt.", "tokens": ["Durch", "das", "noch", "Nie\u00b7mand", "hat", "ein", "ernst\u00b7lich", "Gl\u00fcck", "er\u00b7jagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "PIS", "VAFIN", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er weist das wankle Gl\u00fcck von seinem Land zur\u00fcck,", "tokens": ["Er", "weist", "das", "wank\u00b7le", "Gl\u00fcck", "von", "sei\u00b7nem", "Land", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil selbst er ohne Wank will machen dessen Gl\u00fcck.", "tokens": ["Weil", "selbst", "er", "oh\u00b7ne", "Wank", "will", "ma\u00b7chen", "des\u00b7sen", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "APPR", "NN", "VMFIN", "VVINF", "PRELAT", "NN", "