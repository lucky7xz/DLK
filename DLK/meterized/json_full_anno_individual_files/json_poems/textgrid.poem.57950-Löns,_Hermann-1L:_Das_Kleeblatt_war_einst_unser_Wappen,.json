{"textgrid.poem.57950": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Kleeblatt war einst unser Wappen,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Kleeblatt war einst unser Wappen,", "tokens": ["Das", "Klee\u00b7blatt", "war", "einst", "un\u00b7ser", "Wap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Botanisch dachten fr\u00fcher wir,", "tokens": ["Bo\u00b7ta\u00b7nisch", "dach\u00b7ten", "fr\u00fc\u00b7her", "wir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zoologisch sind wir nun geworden,", "tokens": ["Zoo\u00b7lo\u00b7gisch", "sind", "wir", "nun", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "VAPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Im Wappen sind der Viecher zwier.", "tokens": ["Im", "Wap\u00b7pen", "sind", "der", "Vie\u00b7cher", "zwier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das eine ist ein kleiner Affe,", "tokens": ["Das", "ei\u00b7ne", "ist", "ein", "klei\u00b7ner", "Af\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der froh lebt in den Tag hinein,", "tokens": ["Der", "froh", "lebt", "in", "den", "Tag", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn er was hat, dann ist er fr\u00f6hlich,", "tokens": ["Wenn", "er", "was", "hat", ",", "dann", "ist", "er", "fr\u00f6h\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Kostenpunkt macht ihm nicht Pein.", "tokens": ["Der", "Kos\u00b7ten\u00b7punkt", "macht", "ihm", "nicht", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Das andre aber ist ein Kater,", "tokens": ["Das", "and\u00b7re", "a\u00b7ber", "ist", "ein", "Ka\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des grauen Elends grimmes Bild,", "tokens": ["Des", "grau\u00b7en", "E\u00b7lends", "grim\u00b7mes", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt sitzt er da noch sanft und friedlich,", "tokens": ["Jetzt", "sitzt", "er", "da", "noch", "sanft", "und", "fried\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit