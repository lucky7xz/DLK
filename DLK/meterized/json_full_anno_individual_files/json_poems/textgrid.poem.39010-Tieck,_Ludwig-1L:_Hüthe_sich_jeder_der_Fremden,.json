{"textgrid.poem.39010": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00fcthe sich jeder der Fremden,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00fcthe sich jeder der Fremden,", "tokens": ["H\u00fc\u00b7the", "sich", "je\u00b7der", "der", "Frem\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "PIS", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Freitags dem Pappelthor,", "tokens": ["Frei\u00b7tags", "dem", "Pap\u00b7pel\u00b7thor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Den Pl\u00e4tzen ohne Vorsicht zu nahn,", "tokens": ["Den", "Pl\u00e4t\u00b7zen", "oh\u00b7ne", "Vor\u00b7sicht", "zu", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "ADJA", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Denn wilder Stiere Heerden,", "tokens": ["Denn", "wil\u00b7der", "Stie\u00b7re", "Heer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Treibt ein unb\u00e4ndig Volk", "tokens": ["Treibt", "ein", "un\u00b7b\u00e4n\u00b7dig", "Volk"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Auf eilenden Rossen,", "tokens": ["Auf", "ei\u00b7len\u00b7den", "Ros\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Mit langen St\u00e4ben bewaffnet,", "tokens": ["Mit", "lan\u00b7gen", "St\u00e4\u00b7ben", "be\u00b7waff\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Rasch durch die Stadt;", "tokens": ["Rasch", "durch", "die", "Stadt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Nachschleppen die langen Seile,", "tokens": ["Nach\u00b7schlep\u00b7pen", "die", "lan\u00b7gen", "Sei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Um die Thiere zu halten", "tokens": ["Um", "die", "Thie\u00b7re", "zu", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Und die Str\u00e4ubenden zu lenken", "tokens": ["Und", "die", "Str\u00e4u\u00b7ben\u00b7den", "zu", "len\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "In die Thore des Hauses,", "tokens": ["In", "die", "Tho\u00b7re", "des", "Hau\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Wo sie als Opfer fallen.", "tokens": ["Wo", "sie", "als", "Op\u00b7fer", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Oft st\u00fcrzt die aufgeh\u00e4ufte Frucht", "tokens": ["Oft", "st\u00fcrzt", "die", "auf\u00b7ge\u00b7h\u00e4uf\u00b7te", "Frucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Sammt der alten W\u00e4chterin", "tokens": ["Sammt", "der", "al\u00b7ten", "W\u00e4ch\u00b7te\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Vor den Ungeb\u00e4ndigten zusammen,", "tokens": ["Vor", "den", "Un\u00b7ge\u00b7b\u00e4n\u00b7dig\u00b7ten", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Und die Jugend lacht", "tokens": ["Und", "die", "Ju\u00b7gend", "lacht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.18": {"text": "In Furcht und Schadenfreude.", "tokens": ["In", "Furcht", "und", "Scha\u00b7den\u00b7freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "An meinem Stabe hinkend,", "tokens": ["An", "mei\u00b7nem", "Sta\u00b7be", "hin\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Schlich ich neugeschm\u00fcckt", "tokens": ["Schlich", "ich", "neu\u00b7ge\u00b7schm\u00fcckt"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "PPER", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.21": {"text": "Durch die sonnbegl\u00e4nzten Gassen,", "tokens": ["Durch", "die", "sonn\u00b7be\u00b7gl\u00e4nz\u00b7ten", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Dem vornehmen Freunde meinen Gru\u00df zu sagen.", "tokens": ["Dem", "vor\u00b7neh\u00b7men", "Freun\u00b7de", "mei\u00b7nen", "Gru\u00df", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.23": {"text": "Hinter mir Get\u00fcmmel, \u2013", "tokens": ["Hin\u00b7ter", "mir", "Ge\u00b7t\u00fcm\u00b7mel", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "NN", "$,", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.24": {"text": "Geschrei, und irres Laufen, \u2013", "tokens": ["Ge\u00b7schrei", ",", "und", "ir\u00b7res", "Lau\u00b7fen", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "KON", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "An vielen Seilen", "tokens": ["An", "vie\u00b7len", "Sei\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "---+-", "measure": "unknown.measure.single"}, "line.26": {"text": "Wird ein Stier in die Stra\u00dfe gelenkt,", "tokens": ["Wird", "ein", "Stier", "in", "die", "Stra\u00b7\u00dfe", "ge\u00b7lenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.27": {"text": "Bald gerissen und gehemmt,", "tokens": ["Bald", "ge\u00b7ris\u00b7sen", "und", "ge\u00b7hemmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Bald in scheinbarer Freiheit,", "tokens": ["Bald", "in", "schein\u00b7ba\u00b7rer", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.29": {"text": "F\u00fchrt ihn das Schicksal", "tokens": ["F\u00fchrt", "ihn", "das", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.30": {"text": "Seiner Bestimmung entgegen.", "tokens": ["Sei\u00b7ner", "Be\u00b7stim\u00b7mung", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.31": {"text": "Schnell gewinn' ich ein Thor,", "tokens": ["Schnell", "ge\u00b7winn'", "ich", "ein", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.32": {"text": "Mich hinter die Pfosten zu bergen,", "tokens": ["Mich", "hin\u00b7ter", "die", "Pfos\u00b7ten", "zu", "ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.33": {"text": "Doch weh! \u2013 o Schrecken,", "tokens": ["Doch", "weh", "!", "\u2013", "o", "Schre\u00b7cken", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$.", "$(", "FM", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.34": {"text": "Die Freistatt, die ich w\u00e4hlte,", "tokens": ["Die", "Freis\u00b7tatt", ",", "die", "ich", "w\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Ist des Ge\u00e4ngsteten Opferstelle,", "tokens": ["Ist", "des", "Ge\u00b7\u00e4ngs\u00b7te\u00b7ten", "Op\u00b7fer\u00b7stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.36": {"text": "Frei sich w\u00e4hnend,", "tokens": ["Frei", "sich", "w\u00e4h\u00b7nend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.37": {"text": "St\u00fcrzt er herein,", "tokens": ["St\u00fcrzt", "er", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.38": {"text": "Kaum bleibt mir der Augenblick", "tokens": ["Kaum", "bleibt", "mir", "der", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.39": {"text": "Die Treppe hinauf zu fl\u00fcchten.", "tokens": ["Die", "Trep\u00b7pe", "hin\u00b7auf", "zu", "fl\u00fcch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Nach Hause kehr' ich erm\u00fcdet,", "tokens": ["Nach", "Hau\u00b7se", "kehr'", "ich", "er\u00b7m\u00fc\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den Besuch vergessend.", "tokens": ["Den", "Be\u00b7such", "ver\u00b7ges\u00b7send", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": 