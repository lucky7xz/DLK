", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.23": {"text": "Hinter mir Get\u00fcmmel, \u2013", "tokens": ["Hin\u00b7ter", "mir", "Ge\u00b7t\u00fcm\u00b7mel", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "NN", "$,", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.24": {"text": "Geschrei, und irres Laufen, \u2013", "tokens": ["Ge\u00b7schrei", ",", "und", "ir\u00b7res", "Lau\u00b7fen", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "KON", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "An vielen Seilen", "tokens": ["An", "vie\u00b7len", "Sei\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "---+-", "measure": "unknown.measure.single"}, "line.26": {"text": "Wird ein Stier in die Stra\u00dfe gelenkt,", "tokens": ["Wird", "ein", "Stier", "in", "die", "Stra\u00b7\u00dfe", "ge\u00b7lenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.27": {"text": "Bald gerissen und gehemmt,", "tokens": ["Bald", "ge\u00b7ris\u00b7sen", "und", "ge\u00b7hemmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Bald in scheinbarer Freiheit,", "tokens": ["Bald", "in", "schein\u00b7ba\u00b7rer", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.29": {"text": "F\u00fchrt ihn das Schicksal", "tokens": ["F\u00fchrt", "ihn", "das", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.30": {"text": "Seiner Bestimmung entgegen.", "tokens": ["Sei\u00b7ner", "Be\u00b7stim\u00b7mung", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.31": {"text": "Schnell gewinn' ich ein Thor,", "tokens": ["Schnell", "ge\u00b7winn'", "ich", "ein", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.32": {"text": "Mich hinter die Pfosten zu bergen,", "tokens": ["Mich", "hin\u00b7ter", "die", "Pfos\u00b7ten", "zu", "ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.33": {"text": "Doch weh! \u2013 o Schrecken,", "tokens": ["Doch", "weh", "!", "\u2013", "o", "Schre\u00b7cken", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$.", "$(", "FM", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.34": {"text": "Die Freistatt, die ich w\u00e4hlte,", "tokens": ["Die", "Freis\u00b7tatt", ",", "die", "ich", "w\u00e4hl\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Ist des Ge\u00e4ngsteten Opferstelle,", "tokens": ["Ist", "des", "Ge\u00b7\u00e4ngs\u00b7te\u00b7ten", "Op\u00b7fer\u00b7stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.36": {"text": "Frei sich w\u00e4hnend,", "tokens": ["Frei", "sich", "w\u00e4h\u00b7nend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.37": {"text": "St\u00fcrzt er herein,", "tokens": ["St\u00fcrzt", "er", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.38": {"text": "Kaum bleibt mir der Augenblick", "tokens": ["Kaum", "bleibt", "mir", "der", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.39": {"text": "Die Treppe hinauf zu fl\u00fcchten.", "tokens": ["Die", "Trep\u00b7pe", "hin\u00b7auf", "zu", "fl\u00fcch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Nach Hause kehr' ich erm\u00fcdet,", "tokens": ["Nach", "Hau\u00b7se", "kehr'", "ich", "er\u00b7m\u00fc\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den Besuch vergessend.", "tokens": ["Den", "Be\u00b7such", "ver\u00b7ges\u00b7send", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}