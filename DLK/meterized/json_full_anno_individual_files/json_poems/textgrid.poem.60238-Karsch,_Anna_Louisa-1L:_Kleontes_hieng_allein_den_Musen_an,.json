{"textgrid.poem.60238": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kleontes hieng allein den Musen an,", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kleontes hieng allein den Musen an,", "tokens": ["Kle\u00b7on\u00b7tes", "hieng", "al\u00b7lein", "den", "Mu\u00b7sen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihn reizten seines Landes T\u00f6chter nicht;", "tokens": ["Ihn", "reiz\u00b7ten", "sei\u00b7nes", "Lan\u00b7des", "T\u00f6ch\u00b7ter", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sch\u00f6n, wie der Blumeng\u00f6ttinn Lieblingskind,", "tokens": ["Sch\u00f6n", ",", "wie", "der", "Blu\u00b7men\u00b7g\u00f6t\u00b7tinn", "Lieb\u00b7lings\u00b7kind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Erschluf er in platonischer Entz\u00fcckung sich", "tokens": ["Er\u00b7schluf", "er", "in", "pla\u00b7to\u00b7ni\u00b7scher", "Ent\u00b7z\u00fc\u00b7ckung", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "PRF"], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Oft tr\u00e4umerisch ein reizend Ideal,", "tokens": ["Oft", "tr\u00e4u\u00b7me\u00b7risch", "ein", "rei\u00b7zend", "I\u00b7deal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das nirgend war, als nur in seiner Seele.", "tokens": ["Das", "nir\u00b7gend", "war", ",", "als", "nur", "in", "sei\u00b7ner", "See\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "$,", "KOUS", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Vergebens blickt er Jahre lang umher,", "tokens": ["Ver\u00b7ge\u00b7bens", "blickt", "er", "Jah\u00b7re", "lang", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und endlich fand sein Auge zum Erstaunen", "tokens": ["Und", "end\u00b7lich", "fand", "sein", "Au\u00b7ge", "zum", "Er\u00b7stau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Auf jener Flur, wo meine Thestylis", "tokens": ["Auf", "je\u00b7ner", "Flur", ",", "wo", "mei\u00b7ne", "Thes\u00b7ty\u00b7lis"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Gebohren ward, ein M\u00e4dchen, dessen Miene", "tokens": ["Ge\u00b7boh\u00b7ren", "ward", ",", "ein", "M\u00e4d\u00b7chen", ",", "des\u00b7sen", "Mie\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Dem L\u00e4cheln seiner Selbstgeschaffnen glich;", "tokens": ["Dem", "L\u00e4\u00b7ch