{"dta.poem.7979": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "3.  Die Nachtschw\u00e4rmer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Stille streif\u2019 ich durch die Gassen,", "tokens": ["Stil\u00b7le", "streif'", "ich", "durch", "die", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo sie wohnt, die blonde Kleine;", "tokens": ["Wo", "sie", "wohnt", ",", "die", "blon\u00b7de", "Klei\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch schon seh\u2019 ich Andre passen", "tokens": ["Doch", "schon", "seh'", "ich", "And\u00b7re", "pas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mir war\u2019s im D\u00e4mmerscheine,", "tokens": ["Und", "mir", "wa\u00b7r's", "im", "D\u00e4m\u00b7mer\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Einer w\u00fcrd\u2019 hineingelassen.", "tokens": ["Ei\u00b7ner", "w\u00fcrd'", "hin\u00b7ein\u00b7ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Regt es mir denn gleich die Galle,", "tokens": ["Regt", "es", "mir", "denn", "gleich", "die", "Gal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df sie Andern auch gefalle?", "tokens": ["Da\u00df", "sie", "An\u00b7dern", "auch", "ge\u00b7fal\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sey\u2019s! doch kann ich nicht verschweigen:", "tokens": ["Sey's", "!", "doch", "kann", "ich", "nicht", "ver\u00b7schwei\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Jeder hab\u2019 ein Liebchen eigen!", "tokens": ["Je\u00b7der", "hab'", "ein", "Lieb\u00b7chen", "ei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Eines schickt sich nicht f&#252;r Alle.", "tokens": ["Ei\u00b7nes", "schickt", "sich", "nicht", "f", "&#252;", "r", "Al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PTKNEG", "NE", "$(", "VVFIN", "PIS", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}