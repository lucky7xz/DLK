{"textgrid.poem.54532": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir zehlen was nicht ist und l\u00e4ngst in nichts verschwunden/", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir zehlen was nicht ist und l\u00e4ngst in nichts verschwunden/", "tokens": ["Wir", "zeh\u00b7len", "was", "nicht", "ist", "und", "l\u00e4ngst", "in", "nichts", "ver\u00b7schwun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PTKNEG", "VAFIN", "KON", "ADV", "APPR", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verwichner Zeiten Lauff und Menge vieler Jahr", "tokens": ["Ver\u00b7wich\u00b7ner", "Zei\u00b7ten", "Lauff", "und", "Men\u00b7ge", "vie\u00b7ler", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und was den Augenblick noch kaum verhanden war.", "tokens": ["Und", "was", "den", "Au\u00b7gen\u00b7blick", "noch", "kaum", "ver\u00b7han\u00b7den", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir zehlen was sich noch nicht von der Zeit gefunden.", "tokens": ["Wir", "zeh\u00b7len", "was", "sich", "noch", "nicht", "von", "der", "Zeit", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PRF", "ADV", "PTKNEG", "APPR", "ART", "NN", "VVPP", "$."], "mete