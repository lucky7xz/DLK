{"textgrid.poem.34346": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Sterblichen verg\u00f6nnet w\u00e4re", "tokens": ["Wenn", "Sterb\u00b7li\u00b7chen", "ver\u00b7g\u00f6n\u00b7net", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu seyn wozu der Dichter sie", "tokens": ["Zu", "seyn", "wo\u00b7zu", "der", "Dich\u00b7ter", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "PWAV", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Gotterhitzter Phantasie", "tokens": ["Mit", "Got\u00b7ter\u00b7hitz\u00b7ter", "Phan\u00b7ta\u00b7sie"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erschafft der Welt und sich zur Ehre;", "tokens": ["Er\u00b7schafft", "der", "Welt", "und", "sich", "zur", "Eh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und in des Waldgebirges Thal", "tokens": ["Und", "in", "des", "Wald\u00b7ge\u00b7bir\u00b7ges", "Thal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Versenkte sich in schwarzvertrauten Schatten,", "tokens": ["Ver\u00b7senk\u00b7te", "sich", "in", "schwarz\u00b7ver\u00b7trau\u00b7ten", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Um seiner Quaalen Wuth durch Duldung abzumatten,", "tokens": ["Um", "sei\u00b7ner", "Qua\u00b7a\u00b7len", "Wuth", "durch", "Dul\u00b7dung", "ab\u00b7zu\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "APPR", "NN", "VVIZU", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Ein heutiger ", "tokens": ["Ein", "heu\u00b7ti\u00b7ger"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und hofft' umsonst im dichtrischen Reviere", "tokens": ["Und", "hofft'", "um\u00b7sonst", "im", "dich\u00b7tri\u00b7schen", "Re\u00b7vie\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Den Tag, die Nacht, den andern Tag,", "tokens": ["Den", "Tag", ",", "die", "Nacht", ",", "den", "an\u00b7dern", "Tag", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Den dritten Tag, und Tag um Tag,", "tokens": ["Den", "drit\u00b7ten", "Tag", ",", "und", "Tag", "um", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Auf seine w\u00fcrkliche ", "tokens": ["Auf", "sei\u00b7ne", "w\u00fcrk\u00b7li\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.13": {"text": "Dem Armen, welchen Rath k\u00f6nnt ihm ein Menschenfreund,", "tokens": ["Dem", "Ar\u00b7men", ",", "wel\u00b7chen", "Rath", "k\u00f6nnt", "ihm", "ein", "Men\u00b7schen\u00b7freund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "K\u00f6nnt' ihm der Dichter selbst mit nassen Augen geben?", "tokens": ["K\u00f6nnt'", "ihm", "der", "Dich\u00b7ter", "selbst", "mit", "nas\u00b7sen", "Au\u00b7gen", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Vertraur', Ungl\u00fccklicher! dein hassensw\u00fcrdig Leben,", "tokens": ["Ver\u00b7traur'", ",", "Un\u00b7gl\u00fcck\u00b7li\u00b7cher", "!", "dein", "has\u00b7sens\u00b7w\u00fcr\u00b7dig", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und tr\u00e4gst du's l\u00e4nger nicht, so t\u00f6dte deinen Feind!", "tokens": ["Und", "tr\u00e4gst", "du's", "l\u00e4n\u00b7ger", "nicht", ",", "so", "t\u00f6d\u00b7te", "dei\u00b7nen", "Feind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADJD", "PTKNEG", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich aber w\u00fcste was ich riethe,", "tokens": ["Ich", "a\u00b7ber", "w\u00fcs\u00b7te", "was", "ich", "rie\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich dem der Halbgott Aeskulap,", "tokens": ["Ich", "dem", "der", "Halb\u00b7gott", "A\u00b7es\u00b7ku\u00b7lap", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ART", "NN", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Trotz Antiochus Arzt", "tokens": ["Trotz", "An\u00b7tio\u00b7chus", "Arzt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "----+", "measure": "unknown.measure.single"}, "line.4": {"text": "Und die ich auch nicht jedem Kranken biete:", "tokens": ["Und", "die", "ich", "auch", "nicht", "je\u00b7dem", "Kran\u00b7ken", "bie\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "ADV", "PTKNEG", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich sezte meinem lieben Schw\u00e4rmer", "tokens": ["Ich", "sez\u00b7te", "mei\u00b7nem", "lie\u00b7ben", "Schw\u00e4r\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein klein Spinetchen in sein Thal,", "tokens": ["Ein", "klein", "Spi\u00b7net\u00b7chen", "in", "sein", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und spielt' ihm auf dem kleinen L\u00e4rmer", "tokens": ["Und", "spielt'", "ihm", "auf", "dem", "klei\u00b7nen", "L\u00e4r\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Herzogin Musik einmal;", "tokens": ["Der", "Her\u00b7zo\u00b7gin", "Mu\u00b7sik", "ein\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und wenn dann mein Erwin aus seinen lezten Z\u00fcgen", "tokens": ["Und", "wenn", "dann", "mein", "Er\u00b7win", "aus", "sei\u00b7nen", "lez\u00b7ten", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nicht aufspr\u00e4ng' als ein junges Reh,", "tokens": ["Nicht", "auf\u00b7spr\u00e4ng'", "als", "ein", "jun\u00b7ges", "Reh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und sie allebend kommen s\u00e4h", "tokens": ["Und", "sie", "al\u00b7le\u00b7bend", "kom\u00b7men", "s\u00e4h"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Vom Berg herab ihm in die Arme fliegen,", "tokens": ["Vom", "Berg", "her\u00b7ab", "ihm", "in", "die", "Ar\u00b7me", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und schw\u00fcre nicht, da\u00df sie alliebend vor ihm steh", "tokens": ["Und", "schw\u00fc\u00b7re", "nicht", ",", "da\u00df", "sie", "al\u00b7lie\u00b7bend", "vor", "ihm", "steh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und er f\u00fcr Wohl an ihrer Brust vergeh;", "tokens": ["Und", "er", "f\u00fcr", "Wohl", "an", "ih\u00b7rer", "Brust", "ver\u00b7geh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "So wollt' ich unter seinem Weh", "tokens": ["So", "wollt'", "ich", "un\u00b7ter", "sei\u00b7nem", "Weh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Mit ihm ersinken und erliegen!", "tokens": ["Mit", "ihm", "er\u00b7sin\u00b7ken", "und", "er\u00b7lie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja ja, Durchlauchtigste, Du zauberst uns Elmiren", "tokens": ["Ja", "ja", ",", "Durch\u00b7lauch\u00b7tigs\u00b7te", ",", "Du", "zau\u00b7berst", "uns", "El\u00b7mi\u00b7ren"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "PTKANT", "$,", "ADJA", "$,", "PPER", "VVFIN", "PPER", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "In jede wilde W\u00fcsteney;", "tokens": ["In", "je\u00b7de", "wil\u00b7de", "W\u00fcs\u00b7te\u00b7ney", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und kann der Dichter uns in seelger Raserey", "tokens": ["Und", "kann", "der", "Dich\u00b7ter", "uns", "in", "seel\u00b7ger", "Ra\u00b7se\u00b7rey"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bis an des Todes Schwelle f\u00fchren:", "tokens": ["Bis", "an", "des", "To\u00b7des", "Schwel\u00b7le", "f\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So f\u00fchrst Du uns von da noch seeliger und lieber", "tokens": ["So", "f\u00fchrst", "Du", "uns", "von", "da", "noch", "see\u00b7li\u00b7ger", "und", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADV", "ADV", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bis nach Elysium hin\u00fcber.", "tokens": ["Bis", "nach", "E\u00b7ly\u00b7si\u00b7um", "hin\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NE", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn Sterblichen verg\u00f6nnet w\u00e4re", "tokens": ["Wenn", "Sterb\u00b7li\u00b7chen", "ver\u00b7g\u00f6n\u00b7net", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu seyn wozu der Dichter sie", "tokens": ["Zu", "seyn", "wo\u00b7zu", "der", "Dich\u00b7ter", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "PWAV", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Got