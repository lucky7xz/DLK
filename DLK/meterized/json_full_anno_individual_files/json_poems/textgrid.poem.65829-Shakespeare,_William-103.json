{"textgrid.poem.65829": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "103", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie arme Bl\u00fcten, ach! die Muse treibt,", "tokens": ["Wie", "ar\u00b7me", "Bl\u00fc\u00b7ten", ",", "ach", "!", "die", "Mu\u00b7se", "treibt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "ITJ", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df, mit so reichem Stoff mich zu befeuern,", "tokens": ["Da\u00df", ",", "mit", "so", "rei\u00b7chem", "Stoff", "mich", "zu", "be\u00b7feu\u00b7ern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ADV", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Gegenstand, ganz einfach, edler bleibt", "tokens": ["Der", "Ge\u00b7gen\u00b7stand", ",", "ganz", "ein\u00b7fach", ",", "ed\u00b7ler", "bleibt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "$,", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als wenn ihn r\u00fchmend meine Lieder feiern!", "tokens": ["Als", "wenn", "ihn", "r\u00fch\u00b7mend", "mei\u00b7ne", "Lie\u00b7der", "fei\u00b7ern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O schilt nicht, wenn ich mehr nicht schreiben kann!", "tokens": ["O", "schilt", "nicht", ",", "wenn", "ich", "mehr", "nicht", "schrei\u00b7ben", "kann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sieh in dein Glas, es wird dir Augen zeigen,", "tokens": ["Sieh", "in", "dein", "Glas", ",", "es", "wird", "dir", "Au\u00b7gen", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die, meinem bl\u00f6den Dichten weit voran,", "tokens": ["Die", ",", "mei\u00b7nem", "bl\u00f6\u00b7den", "Dich\u00b7ten", "weit", "vo\u00b7ran", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PPOSAT", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den Stab ihm brechen und die Farben bleichen.", "tokens": ["Den", "Stab", "ihm", "bre\u00b7chen", "und", "die", "Far\u00b7ben", "blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "W\u00e4r's dann nicht S\u00fcnde, wo man will erheben,", "tokens": ["W\u00e4r's", "dann", "nicht", "S\u00fcn\u00b7de", ",", "wo", "man", "will", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "NN", "$,", "PWAV", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Was gut vorher schon, zu verkleinlichen?", "tokens": ["Was", "gut", "vor\u00b7her",