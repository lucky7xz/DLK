{"textgrid.poem.37975": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1. Jungfrau und W\u00e4chter", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von hoher Art ein Fr\u00e4ulein zart,", "tokens": ["Von", "ho\u00b7her", "Art", "ein", "Fr\u00e4u\u00b7lein", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6rt ich dem W\u00e4chter klagen,", "tokens": ["H\u00f6rt", "ich", "dem", "W\u00e4ch\u00b7ter", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus Herzens-Qual, zum erstenmal", "tokens": ["Aus", "Her\u00b7zens\u00b7Qual", ",", "zum", "ers\u00b7ten\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPRART", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wollt sie die Liebe wagen,", "tokens": ["Wollt", "sie", "die", "Lie\u00b7be", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie sprach: \u00bbGeselle mein Ungef\u00e4lle", "tokens": ["Sie", "sprach", ":", "\u00bb", "Ge\u00b7sel\u00b7le", "mein", "Un\u00b7ge\u00b7f\u00e4l\u00b7le"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "PPOSAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ist nah und bringt mir Schmerzen,", "tokens": ["Ist", "nah", "und", "bringt", "mir", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach W\u00e4chter gut, ein argen Muth", "tokens": ["Ach", "W\u00e4ch\u00b7ter", "gut", ",", "ein", "ar\u00b7gen", "Muth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Trag ich in meinem Herzen.\u00ab", "tokens": ["Trag", "ich", "in", "mei\u00b7nem", "Her\u00b7zen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbeinem werthen Mann, dem w\u00fcnsch, ich an,", "tokens": ["\u00bb", "ei\u00b7nem", "wert\u00b7hen", "Mann", ",", "dem", "w\u00fcnsch", ",", "ich", "an", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "$,", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Viel Gl\u00fcck und Heil mit Treuen,", "tokens": ["Viel", "Gl\u00fcck", "und", "Heil", "mit", "Treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Tugend gro\u00df findt niemand blos,", "tokens": ["Sein", "Tu\u00b7gend", "gro\u00df", "findt", "nie\u00b7mand", "blos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf ihn ist wohl zu bauen,", "tokens": ["Auf", "ihn", "ist", "wohl", "zu", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Da\u00df er wohl sey alles Wandels frey,", "tokens": ["Da\u00df", "er", "wohl", "sey", "al\u00b7les", "Wan\u00b7dels", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Ein Mann von hohen Ehren.\u00ab", "tokens": ["Ein", "Mann", "von", "ho\u00b7hen", "Eh\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbo W\u00e4chter mein, mag es wohl seyn,", "tokens": ["\u00bb", "o", "W\u00e4ch\u00b7ter", "mein", ",", "mag", "es", "wohl", "seyn", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NE", "PPOSAT", "$,", "VMFIN", "PPER", "ADV", "VAINF", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "So hilf mir Freude mehren.", "tokens": ["So", "hilf", "mir", "Freu\u00b7de", "meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gut, W\u00e4chter! ich kann ihn ohne dich,", "tokens": ["Gut", ",", "W\u00e4ch\u00b7ter", "!", "ich", "kann", "ihn", "oh\u00b7ne", "dich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NE", "$.", "PPER", "VMFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In mein Gemach nicht bringen,", "tokens": ["In", "mein", "Ge\u00b7mach", "nicht", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O wolle mir nach meiner Begier,", "tokens": ["O", "wol\u00b7le", "mir", "nach", "mei\u00b7ner", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mein Leid nun helfen wenden,", "tokens": ["Mein", "Leid", "nun", "hel\u00b7fen", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich sag f\u00fcrwahr, da\u00df immerdar", "tokens": ["Ich", "sag", "f\u00fcr\u00b7wahr", ",", "da\u00df", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Gab ich dir's vergelte,", "tokens": ["Mit", "Gab", "ich", "dir's", "ver\u00b7gel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "K\u00f6mmt er herbey, gut W\u00e4chter frey,", "tokens": ["K\u00f6mmt", "er", "her\u00b7bey", ",", "gut", "W\u00e4ch\u00b7ter", "frey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ADJD", "NE", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Den Gast gen niemand melde.\u00ab", "tokens": ["Den", "Gast", "gen", "nie\u00b7mand", "mel\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der W\u00e4chter sprach: \u00bbZart Frau ich lach,", "tokens": ["Der", "W\u00e4ch\u00b7ter", "sprach", ":", "\u00bb", "Zart", "Frau", "ich", "lach", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ADJD", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Thut mirs nicht \u00fcbel kehren,", "tokens": ["Thut", "mirs", "nicht", "\u00fc\u00b7bel", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Meine Treu ich gab auf all mein Hab", "tokens": ["Mei\u00b7ne", "Treu", "ich", "gab", "auf", "all", "mein", "Hab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ein Eid mu\u00dft ich wohl schw\u00f6ren,", "tokens": ["Ein", "Eid", "mu\u00dft", "ich", "wohl", "schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und mit der Hand ich mich verband,", "tokens": ["Und", "mit", "der", "Hand", "ich", "mich", "ver\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Herren Schad zu wenden,", "tokens": ["Des", "Her\u00b7ren", "Schad", "zu", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Frau, da\u00df ich thu, muth mir nicht zu,", "tokens": ["Frau", ",", "da\u00df", "ich", "thu", ",", "muth", "mir", "nicht", "zu", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "NN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "So darf mich niemand schelten.", "tokens": ["So", "darf", "mich", "nie\u00b7mand", "schel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mein Herr gebot mir auf den Tod,", "tokens": ["Mein", "Herr", "ge\u00b7bot", "mir", "auf", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da er von hier wollt scheiden,", "tokens": ["Da", "er", "von", "hier", "wollt", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu wachen wohl, ich W\u00e4chter soll", "tokens": ["Zu", "wa\u00b7chen", "wohl", ",", "ich", "W\u00e4ch\u00b7ter", "soll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "ADV", "$,", "PPER", "NE", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es thun bey meinem Eide,", "tokens": ["Es", "thun", "bey", "mei\u00b7nem", "Ei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er sprach: Mit Schall sing, ruf und kall,", "tokens": ["Er", "sprach", ":", "Mit", "Schall", "sing", ",", "ruf", "und", "kall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "APPR", "NN", "VVFIN", "$,", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sey munter an der Zinnen,", "tokens": ["Sey", "mun\u00b7ter", "an", "der", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hab in der Hut, mein Schlo\u00df und Gut,", "tokens": ["Hab", "in", "der", "Hut", ",", "mein", "Schlo\u00df", "und", "Gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So lang ich bin von hinnen.", "tokens": ["So", "lang", "ich", "bin", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VAFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er sprach noch mehr, bey Treu und Ehr,", "tokens": ["Er", "sprach", "noch", "mehr", ",", "bey", "Treu", "und", "Ehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Thu's ehrlich mit mir meinen,", "tokens": ["Thu's", "ehr\u00b7lich", "mit", "mir", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wollt hier ein Gast eindringen fast,", "tokens": ["Wollt", "hier", "ein", "Gast", "ein\u00b7drin\u00b7gen", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So werf ihn todt mit Steinen,", "tokens": ["So", "werf", "ihn", "todt", "mit", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Falsch Weg und Steg mit Sorg verleg,", "tokens": ["Falsch", "Weg", "und", "Steg", "mit", "Sorg", "ver\u00b7leg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "APPR", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Schaden mein zu wehren,", "tokens": ["Den", "Scha\u00b7den", "mein", "zu", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "H\u00fct W\u00e4chter recht, getreuer Knecht,", "tokens": ["H\u00fct", "W\u00e4ch\u00b7ter", "recht", ",", "ge\u00b7treu\u00b7er", "Knecht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Gut will ich dir mehren.", "tokens": ["Dein", "Gut", "will", "ich", "dir", "meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Frau, ihr wi\u00dft wohl, da\u00df ich nicht soll,", "tokens": ["Frau", ",", "ihr", "wi\u00dft", "wohl", ",", "da\u00df", "ich", "nicht", "soll", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "VMFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Thun Schaden mit Untreuen,", "tokens": ["Thun", "Scha\u00b7den", "mit", "Un\u00b7treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem Herren mein, es br\u00e4cht mir Pein,", "tokens": ["Dem", "Her\u00b7ren", "mein", ",", "es", "br\u00e4cht", "mir", "Pein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "$,", "PPER", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00fcrd mich selbsten reuen.\u00ab", "tokens": ["Und", "w\u00fcrd", "mich", "selbs\u00b7ten", "reu\u00b7en", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbdeinem Ungef\u00e4ll, W\u00e4chter Gesell,", "tokens": ["\u00bb", "dei\u00b7nem", "Un\u00b7ge\u00b7f\u00e4ll", ",", "W\u00e4ch\u00b7ter", "Ge\u00b7sell", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "NE", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Will ich nun wohl vorkommen,", "tokens": ["Will", "ich", "nun", "wohl", "vor\u00b7kom\u00b7men", ","], "toke