{"textgrid.poem.56698": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dann wacht ich auf und sah den Ziegelschlot", "tokens": ["Dann", "wacht", "ich", "auf", "und", "sah", "den", "Zie\u00b7gel\u00b7schlot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "vor meinem Fenster leuchten fr\u00fchlichtrot", "tokens": ["vor", "mei\u00b7nem", "Fens\u00b7ter", "leuch\u00b7ten", "fr\u00fch\u00b7lich\u00b7trot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "und \u00fcber ihm und \u00fcber Dach und Wand", "tokens": ["und", "\u00fc\u00b7ber", "ihm", "und", "\u00fc\u00b7ber", "Dach", "und", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "den Morgen kalt und st\u00e4hlern ausgespannt.", "tokens": ["den", "Mor\u00b7gen", "kalt", "und", "st\u00e4h\u00b7lern", "aus\u00b7ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und w\u00e4hrend sie sich noch in Tr\u00e4umen wiegte", "tokens": ["Und", "w\u00e4h\u00b7rend", "sie", "sich", "noch", "in", "Tr\u00e4u\u00b7men", "wieg\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PP