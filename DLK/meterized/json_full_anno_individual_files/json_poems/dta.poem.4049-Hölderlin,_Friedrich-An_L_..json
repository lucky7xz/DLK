{"dta.poem.4049": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An L .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Komm! in's Offene, Freund! zwar gl\u00e4nzt ein", "tokens": ["Komm", "!", "in's", "Of\u00b7fe\u00b7ne", ",", "Freund", "!", "zwar", "gl\u00e4nzt", "ein"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "APPRART", "NN", "$,", "NN", "$.", "ADV", "VVFIN", "ART"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Weniges heute", "tokens": ["We\u00b7ni\u00b7ges", "heu\u00b7te"], "token_info": ["word", "word"], "pos": ["PIS", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Nur herunter und eng schlie\u00dfet der Himmel uns", "tokens": ["Nur", "her\u00b7un\u00b7ter", "und", "eng", "schlie\u00b7\u00dfet", "der", "Him\u00b7mel", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "KON", "ADJD", "VVFIN", "ART", "NN", "PPER"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "ein.", "tokens": ["ein", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Weder die Berge sind, noch aufgegangen des Waldes", "tokens": ["We\u00b7der", "die", "Ber\u00b7ge", "sind", ",", "noch", "auf\u00b7ge\u00b7gan\u00b7gen", "des", "Wal\u00b7des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "$,", "ADV", "VVPP", "ART", "NN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Gipfel nach Wunsch, und leer ruht vom Gesange", "tokens": ["Gip\u00b7fel", "nach", "Wunsch", ",", "und", "leer", "ruht", "vom", "Ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "KON", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "die Luft.", "tokens": ["die", "Luft", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Tr\u00fcb ist's heut, es schlummern die G\u00e4ng' und die", "tokens": ["Tr\u00fcb", "ist's", "heut", ",", "es", "schlum\u00b7mern", "die", "G\u00e4ng'", "und", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "KON", "ART"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Gassen, und fast will", "tokens": ["Gas\u00b7sen", ",", "und", "fast", "will"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ADV", "VMFIN"], "meter": "+-++-", "measure": "unknown.measure.tri"}, "line.10": {"text": "Mir es scheinen, es sey, als in der bleiernen", "tokens": ["Mir", "es", "schei\u00b7nen", ",", "es", "sey", ",", "als", "in", "der", "blei\u00b7er\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "$,", "KOUS", "APPR", "ART", "ADJA"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Zeit.", "tokens": ["Zeit", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Dennoch gelinget der Wunsch, Rechtglaubige zwei-", "tokens": ["Den\u00b7noch", "ge\u00b7lin\u00b7get", "der", "Wunsch", ",", "Recht\u00b7glau\u00b7bi\u00b7ge", "zwei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "feln an Einer", "tokens": ["feln", "an", "Ei\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "PIS"], "meter": "--+-", "measure": "anapaest.init"}, "line.14": {"text": "Stunde nicht, und der Lust bleibe geweihet der", "tokens": ["Stun\u00b7de", "nicht", ",", "und", "der", "Lust", "blei\u00b7be", "ge\u00b7wei\u00b7het", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "KON", "ART", "NN", "VVFIN", "VVFIN", "ART"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.15": {"text": "Tag.", "tokens": ["Tag", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Denn nicht wenig erfreuet, was wir vom Himmel", "tokens": ["Denn", "nicht", "we\u00b7nig", "er\u00b7freu\u00b7et", ",", "was", "wir", "vom", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADV", "VVFIN", "$,", "PRELS", "PPER", "APPRART", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "gewonnen,", "tokens": ["ge\u00b7won\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Wenn er's weigert und doch g\u00f6nnet den Kindern", "tokens": ["Wenn", "er's", "wei\u00b7gert", "und", "doch", "g\u00f6n\u00b7net", "den", "Kin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "KON", "ADV", "VVFIN", "ART", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "zuletzt.", "tokens": ["zu\u00b7letzt", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.20": {"text": "Nur da\u00df solcher Reden und auch der Schritt' und", "tokens": ["Nur", "da\u00df", "sol\u00b7cher", "Re\u00b7den", "und", "auch", "der", "Schritt'", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PIAT", "NN", "KON", "ADV", "ART", "NN", "KON"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.21": {"text": "der M\u00fche", "tokens": ["der", "M\u00fc\u00b7he"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Werth der Gewinn und ganz wahr das Erg\u00f6tz-", "tokens": ["Werth", "der", "Ge\u00b7winn", "und", "ganz", "wahr", "das", "Er\u00b7g\u00f6tz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "ADV", "ADJD", "ART", "TRUNC"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.23": {"text": "liche sey.", "tokens": ["li\u00b7che", "sey", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.24": {"text": "Darum hoff' ich sogar, es werde, wenn das Ge-", "tokens": ["Da\u00b7rum", "hoff'", "ich", "so\u00b7gar", ",", "es", "wer\u00b7de", ",", "wenn", "das", "Ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "w\u00fcnschte", "tokens": ["w\u00fcnschte"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.26": {"text": "Wir beginnen und erst unsere Zunge gel\u00f6st,", "tokens": ["Wir", "be\u00b7gin\u00b7nen", "und", "erst", "un\u00b7se\u00b7re", "Zun\u00b7ge", "ge\u00b7l\u00f6st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Und gefunden das Wort, und aufgegangen das", "tokens": ["Und", "ge\u00b7fun\u00b7den", "das", "Wort", ",", "und", "auf\u00b7ge\u00b7gan\u00b7gen", "das"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVPP", "ART", "NN", "$,", "KON", "VVPP", "ART"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.28": {"text": "Herz ist,", "tokens": ["Herz", "ist", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.29": {"text": "Und von trunkener Stirn' h\u00f6her Besinnen ent-", "tokens": ["Und", "von", "trun\u00b7ke\u00b7ner", "Stirn'", "h\u00f6\u00b7her", "Be\u00b7sin\u00b7nen", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJA", "NN", "TRUNC"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.30": {"text": "springt,", "tokens": ["springt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.31": {"text": "Mit den unsern zugleich des Himmels Bl\u00fcthe", "tokens": ["Mit", "den", "un\u00b7sern", "zu\u00b7gleich", "des", "Him\u00b7mels", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.32": {"text": "beginnen,", "tokens": ["be\u00b7gin\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.33": {"text": "Und dem offenen Blick offen der Leuchtende seyn.", "tokens": ["Und", "dem", "of\u00b7fe\u00b7nen", "Blick", "of\u00b7fen", "der", "Leuch\u00b7ten\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD", "ART", "NN", "VAINF", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}}}}}