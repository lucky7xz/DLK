{"dta.poem.19974": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Nach Katharinas Tod .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O seel'ge Herrin! Stern aus Norden,", "tokens": ["O", "seel'\u00b7ge", "Her\u00b7rin", "!", "Stern", "aus", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der sich einst mild zu uns gewandt,", "tokens": ["Der", "sich", "einst", "mild", "zu", "uns", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du, die zum Liebesstern geworden", "tokens": ["Du", ",", "die", "zum", "Lie\u00b7bess\u00b7tern", "ge\u00b7wor\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "VAPP"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Dem hoffenden, dem armen Land,", "tokens": ["Dem", "hof\u00b7fen\u00b7den", ",", "dem", "ar\u00b7men", "Land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bist schon verschwunden, kaum gekommen,", "tokens": ["Bist", "schon", "ver\u00b7schwun\u00b7den", ",", "kaum", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Morgen \u00fcber Thal und H\u00f6h'n,", "tokens": ["Ein", "Mor\u00b7gen", "\u00fc\u00b7ber", "Thal", "und", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Deine Saat, des Lichts benommen,", "tokens": ["Und", "Dei\u00b7ne", "Saat", ",", "des", "Lichts", "be\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df nun im Keime traurend steh'n.", "tokens": ["Mu\u00df", "nun", "im", "Kei\u00b7me", "trau\u00b7rend", "steh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "ADJD", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie liegt es bang auf jedem Herzen!", "tokens": ["Wie", "liegt", "es", "bang", "auf", "je\u00b7dem", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie thun es tausend Thr\u00e4nen kund!", "tokens": ["Wie", "thun", "es", "tau\u00b7send", "Thr\u00e4\u00b7nen", "kund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wer da spricht, der spricht von Schmerzen,", "tokens": ["Und", "wer", "da", "spricht", ",", "der", "spricht", "von", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "$,", "PRELS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und, wie sein Innres t\u00f6dtlich wund.", "tokens": ["Und", ",", "wie", "sein", "Inn\u00b7res", "t\u00f6dt\u00b7lich", "wund", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPOSAT", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wohl manchem ist's, als k\u00f6nnt' er scheiden", "tokens": ["Wohl", "man\u00b7chem", "ist's", ",", "als", "k\u00f6nnt'", "er", "schei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NE", "$,", "KOUS", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fortan mit Lust von Herd und Haus,", "tokens": ["For\u00b7tan", "mit", "Lust", "von", "Herd", "und", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Als l\u00f6schten mit Dir alle Freuden,", "tokens": ["Als", "l\u00f6schten", "mit", "Dir", "al\u00b7le", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "APPR", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jedwedes Licht auf einmal aus.", "tokens": ["Jed\u00b7we\u00b7des", "Licht", "auf", "ein\u00b7mal", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr Glocken mit geweihtem Schalle!", "tokens": ["Ihr", "Glo\u00b7cken", "mit", "ge\u00b7weih\u00b7tem", "Schal\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ruft durch die traurend stille Luft:", "tokens": ["Ruft", "durch", "die", "trau\u00b7rend", "stil\u00b7le", "Luft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eihr Arme! kniet und betet alle!", "tokens": ["\u201e", "ihr", "Ar\u00b7me", "!", "kniet", "und", "be\u00b7tet", "al\u00b7le", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$.", "VVFIN", "KON", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eh\u00f6rts! eure Mutter deckt die Gruft!\u201c", "tokens": ["\u201e", "h\u00f6rts", "!", "eu\u00b7re", "Mut\u00b7ter", "deckt", "die", "Gruft", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$.", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u201eihr Reiche h\u00f6rts! nun ist verschwunden", "tokens": ["\u201e", "ihr", "Rei\u00b7che", "h\u00f6rts", "!", "nun", "ist", "ver\u00b7schwun\u00b7den"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "NE", "ADV", "$.", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u201esie, euer Stolz, Sie, aller Hort!", "tokens": ["\u201e", "sie", ",", "eu\u00b7er", "Stolz", ",", "Sie", ",", "al\u00b7ler", "Hort", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ekniet! schw\u00f6rt: das Band, das Sie gebunden,", "tokens": ["\u201e", "kniet", "!", "schw\u00f6rt", ":", "das", "Band", ",", "das", "Sie", "ge\u00b7bun\u00b7den", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "VVFIN", "$.", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eein Heiligthum zu binden fort.\u201c", "tokens": ["\u201e", "ein", "Hei\u00b7lig\u00b7thum", "zu", "bin\u00b7den", "fort", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "PTKZU", "VVINF", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie Well' an Well', schlag Z\u00e4hr' an Z\u00e4hre;", "tokens": ["Wie", "Well'", "an", "Well'", ",", "schlag", "Z\u00e4hr'", "an", "Z\u00e4h\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "$,", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wehlaut! fahr \u00fcber Land und Meer,", "tokens": ["Wehl\u00b7aut", "!", "fahr", "\u00fc\u00b7ber", "Land", "und", "Meer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ruf aus: \u201eIhr L\u00e4nder und ihre Meere!", "tokens": ["Ruf", "aus", ":", "\u201e", "Ihr", "L\u00e4n\u00b7der", "und", "ih\u00b7re", "Mee\u00b7re", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "$(", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "O trauret all'! Sie ist nicht mehr!\u201c", "tokens": ["O", "trau\u00b7ret", "all'", "!", "Sie", "ist", "nicht", "mehr", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PIS", "$.", "PPER", "VAFIN", "PTKNEG", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie jubelt's laut in Sternenhallen!", "tokens": ["Wie", "ju\u00b7belt's", "laut", "in", "Ster\u00b7nen\u00b7hal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie flammt in Lust des Himmels Zelt!", "tokens": ["Wie", "flammt", "in", "Lust", "des", "Him\u00b7mels", "Zelt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei uns, wie ist es \u00f6d, zerfallen!", "tokens": ["Bei", "uns", ",", "wie", "ist", "es", "\u00f6d", ",", "zer\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ohne Heimat jezt die Welt!", "tokens": ["Wie", "oh\u00b7ne", "Hei\u00b7mat", "jezt", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Aufflog Sie nun zur ew'gen Sternenhalle,", "tokens": ["Auf\u00b7flog", "Sie", "nun", "zur", "ew'\u00b7gen", "Ster\u00b7nen\u00b7hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dahin, woher Sie segnend einst gekommen,", "tokens": ["Da\u00b7hin", ",", "wo\u00b7her", "Sie", "seg\u00b7nend", "einst", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "PPER", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir aber steh'n, erkrankt in Thr\u00e4nen alle,", "tokens": ["Wir", "a\u00b7ber", "steh'n", ",", "er\u00b7krankt", "in", "Thr\u00e4\u00b7nen", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "VVFIN", "APPR", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein Trost, kein Heilkraut kann uns Armen frommen.", "tokens": ["Kein", "Trost", ",", "kein", "Heil\u00b7kraut", "kann", "uns", "Ar\u00b7men", "from\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Doch wie wir steh'n, so jedes Trosts benommen,", "tokens": ["Doch", "wie", "wir", "steh'n", ",", "so", "je\u00b7des", "Trosts", "be\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ert\u00f6nt's zu uns mit himmlisch s\u00fc\u00dfem Schalle!", "tokens": ["Er\u00b7t\u00f6nt's", "zu", "uns", "mit", "himm\u00b7lisch", "s\u00fc\u00b7\u00dfem", "Schal\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u201eschaut himmelan! ich bin euch ja geblieben!", "tokens": ["\u201e", "schaut", "him\u00b7me\u00b7lan", "!", "ich", "bin", "euch", "ja", "ge\u00b7blie\u00b7ben", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u201eein Schutzgeist schwe'b ich waltend ob euch Lieben.\u201c", "tokens": ["\u201e", "ein", "Schutz\u00b7geist", "schwe'b", "ich", "wal\u00b7tend", "ob", "euch", "Lie\u00b7ben", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "VVPP", "KOUS", "PPER", "ADJA", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Nun ist Sie erst um uns und bey uns allen,", "tokens": ["Nun", "ist", "Sie", "erst", "um", "uns", "und", "bey", "uns", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "KON", "APPR", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von keinem mehr getrennt durch Thal und H\u00f6hen;", "tokens": ["Von", "kei\u00b7nem", "mehr", "ge\u00b7trennt", "durch", "Thal", "und", "H\u00f6\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo Seufzer st\u00f6hnen, hei\u00dfe Thr\u00e4nen fallen,", "tokens": ["Wo", "Seuf\u00b7zer", "st\u00f6h\u00b7nen", ",", "hei\u00b7\u00dfe", "Thr\u00e4\u00b7nen", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verla\u00dfne Arme still zum Himmel flehen,", "tokens": ["Ver\u00b7la\u00df\u00b7ne", "Ar\u00b7me", "still", "zum", "Him\u00b7mel", "fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Da wird man h\u00f6ren oft ein leises Wallen,", "tokens": ["Da", "wird", "man", "h\u00f6\u00b7ren", "oft", "ein", "lei\u00b7ses", "Wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird ungehoffte H\u00fclfe staunend sehen.", "tokens": ["Wird", "un\u00b7ge\u00b7hoff\u00b7te", "H\u00fcl\u00b7fe", "stau\u00b7nend", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann fraget nicht: woher ist das gekommen?", "tokens": ["Dann", "fra\u00b7get", "nicht", ":", "wo\u00b7her", "ist", "das", "ge\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "$.", "PWAV", "VAFIN", "PDS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es kam von ihr, dem Schutzgeist aller Frommen.", "tokens": ["Es", "kam", "von", "ihr", ",", "dem", "Schutz\u00b7geist", "al\u00b7ler", "From\u00b7men", "."], "token_info": ["word", "wo