{"dta.poem.7888": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Bauernregel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Im Sommer such ein Liebchen dir", "tokens": ["Im", "Som\u00b7mer", "such", "ein", "Lieb\u00b7chen", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Garten und Gefild!", "tokens": ["In", "Gar\u00b7ten", "und", "Ge\u00b7fild", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sind die Tage lang genug,", "tokens": ["Da", "sind", "die", "Ta\u00b7ge", "lang", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da sind die N\u00e4chte mild.", "tokens": ["Da", "sind", "die", "N\u00e4ch\u00b7te", "mild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Im Winter mu\u00df der s\u00fc\u00dfe Bund", "tokens": ["Im", "Win\u00b7ter", "mu\u00df", "der", "s\u00fc\u00b7\u00dfe", "Bund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon fest geschlossen seyn,", "tokens": ["Schon", "fest", "ge\u00b7schlos\u00b7sen", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So darfst nicht lange stehn im Schnee", "tokens": ["So", "darfst", "nicht", "lan\u00b7ge", "stehn", "im", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PTKNEG", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei kaltem Mondenschein.", "tokens": ["Bei", "kal\u00b7tem", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}