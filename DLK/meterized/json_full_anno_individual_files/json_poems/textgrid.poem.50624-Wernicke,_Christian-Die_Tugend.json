{"textgrid.poem.50624": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die Tugend", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Tugend ist ein ", "tokens": ["Die", "Tu\u00b7gend", "ist", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der dichte Zweig' ausschie\u00dft in einem engen Raum,", "tokens": ["Der", "dich\u00b7te", "Zweig'", "aus\u00b7schie\u00dft", "in", "ei\u00b7nem", "en\u00b7gen", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der mit den Zweigen so, wie mit dem Schatten dienet,", "tokens": ["Der", "mit", "den", "Zwei\u00b7gen", "so", ",", "wie", "mit", "dem", "Schat\u00b7ten", "die\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "$,", "PWAV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Im Winter in der K\u00e4lt', in Hitz' im Sommer gr\u00fcnet.", "tokens": ["Im", "Win\u00b7ter", "in", "der", "K\u00e4lt'", ",", "in", "Hitz'", "im", "Som\u00b7mer", "gr\u00fc\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,", "APPR", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zwar pflegt die Welt ihr Spott und Hohn,", "tokens": ["Zwar", "pflegt", "die", "Welt", "ihr", "Spott", "und", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Statt der verdienten Ehre, zu bezeigen;", "tokens": ["Statt", "der", "ver\u00b7dien\u00b7ten", "Eh\u00b7re", ",", "zu", "be\u00b7zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch sie ist ihr selbsteigner Lohn:", "tokens": ["Doch", "sie", "ist", "ihr", "selbs\u00b7teig\u00b7ner", "Lohn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Tugend ist ein ", "tokens": ["Die", "Tu\u00b7gend", "ist", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der dichte Zweig' ausschie\u00dft in einem engen Raum,", "tokens": ["Der", "dich\u00b7te", "Zweig'", "aus\u00b7schie\u00dft", "in", "ei\u00b7nem", "en\u00b7gen", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der mit den Zweigen so, wie mit dem Schatten dienet,", "tokens": ["Der", "mit", "den", "Zwei\u00b7gen", "so", ",", "wie", "mit", "dem", "Schat\u00b7ten", "die\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "$,", "PWAV", "APPR", "ART", "NN", "VVFIN", 