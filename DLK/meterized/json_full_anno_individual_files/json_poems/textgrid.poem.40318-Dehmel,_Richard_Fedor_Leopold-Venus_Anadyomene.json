 "ein", "Kna\u00b7be", "mit", "blau\u00b7en", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.21": {"line.1": {"text": "Das war, als du noch Fehle hattest,", "tokens": ["Das", "war", ",", "als", "du", "noch", "Feh\u00b7le", "hat\u00b7test", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "KOUS", "PPER", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "noch alte Furcht und fremde Scham,", "tokens": ["noch", "al\u00b7te", "Furcht", "und", "frem\u00b7de", "Scham", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "als du noch keine Seele hattest,", "tokens": ["als", "du", "noch", "kei\u00b7ne", "See\u00b7le", "hat\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die nur aus ", "tokens": ["die", "nur", "aus"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "APPR"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.22": {"line.1": {"text": "Aber du sahst die Falter leuchten,", "tokens": ["A\u00b7ber", "du", "sahst", "die", "Fal\u00b7ter", "leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "mit flackernden Fl\u00fcgeln bunt sich greifen;", "tokens": ["mit", "fla\u00b7ckern\u00b7den", "Fl\u00fc\u00b7geln", "bunt", "sich", "grei\u00b7fen", ";"], "token_info": ["word"