rd", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "J\u00fcnglingslust mit deiner \u00fcppigen Zierde!", "tokens": ["J\u00fcng\u00b7lings\u00b7lust", "mit", "dei\u00b7ner", "\u00fcp\u00b7pi\u00b7gen", "Zier\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ja, ich h\u00f6r mich, wie ich nach dir st\u00f6hnte,", "tokens": ["Ja", ",", "ich", "h\u00f6r", "mich", ",", "wie", "ich", "nach", "dir", "st\u00f6hn\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "ferne G\u00f6ttin meiner ersten Begierde,", "tokens": ["fer\u00b7ne", "G\u00f6t\u00b7tin", "mei\u00b7ner", "ers\u00b7ten", "Be\u00b7gier\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.19": {"line.1": {"text": "Das ist die alte Stimme wieder,", "tokens": ["Das", "ist", "die", "al\u00b7te", "Stim\u00b7me", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "aus langen Tr\u00e4umen jung erwacht.", "tokens": ["aus", "lan\u00b7gen", "Tr\u00e4u\u00b7men", "jung", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie