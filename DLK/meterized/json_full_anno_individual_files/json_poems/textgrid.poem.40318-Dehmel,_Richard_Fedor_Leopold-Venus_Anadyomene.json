{"textgrid.poem.40318": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Venus Anadyomene", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist die alte Stimme wieder,", "tokens": ["Das", "ist", "die", "al\u00b7te", "Stim\u00b7me", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "aus langen Tr\u00e4umen jung erwacht.", "tokens": ["aus", "lan\u00b7gen", "Tr\u00e4u\u00b7men", "jung", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sang die allerersten Lieder,", "tokens": ["Sie", "sang", "die", "al\u00b7le\u00b7rers\u00b7ten", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "trunken und sch\u00fcchtern. Sie singt und lacht:", "tokens": ["trun\u00b7ken", "und", "sch\u00fcch\u00b7tern", ".", "Sie", "singt", "und", "lacht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVINF", "$.", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "\u00dcber dem gr\u00fcnen Roggenmeere", "tokens": ["\u00dc\u00b7ber", "dem", "gr\u00fc\u00b7nen", "Rog\u00b7gen\u00b7mee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "wiegte die Glut zwei Pfauenaugen.", "tokens": ["wieg\u00b7te", "die", "Glut", "zwei", "Pfau\u00b7en\u00b7au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "CARD", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Bl\u00fchend roch die br\u00fctende Leere.", "tokens": ["Bl\u00fc\u00b7hend", "roch", "die", "br\u00fc\u00b7ten\u00b7de", "Lee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Tief im gr\u00fcnen Roggenmeere", "tokens": ["Tief", "im", "gr\u00fc\u00b7nen", "Rog\u00b7gen\u00b7mee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "lag ein Knabe mit blauen Augen.", "tokens": ["lag", "ein", "Kna\u00b7be", "mit", "blau\u00b7en", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Das war, als du noch Fehle hattest,", "tokens": ["Das", "war", ",", "als", "du", "noch", "Feh\u00b7le", "hat\u00b7test", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "KOUS", "PPER", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "noch alte Furcht und fremde Scham,", "tokens": ["noch", "al\u00b7te", "Furcht", "und", "frem\u00b7de", "Scham", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "als du noch keine Seele hattest,", "tokens": ["als", "du", "noch", "kei\u00b7ne", "See\u00b7le", "hat\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die nur aus ", "tokens": ["die", "nur", "aus"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "APPR"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Aber du sahst die Falter leuchten,", "tokens": ["A\u00b7ber", "du", "sahst", "die", "Fal\u00b7ter", "leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "mit flackernden Fl\u00fcgeln bunt sich greifen;", "tokens": ["mit", "fla\u00b7ckern\u00b7den", "Fl\u00fc\u00b7geln", "bunt", "sich", "grei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PRF", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "tr\u00e4umte dir von zwei dunkelfeuchten", "tokens": ["tr\u00e4um\u00b7te", "dir", "von", "zwei", "dun\u00b7kel\u00b7feuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "CARD", "ADJA"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Augen, und die sahst du leuchten", "tokens": ["Au\u00b7gen", ",", "und", "die", "sahst", "du", "leuch\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ART", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "unter bunten, flatternden Schleifen.", "tokens": ["un\u00b7ter", "bun\u00b7ten", ",", "flat\u00b7tern\u00b7den", "Schlei\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Das war die Zeit des Schaums der S\u00e4fte,", "tokens": ["Das", "war", "die", "Zeit", "des", "Schaums", "der", "S\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die \u00c4hren st\u00e4ubten gelben Seim;", "tokens": ["die", "\u00c4h\u00b7ren", "st\u00e4ub\u00b7ten", "gel\u00b7ben", "Seim", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "vieltausendj\u00e4hrige Sehnsuchtskr\u00e4fte", "tokens": ["viel\u00b7tau\u00b7send\u00b7j\u00e4h\u00b7ri\u00b7ge", "Sehn\u00b7suchts\u00b7kr\u00e4f\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "erregten schwellend einen Keim.", "tokens": ["er\u00b7reg\u00b7ten", "schwel\u00b7lend", "ei\u00b7nen", "Keim", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ahntest unterm andern Kleide", "tokens": ["Ahn\u00b7test", "un\u00b7term", "an\u00b7dern", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "andre nackte Glieder klopfen.", "tokens": ["and\u00b7re", "nack\u00b7te", "Glie\u00b7der", "klop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine H\u00e4nde flackerten beide.", "tokens": ["Dei\u00b7ne", "H\u00e4n\u00b7de", "fla\u00b7cker\u00b7ten", "bei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "In die einsam hei\u00dfe Haide", "tokens": ["In", "die", "ein\u00b7sam", "hei\u00b7\u00dfe", "Hai\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "VVFIN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "quoll ein erster Samentropfen.", "tokens": ["quoll", "ein", "ers\u00b7ter", "Sa\u00b7men\u00b7trop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Das tat die Sehnsucht dieser Erde,", "tokens": ["Das", "tat", "die", "Sehn\u00b7sucht", "die\u00b7ser", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die opfernd um die Sonne schweift.", "tokens": ["die", "op\u00b7fernd", "um", "die", "Son\u00b7ne", "schweift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sprach das allererste Werde.", "tokens": ["Sie", "sprach", "das", "al\u00b7le\u00b7rers\u00b7te", "Wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}}, "stanza.9": {"line.1": {"text": "Habe Dank, du dunkle Geisterstimme!", "tokens": ["Ha\u00b7be", "Dank", ",", "du", "dunk\u00b7le", "Geis\u00b7ters\u00b7tim\u00b7me", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ja, du hilfst mir meine Not begreifen.", "tokens": ["Ja", ",", "du", "hilfst", "mir", "mei\u00b7ne", "Not", "be\u00b7grei\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auf! ich f\u00fchl's, wie tr\u00fcb ich glimme;", "tokens": ["Auf", "!", "ich", "f\u00fchl's", ",", "wie", "tr\u00fcb", "ich", "glim\u00b7me", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "la\u00df uns nach Erleuchtung schweifen!", "tokens": ["la\u00df", "uns", "nach", "Er\u00b7leuch\u00b7tung", "schwei\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "M\u00fchsam von Entt\u00e4uschung zu Entt\u00e4uschung", "tokens": ["M\u00fch\u00b7sam", "von", "Ent\u00b7t\u00e4u\u00b7schung", "zu", "Ent\u00b7t\u00e4u\u00b7schung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "hab ich mich hierher gewunden,", "tokens": ["hab", "ich", "mich", "hier\u00b7her", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "um in eisiger Verkeuschung", "tokens": ["um", "in", "ei\u00b7si\u00b7ger", "Ver\u00b7keu\u00b7schung"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "starr zum Gleichmut zu gesunden.", "tokens": ["starr", "zum", "Gleich\u00b7mut", "zu", "ge\u00b7sun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "O! noch Einmal war mir aufgegangen", "tokens": ["O", "!", "noch", "Ein\u00b7mal", "war", "mir", "auf\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "ADV", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "zweier Augen lockende Hoffnungsmacht:", "tokens": ["zwei\u00b7er", "Au\u00b7gen", "lo\u00b7cken\u00b7de", "Hoff\u00b7nungs\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "eines Sommergl\u00fcckes Prangen", "tokens": ["ei\u00b7nes", "Som\u00b7mer\u00b7gl\u00fc\u00b7ckes", "Pran\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "mitten in der Winternacht.", "tokens": ["mit\u00b7ten", "in", "der", "Win\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Als mein Herz am allerinnigsten bebte", "tokens": ["Als", "mein", "Herz", "am", "al\u00b7le\u00b7rin\u00b7nigs\u00b7ten", "beb\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "ADJA", "VVFIN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u2013 Wundert\u00e4terin, hoffst du ", "tokens": ["\u2013", "Wun\u00b7der\u00b7t\u00e4\u00b7te\u00b7rin", ",", "hoffst", "du"], "token_info": ["punct", "word", "punct", "word", "word"], "pos": ["$(", "NN", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "schlo\u00df ich's ein in dies verspinnewebte", "tokens": ["schlo\u00df", "ich's", "ein", "in", "dies", "ver\u00b7spin\u00b7ne\u00b7web\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "APPR", "PDS", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "kahle Vorstadtkellerloch.", "tokens": ["kah\u00b7le", "Vor\u00b7stadt\u00b7kel\u00b7ler\u00b7loch", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.13": {"line.1": {"text": "Wie's mich anh\u00f6hnt! Hinter mir, ihr Geister,", "tokens": ["Wie's", "mich", "an\u00b7h\u00f6hnt", "!", "Hin\u00b7ter", "mir", ",", "ihr", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "schnarcht die Mitwelt meiner Zelle:", "tokens": ["schnarcht", "die", "Mit\u00b7welt", "mei\u00b7ner", "Zel\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "mein schwerh\u00f6riger Schustermeister,", "tokens": ["mein", "schwer\u00b7h\u00f6\u00b7ri\u00b7ger", "Schus\u00b7ter\u00b7meis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "und sein n\u00e4rrischer Altgeselle.", "tokens": ["und", "sein", "n\u00e4r\u00b7ri\u00b7scher", "Alt\u00b7ge\u00b7sel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Wochendurch hat dieser ledige", "tokens": ["Wo\u00b7chen\u00b7durch", "hat", "die\u00b7ser", "le\u00b7di\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PDAT", "ADJA"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Fleischfeind christlich mich zerrauft,", "tokens": ["Fleischfeind", "christ\u00b7lich", "mich", "zer\u00b7rauft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schmachtriemsweisheit mir gepredigt;", "tokens": ["Schmach\u00b7tri\u00b7ems\u00b7weis\u00b7heit", "mir", "ge\u00b7pre\u00b7digt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Nacht f\u00fcr Nacht versucht von Tr\u00e4umen", "tokens": ["Nacht", "f\u00fcr", "Nacht", "ver\u00b7sucht", "von", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVPP", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dehn'ich mich auf meinem harten Lager,", "tokens": ["dehn'\u00b7ich", "mich", "auf", "mei\u00b7nem", "har\u00b7ten", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "immer zuchtloser mich b\u00e4umend,", "tokens": ["im\u00b7mer", "zucht\u00b7lo\u00b7ser", "mich", "b\u00e4u\u00b7mend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "immer gieriger, immer magrer.", "tokens": ["im\u00b7mer", "gie\u00b7ri\u00b7ger", ",", "im\u00b7mer", "mag\u00b7rer", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.16": {"line.1": {"text": "Wie mich hungert! Wie die roten", "tokens": ["Wie", "mich", "hun\u00b7gert", "!", "Wie", "die", "ro\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "PWAV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freudenfenster dr\u00fcben blinken:", "tokens": ["Freu\u00b7den\u00b7fens\u00b7ter", "dr\u00fc\u00b7ben", "blin\u00b7ken", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blut, von dem die scheinbar toten", "tokens": ["Blut", ",", "von", "dem", "die", "schein\u00b7bar", "to\u00