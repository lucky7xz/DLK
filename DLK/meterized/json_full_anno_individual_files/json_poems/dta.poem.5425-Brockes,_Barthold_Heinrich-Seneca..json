{"dta.poem.5425": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Seneca.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was kann es eigentlich doch f\u00fcr Vergn\u00fcgen geben,", "tokens": ["Was", "kann", "es", "ei\u00b7gent\u00b7lich", "doch", "f\u00fcr", "Ver\u00b7gn\u00fc\u00b7gen", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich mich in die Zahl derjenigen, so leben,", "tokens": ["Da\u00df", "ich", "mich", "in", "die", "Zahl", "der\u00b7je\u00b7ni\u00b7gen", ",", "so", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "PDS", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf dieser Welt gesetzet sehe?", "tokens": ["Auf", "die\u00b7ser", "Welt", "ge\u00b7set\u00b7zet", "se\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df etwann Spei\u00df und Tranck durch meine Gurgel gehe?", "tokens": ["Da\u00df", "et\u00b7wann", "Spei\u00df", "und", "Tranck", "durch", "mei\u00b7ne", "Gur\u00b7gel", "ge\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df ich den morschen Leib, der doch so schw\u00e4chlich,", "tokens": ["Da\u00df", "ich", "den", "mor\u00b7schen", "Leib", ",", "der", "doch", "so", "schw\u00e4ch\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der, wenn man ihn nicht stets erf\u00fcllet, so gebrechlich,", "tokens": ["Der", ",", "wenn", "man", "ihn", "nicht", "stets", "er\u00b7f\u00fcl\u00b7let", ",", "so", "ge\u00b7brech\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PIS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Best\u00e4ndig pfropf\u2019 und stopf\u2019 und fast nur leb\u2019 allein,", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "pfropf'", "und", "stopf'", "und", "fast", "nur", "leb'", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "ADJD", "KON", "ADV", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein Kranckenw\u00e4rter hier zu seyn?", "tokens": ["Ein", "Kran\u00b7cken\u00b7w\u00e4r\u00b7ter", "hier", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wofern man seinen Geist nicht nach dem Sch\u00f6pfer lencket,", "tokens": ["Wo\u00b7fern", "man", "sei\u00b7nen", "Geist", "nicht", "nach", "dem", "Sch\u00f6p\u00b7fer", "len\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und seine weise Lieb\u2019- und wunderbahre F\u00fchrung,", "tokens": ["Und", "sei\u00b7ne", "wei\u00b7se", "Lie\u00b7b'", "und", "wun\u00b7der\u00b7bah\u00b7re", "F\u00fch\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Die Wunder seiner Macht und herrlichen Regierung,", "tokens": ["Die", "Wun\u00b7der", "sei\u00b7ner", "Macht", "und", "herr\u00b7li\u00b7chen", "Re\u00b7gie\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In Ehrfurcht voller Lust, bewundernd \u00fcberdencket,", "tokens": ["In", "Ehr\u00b7furcht", "vol\u00b7ler", "Lust", ",", "be\u00b7wun\u00b7dernd", "\u00fc\u00b7ber\u00b7den\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und inniglich ger\u00fchrt, ihn innig liebt und ehrt;", "tokens": ["Und", "in\u00b7nig\u00b7lich", "ge\u00b7r\u00fchrt", ",", "ihn", "in\u00b7nig", "liebt", "und", "ehrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So ist das Leben hier auf Erden", "tokens": ["So", "ist", "das", "Le\u00b7ben", "hier", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Nicht einst ein Gut genannt zu werden,", "tokens": ["Nicht", "einst", "ein", "Gut", "ge\u00b7nannt", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Nicht, da\u00df man es begehre, wehrt.", "tokens": ["Nicht", ",", "da\u00df", "man", "es", "be\u00b7geh\u00b7re", ",", "wehrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}