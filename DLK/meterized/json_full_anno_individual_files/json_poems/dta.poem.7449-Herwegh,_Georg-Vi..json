{"dta.poem.7449": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Vi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich z\u00e4hle gerne mit bei guten Christen", "tokens": ["Ich", "z\u00e4h\u00b7le", "ger\u00b7ne", "mit", "bei", "gu\u00b7ten", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und streite ritterlich und ohne Wanken,", "tokens": ["Und", "strei\u00b7te", "rit\u00b7ter\u00b7lich", "und", "oh\u00b7ne", "Wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn sie uns wollen das Gem\u00fct abdanken,", "tokens": ["Wenn", "sie", "uns", "wol\u00b7len", "das", "Ge\u00b7m\u00fct", "ab\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die unausstehlich pfiffigen Sophisten.", "tokens": ["Die", "un\u00b7aus\u00b7steh\u00b7lich", "pfif\u00b7fi\u00b7gen", "So\u00b7phis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch hass' ich das Gem\u00fct der Pietisten,", "tokens": ["Doch", "hass'", "ich", "das", "Ge\u00b7m\u00fct", "der", "Pie\u00b7tis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das, frech getreten aus des Anstands Schranken,", "tokens": ["Das", ",", "frech", "ge\u00b7tre\u00b7ten", "aus", "des", "An\u00b7stands", "Schran\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADJD", "VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Uns m\u00f6cht' die reinsten himmlischen Gedanken", "tokens": ["Uns", "m\u00f6cht'", "die", "reins\u00b7ten", "himm\u00b7li\u00b7schen", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit seinen Nebelworten \u00fcberlisten.", "tokens": ["Mit", "sei\u00b7nen", "Ne\u00b7bel\u00b7wor\u00b7ten", "\u00fc\u00b7berl\u00b7is\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Sah ich das Herz mi\u00dfhandelt und zerschlagen", "tokens": ["Sah", "ich", "das", "Herz", "mi\u00df\u00b7han\u00b7delt", "und", "zer\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVPP", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und von den R\u00fcden des Verstands gehetzt.", "tokens": ["Und", "von", "den", "R\u00fc\u00b7den", "des", "Ver\u00b7stands", "ge\u00b7hetzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}}, "stanza.4": {"line.1": {"text": "Es darf das Herz wohl auch ein W\u00f6rtchen sagen;", "tokens": ["Es", "darf", "das", "Herz", "wohl", "auch", "ein", "W\u00f6rt\u00b7chen", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch ward es weislich in die Brust gesetzt,", "tokens": ["Doch", "ward", "es", "weis\u00b7lich", "in", "die", "Brust", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df man's so hoch nicht wie den Kopf soll tragen.", "tokens": ["Da\u00df", "man's", "so", "hoch", "nicht", "wie", "den", "Kopf", "soll", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJD", "PTKNEG", "KOKOM", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}