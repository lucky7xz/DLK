{"dta.poem.497": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er klaget \u00fcber die Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wie gerne wolt' ich frey vom Liebes-Joche seyn/", "tokens": ["Wie", "ger\u00b7ne", "wolt'", "ich", "frey", "vom", "Lie\u00b7bes\u00b7Jo\u00b7che", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "ADJD", "APPRART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mich der ", "tokens": ["Und", "mich", "der"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ich aber bin kaum lo\u00df von einer Sclaverey/", "tokens": ["Ich", "a\u00b7ber", "bin", "kaum", "lo\u00df", "von", "ei\u00b7ner", "Scla\u00b7ve\u00b7rey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So f\u00fchl ich/ da\u00df ich schon wie vor bestricket sey.", "tokens": ["So", "f\u00fchl", "ich", "/", "da\u00df", "ich", "schon", "wie", "vor", "be\u00b7stri\u00b7cket", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "KOKOM", "APPR", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}