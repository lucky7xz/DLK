{"dta.poem.7966": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Vorschlag .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Dem Dichter ist der Fernen Bild geblieben,", "tokens": ["Dem", "Dich\u00b7ter", "ist", "der", "Fer\u00b7nen", "Bild", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bei dem er einsam oftmals Trost gefunden,", "tokens": ["Bei", "dem", "er", "ein\u00b7sam", "oft\u00b7mals", "Trost", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und h\u00e4lt des Lebens Wirrung ihn umwunden,", "tokens": ["Und", "h\u00e4lt", "des", "Le\u00b7bens", "Wir\u00b7rung", "ihn", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er f\u00fchlt am Busen doch das Bild der Lieben.", "tokens": ["Er", "f\u00fchlt", "am", "Bu\u00b7sen", "doch", "das", "Bild", "der", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "ART", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auch was der Dichter sang, sehnsuchtgetrieben,", "tokens": ["Auch", "was", "der", "Dich\u00b7ter", "sang", ",", "sehn\u00b7sucht\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PWS", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sch\u00f6ne liest es oft in Abendstunden,", "tokens": ["Die", "Sch\u00f6\u00b7ne", "liest", "es", "oft", "in", "A\u00b7bends\u00b7tun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Manches hat so innig sie empfunden,", "tokens": ["Und", "Man\u00b7ches", "hat", "so", "in\u00b7nig", "sie", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ihr es tief im Herzen steht geschrieben.", "tokens": ["Da\u00df", "ihr", "es", "tief", "im", "Her\u00b7zen", "steht", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein theures Bild, wohl wirkt es wunderkr\u00e4ftig,", "tokens": ["Ein", "theu\u00b7res", "Bild", ",", "wohl", "wirkt", "es", "wun\u00b7der\u00b7kr\u00e4f\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohl mancher Kummer weicht des Liedes T\u00f6nen,", "tokens": ["Wohl", "man\u00b7cher", "Kum\u00b7mer", "weicht", "des", "Lie\u00b7des", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch ewig bleibt der Trennung Schmerz gesch\u00e4ftig.", "tokens": ["Doch", "e\u00b7wig", "bleibt", "der", "Tren\u00b7nung", "Schmerz", "ge\u00b7sch\u00e4f\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Schicksal! wechsle leicht nur mit den Loosen:", "tokens": ["O", "Schick\u00b7sal", "!", "wechs\u00b7le", "leicht", "nur", "mit", "den", "Loo\u00b7sen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Dichter f\u00fchre wieder zu der Sch\u00f6nen,", "tokens": ["Den", "Dich\u00b7ter", "f\u00fch\u00b7re", "wie\u00b7der", "zu", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Lieder m\u00f6gen mit dem Bilde kosen!", "tokens": ["Die", "Lie\u00b7der", "m\u00f6\u00b7gen", "mit", "dem", "Bil\u00b7de", "ko\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}