{"dta.poem.6275": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was fl\u00fc\u00dfig ist, mu\u00df man wol \u00fcberlegen.", "tokens": ["Was", "fl\u00fc\u00b7\u00dfig", "ist", ",", "mu\u00df", "man", "wol", "\u00fc\u00b7berl\u00b7e\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ist ein Stoff, den, weil er bald verl\u00e4ufft,", "tokens": ["Es", "ist", "ein", "Stoff", ",", "den", ",", "weil", "er", "bald", "ver\u00b7l\u00e4ufft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man nicht so leicht begreifft.", "tokens": ["Man", "nicht", "so", "leicht", "be\u00b7greifft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKNEG", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sehn wir in seinem Bett\u2019 ein Wasser sich bewegen;", "tokens": ["Sehn", "wir", "in", "sei\u00b7nem", "Bett'", "ein", "Was\u00b7ser", "sich", "be\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So lasst uns uns bem\u00fchn, das Reich der Wellen", "tokens": ["So", "lasst", "uns", "uns", "be\u00b7m\u00fchn", ",", "das", "Reich", "der", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVINF", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als kleine C\u00f6rperchen uns vorzustellen,", "tokens": ["Als", "klei\u00b7ne", "C\u00f6r\u00b7per\u00b7chen", "uns", "vor\u00b7zu\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die l\u00e4nglich von Figur, jedoch auch rund dabey,", "tokens": ["Die", "l\u00e4ng\u00b7lich", "von", "Fi\u00b7gur", ",", "je\u00b7doch", "auch", "rund", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "$,", "ADV", "ADV", "ADJD", "PAV", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Die alle, ob sie gleich sich trennen,", "tokens": ["Die", "al\u00b7le", ",", "ob", "sie", "gleich", "sich", "tren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "KOUS", "PPER", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dennoch vor sich best\u00e4ndig rennen:", "tokens": ["Den\u00b7noch", "vor", "sich", "be\u00b7st\u00e4n\u00b7dig", "ren\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dieselben machen nun die Fluth,", "tokens": ["Die\u00b7sel\u00b7ben", "ma\u00b7chen", "nun", "die", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die fl\u00fc\u00df-und fl\u00fcchtig ist und nimmer ruht,", "tokens": ["Die", "fl\u00fc\u00df\u00b7\u00b7und", "fl\u00fcch\u00b7tig", "ist", "und", "nim\u00b7mer", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Von welcher sich die feste Fl\u00fc\u00dfigkeit", "tokens": ["Von", "wel\u00b7cher", "sich", "die", "fes\u00b7te", "Fl\u00fc\u00b7\u00dfig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Vermuthlich unterhalten mu\u00df,", "tokens": ["Ver\u00b7muth\u00b7lich", "un\u00b7ter\u00b7hal\u00b7ten", "mu\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Durch einen d\u00fcnnen Stoff, der durch dieselben eilet,", "tokens": ["Durch", "ei\u00b7nen", "d\u00fcn\u00b7nen", "Stoff", ",", "der", "durch", "die\u00b7sel\u00b7ben", "ei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PDAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und sie stets von einander theilet.", "tokens": ["Und", "sie", "stets", "von", "ein\u00b7an\u00b7der", "thei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "PRF", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Die C\u00f6rperchen, die lang, die glatt, die biegsam seyn,", "tokens": ["Die", "C\u00f6r\u00b7per\u00b7chen", ",", "die", "lang", ",", "die", "glatt", ",", "die", "bieg\u00b7sam", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "$,", "PRELS", "ADJD", "$,", "PRELS", "ADJD", "VAINF", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.17": {"text": "Und, da sie so gelenck, sich leichtlich regen,", "tokens": ["Und", ",", "da", "sie", "so", "ge\u00b7lenck", ",", "sich", "leicht\u00b7lich", "re\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "ADJD", "$,", "PRF", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Wenn wir sie, abgetrennt, absonderlich erwegen;", "tokens": ["Wenn", "wir", "sie", ",", "ab\u00b7ge\u00b7trennt", ",", "ab\u00b7son\u00b7der\u00b7lich", "er\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "VVPP", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die bildet man sich leicht, als Aale, welche klein,", "tokens": ["Die", "bil\u00b7det", "man", "sich", "leicht", ",", "als", "Aa\u00b7le", ",", "wel\u00b7che", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "PRF", "ADJD", "$,", "KOUS", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und rege, glatt, und schl\u00fcpfrig, ein,", "tokens": ["Und", "re\u00b7ge", ",", "glatt", ",", "und", "schl\u00fcpf\u00b7rig", ",", "ein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJA", "$,", "ADJD", "$,", "KON", "ADJD", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die sich stets schl\u00e4ngeln und sich dr\u00e4ngen,", "tokens": ["Die", "sich", "stets", "schl\u00e4n\u00b7geln", "und", "sich", "dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "VVINF", "KON", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Leicht gleiten, ohne sich woran zu h\u00e4ngen.", "tokens": ["Leicht", "glei\u00b7ten", ",", "oh\u00b7ne", "sich", "wo\u00b7ran", "zu", "h\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "KOUI", "PRF", "PWAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}