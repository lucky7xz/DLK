{"dta.poem.18841": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Verm\u00e4chtniss  \n alt persischen Glaubens .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Welch Verm\u00e4chtniss, Br\u00fcder, sollt\u2019 euch kommen", "tokens": ["Welch", "Ver\u00b7m\u00e4cht\u00b7niss", ",", "Br\u00fc\u00b7der", ",", "sollt'", "euch", "kom\u00b7men"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "NN", "$,", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von dem Scheidenden, dem armen Frommen?", "tokens": ["Von", "dem", "Schei\u00b7den\u00b7den", ",", "dem", "ar\u00b7men", "From\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Den ihr J\u00fcngeren geduldig n\u00e4hrtet,", "tokens": ["Den", "ihr", "J\u00fcn\u00b7ge\u00b7ren", "ge\u00b7dul\u00b7dig", "n\u00e4hr\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seine letzten Tage pflegend ehrtet.", "tokens": ["Sei\u00b7ne", "letz\u00b7ten", "Ta\u00b7ge", "pfle\u00b7gend", "ehr\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn wir oft gesehn den K\u00f6nig reiten,", "tokens": ["Wenn", "wir", "oft", "ge\u00b7sehn", "den", "K\u00f6\u00b7nig", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gold an ihm und Gold an allen Seiten,", "tokens": ["Gold", "an", "ihm", "und", "Gold", "an", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KON", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Edelstein auf ihn und seine Grossen", "tokens": ["E\u00b7del\u00b7stein", "auf", "ihn", "und", "sei\u00b7ne", "Gros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ausges\u00e4t wie dichte Hagelschlossen.", "tokens": ["Aus\u00b7ge\u00b7s\u00e4t", "wie", "dich\u00b7te", "Ha\u00b7gel\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Habt ihr jemals ihn darum beneidet?", "tokens": ["Habt", "ihr", "je\u00b7mals", "ihn", "da\u00b7rum", "be\u00b7nei\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "PAV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und nicht herrlicher den Blick geweidet,", "tokens": ["Und", "nicht", "herr\u00b7li\u00b7cher", "den", "Blick", "ge\u00b7wei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJA", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn die Sonne sich auf Morgenfl\u00fcgeln", "tokens": ["Wenn", "die", "Son\u00b7ne", "sich", "auf", "Mor\u00b7gen\u00b7fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PRF", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Darnawends unz\u00e4hligen Gipfelh\u00fcgeln", "tokens": ["Dar\u00b7na\u00b7wends", "un\u00b7z\u00e4h\u00b7li\u00b7gen", "Gip\u00b7fel\u00b7h\u00fc\u00b7geln"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}