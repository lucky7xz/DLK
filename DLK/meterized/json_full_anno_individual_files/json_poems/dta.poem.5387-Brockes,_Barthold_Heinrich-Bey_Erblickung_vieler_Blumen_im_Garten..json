{"dta.poem.5387": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bey Erblickung vieler Blumen im  \n Garten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es sucht mit tausend Lust allhier", "tokens": ["Es", "sucht", "mit", "tau\u00b7send", "Lust", "all\u00b7hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der bunten Blumen bunte Zier", "tokens": ["Der", "bun\u00b7ten", "Blu\u00b7men", "bun\u00b7te", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcber-wunder-sch\u00f6n Gepr\u00e4nge,", "tokens": ["Und", "\u00fc\u00b7ber\u00b7wun\u00b7der\u00b7sch\u00f6n", "Ge\u00b7pr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In der fast ungezehlten Menge", "tokens": ["In", "der", "fast", "un\u00b7ge\u00b7zehl\u00b7ten", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von Liljen, Mah-und Rosen,", "tokens": ["Von", "Lil\u00b7jen", ",", "Mah\u00b7\u00b7und", "Ro\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NE", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Augen lieblich liebzukosen,", "tokens": ["Den", "Au\u00b7gen", "lieb\u00b7lich", "lieb\u00b7zu\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zumahl wenn ein gelinder West,", "tokens": ["Zu\u00b7mahl", "wenn", "ein", "ge\u00b7lin\u00b7der", "West", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der sanft sie hin und wieder schwencket,", "tokens": ["Der", "sanft", "sie", "hin", "und", "wie\u00b7der", "schwen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Farben durch einander lencket,", "tokens": ["Die", "Far\u00b7ben", "durch", "ein\u00b7an\u00b7der", "len\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wodurch es denn nicht anders l\u00e4\u00dft,", "tokens": ["Wo\u00b7durch", "es", "denn", "nicht", "an\u00b7ders", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Als wenn, in bunt-und regem Schein,", "tokens": ["Als", "wenn", ",", "in", "bunt\u00b7\u00b7und", "re\u00b7gem", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "$,", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sie lebende Tapeten seyn.", "tokens": ["Sie", "le\u00b7ben\u00b7de", "Ta\u00b7pe\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VAINF", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Wer kann so sch\u00f6ne Decken sehen,", "tokens": ["Wer", "kann", "so", "sch\u00f6\u00b7ne", "De\u00b7cken", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die unsers Sch\u00f6pfers Allmachts-Hand,", "tokens": ["Die", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "All\u00b7machts\u00b7Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Uns zu erfreuen, ausgespannt,", "tokens": ["Uns", "zu", "er\u00b7freu\u00b7en", ",", "aus\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ohn\u2019 ihn in ihnen zu erh\u00f6hen?", "tokens": ["Ohn'", "ihn", "in", "ih\u00b7nen", "zu", "er\u00b7h\u00f6\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Es scheint der bunten Blumen-Fl\u00e4chen", "tokens": ["Es", "scheint", "der", "bun\u00b7ten", "Blu\u00b7men\u00b7Fl\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "So wundersch\u00f6n ", "tokens": ["So", "wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Nicht nur allein von ihres Sch\u00f6pfers Macht,", "tokens": ["Nicht", "nur", "al\u00b7lein", "von", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Sie scheint auch ", "tokens": ["Sie", "scheint", "auch"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Ein tausendfach gemischter Duft", "tokens": ["Ein", "tau\u00b7send\u00b7fach", "ge\u00b7mischter", "Duft"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.22": {"text": "Ef\u00fcllt und balsamirt die Luft,", "tokens": ["E\u00b7f\u00fcllt", "und", "bal\u00b7sa\u00b7mirt", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und saget gleichsam unserm Sinn:", "tokens": ["Und", "sa\u00b7get", "gleich\u00b7sam", "un\u00b7serm", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Lenckt doch auf uns die Sinnen hin!", "tokens": ["Lenckt", "doch", "auf", "uns", "die", "Sin\u00b7nen", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+----+-+", "measure": "dactylic.init"}, "line.25": {"text": "Jhr k\u00f6nnt durch Riechen, Sehn und Schmecken", "tokens": ["Ihr", "k\u00f6nnt", "durch", "Rie\u00b7chen", ",", "Sehn", "und", "Schme\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "So Lieb' als Macht in uns entdecken.", "tokens": ["So", "Lieb'", "als", "Macht", "in", "uns", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Die Menge so verschiedner S\u00e4fte,", "tokens": ["Die", "Men\u00b7ge", "so", "ver\u00b7schied\u00b7ner", "S\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Die Anmuth tausendfacher Kr\u00e4fte,", "tokens": ["Die", "An\u00b7muth", "tau\u00b7send\u00b7fa\u00b7cher", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}