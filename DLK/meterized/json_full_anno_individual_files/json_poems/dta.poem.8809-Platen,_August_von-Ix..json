{"dta.poem.8809": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Ix.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Dem liebsten folgen kann sie nicht, im Boden sprie\u00dft", "tokens": ["Dem", "liebs\u00b7ten", "fol\u00b7gen", "kann", "sie", "nicht", ",", "im", "Bo\u00b7den", "sprie\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "VVINF", "VMFIN", "PPER", "PTKNEG", "$,", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr seht der Rose sehnend Herz und l\u00e4chelt, stolze", "tokens": ["Ihr", "seht", "der", "Ro\u00b7se", "seh\u00b7nend", "Herz", "und", "l\u00e4\u00b7chelt", ",", "stol\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "NN", "KON", "VVFIN", "$,", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wahr ist's, sie leidet viel, doch auch wie viel genie\u00dft", "tokens": ["Wahr", "ist's", ",", "sie", "lei\u00b7det", "viel", ",", "doch", "auch", "wie", "viel", "ge\u00b7nie\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PPER", "VVFIN", "ADV", "$,", "ADV", "ADV", "KOKOM", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zwar fallen ihre Bl\u00e4tter ab, und flattern durch den", "tokens": ["Zwar", "fal\u00b7len", "ih\u00b7re", "Bl\u00e4t\u00b7ter", "ab", ",", "und", "flat\u00b7tern", "durch", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch jedes Bl\u00e4ttchen wird ein Stern, und Strahlen", "tokens": ["Doch", "je\u00b7des", "Bl\u00e4tt\u00b7chen", "wird", "ein", "Stern", ",", "und", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ART", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wohl euch, da\u00df ", "tokens": ["Wohl", "euch", ",", "da\u00df"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "PPER", "$,", "KOUS"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Weil ihren goldnen Busen doch vor euch verschlie\u00dft die", "tokens": ["Weil", "ih\u00b7ren", "gold\u00b7nen", "Bu\u00b7sen", "doch", "vor", "euch", "ver\u00b7schlie\u00dft", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "PPER", "VVFIN", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}