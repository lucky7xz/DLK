{"textgrid.poem.39857": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was liegt der Schlaf auf meinen Augenlidern", "tokens": ["Was", "liegt", "der", "Schlaf", "auf", "mei\u00b7nen", "Au\u00b7gen\u00b7li\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am hellen Tag? was ist mein Haupt so schwer?", "tokens": ["Am", "hel\u00b7len", "Tag", "?", "was", "ist", "mein", "Haupt", "so", "schwer", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "PWS", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bald ras't mein Puls, bald find' ich ihn nicht mehr!", "tokens": ["Bald", "ras't", "mein", "Puls", ",", "bald", "find'", "ich", "ihn", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Pickt schon der Totenwurm in meinen Gliedern?", "tokens": ["Pickt", "schon", "der", "To\u00b7ten\u00b7wurm", "in", "mei\u00b7nen", "Glie\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbdu bist nicht krank!\u00ab h\u00f6r' ich den Arzt erwiedern", "tokens": ["\u00bb", "du", "bist", "nicht", "krank", "!", "\u00ab", "h\u00f6r'", "ich", "den", "Arzt", "er\u00b7wie\u00b7dern"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "PTKNEG", "ADJD", "$.", "$(", "VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf dieser Klagen ungest\u00fcmes Heer.", "tokens": ["Auf", "die\u00b7ser", "Kla\u00b7gen", "un\u00b7ge\u00b7st\u00fc\u00b7mes", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbsetz' gegen deine ", "tokens": ["\u00bb", "setz'", "ge\u00b7gen", "dei\u00b7ne"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "APPR", "PPOSAT"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "La\u00df dir den tr\u00e4gen Mut Natur befiedern!", "tokens": ["La\u00df", "dir", "den", "tr\u00e4\u00b7gen", "Mut", "Na\u00b7tur", "be\u00b7fie\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Geh' in ein Bad, doch h\u00fcte dich zu baden;", "tokens": ["Geh'", "in", "ein", "Bad", ",", "doch", "h\u00fc\u00b7te", "dich", "zu", "ba\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NE", "$,", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Brunnen, doch das Glas nicht an den Mund,", "tokens": ["Zum", "Brun\u00b7nen", ",", "doch", "das", "Glas", "nicht", "an", "den", "Mund", ","], "token_info": ["word", "word", "punct", "word", "word", "word",