{"textgrid.poem.65195": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Wolken nicht, die, dunkel hingestreckt,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Wolken nicht, die, dunkel hingestreckt,", "tokens": ["Die", "Wol\u00b7ken", "nicht", ",", "die", ",", "dun\u00b7kel", "hin\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$,", "PRELS", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Uns nahe St\u00fcrme zu verk\u00fcnden scheinen,", "tokens": ["Uns", "na\u00b7he", "St\u00fcr\u00b7me", "zu", "ver\u00b7k\u00fcn\u00b7den", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was mich an unsrer Zeit zumeist erschreckt,", "tokens": ["Was", "mich", "an", "uns\u00b7rer", "Zeit", "zu\u00b7meist", "er\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist ihre Lust am Niedern und Gemeinen.", "tokens": ["Ist", "ih\u00b7re", "Lust", "am", "Nie\u00b7dern", "und", "Ge\u00b7mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ins Reich der Kunst hat es sich eingedr\u00e4ngt,", "tokens": ["Ins", "Reich", "der", "Kunst", "hat", "es", "sich", "ein\u00b7ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Wahrheit hehren Namen schn\u00f6d mi\u00dfbrauchend,", "tokens": ["Der", "Wahr\u00b7heit", "heh\u00b7ren", "Na\u00b7men", "schn\u00f6d", "mi\u00df\u00b7brau\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von keiner Scheu und keiner Scham beengt,", "tokens": ["Von", "kei\u00b7ner", "Scheu", "und", "kei\u00b7ner", "Scham", "be\u00b7engt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Wollust sich in Schlamm und Pf\u00fctzen tauchend.", "tokens": ["Mit", "Wol\u00b7lust", "sich", "in", "Schlamm", "und", "Pf\u00fct\u00b7zen", "tau\u00b7chend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ergriff es erst von diesem Reich Besitz,", "tokens": ["Er\u00b7griff", "es", "erst", "von", "die\u00b7sem", "Reich", "Be\u00b7sitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wer k\u00f6nnte ferner seinen Lauf noch wenden?", "tokens": ["Wer", "k\u00f6nn\u00b7te", "fer\u00b7ner", "sei\u00b7nen", "Lauf", "noch", "wen\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "p