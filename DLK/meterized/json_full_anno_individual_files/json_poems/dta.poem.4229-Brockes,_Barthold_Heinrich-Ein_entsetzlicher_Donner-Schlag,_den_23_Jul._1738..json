{"dta.poem.4229": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein entsetzlicher Donner-Schlag,  \n den 23 Jul. 1738.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Blitz, Donnern, Krachen, Prasseln, Knallen,", "tokens": ["Blitz", ",", "Don\u00b7nern", ",", "Kra\u00b7chen", ",", "Pras\u00b7seln", ",", "Knal\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ersch\u00fcttern, Sto\u00dfweis\u2019 abwerts fallen,", "tokens": ["Er\u00b7sch\u00fct\u00b7tern", ",", "Sto\u00df\u00b7weis'", "ab\u00b7werts", "fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gepre\u00dft, bet\u00e4ubt von Schlag und Strahl,", "tokens": ["Ge\u00b7pre\u00dft", ",", "be\u00b7t\u00e4ubt", "von", "Schlag", "und", "Strahl", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erstarrt und sch\u00fcchtern um sich sehn,", "tokens": ["Er\u00b7starrt", "und", "sch\u00fcch\u00b7tern", "um", "sich", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kam, ward, war alles auf einmahl", "tokens": ["Kam", ",", "ward", ",", "war", "al\u00b7les", "auf", "ein\u00b7mahl"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VAFIN", "$,", "VAFIN", "PIS", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gesehn, geh\u00f6rt, gef\u00fchlt, geschehn.", "tokens": ["Ge\u00b7sehn", ",", "ge\u00b7h\u00f6rt", ",", "ge\u00b7f\u00fchlt", ",", "ge\u00b7schehn", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Recht oben \u00fcber meinem Schlo\u00df brach krachend ein ge-", "tokens": ["Recht", "o\u00b7ben", "\u00fc\u00b7ber", "mei\u00b7nem", "Schlo\u00df", "brach", "kra\u00b7chend", "ein", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "ADJD", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "pre\u00dfter Sto\u00df,", "tokens": ["pre\u00df\u00b7ter", "Sto\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Mit einem schmetternden Geprassel, zugleich in Blitz und", "tokens": ["Mit", "ei\u00b7nem", "schmet\u00b7tern\u00b7den", "Ge\u00b7pras\u00b7sel", ",", "zu\u00b7gleich", "in", "Blitz", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV", "APPR", "NN", "KON"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Flammen los.", "tokens": ["Flam\u00b7men", "los", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Der zwey mahl abgesetzte Knall macht\u2019, da\u00df mir zwey", "tokens": ["Der", "zwey", "mahl", "ab\u00b7ge\u00b7setz\u00b7te", "Knall", "macht'", ",", "da\u00df", "mir", "zwey"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "CARD", "ADV", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PPER", "CARD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "mahl in der Brust", "tokens": ["mahl", "in", "der", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Das Herz, als zwey mahl stark gestossen, gewaltsam sich", "tokens": ["Das", "Herz", ",", "als", "zwey", "mahl", "stark", "ges\u00b7tos\u00b7sen", ",", "ge\u00b7walt\u00b7sam", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "CARD", "ADV", "ADJD", "VVPP", "$,", "ADJD", "PRF"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "erheben mu\u00dft'.", "tokens": ["er\u00b7he\u00b7ben", "mu\u00dft'", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Den kleinen ", "tokens": ["Den", "klei\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Schlag,", "tokens": ["Schlag", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Da\u00df ihm die Angel-Ruht\u2019 entfiel, und von der Br\u00fcck\u2019 im", "tokens": ["Da\u00df", "ihm", "die", "An\u00b7gel\u00b7Ruht'", "ent\u00b7fiel", ",", "und", "von", "der", "Br\u00fcck", "im"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "APPRART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wasser lag:", "tokens": ["Was\u00b7ser", "lag", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Er selbst kam, bla\u00df als eine Leiche, mit \u00f6fters unterbrochnem", "tokens": ["Er", "selbst", "kam", ",", "bla\u00df", "als", "ei\u00b7ne", "Lei\u00b7che", ",", "mit", "\u00f6f\u00b7ters", "un\u00b7ter\u00b7broch\u00b7nem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,", "APPR", "ADV", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "Schnaufen,", "tokens": ["Schnau\u00b7fen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Zu mir ins hohe Thor-Gew\u00f6lbe, woselbst ich eben stand,", "tokens": ["Zu", "mir", "ins", "ho\u00b7he", "Thor\u00b7Ge\u00b7w\u00f6l\u00b7be", ",", "wo\u00b7selbst", "ich", "e\u00b7ben", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "gelaufen.", "tokens": ["ge\u00b7lau\u00b7fen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Ein mehr als Ellen-dicker Strahl fuhr, mit entsetzlicher", "tokens": ["Ein", "mehr", "als", "El\u00b7len\u00b7di\u00b7cker", "Strahl", "fuhr", ",", "mit", "ent\u00b7setz\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PIAT", "KOKOM", "NE", "NN", "VVFIN", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.24": {"text": "Gewalt,", "tokens": ["Ge\u00b7walt", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.25": {"text": "(jedoch, GOtt Lob! nicht unter sich) Nord-Ost-werts \u00fcber", "tokens": ["(", "je\u00b7doch", ",", "Gott", "Lob", "!", "nicht", "un\u00b7ter", "sich", ")", "Nord\u00b7Ost\u00b7werts", "\u00fc\u00b7ber"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADV", "$,", "NN", "NN", "$.", "PTKNEG", "APPR", "PRF", "$(", "NE", "APPR"], "meter": "+--+-+--++-+-", "measure": "iambic.hexa.invert"}, "line.26": {"text": "unserm Flecken,", "tokens": ["un\u00b7serm", "Fle\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}}}}}