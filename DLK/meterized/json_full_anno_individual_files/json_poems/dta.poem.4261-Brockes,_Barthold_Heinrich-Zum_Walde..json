{"dta.poem.4261": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zum Walde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In diesem Walde sa\u00df ich j\u00fcngst zur schw\u00fchlen Mittags-", "tokens": ["In", "die\u00b7sem", "Wal\u00b7de", "sa\u00df", "ich", "j\u00fcngst", "zur", "schw\u00fch\u00b7len", "Mit\u00b7tags"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zeit im Schatten,", "tokens": ["Zeit", "im", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "In einer Laube, deren Zweige so dichte sich verschr\u00e4nket", "tokens": ["In", "ei\u00b7ner", "Lau\u00b7be", ",", "de\u00b7ren", "Zwei\u00b7ge", "so", "dich\u00b7te", "sich", "ver\u00b7schr\u00e4n\u00b7ket"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELAT", "NN", "ADV", "VVFIN", "PRF", "VVFIN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "hatten,", "tokens": ["hat\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Da\u00df, durch der Bl\u00e4tter gr\u00fcne Schilde, wie stark der", "tokens": ["Da\u00df", ",", "durch", "der", "Bl\u00e4t\u00b7ter", "gr\u00fc\u00b7ne", "Schil\u00b7de", ",", "wie", "stark", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,", "PWAV", "ADJD", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Strahl der Sonne blitzt,", "tokens": ["Strahl", "der", "Son\u00b7ne", "blitzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Ich vor derselben schw\u00fchlen Gluht und gar zu strengem Licht", "tokens": ["Ich", "vor", "der\u00b7sel\u00b7ben", "schw\u00fch\u00b7len", "Gluht", "und", "gar", "zu", "stren\u00b7gem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PDAT", "ADJA", "NN", "KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "besch\u00fctzt,", "tokens": ["be\u00b7sch\u00fctzt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Statt einer Unlust eine Lust, in angenehmer K\u00fchlung,", "tokens": ["Statt", "ei\u00b7ner", "Un\u00b7lust", "ei\u00b7ne", "Lust", ",", "in", "an\u00b7ge\u00b7neh\u00b7mer", "K\u00fch\u00b7lung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "f\u00fchlte,", "tokens": ["f\u00fchl\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Die unter den belaubten Zweigen und allenthalben um", "tokens": ["Die", "un\u00b7ter", "den", "be\u00b7laub\u00b7ten", "Zwei\u00b7gen", "und", "al\u00b7len\u00b7thal\u00b7ben", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "KON", "ADV", "APPR"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "mich spielte.", "tokens": ["mich", "spiel\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Ich sah zu anfangs \u00fcber mir der Laube sch\u00f6n geflochtnes", "tokens": ["Ich", "sah", "zu", "an\u00b7fangs", "\u00fc\u00b7ber", "mir", "der", "Lau\u00b7be", "sch\u00f6n", "ge\u00b7flocht\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "APPR", "PPER", "ART", "NN", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "Dach,", "tokens": ["Dach", ","], "token_info": ["word", "punct"], "pos": ["PAV", "$,"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Worauf der Sonnen Strahl zwar fiel, doch durch das", "tokens": ["Wo\u00b7rauf", "der", "Son\u00b7nen", "Strahl", "zwar", "fiel", ",", "doch", "durch", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "NN", "ADV", "VVFIN", "$,", "ADV", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "dichte Laub nicht brach.", "tokens": ["dich\u00b7te", "Laub", "nicht", "brach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Da denn der zierlich- runde Bogen, und sein durchlauchtig-", "tokens": ["Da", "denn", "der", "zier\u00b7lich", "run\u00b7de", "Bo\u00b7gen", ",", "und", "sein", "durch\u00b7lauch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "TRUNC", "ADJA", "NN", "$,", "KON", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "zartes Gr\u00fcn", "tokens": ["zar\u00b7tes", "Gr\u00fcn"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Als ein mit Flei\u00df gemachter Thron, als ein gew\u00f6lbter", "tokens": ["Als", "ein", "mit", "Flei\u00df", "ge\u00b7mach\u00b7ter", "Thron", ",", "als", "ein", "ge\u00b7w\u00f6lb\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "APPR", "NN", "ADJA", "NN", "$,", "KOUS", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Baldachin,", "tokens": ["Bal\u00b7da\u00b7ch\u00b7in", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+---", "measure": "dactylic.init"}, "line.21": {"text": "Von selbst gewachsenem Smaragd, der lieblich schimmerte,", "tokens": ["Von", "selbst", "ge\u00b7wach\u00b7se\u00b7nem", "Sma\u00b7ragd", ",", "der", "lieb\u00b7lich", "schim\u00b7mer\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+--++-+-+-+", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "mir schien.", "tokens": ["mir", "schien", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.23": {"text": "Durch ihre Th\u00fcre sah ich nun in den bestrahlten Wald", "tokens": ["Durch", "ih\u00b7re", "Th\u00fc\u00b7re", "sah", "ich", "nun", "in", "den", "be\u00b7strahl\u00b7ten", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.24": {"text": "hinein,", "tokens": ["hin\u00b7ein", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.25": {"text": "Der \u00fcberall erf\u00fcllet war von Schatten und vom Sonnen-", "tokens": ["Der", "\u00fc\u00b7be\u00b7rall", "er\u00b7f\u00fcl\u00b7let", "war", "von", "Schat\u00b7ten", "und", "vom", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "VAFIN", "APPR", "NN", "KON", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.26": {"text": "schein.", "tokens": ["schein", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.27": {"text": "Von linder W\u00e4rm und sanfter K\u00fchlung ein nicht unan-", "tokens": ["Von", "lin\u00b7der", "W\u00e4rm", "und", "sanf\u00b7ter", "K\u00fch\u00b7lung", "ein", "nicht", "un\u00b7an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "ART", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "genehm Gew\u00fchl", "tokens": ["ge\u00b7nehm", "Ge\u00b7w\u00fchl"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Schien mit einander sich zu mischen,", "tokens": ["Schien", "mit", "ein\u00b7an\u00b7der", "sich", "zu", "mi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch einen temperirten Grad, das sanft gestreichelte", "tokens": ["Durch", "ei\u00b7nen", "tem\u00b7per\u00b7ir\u00b7ten", "Grad", ",", "das", "sanft", "ge\u00b7strei\u00b7chel\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+---+-+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Gef\u00fchl", "tokens": ["Ge\u00b7f\u00fchl"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Von unserm C\u00f6rper zu erfrischen.", "tokens": ["Von", "un\u00b7serm", "C\u00f6r\u00b7per", "zu", "er\u00b7fri\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die\u00df zier- und liebliche Gemenge, da bald das Licht, und", "tokens": ["Die\u00df", "zier", "und", "lieb\u00b7li\u00b7che", "Ge\u00b7men\u00b7ge", ",", "da", "bald", "das", "Licht", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "TRUNC", "KON", "ADJA", "NN", "$,", "KOUS", "ADV", "ART", "NN", "$,", "KON"], "meter": "-+-+--+---+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "bald der Schatten,", "tokens": ["bald", "der", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Jm sanften Streit und holden Wechsel, die Oberhand", "tokens": ["Jm", "sanf\u00b7ten", "Streit", "und", "hol\u00b7den", "Wech\u00b7sel", ",", "die", "O\u00b7ber\u00b7hand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KON", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "und Vorzug hatten,", "tokens": ["und", "Vor\u00b7zug", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "War allen Augen, doch am meisten den Augen, die bedacht-", "tokens": ["War", "al\u00b7len", "Au\u00b7gen", ",", "doch", "am", "meis\u00b7ten", "den", "Au\u00b7gen", ",", "die", "be\u00b7dacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "$,", "ADV", "PTKA", "PIS", "ART", "NN", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "sam sehn,", "tokens": ["sam", "sehn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Ein angenehmer Gegenwurf. Unglaublich lieblich, wun-", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7mer", "Ge\u00b7gen\u00b7wurf", ".", "Un\u00b7glaub\u00b7lich", "lieb\u00b7lich", ",", "wun"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADJD", "ADJD", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "dersch\u00f6n,", "tokens": ["der\u00b7sch\u00f6n", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Durchsichtig war hier das Gewebe der zarten Bl\u00e4tter,", "tokens": ["Durch\u00b7sich\u00b7tig", "war", "hier", "das", "Ge\u00b7we\u00b7be", "der", "zar\u00b7ten", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.14": {"text": "dunkel dort,", "tokens": ["dun\u00b7kel", "dort", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Wodurch jedoch, bald hie, bald da, ein hell-erscheinend", "tokens": ["Wo\u00b7durch", "je\u00b7doch", ",", "bald", "hie", ",", "bald", "da", ",", "ein", "hell\u00b7er\u00b7schei\u00b7nend"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "$,", "ADV", "ADV", "$,", "ADV", "ADV", "$,", "ART", "NN"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "schnelles Licht,", "tokens": ["schnel\u00b7les", "Licht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Durch vieler Stellen Dunkelheit, noch desto mehr erh\u00f6het,", "tokens": ["Durch", "vie\u00b7ler", "Stel\u00b7len", "Dun\u00b7kel\u00b7heit", ",", "noch", "des\u00b7to", "mehr", "er\u00b7h\u00f6\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.18": {"text": "bricht.", "tokens": ["bricht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.19": {"text": "Es scheinet zwar in diesem holden, mit Laub und Kraut", "tokens": ["Es", "schei\u00b7net", "zwar", "in", "die\u00b7sem", "hol\u00b7den", ",", "mit", "Laub", "und", "Kraut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PDAT", "ADJA", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "verwachs'nen Ort", "tokens": ["ver\u00b7wachs'\u00b7nen", "Ort"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "In einer einfach- sch\u00f6nen Pracht der Bl\u00e4tter Schmuck,", "tokens": ["In", "ei\u00b7ner", "ein\u00b7fach", "sch\u00f6\u00b7nen", "Pracht", "der", "Bl\u00e4t\u00b7ter", "Schmuck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "TRUNC", "ADJA", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "das Gr\u00fcn allein,", "tokens": ["das", "Gr\u00fcn", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Hier alle Stellen zu erf\u00fcllen, an allen Orten allgemein,", "tokens": ["Hier", "al\u00b7le", "Stel\u00b7len", "zu", "er\u00b7f\u00fcl\u00b7len", ",", "an", "al\u00b7len", "Or\u00b7ten", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKZU", "VVINF", "$,", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.24": {"text": "Zwar sch\u00f6n, doch blo\u00df nur gr\u00fcn zu seyn.", "tokens": ["Zwar", "sch\u00f6n", ",", "doch", "blo\u00df", "nur", "gr\u00fcn", "zu", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADV", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Jedoch in tausendfacher Aendrung, in ungez\u00e4hlter Pracht", "tokens": ["Je\u00b7doch", "in", "tau\u00b7send\u00b7fa\u00b7cher", "A\u00b7en\u00b7drung", ",", "in", "un\u00b7ge\u00b7z\u00e4hl\u00b7ter", "Pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.26": {"text": "und Zier,", "tokens": ["und", "Zier", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.27": {"text": "Versch\u00f6nert, mehret, mildert, st\u00e4rket, und zeiget sich das", "tokens": ["Ver\u00b7sch\u00f6\u00b7nert", 