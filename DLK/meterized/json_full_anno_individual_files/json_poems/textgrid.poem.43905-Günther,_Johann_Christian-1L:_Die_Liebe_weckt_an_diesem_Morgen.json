{"textgrid.poem.43905": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Liebe weckt an diesem Morgen", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe weckt an diesem Morgen", "tokens": ["Die", "Lie\u00b7be", "weckt", "an", "die\u00b7sem", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Kummer der verliebten Sorgen", "tokens": ["Den", "Kum\u00b7mer", "der", "ver\u00b7lieb\u00b7ten", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit mir gar zeitig wieder auf;", "tokens": ["Mit", "mir", "gar", "zei\u00b7tig", "wie\u00b7der", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seufzer wachen in dem Munde,", "tokens": ["Die", "Seuf\u00b7zer", "wa\u00b7chen", "in", "dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Thr\u00e4nen suchen aus dem Grunde", "tokens": ["Die", "Thr\u00e4\u00b7nen", "su\u00b7chen", "aus", "dem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Herzens ihren alten Lauf.", "tokens": ["Des", "Her\u00b7zens", "ih\u00b7ren", "al\u00b7ten", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Schmiedin meiner s\u00fc\u00dfen Kette", "tokens": ["Die", "Schmie\u00b7din", "mei\u00b7ner", "s\u00fc\u00b7\u00dfen", "Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zieht meine Faulheit aus dem Bette,", "tokens": ["Zieht", "mei\u00b7ne", "Faul\u00b7heit", "aus", "dem", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In welchem sie der Schlaf noch wiegt.", "tokens": ["In", "wel\u00b7chem", "sie", "der", "Schlaf", "noch", "wiegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Auge schl\u00e4ft, ich aber weine,", "tokens": ["Ihr", "Au\u00b7ge", "schl\u00e4ft", ",", "ich", "a\u00b7ber", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Einsamkeit sizt auf dem Steine,", "tokens": ["Die", "Ein\u00b7sam\u00b7keit", "sizt", "auf", "dem", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Der mir an meinem Herzen liegt.", "tokens": ["Der", "mir", "an", "mei\u00b7nem", "Her\u00b7zen", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, denck ich, bringt dies nahe Scheiden", "tokens": ["Ach", ",", "denck", "ich", ",", "bringt", "dies", "na\u00b7he", "Schei\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PRELS", "PPER", "$,", "VVFIN", "PDS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von ihrer Brust ein solches Leiden,", "tokens": ["Von", "ih\u00b7rer", "Brust", "ein", "sol\u00b7ches", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da nur ein Zimmer uns zertrennt,", "tokens": ["Da", "nur", "ein", "Zim\u00b7mer", "uns", "zer\u00b7trennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer wird doch meine Wunden heilen,", "tokens": ["Wer", "wird", "doch", "mei\u00b7ne", "Wun\u00b7den", "hei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Land und Luft uns einmahl theilen", "tokens": ["Wenn", "Land", "und", "Luft", "uns", "ein\u00b7mahl", "thei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Schweidniz mir kein Brodt mehr g\u00f6nnt?", "tokens": ["Und", "Schweid\u00b7niz", "mir", "kein", "Brodt", "mehr", "g\u00f6nnt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Z\u00e4hren m\u00fchn sich, meinen Klagen", "tokens": ["Die", "Z\u00e4h\u00b7ren", "m\u00fchn", "sich", ",", "mei\u00b7nen", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PRF", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit stummer Sprache nachzusagen,", "tokens": ["Mit", "stum\u00b7mer", "Spra\u00b7che", "nach\u00b7zu\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Allein die Angst vertrocknet sie.", "tokens": ["Al\u00b7lein", "die", "Angst", "ver\u00b7trock\u00b7net", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, wem vertrau ich diesen Jammer?", "tokens": ["Ach", ",", "wem", "ver\u00b7trau", "ich", "die\u00b7sen", "Jam\u00b7mer", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der freyen Luft, der tauben Kammer,", "tokens": ["Der", "frey\u00b7en", "Luft", ",", "der", "tau\u00b7ben", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und beides ist vergebne M\u00fch.", "tokens": ["Und", "bei\u00b7des", "ist", "ver\u00b7geb\u00b7ne", "M\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Redligkeit von deinem Herzen,", "tokens": ["Die", "Red\u00b7lig\u00b7keit", "von", "dei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Getreues Kind, bringt meinen Schmerzen", "tokens": ["Ge\u00b7treu\u00b7es", "Kind", ",", "bringt", "mei\u00b7nen", "Schmer\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Heimligkeit der schweren Noth;", "tokens": ["Die", "Heim\u00b7lig\u00b7keit", "der", "schwe\u00b7ren", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich deucht, die Last wird halb so leichte,", "tokens": ["Mich", "deucht", ",", "die", "Last", "wird", "halb", "so", "leich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So bald ich dir den Kummer beichte,", "tokens": ["So", "bald", "ich", "dir", "den", "Kum\u00b7mer", "beich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der mir den lezten Abschied droht.", "tokens": ["Der", "mir", "den", "lez\u00b7ten", "Ab\u00b7schied", "droht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Schnidt ich mein Elend in die Rinden,", "tokens": ["Schnidt", "ich", "mein", "E\u00b7lend", "in", "die", "Rin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erzehlt ich es den sanften Winden,", "tokens": ["Er\u00b7zehlt", "ich", "es", "den", "sanf\u00b7ten", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So seh ich \u00fcberall Gefahr:", "tokens": ["So", "seh", "ich", "\u00fc\u00b7be\u00b7rall", "Ge\u00b7fahr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dort kan der Vorwiz scheeler Augen", "tokens": ["Dort", "kan", "der", "Vor\u00b7wiz", "schee\u00b7ler", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bald Nahrung zu der Misgunst sauge", "tokens": ["Bald", "Nah\u00b7rung", "zu", "der", "Mis\u00b7gunst", "sau\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier macht es Echo ofenbahr.", "tokens": ["Hier", "macht", "es", "E\u00b7cho", "o\u00b7fen\u00b7bahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Von dir weis ich, verschwiegne Seele,", "tokens": ["Von", "dir", "weis", "ich", ",", "ver\u00b7schwieg\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da\u00df deine Zunge stets verheele,", "tokens": ["Da\u00df", "dei\u00b7ne", "Zun\u00b7ge", "stets", "ver\u00b7hee\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was dir ein guter Freund vertraut;", "tokens": ["Was", "dir", "ein", "gu\u00b7ter", "Freund", "ver\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich suche Trost, las mein Begehren", "tokens": ["Ich", "su\u00b7che", "Trost", ",", "las", "mein", "Be\u00b7geh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Unschuld diesen Wuntsch gew\u00e4hren", "tokens": ["Der", "Un\u00b7schuld", "die\u00b7sen", "Wunt\u00b7sch", "ge\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der jezt auf deine Gro\u00dfmuth baut.", "tokens": ["Der", "jezt", "auf", "dei\u00b7ne", "Gro\u00df\u00b7muth", "baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Erf\u00fclle, was ich such und glaube,", "tokens": ["Er\u00b7f\u00fcl\u00b7le", ",", "was", "ich", "such", "und", "glau\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erbarme dich der fl\u00fcchtgen Taube,", "tokens": ["Er\u00b7bar\u00b7me", "dich", "der", "fl\u00fccht\u00b7gen", "Tau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die deine Schoos zur Freystatt wehlt;", "tokens": ["Die", "dei\u00b7ne", "Schoos", "zur", "Freys\u00b7tatt", "wehlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie k\u00fcmmert sich um ihren Gatten", "tokens": ["Sie", "k\u00fcm\u00b7mert", "sich", "um", "ih\u00b7ren", "Gat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und sucht in deiner B\u00e4ume Schatten", "tokens": ["Und", "sucht", "in", "dei\u00b7ner", "B\u00e4u\u00b7me", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Ruh, so ihr zu Hause fehlt.", "tokens": ["Die", "Ruh", ",", "so", "ihr", "zu", "Hau\u00b7se", "fehlt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die Liebe weckt an diesem Morgen", "tokens": ["Die", "Lie\u00b7be", "weckt", "an", "die\u00b7sem", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Kummer der verliebten Sorgen", "tokens": ["Den", "Kum\u00b7mer", "der", "ver\u00b7lieb\u00b7ten", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit mir gar zeitig wieder auf;", "tokens": ["Mit", "mir", "gar", "zei\u00b7tig", "wie\u00b7der", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seufzer wachen in dem Munde,", "tokens": ["Die", "Seuf\u00b7zer", "wa\u00b7chen", "in", "dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Thr\u00e4nen suchen aus dem Grunde", "tokens": ["Die", "Thr\u00e4\u00b7nen", "su\u00b7chen", "aus", "dem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Herzens ihren alten Lauf.", "tokens": ["Des", "Her\u00b7zens", "ih\u00b7ren", "al\u00b7ten", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Schmiedin meiner s\u00fc\u00dfen Kette",