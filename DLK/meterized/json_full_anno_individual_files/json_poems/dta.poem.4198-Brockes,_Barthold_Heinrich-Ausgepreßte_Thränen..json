{"dta.poem.4198": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ausgepre\u00dfte Thr\u00e4nen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer mit betrachtendem Gem\u00fcht", "tokens": ["Wer", "mit", "be\u00b7trach\u00b7ten\u00b7dem", "Ge\u00b7m\u00fcht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und aufmerksamen Augen sieht,", "tokens": ["Und", "auf\u00b7merk\u00b7sa\u00b7men", "Au\u00b7gen", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was auf der ausgeschm\u00fcckten Welt,", "tokens": ["Was", "auf", "der", "aus\u00b7ge\u00b7schm\u00fcck\u00b7ten", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Bluhmen, Kr\u00e4utern, Fr\u00fccht\u2019 und Bl\u00fcht,", "tokens": ["In", "Bluh\u00b7men", ",", "Kr\u00e4u\u00b7tern", ",", "Fr\u00fccht'", "und", "Bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Uns unser Sch\u00f6pfer dargestellt,", "tokens": ["Uns", "un\u00b7ser", "Sch\u00f6p\u00b7fer", "dar\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird billig dergestalt ger\u00fchret,", "tokens": ["Wird", "bil\u00b7lig", "der\u00b7ge\u00b7stalt", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df er oft unvermerkt versp\u00fcret,", "tokens": ["Da\u00df", "er", "oft", "un\u00b7ver\u00b7merkt", "ver\u00b7sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie er, f\u00fcr Inbrunst, Freuden-Z\u00e4hren,", "tokens": ["Wie", "er", ",", "f\u00fcr", "In\u00b7brunst", ",", "Freu\u00b7den\u00b7Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Von Dank und Anmuht ausgepre\u00dft,", "tokens": ["Von", "Dank", "und", "An\u00b7muht", "aus\u00b7ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dem, Der ihm alles schenkt, zu Ehren,", "tokens": ["Dem", ",", "Der", "ihm", "al\u00b7les", "schenkt", ",", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "ART", "PPER", "PIS", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Aus frohen Augen fallen l\u00e4\u00dft.", "tokens": ["Aus", "fro\u00b7hen", "Au\u00b7gen", "fal\u00b7len", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Herr! la\u00df doch oft nicht mich allein,", "tokens": ["Herr", "!", "la\u00df", "doch", "oft", "nicht", "mich", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "ADV", "ADV", "PTKNEG", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Auch andre, so ger\u00fchret seyn,", "tokens": ["Auch", "and\u00b7re", ",", "so", "ge\u00b7r\u00fch\u00b7ret", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df wir Dein herrlichs Daseyn fassen,", "tokens": ["Da\u00df", "wir", "Dein", "herr\u00b7lichs", "Da\u00b7seyn", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und, durch der Werke Pracht ger\u00fchrt,", "tokens": ["Und", ",", "durch", "der", "Wer\u00b7ke", "Pracht", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Auf solche Art zu Dir gef\u00fchrt,", "tokens": ["Auf", "sol\u00b7che", "Art", "zu", "Dir", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Oft Freuden-Thr\u00e4nen fallen lassen!", "tokens": ["Oft", "Freu\u00b7den\u00b7Thr\u00e4\u00b7nen", "fal\u00b7len", "las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}