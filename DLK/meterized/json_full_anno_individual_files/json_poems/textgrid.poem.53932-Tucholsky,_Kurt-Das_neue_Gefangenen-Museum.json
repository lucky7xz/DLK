, "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "und wie wirs so herrlich weit gebracht \u2013", "tokens": ["und", "wie", "wirs", "so", "herr\u00b7lich", "weit", "ge\u00b7bracht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "ADV", "ADJD", "ADJD", "VVPP", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ehre sei Gott in der H\u00f6he \u2013!", "tokens": ["Eh\u00b7re", "sei", "Gott", "in", "der", "H\u00f6\u00b7he", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "NN", "APPR", "ART", "NN", "$(", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Wird alles darin zu sehen sein?", "tokens": ["Wird", "al\u00b7les", "da\u00b7rin", "zu", "se\u00b7hen", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PAV", "PTKZU", "VVINF", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "heidi?", "tokens": ["hei\u00b7di", "?"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Es gibt da so reizende K\u00e4mmerlein \u2013", "tokens": ["Es", "gibt", "da", "so", "rei\u00b7zen\u00b7de", "K\u00e4m\u00b7mer\u00b7lein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJA", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "hopla he!", "tokens": ["ho\u00b7pla", "he", "!"], "token_info": ["word", "word", "punct"], "pos": ["FM", "FM", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Darin stinkt es nachts nach menschlichem