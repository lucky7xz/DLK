, "und", "al\u00b7len", "Graus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "PIAT", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "hopla he!", "tokens": ["ho\u00b7pla", "he", "!"], "token_info": ["word", "word", "punct"], "pos": ["FM", "FM", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Und lasset uns doch auch ja nicht vergessen", "tokens": ["Und", "las\u00b7set", "uns", "doch", "auch", "ja", "nicht", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "die Fotos der viereckigen Aufseherfressen,", "tokens": ["die", "Fo\u00b7tos", "der", "vier\u00b7ec\u00b7ki\u00b7gen", "Auf\u00b7se\u00b7her\u00b7fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "und die Qual des Mannes, der in Grau lebt,", "tokens": ["und", "die", "Qual", "des", "Man\u00b7nes", ",", "der", "in", "Grau", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "und der Jahr um Jahr allein ohne Frau lebt \u2013 \u2013", "tokens": ["und", "der", "Jahr", "um", "Jahr", "al\u00b7lein", "oh\u00b7ne", "Frau", "lebt", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "ADV", "APPR", "NN", "VVFIN", "$(", "$("], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"},