{"dta.poem.20115": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Es gl\u00e4nzt der festgeschm\u00fcckte Saal von Rittern", "tokens": ["Es", "gl\u00e4nzt", "der", "fest\u00b7ge\u00b7schm\u00fcck\u00b7te", "Saal", "von", "Rit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wohl und Frauen,", "tokens": ["wohl", "und", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Vor allen ist Maria doch als K\u00f6nigin zu", "tokens": ["Vor", "al\u00b7len", "ist", "Ma\u00b7ria", "doch", "als", "K\u00f6\u00b7ni\u00b7gin", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VAFIN", "NE", "ADV", "KOUS", "NN", "PTKZU"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "schauen,", "tokens": ["schau\u00b7en", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Sie l\u00e4\u00dft die Zeit bei Spiel und Tanz in raschem", "tokens": ["Sie", "l\u00e4\u00dft", "die", "Zeit", "bei", "Spiel", "und", "Tanz", "in", "ra\u00b7schem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Flug enteilen,", "tokens": ["Flug", "ent\u00b7ei\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Und nur ihr Gatte z\u00f6gert noch des Festes Lust", "tokens": ["Und", "nur", "ihr", "Gat\u00b7te", "z\u00f6\u00b7gert", "noch", "des", "Fes\u00b7tes", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "zu theilen.", "tokens": ["zu", "thei\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Die Kerzen und die Wangen gl\u00fchn vor Freuden", "tokens": ["Die", "Ker\u00b7zen", "und", "die", "Wan\u00b7gen", "gl\u00fchn", "vor", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "um die Wette,", "tokens": ["um", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Es schreitet an Lord Seytons Hand Maria", "tokens": ["Es", "schrei\u00b7tet", "an", "Lord", "Sey\u00b7tons", "Hand", "Ma\u00b7ria"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NE", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "zum Bankette,", "tokens": ["zum", "Ban\u00b7ket\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.5": {"text": "Der Becher sch\u00e4umt, Maria winkt ein Saiten-", "tokens": ["Der", "Be\u00b7cher", "sch\u00e4umt", ",", "Ma\u00b7ria", "winkt", "ein", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "NE", "VVFIN", "ART", "TRUNC"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "spiel zu bringen,", "tokens": ["spiel", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ihr Liebling Rizzio nimmt es hin und hebet an", "tokens": ["Ihr", "Lieb\u00b7ling", "Riz\u00b7zio", "nimmt", "es", "hin", "und", "he\u00b7bet", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NE", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "zu singen:", "tokens": ["zu", "sin\u00b7gen", ":"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Der K\u00f6nig zog in finstrem Sinn", "tokens": ["Der", "K\u00f6\u00b7nig", "zog", "in", "fins\u00b7trem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinaus mit seinem Trosse;", "tokens": ["Hin\u00b7aus", "mit", "sei\u00b7nem", "Tros\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nachblickt die sch\u00f6ne K\u00f6nigin", "tokens": ["Nach\u00b7blickt", "die", "sch\u00f6\u00b7ne", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Reiter und dem Rosse.", "tokens": ["Dem", "Rei\u00b7ter", "und", "dem", "Ros\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und als des Waldes Laub und Moos", "tokens": ["Und", "als", "des", "Wal\u00b7des", "Laub", "und", "Moos"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den K\u00f6nig kaum erlaben,", "tokens": ["Den", "K\u00f6\u00b7nig", "kaum", "er\u00b7la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da lockt sie schon auf ihren Schoo\u00df", "tokens": ["Da", "lockt", "sie", "schon", "auf", "ih\u00b7ren", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den blonden Edelknaben.", "tokens": ["Den", "blon\u00b7den", "E\u00b7del\u00b7kna\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie streicht sein Haar, sie k\u00fcsst so hei\u00df", "tokens": ["Sie", "streicht", "sein", "Haar", ",", "sie", "k\u00fcsst", "so", "hei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lippen ihm und Wangen,", "tokens": ["Die", "Lip\u00b7pen", "ihm", "und", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die aber sind heut kalt wie Eis", "tokens": ["Die", "a\u00b7ber", "sind", "heut", "kalt", "wie", "Eis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VAFIN", "ADV", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und athmen kein Verlangen.", "tokens": ["Und", "ath\u00b7men", "kein", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie fl\u00fcstert: \u201elieber Knabe mein", "tokens": ["Sie", "fl\u00fcs\u00b7tert", ":", "\u201e", "lie\u00b7ber", "Kna\u00b7be", "mein"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Halt\u2019 fester mich in Armen,", "tokens": ["Halt'", "fes\u00b7ter", "mich", "in", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir wollen eins zur Stunde sein,", "tokens": ["Wir", "wol\u00b7len", "eins", "zur", "Stun\u00b7de", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "APPRART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das wird Dein Herz erwarmen.\u201c", "tokens": ["Das", "wird", "Dein", "Herz", "er\u00b7war\u00b7men", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er aber spricht: \u201e\u2019s l\u00e4\u00dft heut mich nicht", "tokens": ["Er", "a\u00b7ber", "spricht", ":", "\u201e", "'s", "l\u00e4\u00dft", "heut", "mich", "nicht"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$.", "$(", "PPER", "VVFIN", "ADV", "PPER", "PTKNEG"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Fest dr\u00fccken Dich und pressen,", "tokens": ["Fest", "dr\u00fc\u00b7cken", "Dich", "und", "pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich hatt\u2019 zur Nacht ein Traumgesicht", "tokens": ["Ich", "hatt'", "zur", "Nacht", "ein", "Traum\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das kann ich nicht vergessen:", "tokens": ["Das", "kann", "ich", "nicht", "ver\u00b7ges\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u201ees trat der K\u00f6nig vor mich hin", "tokens": ["\u201e", "es", "trat", "der", "K\u00f6\u00b7nig", "vor", "mich", "hin"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ich Dich wollte k\u00fcssen;", "tokens": ["Als", "ich", "Dich", "woll\u00b7te", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir ist so bang lieb K\u00f6nigin", "tokens": ["Mir", "ist", "so", "bang", "lieb", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als w\u00fcrd\u2019 ich sterben m\u00fcssen....\u201c", "tokens": ["Als", "w\u00fcrd'", "ich", "ster\u00b7ben", "m\u00fcs\u00b7sen", "....", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "VVINF", "VMINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u201e\u201eSo ", "tokens": ["\u201e", "\u201e", "So"], "token_info": ["punct", "punct", "word"], "pos": ["$(", "$(", "ADV"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Darnley ruft\u2019s dazwischen,", "tokens": ["Darn\u00b7ley", "ruft's", "da\u00b7zwi\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PAV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Es fegt im Nu sein Zornesblick die G\u00e4ste von", "tokens": ["Es", "fegt", "im", "Nu", "sein", "Zor\u00b7nes\u00b7blick", "die", "G\u00e4s\u00b7te", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADV", "PPOSAT", "NN", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "den Tischen,", "tokens": ["den", "Ti\u00b7schen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "\u201e\u201eStirb denn, und dank\u2019s im Tode mir, da\u00df", "tokens": ["\u201e", "\u201e", "Stirb", "denn", ",", "und", "dank's", "im", "To\u00b7de", "mir", ",", "da\u00df"], "token_info": ["punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "$(", "VVIMP", "ADV", "$,", "KON", "NE", "APPRART", "NN", "PPER", "$,", "KOUS"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "ich mit guter Klinge", "tokens": ["ich", "mit", "gu\u00b7ter", "Klin\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Zu Deinem b\u00f6sen Bubenlied das letzte Vers-", "tokens": ["Zu", "Dei\u00b7nem", "b\u00f6\u00b7sen", "Bu\u00b7ben\u00b7lied", "das", "letz\u00b7te", "Ver\u00b7s"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "lein singe.\u201c\u201c", "tokens": ["lein", "sin\u00b7ge", ".", "\u201c", "\u201c"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "VVFIN", "$.", "$(", "$("], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.10": {"line.1": {"text": "Es packt den S\u00e4nger Todesangst: in namen-", "tokens": ["Es", "packt", "den", "S\u00e4n\u00b7ger", "To\u00b7de\u00b7sangst", ":", "in", "na\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$.", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "losem Leide", "tokens": ["lo\u00b7sem", "Lei\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "H\u00e4lt fest er, wie ein zitternd Kind, sich an", "tokens": ["H\u00e4lt", "fest", "er", ",", "wie", "ein", "zit\u00b7ternd", "Kind", ",", "sich", "an"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "$,", "PWAV", "ART", "ADJD", "NN", "$,", "PRF", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Maria\u2019s Kleide,", "tokens": ["Ma\u00b7ria's", "Klei\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Die tritt, halb Furcht halb Zorn im Blick, her-", "tokens": ["Die", "tritt", ",", "halb", "Furcht", "halb", "Zorn", "im", "Blick", ",", "her"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "$,", "ADJD", "NN", "ADJD", "NN", "APPRART", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "vor ihn zu bewahren,", "tokens": ["vor", "ihn", "zu", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Umsonst, schon ist des K\u00f6nigs Schwert ihm durch", "tokens": ["Um\u00b7sonst", ",", "schon", "ist", "des", "K\u00f6\u00b7nigs", "Schwert", "ihm", "durch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VAFIN", "ART", "NN", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "die Brust gefahren.", "tokens": ["die", "Brust", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Es h\u00e4lt, die lange Nacht hindurch, Maria", "tokens": ["Es", "h\u00e4lt", ",", "die", "lan\u00b7ge", "Nacht", "hin\u00b7durch", ",", "Ma\u00b7ria"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "PAV", "$,", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Todtenwache,", "tokens": ["Tod\u00b7ten\u00b7wa\u00b7che", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "