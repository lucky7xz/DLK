{"dta.poem.19947": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Kurzes Erwachen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.71", "tl:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin im May gegangen", "tokens": ["Ich", "bin", "im", "May", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und hab es nicht gewu\u00dft,", "tokens": ["Und", "hab", "es", "nicht", "ge\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Also von Schmerz befangen", "tokens": ["Al\u00b7so", "von", "Schmerz", "be\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ist die erkrankte Brust.", "tokens": ["Ist", "die", "er\u00b7krank\u00b7te", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Vogel hat gesungen", "tokens": ["Ein", "Vo\u00b7gel", "hat", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im jungbelaubten Wald,", "tokens": ["Im", "jung\u00b7be\u00b7laub\u00b7ten", "Wald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ist in's Herz gedrungen", "tokens": ["Da", "ist", "in's", "Herz", "ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir seine Stimme bald.", "tokens": ["Mir", "sei\u00b7ne", "Stim\u00b7me", "bald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Vom Aug' ist mir gefallen", "tokens": ["Vom", "Aug'", "ist", "mir", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein schwerer Thr\u00e4nen-Thau,", "tokens": ["Ein", "schwe\u00b7rer", "Thr\u00e4\u00b7nen\u00b7\u00b7T\u00b7hau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Drauf sah den May ich wallen", "tokens": ["Drauf", "sah", "den", "May", "ich", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch Erd' und Himmel blau.", "tokens": ["Durch", "Erd'", "und", "Him\u00b7mel", "blau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Als Vogel ausgesungen", "tokens": ["Als", "Vo\u00b7gel", "aus\u00b7ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NE", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Flog er in's weite Land,", "tokens": ["Flog", "er", "in's", "wei\u00b7te", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wie sein Lied verklungen", "tokens": ["Und", "wie", "sein", "Lied", "ver\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um mich der May verschwand.", "tokens": ["Um", "mich", "der", "May", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}