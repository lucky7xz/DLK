{"textgrid.poem.49155": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Sterblied", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wol geschieht doch dem, mein Gott,", "tokens": ["Wie", "wol", "ge\u00b7schieht", "doch", "dem", ",", "mein", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ADV", "ART", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der fr\u00fch durch einen sanften Tod", "tokens": ["Der", "fr\u00fch", "durch", "ei\u00b7nen", "sanf\u00b7ten", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich machet von der Erden;", "tokens": ["Sich", "ma\u00b7chet", "von", "der", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es ist doch nichts mehr in der Welt,", "tokens": ["Es", "ist", "doch", "nichts", "mehr", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das einer Seele wolgef\u00e4llt,", "tokens": ["Das", "ei\u00b7ner", "See\u00b7le", "wol\u00b7ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die dort will Erbin werden;", "tokens": ["Die", "dort", "will", "Er\u00b7bin", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Verfolgung, Armuth, Schmach und Noth", "tokens": ["Ver\u00b7fol\u00b7gung", ",", "Ar\u00b7muth", ",", "Schmach", "und", "Noth"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist aller Frommen zeitlichs Brod.", "tokens": ["Ist", "al\u00b7ler", "From\u00b7men", "zeit\u00b7lichs", "Brod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "CARD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Muth zu deinem Dienst ist schlecht,", "tokens": ["Der", "Muth", "zu", "dei\u00b7nem", "Dienst", "ist", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Meisten sind des Mammons Knecht,", "tokens": ["Die", "Meis\u00b7ten", "sind", "des", "Mam\u00b7mons", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer h\u00e4lt was von der Tugend?", "tokens": ["Wer", "h\u00e4lt", "was", "von", "der", "Tu\u00b7gend", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Liebe schweigt, der Neid regiert,", "tokens": ["Die", "Lie\u00b7be", "schweigt", ",", "der", "Neid", "re\u00b7giert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Alter wird durch Ehr' verf\u00fchrt,", "tokens": ["Das", "Al\u00b7ter", "wird", "durch", "Ehr'", "ver\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch b\u00f6se Lust die Jugend;", "tokens": ["Durch", "b\u00f6\u00b7se", "Lust", "die", "Ju\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wer nicht hiermit stimmt \u00fcberein,", "tokens": ["Wer", "nicht", "hier\u00b7mit", "stimmt", "\u00fc\u00b7be\u00b7re\u00b7in", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "PAV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Der mu\u00df der Andern Scheusal sein.", "tokens": ["Der", "mu\u00df", "der", "An\u00b7dern", "Scheu\u00b7sal", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So w\u00e4chset immer fort die Schuld,", "tokens": ["So", "w\u00e4ch\u00b7set", "im\u00b7mer", "fort", "die", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man s\u00fcndigt stets auf die Geduld,", "tokens": ["Man", "s\u00fcn\u00b7digt", "stets", "auf", "die", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die du, Herr, mit uns tr\u00e4gest.", "tokens": ["Die", "du", ",", "Herr", ",", "mit", "uns", "tr\u00e4\u00b7gest", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "NN", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "--++-+-", "measure": "anapaest.init"}, "line.4": {"text": "Kommt aber einst das End' heran,", "tokens": ["Kommt", "a\u00b7ber", "einst", "das", "End'", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem Niemand sich entbrechen kann,", "tokens": ["Dem", "Nie\u00b7mand", "sich", "ent\u00b7bre\u00b7chen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da du ihn niederlegest,", "tokens": ["Da", "du", "ihn", "nie\u00b7der\u00b7le\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach Gott, mit was f\u00fcr Angst und Pein", "tokens": ["Ach", "Gott", ",", "mit", "was", "f\u00fcr", "Angst", "und", "Pein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "APPR", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mu\u00df solch ein Mensch umgeben sein!", "tokens": ["Mu\u00df", "solch", "ein", "Mensch", "um\u00b7ge\u00b7ben", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Himmel kommt ihm schrecklich f\u00fcr,", "tokens": ["Der", "Him\u00b7mel", "kommt", "ihm", "schreck\u00b7lich", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An den er nicht gedacht allhier,", "tokens": ["An", "den", "er", "nicht", "ge\u00b7dacht", "all\u00b7hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PTKNEG", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Welt kann ihm nicht dienen,", "tokens": ["Die", "Welt", "kann", "ihm", "nicht", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Satan sch\u00e4rft ihm dein Gericht,", "tokens": ["Der", "Sa\u00b7tan", "sch\u00e4rft", "ihm", "dein", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein eigen Herz das klagt und spricht:", "tokens": ["Sein", "ei\u00b7gen", "Herz", "das", "klagt", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PDS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer will mich hier vers\u00fchnen?", "tokens": ["Wer", "will", "mich", "hier", "ver\u00b7s\u00fch\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ja, wenn nicht deine Gnade w\u00e4r,", "tokens": ["Ja", ",", "wenn", "nicht", "dei\u00b7ne", "Gna\u00b7de", "w\u00e4r", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PTKNEG", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wo k\u00e4m' ihm Trost und Rettung her?", "tokens": ["Wo", "k\u00e4m'", "ihm", "Trost", "und", "Ret\u00b7tung", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer jung stirbt, stirbt mit wen'ger M\u00fch',", "tokens": ["Wer", "jung", "stirbt", ",", "stirbt", "mit", "wen'\u00b7ger", "M\u00fch'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wen du liebst, der stirbet fr\u00fch;", "tokens": ["Und", "wen", "du", "liebst", ",", "der", "stir\u00b7bet", "fr\u00fch", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warum solt' er sich kr\u00e4nken?", "tokens": ["Wa\u00b7rum", "solt'", "er", "sich", "kr\u00e4n\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Er kennt nicht Welt, nicht S\u00fcnde nicht,", "tokens": ["Er", "kennt", "nicht", "Welt", ",", "nicht", "S\u00fcn\u00b7de", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Darf nicht, wie und was ihm geschicht,", "tokens": ["Darf", "nicht", ",", "wie", "und", "was", "ihm", "ge\u00b7schicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "$,", "PWAV", "KON", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn er erliegt, gedenken;", "tokens": ["Wenn", "er", "er\u00b7liegt", ",", "ge\u00b7den\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Je wen'ger Jahr', je wen'ger Schuld,", "tokens": ["Je", "wen'\u00b7ger", "Jahr'", ",", "je", "wen'\u00b7ger", "Schuld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Je wen'ger Schuld, je gr\u00f6\u00dfre Huld.", "tokens": ["Je", "wen'\u00b7ger", "Schuld", ",", "je", "gr\u00f6\u00df\u00b7re", "Huld", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er geht zu dir, zu seinem Gott,", "tokens": ["Er", "geht", "zu", "dir", ",", "zu", "sei\u00b7nem", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beut unserm eiteln Wesen Spott,", "tokens": ["Beut", "un\u00b7serm", "ei\u00b7teln", "We\u00b7sen", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kommt weg aus allen M\u00e4ngeln,", "tokens": ["Kommt", "weg", "aus", "al\u00b7len", "M\u00e4n\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus M\u00fch' und Noth zur h\u00f6chsten Freud',", "tokens": ["Aus", "M\u00fch'", "und", "Noth", "zur", "h\u00f6chs\u00b7ten", "Freud'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus Eitelkeit zur Ewigkeit,", "tokens": ["Aus", "Ei\u00b7tel\u00b7keit", "zur", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Menschen zu den Engeln;", "tokens": ["Von", "Men\u00b7schen", "zu", "den", "En\u00b7geln", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den Zweck, danach so lang' uns bangt,", "tokens": ["Den", "Zweck", ",", "da\u00b7nach", "so", "lang'", "uns", "bangt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den hat er unvermerkt erlangt.", "tokens": ["Den", "hat", "er", "un\u00b7ver\u00b7merkt", "er\u00b7langt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O sel'ges End', o s\u00fc\u00dfer Tod,", "tokens": ["O", "sel'\u00b7ges", "End'", ",", "o", "s\u00fc\u00b7\u00dfer", "Tod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Komm', k\u00fcrz' uns auch doch unsre Noth,", "tokens": ["Komm'", ",", "k\u00fcrz'", "uns", "auch", "doch", "uns\u00b7re", "Noth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns, Gott, uns armen S\u00fcndern.", "tokens": ["Uns", ",", "Gott", ",", "uns", "ar\u00b7men", "S\u00fcn\u00b7dern", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gef\u00e4llt denn unser Alter dir,", "tokens": ["Ge\u00b7f\u00e4llt", "denn", "un\u00b7ser", "Al\u00b7ter", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So lass' uns, liebster Vater, hier", "tokens": ["So", "lass'", "uns", ",", "liebs\u00b7ter", "Va\u00b7ter", ",", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch sterben gleich den Kindern;", "tokens": ["Doch", "ster\u00b7ben", "gleich", "den", "Kin\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.