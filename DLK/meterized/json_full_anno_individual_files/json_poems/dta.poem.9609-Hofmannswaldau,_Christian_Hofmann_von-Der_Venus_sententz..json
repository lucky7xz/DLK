{"dta.poem.9609": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der Venus sententz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beist euch nicht ihr raben\u00e4\u00dfer/ sondern klaget mir den streit/", "tokens": ["Beist", "euch", "nicht", "ihr", "ra\u00b7be\u00b7n\u00e4\u00b7\u00dfer", "/", "son\u00b7dern", "kla\u00b7get", "mir", "den", "streit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Hei\u00dfet das respect erzeiget mir/ als eurer obrigkeit?", "tokens": ["Hei\u00b7\u00dfet", "das", "res\u00b7pect", "er\u00b7zei\u00b7get", "mir", "/", "als", "eu\u00b7rer", "ob\u00b7rig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "PPER", "$(", "KOUS", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Nehmt ein ander mahl in acht mir die sachen vorzutragen/", "tokens": ["Nehmt", "ein", "an\u00b7der", "mahl", "in", "acht", "mir", "die", "sa\u00b7chen", "vor\u00b7zu\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "ADV", "APPR", "CARD", "PPER", "ART", "ADJA", "VVIZU", "$("], "meter": "--+---+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sonsten werd\u2019 ich beyder part bald von etwas anders sagen.", "tokens": ["Sons\u00b7ten", "werd'", "ich", "bey\u00b7der", "part", "bald", "von", "et\u00b7was", "an\u00b7ders", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "ADV", "APPR", "PIS", "ADV", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Jtzo aber solt ihr wissen/ da\u00df ich nicht mehr leiden wil/", "tokens": ["Jt\u00b7zo", "a\u00b7ber", "solt", "ihr", "wis\u00b7sen", "/", "da\u00df", "ich", "nicht", "mehr", "lei\u00b7den", "wil", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF", "$(", "KOUS", "PPER", "PTKNEG", "ADV", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Da\u00df ihr euch zusam\u0303en zancket; drumb so schweiget alle still/", "tokens": ["Da\u00df", "ihr", "euch", "zu\u00b7sam\u0303en", "zan\u00b7cket", ";", "drumb", "so", "schwei\u00b7get", "al\u00b7le", "still", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$.", "PAV", "ADV", "VVFIN", "PIS", "ADJD", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Und nehmt di\u00df zum urtheil an/ gleich itzt solt ihr euch vers\u00f6hne\u0303/", "tokens": ["Und", "nehmt", "di\u00df", "zum", "ur\u00b7theil", "an", "/", "gleich", "itzt", "solt", "ihr", "euch", "ver\u00b7s\u00f6hn\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "APPRART", "NN", "PTKVZ", "$(", "ADV", "ADV", "VMFIN", "PPER", "PPER", "VVFIN", "$("], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und kein b\u00f6ses l\u00e4stermaul soll die andre mehr verh\u00f6hnen.", "tokens": ["Und", "kein", "b\u00f6\u00b7ses", "l\u00e4s\u00b7ter\u00b7maul", "soll", "die", "and\u00b7re", "mehr", "ver\u00b7h\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VMFIN", "ART", "PIS", "ADV", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Da\u00df inde\u00dfen aber keine von euch alten klagen darff/", "tokens": ["Da\u00df", "in\u00b7de\u00b7\u00dfen", "a\u00b7ber", "kei\u00b7ne", "von", "euch", "al\u00b7ten", "kla\u00b7gen", "darff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PIAT", "APPR", "PPER", "ADJA", "VVINF", "VMFIN", "$("], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Als w\u00e4r ich den jungen g\u00fcnstig und den alten gar zu scharff;", "tokens": ["Als", "w\u00e4r", "ich", "den", "jun\u00b7gen", "g\u00fcns\u00b7tig", "und", "den", "al\u00b7ten", "gar", "zu", "scharff", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ART", "ADJA", "ADJD", "KON", "ART", "ADJA", "ADV", "PTKA", "ADJD", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "So beschlie\u00df ich/ da\u00df hinfort keine jungfer soll auf erden/", "tokens": ["So", "be\u00b7schlie\u00df", "ich", "/", "da\u00df", "hin\u00b7fort", "kei\u00b7ne", "jung\u00b7fer", "soll", "auf", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "ADV", "PIAT", "ADJA", "VMFIN", "APPR", "NN", "$("], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Ob sie gleich schon l\u00e4nger lebt/ \u00fcber dreyzehn jahr alt werden.", "tokens": ["Ob", "sie", "gleich", "schon", "l\u00e4n\u00b7ger", "lebt", "/", "\u00fc\u00b7ber", "drey\u00b7zehn", "jahr", "alt", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$(", "APPR", "CARD", "NN", "ADJD", "VAINF", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}