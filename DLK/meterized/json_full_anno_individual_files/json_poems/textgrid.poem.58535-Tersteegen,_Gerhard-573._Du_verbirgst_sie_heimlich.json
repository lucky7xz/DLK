: "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dein stilles K\u00e4mmerlein", "tokens": ["Dein", "stil\u00b7les", "K\u00e4m\u00b7mer\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Sei Gottes Herz allein,", "tokens": ["Sei", "Got\u00b7tes", "Herz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "So hast du Ruh in allen,", "tokens": ["So", "hast", "du", "Ruh", "in", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wenngleich der Leib noch hier und da mu\u00df wallen.", "tokens": ["Wenn\u00b7gleich", "der", "Leib", "noch", "hier", "und", "da", "mu\u00df", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "ADV", "KON", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hinaus, hinaus", "tokens": ["Hin\u00b7aus", ",", "hin\u00b7aus"], "token_info": ["word", "punct", "word"], "pos": ["ADV", "$,", "APZR"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Aus diesem fremden Haus!", "tokens": ["Aus", "die\u00b7sem", "frem\u00b7den", "Haus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zur Ewigkeit dein Geist sich einw\u00e4rts kehre,", "tokens": ["Zur", "E\u00b7wig\u00b7keit", "dein", "Geist", "sich", "ein\u00b7w\u00e4rts", "keh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", 