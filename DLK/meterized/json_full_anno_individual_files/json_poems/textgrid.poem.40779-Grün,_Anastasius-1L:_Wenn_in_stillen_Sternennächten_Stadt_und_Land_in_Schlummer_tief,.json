{"textgrid.poem.40779": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn in stillen Sternenn\u00e4chten Stadt und Land in Schlummer tief,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn in stillen Sternenn\u00e4chten Stadt und Land in Schlummer tief,", "tokens": ["Wenn", "in", "stil\u00b7len", "Ster\u00b7nen\u00b7n\u00e4ch\u00b7ten", "Stadt", "und", "Land", "in", "Schlum\u00b7mer", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "NN", "KON", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und schon l\u00e4ngst von Markt und Pl\u00e4tzen sich das laute Volk verlief,", "tokens": ["Und", "schon", "l\u00e4ngst", "von", "Markt", "und", "Pl\u00e4t\u00b7zen", "sich", "das", "lau\u00b7te", "Volk", "ver\u00b7lief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "NN", "KON", "NN", "PRF", "PDS", "VVFIN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "O wie dann mein Fu\u00df so gerne durch die leeren Gassen wallt,", "tokens": ["O", "wie", "dann", "mein", "Fu\u00df", "so", "ger\u00b7ne", "durch", "die", "lee\u00b7ren", "Gas\u00b7sen", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "PPOSAT", "NN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wo durch ferne, weite Stra\u00dfen dumpfen Klangs sein Tritt verhallt!", "tokens": ["Wo", "durch", "fer\u00b7ne", ",", "wei\u00b7te", "Stra\u00b7\u00dfen", "dum\u00b7pfen", "Klangs", "sein", "Tritt", "ver\u00b7hallt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "$,", "ADJA", "NN", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Wie ein gro\u00dfes \u00f6des Schlachtfeld, schweigend liegt die Stadt vor mir,", "tokens": ["Wie", "ein", "gro\u00b7\u00dfes", "\u00f6\u00b7des", "Schlacht\u00b7feld", ",", "schwei\u00b7gend", "liegt", "die", "Stadt", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN", "$,", "ADJD", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Kleine Leidenschaften fochten ihre kleinen Schlachten hier;", "tokens": ["Klei\u00b7ne", "Lei\u00b7den\u00b7schaf\u00b7ten", "foch\u00b7ten", "ih\u00b7re", "klei\u00b7nen", "Schlach\u00b7ten", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Jetzt doch liegt gebreitet dr\u00fcber gro\u00dfe, stille Todtenruh,", "tokens": ["Jetzt", "doch", "liegt", "ge\u00b7brei\u00b7tet", "dr\u00fc\u00b7ber", "gro\u00b7\u00dfe", ",", "stil\u00b7le", "Tod\u00b7ten\u00b7ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "VVPP", "PAV", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und nur Geister und nur Tr\u00e4ume wallen leise ab und zu.", "tokens": ["Und", "nur", "Geis\u00b7ter", "und", "nur", "Tr\u00e4u\u00b7me", "wal\u00b7len", "lei\u00b7se", "ab", "und", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "KON", "ADV", "NN", "VVINF", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Droben leuchten die Gestirne! Jeder Stern im blauen Raum", "tokens": ["Dro\u00b7ben", "leuch\u00b7ten", "die", "Ge\u00b7stir\u00b7ne", "!", "Je\u00b7der", "Stern", "im", "blau\u00b7en", "Raum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PIAT", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Hat sein Menschenherz hier unten, dem er bringe lichten Traum;", "tokens": ["Hat", "sein", "Men\u00b7schen\u00b7herz", "hier", "un\u00b7ten", ",", "dem", "er", "brin\u00b7ge", "lich\u00b7ten", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADV", "$,", "PRELS", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Drum wohl thun sie so gesch\u00e4ftig, wenn wir Nachts im Schlummer ruh'n!", "tokens": ["Drum", "wohl", "thun", "sie", "so", "ge\u00b7sch\u00e4f\u00b7tig", ",", "wenn", "wir", "Nachts", "im", "Schlum\u00b7mer", "ruh'n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Doch es hat mein Sternlein droben heute wohl nicht viel zu thun? \u2013", "tokens": ["Doch", "es", "hat", "mein", "Stern\u00b7lein", "dro\u00b7ben", "heu\u00b7te", "wohl", "nicht", "viel", "zu", "thun", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "ADV", "PTKNEG", "ADV", "PTKZU", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Sch\u00fcttle, Himmel, deine Sterne nieder auf den Erdenball,", "tokens": ["Sch\u00fctt\u00b7le", ",", "Him\u00b7mel", ",", "dei\u00b7ne", "Ster\u00b7ne", "nie\u00b7der", "auf", "den", "Er\u00b7den\u00b7ball", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PPOSAT", "NN", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dicht als goldne Saatenk\u00f6rner in der Schl\u00e4fer Herzen all!", "tokens": ["Dicht", "als", "gold\u00b7ne", "Saa\u00b7ten\u00b7k\u00f6r\u00b7ner", "in", "der", "Schl\u00e4\u00b7fer", "Her\u00b7zen", "all", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ADJA", "NN", "APPR", "ART", "NN", "NN", "PIAT", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Da\u00df die blanke Silberbl\u00fcthe lichten Traums am n\u00e4chsten Tag", "tokens": ["Da\u00df", "die", "blan\u00b7ke", "Sil\u00b7ber\u00b7bl\u00fct\u00b7he", "lich\u00b7ten", "Traums", "am", "n\u00e4chs\u00b7ten", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Frei als reiche Frucht erwachsen, hell und golden schwellen mag!", "tokens": ["Frei", "als", "rei\u00b7che", "Frucht", "er\u00b7wach\u00b7sen", ",", "hell", "und", "gol\u00b7den", "schwel\u00b7len", "mag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ADJA", "NN", "VVPP", "$,", "ADJD", "KON", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Lieblich pl\u00e4tschern dort die Brunnen, silbern steigt des Springquells Pracht,", "tokens": ["Lieb\u00b7lich", "pl\u00e4t\u00b7schern", "dort", "die", "Brun\u00b7nen", ",", "sil\u00b7bern", "steigt", "des", "Spring\u00b7quells", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Rosen und Violen duften von den Fenstern durch die Nacht,", "tokens": ["Ro\u00b7sen", "und", "Vi\u00b7o\u00b7len", "duf\u00b7ten", "von", "den", "Fens\u00b7tern", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "O wie s\u00fc\u00df dort vom Balkone Nachtigallenlied erschallt!", "tokens": ["O", "wie", "s\u00fc\u00df", "dort", "vom", "Bal\u00b7ko\u00b7ne", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7lied", "er\u00b7schallt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "ADV", "APPRART", "NN", "NN", "VVPP", "$."], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Fast bed\u00fcnkt es mich, als wallte fern ich durch den gr\u00fcnen Wald.", "tokens": ["Fast", "be\u00b7d\u00fcnkt", "es", "mich", ",", "als", "wall\u00b7te", "fern", "ich", "durch", "den", "gr\u00fc\u00b7nen", "Wald", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "KOUS", "VVFIN", "ADJD", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Ueber Quell und Rosen aber, und Viol' und Nachtigall,", "tokens": ["Ue\u00b7ber", "Quell", "und", "Ro\u00b7sen", "a\u00b7ber", ",", "und", "Vi\u00b7ol'", "und", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ueber Domen und Pal\u00e4sten stand des Mondes Strahlenball,", "tokens": ["Ue\u00b7ber", "Do\u00b7men", "und", "Pa\u00b7l\u00e4s\u00b7ten", "stand", "des", "Mon\u00b7des", "Strah\u00b7len\u00b7ball", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wie ein leuchtender Gedanke heil'ger Freiheit, licht und klar! \u2013 \u2013", "tokens": ["Wie", "ein", "leuch\u00b7ten\u00b7der", "Ge\u00b7dan\u00b7ke", "heil'\u00b7ger", "Frei\u00b7heit", ",", "licht", "und", "klar", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJA", "NN", "$,", "NN", "KON", "ADJD", "$.", "$(", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "O wie schade, jammerschade, da\u00df es rings der einz'ge war!", "tokens": ["O", "wie", "scha\u00b7de", ",", "jam\u00b7mer\u00b7scha\u00b7de", ",", "da\u00df", "es", "rings", "der", "einz'\u00b7ge", "war", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "$,", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Wenn in stillen Sternenn\u00e4chten Stadt und Land in Schlummer tief,", "tokens": ["Wenn", "in", "stil\u00b7len", "Ster\u00b7nen\u00b7n\u00e4ch\u00b7ten", "Stadt", "und", "Land", "in", "Schlum\u00b7mer", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "NN", "KON", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und schon l\u00e4ngst von Markt und Pl\u00e4tzen sich das laute Volk verlief,", "tokens": ["Und", "schon", "l\u00e4ngst", "von", "Markt", "und", "Pl\u00e4t\u00b7zen", "sich", "das", "lau\u00b7te", "Volk", "ver\u00b7lief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "NN", "KON", "NN", "PRF", "PDS", "VVFIN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "O wie dann mein Fu\u00df so gerne durch die leeren Gassen wallt,", "tokens": ["O", "wie", "dann", "mein", "Fu\u00df", "so", "ger\u00b7ne", "durch", "die", "lee\u00b7ren", "Gas\u00b7sen", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "PPOSAT", "NN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4":