{"dta.poem.19670": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Feuerelement .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er. Du kannst mir glauben liebes Herz,               ", "tokens": ["Er", ".", "Du", "kannst", "mir", "glau\u00b7ben", "lie\u00b7bes", "Herz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "PPER", "VMFIN", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Geh dich am Bronnen frischen,", "tokens": ["Geh", "dich", "am", "Bron\u00b7nen", "fri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn heut die Stern am Himmel sind", "tokens": ["Wenn", "heut", "die", "Stern", "am", "Him\u00b7mel", "sind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Komm ich zu dir mein sch\u00f6nes Kind,", "tokens": ["Komm", "ich", "zu", "dir", "mein", "sch\u00f6\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da denkst du nicht der Schmerzen", "tokens": ["Da", "denkst", "du", "nicht", "der", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Herzen.", "tokens": ["Im", "Her\u00b7zen", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Sie. Geh hin und nimm ein k\u00fchles Bad,               ", "tokens": ["Sie", ".", "Geh", "hin", "und", "nimm", "ein", "k\u00fch\u00b7les", "Bad", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "NE", "PTKVZ", "KON", "VVIMP", "ART", "ADJA", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Thu dich im Thau erlaben,", "tokens": ["Thu", "dich", "im", "Thau", "er\u00b7la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn Feuer und Stroh beysammen sind,", "tokens": ["Wenn", "Feu\u00b7er", "und", "Stroh", "bey\u00b7sam\u00b7men", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Schnee darzwischen treibt der Wind,", "tokens": ["Den", "Schnee", "darz\u00b7wi\u00b7schen", "treibt", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So mu\u00df es dennoch brennen,", "tokens": ["So", "mu\u00df", "es", "den\u00b7noch", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ja brennen.", "tokens": ["Ja", "bren\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}