{"textgrid.poem.38138": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Wunderliche Zumuthung", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsmals zu Frankfurt an dem Main", "tokens": ["Eins\u00b7mals", "zu", "Frank\u00b7furt", "an", "dem", "Main"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Viel F\u00fcrsten th\u00e4ten ziehen ein,", "tokens": ["Viel", "F\u00fcrs\u00b7ten", "th\u00e4\u00b7ten", "zie\u00b7hen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihrer lutherischen Religion gem\u00e4\u00df,", "tokens": ["Ih\u00b7rer", "lu\u00b7the\u00b7ri\u00b7schen", "Re\u00b7li\u00b7gi\u00b7on", "ge\u00b7m\u00e4\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Nach dem Stift zu St. Barthelm\u00e4s.", "tokens": ["Nach", "dem", "Stift", "zu", "St.", "Bar\u00b7thel\u00b7m\u00e4s", "."], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als dieser Schlu\u00df ward offenbar,", "tokens": ["Als", "die\u00b7ser", "Schlu\u00df", "ward", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Volk ein gro\u00dfer Zulauf war;", "tokens": ["Vom", "Volk", "ein", "gro\u00b7\u00dfer", "Zu\u00b7lauf", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da nun ein Zeichen ward gel\u00e4ut,", "tokens": ["Da", "nun", "ein", "Zei\u00b7chen", "ward", "ge\u00b7l\u00e4ut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dadurch die Predigt angedeut,", "tokens": ["Da\u00b7durch", "die", "Pre\u00b7digt", "an\u00b7ge\u00b7deut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Siehe, da kam ein Priester dar,", "tokens": ["Sie\u00b7he", ",", "da", "kam", "ein", "Pries\u00b7ter", "dar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der dem Papstthum anh\u00e4ngig war:", "tokens": ["Der", "dem", "Papst\u00b7thum", "an\u00b7h\u00e4n\u00b7gig", "war", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Trat auf die Kanzel stracks hinauf.", "tokens": ["Trat", "auf", "die", "Kan\u00b7zel", "stracks", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Des wundert sich des Volkes Hauf,", "tokens": ["Des", "wun\u00b7dert", "sich", "des", "Vol\u00b7kes", "Hauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Th\u00e4t sich doch nicht besinnen lang,", "tokens": ["Th\u00e4t", "sich", "doch", "nicht", "be\u00b7sin\u00b7nen", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PTKNEG", "VVFIN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Sondern fing bald an den Gesang:", "tokens": ["Son\u00b7dern", "fing", "bald", "an", "den", "Ge\u00b7sang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Nun bitten wir den H. Geist", "tokens": ["Nun", "bit\u00b7ten", "wir", "den", "H.", "Geist"], "token_info": ["word", "word", "word", "word", "abbreviation", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NE", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Um den rechten Glauben allermeist.", "tokens": ["Um", "den", "rech\u00b7ten", "Glau\u00b7ben", "al\u00b7ler\u00b7meist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Da nun der Gesang vollendet was,", "tokens": ["Da", "nun", "der", "Ge\u00b7sang", "voll\u00b7en\u00b7det", "was", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "PIS", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Das Evangelium er las,", "tokens": ["Das", "E\u00b7van\u00b7ge\u00b7li\u00b7um", "er", "las", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Das Volk mit Fleis solchs h\u00f6ret an,", "tokens": ["Das", "Volk", "mit", "Fleis", "solchs", "h\u00f6\u00b7ret", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Doch, da ers wolt erkl\u00e4ren dann,", "tokens": ["Doch", ",", "da", "ers", "wolt", "er\u00b7kl\u00e4\u00b7ren", "dann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "VMFIN", "VVINF", "ADV", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.21": {"text": "Woltens nicht h\u00f6ren \u00fcberall", "tokens": ["Wol\u00b7tens", "nicht", "h\u00f6\u00b7ren", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "VVINF", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.22": {"text": "Fingen an mit fr\u00f6lichem Schall:", "tokens": ["Fin\u00b7gen", "an", "mit", "fr\u00f6\u00b7li\u00b7chem", "Schall", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.23": {"text": "\u00bbnun freut euch lieben Christen gemein", "tokens": ["\u00bb", "nun", "freut", "euch", "lie\u00b7ben", "Chris\u00b7ten", "ge\u00b7mein"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADJA", "NN", "ADJD"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.24": {"text": "Und la\u00dft uns fr\u00f6lich springen.\u00ab", "tokens": ["Und", "la\u00dft", "uns", "fr\u00f6\u00b7lich", "sprin\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Der Pfaff stand, wundert ob den Sachen,", "tokens": ["Der", "Pfaff", "stand", ",", "wun\u00b7dert", "ob", "den", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "VVFIN", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Weil man am Gesang kein End wolt machen;", "tokens": ["Weil", "man", "am", "Ge\u00b7sang", "kein", "End", "wolt", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Da stand er und ward gleich erstart,", "tokens": ["Da", "stand", "er", "und", "ward", "gleich", "er\u00b7start", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.28": {"text": "Letzlich er halb unsinnig ward,", "tokens": ["Letz\u00b7lich", "er", "halb", "un\u00b7sin\u00b7nig", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "ADJD", "VAFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.29": {"text": "Lief von der Kanzel ungest\u00fcm,", "tokens": ["Lief", "von", "der", "Kan\u00b7zel", "un\u00b7ge\u00b7st\u00fcm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.30": {"text": "Und ging mit gro\u00dfem Zorn und Grimm,", "tokens": ["Und", "ging", "mit", "gro\u00b7\u00dfem", "Zorn", "und", "Grimm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Zu einem J\u00fclichschen F\u00fcrsten dar,", "tokens": ["Zu", "ei\u00b7nem", "J\u00fc\u00b7lich\u00b7schen", "F\u00fcrs\u00b7ten", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Denn sonst noch kein F\u00fcrst drinnen war,", "tokens": ["Denn", "sonst", "noch", "kein", "F\u00fcrst", "drin\u00b7nen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PIAT", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Klagt ihm, er w\u00fcrd von seinem Ort", "tokens": ["Klagt", "ihm", ",", "er", "w\u00fcrd", "von", "sei\u00b7nem", "Ort"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Mit Gewalt, ohn Recht gedrungen fort,", "tokens": ["Mit", "Ge\u00b7walt", ",", "ohn", "Recht", "ge\u00b7drun\u00b7gen", "fort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "VVPP", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.35": {"text": "Und k\u00f6nnt sein Amt verrichten nicht,", "tokens": ["Und", "k\u00f6nnt", "sein", "Amt", "ver\u00b7rich\u00b7ten", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Das wollt er klagen ihm hiermit,", "tokens": ["Das", "wollt", "er", "kla\u00b7gen", "ihm", "hier\u00b7mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Und sollt er ihm auf diese Klag", "tokens": ["Und", "sollt", "er", "ihm", "auf", "die\u00b7se", "Klag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Zeugni\u00df geben am J\u00fcngsten Tag.", "tokens": ["Zeug\u00b7ni\u00df", "ge\u00b7ben", "am", "J\u00fcng\u00b7sten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.39": {"text": "Der F\u00fcrst sprach: \u00bbLieber Priester mein,", "tokens": ["Der", "F\u00fcrst", "sprach", ":", "\u00bb", "Lie\u00b7ber", "Pries\u00b7ter", "mein", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ADJD", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Die F\u00fcrsten kamen \u00fcberein,", "tokens": ["Die", "F\u00fcrs\u00b7ten", "ka\u00b7men", "\u00fc\u00b7be\u00b7re\u00b7in", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.41": {"text": "Da\u00df sie wolten an diesem Ort,", "tokens": ["Da\u00df", "sie", "wol\u00b7ten", "an", "die\u00b7sem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "PDAT", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.42": {"text": "Anh\u00f6ren das G\u00f6ttliche Wort,", "tokens": ["An\u00b7h\u00f6\u00b7ren", "das", "G\u00f6tt\u00b7li\u00b7che", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.43": {"text": "Von einem, welcher zugethan", "tokens": ["Von", "ei\u00b7nem", ",", "wel\u00b7cher", "zu\u00b7ge\u00b7than"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Ihrem Glauben und Religion,", "tokens": ["Ih\u00b7rem", "Glau\u00b7ben", "und", "Re\u00b7li\u00b7gi\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.45": {"text": "Solchem, der F\u00fcrsten Schlu\u00df gemein,", "tokens": ["Sol\u00b7chem", ",", "der", "F\u00fcrs\u00b7ten", "Schlu\u00df", "ge\u00b7mein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "NN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.46": {"text": "Solt ihr nicht widerstanden sein.", "tokens": ["Solt", "ihr", "nicht", "wi\u00b7der\u00b7stan\u00b7den", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Zudem k\u00f6mmt mir beschwerlich f\u00fcr,", "tokens": ["Zu\u00b7dem", "k\u00f6mmt", "mir", "be\u00b7schwer\u00b7lich", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "APPR", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.48": {"text": "Da\u00df ihr habt zugemuthet mir,", "tokens": ["Da\u00df", "ihr", "habt", "zu\u00b7ge\u00b7mu\u00b7thet", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVFIN", "PPER", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.49": {"text": "Ich soll von dieser eurer Klag", "tokens": ["Ich", "soll", "von", "die\u00b7ser", "eu\u00b7rer", "Klag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Zeugni\u00df geben am J\u00fcngsten Tag,", "tokens": ["Zeug\u00b7ni\u00df", "ge\u00b7ben", "am", "J\u00fcng\u00b7sten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.51": {"text": "Denn dort entweder werdet ihr", "tokens": ["Denn", "dort", "ent\u00b7we\u00b7der", "wer\u00b7det", "ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Nicht kommen wiederum zu mir,", "tokens": ["Nicht", "kom\u00b7men", "wie\u00b7de\u00b7rum", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Oder, wenn solches schon geschicht,", "tokens": ["O\u00b7der", ",", "wenn", "sol\u00b7ches", "schon", "ge\u00b7schicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "So werd ich euch doch kennen nicht.\u00ab", "tokens": ["So", "werd", "ich", "euch", "doch", "ken\u00b7nen", "nicht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "VVFIN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Hierauf lief der Pfaff davon mit Grimm,", "tokens": ["Hier\u00b7auf", "lief", "der", 