{"textgrid.poem.48774": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "61. Auf Ihr F\u00fcrstl. Gn. Herzogen Friedrichs zu Schleswig-Holstein Namenstag, 1639 Juli 18.", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Held einer Treflichkeit, die keine gleiche wei\u00df,", "tokens": ["Held", "ei\u00b7ner", "Tref\u00b7lich\u00b7keit", ",", "die", "kei\u00b7ne", "glei\u00b7che", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PIAT", "ADJA", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "als deiner Sonnen Licht, die Erde, Luft und Feste", "tokens": ["als", "dei\u00b7ner", "Son\u00b7nen", "Licht", ",", "die", "Er\u00b7de", ",", "Luft", "und", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$,", "ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mit seinem Glanz erf\u00fcllt, wir w\u00fcnschen deinem Weste", "tokens": ["mit", "sei\u00b7nem", "Glanz", "er\u00b7f\u00fcllt", ",", "wir", "w\u00fcn\u00b7schen", "dei\u00b7nem", "Wes\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "aus Ost und Norden Gl\u00fcck. Es wacht der Sternen Flei\u00df", "tokens": ["aus", "Ost", "und", "Nor\u00b7den", "Gl\u00fcck", ".", "Es", "wacht", "der", "Ster\u00b7nen", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "durch das gevierte Jahr f\u00fcr deinen gro\u00dfen Preis.", "tokens": ["durch", "das", "ge\u00b7vier\u00b7te", "Jahr", "f\u00fcr", "dei\u00b7nen", "gro\u00b7\u00dfen", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht deine ", "tokens": ["Nicht", "dei\u00b7ne"], "token_info": ["word", "word"], "pos": ["PTKNEG", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "auch ", "tokens": ["auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "und machet ihren Frost in deinem Weine hei\u00df.", "tokens": ["und", "ma\u00b7chet", "ih\u00b7ren", "Frost", "in", "dei\u00b7nem", "Wei\u00b7ne", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O du der ", "tokens": ["O", "du", "der"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "mehr deiner Christen Trost, la\u00df heute Freude herrschen", "tokens": ["mehr", "dei\u00b7ner", "Chris\u00b7ten", "Trost", ",", "la\u00df", "heu\u00b7te", "Freu\u00b7de", "herr\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,", "VVIMP", "ADV", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und schleu\u00df f\u