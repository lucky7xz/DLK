{"textgrid.poem.33946": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Sch\u00f6nheit der Welt", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00df ich nicht, wie du entsprungen,", "tokens": ["Wei\u00df", "ich", "nicht", ",", "wie", "du", "ent\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00df ich doch, was dich erh\u00e4lt,", "tokens": ["Wei\u00df", "ich", "doch", ",", "was", "dich", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was den Streit in dir bezwungen,", "tokens": ["Was", "den", "Streit", "in", "dir", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit ihm den Tod, o Welt!", "tokens": ["Und", "mit", "ihm", "den", "Tod", ",", "o", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "$,", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Zerst\u00f6rung wilde Triebe,", "tokens": ["Der", "Zer\u00b7st\u00f6\u00b7rung", "wil\u00b7de", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die kein Selbstgenu\u00df noch band,", "tokens": ["Die", "kein", "Selbst\u00b7ge\u00b7nu\u00df", "noch", "band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind erloschen in der Liebe,", "tokens": ["Sind", "er\u00b7lo\u00b7schen", "in", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seit du dich als sch\u00f6n erkannt!", "tokens": ["Seit", "du", "dich", "als", "sch\u00f6n", "er\u00b7kannt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "KOUS", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dem Adon, der sterben wollte,", "tokens": ["Dem", "A\u00b7don", ",", "der", "ster\u00b7ben", "woll\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zeigt sein erstes Bild das Grab,", "tokens": ["Zeigt", "sein", "ers\u00b7tes", "Bild", "das", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das im Flu\u00df ihn decken sollte,", "tokens": ["Das", "im", "Flu\u00df", "ihn", "de\u00b7cken", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er springt nicht mehr hinab.", "tokens": ["Und", "er", "springt", "nicht", "mehr", "hin\u00b7ab", "."]