{"textgrid.poem.47991": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Kuh'", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun wird es Friede; nun schweigt einmal", "tokens": ["Nun", "wird", "es", "Frie\u00b7de", ";", "nun", "schweigt", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$.", "ADV", "VVFIN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "des Lebens Schmerz;", "tokens": ["des", "Le\u00b7bens", "Schmerz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "es senkt sich der Mond ins tr\u00e4umende Tal", "tokens": ["es", "senkt", "sich", "der", "Mond", "ins", "tr\u00e4u\u00b7men\u00b7de", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und in mein Herz;", "tokens": ["und", "in", "mein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "die Sonne in schimmernden Fluten schwand,", "tokens": ["die", "Son\u00b7ne", "in", "schim\u00b7mern\u00b7den", "Flu\u00b7ten", "schwand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "die Bl\u00fcten schlie\u00dfen sich zu \u2013", "tokens": ["die", "Bl\u00fc\u00b7ten", "schlie\u00b7\u00dfen", "sich", "zu", "\u2013"], "token_info": ["word"