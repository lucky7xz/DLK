{"dta.poem.19488": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "S\u00e4erspruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Beme\u00dft den Schritt! Beme\u00dft den Schwung!", "tokens": ["Be\u00b7me\u00dft", "den", "Schritt", "!", "Be\u00b7me\u00dft", "den", "Schwung", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Erde bleibt noch lange jung!", "tokens": ["Die", "Er\u00b7de", "bleibt", "noch", "lan\u00b7ge", "jung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort f\u00e4llt ein Korn das stirbt und ruht.", "tokens": ["Dort", "f\u00e4llt", "ein", "Korn", "das", "stirbt", "und", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PDS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Ruh ist s\u00fc\u00df. Es hat es gut.", "tokens": ["Die", "Ruh", "ist", "s\u00fc\u00df", ".", "Es", "hat", "es", "gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier eins das durch die Scholle bricht.", "tokens": ["Hier", "eins", "das", "durch", "die", "Schol\u00b7le", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es hat es gut. S\u00fc\u00df ist das Licht.", "tokens": ["Es", "hat", "es", "gut", ".", "S\u00fc\u00df", "ist", "das", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$.", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und keines f\u00e4llt aus dieser Welt", "tokens": ["Und", "kei\u00b7nes", "f\u00e4llt", "aus", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und jedes f\u00e4llt wie's Gott gef\u00e4llt.", "tokens": ["Und", "je\u00b7des", "f\u00e4llt", "wie's", "Gott", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}