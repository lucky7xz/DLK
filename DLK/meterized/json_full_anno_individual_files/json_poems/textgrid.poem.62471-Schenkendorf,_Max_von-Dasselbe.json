{"textgrid.poem.62471": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Dasselbe", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft wenn im wunderbaren Schimmer", "tokens": ["Oft", "wenn", "im", "wun\u00b7der\u00b7ba\u00b7ren", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Schlosses Tr\u00fcmmer vor mir stehn,", "tokens": ["Des", "Schlos\u00b7ses", "Tr\u00fcm\u00b7mer", "vor", "mir", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Sonnenschein, glaub' ich noch immer", "tokens": ["Im", "Son\u00b7nen\u00b7schein", ",", "glaub'", "ich", "noch", "im\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In seiner Jugend es zu sehn.", "tokens": ["In", "sei\u00b7ner", "Ju\u00b7gend", "es", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit seinen Mauern, seinen Zinnen", "tokens": ["Mit", "sei\u00b7nen", "Mau\u00b7ern", ",", "sei\u00b7nen", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fern leuchtend in das freie Thal,", "tokens": ["Fern", "leuch\u00b7tend", "in