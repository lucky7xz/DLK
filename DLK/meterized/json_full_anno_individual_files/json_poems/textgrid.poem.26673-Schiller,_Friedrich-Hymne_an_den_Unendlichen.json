{"textgrid.poem.26673": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Hymne an den Unendlichen", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwischen Himmel und Erd, hoch in der L\u00fcfte Meer,", "tokens": ["Zwi\u00b7schen", "Him\u00b7mel", "und", "Erd", ",", "hoch", "in", "der", "L\u00fcf\u00b7te", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "In der Wiege des Sturms tr\u00e4gt mich ein Zackenfels,", "tokens": ["In", "der", "Wie\u00b7ge", "des", "Sturms", "tr\u00e4gt", "mich", "ein", "Za\u00b7cken\u00b7fels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wolken t\u00fcrmen", "tokens": ["Wol\u00b7ken", "t\u00fcr\u00b7men"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Unter mir sich zu St\u00fcrmen,", "tokens": ["Un\u00b7ter", "mir", "sich", "zu", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Schwindelnd gaukelt der Blick umher,", "tokens": ["Schwin\u00b7delnd", "gau\u00b7kelt", "der", "Blick", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Und ich denke dich, Ewiger.", "tokens": ["Und", "ich", "den\u00b7ke", "dich", ",", "E\u00b7wi\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "ADJA", "$."], "meter": "--+-+++-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Deinen schauernden Pomp borge dem Endlichen,", "tokens": ["Dei\u00b7nen", "schau\u00b7ern\u00b7den", "Pomp", "bor\u00b7ge", "dem", "End\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ungeheure Natur! Du, der Unendlichkeit", "tokens": ["Un\u00b7ge\u00b7heu\u00b7re", "Na\u00b7tur", "!", "Du", ",", "der", "Un\u00b7end\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "$,", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Riesentochter,", "tokens": ["Rie\u00b7sen\u00b7toch\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Sei mir Spiegel Jehovas!", "tokens": ["Sei", "mir", "Spie\u00b7gel", "Je\u00b7ho\u00b7vas", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seinen Gott dem vern\u00fcnftgen Wurm", "tokens": ["Sei\u00b7nen", "Gott", "dem", "ver\u00b7n\u00fcnft\u00b7gen", "Wurm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Orgle pr\u00e4chtig, Gewittersturm!", "tokens": ["Org\u00b7le", "pr\u00e4ch\u00b7tig", ",", "Ge\u00b7wit\u00b7ter\u00b7sturm", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "ADJD", "$,", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Horch! er orgelt \u2013 Den Fels, wie er herunterdr\u00f6hnt!", "tokens": ["Horch", "!", "er", "or\u00b7gelt", "\u2013", "Den", "Fels", ",", "wie", "er", "her\u00b7un\u00b7ter\u00b7dr\u00f6hnt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "$(", "ART", "NN", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Br\u00fcllend spricht der Orkan Zebaoths Namen aus.", "tokens": ["Br\u00fcl\u00b7lend", "spricht", "der", "Or\u00b7kan", "Ze\u00b7baoths", "Na\u00b7men", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Hingeschrieben", "tokens": ["Hin\u00b7ge\u00b7schrie\u00b7ben"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Mit dem Griffel des Blitzes:", "tokens": ["Mit", "dem", "Grif\u00b7fel", "des", "Blit\u00b7zes", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Schone, Herr! wir erkennen dich.", "tokens": ["Scho\u00b7ne", ",", "Herr", "!", "wir", "er\u00b7ken\u00b7nen", "dich", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Zwischen Himmel und Erd, hoch in der L\u00fcfte Meer,", "tokens": ["Zwi\u00b7schen", "Him\u00b7mel", "und", "Erd", ",", "hoch", "in", "der", "L\u00fcf\u00b7te", "Meer", ","], "