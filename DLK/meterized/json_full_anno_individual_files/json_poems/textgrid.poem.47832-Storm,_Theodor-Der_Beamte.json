{"textgrid.poem.47832": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der Beamte", "genre": "verse", "period": "N.A.", "pub_year": 1867, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er reibt sich die H\u00e4nde: \u00bbWir kriegen's jetzt!", "tokens": ["Er", "reibt", "sich", "die", "H\u00e4n\u00b7de", ":", "\u00bb", "Wir", "krie\u00b7gen's", "jetzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Auch der frechste Bursche sp\u00fcret", "tokens": ["Auch", "der", "frechs\u00b7te", "Bur\u00b7sche", "sp\u00fc\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schon bis hinab in die Fingerspitz',", "tokens": ["Schon", "bis", "hin\u00b7ab", "in", "die", "Fin\u00b7ger\u00b7spitz'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df von oben er wird regieret.", "tokens": ["Da\u00df", "von", "o\u00b7ben", "er", "wird", "re\u00b7gie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADV", "PPER", "VAFIN", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Bei jeder Geburt ist k\u00fcnftig sofort", "tokens": ["Bei", "je\u00b7der", "Ge\u00b7burt", "ist", "k\u00fcnf\u00b7tig", "so\u00b7fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ADJD", "ADV"],