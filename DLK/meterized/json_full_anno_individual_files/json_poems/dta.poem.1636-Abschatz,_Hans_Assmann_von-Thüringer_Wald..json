{"dta.poem.1636": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Th\u00fcringer Wald.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Osch\u00f6ne Gegend/ welche mir", "tokens": ["O\u00b7sch\u00f6\u00b7ne", "Ge\u00b7gend", "/", "wel\u00b7che", "mir"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$(", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das alte Deutschland stellet f\u00fcr/", "tokens": ["Das", "al\u00b7te", "Deutschland", "stel\u00b7let", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie unsrer Tuisconen Schaar", "tokens": ["Wie", "uns\u00b7rer", "Tu\u00b7is\u00b7co\u00b7nen", "Schaar"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vorzeiten so vergn\u00fcget war/", "tokens": ["Vor\u00b7zei\u00b7ten", "so", "ver\u00b7gn\u00fc\u00b7get", "war", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie sie dem linden Strome nach", "tokens": ["Wie", "sie", "dem", "lin\u00b7den", "Stro\u00b7me", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Silber-hellen Spiegel-Bach", "tokens": ["Der", "Sil\u00b7ber\u00b7hel\u00b7len", "Spie\u00b7gel\u00b7Bach"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In ein begr\u00fcntes Thal hinaus", "tokens": ["In", "ein", "be\u00b7gr\u00fcn\u00b7tes", "Thal", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gebauet manch geringes Hau\u00df/", "tokens": ["Ge\u00b7bau\u00b7et", "manch", "ge\u00b7rin\u00b7ges", "Hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In welchem sie mit Fried und Ruh", "tokens": ["In", "wel\u00b7chem", "sie", "mit", "Fried", "und", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ihr langes Alter brachten zu/", "tokens": ["Ihr", "lan\u00b7ges", "Al\u00b7ter", "brach\u00b7ten", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und/ ohne schn\u00f6des Mein und Dein/", "tokens": ["Und", "/", "oh\u00b7ne", "schn\u00f6\u00b7des", "Mein", "und", "Dein", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "APPR", "ADJA", "NN", "KON", "PPOSAT", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mit dem vergn\u00fcget konten seyn/", "tokens": ["Mit", "dem", "ver\u00b7gn\u00fc\u00b7get", "kon\u00b7ten", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Was sonder viel-gebrauchte M\u00fch", "tokens": ["Was", "son\u00b7der", "viel\u00b7ge\u00b7brauch\u00b7te", "M\u00fch"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Darreichte Wiese/ Wald und Vieh.", "tokens": ["Dar\u00b7reich\u00b7te", "Wie\u00b7se", "/", "Wald", "und", "Vieh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "O selig/ wer zu dem bereit", "tokens": ["O", "se\u00b7lig", "/", "wer", "zu", "dem", "be\u00b7reit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$(", "PWS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Beschl\u00fcssen kan den Lauff der Zeit/", "tokens": ["Be\u00b7schl\u00fcs\u00b7sen", "kan", "den", "Lauff", "der", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "In seines Vaterlandes Scho\u00df/", "tokens": ["In", "sei\u00b7nes", "Va\u00b7ter\u00b7lan\u00b7des", "Scho\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Von gutten Freunden nimmer blo\u00df/", "tokens": ["Von", "gut\u00b7ten", "Freun\u00b7den", "nim\u00b7mer", "blo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Um Sch\u00e4tz und G\u00fctter unbetr\u00fcbt/", "tokens": ["Um", "Sch\u00e4tz", "und", "G\u00fct\u00b7ter", "un\u00b7be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "In keine Pracht der Welt verliebt/", "tokens": ["In", "kei\u00b7ne", "Pracht", "der", "Welt", "ver\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "L\u00e4st seine beste Lust allein", "tokens": ["L\u00e4st", "sei\u00b7ne", "bes\u00b7te", "Lust", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Freund/ B\u00fccher und Gewissen seyn/", "tokens": ["Freund", "/", "B\u00fc\u00b7cher", "und", "Ge\u00b7wis\u00b7sen", "seyn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "KON", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Geniest des Orts in stiller Ruh", "tokens": ["Ge\u00b7niest", "des", "Orts", "in", "stil\u00b7ler", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Der seinen Sitten saget zu.", "tokens": ["Der", "sei\u00b7nen", "Sit\u00b7ten", "sa\u00b7get", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Ihm zeiget sich der gr\u00fcne Wald", "tokens": ["Ihm", "zei\u00b7get", "sich", "der", "gr\u00fc\u00b7ne", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Nicht minder n\u00fctz als wohlgestalt/", "tokens": ["Nicht", "min\u00b7der", "n\u00fctz", "als", "wohl\u00b7ge\u00b7stalt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "KOKOM", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Der Glutt und K\u00fchlung bey sich tr\u00e4gt/", "tokens": ["Der", "Glutt", "und", "K\u00fch\u00b7lung", "bey", "sich", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Und tausend S\u00e4nger in sich hegt.", "tokens": ["Und", "tau\u00b7send", "S\u00e4n\u00b7ger", "in", "sich", "hegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Da sieht er/ wie der klare Flu\u00df", "tokens": ["Da", "sieht", "er", "/", "wie", "der", "kla\u00b7re", "Flu\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Zum Tranck und Spiegel dienen mu\u00df:", "tokens": ["Zum", "Tranck", "und", "Spie\u00b7gel", "die\u00b7nen", "mu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Da giebt ihm Ruhe bey der Nacht", "tokens": ["Da", "giebt", "ihm", "Ru\u00b7he", "bey", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Vor Sorgen-voller Bette Pracht/", "tokens": ["Vor", "Sor\u00b7gen\u00b7vol\u00b7ler", "Bet\u00b7te", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Die mehr als Bley und Centner schwer/", "tokens": ["Die", "mehr", "als", "Bley", "und", "Cent\u00b7ner", "schwer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "KOKOM", "NN", "KON", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Das auffgebreitte Blumen-Heer/", "tokens": ["Das", "auff\u00b7ge\u00b7breit\u00b7te", "Blu\u00b7men\u00b7Heer", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Wenn seine Lichter schl\u00e4ffet ein", "tokens": ["Wenn", "sei\u00b7ne", "Lich\u00b7ter", "schl\u00e4f\u00b7fet", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Der viel beschaute Monden-Schein/", "tokens": ["Der", "viel", "be\u00b7schau\u00b7te", "Mon\u00b7den\u00b7Schein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Der Himmel und ein freyer Mutt", "tokens": ["Der", "Him\u00b7mel", "und", "ein", "frey\u00b7er", "Mutt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Deckt seine Glieder mehr als gutt/", "tokens": ["Deckt", "sei\u00b7ne", "Glie\u00b7der", "mehr", "als", "gutt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PIAT", "KOKOM", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Zur Speise dienet Frucht und Wild/", "tokens": ["Zur", "Spei\u00b7se", "die\u00b7net", "Frucht", "und", "Wild", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "KON", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "So den gesunden Magen f\u00fcllt", "tokens": ["So", "den", "ge\u00b7sun\u00b7den", "Ma\u00b7gen", "f\u00fcllt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Ohn eitle Sorge f\u00fcr den Leib.", "tokens": ["Ohn", "eit\u00b7le", "Sor\u00b7ge", "f\u00fcr", "den", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Gedancken seyn der Zeit-Vertreib/", "tokens": ["Ge\u00b7dan\u00b7cken", "seyn", "der", "Zeit\u00b7Ver\u00b7treib", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Die sich ohn allen Kummer frey", "tokens": ["Die", "sich", "ohn", "al\u00b7len", "Kum\u00b7mer", "frey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Dem Sternen-Dache schwingen bey/", "tokens": ["Dem", "Ster\u00b7nen\u00b7Da\u00b7che", "schwin\u00b7gen", "bey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Und hat er keinen Uberflu\u00df", "tokens": ["Und", "hat", "er", "kei\u00b7nen", "U\u00b7ber\u00b7flu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "So ist nichts/ was er darben mu\u00df.", "tokens": ["So", "ist", "nichts", "/", "was", "er", "dar\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "$(", "PWS", "PPER", "PAV", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}