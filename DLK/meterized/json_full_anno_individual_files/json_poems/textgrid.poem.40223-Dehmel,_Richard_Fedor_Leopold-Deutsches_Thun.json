{"textgrid.poem.40223": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Deutsches Thun", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieber Freund! Zu Besuch bin ich eben bei Goethen und Schillern,", "tokens": ["Lie\u00b7ber", "Freund", "!", "Zu", "Be\u00b7such", "bin", "ich", "e\u00b7ben", "bei", "Goe\u00b7then", "und", "Schil\u00b7lern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "APPR", "NN", "VAFIN", "PPER", "ADV", "APPR", "NE", "KON", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "da \u2013 aus dem Schornstein herab \u2013 fliegt dein toscanischer Gru\u00df.", "tokens": ["da", "\u2013", "aus", "dem", "Schorn\u00b7stein", "her\u00b7ab", "\u2013", "fliegt", "dein", "tos\u00b7ca\u00b7ni\u00b7scher", "Gru\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "APPR", "ART", "NN", "ADV", "$(", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "N\u00e4mlich die hellen Gem\u00e4cher und gl\u00e4nzenden S\u00e4le der Beiden", "tokens": ["N\u00e4m\u00b7lich", "die", "hel\u00b7len", "Ge\u00b7m\u00e4\u00b7cher", "und", "gl\u00e4n\u00b7zen\u00b7den", "S\u00e4\u00b7le", "der", "Bei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "hatt' ich verlassen und war bis in die K\u00fcche gefolgt,", "tokens": ["hatt'", "ich", "ver\u00b7las\u00b7sen", "und", "war", "bis", "in", "die", "K\u00fc\u00b7che", "ge\u00b7folgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "KON", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "wo sie im Dienste des Morgens besorgten das pfeffrige Fr\u00fchst\u00fcck", "tokens": ["wo", "sie", "im", "Diens\u00b7te", "des", "Mor\u00b7gens", "be\u00b7sorg\u00b7ten", "das", "pfeff\u00b7ri\u00b7ge", "Fr\u00fch\u00b7st\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ART", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "gegen den n\u00e4chtlichen Rausch ihrer verjammerten Zeit.", "tokens": ["ge\u00b7gen", "den", "n\u00e4cht\u00b7li\u00b7chen", "Rausch", "ih\u00b7rer", "ver\u00b7jam\u00b7mer\u00b7ten", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Doch nun empfahl ich mich schnell; und Goethe l\u00e4chelte nickend,", "tokens": ["Doch", "nun", "emp\u00b7fahl", "ich", "mich", "schnell", ";", "und", "Goe\u00b7the", "l\u00e4\u00b7chel\u00b7te", "ni\u00b7ckend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "$.", "KON", "NE", "VVFIN", "VVPP", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "denn er witterte wohl etwas Italisches gleich.", "tokens": ["denn", "er", "wit\u00b7ter\u00b7te", "wohl", "et\u00b7was", "I\u00b7ta\u00b7li\u00b7sches", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PIAT", "NN", "ADV", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Und mein Handwerkszeug ergriff ich und wollte geschwinde", "tokens": ["Und", "mein", "Hand\u00b7werks\u00b7zeug", "er\u00b7griff", "ich", "und", "woll\u00b7te", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "KON", "VMFIN", "ADJA"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "dir im gef\u00fcgigen ", "tokens": ["dir", "im", "ge\u00b7f\u00fc\u00b7gi\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "APPRART", "ADJA"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.11": {"text": "aber da sa\u00df mir der K\u00fcchengeruch von Goethen und Schillern", "tokens": ["a\u00b7ber", "da", "sa\u00df", "mir", "der", "K\u00fc\u00b7chen\u00b7ge\u00b7ruch", "von", "Goe\u00b7then", "und", "Schil\u00b7lern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "NE", "KON", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "fest in der Nase und hat ganz das Gehirn mir bet\u00e4ubt.", "tokens": ["fest", "in", "der", "Na\u00b7se", "und", "hat", "ganz", "das", "Ge\u00b7hirn", "mir", "be\u00b7t\u00e4ubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "KON", "VAFIN", "ADV", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Ja! sie haben gar Manchen auf ihrem olymp'schen Gewissen,", "tokens": ["Ja", "!", "sie", "ha\u00b7ben", "gar", "Man\u00b7chen", "auf", "ih\u00b7rem", "o\u00b7lym\u00b7p'\u00b7schen", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "ADV", "PIS", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "seit sie ihr deutsches Gericht f\u00fcllten in griechisch Geschirr.", "tokens": ["seit", "sie", "ihr", "deut\u00b7sches", "Ge\u00b7richt", "f\u00fcll\u00b7ten", "in", "grie\u00b7chisch", "Ge\u00b7schirr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ADJD", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Oder liegt es dem Deutschen im ", "tokens": ["O\u00b7der", "liegt", "es", "dem", "Deut\u00b7schen", "im"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPRART"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "immer zu h\u00fcllen ein Kleid, das er der Fremde geraubt?!", "tokens": ["im\u00b7mer", "zu", "h\u00fcl\u00b7len", "ein", "Kleid", ",", "das", "er", "der", "Frem\u00b7de", "ge\u00b7raubt", "?!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Mi\u00dft er nicht Freiheit und Recht sich zu nach der R\u00f6mischen Elle,", "tokens": ["Mi\u00dft", "er", "nicht", "Frei\u00b7heit", "und", "Recht", "sich", "zu", "nach", "der", "R\u00f6\u00b7mi\u00b7schen", "El\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "KON", "NN", "PRF", "APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.18": {"text": "gab nicht zum Bau seines Staats Gallien das Winkelma\u00df her?", "tokens": ["gab", "nicht", "zum", "Bau", "sei\u00b7nes", "Staats", "Gal\u00b7li\u00b7en", "das", "Win\u00b7kel\u00b7ma\u00df", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPRART", "NN", "PPOSAT", "NN", "NE", "ART", "NN", "PTKVZ", "$."], "meter": "+--+---+-+-+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Will in die Tiefen der Welt, in der Menschheit Tiefen er dringen,", "tokens": ["Will", "in", "die", "Tie\u00b7fen", "der", "Welt", ",", "in", "der", "Menschheit", "Tie\u00b7fen", "er", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "steigt er hinab durch den Schacht, welchen der Britte ihm grub!", "tokens": ["steigt", "er", "hin\u00b7ab", "durch", "den", "Schacht", ",", "wel\u00b7chen", "der", "Brit\u00b7te", "ihm", "grub", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,", "PWAT", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.21": {"text": "Oder sucht er im Glauben zu ruhn, so klimmt er zum Himmel", "tokens": ["O\u00b7der", "sucht", "er", "im", "Glau\u00b7ben", "zu", "ruhn", ",", "so", "klimmt", "er", "zum", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,", "ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.22": {"text": "gar auf der Leiter empor, die ihm der Jude gef\u00fcgt! \u2013", "tokens": ["gar", "auf", "der", "Lei\u00b7ter", "em\u00b7por", ",", "die", "ihm", "der", "Ju\u00b7de", "ge\u00b7f\u00fcgt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$.", "$("], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Doch nun ", "tokens": ["Doch", "nun"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.24": {"text": "und es entschwebt dem Gewirr sieghaft ein Einiger Geist!", "tokens": ["und", "es", "ent\u00b7schwebt", "dem", "Ge\u00b7wirr", "sieg\u00b7haft", "ein", "Ei\u00b7ni\u00b7ger", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Zwar die ", "tokens": ["Zwar", "die"], "token_info": ["word", "word"], "pos": ["ADV", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "aber die ", "tokens": ["a\u00b7ber", "die"], "token_info": ["word", "word"], "pos": ["ADV", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.27": {"text": "da\u00df er rauschender noch rege den Fittig dereinst:", "tokens": ["da\u00df", "er", "rau\u00b7schen\u00b7der", "noch", "re\u00b7ge", "den", "Fit\u00b7tig", "de\u00b7reinst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJA", "ART", "NN", "ADV", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "bis er die Fernen erreicht, wo dem Blick auch die V\u00f6lker verschwinden,", "tokens": ["bis", "er", "die", "Fer\u00b7nen", "er\u00b7reicht", ",", "wo", "dem", "Blick", "auch", "die", "V\u00f6l\u00b7ker", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "PWAV", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.29": {"text": "wo ihn, das ewige Haupt neigend, die ", "tokens": ["wo", "ihn", ",", "das", "e\u00b7wi\u00b7ge", "Haupt", "nei\u00b7gend", ",", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "$,", "ART", "ADJA", "NN", "ADJD", "$,", "PRELS"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Ja \u2013 mein Volk! den Beruf, den ", "tokens": ["Ja", "\u2013", "mein", "Volk", "!", "den", "Be\u00b7ruf", ",", "den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PTKANT", "$(", "PPOSAT", "NN", "$.", "ART", "NN", "$,", "ART"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.31": {"text": "hast du die Andern erl\u00f6st, hast du dich selber erl\u00f6st!", "tokens": ["hast", "du", "die", "An\u00b7dern", "er\u00b7l\u00f6st", ",", "hast", "du", "dich", "sel\u00b7ber", "er\u00b7l\u00f6st", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "VVFIN", "$,", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.32": {"text": "Nein! ", "tokens": ["Nein", "!"], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.33": {"text": "So auch hab' ich getrost mir gesattelt das Ro\u00df des Hellenen,", "tokens": ["So", "auch", "hab'", "ich", "ge\u00b7trost", "mir", "ge\u00b7sat\u00b7telt", "das", "Ro\u00df", "des", "Hel\u00b7le\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVFIN", "PPER", "VVPP", "ART", "NN", "ART", "NN", "$,"], "meter": "-+---+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "da\u00df ich mit Eigener Faust reite Parade vor Dir.", "tokens": ["da\u00df", "ich", "mit", "Ei\u00b7ge\u00b7ner", "Faust", "rei\u00b7te", "Pa\u00b7ra\u00b7de", "vor", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "L\u00e4cheln wirst du vielleicht: ", "tokens": ["L\u00e4\u00b7cheln", "wirst", "du", "viel\u00b7leicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.36": {"text": "da\u00df umso sicherer nur throne das \u00e4rmliche Ich?! \u2013", "tokens": ["da\u00df", "um\u00b7so", "si\u00b7che\u00b7rer", "nur", "thro\u00b7ne", "das", "\u00e4rm\u00b7li\u00b7che", "Ich", "?!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "ADJD", "ADV", "VVFIN", "ART", "ADJA", "PPER", "$.", "$("], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.37": {"text": "Aber so geht es wohl stets: der Gedanken jeglicher deutet", "tokens": ["A\u00b7ber", "so", "geht", "es", "wohl", "stets", ":", "der", "Ge\u00b7dan\u00b7ken", "jeg\u00b7li\u00b7cher", "deu\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$.", "ART", "NN", "PIAT", "VVFIN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.38": {"text": "immer aufs Ganze zur\u00fcck, immer vom Ganzen auf Uns.", "tokens": ["im\u00b7mer", "aufs", "Gan\u00b7ze", "zu\u00b7r\u00fcck", ",", "im\u00b7mer", "vom", "Gan\u00b7zen", "auf", "Uns", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKVZ", "$,", "ADV", "APPRART", "NN", "APPR", "PPER", "$."], "meter": "+--+---+--+-+-", "measure": "dactylic.di.plus"}, "line.39": {"text": "Und so schwanket der Mensch dahin zwischen Rechten und Pflichten;", "tokens": ["Und", "so", "schwan\u00b7ket", "der", "Mensch", "da\u00b7hin", "zwi\u00b7schen", "Rech\u00b7ten", "und", "Pflich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PAV", "APPR", "NN", "KON", "NN", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.40": {"text": "heilig erscheint ihm ", "tokens": ["hei\u00b7lig", "er\u00b7scheint", "ihm"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.41": {"text": "Darum hab' ich mich auch der Zweifel und F\u00fcrchte begeben,", "tokens": ["Da\u00b7rum", "hab'", "ich", "mich", "auch", "der", "Zwei\u00b7fel", "und", "F\u00fcrch\u00b7te", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PRF", "ADV", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "ob ich ", "tokens": ["ob", "ich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.43": {"text": "Denn ich hab' es erkannt, die Vers\u00f6hnung ruht in der", "tokens": ["Denn", "ich", "hab'", "