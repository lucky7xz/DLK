{"dta.poem.3852": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine Ehe-Frau.  \n  A. C. Nierin.  1739.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Allhier liegt und schl\u00e4fft mit Frieden, ein belieb-", "tokens": ["All\u00b7hier", "liegt", "und", "schl\u00e4fft", "mit", "Frie\u00b7den", ",", "ein", "be\u00b7lie\u00b7b"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$,", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "tes Ehe-Weib,", "tokens": ["tes", "E\u00b7he\u00b7Weib", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Eine GOtt-ergebne Seele; deren abgematter Leib", "tokens": ["Ei\u00b7ne", "Got\u00b7ter\u00b7geb\u00b7ne", "See\u00b7le", ";", "de\u00b7ren", "ab\u00b7ge\u00b7mat\u00b7ter", "Leib"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$.", "PRELAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "In der s\u00fc\u00dften Nuhe liegt. O wie sanffte wird sie", "tokens": ["In", "der", "s\u00fc\u00df\u00b7ten", "Nu\u00b7he", "liegt", ".", "O", "wie", "sanff\u00b7te", "wird", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$.", "NE", "KOKOM", "VVFIN", "VAFIN", "PPER"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "schlaffen!", "tokens": ["schlaf\u00b7fen", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Nunmehr ist sie an den Port des so l\u00e4ngst erw\u00fcnsch-", "tokens": ["Nun\u00b7mehr", "ist", "sie", "an", "den", "Port", "des", "so", "l\u00e4ngst", "er\u00b7w\u00fcn\u00b7sch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "ADV", "ADV", "TRUNC"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "ten Hafen,", "tokens": ["ten", "Ha\u00b7fen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Den sie allezeit gesuchet und darnach gerungen hat.", "tokens": ["Den", "sie", "al\u00b7le\u00b7zeit", "ge\u00b7su\u00b7chet", "und", "dar\u00b7nach", "ge\u00b7run\u00b7gen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "KON", "PAV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Nunmehr schwebet ihre Seele dort in Elims Pal-", "tokens": ["Nun\u00b7mehr", "schwe\u00b7bet", "ih\u00b7re", "See\u00b7le", "dort", "in", "E\u00b7lims", "Pa\u00b7l"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "NE", "TRUNC"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.10": {"text": "men-Stadt.", "tokens": ["men\u00b7Stadt", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Mann und Kinder, hemmt die Z\u00e4hren, dort solt ihr", "tokens": ["Mann", "und", "Kin\u00b7der", ",", "hemmt", "die", "Z\u00e4h\u00b7ren", ",", "dort", "solt", "ihr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "VVFIN", "ART", "NN", "$,", "ADV", "VMFIN", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "sie wieder sehen,", "tokens": ["sie", "wie\u00b7der", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Sie ruft aus der Ruhe-Kammer: ", "tokens": ["Sie", "ruft", "aus", "der", "Ru\u00b7he\u00b7Kam\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "ist mir geschehen.", "tokens": ["ist", "mir", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}