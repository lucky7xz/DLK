{"dta.poem.13597": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Terzett .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Doch wenn die Th\u00e4ler unten l\u00e4ngst schon dunkeln,", "tokens": ["Doch", "wenn", "die", "Th\u00e4\u00b7ler", "un\u00b7ten", "l\u00e4ngst", "schon", "dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seh' ich vom Berge noch die Sonne funkeln,", "tokens": ["Seh'", "ich", "vom", "Ber\u00b7ge", "noch", "die", "Son\u00b7ne", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Adler st\u00fcrzt sich jauchzend in die Gluten,", "tokens": ["Der", "Ad\u00b7ler", "st\u00fcrzt", "sich", "jauch\u00b7zend", "in", "die", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es bricht der Strom mit feuertrunk'nen Fluten", "tokens": ["Es", "bricht", "der", "Strom", "mit", "feu\u00b7er\u00b7trunk'\u00b7nen", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Durch's enge Steingekl\u00fcft, wie er sich rette", "tokens": ["Durch's", "en\u00b7ge", "Stein\u00b7ge\u00b7kl\u00fcft", ",", "wie", "er", "sich", "ret\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "PPER", "PRF", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Zum ew'gen Meer \u2014 ach, wer da Fl\u00fcgel h\u00e4tte!", "tokens": ["Zum", "ew'\u00b7gen", "Meer", "ach", ",", "wer", "da", "Fl\u00fc\u00b7gel", "h\u00e4t\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$(", "ITJ", "$,", "PWS", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}