{"textgrid.poem.67792": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lebten wir, Ihr Christen, so auf Erden,", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lebten wir, Ihr Christen, so auf Erden,", "tokens": ["Leb\u00b7ten", "wir", ",", "Ihr", "Chris\u00b7ten", ",", "so", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df wir Christo m\u00f6chten \u00e4hnlich werden!", "tokens": ["Da\u00df", "wir", "Chris\u00b7to", "m\u00f6ch\u00b7ten", "\u00e4hn\u00b7lich", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Durch Lieb' und Leiden", "tokens": ["Durch", "Lieb'", "und", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ging er auf zu Gott ins Reich der Freuden.", "tokens": ["Ging", "er", "auf", "zu", "Gott", "ins", "Reich", "der", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sohn war er, o, mit wie edlem Geize", "tokens": ["Sohn", "war", "er", ",", "o", ",", "mit", "wie", "ed\u00b7lem", "Gei\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "FM", "$,", "APPR", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gott gehorsam, bis zum Tod am Kreuze,", "tokens": ["Gott", "ge\u00b7hor\u00b7sam", ",", "bis", "zum", "Tod", "am", "Kreu\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "KOUS", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "In Vaters Willen", "tokens": ["In", "Va\u00b7ters", "Wil\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Alles bis zum Tode zu erf\u00fcllen.", "tokens": ["Al\u00b7les", "bis", "zum", "To\u00b7de", "zu", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbwer ist Mutter mir? wer sind mir Br\u00fcder?", "tokens": ["\u00bb", "wer", "ist", "Mut\u00b7ter", "mir", "?", "wer", "sind", "mir", "Br\u00fc\u00b7der", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "NN", "PPER", "$.", "PWS", "VAFIN", "PPER", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die mit mir zu Vaters Willen nieder,", "tokens": ["Die", "mit", "mir", "zu", "Va\u00b7ters", "Wil\u00b7len", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "APPR", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Danieder sinken", "tokens": ["Da\u00b7nie\u00b7der", "sin\u00b7ken"], "token_info": ["word", "word"], "pos": ["PAV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und in Gotterkenntni\u00df Leben trinken.", "tokens": ["Und", "in", "Got\u00b7ter\u00b7kennt\u00b7ni\u00df", "Le\u00b7ben", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ewig Leben ist's, den Vater kennen,", "tokens": ["E\u00b7wig", "Le\u00b7ben", "ist's", ",", "den", "Va\u00b7ter", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihn im Bruder, Sohne Vater nennen!", "tokens": ["Ihn", "im", "Bru\u00b7der", ",", "Soh\u00b7ne", "Va\u00b7ter", "nen\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "In Kindesglauben", "tokens": ["In", "Kin\u00b7des\u00b7glau\u00b7ben"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Nie, sich nie den Vater lassen rauben!\u00ab", "tokens": ["Nie", ",", "sich", "nie", "den", "Va\u00b7ter", "las\u00b7sen", "rau\u00b7ben", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "PRF", "ADV", "ART", "NN", "VVINF", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ruft er nicht am Berg in Todeswehe:", "tokens": ["Ruft", "er", "nicht", "am", "Berg", "in", "To\u00b7des\u00b7we\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00bbmeiner nicht, ", "tokens": ["\u00bb", "mei\u00b7ner", "nicht", ","], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "PTKNEG", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Geh' Kelch vor\u00fcber", "tokens": ["Geh'", "Kelch", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "PTKVZ"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Oder nicht \u2013 sein Wille ist mir lieber!\u00ab", "tokens": ["O\u00b7der", "nicht", "\u2013", "sein", "Wil\u00b7le", "ist", "mir", "lie\u00b7ber", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PTKNEG", "$(", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Hing er nicht am Kreuze gottverlassen,", "tokens": ["Hing", "er", "nicht", "am", "Kreu\u00b7ze", "gott\u00b7ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unter Menschenfluch und S\u00fcnderhassen?", "tokens": ["Un\u00b7ter", "Men\u00b7schen\u00b7fluch", "und", "S\u00fcn\u00b7der\u00b7has\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da gab sein Ende,", "tokens": ["Da", "gab", "sein", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Vater, Dir den Geist in Deine H\u00e4nde!", "tokens": ["Va\u00b7ter", ",", "Dir", "den", "Geist", "in", "Dei\u00b7ne", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Ach, er hielt's nicht Raub, mit Gott zu prangen;", "tokens": ["Ach", ",", "er", "hielt's", "nicht", "Raub", ",", "mit", "Gott", "zu", "pran\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PTKNEG", "NN", "$,", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihr Menschenbr\u00fcder,", "tokens": ["Ihr", "Men\u00b7schen\u00b7br\u00fc\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Mensch zu werden, stieg er zu uns nieder!", "tokens": ["Mensch", "zu", "wer\u00b7den", ",", "stieg", "er", "zu", "uns", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "$,", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "War nur Gottessohn in Menschenliebe,", "tokens": ["War", "nur", "Got\u00b7tes\u00b7sohn", "in", "Men\u00b7schen\u00b7lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Liebte Menschen nur mit Gottestriebe,", "tokens": ["Lieb\u00b7te", "Men\u00b7schen", "nur", "mit", "Got\u00b7te\u00b7strie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sie zu erretten,", "tokens": ["Sie", "zu", "er\u00b7ret\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sie zu freien aus der Laster Ketten.", "tokens": ["Sie", "zu", "frei\u00b7en", "aus", "der", "Las\u00b7ter", "Ket\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "S\u00fcnder sucht' er, war ein Arzt f\u00fcr Kranke;", "tokens": ["S\u00fcn\u00b7der", "sucht'", "er", ",", "war", "ein", "Arzt", "f\u00fcr", "Kran\u00b7ke", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Speis' und Trank war ihm der Gottgedanke,", "tokens": ["Speis'", "und", "Trank", "war", "ihm", "der", "Gott\u00b7ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sein Blut und Leben", "tokens": ["Sein", "Blut", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "F\u00fcr die Br\u00fcder alle hinzugeben!", "tokens": ["F\u00fcr", "die", "Br\u00fc\u00b7der", "al\u00b7le", "hin\u00b7zu\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Sucht' er je, womit er Ruh sich pflegte?", "tokens": ["Sucht'", "er", "je", ",", "wo\u00b7mit", "er", "Ruh", "sich", "pfleg\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hatt' er nur, wo er sein Haupt hinlegte?", "tokens": ["Hatt'", "er", "nur", ",", "wo", "er", "sein", "Haupt", "hin\u00b7leg\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Der Erde Kronen", "tokens": ["Der", "Er\u00b7de", "Kro\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "War ihm Lohn, den Satan konnte lohnen!", "tokens": ["War", "ihm", "Lohn", ",", "den", "Sa\u00b7tan", "konn\u00b7te", "loh\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Mensch f\u00fcr Alle, Bruder aller Br\u00fcder,", "tokens": ["Mensch", "f\u00fcr", "Al\u00b7le", ",", "Bru\u00b7der", "al\u00b7ler", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "$,", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bild und Haupt f\u00fcr alle seine Glieder,", "tokens": ["Bild", "und", "Haupt", "f\u00fcr", "al\u00b7le", "sei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Des Lebens Quelle,", "tokens": ["Des", "Le\u00b7bens", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die durch Alle rinnet, tr\u00fcb und helle!", "tokens": ["Die", "durch", "Al\u00b7le", "rin\u00b7net", ",", "tr\u00fcb", "und", "hel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIS", "VVFIN", "$,", "ADJD", "KON", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Hell, o helle rinn in uns, o Leben,", "tokens": ["Hell", ",", "o", "hel\u00b7le", "rinn", "in", "uns", ",", "o", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "ADJA", "NN", "APPR", "PPER", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Christusleben! sich Gott hinzugeben,", "tokens": ["Chris\u00b7tus\u00b7le\u00b7ben", "!", "sich", "Gott", "hin\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PRF", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Durch Lieb' und Leiden", "tokens": ["Durch", "Lieb'", "und", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Aufzusteigen in sein Reich der Freuden!", "tokens": ["Auf\u00b7zu\u00b7stei\u00b7gen", "in", "sein", "Reich", "der", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Freut Euch, meine Glieder, Christus' Glieder!", "tokens": ["Freut", "Euch", ",", "mei\u00b7ne", "Glie\u00b7der", ",", "Chris\u00b7tus'", "Glie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "NE", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Christus' Geist, o hauche sanft hernieder!", "tokens": ["Chris\u00b7tus'", "Geist", ",", "o", "hau\u00b7che", "sanft", "her\u00b7nie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die Todgebeine", "tokens": ["Die", "Tod\u00b7ge\u00b7bei\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sind ja ihm zur Auferstehung seine!", "tokens": ["Sind", "ja", "ihm", "zur", "Auf\u00b7er\u00b7ste\u00b7hung", "sei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "APPRART", "NN", "PPOSAT", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Christus' Bild, wann wirst Du mich verkl\u00e4ren?", "tokens": ["Chris\u00b7tus'", "Bild", ",", "wann", "wirst", "Du", "mich", "ver\u00b7kl\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Grab, wie lange, lange wirst Du w\u00e4hren,", "tokens": ["Grab", ",", "wie", "lan\u00b7ge", ",", "lan\u00b7ge", "wirst", "Du", "w\u00e4h\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "$,", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eh er im Lichte", "tokens": ["Eh", "er", "im", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mir erschein' und Gottes Angesichte?", "tokens": ["Mir", "er\u00b7schein'", "und", "Got\u00b7tes", "An\u00b7ge\u00b7sich\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.15": {"line.1": {"text": "Harr auf Gott, o Seele, harr und warte,", "tokens": ["Harr", "auf", "Gott", ",", "o", "See\u00b7le", ",", "harr", "und", "war\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "FM", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie auch er ja lange Gott erharrte!", "tokens": ["Wie", "auch", "er", "ja", "lan\u00b7ge", "Gott", "er\u00b7harr\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ein Knecht auf Erden,", "tokens": ["Ein", "Knecht", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Nichts zu sein, hei\u00dft Christus \u00e4hnlich werden.", "tokens": ["Nichts", "zu", "sein", ",", "hei\u00dft", "Chris\u00b7tus", "\u00e4hn\u00b7lich", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VAINF", "$,", "VVFIN", "NE", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.16": {"line.1": {"text": "Lebten wir, Ihr Christen, so auf Erden,", "tokens": ["Leb\u00b7ten", "wir", ",", "Ihr", "Chris\u00b7ten", ",", "so", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df wir Christo m\u00f6chten \u00e4hnlich werden!", "tokens": ["Da\u00df", "wir", "Chris\u00b7to", "m\u00f6ch\u00b7ten", "\u00e4hn\u00b7lich", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Durch Lieb' und Leiden", "tokens": ["Durch", "Lieb'", "und", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ging er auf zu Gott ins Reich der Freuden.", "tokens": ["Ging", "er", "auf", "zu", "Gott", "ins", "Reich", "der", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.17": {"line.1": {"text": "Sohn war er, o, mit wie edlem Geize", "tokens": ["Sohn", "war", "er", ",", "o", ",", "mit", "wie", "ed\u00b7lem", "Gei\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "FM", "$,", "APPR", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gott gehorsam, bis zum Tod am Kreuze,", "tokens": ["Gott", "ge\u00b7hor\u00b7sam", ",", "bis", "zum", "Tod", "am", "Kreu\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "KOUS", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "In Vaters Willen", "tokens": ["In", "Va\u00b7ters", "Wil\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Alles bis zum Tode zu erf\u00fcllen.", "tokens": ["Al\u00b7les", "bis", "zum", "To\u00b7de", "zu", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.18": {"line.1": {"text": "\u00bbwer ist Mutter mir? wer sind mir Br\u00fcder?", "tokens": ["\u00bb", "wer", "ist", "Mut\u00b7ter", "mir", "?", "wer", "sind", "mir", "Br\u00fc\u00b7der", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "NN", "PPER", "$.", "PWS", "VAFIN", "PPER", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die mit mir zu Vaters Willen nieder,", "tokens": ["Die", "mit", "mir", "zu", "Va\u00b7ters", "Wil\u00b7len", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "APPR", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Danieder sinken", "tokens": ["Da\u00b7nie\u00b7der", "sin\u00b7ken"], "token_info": ["word", "word"], "pos": ["PAV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und in Gotterkenntni\u00df Leben trinken.", "tokens": ["Und", "in", "Got\u00b7ter\u00b7kennt\u00b7ni\u00df", "Le\u00b7ben", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.19": {"line.1": {"text": "Ewig Leben ist's, den Vater kennen,", "tokens": ["E\u00b7wig", "Le\u00b7ben", "ist's", ",", "den", "Va\u00b7ter", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihn im Bruder, Sohne Vater nennen!", "tokens": ["Ihn", "im", "Bru\u00b7der", ",", "Soh\u00b7ne", "Va\u00b7ter", "nen\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "In Kindesglauben", "tokens": ["In", "Kin\u00b7des\u00b7glau\u00b7ben"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Nie, sich nie den Vater lassen rauben!\u00ab", "tokens": ["Nie", ",", "sich", "nie", "den", "Va\u00b7ter", "las\u00b7sen", "rau\u00b7ben", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "PRF", "ADV", "ART", "NN", "VVINF", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.20": {"line.1": {"text": "Ruft er nicht am Berg in Todeswehe:", "tokens": ["Ruft", "er", "nicht", "am", "Berg", "in", "To\u00b7des\u00b7we\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00bbmeiner nicht, ", "tokens": ["\u00bb", "mei\u00b7ner", "nicht", ","], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "PTKNEG", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Geh' Kelch vor\u00fcber", "tokens": ["Geh'", "Kelch", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "PTKVZ"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Oder nicht \u2013 sein Wille ist mir lieber!\u00ab", "tokens": ["O\u00b7der", "nicht", "\u2013", "sein", "Wil\u00b7le", "ist", "mir", "lie\u00b7ber", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PTKNEG", "$(", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.21": {"line.1": {"text": "Hing er nicht am Kreuze gottverlassen,", "tokens": ["Hing", "er", "nicht", "am", "Kreu\u00b7ze", "gott\u00b7ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unter Menschenfluch und S\u00fcnderhassen?", "tokens": ["Un\u00b7ter", "Men\u00b7schen\u00b7fluch", "und", "S\u00fcn\u00b7der\u00b7has\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da gab sein Ende,", "tokens": ["Da", "gab", "sein", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Vater, Dir den Geist in Deine H\u00e4nde!", "tokens": ["Va\u00b7ter", ",", "Dir", "den", "Geist", "in", "Dei\u00b7ne", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.22": {"line.1": {"text": "Ach, er hielt's nicht Raub, mit Gott zu prangen;", "tokens": ["Ach", ",", "er", "hielt's", "nicht", "Raub", ",", "mit", "Gott", "zu", "pran\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PTKNEG", "NN", "$,", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihr Menschenbr\u00fcder,", "tokens": ["Ihr", "Men\u00b7schen\u00b7br\u00fc\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Mensch zu werden, stieg er zu uns nieder!", "tokens": ["Mensch", "zu", "wer\u00b7den", ",", "stieg", "er", "zu", "uns", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "$,", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.23": {"line.1": {"text": "War nur Gottessohn in Menschenliebe,", "tokens": ["War", "nur", "Got\u00b7tes\u00b7sohn", "in", "Men\u00b7schen\u00b7lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Liebte Menschen nur mit Gottestriebe,", "tokens": ["Lieb\u00b7te", "Men\u00b7schen", "nur", "mit", "Got\u00b7te\u00b7strie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sie zu erretten,", "tokens": ["Sie", "zu", "er\u00b7ret\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sie zu freien aus der Laster Ketten.", "tokens": ["Sie", "zu", "frei\u00b7en", "aus", "der", "Las\u00b7ter", "Ket\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.24": {"line.1": {"text": "S\u00fcnder sucht' er, war ein Arzt f\u00fcr Kranke;", "tokens": ["S\u00fcn\u00b7der", "sucht'", "er", ",", "war", "ein", "Arzt", "f\u00fcr", "Kran\u00b7ke", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Speis' und Trank war ihm der Gottgedanke,", "tokens": ["Speis'", "und", "Trank", "war", "ihm", "der", "Gott\u00b7ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sein Blut und Leben", "tokens": ["Sein", "Blut", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "F\u00fcr die Br\u00fcder alle hinzugeben!", "tokens": ["F\u00fcr", "die", "Br\u00fc\u00b7der", "al\u00b7le", "hin\u00b7zu\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.25": {"line.1": {"text": "Sucht' er je, womit er Ruh sich pflegte?", "tokens": ["Sucht'", "er", "je", ",", "wo\u00b7mit", "er", "Ruh", "sich", "pfleg\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hatt' er nur, wo er sein Haupt hinlegte?", "tokens": ["Hatt'", "er", "nur", ",", "wo", "er", "sein", "Haupt", "hin\u00b7leg\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Der Erde Kronen", "tokens": ["Der", "Er\u00b7de", "Kro\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "War ihm Lohn, den Satan konnte lohnen!", "tokens": ["War", "ihm", "Lohn", ",", "den", "Sa\u00b7tan", "konn\u00b7te", "loh\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.26": {"line.1": {"text": "Mensch f\u00fcr Alle, Bruder aller Br\u00fcder,", "tokens": ["Mensch", "f\u00fcr", "Al\u00b7le", ",", "Bru\u00b7der", "al\u00b7ler", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "$,", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bild und Haupt f\u00fcr alle seine Glieder,", "tokens": ["Bild", "und", "Haupt", "f\u00fcr", "al\u00b7le", "sei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Des Lebens Quelle,", "tokens": ["Des", "Le\u00b7bens", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die durch Alle rinnet, tr\u00fcb und helle!", "tokens": ["Die", "durch", "Al\u00b7le", "rin\u00b7net", ",", "tr\u00fcb", "und", "hel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIS", "VVFIN", "$,", "ADJD", "KON", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.27": {"line.1": {"text": "Hell, o helle rinn in uns, o Leben,", "tokens": ["Hell", ",", "o", "hel\u00b7le", "rinn", "in", "uns", ",", "o", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "ADJA", "NN", "APPR", "PPER", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Christusleben! sich Gott hinzugeben,", "tokens": ["Chris\u00b7tus\u00b7le\u00b7ben", "!", "sich", "Gott", "hin\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PRF", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Durch Lieb' und Leiden", "tokens": ["Durch", "Lieb'", "und", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Aufzusteigen in sein Reich der Freuden!", "tokens": ["Auf\u00b7zu\u00b7stei\u00b7gen", "in", "sein", "Reich", "der", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.28": {"line.1": {"text": "Freut Euch, meine Glieder, Christus' Glieder!", "tokens": ["Freut", "Euch", ",", "mei\u00b7ne", "Glie\u00b7der", ",", "Chris\u00b7tus'", "Glie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "NE", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Christus' Geist, o hauche sanft hernieder!", "tokens": ["Chris\u00b7tus'", "Geist", ",", "o", "hau\u00b7che", "sanft", "her\u00b7nie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die Todgebeine", "tokens": ["Die", "Tod\u00b7ge\u00b7bei\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sind ja ihm zur Auferstehung seine!", "tokens": ["Sind", "ja", "ihm", "zur", "Auf\u00b7er\u00b7ste\u00b7hung", "sei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "APPRART", "NN", "PPOSAT", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.29": {"line.1": {"text": "Christus' Bild, wann wirst Du mich verkl\u00e4ren?", "tokens": ["Chris\u00b7tus'", "Bild", ",", "wann", "wirst", "Du", "mich", "ver\u00b7kl\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Grab, wie lange, lange wirst Du w\u00e4hren,", "tokens": ["Grab", ",", "wie", "lan\u00b7ge", ",", "lan\u00b7ge", "wirst", "Du", "w\u00e4h\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "$,", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eh er im Lichte", "tokens": ["Eh", "er", "im", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mir erschein' und Gottes Angesichte?", "tokens": ["Mir", "er\u00b7schein'", "und", "Got\u00b7tes", "An\u00b7ge\u00b7sich\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.30": {"line.1": {"text": "Harr auf Gott, o Seele, harr und warte,", "tokens": ["Harr", "auf", "Gott", ",", "o", "See\u00b7le", ",", "harr", "und", "war\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "FM", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie auch er ja lange Gott erharrte!", "tokens": ["Wie", "auch", "er", "ja", "lan\u00b7ge", "Gott", "er\u00b7harr\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ein Knecht auf Erden,", "tokens": ["Ein", "Knecht", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Nichts zu sein, hei\u00dft Christus \u00e4hnlich werden.", "tokens": ["Nichts", "zu", "sein", ",", "hei\u00dft", "Chris\u00b7tus", "\u00e4hn\u00b7lich", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VAINF", "$,", "VVFIN", "NE", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}