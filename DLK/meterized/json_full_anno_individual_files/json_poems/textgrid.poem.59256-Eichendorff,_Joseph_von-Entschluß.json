{"textgrid.poem.59256": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Entschlu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gebannt im stillen Kreise sanfter H\u00fcgel,", "tokens": ["Ge\u00b7bannt", "im", "stil\u00b7len", "Krei\u00b7se", "sanf\u00b7ter", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schlingt sich ein Strom von ewig gleichen Tagen,", "tokens": ["Schlingt", "sich", "ein", "Strom", "von", "e\u00b7wig", "glei\u00b7chen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da mag die Brust nicht nach der Ferne fragen,", "tokens": ["Da", "mag", "die", "Brust", "nicht", "nach", "der", "Fer\u00b7ne", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und l\u00e4chelnd senkt die Sehnsucht ihre Fl\u00fcgel.", "tokens": ["Und", "l\u00e4\u00b7chelnd", "senkt", "die", "Sehn\u00b7sucht", "ih\u00b7re", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Viel andre stehen k\u00fchn im Rossesb\u00fcgel,", "tokens": ["Viel", "and\u00b7re", "ste\u00b7hen", "k\u00fchn", "im", "Ros\u00b7ses\u00b7b\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Lebens h\u00f6chste G\u00fcter zu erjagen,", "tokens": ["Des", "Le\u00b7bens", "h\u00f6chs\u00b7te", "G\u00fc\u00b7ter", "zu", "er\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und was sie w\u00fcnschen, m\u00fcssen sie erst wagen,", "tokens": ["Und", "was", "sie", "w\u00fcn\u00b7schen", ",", "m\u00fcs\u00b7sen", "sie", "erst", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVINF", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein strenger Geist regiert des Rosses Z\u00fcgel. \u2013", "tokens": ["Ein", "stren\u00b7ger", "Geist", "re\u00b7giert", "des", "Ros\u00b7ses", "Z\u00fc\u00b7gel", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was singt ihr lockend so, ihr stillen Matten,", "tokens": ["Was", "singt", "ihr", "lo\u00b7ckend", "so", ",", "ihr", "stil\u00b7len", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du Heimat mit den Regenbogenbr\u00fccken,", "tokens": ["Du", "Hei\u00b7mat", "mit", "den", "Re\u00b7gen\u00b7bo\u00b7gen\u00b7br\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr heitern Bilder, harmlos bunte Spiele?", "tokens": ["Ihr", "hei\u00b7tern", "Bil\u00b7der", ",", "harm\u00b7los", "bun\u00b7te", "Spie\u00b7le", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mich fa\u00dft der Sturm, wild ringen Licht und Schatten,", "tokens": ["Mich", "fa\u00dft", "der", "Sturm", ",", "wild", "rin\u00b7gen", "Licht", "und", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Wolkenri\u00df bricht flammendes Entz\u00fccken \u2013", "tokens": ["Durch", "Wol\u00b7ken\u00b7ri\u00df", "bricht", "flam\u00b7men\u00b7des", "Ent\u00b7z\u00fc\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur zu, mein Ro\u00df! wir finden noch zum Ziele!", "tokens": ["Nur", "zu", ",", "mein", "Ro\u00df", "!", "wir", "fin\u00b7den", "noch", "zum", "Zie\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gebannt im stillen Kreise sanfter H\u00fcgel,", "tokens": ["Ge\u00b7bannt", "im", "stil\u00b