{"textgrid.poem.35300": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mancher, der die See gepriesen,", "tokens": ["Man\u00b7cher", ",", "der", "die", "See", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah sie nur vom sichern Strand;", "tokens": ["Sah", "sie", "nur", "vom", "si\u00b7chern", "Strand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichts als seinen Unverstand", "tokens": ["Nichts", "als", "sei\u00b7nen", "Un\u00b7ver\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat ein solcher Mann bewiesen.", "tokens": ["Hat", "ein", "sol\u00b7cher", "Mann", "be\u00b7wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Freilich gab es jederzeit", "tokens": ["Frei\u00b7lich", "gab", "es", "je\u00b7der\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leute, die sich selbst betrogen,", "tokens": ["Leu\u00b7te", ",", "die", "sich", "selbst", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch beherrscht von Wind und Wogen,", "tokens": ["Doch", "be\u00b7herrscht", "von", "Wind", "und", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Glaubt man an die Wirklichkeit.", "tokens": ["Glaubt", "man", "an", "die", "Wirk\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer da schw\u00e4rmt f\u00fcr weite Reisen,", "tokens": ["Wer", "da", "schw\u00e4rmt", "f\u00fcr", "wei\u00b7te", "Rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Komme auf die salz'ge Flut,", "tokens": ["Kom\u00b7me", "auf", "die", "sa\u00b7lz'\u00b7ge", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Zeige seinen Seemannsmut,", "tokens": ["Zei\u00b7ge", "sei\u00b7nen", "See\u00b7manns\u00b7mut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sehe selbst, ob sie zu preisen,", "tokens": ["Se\u00b7he", "selbst", ",", "ob", "sie", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die sich wie geschmolznes Blei", "tokens": ["Die", "sich", "wie", "ge\u00b7schmolz\u00b7nes", "Blei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gegen unsrer Barke Flanken", "tokens": ["Ge\u00b7gen", "uns\u00b7rer", "Bar\u00b7ke", "Flan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Jetzt emp\u00f6rt \u2013 verdammtes Schwanken! \u2013", "tokens": ["Jetzt", "em\u00b7p\u00f6rt", "\u2013", "ver\u00b7damm\u00b7tes", "Schwan\u00b7ken", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "$(", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ob die See zu loben sei.", "tokens": ["Ob", "die", "See", "zu", "lo\u00b7ben", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jedem Schiff, bei solchem Dr\u00e4ngen,", "tokens": ["Je\u00b7dem", "Schiff", ",", "bei", "sol\u00b7chem", "Dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht zuletzt der Atem aus;", "tokens": ["Geht", "zu\u00b7letzt", "der", "A\u00b7tem", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heute l\u00e4\u00dft die Fledermaus", "tokens": ["Heu\u00b7te", "l\u00e4\u00dft", "die", "Fle\u00b7der\u00b7maus"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kraftlos ihre Fl\u00fcgel h\u00e4ngen;", "tokens": ["Kraft\u00b7los", "ih\u00b7re", "Fl\u00fc\u00b7gel", "h\u00e4n\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "T\u00e4glich Aerger und Verdru\u00df,", "tokens": ["T\u00e4g\u00b7lich", "A\u00b7er\u00b7ger", "und", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Und von menschlichen Gebrechen,", "tokens": ["Und", "von", "menschli\u00b7chen", "Ge\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Von so vielem nicht zu sprechen,", "tokens": ["Von", "so", "vie\u00b7lem", "nicht", "zu", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIS", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was man sonst ertragen mu\u00df.", "tokens": ["Was", "man", "sonst", "er\u00b7tra\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Setzt man hungrig sich zu Tische,", "tokens": ["Setzt", "man", "hung\u00b7rig", "sich", "zu", "Ti\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u2013 Manches k\u00f6nnte besser sein,", "tokens": ["\u2013", "Man\u00b7ches", "k\u00f6nn\u00b7te", "bes\u00b7ser", "sein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selten ist die Nahrung fein,", "tokens": ["Sel\u00b7ten", "ist", "die", "Nah\u00b7rung", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch das Fleisch von erster Frische \u2013,", "tokens": ["Noch", "das", "Fleisch", "von", "ers\u00b7ter", "Fri\u00b7sche", "\u2013", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "$(", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ei, das t\u00e4nzelt hin und her:", "tokens": ["Ei", ",", "das", "t\u00e4n\u00b7zelt", "hin", "und", "her", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Teller, Gl\u00e4ser, L\u00f6ffel, Messer,", "tokens": ["Tel\u00b7ler", ",", "Gl\u00e4\u00b7ser", ",", "L\u00f6f\u00b7fel", ",", "Mes\u00b7ser", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und dem unbefangnen Esser", "tokens": ["Und", "dem", "un\u00b7be\u00b7fang\u00b7nen", "Es\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird die Arbeit doppelt schwer.", "tokens": ["Wird", "die", "Ar\u00b7beit", "dop\u00b7pelt", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Liegt man still in seiner Kammer,", "tokens": ["Liegt", "man", "still", "in", "sei\u00b7ner", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u2013 Die Matratzen sind nicht weich \u2013", "tokens": ["\u2013", "Die", "Ma\u00b7trat\u00b7zen", "sind", "nicht", "weich", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und versucht zu schlummern, gleich", "tokens": ["Und", "ver\u00b7sucht", "zu", "schlum\u00b7mern", ",", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVPP", "PTKZU", "VVINF", "$,", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Statt des Schlafs kommt neuer Jammer,", "tokens": ["Statt", "des", "Schlafs", "kommt", "neu\u00b7er", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil auf eines Schiffes Deck", "tokens": ["Weil", "auf", "ei\u00b7nes", "Schif\u00b7fes", "Deck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ohne Schreien und Gepolter", "tokens": ["Oh\u00b7ne", "Schrei\u00b7en", "und", "Ge\u00b7pol\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nichts gedeiht \u2013 und keine Folter", "tokens": ["Nichts", "ge\u00b7deiht", "\u2013", "und", "kei\u00b7ne", "Fol\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$(", "KON", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bringt geschwinder uns vom Fleck.", "tokens": ["Bringt", "ge\u00b7schwin\u00b7der", "uns", "vom", "Fleck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Zahllos sind des Meeres Launen;", "tokens": ["Zahl\u00b7los", "sind", "des", "Mee\u00b7res", "Lau\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was die Jugend nicht geglaubt,", "tokens": ["Was", "die", "Ju\u00b7gend", "nicht", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Erfahrung sei's erlaubt,", "tokens": ["Der", "Er\u00b7fah\u00b7rung", "sei's", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Leser, dir ins Ohr zu raunen:", "tokens": ["Le\u00b7ser", ",", "dir", "ins", "Ohr", "zu", "rau\u00b7nen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "N\u00e4hre du am sichern Strand", "tokens": ["N\u00e4h\u00b7re", "du", "am", "si\u00b7chern", "Strand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dein poetisches Entz\u00fccken;", "tokens": ["Dein", "po\u00b7e\u00b7ti\u00b7sches", "Ent\u00b7z\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf des Meeres breitem R\u00fccken", "tokens": ["Auf", "des", "Mee\u00b7res", "brei\u00b7tem", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hat es leider nicht Bestand.", "tokens": ["Hat", "es", "lei\u00b7der", "nicht", "Be\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Mancher, der die See gepriesen,", "tokens": ["Man\u00b7cher", ",", "der", "die", "See", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah sie nur vom sichern Strand;", "tokens": ["Sah", "sie", "nur", "vom", "si\u00b7chern", "Strand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichts als seinen Unverstand", "tokens": ["Nichts", "als", "sei\u00b7nen", "Un\u00b7ver\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat ein solcher Mann bewiesen.", "tokens": ["Hat", "ein", "sol\u00b7cher", "Mann", "be\u00b7wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Freilich gab es jederzeit", "tokens": ["Frei\u00b7lich", "gab", "es", "je\u00b7der\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leute, die sich selbst betrogen,", "tokens": ["Leu\u00b7te", ",", "die", "sich", "selbst", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch beherrscht von Wind und Wogen,", "tokens": ["Doch", "be\u00b7herrscht", "von", "Wind", "und", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Glaubt man an die Wirklichkeit.", "tokens": ["Glaubt", "man", "an", "die", "Wirk\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wer da schw\u00e4rmt f\u00fcr weite Reisen,", "tokens": ["Wer", "da", "schw\u00e4rmt", "f\u00fcr", "wei\u00b7te", "Rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Komme auf die salz'ge Flut,", "tokens": ["Kom\u00b7me", "auf", "die", "sa\u00b7lz'\u00b7ge", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Zeige seinen Seemannsmut,", "tokens": ["Zei\u00b7ge", "sei\u00b7nen", "See\u00b7manns\u00b7mut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sehe selbst, ob sie zu preisen,", "tokens": ["Se\u00b7he", "selbst", ",", "ob", "sie", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die sich wie geschmolznes Blei", "tokens": ["Die", "sich", "wie", "ge\u00b7schmolz\u00b7nes", "Blei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gegen unsrer Barke Flanken", "tokens": ["Ge\u00b7gen", "uns\u00b7rer", "Bar\u00b7ke", "Flan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Jetzt emp\u00f6rt \u2013 verdammtes Schwanken! \u2013", "tokens": ["Jetzt", "em\u00b7p\u00f6rt", "\u2013", "ver\u00b7damm\u00b7tes", "Schwan\u00b7ken", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "$(", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ob die See zu loben sei.", "tokens": ["Ob", "die", "See", "zu", "lo\u00b7ben", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Jedem Schiff, bei solchem Dr\u00e4ngen,", "tokens": ["Je\u00b7dem", "Schiff", ",", "bei", "sol\u00b7chem", "Dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht zuletzt der Atem aus;", "tokens": ["Geht", "zu\u00b7letzt", "der", "A\u00b7tem", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heute l\u00e4\u00dft die Fledermaus", "tokens": ["Heu\u00b7te", "l\u00e4\u00dft", "die", "Fle\u00b7der\u00b7maus"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kraftlos ihre Fl\u00fcgel h\u00e4ngen;", "tokens": ["Kraft\u00b7los", "ih\u00b7re", "Fl\u00fc\u00b7gel", "h\u00e4n\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "T\u00e4glich Aerger und Verdru\u00df,", "tokens": ["T\u00e4g\u00b7lich", "A\u00b7er\u00b7ger", "und", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Und von menschlichen Gebrechen,", "tokens": ["Und", "von", "menschli\u00b7chen", "Ge\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Von so vielem nicht zu sprechen,", "tokens": ["Von", "so", "vie\u00b7lem", "nicht", "zu", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIS", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was man sonst ertragen mu\u00df.", "tokens": ["Was", "man", "sonst", "er\u00b7tra\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Setzt man hungrig sich zu Tische,", "tokens": ["Setzt", "man", "hung\u00b7rig", "sich", "zu", "Ti\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u2013 Manches k\u00f6nnte besser sein,", "tokens": ["\u2013", "Man\u00b7ches", "k\u00f6nn\u00b7te", "bes\u00b7ser", "sein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selten ist die Nahrung fein,", "tokens": ["Sel\u00b7ten", "ist", "die", "Nah\u00b7rung", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch das Fleisch von erster Frische \u2013,", "tokens": ["Noch", "das", "Fleisch", "von", "ers\u00b7ter", "Fri\u00b7sche", "\u2013", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "$(", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ei, das t\u00e4nzelt hin und her:", "tokens": ["Ei", ",", "das", "t\u00e4n\u00b7zelt", "hin", "und", "her", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Teller, Gl\u00e4ser, L\u00f6ffel, Messer,", "tokens": ["Tel\u00b7ler", ",", "Gl\u00e4\u00b7ser", ",", "L\u00f6f\u00b7fel", ",", 