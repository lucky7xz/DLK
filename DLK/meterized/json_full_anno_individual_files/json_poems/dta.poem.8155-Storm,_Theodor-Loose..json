{"dta.poem.8155": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Loose.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Sonnige Liebe gebracht,", "tokens": ["Son\u00b7ni\u00b7ge", "Lie\u00b7be", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Die hat ihn gehen hei\u00dfen,", "tokens": ["Die", "hat", "ihn", "ge\u00b7hen", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht weiter sein gedacht.", "tokens": ["Nicht", "wei\u00b7ter", "sein", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "D'rauf hat er heimgef\u00fchret", "tokens": ["D'\u00b7rauf", "hat", "er", "heim\u00b7ge\u00b7f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein M\u00e4dchen still und hold;", "tokens": ["Ein", "M\u00e4d\u00b7chen", "still", "und", "hold", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die hat aus allen Menschen", "tokens": ["Die", "hat", "aus", "al\u00b7len", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur einzig ihn gewollt.", "tokens": ["Nur", "ein\u00b7zig", "ihn", "ge\u00b7wollt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VMPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und ob sein Herz in Liebe", "tokens": ["Und", "ob", "sein", "Herz", "in", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Niemals f\u00fcr sie gebebt,", "tokens": ["Nie\u00b7mals", "f\u00fcr", "sie", "ge\u00b7bebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sie hat um ihn gelitten", "tokens": ["Sie", "hat", "um", "ihn", "ge\u00b7lit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nur f\u00fcr ihn gelebt.", "tokens": ["Und", "nur", "f\u00fcr", "ihn", "ge\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}