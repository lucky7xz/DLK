{"dta.poem.18572": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "U ber die  L ust in  L ieben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Lieben ist das sch\u00f6nste Leben/", "tokens": ["Lie\u00b7ben", "ist", "das", "sch\u00f6ns\u00b7te", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das uns solche Lust kan geben/", "tokens": ["Das", "uns", "sol\u00b7che", "Lust", "kan", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PIAT", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die man unvergleichlich h\u00e4lt.", "tokens": ["Die", "man", "un\u00b7ver\u00b7gleich\u00b7lich", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ehre/ Pracht/ und grosse Sch\u00e4tze/", "tokens": ["Eh\u00b7re", "/", "Pracht", "/", "und", "gros\u00b7se", "Sch\u00e4t\u00b7ze", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "KON", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sind versichert nur die Netze/", "tokens": ["Sind", "ver\u00b7si\u00b7chert", "nur", "die", "Net\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die man eitlen Sinnen stellt:", "tokens": ["Die", "man", "eit\u00b7len", "Sin\u00b7nen", "stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier sind Florens Wunder-Auen/", "tokens": ["Hier", "sind", "Flo\u00b7rens", "Wun\u00b7der\u00b7Au\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in Paradie\u00df zu schauen", "tokens": ["Und", "in", "Pa\u00b7ra\u00b7die\u00df", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "PTKZU", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Engel/ die empfindlich sind.", "tokens": ["En\u00b7gel", "/", "die", "emp\u00b7find\u00b7lich", "sind", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zucker Rosen und Narcissen", "tokens": ["Zu\u00b7cker", "Ro\u00b7sen", "und", "Nar\u00b7cis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bricht man durch das s\u00fcsse K\u00fcssen/", "tokens": ["Bricht", "man", "durch", "das", "s\u00fcs\u00b7se", "K\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo der Himmel selbst zerrinnt.", "tokens": ["Wo", "der", "Him\u00b7mel", "selbst", "zer\u00b7rinnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch man mu\u00df sein Gl\u00fccke Wagen/", "tokens": ["Doch", "man", "mu\u00df", "sein", "Gl\u00fc\u00b7cke", "Wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn auf einen Sturm verzagen/", "tokens": ["Denn", "auf", "ei\u00b7nen", "Sturm", "ver\u00b7za\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df gewinnt die Vestung nicht,", "tokens": ["Da\u00df", "ge\u00b7winnt", "die", "Ves\u00b7tung", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob zu erst die Dornen stechen/", "tokens": ["Ob", "zu", "erst", "die", "Dor\u00b7nen", "ste\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKA", "ADV", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach! so ist nicht auszusprechen/", "tokens": ["Ach", "!", "so", "ist", "nicht", "aus\u00b7zu\u00b7spre\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VAFIN", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie entz\u00fcckt man Rosen bricht.", "tokens": ["Wie", "ent\u00b7z\u00fcckt", "man", "Ro\u00b7sen", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wills die Tugend gleich verfluchen/", "tokens": ["Wills", "die", "Tu\u00b7gend", "gleich", "ver\u00b7flu\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und heist mich was sch\u00f6nres suchen/", "tokens": ["Und", "heist", "mich", "was", "sch\u00f6n\u00b7res", "su\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "ADJA", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als in schn\u00f6der Wollust ruht:", "tokens": ["Als", "in", "schn\u00f6\u00b7der", "Wol\u00b7lust", "ruht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun so ehr ich sie von beyden/", "tokens": ["Nun", "so", "ehr", "ich", "sie", "von", "bey\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "PPER", "APPR", "PIAT", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unterdessen kan ichs leiden/", "tokens": ["Un\u00b7ter\u00b7des\u00b7sen", "kan", "ichs", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df mirs was Galantes thut.", "tokens": ["Da\u00df", "mirs", "was", "Ga\u00b7lan\u00b7tes", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PWS", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}