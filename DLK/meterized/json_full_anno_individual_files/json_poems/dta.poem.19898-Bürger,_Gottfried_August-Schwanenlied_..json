{"dta.poem.19898": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Schwanenlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.85", "en:0.14"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Mir thut's so weh im Herzen!               ", "tokens": ["Mir", "thut's", "so", "weh", "im", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich bin so mat und krank!", "tokens": ["Ich", "bin", "so", "mat", "und", "krank", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich schlafe nicht vor Schmerzen;", "tokens": ["Ich", "schla\u00b7fe", "nicht", "vor", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mag Speise nicht und Trank;", "tokens": ["Mag", "Spei\u00b7se", "nicht", "und", "Trank", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PTKNEG", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Seh\u2019 alles sich entf\u00e4rben,", "tokens": ["Seh'", "al\u00b7les", "sich", "ent\u00b7f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was sch\u00f6nes mir gebl\u00fcht.", "tokens": ["Was", "sch\u00f6\u00b7nes", "mir", "ge\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ach, Liebchen, wil nur sterben!", "tokens": ["Ach", ",", "Lieb\u00b7chen", ",", "wil", "nur", "ster\u00b7ben", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dies ist mein Schwanenlied.", "tokens": ["Dies", "ist", "mein", "Schwa\u00b7nen\u00b7lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du w\u00e4rst mir zwar ein Becher,", "tokens": ["Du", "w\u00e4rst", "mir", "zwar", "ein", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Heilungslabsal vol. \u2014", "tokens": ["Von", "Hei\u00b7lungs\u00b7lab\u00b7sal", "vol", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur \u2014 da\u00df ich armer Lecher", "tokens": ["Nur", "da\u00df", "ich", "ar\u00b7mer", "Le\u00b7cher"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$(", "KOUS", "PPER", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nicht ganz ihn trinken sol!", "tokens": ["Nicht", "ganz", "ihn", "trin\u00b7ken", "sol", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihn, welcher so viel S\u00fcsses,", "tokens": ["Ihn", ",", "wel\u00b7cher", "so", "viel", "S\u00fcs\u00b7ses", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So tausend S\u00fcsses hat! \u2014", "tokens": ["So", "tau\u00b7send", "S\u00fcs\u00b7ses", "hat", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "CARD", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch \u2014 h\u00e4tt\u2019 ich des Geniesses,", "tokens": ["Doch", "h\u00e4tt'", "ich", "des", "Ge\u00b7nies\u00b7ses", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Nie h\u00e4tt\u2019 ich dennoch sat.", "tokens": ["Nie", "h\u00e4tt'", "ich", "den\u00b7noch", "sat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Drum las mich, vor den Wehen", "tokens": ["Drum", "las", "mich", ",", "vor", "den", "We\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der ungestilten Lust,", "tokens": ["Der", "un\u00b7ge\u00b7stil\u00b7ten", "Lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zerschmelzen und vergehen,", "tokens": ["Zer\u00b7schmel\u00b7zen", "und", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vergehn an deiner Brust!", "tokens": ["Ver\u00b7gehn", "an", "dei\u00b7ner", "Brust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Aus deinem s\u00fcssen Munde", "tokens": ["Aus", "dei\u00b7nem", "s\u00fcs\u00b7sen", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Las saugen s\u00fcssen Tod!", "tokens": ["Las", "sau\u00b7gen", "s\u00fcs\u00b7sen", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Denn, Herzchen, ich gesunde", "tokens": ["Denn", ",", "Herz\u00b7chen", ",", "ich", "ge\u00b7sun\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "PPER", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sonst nie von meiner Not.", "tokens": ["Sonst", "nie", "von", "mei\u00b7ner", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}