{"textgrid.poem.48863": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "88. Er verwundert sich seiner Gl\u00fcckseligkeit", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie mir es gestern ging und wie ich ward empfangen", "tokens": ["Wie", "mir", "es", "ge\u00b7stern", "ging", "und", "wie", "ich", "ward", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVFIN", "KON", "PWAV", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "in meiner Freundin Scho\u00df, wei\u00df sie nur und nur ich.", "tokens": ["in", "mei\u00b7ner", "Freun\u00b7din", "Scho\u00df", ",", "wei\u00df", "sie", "nur", "und", "nur", "ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "VVFIN", "PPER", "ADV", "KON", "ADV", "PPER", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Das allerliebste Kind, das herzt' und gr\u00fc\u00dfte mich,", "tokens": ["Das", "al\u00b7ler\u00b7liebs\u00b7te", "Kind", ",", "das", "herzt'", "und", "gr\u00fc\u00df\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PDS", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "sie hielte feste mich, wie ich sie hart' umfangen.", "tokens": ["sie", "hiel\u00b7te", "fes\u00b7te", "mich", ",", "wie", "ich", "sie", "hart'", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "PPER", "$,", "PWAV", "PPER", "PPER", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auf meinem lag ihr Mund, auf ihren meine Wangen.", "tokens": ["Auf", "mei\u00b7nem", "lag", "ihr", "Mund", ",", "auf", "ih\u00b7ren", "mei\u00b7ne", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Oft sagte sie mir auch, was nicht l\u00e4\u00dft sagen sich.", "tokens": ["Oft", "sag\u00b7te", "sie", "mir", "auch", ",", "was", "nicht", "l\u00e4\u00dft", "sa\u00b7gen", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$,", "PRELS", "PTKNEG", "VVFIN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "darum du, Momus, nicht hast zu bek\u00fcmmern dich,", "tokens": ["da\u00b7rum", "du", ",", "Mo\u00b7mus", ",", "nicht", "hast", "zu", "be\u00b7k\u00fcm\u00b7mern", "dich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "$,", "NE", "$,", "PTKNEG", "VAFIN", "PTKZU", "VVINF", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bei mir ist noch mein Sinn, bei mir noch ihr Verlangen;", "tokens": ["Bei", "mir", "ist", "noch", "mein", "Sinn", ",", "bei", "mir", "noch", "ihr", "Ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "APPR", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "o wol mir, der ich wei\u00df, was nur die G\u00f6tter wissen,", "tokens": ["o", "wol", "mir", ",", "der", "ich", "wei\u00df", ",", "was", "nur", "die", "G\u00f6t\u00b7ter", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "PRELS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die sich auch, wie wir uns, in reiner Keuschheit k\u00fcssen,", "tokens": ["die", "sich", "auch", ",", "wie", "wir", "uns", ",", "in", "rei\u00b7ner", "Keuschheit", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "ADV", "$,", "PWAV", "PPER", "PPER", "$,", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "o wol mir, der ich wei\u00df, was kein Verliebter wei\u00df.", "tokens": ["o", "wol", "mir", ",", "der", "ich", "wei\u00df", ",", "was", "kein", "Ver\u00b7lieb\u00b7ter", "wei\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADV", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wird meiner Seelen Trost mich allzeit also laben,", "tokens": ["Wird", "mei\u00b7ner", "See\u00b7len", "Trost", "mich", "all\u00b7zeit", "al\u00b7so", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mir allzeit also tun, so werd' ich an ihr haben", "tokens": ["mir", "all\u00b7zeit", "al\u00b7so", "tun", ",", "so", "werd'", "ich", "an", "ihr", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "VVINF", "$,", "ADV", "VAFIN", "PPER", "APPR", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ein weltlichs Himmelreich, ein sterblichs Paradeis.", "tokens": ["ein", "welt\u00b7lichs", "Him\u00b7mel\u00b7reich", ",", "ein", "sterb\u00b7lichs", "Pa\u00b7ra\u00b7deis", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie mir es gestern ging und wie ich ward empfangen", "tokens": ["Wie", "mir", "es", "ge\u00b7stern", "ging", "und", "wie", "ich", "ward", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVFIN", "KON", "PWAV", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "in meiner Freundin Scho\u00df, wei\u00df sie nur und nur ich.", "tokens": ["in", "mei\u00b7ner", "Freun\u00b7din", "Scho\u00df", ",", "wei\u00df", "sie", "nur", "und", "nur", "ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "VVFIN", "PPER", "ADV", "KON", "ADV", "PPER", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Das allerliebste Kind, das herzt' und gr\u00fc\u00dfte mich,", "tokens": ["Das", "al\u00b7ler\u00b7liebs\u00b7te", "Kind", ",", "das", "herzt'", "und", "gr\u00fc