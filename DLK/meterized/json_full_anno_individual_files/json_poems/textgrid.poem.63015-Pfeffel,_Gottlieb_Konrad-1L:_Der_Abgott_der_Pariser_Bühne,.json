{"textgrid.poem.63015": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Abgott der Pariser B\u00fchne,", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Abgott der Pariser B\u00fchne,", "tokens": ["Der", "Ab\u00b7gott", "der", "Pa\u00b7ri\u00b7ser", "B\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der unnachahmliche Racine,", "tokens": ["Der", "un\u00b7nach\u00b7ahm\u00b7li\u00b7che", "Ra\u00b7ci\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Trat kaum aus Charons schwarzem Kahn", "tokens": ["Trat", "kaum", "aus", "Cha\u00b7rons", "schwar\u00b7zem", "Kahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So suchte der galante Dichter", "tokens": ["So", "such\u00b7te", "der", "ga\u00b7lan\u00b7te", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bey dem Dekan der H\u00f6llenrichter", "tokens": ["Bey", "dem", "De\u00b7kan", "der", "H\u00f6l\u00b7len\u00b7rich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Geb\u00fchrend um Erlaubni\u00df an,", "tokens": ["Ge\u00b7b\u00fch\u00b7rend", "um", "Er\u00b7laub\u00b7ni\u00df", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem Musenchor den Hof zu machen:", "tokens": ["Dem", "Mu\u00b7sen\u00b7chor", "den", "Hof", "zu", "ma\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der ernste Minos strich den Bart,", "tokens": ["Der", "erns\u00b7te", "Mi\u00b7nos", "strich", "den", "Bart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Um heimlich in die Faust zu lachen;", "tokens": ["Um", "heim\u00b7lich", "in", "die", "Faust", "zu", "la\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Doch er bewilligte die Fahrt:", "tokens": ["Doch", "er", "be\u00b7wil\u00b7lig\u00b7te", "die", "Fahrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und um nicht lange Zeit zu kriegen,", "tokens": ["Und", "um", "nicht", "lan\u00b7ge", "Zeit", "zu", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PTKNEG", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Nahm der Poet die Helden mit,", "tokens": ["Nahm", "der", "Po\u00b7et", "die", "Hel\u00b7den", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die sein Theater einst bestiegen.", "tokens": ["Die", "sein", "The\u00b7a\u00b7ter", "einst", "be\u00b7stie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Achilles, Titus, Hyppolit,", "tokens": ["A\u00b7chil\u00b7les", ",", "Ti\u00b7tus", ",", "Hyp\u00b7po\u00b7lit", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Und wie sich all die Herren nannten;", "tokens": ["Und", "wie", "sich", "all", "die", "Her\u00b7ren", "nann\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PRF", "PIAT", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Umschimmerten ihn als Trabanten,", "tokens": ["Um\u00b7schim\u00b7mer\u00b7ten", "ihn", "als", "Tra\u00b7ban\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Die Schatten gehn im Doppelschritt:", "tokens": ["Die", "Schat\u00b7ten", "gehn", "im", "Dop\u00b7pel\u00b7schritt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Sie glitschen \u00fcber Berg und Seen,", "tokens": ["Sie", "glit\u00b7schen", "\u00fc\u00b7ber", "Berg", "und", "Seen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und langten, eh sie sichs versahn,", "tokens": ["Und", "lang\u00b7ten", ",", "eh", "sie", "sichs", "ver\u00b7sahn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "In den bezauberten Alleen", "tokens": ["In", "den", "be\u00b7zau\u00b7ber\u00b7ten", "Al\u00b7leen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Des blumichten Parnasses an.", "tokens": ["Des", "blu\u00b7mich\u00b7ten", "Par\u00b7nas\u00b7ses", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Hier spielten wirklich die neun Musen", "tokens": ["Hier", "spiel\u00b7ten", "wirk\u00b7lich", "die", "neun", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Mit frohem L\u00e4rmen blinde Kuh.", "tokens": ["Mit", "fro\u00b7hem", "L\u00e4r\u00b7men", "blin\u00b7de", "Kuh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Der Franzmann lief mit warmem Busen", "tokens": ["Der", "Franz\u00b7mann", "lief", "mit", "war\u00b7mem", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Sogleich auf Melpomenen zu;", "tokens": ["Sog\u00b7leich", "auf", "Mel\u00b7po\u00b7me\u00b7nen", "zu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Der Ernst, der aus den Augen blickte,", "tokens": ["Der", "Ernst", ",", "der", "aus", "den", "Au\u00b7gen", "blick\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Der Dolch, der ihre H\u00fcfte schm\u00fcckte,", "tokens": ["Der", "Dolch", ",", "der", "ih\u00b7re", "H\u00fcf\u00b7te", "schm\u00fcck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Das schwarze Wamms, der hohe Schuh", "tokens": ["Das", "schwar\u00b7ze", "Wamms", ",", "der", "ho\u00b7he", "Schuh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Ersparten ihm das lange Fragen.", "tokens": ["Er\u00b7spar\u00b7ten", "ihm", "das", "lan\u00b7ge", "Fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Madam, so sprach er tiefgeb\u00fcckt,", "tokens": ["Ma\u00b7dam", ",", "so", "sprach", "er", "tief\u00b7ge\u00b7b\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Darf es dein Lieblingsdichter wagen,", "tokens": ["Darf", "es", "dein", "Lieb\u00b7lings\u00b7dich\u00b7ter", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Dem du so manchen Kranz gepfl\u00fcckt,", "tokens": ["Dem", "du", "so", "man\u00b7chen", "Kranz", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Mit den Heroen seiner Scene", "tokens": ["Mit", "den", "He\u00b7roen", "sei\u00b7ner", "Sce\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Dir aufzuwarten? Die Kam\u00f6ne", "tokens": ["Dir", "auf\u00b7zu\u00b7war\u00b7ten", "?", "Die", "Ka\u00b7m\u00f6\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVIZU", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Ward roth. Wer bist du Menschenkind,", "tokens": ["Ward", "roth", ".", "Wer", "bist", "du", "Men\u00b7schen\u00b7kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$.", "PWS", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Sprach sie zum Liebling, und wer sind", "tokens": ["Sprach", "sie", "zum", "Lieb\u00b7ling", ",", "und", "wer", "sind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN", "$,", "KON", "PWS", "VAFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.37": {"text": "Die Leutlein mit den seidnen Hosen?", "tokens": ["Die", "Leut\u00b7lein", "mit", "den", "seid\u00b7nen", "Ho\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Hier dreht er nach der Heldenschaar", "tokens": ["Hier", "dreht", "er", "nach", "der", "Hel\u00b7den\u00b7schaar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Sich bebend um, und sieh, es war", "tokens": ["Sich", "be\u00b7bend", "um", ",", "und", "sieh", ",", "es", "war"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PRF", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Ein Trupp gekr\u00e4uselter Franzosen!", "tokens": ["Ein", "Trupp", "ge\u00b7kr\u00e4u\u00b7sel\u00b7ter", "Fran\u00b7zo\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Der Abgott der Pariser B\u00fchne,", "tokens": ["Der", "Ab\u00b7gott", "der", "Pa\u00b7ri\u00b7ser", "B\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der unnachahmliche Racine,", "tokens": ["Der", "un\u00b7nach\u00b7ahm\u00b7li\u00b7che", "Ra\u00b7ci\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Trat kaum aus Charons schwarzem Kahn", "tokens": ["Trat", "kaum", "aus", "Cha\u00b7rons", "schwar\u00b7zem", "Kahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So suchte der galante Dichter", "tokens": ["So", "such\u00b7te", "der", "ga\u00b7lan\u00b7te", "Dich\u00b7ter"], "token_info"