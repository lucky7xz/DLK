{"textgrid.poem.41548": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Pfingstmontage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist es der Glaube nur, dem du verhei\u00dfen,", "tokens": ["Ist", "es", "der", "Glau\u00b7be", "nur", ",", "dem", "du", "ver\u00b7hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann bin ich tot.", "tokens": ["Dann", "bin", "ich", "tot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "O Glaube! wie lebend'gen Blutes Kreisen,", "tokens": ["O", "Glau\u00b7be", "!", "wie", "le\u00b7ben\u00b7d'\u00b7gen", "Blu\u00b7tes", "Krei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "ADJA", "NN", "NN", "$,"], "meter": "-+------+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Er tut mir not;", "tokens": ["Er", "tut", "mir", "not", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich hab' ihn nicht.", "tokens": ["Ich", "hab'", "ihn", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ach nimmst du statt des Glaubens nicht die Liebe", "tokens": ["Ach", "nimmst", "du", "statt", "des", "Glau\u00b7bens", "nicht", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und des Verlangens tr\u00e4nenschweren Zoll:", "tokens": ["Und", "des", "Ver\u00b7lan\u00b7gens", "tr\u00e4\u00b7nen\u00b7schwe\u00b7ren", "Zoll", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So wei\u00df ich nicht, wie mir noch Hoffnung bliebe;", "tokens": ["So", "wei\u00df", "ich", "nicht", ",", "wie", "mir", "noch", "Hoff\u00b7nung", "blie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Gebrochen ist der Stab, das Ma\u00df ist voll", "tokens": ["Ge\u00b7bro\u00b7chen", "ist", "der", "Stab", ",", "das", "Ma\u00df", "ist", "voll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mir zum Gericht.", "tokens": ["Mir", "zum", "Ge\u00b7richt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Mein Heiland, der du liebst, wie niemand liebet,", "tokens": ["Mein", "Hei\u00b7land", ",", "der", "du", "liebst", ",", "wie", "nie\u00b7mand", "lie\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchlst du denn kein", "tokens": ["F\u00fchlst", "du", "denn", "kein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PIAT"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Erbarmen, wenn so krank und tiefbetr\u00fcbet", "tokens": ["Er\u00b7bar\u00b7men", ",", "wenn", "so", "krank", "und", "tief\u00b7be\u00b7tr\u00fc\u00b7bet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ADV", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf hartem Stein", "tokens": ["Auf", "har\u00b7tem", "Stein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dein Ebenbild", "tokens": ["Dein", "E\u00b7ben\u00b7bild"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "In seiner Angst vergehend kniet und flehet?", "tokens": ["In", "sei\u00b7ner", "Angst", "ver\u00b7ge\u00b7hend", "kniet", "und", "fle\u00b7het", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ist denn der Glaube nur dein Gotteshauch?", "tokens": ["Ist", "denn", "der", "Glau\u00b7be", "nur", "dein", "Got\u00b7tes\u00b7hauch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Hast du nicht tief in unsre Brust ges\u00e4et", "tokens": ["Hast", "du", "nicht", "tief", "in", "uns\u00b7re", "Brust", "ge\u00b7s\u00e4et"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Mit deinem eignen Blut die Liebe auch?", "tokens": ["Mit", "dei\u00b7nem", "eig\u00b7nen", "Blut", "die", "Lie\u00b7be", "auch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "O sei doch mild!", "tokens": ["O", "sei", "doch", "mild", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ein hartes schweres Wort hast du gesprochen,", "tokens": ["Ein", "har\u00b7tes", "schwe\u00b7res", "Wort", "hast", "du", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df \u00bbwer nicht glaubt,", "tokens": ["Da\u00df", "\u00bb", "wer", "nicht", "glaubt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PWS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Gerichtet ist\u00ab \u2013 so bin ich ganz gebrochen.", "tokens": ["Ge\u00b7rich\u00b7tet", "ist", "\u00ab", "\u2013", "so", "bin", "ich", "ganz", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$(", "$(", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch so beraubt", "tokens": ["Doch", "so", "be\u00b7raubt"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "L\u00e4\u00dft er mich nicht,", "tokens": ["L\u00e4\u00dft", "er", "mich", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Der hingab seinen Sohn, den eingebornen,", "tokens": ["Der", "hin\u00b7gab", "sei\u00b7nen", "Sohn", ",", "den", "ein\u00b7ge\u00b7bor\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "F\u00fcr S\u00fcnder wie f\u00fcr Fromme allzugleich.", "tokens": ["F\u00fcr", "S\u00fcn\u00b7der", "wie", "f\u00fcr", "From\u00b7me", "all\u00b7zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOKOM", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Zu ihm ich schau', die \u00c4rmste der Verlornen,", "tokens": ["Zu", "ihm", "ich", "schau'", ",", "die", "\u00c4rms\u00b7te", "der", "Ver\u00b7lor\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nur um ein Hoffnungswort, er ist so reich", "tokens": ["Nur", "um", "ein", "Hoff\u00b7nungs\u00b7wort", ",", "er", "ist", "so", "reich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mein Gnadenlicht!", "tokens": ["Mein", "Gna\u00b7den\u00b7licht", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Du Milder, der die Taufe der Begierde", "tokens": ["Du", "Mil\u00b7der", ",", "der", "die", "Tau\u00b7fe", "der", "Be\u00b7gier\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So gn\u00e4diglich", "tokens": ["So", "gn\u00e4\u00b7di\u00b7glich"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Besiegelt selbst mit Sakramentes W\u00fcrde,", "tokens": ["Be\u00b7sie\u00b7gelt", "selbst", "mit", "Sak\u00b7ra\u00b7men\u00b7tes", "W\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht zweifle ich,", "tokens": ["Nicht", "zweif\u00b7le", "ich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Du hast gewi\u00df", "tokens": ["Du", "hast", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Den Glauben des Verlangens, Sehnens Weihe", "tokens": ["Den", "Glau\u00b7ben", "des", "Ver\u00b7lan\u00b7gens", ",", "Seh\u00b7nens", "Wei\u00b7he"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Gesegnet auch; sonst w\u00e4rst du wahrlich nicht", "tokens": ["Ge\u00b7seg\u00b7net", "auch", ";", "sonst", "w\u00e4rst", "du", "wahr\u00b7lich", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$.", "ADV", "VAFIN", "PPER", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So gro\u00df an Milde und so stark an Treue,", "tokens": ["So", "gro\u00df", "an", "Mil\u00b7de", "und", "so", "stark", "an", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Br\u00e4chst du ein Zweiglein, draus die Knospe bricht", "tokens": ["Br\u00e4chst", "du", "ein", "Zwei\u00b7glein", ",", "draus", "die", "Knos\u00b7pe", "bricht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und Frucht verhie\u00df.", "tokens": ["Und", "Frucht", "ver\u00b7hie\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Was durch Verstandes Irren ich verbrochen,", "tokens": ["Was", "durch", "Ver\u00b7stan\u00b7des", "Ir\u00b7ren", "ich", "ver\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich hab' es ja", "tokens": ["Ich", "hab'", "es", "ja"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Geb\u00fc\u00dft so manchen Tag und manche Wochen;", "tokens": ["Ge\u00b7b\u00fc\u00dft", "so", "man\u00b7chen", "Tag", "und", "man\u00b7che", "Wo\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So sei mir nah!", "tokens": ["So", "sei", "mir", "nah", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Nach meiner Kraft,", "tokens": ["Nach", "mei\u00b7ner", "Kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die freilich ich geknickt durch eigne Schulden,", "tokens": ["Die", "frei\u00b7lich", "ich", "ge\u00b7knickt", "durch", "eig\u00b7ne", "Schul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch einmal aufzurichten nicht vermag,", "tokens": ["Doch", "ein\u00b7mal", "auf\u00b7zu\u00b7rich\u00b7ten", "nicht", "ver\u00b7mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Will hoffen ich, will sehnen ich, will dulden;", "tokens": ["Will", "hof\u00b7fen", "ich", ",", "will", "seh\u00b7nen", "ich", ",", "will", "dul\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PPER", "$,", "VMFIN", "VVINF", "PPER", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Dann gibst du, Treuer, wohl den Glauben nach,", "tokens": ["Dann", "gibst", "du", ",", "Treu\u00b7er", ",", "wohl", "den", "Glau\u00b7ben", "nach", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der H\u00fclfe schafft.", "tokens": ["Der", "H\u00fcl\u00b7fe", "schafft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Ist es der Glaube nur, dem du verhei\u00dfen,", "tokens": ["Ist", "es", "der", "Glau\u00b7be", "nur", ",", "dem", "du", "ver\u00b7hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann bin ich tot.", "tokens": ["Dann", "bin", "ich", "tot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "O Glaube! wie lebend'gen Blutes Kreisen,", "tokens": ["O", "Glau\u00b7be", "!", "wie", "le\u00b7ben\u00b7d'\u00b7gen", "Blu\u00b7tes", "Krei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "ADJA", "NN", "NN", "$,"], "meter": "-+------+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Er tut mir not;", "tokens": ["Er", "tut", "mir", "not", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich hab' ihn nicht.", "tokens": ["Ich", "hab'", "ihn", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ach nimmst du statt des Glaubens nicht die Liebe", "tokens": ["Ach", "nimmst", "du", "statt", "des", "Glau\u00b7bens", "nicht", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "wo