{"dta.poem.11023": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unverantwortliches Zanken  \n \u00fcber das g\u00f6ttliche Wesen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sollt einer nur, von deinen Sinnen, fehlen;", "tokens": ["Sollt", "ei\u00b7ner", "nur", ",", "von", "dei\u00b7nen", "Sin\u00b7nen", ",", "feh\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was w\u00fcrde nicht dein Geist, von jetzt bekannten Sachen,", "tokens": ["Was", "w\u00fcr\u00b7de", "nicht", "dein", "Geist", ",", "von", "jetzt", "be\u00b7kann\u00b7ten", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00fcr ungereimt und falsche Ding erz\u00e4hlen,", "tokens": ["F\u00fcr", "un\u00b7ge\u00b7reimt", "und", "fal\u00b7sche", "Ding", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr irrigen Begriff sich machen!", "tokens": ["F\u00fcr", "ir\u00b7ri\u00b7gen", "Be\u00b7griff", "sich", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und eben dieser Geist, der keinen K\u00f6rper kennt,", "tokens": ["Und", "e\u00b7ben", "die\u00b7ser", "Geist", ",", "der", "kei\u00b7nen", "K\u00f6r\u00b7per", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDAT", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und blo\u00df durch Selbstbetrug, sich unbetrieglich nennt,", "tokens": ["Und", "blo\u00df", "durch", "Selbst\u00b7be\u00b7trug", ",", "sich", "un\u00b7be\u00b7trieg\u00b7lich", "nennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "$,", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Will sich vermessen unterstehen,", "tokens": ["Will", "sich", "ver\u00b7mes\u00b7sen", "un\u00b7ter\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "VVINF", "VVINF", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ins undurchdringlich helle Licht", "tokens": ["Ins", "un\u00b7durch\u00b7dring\u00b7lich", "hel\u00b7le", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Gottheit, das die Cherubinen,", "tokens": ["Der", "Got\u00b7theit", ",", "das", "die", "Che\u00b7ru\u00b7bi\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Kaum anzuschauen, sich erk\u00fchnen,", "tokens": ["Kaum", "an\u00b7zu\u00b7schau\u00b7en", ",", "sich", "er\u00b7k\u00fch\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVIZU", "$,", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So freventlich hinein zu sehen?", "tokens": ["So", "fre\u00b7vent\u00b7lich", "hin\u00b7ein", "zu", "se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ja sucht die aus ihm selbst gesponnene Gedanken,", "tokens": ["Ja", "sucht", "die", "aus", "ihm", "selbst", "ge\u00b7spon\u00b7ne\u00b7ne", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "ART", "APPR", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Auch andern Geistern aufzudringen,", "tokens": ["Auch", "an\u00b7dern", "Geis\u00b7tern", "auf\u00b7zu\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und durch ein mehr als h\u00fcndisch Zanken,", "tokens": ["Und", "durch", "ein", "mehr", "als", "h\u00fcn\u00b7disch", "Zan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "PIAT", "KOKOM", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Zu seiner Meynung sie zu zwingen!", "tokens": ["Zu", "sei\u00b7ner", "Mey\u00b7nung", "sie", "zu", "zwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Vermaledeyte Frucht des Hochmuths! kann auf Erden,", "tokens": ["Ver\u00b7ma\u00b7le\u00b7dey\u00b7te", "Frucht", "des", "Hoch\u00b7muths", "!", "kann", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$.", "VMFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was toll-was n\u00e4rrischer,", "tokens": ["Was", "toll\u00b7was", "n\u00e4r\u00b7ri\u00b7scher", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NE", "ADJA", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Ja fast was viehischer,", "tokens": ["Ja", "fast", "was", "vie\u00b7hi\u00b7scher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "PWS", "PIS", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Und Gott vergessener gefunden werden?", "tokens": ["Und", "Gott", "ver\u00b7ges\u00b7se\u00b7ner", "ge\u00b7fun\u00b7den", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.20": {"text": "Die wahre Gottheit mu\u00df allein,", "tokens": ["Die", "wah\u00b7re", "Got\u00b7theit", "mu\u00df", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Mit tiefester Erniedrigung,", "tokens": ["Mit", "tie\u00b7fes\u00b7ter", "Er\u00b7nied\u00b7ri\u00b7gung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+---+--", "measure": "dactylic.init"}, "line.22": {"text": "Mit ehrerbietigster Bewunderung,", "tokens": ["Mit", "ehr\u00b7er\u00b7bie\u00b7tigs\u00b7ter", "Be\u00b7wun\u00b7de\u00b7rung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Mit untergebenem Verstand,", "tokens": ["Mit", "un\u00b7ter\u00b7ge\u00b7be\u00b7nem", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.24": {"text": "Und auf den Mund gelegter Hand,", "tokens": ["Und", "auf", "den", "Mund", "ge\u00b7leg\u00b7ter", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Am w\u00fcrdigsten geehrt und angebethet seyn.", "tokens": ["Am", "w\u00fcr\u00b7digs\u00b7ten", "ge\u00b7ehrt", "und", "an\u00b7ge\u00b7be\u00b7thet", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VAFIN", "VVPP", "KON", "VVPP", "VAINF", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}