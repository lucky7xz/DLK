{"dta.poem.4294": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n bey ihren Jungen von einem B\u00e4ren  \n \u00fcberfallene L\u00f6win.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier bricht die m\u00fctterliche Lieb' in lichten Flammen aus!", "tokens": ["Hier", "bricht", "die", "m\u00fct\u00b7ter\u00b7li\u00b7che", "Lieb'", "in", "lich\u00b7ten", "Flam\u00b7men", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Wir sehen", "tokens": ["Wir", "se\u00b7hen"], "token_info": ["word", "word"], "pos": ["PPER", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Die L\u00f6win, ihre Zucht beschirmend, dem B\u00e4ren nicht entge-", "tokens": ["Die", "L\u00f6\u00b7win", ",", "ih\u00b7re", "Zucht", "be\u00b7schir\u00b7mend", ",", "dem", "B\u00e4\u00b7ren", "nicht", "ent\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "VVPP", "$,", "ART", "NN", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "gen gehen,", "tokens": ["gen", "ge\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Entgegen fahren, springen, fliegen, und, blind vor Zorn,", "tokens": ["Ent\u00b7ge\u00b7gen", "fah\u00b7ren", ",", "sprin\u00b7gen", ",", "flie\u00b7gen", ",", "und", ",", "blind", "vor", "Zorn", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$,", "KON", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "von Wut entbrannt,", "tokens": ["von", "Wut", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Gefahr und Noht und Tod verachten, die seines Feinds", "tokens": ["Ge\u00b7fahr", "und", "Noht", "und", "Tod", "ver\u00b7ach\u00b7ten", ",", "die", "sei\u00b7nes", "Feinds"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "erhab'ner Stand", "tokens": ["er\u00b7hab'\u00b7ner", "Stand"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "In vortheilhafter Stellung droht. Sie greift, da sie", "tokens": ["In", "vor\u00b7theil\u00b7haf\u00b7ter", "Stel\u00b7lung", "droht", ".", "Sie", "greift", ",", "da", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "PPER", "VVFIN", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "nicht anders kann,", "tokens": ["nicht", "an\u00b7ders", "kann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VMFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Um ihn zu sich herab zu ziehn, sogar des B\u00e4ren Bratzen", "tokens": ["Um", "ihn", "zu", "sich", "her\u00b7ab", "zu", "ziehn", ",", "so\u00b7gar", "des", "B\u00e4\u00b7ren", "Brat\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "APPR", "PRF", "ADV", "PTKZU", "VVINF", "$,", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "an.", "tokens": ["an", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Sie scheut die scharfen Z\u00e4hne nicht, sie f\u00fchlt f\u00fcr Wut", "tokens": ["Sie", "scheut", "die", "schar\u00b7fen", "Z\u00e4h\u00b7ne", "nicht", ",", "sie", "f\u00fchlt", "f\u00fcr", "Wut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$,", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "nicht Schmerz, nicht Bisse,", "tokens": ["nicht", "Schmerz", ",", "nicht", "Bis\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Ja sie empf\u00fcnd\u2019 es, glaub ich, kaum, wenn man sie von ein-", "tokens": ["Ja", "sie", "emp\u00b7f\u00fcnd'", "es", ",", "glaub", "ich", ",", "kaum", ",", "wenn", "man", "sie", "von", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADV", "$,", "KOUS", "PIS", "PPER", "APPR", "TRUNC"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.16": {"text": "ander risse.", "tokens": ["an\u00b7der", "ris\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Des jungen Welpens reges Taumeln zeigt etwas, das", "tokens": ["Des", "jun\u00b7gen", "Wel\u00b7pens", "re\u00b7ges", "Tau\u00b7meln", "zeigt", "et\u00b7was", ",", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "PIS", "$,", "PRELS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "bereits vorbey,", "tokens": ["be\u00b7reits", "vor\u00b7bey", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Zeigt, da\u00df die Zitz, an der er lag, ihm aus dem Maul", "tokens": ["Zeigt", ",", "da\u00df", "die", "Zitz", ",", "an", "der", "er", "lag", ",", "ihm", "aus", "dem", "Maul"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "entrissen sey,", "tokens": ["ent\u00b7ris\u00b7sen", "sey", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Durch den un\u00fcberlegten Sprung, der so gewaltig ihn ge-", "tokens": ["Durch", "den", "un\u00b7\u00fc\u00b7ber\u00b7leg\u00b7ten", "Sprung", ",", "der", "so", "ge\u00b7wal\u00b7tig", "ihn", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "PPER", "TRUNC"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "drehet,", "tokens": ["dre\u00b7het", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Da\u00df ihr, wollt ihr den Augen trauen, ihn noch best\u00e4ndig", "tokens": ["Da\u00df", "ihr", ",", "wollt", "ihr", "den", "Au\u00b7gen", "trau\u00b7en", ",", "ihn", "noch", "be\u00b7st\u00e4n\u00b7dig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,", "PPER", "ADV", "ADJD"], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.24": {"text": "purzeln sehet.", "tokens": ["pur\u00b7zeln", "se\u00b7het", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}