{"dta.poem.20554": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "13.  \n  Gneisenau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1848", "urn": "urn:nbn:de:kobv:b4-25129-1", "language": ["de:0.99"], "booktitle": "[N. N.]: Erinnerungen eines freiwilligen reitenden J\u00e4gers aus den Kriegsjahren 1813\u20131815. Berlin, 1848."}, "poem": {"stanza.1": {"line.1": {"text": "Wie hei\u00dft der Mann, der in besonn'ner Richtung", "tokens": ["Wie", "hei\u00dft", "der", "Mann", ",", "der", "in", "be\u00b7sonn'\u00b7ner", "Rich\u00b7tung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Geistes wirkte: kr\u00e4ftig, still und klar;", "tokens": ["Des", "Geis\u00b7tes", "wirk\u00b7te", ":", "kr\u00e4f\u00b7tig", ",", "still", "und", "klar", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bescheiden dastand und nur der Verpflichtung", "tokens": ["Be\u00b7schei\u00b7den", "das\u00b7tand", "und", "nur", "der", "Ver\u00b7pflich\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "ADV", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Stimme folgte, welche in ihm war,", "tokens": ["Der", "Stim\u00b7me", "folg\u00b7te", ",", "wel\u00b7che", "in", "ihm", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An Kriegesbildung Jeden \u00fcberragend,", "tokens": ["An", "Krie\u00b7ges\u00b7bil\u00b7dung", "Je\u00b7den", "\u00fc\u00b7berr\u00b7a\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Schon vor der Schlacht ihr Bild im Innern tragend?", "tokens": ["Schon", "vor", "der", "Schlacht", "ihr", "Bild", "im", "In\u00b7nern", "tra\u00b7gend", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wo f\u00e4nd\u2019 ich Worte, w\u00fcrdig dich zu schildern,", "tokens": ["Wo", "f\u00e4nd'", "ich", "Wor\u00b7te", ",", "w\u00fcr\u00b7dig", "dich", "zu", "schil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "$,", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich, der nie eig\u2019ne Hoheit trug zur Schau?", "tokens": ["Dich", ",", "der", "nie", "eig'\u00b7ne", "Ho\u00b7heit", "trug", "zur", "Schau", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Du, der Naturkraft ", "tokens": ["Du", ",", "der", "Na\u00b7tur\u00b7kraft"], "token_info": ["word", "punct", "word", "word"], "pos": ["PPER", "$,", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Unsterblicher, erhab\u2019ner ", "tokens": ["U\u00b7nsterb\u00b7li\u00b7cher", ",", "er\u00b7hab'\u00b7ner"], "token_info": ["word", "punct", "word"], "pos": ["ADJD", "$,", "ADJA"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "G\u00fctig gesinnt, stand\u2019st felsenfest als Ritter", "tokens": ["G\u00fc\u00b7tig", "ge\u00b7sinnt", ",", "stan\u00b7d'st", "fel\u00b7sen\u00b7fest", "als", "Rit\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "VVFIN", "ADJD", "KOKOM", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Du, droht\u2019 dem Staat ein nahendes Gewitter", "tokens": ["Du", ",", "droht'", "dem", "Staat", "ein", "na\u00b7hen\u00b7des", "Ge\u00b7wit\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was Du uns warst, lehrt kein Geschichtskunstschreiber.", "tokens": ["Was", "Du", "uns", "warst", ",", "lehrt", "kein", "Ge\u00b7schichts\u00b7kunst\u00b7schrei\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VAFIN", "$,", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Du warst der geist\u2019ge Keil, der m\u00e4cht\u2019ge Treiber;", "tokens": ["Du", "warst", "der", "geist'\u00b7ge", "Keil", ",", "der", "m\u00e4cht'\u00b7ge", "Trei\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Dampfrad, das der Freiheit Werk bewegt.", "tokens": ["Das", "Dampf\u00b7rad", ",", "das", "der", "Frei\u00b7heit", "Werk", "be\u00b7wegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es war Dein Sinn, der nach der Tugend strebte,", "tokens": ["Es", "war", "Dein", "Sinn", ",", "der", "nach", "der", "Tu\u00b7gend", "streb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und jedes Herz durch seine Glut belebte.", "tokens": ["Und", "je\u00b7des", "Herz", "durch", "sei\u00b7ne", "Glut", "be\u00b7leb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}