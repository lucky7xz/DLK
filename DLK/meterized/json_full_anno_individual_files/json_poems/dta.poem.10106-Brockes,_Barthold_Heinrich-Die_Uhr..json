{"dta.poem.10106": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Uhr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es sieht der eitle Mensch, im Reiche der Natur,", "tokens": ["Es", "sieht", "der", "eit\u00b7le", "Mensch", ",", "im", "Rei\u00b7che", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "APPRART", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich selbst (wofern nicht gar f\u00fcr einen, der sie machet)", "tokens": ["Sich", "selbst", "(", "wo\u00b7fern", "nicht", "gar", "f\u00fcr", "ei\u00b7nen", ",", "der", "sie", "ma\u00b7chet", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "$(", "KOUS", "PTKNEG", "ADV", "APPR", "ART", "$,", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aufs wenigst an f\u00fcr eine Uhr:", "tokens": ["Aufs", "we\u00b7nigst", "an", "f\u00fcr", "ei\u00b7ne", "Uhr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da er sich doch nicht einst f\u00fcr einen einz\u2019gen Zahn", "tokens": ["Da", "er", "sich", "doch", "nicht", "einst", "f\u00fcr", "ei\u00b7nen", "einz'\u00b7gen", "Zahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKNEG", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "An einem Rad\u2019 in diesem Welt-Geb\u00e4ude", "tokens": ["An", "ei\u00b7nem", "Rad'", "in", "die\u00b7sem", "Welt\u00b7Ge\u00b7b\u00e4u\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit Rechte sch\u00e4tzen lassen kann.", "tokens": ["Mit", "Rech\u00b7te", "sch\u00e4t\u00b7zen", "las\u00b7sen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn, wenn in einer Uhr ein Zahn zerbricht,", "tokens": ["Denn", ",", "wenn", "in", "ei\u00b7ner", "Uhr", "ein", "Zahn", "zer\u00b7bricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verhindert es sogleich die Richtigkeit,", "tokens": ["Ver\u00b7hin\u00b7dert", "es", "sog\u00b7leich", "die", "Rich\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und mindert die Vollkommenheit:", "tokens": ["Und", "min\u00b7dert", "die", "Voll\u00b7kom\u00b7men\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch dein Zerbrechen f\u00fchlt der Kreis der Erden nicht.", "tokens": ["Doch", "dein", "Zer\u00b7bre\u00b7chen", "f\u00fchlt", "der", "Kreis", "der", "Er\u00b7den", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}