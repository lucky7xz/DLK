{"dta.poem.14350": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Auf meines Kindes Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt treibt fort ihr Wesen,", "tokens": ["Die", "Welt", "treibt", "fort", "ihr", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Leute kommen und geh'n,", "tokens": ["Die", "Leu\u00b7te", "kom\u00b7men", "und", "geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Als w\u00e4rst du nie gewesen,", "tokens": ["Als", "w\u00e4rst", "du", "nie", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als w\u00e4re nichts gescheh'n.", "tokens": ["Als", "w\u00e4\u00b7re", "nichts", "ge\u00b7scheh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "VAFIN", "PIS", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}