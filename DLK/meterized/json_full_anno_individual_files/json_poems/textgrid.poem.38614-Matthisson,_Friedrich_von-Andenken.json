{"textgrid.poem.38614": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Andenken", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bl\u00fcht im Kranz, o M\u00e4dchen, dir noch die Rose,", "tokens": ["Bl\u00fcht", "im", "Kranz", ",", "o", "M\u00e4d\u00b7chen", ",", "dir", "noch", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "FM", "NN", "$,", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Wenn du, gleich Sylfiden, bei'm Abendreigen", "tokens": ["Wenn", "du", ",", "gleich", "Syl\u00b7fi\u00b7den", ",", "bei'm", "A\u00b7ben\u00b7drei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADV", "NN", "$,", "APPRART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Hinschwebst, oder deckte sie schon dein Grabmaal", "tokens": ["Hin\u00b7schwebst", ",", "o\u00b7der", "deck\u00b7te", "sie", "schon", "dein", "Grab\u00b7maal"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sterbend mit Purpur?", "tokens": ["Ster\u00b7bend", "mit", "Pur\u00b7pur", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Jahre schwanden: aber dein Bild erscheint mir,", "tokens": ["Jah\u00b7re", "schwan\u00b7den", ":", "a\u00b7ber", "dein", "Bild", "er\u00b7scheint", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "ADV", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Wo durch Alpenschl\u00fcnde der Bergstrom donnert,", "tokens": ["Wo", "durch", "Al\u00b7pen\u00b7schl\u00fcn\u00b7de", "der", "Berg\u00b7strom", "don\u00b7nert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wo Nachtigallen am Quell auf Myrten", "tokens": ["Und", "wo", "Nach\u00b7ti\u00b7gal\u00b7len", "am", "Quell", "auf", "Myr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "APPRART", "NN", "APPR", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Fl\u00f6tend sich wiegen.", "tokens": ["Fl\u00f6\u00b7tend", "sich", "wie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Wiedersehn der Liebenden, wo der Heimath", "tokens": ["Wie\u00b7der\u00b7sehn", "der", "Lie\u00b7ben\u00b7den", ",", "wo", "der", "Hei\u00b7math"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Goldne Sterne funkeln, o du der armen", "tokens": ["Gold\u00b7ne", "Ster\u00b7ne", "fun\u00b7keln", ",", "o", "du", "der", "ar\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVINF", "$,", "FM", "PPER", "ART", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Psyche, die gebunden im Grabthal schmachtet,", "tokens": ["Psy\u00b7che", ",", "die", "ge\u00b7bun\u00b7den", "im", "Grab\u00b7thal", "schmach\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVPP", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Heiligste Sehnsucht!", "tokens": ["Hei\u00b7ligs\u00b7te", "Sehn\u00b7sucht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Bl\u00fcht im Kranz, o M\u00e4dchen, dir noch die Rose,", "tokens": ["Bl\u00fcht", "im", "Kranz", ",", "o", "M\u00e4d\u00b7chen", ",", "dir", "noch", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "FM", "NN", "$,", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Wenn du, gleich Sylfiden, bei'm Abendreigen", "tokens": ["Wenn", "du", ",", "gleich", "Syl\u00b7fi\u00b7den", ",", "bei'm", "A\u00b7ben\u00b7drei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADV", "NN", "$,", "APPRART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Hinschwebst, oder deckte sie schon dein Grabmaal", "tokens": ["Hin\u00b7schwebst", ",", "o\u00b7der", "deck\u00b7te", "sie", "schon", "dein", "Grab\u00b7maal"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sterbend mit Purpur?", "tokens": ["Ster\u00b7bend", "mit", "Pur\u00b7pur", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.