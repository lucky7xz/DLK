{"dta.poem.2424": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Anders.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Gehstu betr\u00fcbter Mensch du armer S\u00fcndenknecht/", "tokens": ["Gehs\u00b7tu", "be\u00b7tr\u00fcb\u00b7ter", "Mensch", "du", "ar\u00b7mer", "S\u00fcn\u00b7den\u00b7knecht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Lastern \u00fcberheuft in deines Gottes Tempel", "tokens": ["Mit", "Las\u00b7tern", "\u00fc\u00b7berh\u00b7euft", "in", "dei\u00b7nes", "Got\u00b7tes", "Tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So bete so wie ich/ mich haflu zum Exempel:", "tokens": ["So", "be\u00b7te", "so", "wie", "ich", "/", "mich", "haf\u00b7lu", "zum", "Ex\u00b7em\u00b7pel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "KOKOM", "PPER", "$(", "PPER", "NE", "APPRART", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Gott stellt sich gn\u00e4dig ein/ du aber wirst gerecht-", "tokens": ["Gott", "stellt", "sich", "gn\u00e4\u00b7dig", "ein", "/", "du", "a\u00b7ber", "wirst", "ge\u00b7recht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ADJD", "ART", "$(", "PPER", "ADV", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measu