{"dta.poem.11027": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wunderlicher Handel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Gold und Geld, so wir, im Westen, der Erden Schoo\u00df\nentrissen haben,", "tokens": ["Das", "Gold", "und", "Geld", ",", "so", "wir", ",", "im", "Wes\u00b7ten", ",", "der", "Er\u00b7den", "Schoo\u00df", "ent\u00b7ris\u00b7sen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "ADV", "PPER", "$,", "APPRART", "NN", "$,", "ART", "NN", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Bestrebt die Menschheit sich, mit M\u00fch, in ", "tokens": ["Be\u00b7strebt", "die", "Menschheit", "sich", ",", "mit", "M\u00fch", ",", "in"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "NN", "PRF", "$,", "APPR", "NN", "$,", "APPR"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Europa, der das wenigste von allen in dem Beutel bleibt,", "tokens": ["Eu\u00b7ro\u00b7pa", ",", "der", "das", "we\u00b7nigs\u00b7te", "von", "al\u00b7len", "in", "dem", "Beu\u00b7tel", "bleibt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "PIS", "APPR", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Scheint, da\u00df sie, blo\u00df als M\u00e4cklerinn, den Handel f\u00fcr Cour-", "tokens": ["Scheint", ",", "da\u00df", "sie", ",", "blo\u00df", "als", "M\u00e4c