{"textgrid.poem.40667": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Wildbach", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reich ist das Meer! Gestirn' und Sonne pr\u00e4gen", "tokens": ["Reich", "ist", "das", "Meer", "!", "Ge\u00b7stirn'", "und", "Son\u00b7ne", "pr\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$.", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Bild in sein Brokatgewand; ihm wallen", "tokens": ["Ihr", "Bild", "in", "sein", "Bro\u00b7kat\u00b7ge\u00b7wand", ";", "ihm", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ins Becken, das voll Perlen und Korallen,", "tokens": ["Ins", "Be\u00b7cken", ",", "das", "voll", "Per\u00b7len", "und", "Ko\u00b7ral\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zinspflicht'ge Str\u00f6me, sch\u00fcttend Goldessegen.", "tokens": ["Zins\u00b7pflicht'\u00b7ge", "Str\u00f6\u00b7me", ",", "sch\u00fct\u00b7tend", "Gol\u00b7des\u00b7se\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Schmuckk\u00e4stchen gleich die Silberflotten w\u00e4gen,", "tokens": ["Schmuck\u00b7k\u00e4st\u00b7chen", "gleich", "die", "Sil\u00b7ber\u00b7flot\u00b7ten", "w\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Es leert, zerschl\u00e4gt sie spielend nach Gefallen!", "tokens": ["Es", "leert", ",", "zer\u00b7schl\u00e4gt", "sie", "spie\u00b7lend", "nach", "Ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch welche Botschaft macht so eilig wallen,", "tokens": ["Doch", "wel\u00b7che", "Bot\u00b7schaft", "macht", "so", "ei\u00b7lig", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "VVFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wildb\u00e4chlein, dich aus armen Waldgehegen?", "tokens": ["Wild\u00b7b\u00e4ch\u00b7lein", ",", "dich", "aus", "ar\u00b7men", "Wald\u00b7ge\u00b7he\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.3": {"line.1": {"text": "\u00bbreich ist das Meer, die F\u00fcrstin, die zum Feste", "tokens": ["\u00bb", "reich", "ist", "das", "Meer", ",", "die", "F\u00fcrs\u00b7tin", ",", "die", "zum", "Fes\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADJD", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kostbar geschm\u00fcckt mit Stoffen, Steinen, Ringen;", "tokens": ["Kost\u00b7bar", "ge\u00b7schm\u00fcckt", "mit", "Stof\u00b7fen", ",", "Stei\u00b7nen", ",", "Rin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch fehlt der Blumenstrau\u00df ans Herz, das Beste!", "tokens": ["Doch", "fehlt", "der", "Blu\u00b7men\u00b7strau\u00df", "ans", "Herz", ",", "das", "Bes\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das Meer sehnt sich nach fernem Waldesbildni\u00df,", "tokens": ["Das", "Meer", "sehnt", "sich", "nach", "fer\u00b7nem", "Wal\u00b7des\u00b7bild\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich nahm es auf, ihm's unentstellt zu bringen:", "tokens": ["Ich", "nahm", "es", "auf", ",", "ihm's", "un\u00b7ent\u00b7stellt", "zu", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Sch\u00f6nheit Macht erg\u00e4nzt die arme Wildni\u00df.\u00ab", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "Macht", "er\u00b7g\u00e4nzt", "die", "ar\u00b7me", "Wild\u00b7ni\u00df", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Reich ist das Meer! Ge