{"textgrid.poem.49504": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "An das Gra\u00df, worauf Phillis geruht", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Phillis, die die Tugend kennet,", "tokens": ["Phil\u00b7lis", ",", "die", "die", "Tu\u00b7gend", "ken\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die selbst Amor Schwester nennet,", "tokens": ["Die", "selbst", "A\u00b7mor", "Schwes\u00b7ter", "nen\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat, o junger Myrtenhain,", "tokens": ["Hat", ",", "o", "jun\u00b7ger", "Myr\u00b7ten\u00b7hain", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Welcher noch f\u00fcr Liebe brennet,", "tokens": ["Wel\u00b7cher", "noch", "f\u00fcr", "Lie\u00b7be", "bren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mir, zu Lindrung meiner Pein,", "tokens": ["Mir", ",", "zu", "Lind\u00b7rung", "mei\u00b7ner", "Pein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Den Besuch in dir geg\u00f6nnet.", "tokens": ["Den", "Be\u00b7such", "in", "dir", "ge\u00b7g\u00f6n\u00b7net", "."],