{"dta.poem.6337": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Holtzes \u00e4stige und Zweigen-f\u00f6rm'ge Theile", "tokens": ["Des", "Holt\u00b7zes", "\u00e4s\u00b7ti\u00b7ge", "und", "Zwei\u00b7gen\u00b7f\u00f6r\u00b7m'\u00b7ge", "Thei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sind, wenn sie allgemach sich l\u00f6sen und sich trennen,", "tokens": ["Sind", ",", "wenn", "sie", "all\u00b7ge\u00b7mach", "sich", "l\u00f6\u00b7sen", "und", "sich", "tren\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "ADV", "PRF", "VVINF", "KON", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geschickt die Gluht zu n\u00e4hren, und zu brennen;", "tokens": ["Ge\u00b7schickt", "die", "Gluht", "zu", "n\u00e4h\u00b7ren", ",", "und", "zu", "bren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "PTKZU", "VVINF", "$,", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wenn das Holtz die Theilchen, welche feucht,", "tokens": ["Und", "wenn", "das", "Holtz", "die", "Theil\u00b7chen", ",", "wel\u00b7che", "feucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Indem es brennt, verliehrt, entstehn die Flammen leicht.", "tokens": ["In\u00b7dem", "es", "brennt", ",", "ver\u00b7liehrt", ",", "ent\u00b7stehn", "die", "Flam\u00b7men", "leicht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es brennt in wilder Gluht der fetten Theilchen Krafft,", "tokens": ["Es", "brennt", "in", "wil\u00b7der", "Gluht", "der", "fet\u00b7ten", "Theil\u00b7chen", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von Naphta, Hartz und manchem z\u00e4hen Safft.", "tokens": ["Von", "Naph\u00b7ta", ",", "Hartz", "und", "man\u00b7chem", "z\u00e4\u00b7hen", "Safft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}