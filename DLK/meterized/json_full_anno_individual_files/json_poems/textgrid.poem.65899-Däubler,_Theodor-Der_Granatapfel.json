{"textgrid.poem.65899": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der Granatapfel", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbvon diesem s\u00fc\u00dfen Feuerkern genie\u00dfe!\u00ab", "tokens": ["\u00bb", "von", "die\u00b7sem", "s\u00fc\u00b7\u00dfen", "Feu\u00b7er\u00b7kern", "ge\u00b7nie\u00b7\u00dfe", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PDAT", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Spricht Hades und gibt Kore sacht die Frucht:", "tokens": ["Spricht", "Ha\u00b7des", "und", "gibt", "Ko\u00b7re", "sacht", "die", "Frucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "VVFIN", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbsei furchtlos nun, tilg mir die Eifersucht,", "tokens": ["\u00bb", "sei", "furcht\u00b7los", "nun", ",", "tilg", "mir", "die", "Ei\u00b7fer\u00b7sucht", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJD", "ADV", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Bis nie dein Wunsch zur Mutter mich verdrie\u00dfe!", "tokens": ["Bis", "nie", "dein", "Wunsch", "zur", "Mut\u00b7ter", "mich", "ver\u00b7drie\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPOSAT", "NN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bleib Kaiserin in unserm Reich: vergie\u00dfe", "tokens": ["Bleib", "Kai\u00b7se\u00b7rin", "in", "un\u00b7serm", "Reich", ":", "ver\u00b7gie\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "NN", "APPR", "PPOSAT", "NN", "$.", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Tr\u00e4nen in des Herzens Perlenschlucht,", "tokens": ["Die", "Tr\u00e4\u00b7nen", "in", "des", "Her\u00b7zens", "Per\u00b7len\u00b7schlucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die ich im Busen auftue \u2013 mit Wucht,", "tokens": ["Die", "ich", "im", "Bu\u00b7sen", "auf\u00b7tue", "\u2013", "mit", "Wucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$(", "APPR", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Da\u00df treuer Lenz bei Demeter ersprie\u00dfe!\u00ab", "tokens": ["Da\u00df", "treu\u00b7er", "Lenz", "bei", "De\u00b7me\u00b7ter", "er\u00b7sprie\u00b7\u00dfe", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Schale Bitterkeit ist tief vergangen,", "tokens": ["Der", "Scha\u00b7le", "Bit\u00b7ter\u00b7keit", "ist", "tief", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Braut schenkt Hades reif den Ku\u00df", "tokens": ["Die", "Braut", "schenkt", "Ha\u00b7des", "reif", "den", "Ku\u00df"], "token_info": ["word", "word", "word", "w