{"textgrid.poem.33880": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1. Die Jungfrau", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O s\u00fc\u00dfes, s\u00fc\u00dfes Jungfraunbild!", "tokens": ["O", "s\u00fc\u00b7\u00dfes", ",", "s\u00fc\u00b7\u00dfes", "Jung\u00b7fraun\u00b7bild", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Engelfrieden hingegossen!", "tokens": ["In", "En\u00b7gel\u00b7frie\u00b7den", "hin\u00b7ge\u00b7gos\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Noch Kind, und doch so g\u00f6ttlich angeschlossen!", "tokens": ["Noch", "Kind", ",", "und", "doch", "so", "g\u00f6tt\u00b7lich", "an\u00b7ge\u00b7schlos\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "KON", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem\u00fcthig, sicher, stolz und mild!", "tokens": ["De\u00b7m\u00fct\u00b7hig", ",", "si\u00b7cher", ",", "stolz", "und", "mild", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Jungfraunbild, dich m\u00f6gt' ich nicht \u2013", "tokens": ["O", "Jung\u00b7fraun\u00b7bild", ",", "dich", "m\u00f6gt'", "ich", "nicht", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", 