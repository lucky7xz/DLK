{"dta.poem.22697": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "248. Der Jahrs Beschlu\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Es wird da\u00df alte Jahr/ da\u00df sich nu schleust/ gehalten", "tokens": ["Es", "wird", "da\u00df", "al\u00b7te", "Jahr", "/", "da\u00df", "sich", "nu", "schleust", "/", "ge\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "KOUS", "ADJA", "NN", "$(", "KOUS", "PRF", "ADV", "VVFIN", "$(", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wan\u0303s vergangen w\u00e4r\u2019: un\u0303 di\u00df ist war mein Krist/", "tokens": ["Als", "wa\u00f1s", "ver\u00b7gan\u00b7gen", "w\u00e4r'", ":", "u\u00f1", "di\u00df", "ist", "war", "mein", "Krist", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVPP", "VAFIN", "$.", "KON", "PDS", "VAFIN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo du ein Neuer Mensch in GOtt geworden bist:", "tokens": ["Wo", "du", "ein", "Neu\u00b7er", "Mensch", "in", "Gott", "ge\u00b7wor\u00b7den", "bist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ists nicht: so lebstu noch wahrhafftig in dem alten.", "tokens": ["Ists", "nicht", ":", "so", "lebs\u00b7tu", "noch", "wahr\u00b7haff\u00b7tig", "in", "dem", "al\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$.", "ADV", "VVFIN", "ADV", "ADJD", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}