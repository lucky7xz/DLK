{"textgrid.poem.52633": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kommt herein, ihr frohen Freunde,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt herein, ihr frohen Freunde,", "tokens": ["Kommt", "her\u00b7ein", ",", "ihr", "fro\u00b7hen", "Freun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eilt heran, ihr Br\u00fcder all,", "tokens": ["Eilt", "he\u00b7ran", ",", "ihr", "Br\u00fc\u00b7der", "all", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "PIAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rosenjunge, sonngebr\u00e4unte,", "tokens": ["Ro\u00b7sen\u00b7jun\u00b7ge", ",", "sonn\u00b7ge\u00b7br\u00e4un\u00b7te", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geistesk\u00f6nig und Vasall!", "tokens": ["Geis\u00b7tes\u00b7k\u00f6\u00b7nig", "und", "Va\u00b7sall", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Nur ein freudiges Gem\u00fcth,", "tokens": ["Nur", "ein", "freu\u00b7di\u00b7ges", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das in Zecherwonnen gl\u00fcht,", "tokens": ["Das", "in", "Ze\u00b7cher\u00b7won\u00b7nen", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ist der Pa\u00df, den wir begehren,", "tokens": ["Ist", "der", "Pa\u00df", ",", "den", "wir", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einen Becher heut zu leeren.", "tokens": ["Ei\u00b7nen", "Be\u00b7cher", "heut", "zu", "lee\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eure Kehlen sind so trocken,", "tokens": ["Eu\u00b7re", "Keh\u00b7len", "sind", "so", "tro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eure Zungen, o wie stumm,", "tokens": ["Eu\u00b7re", "Zun\u00b7gen", ",", "o", "wie", "stumm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "KOKOM", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eppich windet um die Locken,", "tokens": ["Ep\u00b7pich", "win\u00b7det", "um", "die", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blumen um den Leib herum!", "tokens": ["Blu\u00b7men", "um", "den", "Leib", "he\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf den Rasen lagert euch", "tokens": ["Auf", "den", "Ra\u00b7sen", "la\u00b7gert", "euch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Neben duftiges Gestr\u00e4uch,", "tokens": ["Ne\u00b7ben", "duf\u00b7ti\u00b7ges", "Ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fangt zu trinken an, zu singen,", "tokens": ["Fangt", "zu", "trin\u00b7ken", "an", ",", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKZU", "VVINF", "PTKVZ", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und wer tanzen will, soll springen!", "tokens": ["Und", "wer", "tan\u00b7zen", "will", ",", "soll", "sprin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "VVINF", "VMFIN", "$,", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ausgegohrnen Weisheitsbecher,", "tokens": ["Aus\u00b7ge\u00b7gohr\u00b7nen", "Weis\u00b7heits\u00b7be\u00b7cher", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wein, wir trinken keinen Wein!", "tokens": ["Wein", ",", "wir", "trin\u00b7ken", "kei\u00b7nen", "Wein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ehrentrunk biderber Zecher,", "tokens": ["Eh\u00b7ren\u00b7trunk", "bi\u00b7der\u00b7ber", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Saft der Gerste wird es sein?", "tokens": ["Saft", "der", "Gers\u00b7te", "wird", "es", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kunstgew\u00e4sser? schlechte Post!", "tokens": ["Kunst\u00b7ge\u00b7w\u00e4s\u00b7ser", "?", "schlech\u00b7te", "Post", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nein wir trinken Traubenmost,", "tokens": ["Nein", "wir", "trin\u00b7ken", "Trau\u00b7ben\u00b7most", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Most, der vor dem Wein erfunden,", "tokens": ["Most", ",", "der", "vor", "dem", "Wein", "er\u00b7fun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Most der Trauben soll uns munden!", "tokens": ["Most", "der", "Trau\u00b7ben", "soll", "uns", "mun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr m\u00fc\u00dft werden wie die Kinder,", "tokens": ["Ihr", "m\u00fc\u00dft", "wer\u00b7den", "wie", "die", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VAINF", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Darum trinket einmal Most,", "tokens": ["Da\u00b7rum", "trin\u00b7ket", "ein\u00b7mal", "Most", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Wein, den alten S\u00fcnder,", "tokens": ["Und", "den", "Wein", ",", "den", "al\u00b7ten", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lasset schlafen nur getrost!", "tokens": ["Las\u00b7set", "schla\u00b7fen", "nur", "ge\u00b7trost", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gerne liegt er tr\u00e4g im Fa\u00df,", "tokens": ["Ger\u00b7ne", "liegt", "er", "tr\u00e4g", "im", "Fa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Freut sich, so zu werden ba\u00df,", "tokens": ["Freut", "sich", ",", "so", "zu", "wer\u00b7den", "ba\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "ADV", "PTKZU", "VAINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df er gar gesetzt, beim L\u00fcften,", "tokens": ["Da\u00df", "er", "gar", "ge\u00b7setzt", ",", "beim", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weniger Unheil m\u00f6chte stiften.", "tokens": ["We\u00b7ni\u00b7ger", "Un\u00b7heil", "m\u00f6ch\u00b7te", "stif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Wer vermag den Most zu preisen,", "tokens": ["Wer", "ver\u00b7mag", "den", "Most", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fchmen unter Sang und Klang,", "tokens": ["R\u00fch\u00b7men", "un\u00b7ter", "Sang", "und", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Athmen nicht schon unsre Weisen", "tokens": ["Ath\u00b7men", "nicht", "schon", "uns\u00b7re", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Munterkeit und Thatendrang?", "tokens": ["Mun\u00b7ter\u00b7keit", "und", "Tha\u00b7ten\u00b7drang", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schaut mir an den tollen Most,", "tokens": ["Schaut", "mir", "an", "den", "tol\u00b7len", "Most", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie er tobt im Glas und tost!", "tokens": ["Wie", "er", "tobt", "im", "Glas", "und", "tost", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ohne T\u00fccke, ausgelassen,", "tokens": ["Oh\u00b7ne", "T\u00fc\u00b7cke", ",", "aus\u00b7ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie ein Trotzkopf auf der Gassen.", "tokens": ["Wie", "ein", "Trotz\u00b7kopf", "auf", "der", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wer soll leben, wer ist w\u00fcrdig", "tokens": ["Wer", "soll", "le\u00b7ben", ",", "wer", "ist", "w\u00fcr\u00b7dig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VMFIN", "VVINF", "$,", "PWS", "VAFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eines donnervollen Tosts?", "tokens": ["Ei\u00b7nes", "don\u00b7ner\u00b7vol\u00b7len", "Tosts", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer ist Geistesebenb\u00fcrtig,", "tokens": ["Wer", "ist", "Geis\u00b7te\u00b7se\u00b7ben\u00b7b\u00fcr\u00b7tig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer verwandt der Kraft des Mosts?", "tokens": ["Wer", "ver\u00b7wandt", "der", "Kraft", "des", "Mosts", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Helden gibt es nur genug:", "tokens": ["Hel\u00b7den", "gibt", "es", "nur", "ge\u00b7nug", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "David, der den Goliath schlug!", "tokens": ["Da\u00b7vid", ",", "der", "den", "Go\u00b7li\u00b7ath", "schlug", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Jung Roland, jung Siegfried lebe!", "tokens": ["Jung", "Ro\u00b7land", ",", "jung", "Sieg\u00b7fried", "le\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJD", "NN", "VVFIN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und \u2013 das G\u00f6tterkind jung Hebe!", "tokens": ["Und", "\u2013", "das", "G\u00f6t\u00b7ter\u00b7kind", "jung", "He\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Kommt herein, ihr frohen Freunde,", "tokens": ["Kommt", "her\u00b7ein", ",", "ihr", "fro\u00b7hen", "Freun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eilt heran, ihr Br\u00fcder all,", "tokens": ["Eilt", "he\u00b7ran", ",", "ihr", "Br\u00fc\u00b7der", "all", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "PIAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rosenjunge, sonngebr\u00e4unte,", "tokens": ["Ro\u00b7sen\u00b7jun\u00b7ge", ",", "sonn\u00b7ge\u00b7br\u00e4un\u00b7te", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geistesk\u00f6nig und Vasall!", "tokens": ["Geis\u00b7tes\u00b7k\u00f6\u00b7nig", "und", "Va\u00b7sall", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Nur ein freudiges Gem\u00fcth,", "tokens": ["Nur", "ein", "freu\u00b7di\u00b7ges", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das in Zecherwonnen gl\u00fcht,", "tokens": ["Das", "in", "Ze\u00b7cher\u00b7won\u00b7nen", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ist der Pa\u00df, den wir begehren,", "tokens": ["Ist", "der", "Pa\u00df", ",", "den", "wir", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einen Becher heut zu leeren.", "tokens": ["Ei\u00b7nen", "Be\u00b7cher", "heut", "zu", "lee\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Eure Kehlen sind so trocken,", "tokens": ["Eu\u00b7re", "Keh\u00b7len", "sind", "so", "tro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eure Zungen, o wie stumm,", "tokens": ["Eu\u00b7re", "Zun\u00b7gen", ",", "o", "wie", "stumm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "KOKOM", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eppich windet um die Locken,", "tokens": ["Ep\u00b7pich", "win\u00b7det", "um", "die", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blumen um den Leib herum!", "tokens": ["Blu\u00b7men", "um", "den", "Leib", "he\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf den Rasen lagert euch", "tokens": ["Auf", "den", "Ra\u00b7sen", "la\u00b7gert", "euch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Neben duftiges Gestr\u00e4uch,", "tokens": ["Ne\u00b7ben", "duf\u00b7ti\u00b7ges", "Ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fangt zu trinken an, zu singen,", "tokens": ["Fangt", "zu", "trin\u00b7ken", "an", ",", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKZU", "VVINF", "PTKVZ", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und wer tanzen will, soll springen!", "tokens": ["Und", "wer", "tan\u00b7zen", "will", ",", "soll", "sprin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "VVINF", "VMFIN", "$,", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ausgegohrnen Weisheitsbecher,", "tokens": ["Aus\u00b7ge\u00b7gohr\u00b7nen", "Weis\u00b7heits\u00b7be\u00b7cher", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wein, wir trinken keinen Wein!", "tokens": ["Wein", ",", "wir", "trin\u00b7ken", "kei\u00b7nen", "Wein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ehrentrunk biderber Zecher,", "tokens": ["Eh\u00b7ren\u00b7trunk", "bi\u00b7der\u00b7ber", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Saft der Gerste wird es sein?", "tokens": ["Saft", "der", "Gers\u00b7te", "wird", "es", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kunstgew\u00e4sser? schlechte Post!", "tokens": ["Kunst\u00b7ge\u00b7w\u00e4s\u00b7ser", "?", "schlech\u00b7te", "Post", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nein wir trinken Traubenmost,", "tokens": ["Nein", "wir", "trin\u00b7ken", "Trau\u00b7ben\u00b7most", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Most, der vor dem Wein erfunden,", "tokens": ["Most", ",", "der", "vor", "dem", "Wein", "er\u00b7fun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Most der Trauben soll uns munden!", "tokens": ["Most", "der", "Trau\u00b7ben", "soll", "uns", "mun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ihr m\u00fc\u00dft werden wie die Kinder,", "tokens": ["Ihr", "m\u00fc\u00dft", "wer\u00b7den", "wie", "die", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VAINF", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Darum trinket einmal Most,", "tokens": ["Da\u00b7rum", "trin\u00b7ket", "ein\u00b7mal", "Most", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Wein, den alten S\u00fcnder,", "tokens": ["Und", "den", "Wein", ",", "den", "al\u00b7ten", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lasset schlafen nur getrost!", "tokens": ["Las\u00b7set", "schla\u00b7fen", "nur", "ge\u00b7trost", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gerne liegt er tr\u00e4g im Fa\u00df,", "tokens": ["Ger\u00b7ne", "liegt", "er", "tr\u00e4g", "im", "Fa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Freut sich, so zu werden ba\u00df,", "tokens": ["Freut", "sich", ",", "so", "zu", "wer\u00b7den", "ba\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "ADV", "PTKZU", "VAINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df er gar gesetzt, beim L\u00fcften,", "tokens": ["Da\u00df", "er", "gar", "ge\u00b7setzt", ",", "beim", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weniger Unheil m\u00f6chte stiften.", "tokens": ["We\u00b7ni\u00b7ger", "Un\u00b7heil", "m\u00f6ch\u00b7te", "stif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.11": {"line.1": {"text": "Wer vermag den Most zu preisen,", "tokens": ["Wer", "ver\u00b7mag", "den", "Most", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fchmen unter Sang und Klang,", "tokens": ["R\u00fch\u00b7men", "un\u00b7ter", "Sang", "und", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Athmen nicht schon unsre Weisen", "tokens": ["Ath\u00b7men", "nicht", "schon", "uns\u00b7re", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Munterkeit und Thatendrang?", "tokens": ["Mun\u00b7ter\u00b7keit", "und", "Tha\u00b7ten\u00b7drang", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schaut mir an den tollen Most,", "tokens": ["Schaut", "mir", "an", "den", "tol\u00b7len", "Most", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie er tobt im Glas und tost!", "tokens": ["Wie", "er", "tobt", "im", "Glas", "und", "tost", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ohne T\u00fccke, ausgelassen,", "tokens": ["Oh\u00b7ne", "T\u00fc\u00b7cke", ",", "aus\u00b7ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie ein Trotzkopf auf der Gassen.", "tokens": ["Wie", "ein", "Trotz\u00b7kopf", "auf", "der", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Wer soll leben, wer ist w\u00fcrdig", "tokens": ["Wer", "soll", "le\u00b7ben", ",", "wer", "ist", "w\u00fcr\u00b7dig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VMFIN", "VVINF", "$,", "PWS", "VAFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eines donnervollen Tosts?", "tokens": ["Ei\u00b7nes", "don\u00b7ner\u00b7vol\u00b7len", "Tosts", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer ist Geistesebenb\u00fcrtig,", "tokens": ["Wer", "ist", "Geis\u00b7te\u00b7se\u00b7ben\u00b7b\u00fcr\u00b7tig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer verwandt der Kraft des Mosts?", "tokens": ["Wer", "ver\u00b7wandt", "der", "Kraft", "des", "Mosts", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Helden gibt es nur genug:", "tokens": ["Hel\u00b7den", "gibt", "es", "nur", "ge\u00b7nug", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "David, der den Goliath schlug!", "tokens": ["Da\u00b7vid", ",", "der", "den", "Go\u00b7li\u00b7ath", "schlug", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Jung Roland, jung Siegfried lebe!", "tokens": ["Jung", "Ro\u00b7land", ",", "jung", "Sieg\u00b7fried", "le\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJD", "NN", "VVFIN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und \u2013 das G\u00f6tterkind jung Hebe!", "tokens": ["Und", "\u2013", "das", "G\u00f6t\u00b7ter\u00b7kind", "jung", "He\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}