{"textgrid.poem.36143": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du, o Theure meiner Seelen,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, o Theure meiner Seelen,", "tokens": ["Du", ",", "o", "Theu\u00b7re", "mei\u00b7ner", "See\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine auserkorne Braut,", "tokens": ["Mei\u00b7ne", "au\u00b7ser\u00b7kor\u00b7ne", "Braut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die nach so viel bitterm Qu\u00e4len", "tokens": ["Die", "nach", "so", "viel", "bit\u00b7term", "Qu\u00e4\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mir die Liebe selbst vertraut;", "tokens": ["Mir", "die", "Lie\u00b7be", "selbst", "ver\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die aus einer Welt von Sch\u00f6nen", "tokens": ["Die", "aus", "ei\u00b7ner", "Welt", "von", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich mein Herze auserkor,", "tokens": ["Sich", "mein", "Her\u00b7ze", "au\u00b7ser\u00b7kor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die mir vor allen S\u00f6hnen", "tokens": ["Und", "die", "mir", "vor", "al\u00b7len", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Erde Treue schwor \u2013", "tokens": ["Die\u00b7ser", "Er\u00b7de", "Treu\u00b7e", "schwor", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier, ach! in der trauten Stunde,", "tokens": ["Hier", ",", "ach", "!", "in", "der", "trau\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ITJ", "$.", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wo ich Lieb' aus deinem Aug',", "tokens": ["Wo", "ich", "Lieb'", "aus", "dei\u00b7nem", "Aug'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus deinem Honigmunde", "tokens": ["Und", "aus", "dei\u00b7nem", "Ho\u00b7nig\u00b7mun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Paradieseswonne saug';", "tokens": ["Pa\u00b7ra\u00b7die\u00b7ses\u00b7won\u00b7ne", "saug'", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo an deiner Rosenwange", "tokens": ["Wo", "an", "dei\u00b7ner", "Ro\u00b7sen\u00b7wan\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine hei\u00dfe Wange strebt,", "tokens": ["Mei\u00b7ne", "hei\u00b7\u00dfe", "Wan\u00b7ge", "strebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit immer st\u00e4rkerm Drange", "tokens": ["Und", "mit", "im\u00b7mer", "st\u00e4r\u00b7kerm", "Dran\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Brust an deiner bebt;", "tokens": ["Mei\u00b7ne", "Brust", "an", "dei\u00b7ner", "bebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo dein Hauch mit leisem Fluge", "tokens": ["Wo", "dein", "Hauch", "mit", "lei\u00b7sem", "Flu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich ums\u00e4uselt, und mein Geist", "tokens": ["Mich", "um\u00b7s\u00e4u\u00b7selt", ",", "und", "mein", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich bei jedem Odemzuge", "tokens": ["Sich", "bei", "je\u00b7dem", "O\u00b7dem\u00b7zu\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den deinigen ergeu\u00dft.", "tokens": ["In", "den", "dei\u00b7ni\u00b7gen", "er\u00b7geu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hier, ach! in das Meer der Wonne", "tokens": ["Hier", ",", "ach", "!", "in", "das", "Meer", "der", "Won\u00b7ne"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ITJ", "$.", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Fleu\u00dft ein Tropfen Bitterkeit:", "tokens": ["Fleu\u00dft", "ein", "Trop\u00b7fen", "Bit\u00b7ter\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie den Glanz der Mittagsonne", "tokens": ["Wie", "den", "Glanz", "der", "Mit\u00b7ta\u00b7gson\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wolkendunkel \u00fcberstreut.", "tokens": ["Wol\u00b7ken\u00b7dun\u00b7kel", "\u00fc\u00b7bers\u00b7treut", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "D\u00e4mm'rung sinkt vom Himmel nieder.", "tokens": ["D\u00e4m\u00b7m'\u00b7rung", "sinkt", "vom", "Him\u00b7mel", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Noch, du Liebe, bin ich hier.", "tokens": ["Noch", ",", "du", "Lie\u00b7be", ",", "bin", "ich", "hier", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "NN", "$,", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwar die D\u00e4mm'rung kommt wol wieder \u2013", "tokens": ["Zwar", "die", "D\u00e4m\u00b7m'\u00b7rung", "kommt", "wol", "wie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "ADV", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Aber ich nur nicht mit ihr.", "tokens": ["A\u00b7ber", "ich", "nur", "nicht", "mit", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PTKNEG", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Eh' noch mit der gold'nen Locke", "tokens": ["Eh'", "noch", "mit", "der", "gold'\u00b7nen", "Lo\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eos durch die Himmel f\u00e4hrt,", "tokens": ["E\u00b7os", "durch", "die", "Him\u00b7mel", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "St\u00fcrmt die dunkle Abschiedsglocke,", "tokens": ["St\u00fcrmt", "die", "dunk\u00b7le", "Ab\u00b7schieds\u00b7glo\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00f6\u00dft in meine Brust ein Schwert.", "tokens": ["St\u00f6\u00dft", "in", "mei\u00b7ne", "Brust", "ein", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und das Seelenschwert im Busen,", "tokens": ["Und", "das", "See\u00b7len\u00b7schwert", "im", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df ich deinem Aug' entfliehn,", "tokens": ["Mu\u00df", "ich", "dei\u00b7nem", "Aug'", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Darf nicht mehr an deinem Busen,", "tokens": ["Darf", "nicht", "mehr", "an", "dei\u00b7nem", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht an deinen Lippen gl\u00fchn.", "tokens": ["Nicht", "an", "dei\u00b7nen", "Lip\u00b7pen", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Hin, wo Oceane st\u00fcrmen,", "tokens": ["Hin", ",", "wo", "O\u00b7cea\u00b7ne", "st\u00fcr\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo sich hoch vom wei\u00dfen Strand", "tokens": ["Wo", "sich", "hoch", "vom", "wei\u00b7\u00dfen", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueberschnei'te Berge th\u00fcrmen,", "tokens": ["Ue\u00b7ber\u00b7schnei'\u00b7te", "Ber\u00b7ge", "th\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werd' ich einsam hingebannt. \u2013 \u2013", "tokens": ["Werd'", "ich", "ein\u00b7sam", "hin\u00b7ge\u00b7bannt", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Aber st\u00fcrmten gleich der Meere", "tokens": ["A\u00b7ber", "st\u00fcrm\u00b7ten", "gleich", "der", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwanzig tausend vor mir hin;", "tokens": ["Zwan\u00b7zig", "tau\u00b7send", "vor", "mir", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "CARD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Riss' gleich eine ganze Sph\u00e4re", "tokens": ["Riss'", "gleich", "ei\u00b7ne", "gan\u00b7ze", "Sph\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich von dir, o Lieblinginn \u2013", "tokens": ["Mich", "von", "dir", ",", "o", "Lieb\u00b7lin\u00b7ginn", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "$,", "FM", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Br\u00fcllt, ihr Meere, heult, ihr Winde;", "tokens": ["Br\u00fcllt", ",", "ihr", "Mee\u00b7re", ",", "heult", ",", "ihr", "Win\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$."], "met