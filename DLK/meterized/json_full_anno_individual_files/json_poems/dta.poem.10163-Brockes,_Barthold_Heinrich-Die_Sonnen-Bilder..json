{"dta.poem.10163": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Sonnen-Bilder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst, als die Nacht sich kaum verborgen,", "tokens": ["J\u00fcngst", ",", "als", "die", "Nacht", "sich", "kaum", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lie\u00df mich, im Herbst, ein heitrer Morgen", "tokens": ["Lie\u00df", "mich", ",", "im", "Herbst", ",", "ein", "hei\u00b7trer", "Mor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf einmahl wunder-wunder-sch\u00f6n,", "tokens": ["Auf", "ein\u00b7mahl", "wun\u00b7der\u00b7wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stat einer, sieben Sonnen sehn.", "tokens": ["Stat", "ei\u00b7ner", ",", "sie\u00b7ben", "Son\u00b7nen", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus sieben Fenstern auf einmahl", "tokens": ["Aus", "sie\u00b7ben", "Fens\u00b7tern", "auf", "ein\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fiel mir, im Wiederschlag, ein siebenfacher Strahl", "tokens": ["Fiel", "mir", ",", "im", "Wie\u00b7der\u00b7schlag", ",", "ein", "sie\u00b7ben\u00b7fa\u00b7cher", "Strahl"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch einen gr\u00fcnen Baum, der von der Bl\u00e4tter-", "tokens": ["Durch", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Baum", ",", "der", "von", "der", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schon guten theils entbl\u00f6sset war,", "tokens": ["Schon", "gu\u00b7ten", "theils", "ent\u00b7bl\u00f6s\u00b7set", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit hellem Funckeln, ins Gesicht.", "tokens": ["Mit", "hel\u00b7lem", "Fun\u00b7ckeln", ",", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die wahre Sonne sah\u2019 ich nicht.", "tokens": ["Die", "wah\u00b7re", "Son\u00b7ne", "sah'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr, durch ein ander Haus verdecktes, Licht", "tokens": ["Ihr", ",", "durch", "ein", "an\u00b7der", "Haus", "ver\u00b7deck\u00b7tes", ",", "Licht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "$,", "APPR", "ART", "ADJA", "NN", "ADJA", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Stand schr\u00e4ge, so da\u00df es dadurch den Winckel machte,", "tokens": ["Stand", "schr\u00e4\u00b7ge", ",", "so", "da\u00df", "es", "da\u00b7durch", "den", "Win\u00b7ckel", "mach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADV", "KOUS", "PPER", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und eben diesen Glantz dadurch zu wege brachte.", "tokens": ["Und", "e\u00b7ben", "die\u00b7sen", "Glantz", "da\u00b7durch", "zu", "we\u00b7ge", "brach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDAT", "NN", "PAV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein", "tokens": ["Al\u00b7lein"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Durch den verdoppelten, obgleich verkleinten Schein,", "tokens": ["Durch", "den", "ver\u00b7dop\u00b7pel\u00b7ten", ",", "ob\u00b7gleich", "ver\u00b7klein\u00b7ten", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Erregte sie in meiner Brust", "tokens": ["Er\u00b7reg\u00b7te", "sie", "in", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein\u2019 unausdr\u00fccklich s\u00fcsse Lust.", "tokens": ["Ein'", "un\u00b7aus\u00b7dr\u00fcck\u00b7lich", "s\u00fcs\u00b7se", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nicht nur die sieben runden, hellen,", "tokens": ["Nicht", "nur", "die", "sie\u00b7ben", "run\u00b7den", ",", "hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "CARD", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durchs Laub so rund formirten Stellen", "tokens": ["Durchs", "Laub", "so", "rund", "for\u00b7mir\u00b7ten", "Stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durchdrungen mein Gesicht, mit einem hellen Blitze;", "tokens": ["Durch\u00b7drun\u00b7gen", "mein", "Ge\u00b7sicht", ",", "mit", "ei\u00b7nem", "hel\u00b7len", "Blit\u00b7ze", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es fuhr, aus ieder, eine Spitze,", "tokens": ["Es", "fuhr", ",", "aus", "ie\u00b7der", ",", "ei\u00b7ne", "Spit\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PIAT", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die sich verbreitete in eine Menge Strahlen,", "tokens": ["Die", "sich", "ver\u00b7brei\u00b7te\u00b7te", "in", "ei\u00b7ne", "Men\u00b7ge", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die alle feurig bunt: wodurch denn sonderlich", "tokens": ["Die", "al\u00b7le", "feu\u00b7rig", "bunt", ":", "wo\u00b7durch", "denn", "son\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIS", "ADJD", "ADJD", "$.", "PWAV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein ieglichs destomehr der wahren Sonne glich.", "tokens": ["Ein", "ieg\u00b7lichs", "des\u00b7to\u00b7mehr", "der", "wah\u00b7ren", "Son\u00b7ne", "glich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Bl\u00e4tter gr\u00fcne Dunckelheit", "tokens": ["Der", "Bl\u00e4t\u00b7ter", "gr\u00fc\u00b7ne", "Dun\u00b7ckel\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dient\u2019 ihnen recht zum sanften Grunde.", "tokens": ["Dient'", "ih\u00b7nen", "recht", "zum", "sanf\u00b7ten", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es ward dadurch der Strahlen Herrlichkeit", "tokens": ["Es", "ward", "da\u00b7durch", "der", "Strah\u00b7len", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PAV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Noch eins so sehr erhoben und vermehrt:", "tokens": ["Noch", "eins", "so", "sehr", "er\u00b7ho\u00b7ben", "und", "ver\u00b7mehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "ADV", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So da\u00df mein Hertz, f\u00fcr Luft, die es empfunde,", "tokens": ["So", "da\u00df", "mein", "Hertz", ",", "f\u00fcr", "Luft", ",", "die", "es", "emp\u00b7fun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "$,", "APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Fast selber halb entz\u00fcckt ward und verkl\u00e4rt:", "tokens": ["Fast", "sel\u00b7ber", "halb", "ent\u00b7z\u00fcckt", "ward", "und", "ver\u00b7kl\u00e4rt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVPP", "VAFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zumahl als ich, durch die\u00df so herrliche Gepr\u00e4nge", "tokens": ["Zu\u00b7mahl", "als", "ich", ",", "durch", "die\u00df", "so", "herr\u00b7li\u00b7che", "Ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "$,", "APPR", "PDS", "ADV", "ADJA", "NN"], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Der Sonnen-Bilder, auf die Menge", "tokens": ["Der", "Son\u00b7nen\u00b7Bil\u00b7der", ",", "auf", "die", "Men\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der wahren Sonnen, die die Tieffe", "tokens": ["Der", "wah\u00b7ren", "Son\u00b7nen", ",", "die", "die", "Tief\u00b7fe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Von aller Himmel Himmel f\u00fcllen,", "tokens": ["Von", "al\u00b7ler", "Him\u00b7mel", "Him\u00b7mel", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Mit einer frohen Achtung kam,", "tokens": ["Mit", "ei\u00b7ner", "fro\u00b7hen", "Ach\u00b7tung", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und, so dadurch zu dencken, Anla\u00df nahm:", "tokens": ["Und", ",", "so", "da\u00b7durch", "zu", "den\u00b7cken", ",", "An\u00b7la\u00df", "nahm", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "PAV", "PTKZU", "VVINF", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Da ein, im Wiederschein nur blo\u00df vervielfacht, Licht", "tokens": ["Da", "ein", ",", "im", "Wie\u00b7der\u00b7schein", "nur", "blo\u00df", "ver\u00b7viel\u00b7facht", ",", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PTKVZ", "$,", "APPRART", "NN", "ADV", "ADV", "VVPP", "$,", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von einer Sonne, selbst der Seele, durchs Gesicht,", "tokens": ["Von", "ei\u00b7ner", "Son\u00b7ne", ",", "selbst", "der", "See\u00b7le", ",", "durchs", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "ART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Solch eine Lust erweckt, so gar ihr inners schm\u00fccket,", "tokens": ["Solch", "ei\u00b7ne", "Lust", "er\u00b7weckt", ",", "so", "gar", "ihr", "in\u00b7ners", "schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VVPP", "$,", "ADV", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erheitert und verkl\u00e4rt, ja fast entz\u00fccket;", "tokens": ["Er\u00b7hei\u00b7tert", "und", "ver\u00b7kl\u00e4rt", ",", "ja", "fast", "ent\u00b7z\u00fc\u00b7cket", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In welchem klarem Licht, in welchem hellen Gl\u00e4ntzen", "tokens": ["In", "wel\u00b7chem", "kla\u00b7rem", "Licht", ",", "in", "wel\u00b7chem", "hel\u00b7len", "Gl\u00e4nt\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN", "$,", "APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mu\u00df wunder-wunder-wunder-sch\u00f6n", "tokens": ["Mu\u00df", "wun\u00b7der\u00b7wun\u00b7der\u00b7wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word"], "pos": ["VMFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Tieffe sonder Grund und Grentzen,", "tokens": ["Die", "Tief\u00b7fe", "son\u00b7der", "Grund", "und", "Grent\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wo Millionen Sonnen, stehn!", "tokens": ["Wo", "Mil\u00b7lion\u00b7en", "Son\u00b7nen", ",", "stehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "NN", "$,", "VVINF", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.9": {"text": "Wie mu\u00df im seelgen Himmel nicht", "tokens": ["Wie", "mu\u00df", "im", "seel\u00b7gen", "Him\u00b7mel", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "APPRART", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Doch aller Sonnen Sonnen-Licht,", "tokens": ["Doch", "al\u00b7ler", "Son\u00b7nen", "Son\u00b7nen\u00b7Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Aus dessen Gottheits-Meer, sie, blo\u00df als Tropfen, quillen,", "tokens": ["Aus", "des\u00b7sen", "Gott\u00b7heits\u00b7Meer", ",", "sie", ",", "blo\u00df", "als", "Trop\u00b7fen", ",", "quil\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "$,", "PPER", "$,", "ADV", "KOUS", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das (so wie unsrer Sonnen-Pracht", "tokens": ["Das", "(", "so", "wie", "uns\u00b7rer", "Son\u00b7nen\u00b7Pracht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$(", "ADV", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die C\u00f6rper nur) die Geister sichtbar macht,", "tokens": ["Die", "C\u00f6r\u00b7per", "nur", ")", "die", "Geis\u00b7ter", "sicht\u00b7bar", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$(", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-