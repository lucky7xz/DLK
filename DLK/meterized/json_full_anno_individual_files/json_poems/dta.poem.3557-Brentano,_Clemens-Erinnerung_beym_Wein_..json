{"dta.poem.3557": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Erinnerung beym Wein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es dunkelt auf jenem Berge,               ", "tokens": ["Es", "dun\u00b7kelt", "auf", "je\u00b7nem", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nach Hause wollen wir gehen;", "tokens": ["Nach", "Hau\u00b7se", "wol\u00b7len", "wir", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Den Wein, den wollen wir trinken,", "tokens": ["Den", "Wein", ",", "den", "wol\u00b7len", "wir", "trin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den wir gewohnet seyn.", "tokens": ["Den", "wir", "ge\u00b7woh\u00b7net", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6r ein Hirschlein rauschen,", "tokens": ["Ich", "h\u00f6r", "ein", "Hirsc\u00b7hlein", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl rauschen durch den Wald;", "tokens": ["Wohl", "rau\u00b7schen", "durch", "den", "Wald", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich h\u00f6r ein feines Lieb klagen,", "tokens": ["Ich", "h\u00f6r", "ein", "fei\u00b7nes", "Lieb", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Klagen, es h\u00e4tt' die Ehr verloren.", "tokens": ["Kla\u00b7gen", ",", "es", "h\u00e4tt'", "die", "Ehr", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Hast du deine Ehr verloren,", "tokens": ["Hast", "du", "dei\u00b7ne", "Ehr", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab ich die meine noch;", "tokens": ["Hab", "ich", "die", "mei\u00b7ne", "noch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "PPOSAT", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So gehen wir miteinander,", "tokens": ["So", "ge\u00b7hen", "wir", "mi\u00b7tein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und tragen die Kr\u00e4nzelein.", "tokens": ["Und", "tra\u00b7gen", "die", "Kr\u00e4n\u00b7zel\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein Kr\u00e4nzelein von Rosen,", "tokens": ["Ein", "Kr\u00e4n\u00b7zel\u00b7ein", "von", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Kr\u00e4nzelein von Klee;", "tokens": ["Ein", "Kr\u00e4n\u00b7zel\u00b7ein", "von", "Klee", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu Stra\u00dfburg auf der Brucke,", "tokens": ["Zu", "Stra\u00df\u00b7burg", "auf", "der", "Bru\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da liegt ein tiefer Schnee.", "tokens": ["Da", "liegt", "ein", "tie\u00b7fer", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn der Schnee thut schmelzen,", "tokens": ["Wenn", "der", "Schnee", "thut", "schmel\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "So lauft das Wasser in See;", "tokens": ["So", "lauft", "das", "Was\u00b7ser", "in", "See", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Darauf bin ich gesessen,", "tokens": ["Da\u00b7rauf", "bin", "ich", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und gefahren bis hieher.", "tokens": ["Und", "ge\u00b7fah\u00b7ren", "bis", "hie\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "KON", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}