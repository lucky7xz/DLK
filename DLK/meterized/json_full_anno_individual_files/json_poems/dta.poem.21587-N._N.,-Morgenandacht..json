{"dta.poem.21587": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Morgenandacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Es rauscht zur Seite mir der Strom,", "tokens": ["Es", "rauscht", "zur", "Sei\u00b7te", "mir", "der", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das klingt wie Orgelsang,", "tokens": ["Das", "klingt", "wie", "Or\u00b7gel\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und immer m\u00e4cht\u2019ger schwillt der Klang", "tokens": ["Und", "im\u00b7mer", "m\u00e4cht'\u00b7ger", "schwillt", "der", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Empor zum blauen Himmelsdom.", "tokens": ["Em\u00b7por", "zum", "blau\u00b7en", "Him\u00b7mels\u00b7dom", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch aller B\u00e4ume Wipfel geht", "tokens": ["Durch", "al\u00b7ler", "B\u00e4u\u00b7me", "Wip\u00b7fel", "geht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein S\u00e4useln leis und andachtsvoll", "tokens": ["Ein", "S\u00e4u\u00b7seln", "leis", "und", "an\u00b7dachts\u00b7voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Lippen fl\u00fcstern ein Gebet,", "tokens": ["Wie", "Lip\u00b7pen", "fl\u00fcs\u00b7tern", "ein", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Frieden bringen soll.", "tokens": ["Das", "Frie\u00b7den", "brin\u00b7gen", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}