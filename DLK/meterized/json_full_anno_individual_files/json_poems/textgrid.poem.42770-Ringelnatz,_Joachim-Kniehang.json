{"textgrid.poem.42770": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Kniehang", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wollte, ich w\u00e4r eine Fledermaus,", "tokens": ["Ich", "woll\u00b7te", ",", "ich", "w\u00e4r", "ei\u00b7ne", "Fle\u00b7der\u00b7maus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+++-+-+", "measure": "zehnsilber"}, "line.2": {"text": "Eine ganz verluschte, verlauste,", "tokens": ["Ei\u00b7ne", "ganz", "ver\u00b7luschte", ",", "ver\u00b7laus\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann hing ich mich fr\u00fch in ein Warenhaus", "tokens": ["Dann", "hing", "ich", "mich", "fr\u00fch", "in", "ein", "Wa\u00b7ren\u00b7haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und flederte nachts und mauste,", "tokens": ["Und", "fle\u00b7der\u00b7te", "nachts", "und", "maus\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "KON", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da\u00df es Herrn Silberstein grauste.", "tokens": ["Da\u00df", "es", "Herrn", "Sil\u00b7ber\u00b7stein", "graus\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "NN", "ADJA", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.6": {"text": "Denn Meterflau